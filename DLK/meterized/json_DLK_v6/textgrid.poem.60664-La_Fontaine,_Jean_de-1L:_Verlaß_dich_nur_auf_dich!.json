{"textgrid.poem.60664": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verla\u00df dich nur auf dich!", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verla\u00df dich nur auf dich!", "tokens": ["Ver\u00b7la\u00df", "dich", "nur", "auf", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das ist ein Wort voll Wahrheitskraft.", "tokens": ["Das", "ist", "ein", "Wort", "voll", "Wahr\u00b7heits\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt, wie \u00c4sop ihm Glauben schafft.", "tokens": ["H\u00f6rt", ",", "wie", "\u00c4\u00b7sop", "ihm", "Glau\u00b7ben", "schafft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NE", "PPER", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Die Lerche baut ihr Nestchen sich", "tokens": ["Die", "Ler\u00b7che", "baut", "ihr", "Nest\u00b7chen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Acker, wenn noch gr\u00fcn das Feld,", "tokens": ["Im", "A\u00b7cker", ",", "wenn", "noch", "gr\u00fcn", "das", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das hei\u00dft, zur Zeit da alles in der Welt", "tokens": ["Das", "hei\u00dft", ",", "zur", "Zeit", "da", "al\u00b7les", "in", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "APPRART", "NN", "ADV", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Liebe sich vereint, da\u00df sich die Art erh\u00e4lt:", "tokens": ["In", "Lie\u00b7be", "sich", "ver\u00b7eint", ",", "da\u00df", "sich", "die", "Art", "er\u00b7h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Ungeheuer tief im Meer,", "tokens": ["Die", "Un\u00b7ge\u00b7heu\u00b7er", "tief", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Tiger in den W\u00e4ldern,", "tokens": ["Die", "Ti\u00b7ger", "in", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Lerchen in den Feldern.", "tokens": ["Die", "Ler\u00b7chen", "in", "den", "Fel\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von diesen letzten s\u00e4umte eine sehr", "tokens": ["Von", "die\u00b7sen", "letz\u00b7ten", "s\u00e4um\u00b7te", "ei\u00b7ne", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "VVFIN", "ART", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und lie\u00df den halben Lenz verflie\u00dfen,", "tokens": ["Und", "lie\u00df", "den", "hal\u00b7ben", "Lenz", "ver\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ohne die Fr\u00fchlingsfreuden zu genie\u00dfen;", "tokens": ["Oh\u00b7ne", "die", "Fr\u00fch\u00b7lings\u00b7freu\u00b7den", "zu", "ge\u00b7nie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Nun aber gab ihr die Natur voll Kraft", "tokens": ["Nun", "a\u00b7ber", "gab", "ihr", "die", "Na\u00b7tur", "voll", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Den hei\u00dfen Wunsch nach Mutterschaft.", "tokens": ["Den", "hei\u00b7\u00dfen", "Wunsch", "nach", "Mut\u00b7ter\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sie baute, legte, br\u00fctete im Neste", "tokens": ["Sie", "bau\u00b7te", ",", "leg\u00b7te", ",", "br\u00fc\u00b7te\u00b7te", "im", "Nes\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die Eier aus, und alles ging aufs beste.", "tokens": ["Die", "Ei\u00b7er", "aus", ",", "und", "al\u00b7les", "ging", "aufs", "bes\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "PIS", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Rings das Getreide aber trug", "tokens": ["Rings", "das", "Ge\u00b7trei\u00b7de", "a\u00b7ber", "trug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Schon wei\u00dfe K\u00f6rner, eh' die Jungen stark genug", "tokens": ["Schon", "wei\u00b7\u00dfe", "K\u00f6r\u00b7ner", ",", "eh'", "die", "Jun\u00b7gen", "stark", "ge\u00b7nug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zum Fliegen waren.", "tokens": ["Zum", "Flie\u00b7gen", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Die Lerche sah: hier drohten bald Gefahren!", "tokens": ["Die", "Ler\u00b7che", "sah", ":", "hier", "droh\u00b7ten", "bald", "Ge\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Voll banger Sorgen flog sie auf den Beutezug,", "tokens": ["Voll", "ban\u00b7ger", "Sor\u00b7gen", "flog", "sie", "auf", "den", "Beu\u00b7te\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nachdem sie ihre Kinder so belehrt:", "tokens": ["Nach\u00b7dem", "sie", "ih\u00b7re", "Kin\u00b7der", "so", "be\u00b7lehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "\u00bbseid auf der Hut", "tokens": ["\u00bb", "seid", "auf", "der", "Hut"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.22": {"text": "Und achtet gut:", "tokens": ["Und", "ach\u00b7tet", "gut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Wenn der, dem dieses Feld geh\u00f6rt,", "tokens": ["Wenn", "der", ",", "dem", "die\u00b7ses", "Feld", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Mit seinem Sohne kommt (es wird geschehen!),", "tokens": ["Mit", "sei\u00b7nem", "Soh\u00b7ne", "kommt", "(", "es", "wird", "ge\u00b7sche\u00b7hen", "!", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "PPER", "VAFIN", "VVPP", "$.", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Belauschet ihn; denn je nach seinem Wort", "tokens": ["Be\u00b7lau\u00b7schet", "ihn", ";", "denn", "je", "nach", "sei\u00b7nem", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Gilt's noch zu bleiben oder \u2013 schleunigst fort!\u00ab", "tokens": ["Gilt's", "noch", "zu", "blei\u00b7ben", "o\u00b7der", "\u2013", "schleu\u00b7nigst", "fort", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "PTKZU", "VVINF", "KON", "$(", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Kaum hat die Mutter sie verlassen, sehen", "tokens": ["Kaum", "hat", "die", "Mut\u00b7ter", "sie", "ver\u00b7las\u00b7sen", ",", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Den Herrn mit seinem Sohn sie nahn. Er spricht:", "tokens": ["Den", "Herrn", "mit", "sei\u00b7nem", "Sohn", "sie", "nahn", ".", "Er", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "\u00bbdas Korn ist reif. Nun s\u00e4ume nicht,", "tokens": ["\u00bb", "das", "Korn", "ist", "reif", ".", "Nun", "s\u00e4u\u00b7me", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Zu unsern Freunden hinzugehen,", "tokens": ["Zu", "un\u00b7sern", "Freun\u00b7den", "hin\u00b7zu\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und bitte sie, sobald der n\u00e4chste Tag anbricht,", "tokens": ["Und", "bit\u00b7te", "sie", ",", "so\u00b7bald", "der", "n\u00e4chs\u00b7te", "Tag", "an\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mit ihren Sensen zu erscheinen,", "tokens": ["Mit", "ih\u00b7ren", "Sen\u00b7sen", "zu", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Um unsern Acker abzum\u00e4hen.\u00ab", "tokens": ["Um", "un\u00b7sern", "A\u00b7cker", "ab\u00b7zu\u00b7m\u00e4\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVIZU", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Die Lerche findet, als sie wiederkehrt,", "tokens": ["Die", "Ler\u00b7che", "fin\u00b7det", ",", "als", "sie", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "In gro\u00dfer Angst die armen Kleinen.", "tokens": ["In", "gro\u00b7\u00dfer", "Angst", "die", "ar\u00b7men", "Klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Sie sagen ihr, was sie geh\u00f6rt:", "tokens": ["Sie", "sa\u00b7gen", "ihr", ",", "was", "sie", "ge\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "\u00bber hat f\u00fcr morgen fr\u00fch die Freunde all bestellt,", "tokens": ["\u00bb", "er", "hat", "f\u00fcr", "mor\u00b7gen", "fr\u00fch", "die", "Freun\u00b7de", "all", "be\u00b7stellt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "ADV", "ADJD", "ART", "NN", "PIAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Um abzum\u00e4hen dieses Feld.\u00ab", "tokens": ["Um", "ab\u00b7zu\u00b7m\u00e4\u00b7hen", "die\u00b7ses", "Feld", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "VVIZU", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Da sprach die Lerche: \u00bbHat er dieses nur gesagt,", "tokens": ["Da", "sprach", "die", "Ler\u00b7che", ":", "\u00bb", "Hat", "er", "die\u00b7ses", "nur", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "VAFIN", "PPER", "PDS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So dr\u00e4ngt uns nichts, so haben wir noch Ruh!", "tokens": ["So", "dr\u00e4ngt", "uns", "nichts", ",", "so", "ha\u00b7ben", "wir", "noch", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "ADV", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Doch h\u00f6ret morgen wieder zu.", "tokens": ["Doch", "h\u00f6\u00b7ret", "mor\u00b7gen", "wie\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Jetzt e\u00dft und labt euch, wie es euch behagt.\u00ab", "tokens": ["Jetzt", "e\u00dft", "und", "labt", "euch", ",", "wie", "es", "euch", "be\u00b7hagt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Sie essen, schlafen dann,", "tokens": ["Sie", "es\u00b7sen", ",", "schla\u00b7fen", "dann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Die Mutter wie die Kleinen.", "tokens": ["Die", "Mut\u00b7ter", "wie", "die", "Klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Der neue Tag bricht an,", "tokens": ["Der", "neu\u00b7e", "Tag", "bricht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "Von all den Freunden aber sieht man keinen.", "tokens": ["Von", "all", "den", "Freun\u00b7den", "a\u00b7ber", "sieht", "man", "kei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "ADV", "VVFIN", "PIS", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Die Lerche fliegt, um sich nach Futter umzusehen,", "tokens": ["Die", "Ler\u00b7che", "fliegt", ",", "um", "sich", "nach", "Fut\u00b7ter", "um\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUI", "PRF", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "