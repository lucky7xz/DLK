{"textgrid.poem.43848": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Franz, sag' ich, la\u00df das K\u00e4tzchen in Ruh!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Franz, sag' ich, la\u00df das K\u00e4tzchen in Ruh!", "tokens": ["Franz", ",", "sag'", "ich", ",", "la\u00df", "das", "K\u00e4tz\u00b7chen", "in", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VVIMP", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es wird dich kratzen.", "tokens": ["Es", "wird", "dich", "krat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "La\u00df dich doch nicht warnen immerzu!", "tokens": ["La\u00df", "dich", "doch", "nicht", "war\u00b7nen", "im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Falsch sind die Katzen.", "tokens": ["Falsch", "sind", "die", "Kat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie k\u00f6nnen schnurren und z\u00e4rtlich sein,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "schnur\u00b7ren", "und", "z\u00e4rt\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich schmiegen und schmeicheln,", "tokens": ["Sich", "schmie\u00b7gen", "und", "schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie t\u00e4tzeln und ziehen die Krallen ein", "tokens": ["Sie", "t\u00e4t\u00b7zeln", "und", "zie\u00b7hen", "die", "Kral\u00b7len", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "ART"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und lassen sich streicheln.", "tokens": ["Und", "las\u00b7sen", "sich", "strei\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch \u00e4ndern sich auch oft geschwind", "tokens": ["Doch", "\u00e4n\u00b7dern", "sich", "auch", "oft", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die freundlichsten Katzen,", "tokens": ["Die", "freund\u00b7lichs\u00b7ten", "Kat\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Denn Katzen nun einmal Katzen sind", "tokens": ["Denn", "Kat\u00b7zen", "nun", "ein\u00b7mal", "Kat\u00b7zen", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "ADV", "NN", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und m\u00fcssen kratzen.", "tokens": ["Und", "m\u00fcs\u00b7sen", "krat\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}