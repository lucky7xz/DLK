{"textgrid.poem.36510": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "9. Der Hahn und das H\u00e4schen", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hahn, des Hofes Stolz, trat hin, und scharrte tief", "tokens": ["Ein", "Hahn", ",", "des", "Ho\u00b7fes", "Stolz", ",", "trat", "hin", ",", "und", "scharr\u00b7te", "tief"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seinem alten Mist.", "tokens": ["In", "sei\u00b7nem", "al\u00b7ten", "Mist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Hase, der vor\u00fcber lief,", "tokens": ["Ein", "Ha\u00b7se", ",", "der", "vor\u00b7\u00fc\u00b7ber", "lief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprach: Nachbar! lieber Nachbar! ist", "tokens": ["Sprach", ":", "Nach\u00b7bar", "!", "lie\u00b7ber", "Nach\u00b7bar", "!", "ist"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$.", "NN", "$.", "ADV", "NN", "$.", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn da noch was heraus zu scharrn?", "tokens": ["Denn", "da", "noch", "was", "he\u00b7raus", "zu", "scharrn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PWS", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja, sprach der stolze Hahn, noch etwas f\u00fcr die Narrn,", "tokens": ["Ja", ",", "sprach", "der", "stol\u00b7ze", "Hahn", ",", "noch", "et\u00b7was", "f\u00fcr", "die", "Narrn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die weisen Leute lie\u00dfen's drin! \u2013", "tokens": ["Die", "wei\u00b7sen", "Leu\u00b7te", "lie\u00b7\u00dfen's", "drin", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und gab, verachtend, und mit Zorn,", "tokens": ["Und", "gab", ",", "ver\u00b7ach\u00b7tend", ",", "und", "mit", "Zorn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVPP", "$,", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein halbverfaultes Weizenkorn", "tokens": ["Ein", "halb\u00b7ver\u00b7faul\u00b7tes", "Wei\u00b7zen\u00b7korn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dem guten Hasen hin.", "tokens": ["Dem", "gu\u00b7ten", "Ha\u00b7sen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Hase, kein ver\u00e4chtlich Tier,", "tokens": ["Der", "Ha\u00b7se", ",", "kein", "ver\u00b7\u00e4cht\u00b7lich", "Tier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4lt's au die Nase, spricht:", "tokens": ["H\u00e4lt's", "au", "die", "Na\u00b7se", ",", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verfaultes essen wir,", "tokens": ["Ver\u00b7faul\u00b7tes", "es\u00b7sen", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf freiem Felde, nicht!", "tokens": ["Auf", "frei\u00b7em", "Fel\u00b7de", ",", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}