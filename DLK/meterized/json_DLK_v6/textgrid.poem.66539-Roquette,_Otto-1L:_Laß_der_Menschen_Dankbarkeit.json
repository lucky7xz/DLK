{"textgrid.poem.66539": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df der Menschen Dankbarkeit", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df der Menschen Dankbarkeit", "tokens": ["La\u00df", "der", "Men\u00b7schen", "Dank\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Immerhin dir sein entbehrlich,", "tokens": ["Im\u00b7mer\u00b7hin", "dir", "sein", "ent\u00b7behr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Wort sind sie bereit,", "tokens": ["Mit", "dem", "Wort", "sind", "sie", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch die That ist gar beschwerlich.", "tokens": ["Doch", "die", "That", "ist", "gar", "be\u00b7schwer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur wo Gab' und Dankeszoll", "tokens": ["Nur", "wo", "Gab'", "und", "Dan\u00b7kes\u00b7zoll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stets sich in einander ranken,", "tokens": ["Stets", "sich", "in", "ein\u00b7an\u00b7der", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprossen, gleicher Bl\u00fcthen voll,", "tokens": ["Spros\u00b7sen", ",", "glei\u00b7cher", "Bl\u00fc\u00b7then", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die Liebe wei\u00df zu danken.", "tokens": ["Nur", "die", "Lie\u00b7be", "wei\u00df", "zu", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}