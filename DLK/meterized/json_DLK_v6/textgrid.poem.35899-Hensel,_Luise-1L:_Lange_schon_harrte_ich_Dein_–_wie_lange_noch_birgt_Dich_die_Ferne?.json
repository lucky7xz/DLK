{"textgrid.poem.35899": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lange schon harrte ich Dein \u2013 wie lange noch birgt Dich die Ferne?", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange schon harrte ich Dein \u2013 wie lange noch birgt Dich die Ferne?", "tokens": ["Lan\u00b7ge", "schon", "harr\u00b7te", "ich", "Dein", "\u2013", "wie", "lan\u00b7ge", "noch", "birgt", "Dich", "die", "Fer\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "$(", "PWAV", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Lange schon blick' ich nach Dir \u2013 ach! wann erblicke ich Dich?", "tokens": ["Lan\u00b7ge", "schon", "blick'", "ich", "nach", "Dir", "\u2013", "ach", "!", "wann", "er\u00b7bli\u00b7cke", "ich", "Dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$(", "ITJ", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wenn sich die Sonne erhebt, so hoff' ich: sie bringt Dich, Geliebter!", "tokens": ["Wenn", "sich", "die", "Son\u00b7ne", "er\u00b7hebt", ",", "so", "hoff'", "ich", ":", "sie", "bringt", "Dich", ",", "Ge\u00b7lieb\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Wenn sie hinunter dann sinkt, seufz' ich, da\u00df Du mir noch fehlst.", "tokens": ["Wenn", "sie", "hin\u00b7un\u00b7ter", "dann", "sinkt", ",", "seuf\u00b7z'", "ich", ",", "da\u00df", "Du", "mir", "noch", "fehlst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Siehe, so spinnt sich mein Tag der eilenden Zeit von der Spindel.", "tokens": ["Sie\u00b7he", ",", "so", "spinnt", "sich", "mein", "Tag", "der", "ei\u00b7len\u00b7den", "Zeit", "von", "der", "Spin\u00b7del", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gestern verlangt' ich wie heut; Gestern auch trog mich wie Heut.", "tokens": ["Ge\u00b7stern", "ver\u00b7langt'", "ich", "wie", "heut", ";", "Ge\u00b7stern", "auch", "trog", "mich", "wie", "Heut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KOKOM", "ADV", "$.", "NN", "ADV", "VVFIN", "PPER", "KOKOM", "ADV", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Betend und ringend f\u00fcr Dich begr\u00fc\u00df' ich den leuchtenden Morgen,", "tokens": ["Be\u00b7tend", "und", "rin\u00b7gend", "f\u00fcr", "Dich", "be\u00b7gr\u00fc\u00df'", "ich", "den", "leuch\u00b7ten\u00b7den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "APPR", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Bittend und sorgend um Dich schlie\u00df' ich mein Augenlid zu.", "tokens": ["Bit\u00b7tend", "und", "sor\u00b7gend", "um", "Dich", "schlie\u00df'", "ich", "mein", "Au\u00b7gen\u00b7lid", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Bruder, was z\u00f6gerst Du noch? L\u00e4ngst hat Dir der Heiland gerufen,", "tokens": ["Bru\u00b7der", ",", "was", "z\u00f6\u00b7gerst", "Du", "noch", "?", "L\u00e4ngst", "hat", "Dir", "der", "Hei\u00b7land", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$.", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ach! und Du ringst und Du s\u00e4umst \u2013 ach! und es reizt Dich die Welt.", "tokens": ["Ach", "!", "und", "Du", "ringst", "und", "Du", "s\u00e4umst", "\u2013", "ach", "!", "und", "es", "reizt", "Dich", "die", "Welt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$(", "ITJ", "$.", "KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Bruder, ", "tokens": ["Bru\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Sch\u00e4me der Fesseln Dich, Held! acht' ihre Kr\u00e4nze f\u00fcr Tand.", "tokens": ["Sch\u00e4\u00b7me", "der", "Fes\u00b7seln", "Dich", ",", "Held", "!", "acht'", "ih\u00b7re", "Kr\u00e4n\u00b7ze", "f\u00fcr", "Tand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$,", "NN", "$.", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Aber Du stehst noch und schwankst und h\u00e4ltst noch die Waag' in den H\u00e4nden.", "tokens": ["A\u00b7ber", "Du", "stehst", "noch", "und", "schwankst", "und", "h\u00e4ltst", "noch", "die", "Waag'", "in", "den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KON", "VVFIN", "KON", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Bruder, ergreife das Gut! Bruder, verachte den Schein!", "tokens": ["Bru\u00b7der", ",", "er\u00b7grei\u00b7fe", "das", "Gut", "!", "Bru\u00b7der", ",", "ver\u00b7ach\u00b7te", "den", "Schein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$.", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Eile zu Jesus, Er streckt so lange nach Dir schon die Arme,", "tokens": ["Ei\u00b7le", "zu", "Je\u00b7sus", ",", "Er", "streckt", "so", "lan\u00b7ge", "nach", "Dir", "schon", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+---+-+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Eile zur Kirche, sie schaut lange schon sehnend nach Dir.", "tokens": ["Ei\u00b7le", "zur", "Kir\u00b7che", ",", "sie", "schaut", "lan\u00b7ge", "schon", "seh\u00b7nend", "nach", "Dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.5": {"line.1": {"text": "Komm und erringe den Kranz, den herrlichen, der Dir gewunden,", "tokens": ["Komm", "und", "er\u00b7rin\u00b7ge", "den", "Kranz", ",", "den", "herr\u00b7li\u00b7chen", ",", "der", "Dir", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Komm und erfasse den Kelch, der Dir bereitet schon steht.", "tokens": ["Komm", "und", "er\u00b7fas\u00b7se", "den", "Kelch", ",", "der", "Dir", "be\u00b7rei\u00b7tet", "schon", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "K\u00f6nnt' ich erk\u00e4mpfen Dein Heil, Geliebter, mein Bruder! Dein w\u00e4r' es;", "tokens": ["K\u00f6nnt'", "ich", "er\u00b7k\u00e4mp\u00b7fen", "Dein", "Heil", ",", "Ge\u00b7lieb\u00b7ter", ",", "mein", "Bru\u00b7der", "!", "Dein", "w\u00e4r'", "es", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "VAFIN", "PPER", "$."], "meter": "+--+--+-+--+-+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wisse: mich d\u00fcrstet danach, wie nach dem eigenen Heil.", "tokens": ["Wis\u00b7se", ":", "mich", "d\u00fcrs\u00b7tet", "da\u00b7nach", ",", "wie", "nach", "dem", "ei\u00b7ge\u00b7nen", "Heil", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "PAV", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}