{"textgrid.poem.43785": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Schneemann", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht, da steht er, unser Schneemann!", "tokens": ["Seht", ",", "da", "steht", "er", ",", "un\u00b7ser", "Schnee\u00b7mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ist ein Geselle!", "tokens": ["Das", "ist", "ein", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Stehet fest und unverzagt,", "tokens": ["Ste\u00b7het", "fest", "und", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weicht nicht von der Stelle.", "tokens": ["Weicht", "nicht", "von", "der", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Schaut ihm in die schwarzen Augen!", "tokens": ["Schaut", "ihm", "in", "die", "schwar\u00b7zen", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird euch denn nicht bange?", "tokens": ["Wird", "euch", "denn", "nicht", "ban\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In der linken Hand da hat er", "tokens": ["In", "der", "lin\u00b7ken", "Hand", "da", "hat", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine lange Stange.", "tokens": ["Ei\u00b7ne", "lan\u00b7ge", "Stan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Einen gro\u00dfen S\u00e4bel h\u00e4lt er", "tokens": ["Ei\u00b7nen", "gro\u00b7\u00dfen", "S\u00e4\u00b7bel", "h\u00e4lt", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fest in seiner Rechten.", "tokens": ["Fest", "in", "sei\u00b7ner", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kommt heran! er wird sich wehren,", "tokens": ["Kommt", "he\u00b7ran", "!", "er", "wird", "sich", "weh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "PPER", "VAFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird mit Allen fechten.", "tokens": ["Wird", "mit", "Al\u00b7len", "fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ueber ihn kann nur der Fr\u00fchling", "tokens": ["Ue\u00b7ber", "ihn", "kann", "nur", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "ADV", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Einen Sieg gewinnen:", "tokens": ["Ei\u00b7nen", "Sieg", "ge\u00b7win\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blickt ihn ", "tokens": ["Blickt", "ihn"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wird er gleich zerrinnen.", "tokens": ["Wird", "er", "gleich", "zer\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Aber halt dich tapfer, Schneemann!", "tokens": ["A\u00b7ber", "halt", "dich", "tap\u00b7fer", ",", "Schnee\u00b7mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dir offenbaren:", "tokens": ["La\u00df", "dir", "of\u00b7fen\u00b7ba\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Stehst du morgen noch, so wollen", "tokens": ["Stehst", "du", "mor\u00b7gen", "noch", ",", "so", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir dich schlittenfahren.", "tokens": ["Wir", "dich", "schlit\u00b7ten\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}