{"textgrid.poem.62572": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Der 3te Vers des 11ten Cap. des Prediger Salomonis.", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sterb und komme vors Gericht,", "tokens": ["Ich", "sterb", "und", "kom\u00b7me", "vors", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mu\u00df die Erde meiden;", "tokens": ["Ich", "mu\u00df", "die", "Er\u00b7de", "mei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein die Stunde wei\u00df ich nicht,", "tokens": ["Al\u00b7lein", "die", "Stun\u00b7de", "wei\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich von hier soll scheiden.", "tokens": ["Wenn", "ich", "von", "hier", "soll", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist nun mein Geist darzu geschickt,", "tokens": ["Ist", "nun", "mein", "Geist", "dar\u00b7zu", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So werd ich ewiglich erquickt!", "tokens": ["So", "werd", "ich", "e\u00b7wig\u00b7lich", "er\u00b7quickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn Gott das Urtheil f\u00e4llet.", "tokens": ["Wenn", "Gott", "das", "Ur\u00b7theil", "f\u00e4l\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df nicht, wenn der Todes-Schlag", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wenn", "der", "To\u00b7des\u00b7Schlag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Lebens-Baum zernichtet;", "tokens": ["Den", "Le\u00b7bens\u00b7Baum", "zer\u00b7nich\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob sich sein Stamm am hellen Tag,", "tokens": ["Ob", "sich", "sein", "Stamm", "am", "hel\u00b7len", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum schnellen Falle richtet:", "tokens": ["Zum", "schnel\u00b7len", "Fal\u00b7le", "rich\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df nicht, welche Zeit er f\u00e4llt.", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wel\u00b7che", "Zeit", "er", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie nun hiebey der Geist bestellt,", "tokens": ["Wie", "nun", "hie\u00b7bey", "der", "Geist", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Darnach wirds ihm auch gehen.", "tokens": ["Dar\u00b7nach", "wirds", "ihm", "auch", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum bitt ich dich aus Herzens-Grund,", "tokens": ["Drum", "bitt", "ich", "dich", "aus", "Her\u00b7zens\u00b7Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Herr der Herrlichkeiten", "tokens": ["O", "Herr", "der", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du wollest mich zur Todes-Stund", "tokens": ["Du", "wol\u00b7lest", "mich", "zur", "To\u00b7des\u00b7Stund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch deinen Geist bereiten.", "tokens": ["Durch", "dei\u00b7nen", "Geist", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gieb, da\u00df ich sey bey Tag und Nacht", "tokens": ["Gieb", ",", "da\u00df", "ich", "sey", "bey", "Tag", "und", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf meine Sterbens-Zeit bedacht,", "tokens": ["Auf", "mei\u00b7ne", "Ster\u00b7bens\u00b7Zeit", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Damit ich nicht verderbe.", "tokens": ["Da\u00b7mit", "ich", "nicht", "ver\u00b7der\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vor allen tilge meine S\u00fcnd", "tokens": ["Vor", "al\u00b7len", "til\u00b7ge", "mei\u00b7ne", "S\u00fcnd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Jesu Schwei\u00df und Blute.", "tokens": ["Mit", "Je\u00b7su", "Schwei\u00df", "und", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schau Vater auf dein heilig Kind!", "tokens": ["Schau", "Va\u00b7ter", "auf", "dein", "hei\u00b7lig", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blick auf die Geisel-Ruthe:", "tokens": ["Blick", "auf", "die", "Gei\u00b7sel\u00b7Ru\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schau seinen Tod! die\u00df halt ich dir", "tokens": ["Schau", "sei\u00b7nen", "Tod", "!", "die\u00df", "halt", "ich", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "PDS", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Bu\u00df und wahrem Glauben f\u00fcr;", "tokens": ["Mit", "Bu\u00df", "und", "wah\u00b7rem", "Glau\u00b7ben", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So kan ich allzeit sterben.", "tokens": ["So", "kan", "ich", "all\u00b7zeit", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ist S\u00fcnd und Tod auf mich erbo\u00dft,", "tokens": ["Ist", "S\u00fcnd", "und", "Tod", "auf", "mich", "er\u00b7bo\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und will mir Grauen machen;", "tokens": ["Und", "will", "mir", "Grau\u00b7en", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So komm zu mir mit deinem Trost;", "tokens": ["So", "komm", "zu", "mir", "mit", "dei\u00b7nem", "Trost", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So kan ich ihrer lachen.", "tokens": ["So", "kan", "ich", "ih\u00b7rer", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Fl\u00f6\u00df in mein Herz den Lebens-Saft,", "tokens": ["Fl\u00f6\u00df", "in", "mein", "Herz", "den", "Le\u00b7bens\u00b7Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und gieb mir St\u00e4rkung, Muth und Kraft;", "tokens": ["Und", "gieb", "mir", "St\u00e4r\u00b7kung", ",", "Muth", "und", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So kan ich fr\u00f6lich sterben.", "tokens": ["So", "kan", "ich", "fr\u00f6\u00b7lich", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Herr! schenk mir deine Freudigkeit", "tokens": ["Herr", "!", "schenk", "mir", "dei\u00b7ne", "Freu\u00b7dig\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt mit Lust zu lassen,", "tokens": ["Die", "Welt", "mit", "Lust", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jene s\u00fc\u00dfe Ewigkeit", "tokens": ["Und", "je\u00b7ne", "s\u00fc\u00b7\u00dfe", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Freuden zu erfassen.", "tokens": ["Mit", "Freu\u00b7den", "zu", "er\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gieb mir zu meinem Todes-Schrit,", "tokens": ["Gieb", "mir", "zu", "mei\u00b7nem", "To\u00b7des\u00b7Schrit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Gott! die Glaubens-Fl\u00fcgel mit;", "tokens": ["O", "Gott", "!", "die", "Glau\u00b7bens\u00b7F\u00b7l\u00fc\u00b7gel", "mit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So kan ich seelig sterben.", "tokens": ["So", "kan", "ich", "see\u00b7lig", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df nicht die Lampe des Gebeths", "tokens": ["La\u00df", "nicht", "die", "Lam\u00b7pe", "des", "Ge\u00b7beths"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "ART", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Verl\u00f6schen, noch versiegen:", "tokens": ["Ver\u00b7l\u00f6\u00b7schen", ",", "noch", "ver\u00b7sie\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich die\u00df einge, bitt ich stets,", "tokens": ["La\u00df", "mich", "die\u00df", "ein\u00b7ge", ",", "bitt", "ich", "stets", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu eigen \u00fcberkriegen;", "tokens": ["Zu", "ei\u00b7gen", "\u00fc\u00b7ber\u00b7krie\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So ruf ich, wenn mein Faden rei\u00dft:", "tokens": ["So", "ruf", "ich", ",", "wenn", "mein", "Fa\u00b7den", "rei\u00dft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir Herr! befehl ich meinen Geist;", "tokens": ["Dir", "Herr", "!", "be\u00b7fehl", "ich", "mei\u00b7nen", "Geist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So kan ich seelig sterben.", "tokens": ["So", "kan", "ich", "see\u00b7lig", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drum Herr! bereite meine Seel,", "tokens": ["Drum", "Herr", "!", "be\u00b7rei\u00b7te", "mei\u00b7ne", "Seel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sie kan allzeit sterben:", "tokens": ["Das", "sie", "kan", "all\u00b7zeit", "ster\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und soll denn meine Leibes-H\u00f6hl,", "tokens": ["Und", "soll", "denn", "mei\u00b7ne", "Lei\u00b7bes\u00b7H\u00f6hl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie ein Baum verderben;", "tokens": ["Als", "wie", "ein", "Baum", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So mache mich darzu bereit;", "tokens": ["So", "ma\u00b7che", "mich", "dar\u00b7zu", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kan ich in die Ewigkeit", "tokens": ["So", "kan", "ich", "in", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu jeder Stunde gehen.", "tokens": ["Zu", "je\u00b7der", "Stun\u00b7de", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}