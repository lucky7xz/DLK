{"dta.poem.14890": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderliche Prinzessin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und es hebt die ewig Sch\u00f6ne,", "tokens": ["Und", "es", "hebt", "die", "e\u00b7wig", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da der Morgen herrlich schiene,", "tokens": ["Da", "der", "Mor\u00b7gen", "herr\u00b7lich", "schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Augen gro\u00dfe Thr\u00e4nen,", "tokens": ["In", "den", "Au\u00b7gen", "gro\u00b7\u00dfe", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell die jugendlichen Glieder.", "tokens": ["Hell", "die", "ju\u00b7gend\u00b7li\u00b7chen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201ewie so anders war es damals,", "tokens": ["\u201e", "wie", "so", "an\u00b7ders", "war", "es", "da\u00b7mals", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da mich, br\u00e4utlich Ausgeschm\u00fcckte,", "tokens": ["Da", "mich", ",", "br\u00e4ut\u00b7lich", "Aus\u00b7ge\u00b7schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus dem heimathlichen Garten", "tokens": ["Aus", "dem", "hei\u00b7math\u00b7li\u00b7chen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.8": {"text": "Hier herab der Vater schickte!", "tokens": ["Hier", "her\u00b7ab", "der", "Va\u00b7ter", "schick\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie die Erde frisch und jung noch,", "tokens": ["Wie", "die", "Er\u00b7de", "frisch", "und", "jung", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Von Ges\u00e4ngen rings erklingend,", "tokens": ["Von", "Ge\u00b7s\u00e4n\u00b7gen", "rings", "er\u00b7klin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Schauernd in Erinnerungen,", "tokens": ["Schau\u00b7ernd", "in", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Helle in das Herz mir blickte,", "tokens": ["Hel\u00b7le", "in", "das", "Herz", "mir", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df ich, schamhaft mich verh\u00fcllend,", "tokens": ["Da\u00df", "ich", ",", "scham\u00b7haft", "mich", "ver\u00b7h\u00fcl\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Meinen Ring, vom Glanz geblendet,", "tokens": ["Mei\u00b7nen", "Ring", ",", "vom", "Glanz", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Schleudert' in die pr\u00e4cht'ge F\u00fclle,", "tokens": ["Schleu\u00b7dert'", "in", "die", "pr\u00e4cht'\u00b7ge", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Als die ew'ge Braut der Erde.", "tokens": ["Als", "die", "ew'\u00b7ge", "Braut", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Wo ist nun die Pracht geblieben,", "tokens": ["Wo", "ist", "nun", "die", "Pracht", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Treuer Ernst im r\u00fcst'gen Treiben,", "tokens": ["Treu\u00b7er", "Ernst", "im", "r\u00fcst'\u00b7gen", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Rechtes Thun und rechtes Lieben", "tokens": ["Rech\u00b7tes", "Thun", "und", "rech\u00b7tes", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und die Sch\u00f6nheit und die Freude?", "tokens": ["Und", "die", "Sch\u00f6n\u00b7heit", "und", "die", "Freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Ach! ringsum die Helden alle,", "tokens": ["Ach", "!", "ring\u00b7sum", "die", "Hel\u00b7den", "al\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Die sonst sch\u00f6n und helle schauten,", "tokens": ["Die", "sonst", "sch\u00f6n", "und", "hel\u00b7le", "schau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Um mich in den lichten Tagen", "tokens": ["Um", "mich", "in", "den", "lich\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Durch die Welt sich fr\u00f6hlich hauten,", "tokens": ["Durch", "die", "Welt", "sich", "fr\u00f6h\u00b7lich", "hau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Strecken steinern nun die Glieder,", "tokens": ["Stre\u00b7cken", "stei\u00b7nern", "nun", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Eingeh\u00fcllt in ihre Fahnen,", "tokens": ["Ein\u00b7ge\u00b7h\u00fcllt", "in", "ih\u00b7re", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Sind seitdem so alt geworden,", "tokens": ["Sind", "seit\u00b7dem", "so", "alt", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ADV", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}