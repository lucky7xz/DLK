{"textgrid.poem.57244": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stirb, prophetischer Greis, stirb! denn dein Palmenzweig", "genre": "verse", "period": "N.A.", "pub_year": 1752, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stirb, prophetischer Greis, stirb! denn dein Palmenzweig", "tokens": ["Stirb", ",", "pro\u00b7phe\u00b7ti\u00b7scher", "Greis", ",", "stirb", "!", "denn", "dein", "Pal\u00b7men\u00b7zweig"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "VVIMP", "$.", "KON", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sprosste lang schon empor; dass sie dir rinne, steht", "tokens": ["Spross\u00b7te", "lang", "schon", "em\u00b7por", ";", "dass", "sie", "dir", "rin\u00b7ne", ",", "steht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ADJD", "ADV", "PTKVZ", "$.", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schon die freudige Thr\u00e4ne", "tokens": ["Schon", "die", "freu\u00b7di\u00b7ge", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In dem Auge der Himlischen.", "tokens": ["In", "dem", "Au\u00b7ge", "der", "Him\u00b7li\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Du verweilst noch? und hast hoch an die Wolken hin", "tokens": ["Du", "ver\u00b7weilst", "noch", "?", "und", "hast", "hoch", "an", "die", "Wol\u00b7ken", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "KON", "VAFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schon dein Denkmal gebaut! Denn die geheiligten,", "tokens": ["Schon", "dein", "Denk\u00b7mal", "ge\u00b7baut", "!", "Denn", "die", "ge\u00b7hei\u00b7lig\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$.", "KON", "ART", "ADJA", "$,"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ernsten, festlichen N\u00e4chte", "tokens": ["Erns\u00b7ten", ",", "fest\u00b7li\u00b7chen", "N\u00e4ch\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wacht der Freigeist mit dir, und f\u00fchlts,", "tokens": ["Wacht", "der", "Frei\u00b7geist", "mit", "dir", ",", "und", "f\u00fchlts", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$,", "KON", "VVFIN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Dass dein tiefer Gesang drohend des Weltgerichts", "tokens": ["Dass", "dein", "tie\u00b7fer", "Ge\u00b7sang", "dro\u00b7hend", "des", "Welt\u00b7ge\u00b7richts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Prophezeyung ihm singt! f\u00fchlts, was die Weisheit will,", "tokens": ["Pro\u00b7phe\u00b7ze\u00b7yung", "ihm", "singt", "!", "f\u00fchlts", ",", "was", "die", "Weis\u00b7heit", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$.", "VVFIN", "$,", "PRELS", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn sie von der Posaune", "tokens": ["Wenn", "sie", "von", "der", "Po\u00b7sau\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spricht, der Todtenerweckerin!", "tokens": ["Spricht", ",", "der", "Tod\u00b7ten\u00b7er\u00b7we\u00b7cke\u00b7rin", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Stirb! du hast mich gelehrt, dass mir der Name Tod,", "tokens": ["Stirb", "!", "du", "hast", "mich", "ge\u00b7lehrt", ",", "dass", "mir", "der", "Na\u00b7me", "Tod", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie der Jubel ert\u00f6nt, den ein Gerechter singt:", "tokens": ["Wie", "der", "Ju\u00b7bel", "er\u00b7t\u00f6nt", ",", "den", "ein", "Ge\u00b7rech\u00b7ter", "singt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber bleibe mein Lehrer,", "tokens": ["A\u00b7ber", "blei\u00b7be", "mein", "Leh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Stirb, und werde mein Genius!", "tokens": ["Stirb", ",", "und", "wer\u00b7de", "mein", "Ge\u00b7nius", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}