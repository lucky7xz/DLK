{"textgrid.poem.60370": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Kruzifix", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4lt der Mensch die Blicke himmelw\u00e4rts", "tokens": ["H\u00e4lt", "der", "Mensch", "die", "Bli\u00b7cke", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Arme liebend ausgebreitet,", "tokens": ["Und", "die", "Ar\u00b7me", "lie\u00b7bend", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Um die Welt zu dr\u00fccken an sein Herz,", "tokens": ["Um", "die", "Welt", "zu", "dr\u00fc\u00b7cken", "an", "sein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hat er sich zur Kreuzigung bereitet.", "tokens": ["Hat", "er", "sich", "zur", "Kreu\u00b7zi\u00b7gung", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Solche Lieb ist selten auf der Erde;", "tokens": ["Sol\u00b7che", "Lieb", "ist", "sel\u00b7ten", "auf", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ihr Bild die Welt nicht ganz verl\u00e4\u00dft,", "tokens": ["Da\u00df", "ihr", "Bild", "die", "Welt", "nicht", "ganz", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hielt am Kreuz die Menschheit eilig fest,", "tokens": ["Hielt", "am", "Kreuz", "die", "Menschheit", "ei\u00b7lig", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Jesus, deine liebende Geb\u00e4rde!", "tokens": ["Je\u00b7sus", ",", "dei\u00b7ne", "lie\u00b7ben\u00b7de", "Ge\u00b7b\u00e4r\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}}}}}