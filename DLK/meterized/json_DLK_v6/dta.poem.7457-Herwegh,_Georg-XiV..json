{"dta.poem.7457": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XiV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ein treues Liebchen in den Armen wiegen,", "tokens": ["Ein", "treu\u00b7es", "Lieb\u00b7chen", "in", "den", "Ar\u00b7men", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Statt also mir das Leben zu verd\u00fcstern!", "tokens": ["Statt", "al\u00b7so", "mir", "das", "Le\u00b7ben", "zu", "ver\u00b7d\u00fcs\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Lie\u00df' nur, wie sonst, der Lorber sich erfl\u00fcstern,", "tokens": ["Lie\u00df'", "nur", ",", "wie", "sonst", ",", "der", "Lor\u00b7ber", "sich", "er\u00b7fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "ADV", "$,", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lie\u00df' nur, wie sonst, die Palme sich ersiegen,", "tokens": ["Lie\u00df'", "nur", ",", "wie", "sonst", ",", "die", "Pal\u00b7me", "sich", "er\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "ADV", "$,", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Musenpferd mu\u00df jetzt zum Ziele fliegen", "tokens": ["Das", "Mu\u00b7sen\u00b7pferd", "mu\u00df", "jetzt", "zum", "Zie\u00b7le", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit wildrem Hufschlag, flammenspr\u00fchnden N\u00fcstern.", "tokens": ["Mit", "wild\u00b7rem", "Huf\u00b7schlag", ",", "flam\u00b7men\u00b7spr\u00fchn\u00b7den", "N\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die gro\u00dfe Zeit zertr\u00fcmmerte die Fl\u00f6te,", "tokens": ["Die", "gro\u00b7\u00dfe", "Zeit", "zer\u00b7tr\u00fcm\u00b7mer\u00b7te", "die", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie braucht Posaunen und den tiefsten Basso,", "tokens": ["Sie", "braucht", "Po\u00b7sau\u00b7nen", "und", "den", "tiefs\u00b7ten", "Bas\u00b7so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schwarze Nacht statt milder Abendr\u00f6te.", "tokens": ["Und", "schwar\u00b7ze", "Nacht", "statt", "mil\u00b7der", "A\u00b7bend\u00b7r\u00f6\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Losung ist nun Dante, und nicht Tasso.", "tokens": ["Die", "Lo\u00b7sung", "ist", "nun", "Dan\u00b7te", ",", "und", "nicht", "Tas\u00b7so", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NE", "$,", "KON", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sollen uns noch Schiller oder G\u00f6the?", "tokens": ["Was", "sol\u00b7len", "uns", "noch", "Schil\u00b7ler", "o\u00b7der", "G\u00f6\u00b7the", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was soll uns gar der Pascha Semilasso?", "tokens": ["Was", "soll", "uns", "gar", "der", "Pa\u00b7scha", "Se\u00b7mi\u00b7las\u00b7so", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "NE", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}