{"textgrid.poem.66689": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sterne glitzern und die Blumen saugen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sterne glitzern und die Blumen saugen", "tokens": ["Die", "Ster\u00b7ne", "glit\u00b7zern", "und", "die", "Blu\u00b7men", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den lauen Balsam dieser Julinacht,", "tokens": ["Den", "lau\u00b7en", "Bal\u00b7sam", "die\u00b7ser", "Ju\u00b7li\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich \u00f6ffne weit die welterf\u00fcllten Augen", "tokens": ["Ich", "\u00f6ff\u00b7ne", "weit", "die", "wel\u00b7ter\u00b7f\u00fcll\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und halte wunderstille Sternenwacht.", "tokens": ["Und", "hal\u00b7te", "wun\u00b7der\u00b7stil\u00b7le", "Ster\u00b7nen\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Blumen duften und die Blicke saugen", "tokens": ["Die", "Blu\u00b7men", "duf\u00b7ten", "und", "die", "Bli\u00b7cke", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus blauen Baldachinen Schauensmacht:", "tokens": ["Aus", "blau\u00b7en", "Bal\u00b7da\u00b7chi\u00b7nen", "Schau\u00b7ens\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit goldnen Himmelsblumen hold im Bunde", "tokens": ["Mit", "gold\u00b7nen", "Him\u00b7mels\u00b7blu\u00b7men", "hold", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zittr' ich im milchig hellen \u00c4thergrunde.", "tokens": ["Zit\u00b7tr'", "ich", "im", "mil\u00b7chig", "hel\u00b7len", "\u00c4t\u00b7her\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Und dieser sausend sternums\u00e4ten Erden", "tokens": ["Und", "die\u00b7ser", "sau\u00b7send", "ster\u00b7num\u00b7s\u00e4\u00b7ten", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zeichn' ich der Seele lichte Lettern hin:", "tokens": ["Zeichn'", "ich", "der", "See\u00b7le", "lich\u00b7te", "Let\u00b7tern", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann kein d\u00fcrftiger Knecht des Daseins werden,", "tokens": ["Ich", "kann", "kein", "d\u00fcrf\u00b7ti\u00b7ger", "Knecht", "des", "Da\u00b7seins", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Weil ich in Ewigkeiten web' und bin.", "tokens": ["Weil", "ich", "in", "E\u00b7wig\u00b7kei\u00b7ten", "web'", "und", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mag das Geschick Sich, wie \u00bbEs\u00ab will, geberden,", "tokens": ["Mag", "das", "Ge\u00b7schick", "Sich", ",", "wie", "\u00bb", "Es", "\u00ab", "will", ",", "ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "$,", "PWAV", "$(", "PPER", "$(", "VMFIN", "$,", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Hoch thront Unendlichkeit \u2013 Befreierin,", "tokens": ["Hoch", "thront", "Un\u00b7end\u00b7lich\u00b7keit", "\u2013", "Be\u00b7frei\u00b7e\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "$(", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Mit schicksallosen, zeiterhabnen Z\u00fcgen,", "tokens": ["Mit", "schick\u00b7sal\u00b7lo\u00b7sen", ",", "zei\u00b7ter\u00b7hab\u00b7nen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die nie sich wandeln und die niemals l\u00fcgen.", "tokens": ["Die", "nie", "sich", "wan\u00b7deln", "und", "die", "nie\u00b7mals", "l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVFIN", "KON", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Lebensangst, du schlotternder Philister,", "tokens": ["O", "Le\u00b7ben\u00b7sangst", ",", "du", "schlot\u00b7tern\u00b7der", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschlichen fast in diese feine Haut,", "tokens": ["Ge\u00b7schli\u00b7chen", "fast", "in", "die\u00b7se", "fei\u00b7ne", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verbrenne nun mit kl\u00e4glichem Geknister", "tokens": ["Ver\u00b7bren\u00b7ne", "nun", "mit", "kl\u00e4g\u00b7li\u00b7chem", "Ge\u00b7knis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Strahlenherde ganz, den ich geschaut!", "tokens": ["Am", "Strah\u00b7len\u00b7her\u00b7de", "ganz", ",", "den", "ich", "ge\u00b7schaut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was ficht mich an Frau Missis und Herr Mister,", "tokens": ["Was", "ficht", "mich", "an", "Frau", "Mis\u00b7sis", "und", "Herr", "Mis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "NN", "NE", "KON", "NN", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die mit gedr\u00fccktem Wirbelglied gebaut?", "tokens": ["Die", "mit", "ge\u00b7dr\u00fcck\u00b7tem", "Wir\u00b7bel\u00b7glied", "ge\u00b7baut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich will mich nicht den Kleinen \u00fcberheben,", "tokens": ["Ich", "will", "mich", "nicht", "den", "Klei\u00b7nen", "\u00fc\u00b7ber\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch \u00fcber dem Gemeinen will ich schweben.", "tokens": ["Doch", "\u00fc\u00b7ber", "dem", "Ge\u00b7mei\u00b7nen", "will", "ich", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sternfl\u00fcstern und geheimnisvoll Gefunkel!", "tokens": ["Stern\u00b7fl\u00fcs\u00b7tern", "und", "ge\u00b7heim\u00b7nis\u00b7voll", "Ge\u00b7fun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bekenntnis flutet durch den blauen Dom.", "tokens": ["Be\u00b7kennt\u00b7nis", "flu\u00b7tet", "durch", "den", "blau\u00b7en", "Dom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben ist ein lichtdurchrieselt Dunkel,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "licht\u00b7durc\u00b7hrie\u00b7selt", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein unergr\u00fcndlich ewiggleicher Strom.", "tokens": ["Ein", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "e\u00b7wig\u00b7glei\u00b7cher", "Strom", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was spinnt Urahne Kraft auf ihrer Kunkel?", "tokens": ["Was", "spinnt", "Ur\u00b7ah\u00b7ne", "Kraft", "auf", "ih\u00b7rer", "Kun\u00b7kel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kein Bruno wei\u00df es und kein Papst in Rom \u2013", "tokens": ["Kein", "Bru\u00b7no", "wei\u00df", "es", "und", "kein", "Papst", "in", "Rom", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "KON", "PIAT", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kein freier Geist, kein formgebundner Glaube", "tokens": ["Kein", "frei\u00b7er", "Geist", ",", "kein", "form\u00b7ge\u00b7bund\u00b7ner", "Glau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Begreift den Sinn des Adlers und der Taube.", "tokens": ["Be\u00b7greift", "den", "Sinn", "des", "Ad\u00b7lers", "und", "der", "Tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bball-einig \u2013 einig \u2013 einig\u00ab, h\u00f6r' ich rauschen,", "tokens": ["\u00bb", "all\u00b7ei\u00b7nig", "\u2013", "ei\u00b7nig", "\u2013", "ei\u00b7nig", "\u00ab", ",", "h\u00f6r'", "ich", "rau\u00b7schen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "ADJD", "$(", "ADJD", "$(", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbsei ruhig, Mensch, du Spiegel der Natur!", "tokens": ["\u00bb", "sei", "ru\u00b7hig", ",", "Mensch", ",", "du", "Spie\u00b7gel", "der", "Na\u00b7tur", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$,", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin ein Lied, dem Narr und Weiser lauschen,", "tokens": ["Ich", "bin", "ein", "Lied", ",", "dem", "Narr", "und", "Wei\u00b7ser", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In R\u00e4tselsang verzittert meine Spur.", "tokens": ["In", "R\u00e4t\u00b7sel\u00b7sang", "ver\u00b7zit\u00b7tert", "mei\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was frommt's, mit falschem Tand sich aufzubauschen?", "tokens": ["Was", "frommt's", ",", "mit", "fal\u00b7schem", "Tand", "sich", "auf\u00b7zu\u00b7bau\u00b7schen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sei Kind, such Blumen auf der Sternenflur!", "tokens": ["Sei", "Kind", ",", "such", "Blu\u00b7men", "auf", "der", "Ster\u00b7nen\u00b7flur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kind Genius \u2013 dir tut die Welt kein Schaden:", "tokens": ["Kind", "Ge\u00b7nius", "\u2013", "dir", "tut", "die", "Welt", "kein", "Scha\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Dein ist die Unschuld und die Kraft in Gnaden.\u00ab", "tokens": ["Dein", "ist", "die", "Un\u00b7schuld", "und", "die", "Kraft", "in", "Gna\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "KON", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}