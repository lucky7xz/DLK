{"textgrid.poem.32826": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kleine stolze Amazone,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kleine stolze Amazone,", "tokens": ["Du", "klei\u00b7ne", "stol\u00b7ze", "A\u00b7ma\u00b7zo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinem gro\u00dfen Federhut!", "tokens": ["Mit", "dei\u00b7nem", "gro\u00b7\u00dfen", "Fe\u00b7der\u00b7hut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sehr ich meines Lebens schone,", "tokens": ["So", "sehr", "ich", "mei\u00b7nes", "Le\u00b7bens", "scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hab ich doch bey dir noch Muth.", "tokens": ["So", "hab", "ich", "doch", "bey", "dir", "noch", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du sollst mich auf dem Kampfplatz finden,", "tokens": ["Du", "sollst", "mich", "auf", "dem", "Kampf\u00b7platz", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bald dein Mund, \u00bbzum Waffen!\u00ab spricht:", "tokens": ["So", "bald", "dein", "Mund", ",", "\u00bb", "zum", "Waf\u00b7fen", "!", "\u00ab", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,", "$(", "APPRART", "NN", "$.", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch la\u00df die Augen dir verbinden,", "tokens": ["Doch", "la\u00df", "die", "Au\u00b7gen", "dir", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinen Augen fecht ich nicht.", "tokens": ["Mit", "dei\u00b7nen", "Au\u00b7gen", "fecht", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}