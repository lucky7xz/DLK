{"textgrid.poem.55197": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Stammbuchsweihe", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Muntre G\u00e4rten lieb ich mir,", "tokens": ["Mun\u00b7tre", "G\u00e4r\u00b7ten", "lieb", "ich", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Viele Blumen drinne,", "tokens": ["Vie\u00b7le", "Blu\u00b7men", "drin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und du hast so einen hier,", "tokens": ["Und", "du", "hast", "so", "ei\u00b7nen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Merk ich wohl, im Sinne.", "tokens": ["Merk", "ich", "wohl", ",", "im", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "M\u00f6gen W\u00fcnsche f\u00fcr dein Gl\u00fcck", "tokens": ["M\u00f6\u00b7gen", "W\u00fcn\u00b7sche", "f\u00fcr", "dein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausendfach erscheinen;", "tokens": ["Tau\u00b7send\u00b7fach", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gr\u00fc\u00dfe sie mit heitrem Blick,", "tokens": ["Gr\u00fc\u00b7\u00dfe", "sie", "mit", "heit\u00b7rem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und voran die meinen.", "tokens": ["Und", "vo\u00b7ran", "die", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}