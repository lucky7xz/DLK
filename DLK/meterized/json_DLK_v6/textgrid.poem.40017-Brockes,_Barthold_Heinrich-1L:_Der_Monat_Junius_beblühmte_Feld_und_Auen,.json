{"textgrid.poem.40017": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Monat Junius bebl\u00fchmte Feld und Auen,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Monat Junius bebl\u00fchmte Feld und Auen,", "tokens": ["Der", "Mo\u00b7nat", "Ju\u00b7nius", "be\u00b7bl\u00fchm\u00b7te", "Feld", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Als ich, die Wunder-Pracht der Bluhmen zu beschauen,", "tokens": ["Als", "ich", ",", "die", "Wun\u00b7der\u00b7Pracht", "der", "Bluh\u00b7men", "zu", "be\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Garten gieng. Mein \u00e4lt'ster Sohn lief mit;", "tokens": ["Im", "Gar\u00b7ten", "gieng", ".", "Mein", "\u00e4lt'\u00b7ster", "Sohn", "lief", "mit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein reger Fu\u00df h\u00fcpft' immer hin und her,", "tokens": ["Sein", "re\u00b7ger", "Fu\u00df", "h\u00fcpft'", "im\u00b7mer", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit fr\u00f6hlichem, fast nimmer stillem Schritt.", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7chem", ",", "fast", "nim\u00b7mer", "stil\u00b7lem", "Schritt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als er nun ungefehr", "tokens": ["Als", "er", "nun", "un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein g\u00fcld'nes K\u00e4ferchen auf einer Rose fand;", "tokens": ["Ein", "g\u00fcld'\u00b7nes", "K\u00e4\u00b7fer\u00b7chen", "auf", "ei\u00b7ner", "Ro\u00b7se", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Ergriff er es mit seiner kleinen Hand,", "tokens": ["Er\u00b7griff", "er", "es", "mit", "sei\u00b7ner", "klei\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und kam darauf, in vollen Spr\u00fcngen,", "tokens": ["Und", "kam", "da\u00b7rauf", ",", "in", "vol\u00b7len", "Spr\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mir den gefund'nen Schatz zu bringen.", "tokens": ["Mir", "den", "ge\u00b7fun\u00b7d'\u00b7nen", "Schatz", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ich lobte seinen Fund, und nahm ihn l\u00e4chelnd hin,", "tokens": ["Ich", "lob\u00b7te", "sei\u00b7nen", "Fund", ",", "und", "nahm", "ihn", "l\u00e4\u00b7chelnd", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Betrachtete, mit fast erschrock'nem Sinn,", "tokens": ["Be\u00b7trach\u00b7te\u00b7te", ",", "mit", "fast", "er\u00b7schrock'\u00b7nem", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die Sch\u00f6nheit, Farben und Figur,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "Far\u00b7ben", "und", "Fi\u00b7gur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit welcher ihn die bildende Natur", "tokens": ["Mit", "wel\u00b7cher", "ihn", "die", "bil\u00b7den\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Begabt und ausgeziert.", "tokens": ["Be\u00b7gabt", "und", "aus\u00b7ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Durch's Auge ward mein Hertz ger\u00fchrt,", "tokens": ["Durch's", "Au\u00b7ge", "ward", "mein", "Hertz", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Als ich, mit h\u00f6chster Lust, erblickte,", "tokens": ["Als", "ich", ",", "mit", "h\u00f6chs\u00b7ter", "Lust", ",", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wie ihm Smaragd und Gold den glatten R\u00fccken schm\u00fcckte;", "tokens": ["Wie", "ihm", "Sma\u00b7ragd", "und", "Gold", "den", "glat\u00b7ten", "R\u00fc\u00b7cken", "schm\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Und ich bewunderte sein wandelbares Gr\u00fcn,", "tokens": ["Und", "ich", "be\u00b7wun\u00b7der\u00b7te", "sein", "wan\u00b7del\u00b7ba\u00b7res", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das bald wie Gold, bald wie Rubin,", "tokens": ["Das", "bald", "wie", "Gold", ",", "bald", "wie", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "KOKOM", "NN", "$,", "ADV", "KOKOM", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Und bald aufs neu Smaragden, schien,", "tokens": ["Und", "bald", "aufs", "neu", "Sma\u00b7rag\u00b7den", ",", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Nachdem der F\u00fcrst des Lichts auf seine Theilchen strahlte,", "tokens": ["Nach\u00b7dem", "der", "F\u00fcrst", "des", "Lichts", "auf", "sei\u00b7ne", "Theil\u00b7chen", "strahl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und die verschied'ne Fl\u00e4che malte.", "tokens": ["Und", "die", "ver\u00b7schie\u00b7d'\u00b7ne", "Fl\u00e4\u00b7che", "mal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Als ich mich lange nun an seinem Glantz ergetzet,", "tokens": ["Als", "ich", "mich", "lan\u00b7ge", "nun", "an", "sei\u00b7nem", "Glantz", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und diese Sch\u00f6nheit hoch gesch\u00e4tzet;", "tokens": ["Und", "die\u00b7se", "Sch\u00f6n\u00b7heit", "hoch", "ge\u00b7sch\u00e4t\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Versp\u00fcret' ich, wie die ver\u00e4nderliche Pracht", "tokens": ["Ver\u00b7sp\u00fc\u00b7ret'", "ich", ",", "wie", "die", "ver\u00b7\u00e4n\u00b7der\u00b7li\u00b7che", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich allgemach auf die Gedancken bracht':", "tokens": ["Mich", "all\u00b7ge\u00b7mach", "auf", "die", "Ge\u00b7dan\u00b7cken", "bracht'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was sind die Farben doch? Nichts, als ein blosses Nichts.", "tokens": ["Was", "sind", "die", "Far\u00b7ben", "doch", "?", "Nichts", ",", "als", "ein", "blos\u00b7ses", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "$.", "PIS", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Denn, wenn der Schein des all-erfreu'nden Lichts", "tokens": ["Denn", ",", "wenn", "der", "Schein", "des", "all\u00b7er\u00b7freu'n\u00b7den", "Lichts"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich von uns trennet, schwinden,", "tokens": ["Sich", "von", "uns", "tren\u00b7net", ",", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergehn und sterben sie; man kann nicht einst die Spur", "tokens": ["Ver\u00b7gehn", "und", "ster\u00b7ben", "sie", ";", "man", "kann", "nicht", "einst", "die", "Spur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPER", "$.", "PIS", "VMFIN", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von ihrer Pracht, von ihrem Wesen, finden.", "tokens": ["Von", "ih\u00b7rer", "Pracht", ",", "von", "ih\u00b7rem", "We\u00b7sen", ",", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die\u00df heisst mich weiter gehn, und auch: Was ist die Welt?", "tokens": ["Die\u00df", "heisst", "mich", "wei\u00b7ter", "gehn", ",", "und", "auch", ":", "Was", "ist", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$,", "KON", "ADV", "$.", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was ist das Irdische? Was ist die Creatur?", "tokens": ["Was", "ist", "das", "Ir\u00b7di\u00b7sche", "?", "Was", "ist", "die", "Crea\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Was sind wir selber? fragen;", "tokens": ["Was", "sind", "wir", "sel\u00b7ber", "?", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$.", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Worauf mir Gottes Wort, Witz und Erfahrung sagen:", "tokens": ["Wo\u00b7rauf", "mir", "Got\u00b7tes", "Wort", ",", "Witz", "und", "Er\u00b7fah\u00b7rung", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Farben sind es, was ihr sehet,", "tokens": ["Far\u00b7ben", "sind", "es", ",", "was", "ihr", "se\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6ret, riechet, schmeckt und f\u00fchlt.", "tokens": ["H\u00f6\u00b7ret", ",", "rie\u00b7chet", ",", "schmeckt", "und", "f\u00fchlt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Gott, den Brunn des Lichts,", "tokens": ["Oh\u00b7ne", "Gott", ",", "den", "Brunn", "des", "Lichts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind wir, und ist alles, nichts.", "tokens": ["Sind", "wir", ",", "und", "ist", "al\u00b7les", ",", "nichts", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KON", "VAFIN", "PIS", "$,", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles schwindet und vergehet,", "tokens": ["Al\u00b7les", "schwin\u00b7det", "und", "ver\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was auch noch so herrlich spielt.", "tokens": ["Was", "auch", "noch", "so", "herr\u00b7lich", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da ich dem Knaben nun das W\u00fcrmchen wieder reichte;", "tokens": ["Da", "ich", "dem", "Kna\u00b7ben", "nun", "das", "W\u00fcrm\u00b7chen", "wie\u00b7der", "reich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entflog es ihm, und alle Freude mit.", "tokens": ["Ent\u00b7flog", "es", "ihm", ",", "und", "al\u00b7le", "Freu\u00b7de", "mit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Kummer war, der seinem gleichte;", "tokens": ["Kein", "Kum\u00b7mer", "war", ",", "der", "sei\u00b7nem", "gleich\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PRELS", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es wanckte sein verwirrter Tritt;", "tokens": ["Es", "wanck\u00b7te", "sein", "ver\u00b7wirr\u00b7ter", "Tritt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er fieng erb\u00e4rmlich an zu weinen;", "tokens": ["Er", "fi\u00b7eng", "er\u00b7b\u00e4rm\u00b7lich", "an", "zu", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die kleine Hand rieb die bethr\u00e4nten Augen;", "tokens": ["Die", "klei\u00b7ne", "Hand", "rieb", "die", "be\u00b7thr\u00e4n\u00b7ten", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er \u00e4nderte Geberden und Gestalt,", "tokens": ["Er", "\u00e4n\u00b7der\u00b7te", "Ge\u00b7ber\u00b7den", "und", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und konnt' ihn nichts zu tr\u00f6sten taugen.", "tokens": ["Und", "konnt'", "ihn", "nichts", "zu", "tr\u00f6s\u00b7ten", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wor\u00fcber ich denn hertzlich lachte;", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "ich", "denn", "hertz\u00b7lich", "lach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Doch \u00e4nderte sich die\u00df mein Lachen bald,", "tokens": ["Doch", "\u00e4n\u00b7der\u00b7te", "sich", "die\u00df", "mein", "La\u00b7chen", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PDS", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Als ich auch unser Werck und kindisch Thun bedachte.", "tokens": ["Als", "ich", "auch", "un\u00b7ser", "Werck", "und", "kin\u00b7disch", "Thun", "be\u00b7dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "KON", "ADJD", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Wurm ergetzt ein Kind, ein gelber Koth die Alten;", "tokens": ["Ein", "Wurm", "er\u00b7getzt", "ein", "Kind", ",", "ein", "gel\u00b7ber", "Koth", "die", "Al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man will ihn mit Gewalt erhalten und behalten.", "tokens": ["Man", "will", "ihn", "mit", "Ge\u00b7walt", "er\u00b7hal\u00b7ten", "und", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Kind hat kurtze Lust, der Alte kleine Freude;", "tokens": ["Das", "Kind", "hat", "kurt\u00b7ze", "Lust", ",", "der", "Al\u00b7te", "klei\u00b7ne", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bald nur Wurm und Gold dahin sind, weinen beyde.", "tokens": ["So", "bald", "nur", "Wurm", "und", "Gold", "da\u00b7hin", "sind", ",", "wei\u00b7nen", "bey\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "KON", "NN", "PAV", "VAFIN", "$,", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}