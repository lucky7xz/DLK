{"dta.poem.4101": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der M\u00fcller und der Bach.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Liebe vergeht,", "tokens": ["In", "Lie\u00b7be", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Da welken die Lilien", "tokens": ["Da", "wel\u00b7ken", "die", "Li\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf jedem Beet.", "tokens": ["Auf", "je\u00b7dem", "Beet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Da mu\u00df in die Wolken", "tokens": ["Da", "mu\u00df", "in", "die", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Vollmond gehn,", "tokens": ["Der", "Voll\u00b7mond", "gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Damit seine Thr\u00e4nen", "tokens": ["Da\u00b7mit", "sei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die Menschen nicht sehn.", "tokens": ["Die", "Men\u00b7schen", "nicht", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Da halten die Englein", "tokens": ["Da", "hal\u00b7ten", "die", "En\u00b7glein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Augen sich zu,", "tokens": ["Die", "Au\u00b7gen", "sich", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und schluchzen und singen", "tokens": ["Und", "schluch\u00b7zen", "und", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Seele zu Ruh'.", "tokens": ["Die", "See\u00b7le", "zu", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Und wenn sich die Liebe", "tokens": ["Und", "wenn", "sich", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dem Schmerz entringt,", "tokens": ["Dem", "Schmerz", "ent\u00b7ringt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Sternlein, ein neues,", "tokens": ["Ein", "Stern\u00b7lein", ",", "ein", "neu\u00b7es", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Am Himmel erblinkt.", "tokens": ["Am", "Him\u00b7mel", "er\u00b7blinkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Da springen drei Rosen,", "tokens": ["Da", "sprin\u00b7gen", "drei", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Halb roth, halb wei\u00df,", "tokens": ["Halb", "roth", ",", "halb", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die welken nicht wieder,", "tokens": ["Die", "wel\u00b7ken", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Aus Dornenreis.", "tokens": ["Aus", "Dor\u00b7nen\u00b7reis", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und die Engelein schneiden", "tokens": ["Und", "die", "En\u00b7ge\u00b7lein", "schnei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Fl\u00fcgel sich ab,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "sich", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und gehn alle Morgen", "tokens": ["Und", "gehn", "al\u00b7le", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zur Erde herab.", "tokens": ["Zur", "Er\u00b7de", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Ach, B\u00e4chlein, liebes B\u00e4chlein,", "tokens": ["Ach", ",", "B\u00e4ch\u00b7lein", ",", "lie\u00b7bes", "B\u00e4ch\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du meinst es so gut:", "tokens": ["Du", "meinst", "es", "so", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ach, B\u00e4chlein, aber wei\u00dft du", "tokens": ["Ach", ",", "B\u00e4ch\u00b7lein", ",", "a\u00b7ber", "wei\u00dft", "du"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wie Liebe thut?", "tokens": ["Wie", "Lie\u00b7be", "thut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Ach, unten, da unten,", "tokens": ["Ach", ",", "un\u00b7ten", ",", "da", "un\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "$,", "KOUS", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die k\u00fchle Ruh'!", "tokens": ["Die", "k\u00fch\u00b7le", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ach, B\u00e4chlein, liebes B\u00e4chlein,", "tokens": ["Ach", ",", "B\u00e4ch\u00b7lein", ",", "lie\u00b7bes", "B\u00e4ch\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So singe nur zu.", "tokens": ["So", "sin\u00b7ge", "nur", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}