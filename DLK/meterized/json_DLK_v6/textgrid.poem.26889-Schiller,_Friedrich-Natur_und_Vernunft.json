{"textgrid.poem.26889": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Natur und Vernunft", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4rt ihr, Schw\u00e4rmer, imstande, die Ideale zu fassen,", "tokens": ["W\u00e4rt", "ihr", ",", "Schw\u00e4r\u00b7mer", ",", "ims\u00b7tan\u00b7de", ",", "die", "I\u00b7dea\u00b7le", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "ADJA", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "O so verehrtet ihr auch, wie sichs geb\u00fchrt, die Natur.", "tokens": ["O", "so", "ver\u00b7ehr\u00b7tet", "ihr", "auch", ",", "wie", "sichs", "ge\u00b7b\u00fchrt", ",", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PIS", "VVPP", "$,", "ART", "NN", "$."], "meter": "+--+------+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "W\u00e4rt ihr, Philister, imstand, die Natur im Gro\u00dfen zu sehen,", "tokens": ["W\u00e4rt", "ihr", ",", "Phi\u00b7lis\u00b7ter", ",", "ims\u00b7tand", ",", "die", "Na\u00b7tur", "im", "Gro\u00b7\u00dfen", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Sicher f\u00fchrte sie selbst euch zu Ideen empor.", "tokens": ["Si\u00b7cher", "f\u00fchr\u00b7te", "sie", "selbst", "euch", "zu", "I\u00b7deen", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}}}}