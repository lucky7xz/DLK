{"textgrid.poem.41754": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O la\u00dft mich einmal tr\u00e4umen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O la\u00dft mich einmal tr\u00e4umen", "tokens": ["O", "la\u00dft", "mich", "ein\u00b7mal", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im wonniglichen Mai,", "tokens": ["Im", "won\u00b7nig\u00b7li\u00b7chen", "Mai", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich ruhn unter Bl\u00fctenb\u00e4umen,", "tokens": ["Mich", "ruhn", "un\u00b7ter", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh Mai und Lenz vorbei!", "tokens": ["Eh", "Mai", "und", "Lenz", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Wasserfalles Rauschen,", "tokens": ["Des", "Was\u00b7ser\u00b7fal\u00b7les", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Nachtigallen Gesang \u2013", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len", "Ge\u00b7sang", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "La\u00dft mich ihn stille belauschen,", "tokens": ["La\u00dft", "mich", "ihn", "stil\u00b7le", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Er t\u00f6nt ja so nicht lang.", "tokens": ["Er", "t\u00f6nt", "ja", "so", "nicht", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00dft unter gr\u00fcnen Bl\u00e4ttern", "tokens": ["La\u00dft", "un\u00b7ter", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir suchen ein Lager auf,", "tokens": ["Mir", "su\u00b7chen", "ein", "La\u00b7ger", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn wirbelnde Lerchen schmettern", "tokens": ["Wenn", "wir\u00b7beln\u00b7de", "Ler\u00b7chen", "schmet\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr Lied zum Himmel hinauf.", "tokens": ["Ihr", "Lied", "zum", "Him\u00b7mel", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und wenn die Wachteln schlagen", "tokens": ["Und", "wenn", "die", "Wach\u00b7teln", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im hohen bl\u00fchenden Korn,", "tokens": ["Im", "ho\u00b7hen", "bl\u00fc\u00b7hen\u00b7den", "Korn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Indes in duftigen Hagen", "tokens": ["In\u00b7des", "in", "duf\u00b7ti\u00b7gen", "Ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erkeimet jeder Dorn \u2013", "tokens": ["Er\u00b7kei\u00b7met", "je\u00b7der", "Dorn", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da sollt Ihr mich nicht qu\u00e4len", "tokens": ["Da", "sollt", "Ihr", "mich", "nicht", "qu\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Fragen her und hin:", "tokens": ["Mit", "Fra\u00b7gen", "her", "und", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwie k\u00f6nnen wir auf Dich z\u00e4hlen,", "tokens": ["\u00bb", "wie", "k\u00f6n\u00b7nen", "wir", "auf", "Dich", "z\u00e4h\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Treulose K\u00e4mpferin.", "tokens": ["Treu\u00b7lo\u00b7se", "K\u00e4mp\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Wenn Du von Tr\u00e4umen befangen", "tokens": ["Wenn", "Du", "von", "Tr\u00e4u\u00b7men", "be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Unter den Bl\u00fcten liegst,", "tokens": ["Un\u00b7ter", "den", "Bl\u00fc\u00b7ten", "liegst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wo Schmetterlinge prangen", "tokens": ["Wo", "Schmet\u00b7ter\u00b7lin\u00b7ge", "pran\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im s\u00fc\u00dfen Taumel Dich wiegst?", "tokens": ["Im", "s\u00fc\u00b7\u00dfen", "Tau\u00b7mel", "Dich", "wiegst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Wenn Du im Mondenglanze", "tokens": ["Wenn", "Du", "im", "Mon\u00b7den\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als n\u00e4chtliche Schw\u00e4rmerin", "tokens": ["Als", "n\u00e4cht\u00b7li\u00b7che", "Schw\u00e4r\u00b7me\u00b7rin"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dich schm\u00fcckst mit bl\u00fchendem Kranze,", "tokens": ["Dich", "schm\u00fcckst", "mit", "bl\u00fc\u00b7hen\u00b7dem", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dess' Duft bet\u00e4ubt den Sinn?", "tokens": ["Dess'", "Duft", "be\u00b7t\u00e4ubt", "den", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Jetzt schlummerst Du selbst im Wachen,", "tokens": ["Jetzt", "schlum\u00b7merst", "Du", "selbst", "im", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einst wachtest im Schlummer Du auf", "tokens": ["Einst", "wach\u00b7test", "im", "Schlum\u00b7mer", "Du", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "APPR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und rangst mit den feindlichen Drachen,", "tokens": ["Und", "rangst", "mit", "den", "feind\u00b7li\u00b7chen", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die hemmen der Zeiten Lauf.", "tokens": ["Die", "hem\u00b7men", "der", "Zei\u00b7ten", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Und wolltest k\u00e4mpfen und dienen", "tokens": ["Und", "woll\u00b7test", "k\u00e4mp\u00b7fen", "und", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem Volke bis es frei;", "tokens": ["Dem", "Vol\u00b7ke", "bis", "es", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt lauschst Du summenden Bienen", "tokens": ["Jetzt", "lauschst", "Du", "sum\u00b7men\u00b7den", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als sei aller Kampf vorbei?!\u00ab", "tokens": ["Als", "sei", "al\u00b7ler", "Kampf", "vor\u00b7bei", "?!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VAFIN", "PIAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "H\u00f6rt auf, mich so zu qu\u00e4len", "tokens": ["H\u00f6rt", "auf", ",", "mich", "so", "zu", "qu\u00e4\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Fragen hin und her;", "tokens": ["Mit", "Fra\u00b7gen", "hin", "und", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich werd' im Kampfe nicht fehlen,", "tokens": ["Ich", "werd'", "im", "Kamp\u00b7fe", "nicht", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Schm\u00fcck ihn mit bl\u00fchenden Sprossen,", "tokens": ["Schm\u00fcck", "ihn", "mit", "bl\u00fc\u00b7hen\u00b7den", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mit Halmen schwer und voll,", "tokens": ["Mit", "Hal\u00b7men", "schwer", "und", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und glaubt mir, Kampfgenossen,", "tokens": ["Und", "glaubt", "mir", ",", "Kampf\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er noch treffen soll!", "tokens": ["Da\u00df", "er", "noch", "tref\u00b7fen", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Beim fr\u00f6hlichen Kr\u00e4nzebinden", "tokens": ["Beim", "fr\u00f6h\u00b7li\u00b7chen", "Kr\u00e4n\u00b7ze\u00b7bin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bleibt jung und frisch mein Mut,", "tokens": ["Bleibt", "jung", "und", "frisch", "mein", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Starren zu \u00fcberwinden", "tokens": ["Die", "Star\u00b7ren", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Lenzbegeisterungsglut.", "tokens": ["Mit", "Lenz\u00b7be\u00b7geis\u00b7te\u00b7rungs\u00b7glut", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Die Halme sollen's erkl\u00e4ren:", "tokens": ["Die", "Hal\u00b7me", "sol\u00b7len's", "er\u00b7kl\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Speer ist den Armen geweiht,", "tokens": ["Mein", "Speer", "ist", "den", "Ar\u00b7men", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nicht l\u00e4nger soll es w\u00e4hren,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "soll", "es", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um's t\u00e4gliche Brot ihr Leid.", "tokens": ["Um's", "t\u00e4g\u00b7li\u00b7che", "Brot", "ihr", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Doch selbst in blutigen Kriegen", "tokens": ["Doch", "selbst", "in", "blu\u00b7ti\u00b7gen", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Man Waffenstillstand h\u00e4lt,", "tokens": ["Man", "Waf\u00b7fen\u00b7still\u00b7stand", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn es sich just mu\u00df f\u00fcgen,", "tokens": ["Wenn", "es", "sich", "just", "mu\u00df", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df drein ein Festtag f\u00e4llt.", "tokens": ["Da\u00df", "drein", "ein", "Fest\u00b7tag", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Den fromme Gl\u00e4ubige ehren", "tokens": ["Den", "from\u00b7me", "Gl\u00e4u\u00b7bi\u00b7ge", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So wollen im Lenz wir thun,", "tokens": ["So", "wol\u00b7len", "im", "Lenz", "wir", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Woll'n seine Feier verkl\u00e4ren", "tokens": ["Woll'n", "sei\u00b7ne", "Fei\u00b7er", "ver\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und von den Waffen ruhn.", "tokens": ["Und", "von", "den", "Waf\u00b7fen", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und seid Ihr's nicht zufrieden,", "tokens": ["Und", "seid", "Ih\u00b7r's", "nicht", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seid Ihr zu strenge und kalt,", "tokens": ["Seid", "Ihr", "zu", "stren\u00b7ge", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PTKZU", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So ist doch mir beschieden", "tokens": ["So", "ist", "doch", "mir", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die fromme Feier im Wald.", "tokens": ["Die", "from\u00b7me", "Fei\u00b7er", "im", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.17": {"line.1": {"text": "So la\u00dft doch den Poeten", "tokens": ["So", "la\u00dft", "doch", "den", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Lenze werden zum Kind,", "tokens": ["Im", "Len\u00b7ze", "wer\u00b7den", "zum", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit V\u00f6geln und Blumen ihn reden,", "tokens": ["Mit", "V\u00f6\u00b7geln", "und", "Blu\u00b7men", "ihn", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die seine Vertrauten sind.", "tokens": ["Die", "sei\u00b7ne", "Ver\u00b7trau\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.18": {"line.1": {"text": "So la\u00dft mich einmal tr\u00e4umen", "tokens": ["So", "la\u00dft", "mich", "ein\u00b7mal", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im wonniglichen Mai,", "tokens": ["Im", "won\u00b7nig\u00b7li\u00b7chen", "Mai", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich ruhn unter Bl\u00fctenb\u00e4umen,", "tokens": ["Mich", "ruhn", "un\u00b7ter", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh Mai und Lenz vorbei.", "tokens": ["Eh", "Mai", "und", "Lenz", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Und auf h\u00f6rt mich zu qu\u00e4len", "tokens": ["Und", "auf", "h\u00f6rt", "mich", "zu", "qu\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Fragen hin und her \u2013,", "tokens": ["Mit", "Fra\u00b7gen", "hin", "und", "her", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "ADV", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich werd' im Kampfe nicht fehlen,", "tokens": ["Ich", "werd'", "im", "Kamp\u00b7fe", "nicht", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch schm\u00fcck ich meinen Speer!", "tokens": ["Doch", "schm\u00fcck", "ich", "mei\u00b7nen", "Speer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}