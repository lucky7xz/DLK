{"textgrid.poem.61328": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lehr' nur die Jungen weisheitsvoll,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lehr' nur die Jungen weisheitsvoll,", "tokens": ["Lehr'", "nur", "die", "Jun\u00b7gen", "weis\u00b7heits\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wirst ihnen keinen Irrtum sparen;", "tokens": ["Wirst", "ih\u00b7nen", "kei\u00b7nen", "Irr\u00b7tum", "spa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was ihnen gr\u00fcndlich helfen soll,", "tokens": ["Was", "ih\u00b7nen", "gr\u00fcnd\u00b7lich", "hel\u00b7fen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das m\u00fcssen sie eben selbst erfahren.", "tokens": ["Das", "m\u00fcs\u00b7sen", "sie", "e\u00b7ben", "selbst", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}