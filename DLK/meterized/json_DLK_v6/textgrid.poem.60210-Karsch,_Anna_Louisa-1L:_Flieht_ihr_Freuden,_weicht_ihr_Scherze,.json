{"textgrid.poem.60210": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Flieht ihr Freuden, weicht ihr Scherze,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flieht ihr Freuden, weicht ihr Scherze,", "tokens": ["Flieht", "ihr", "Freu\u00b7den", ",", "weicht", "ihr", "Scher\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du Gesellschaft, Saitenspiel und Tanz;", "tokens": ["Du", "Ge\u00b7sell\u00b7schaft", ",", "Sai\u00b7ten\u00b7spiel", "und", "Tanz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nichts erg\u00f6tzt mein traurig Herze,", "tokens": ["Nichts", "er\u00b7g\u00f6tzt", "mein", "trau\u00b7rig", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiche, beste Welt, mit deinem Glanz!", "tokens": ["Wei\u00b7che", ",", "bes\u00b7te", "Welt", ",", "mit", "dei\u00b7nem", "Glanz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ewig will ich klagen", "tokens": ["E\u00b7wig", "will", "ich", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und von meinen Tagen", "tokens": ["Und", "von", "mei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Soll nicht einer aufgeheitert seyn.", "tokens": ["Soll", "nicht", "ei\u00b7ner", "auf\u00b7ge\u00b7hei\u00b7tert", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PIS", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ach ich will f\u00fcr nichts empfinden,", "tokens": ["Ach", "ich", "will", "f\u00fcr", "nichts", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VMFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Als f\u00fcr meine Pein!", "tokens": ["Als", "f\u00fcr", "mei\u00b7ne", "Pein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "In den wildesten Geb\u00fcschen", "tokens": ["In", "den", "wil\u00b7des\u00b7ten", "Ge\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.2": {"text": "Will ich mit verscheuchten Hirschen gehn,", "tokens": ["Will", "ich", "mit", "ver\u00b7scheuch\u00b7ten", "Hir\u00b7schen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und wo giftge Schlangen zischen", "tokens": ["Und", "wo", "gift\u00b7ge", "Schlan\u00b7gen", "zi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJA", "NN", "VVINF"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Will ich stolz den Tod erwartend stehn!", "tokens": ["Will", "ich", "stolz", "den", "Tod", "er\u00b7war\u00b7tend", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Einsam will ich irren", "tokens": ["Ein\u00b7sam", "will", "ich", "ir\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Melancholisch girren", "tokens": ["Me\u00b7lan\u00b7cho\u00b7lisch", "gir\u00b7ren"], "token_info": ["word", "word"], "pos": ["NE", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wie des Turtelt\u00e4ubchens Gatte thut;", "tokens": ["Wie", "des", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chens", "Gat\u00b7te", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Dem der Habicht sein Vergn\u00fcgen", "tokens": ["Dem", "der", "Ha\u00b7bicht", "sein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nahm, mit R\u00e4uberwuth.", "tokens": ["Nahm", ",", "mit", "R\u00e4u\u00b7ber\u00b7wuth", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Da, wo nie ein Thau gefallen,", "tokens": ["Da", ",", "wo", "nie", "ein", "Thau", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo noch nie ein M\u00e4dchen ward gek\u00fc\u00dft.", "tokens": ["Wo", "noch", "nie", "ein", "M\u00e4d\u00b7chen", "ward", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo kein Lied der Nachtigallen,", "tokens": ["Wo", "kein", "Lied", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein Sch\u00e4fer-Rohr zu h\u00f6ren ist,", "tokens": ["Und", "kein", "Sch\u00e4\u00b7fer\u00b7Rohr", "zu", "h\u00f6\u00b7ren", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da, wo mittern\u00e4chtig", "tokens": ["Da", ",", "wo", "mit\u00b7ter\u00b7n\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schwarz und schr\u00f6cklich pr\u00e4chtig", "tokens": ["Schwarz", "und", "schr\u00f6ck\u00b7lich", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nur die Furcht ihr ewig Wohnhaus hat,", "tokens": ["Nur", "die", "Furcht", "ihr", "e\u00b7wig", "Wohn\u00b7haus", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "ADJD", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "In der W\u00fcste will ich taumeln,", "tokens": ["In", "der", "W\u00fcs\u00b7te", "will", "ich", "tau\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Meines Lebens satt.", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "satt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}