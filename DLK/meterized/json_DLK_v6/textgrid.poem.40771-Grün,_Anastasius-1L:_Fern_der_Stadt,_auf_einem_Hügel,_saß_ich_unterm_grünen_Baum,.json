{"textgrid.poem.40771": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fern der Stadt, auf einem H\u00fcgel, sa\u00df ich unterm gr\u00fcnen Baum,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern der Stadt, auf einem H\u00fcgel, sa\u00df ich unterm gr\u00fcnen Baum,", "tokens": ["Fern", "der", "Stadt", ",", "auf", "ei\u00b7nem", "H\u00fc\u00b7gel", ",", "sa\u00df", "ich", "un\u00b7term", "gr\u00fc\u00b7nen", "Baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Der mir s\u00e4uselnd um die Schl\u00e4fen spielte, wie ein Fr\u00fchlingstraum,", "tokens": ["Der", "mir", "s\u00e4u\u00b7selnd", "um", "die", "Schl\u00e4\u00b7fen", "spiel\u00b7te", ",", "wie", "ein", "Fr\u00fch\u00b7lings\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Frei die Blicke lie\u00df ich schweifen \u00fcber Felder, H\u00f6h'n und Wald,", "tokens": ["Frei", "die", "Bli\u00b7cke", "lie\u00df", "ich", "schwei\u00b7fen", "\u00fc\u00b7ber", "Fel\u00b7der", ",", "H\u00f6h'n", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bis die fernen, blauen Berge ihnen h\u00f6hnend riefen: Halt!", "tokens": ["Bis", "die", "fer\u00b7nen", ",", "blau\u00b7en", "Ber\u00b7ge", "ih\u00b7nen", "h\u00f6h\u00b7nend", "rie\u00b7fen", ":", "Halt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$.", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Sieh, da nahmen die Gedanken ihren leichten Wanderstab,", "tokens": ["Sieh", ",", "da", "nah\u00b7men", "die", "Ge\u00b7dan\u00b7ken", "ih\u00b7ren", "leich\u00b7ten", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schritten \u00fcber jene Berge, \u2013 jenseits in das Thal hinab, \u2013", "tokens": ["Schrit\u00b7ten", "\u00fc\u00b7ber", "je\u00b7ne", "Ber\u00b7ge", ",", "\u2013", "jen\u00b7seits", "in", "das", "Thal", "hin\u00b7ab", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$,", "$(", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Schritten fort unaufgehalten, \u00fcber neue Bergeswand,", "tokens": ["Schrit\u00b7ten", "fort", "un\u00b7auf\u00b7ge\u00b7hal\u00b7ten", ",", "\u00fc\u00b7ber", "neu\u00b7e", "Ber\u00b7ges\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ADJA", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und sie sah'n, so weit sie wallten, ringsum sch\u00f6nes reiches Land!", "tokens": ["Und", "sie", "sah'n", ",", "so", "weit", "sie", "wall\u00b7ten", ",", "ring\u00b7sum", "sch\u00f6\u00b7nes", "rei\u00b7ches", "Land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Herrscher dieses sch\u00f6nen Landes, s\u00e4\u00dfest du statt meiner hier!", "tokens": ["Herr\u00b7scher", "die\u00b7ses", "sch\u00f6\u00b7nen", "Lan\u00b7des", ",", "s\u00e4\u00b7\u00dfest", "du", "statt", "mei\u00b7ner", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "S\u00e4uselten, wie Fr\u00fchlingstr\u00e4ume, um das Haupt die Zweige dir!", "tokens": ["S\u00e4u\u00b7sel\u00b7ten", ",", "wie", "Fr\u00fch\u00b7lings\u00b7tr\u00e4u\u00b7me", ",", "um", "das", "Haupt", "die", "Zwei\u00b7ge", "dir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "KOUI", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Riefst du in das Thal hernieder, wie ich's gerne rufen mag:", "tokens": ["Riefst", "du", "in", "das", "Thal", "her\u00b7nie\u00b7der", ",", "wie", "ich's", "ger\u00b7ne", "ru\u00b7fen", "mag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,", "PWAV", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Oesterreich, du Land des Ostens, auch in dir nun werd' es Tag! \u2013", "tokens": ["O\u00b7es\u00b7ter\u00b7reich", ",", "du", "Land", "des", "Os\u00b7tens", ",", "auch", "in", "dir", "nun", "werd'", "es", "Tag", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "NN", "ART", "NN", "$,", "ADV", "APPR", "PPER", "ADV", "VAFIN", "PPER", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Vaterland, von Gott gesegnet also reich mit jeder Lust,", "tokens": ["Va\u00b7ter\u00b7land", ",", "von", "Gott", "ge\u00b7seg\u00b7net", "al\u00b7so", "reich", "mit", "je\u00b7der", "Lust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "VVPP", "ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df f\u00fcr dich der Ueberreiche andre fast enterben mu\u00dft'!", "tokens": ["Da\u00df", "f\u00fcr", "dich", "der", "Ue\u00b7ber\u00b7rei\u00b7che", "and\u00b7re", "fast", "en\u00b7ter\u00b7ben", "mu\u00dft'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "O entrolle mir die B\u00fccher deiner Thaten, inhaltschwer!", "tokens": ["O", "ent\u00b7rol\u00b7le", "mir", "die", "B\u00fc\u00b7cher", "dei\u00b7ner", "Tha\u00b7ten", ",", "in\u00b7halt\u00b7schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Solche Saat mu\u00df steh'n voll Garben, voll von Perlen solch ein Meer:", "tokens": ["Sol\u00b7che", "Saat", "mu\u00df", "steh'n", "voll", "Gar\u00b7ben", ",", "voll", "von", "Per\u00b7len", "solch", "ein", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "ADJD", "NN", "$,", "ADJD", "APPR", "NN", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Wohl hast du dir gro\u00dfe Thaten \u2013 deiner S\u00f6hne Stolz und Muth! \u2013", "tokens": ["Wohl", "hast", "du", "dir", "gro\u00b7\u00dfe", "Tha\u00b7ten", "\u2013", "dei\u00b7ner", "S\u00f6h\u00b7ne", "Stolz", "und", "Muth", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJA", "NN", "$(", "PPOSAT", "NN", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie gediegnes Gold gesammelt, schreitend durch der Zeiten Fluth?", "tokens": ["Wie", "ge\u00b7dieg\u00b7nes", "Gold", "ge\u00b7sam\u00b7melt", ",", "schrei\u00b7tend", "durch", "der", "Zei\u00b7ten", "Fluth", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVPP", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sicherlich baust du am Dome hoher Kunst und Wissenschaft,", "tokens": ["Si\u00b7cher\u00b7lich", "baust", "du", "am", "Do\u00b7me", "ho\u00b7her", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df er deiner w\u00fcrdig rage, r\u00fcstig fort mit Jugendkraft?", "tokens": ["Da\u00df", "er", "dei\u00b7ner", "w\u00fcr\u00b7dig", "ra\u00b7ge", ",", "r\u00fcs\u00b7tig", "fort", "mit", "Ju\u00b7gend\u00b7kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "VVFIN", "$,", "ADJD", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Wo das Blut flo\u00df deines Volkes, standen in der Schlachtenreih'", "tokens": ["Wo", "das", "Blut", "flo\u00df", "dei\u00b7nes", "Vol\u00b7kes", ",", "stan\u00b7den", "in", "der", "Schlach\u00b7ten\u00b7reih'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Recht und Licht und Freiheit immer dir als Waffenbr\u00fcder bei?", "tokens": ["Recht", "und", "Licht", "und", "Frei\u00b7heit", "im\u00b7mer", "dir", "als", "Waf\u00b7fen\u00b7br\u00fc\u00b7der", "bei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "ADV", "PPER", "KOUS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Stets war deiner K\u00e4mpfe Losung edel und gerecht gewi\u00df? \u2013", "tokens": ["Stets", "war", "dei\u00b7ner", "K\u00e4mp\u00b7fe", "Lo\u00b7sung", "e\u00b7del", "und", "ge\u00b7recht", "ge\u00b7wi\u00df", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "ADJD", "KON", "ADJD", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Mir im Aug' steht eine Thr\u00e4ne! \u2013 ach, die Antwort ist nicht s\u00fc\u00df! \u2013", "tokens": ["Mir", "im", "Aug'", "steht", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "!", "\u2013", "ach", ",", "die", "Ant\u00b7wort", "ist", "nicht", "s\u00fc\u00df", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "ART", "NN", "$.", "$(", "XY", "$,", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Ebnes Land liegt mir zu F\u00fc\u00dfen, wie ein stilles gr\u00fcnes Meer,", "tokens": ["Eb\u00b7nes", "Land", "liegt", "mir", "zu", "F\u00fc\u00b7\u00dfen", ",", "wie", "ein", "stil\u00b7les", "gr\u00fc\u00b7nes", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$,", "PWAV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Weit hinaus, wie M\u00f6ven, kreisen meine Blicke dr\u00fcber her;", "tokens": ["Weit", "hin\u00b7aus", ",", "wie", "M\u00f6\u00b7ven", ",", "krei\u00b7sen", "mei\u00b7ne", "Bli\u00b7cke", "dr\u00fc\u00b7ber", "her", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PWAV", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Gleichwie schmale lichte Furchen, die durchs Meer die Schiffe zieh'n,", "tokens": ["Gleich\u00b7wie", "schma\u00b7le", "lich\u00b7te", "Fur\u00b7chen", ",", "die", "durchs", "Meer", "die", "Schif\u00b7fe", "zieh'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Schl\u00e4ngeln Donaustrom und Stra\u00dfen sich als Silberstreifen hin.", "tokens": ["Schl\u00e4n\u00b7geln", "Do\u00b7naus\u00b7trom", "und", "Stra\u00b7\u00dfen", "sich", "als", "Sil\u00b7ber\u00b7strei\u00b7fen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PRF", "KOUS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Rings empor als inselreicher, stolzer Archipelagus", "tokens": ["Rings", "em\u00b7por", "als", "in\u00b7sel\u00b7rei\u00b7cher", ",", "stol\u00b7zer", "Ar\u00b7chi\u00b7pe\u00b7la\u00b7gus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "KOKOM", "ADV", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ragen D\u00f6rfer, Schl\u00f6sser, St\u00e4dte, blinkend wie aus Silbergu\u00df,", "tokens": ["Ra\u00b7gen", "D\u00f6r\u00b7fer", ",", "Schl\u00f6s\u00b7ser", ",", "St\u00e4d\u00b7te", ",", "blin\u00b7kend", "wie", "aus", "Sil\u00b7ber\u00b7gu\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "ADJD", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch vor allen gro\u00df und m\u00e4chtig ragt ein Eiland aus dem Meer,", "tokens": ["Doch", "vor", "al\u00b7len", "gro\u00df", "und", "m\u00e4ch\u00b7tig", "ragt", "ein", "Ei\u00b7land", "aus", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dem als Tannenwald die Stirne kr\u00f6nt gewalt'ger Th\u00fcrme Heer.", "tokens": ["Dem", "als", "Tan\u00b7nen\u00b7wald", "die", "Stir\u00b7ne", "kr\u00f6nt", "ge\u00b7walt'\u00b7ger", "Th\u00fcr\u00b7me", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOUS", "NN", "ART", "NN", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Wien, du bist's, Stadt der C\u00e4saren! \u2013 Doch wie d\u00fcnkst du jetzt mir klein!", "tokens": ["Wi\u00b7en", ",", "du", "bist's", ",", "Stadt", "der", "C\u00e4\u00b7sa\u00b7ren", "!", "\u2013", "Doch", "wie", "d\u00fcnkst", "du", "jetzt", "mir", "klein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NE", "$,", "NN", "ART", "NN", "$.", "$(", "KON", "PWAV", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "$."], "meter": "+-+-+--+---+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Selbst ein Meer sonst meinem Auge, schrumpfst du nun zur Insel ein!", "tokens": ["Selbst", "ein", "Meer", "sonst", "mei\u00b7nem", "Au\u00b7ge", ",", "schrumpfst", "du", "nun", "zur", "In\u00b7sel", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure