{"textgrid.poem.64147": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im rebengr\u00fcnen Neckartal,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im rebengr\u00fcnen Neckartal,", "tokens": ["Im", "re\u00b7ben\u00b7gr\u00fc\u00b7nen", "Ne\u00b7ckar\u00b7tal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da steht mein V\u00e4terschlo\u00df,", "tokens": ["Da", "steht", "mein", "V\u00e4\u00b7ter\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das jetzt zur Stund' der Abendstrahl", "tokens": ["Das", "jetzt", "zur", "Stund'", "der", "A\u00b7bend\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl goldig \u00fcbergo\u00df:", "tokens": ["Wohl", "gol\u00b7dig", "\u00fc\u00b7ber\u00b7go\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch ich zieh' fern im Heidenland,", "tokens": ["Doch", "ich", "zieh'", "fern", "im", "Hei\u00b7den\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In W\u00fcstenglut, in Sonnenbrand:", "tokens": ["In", "W\u00fcs\u00b7ten\u00b7glut", ",", "in", "Son\u00b7nen\u00b7brand", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um Palmenwipfel schwanken", "tokens": ["Um", "Pal\u00b7men\u00b7wip\u00b7fel", "schwan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die sehnenden Gedanken.", "tokens": ["Die", "seh\u00b7nen\u00b7den", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt reitet wohl durch Wald und Au,", "tokens": ["Jetzt", "rei\u00b7tet", "wohl", "durch", "Wald", "und", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im gr\u00fcnen Jagdgewand", "tokens": ["Im", "gr\u00fc\u00b7nen", "Jagd\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Daheim die allersch\u00f6nste Frau,", "tokens": ["Da\u00b7heim", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Falken auf der Hand:", "tokens": ["Den", "Fal\u00b7ken", "auf", "der", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch mir winkt hehr und streng zur Pflicht", "tokens": ["Doch", "mir", "winkt", "hehr", "und", "streng", "zur", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "VVFIN", "APPRART", "NN"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Der heil'gen Jungfrau Angesicht", "tokens": ["Der", "heil'\u00b7gen", "Jung\u00b7frau", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Herab aus unsern Fahnen,", "tokens": ["Her\u00b7ab", "aus", "un\u00b7sern", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu Kampf und Tod zu mahnen.", "tokens": ["Zu", "Kampf", "und", "Tod", "zu", "mah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jetzt t\u00f6nt daheim im Feierklang", "tokens": ["Jetzt", "t\u00f6nt", "da\u00b7heim", "im", "Fei\u00b7er\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Abendglocke Lied: \u2013", "tokens": ["Der", "A\u00b7bend\u00b7glo\u00b7cke", "Lied", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ins Dorf zur\u00fcck vom Wiesenhang", "tokens": ["Ins", "Dorf", "zu\u00b7r\u00fcck", "vom", "Wie\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Herde friedlich zieht:", "tokens": ["Die", "Her\u00b7de", "fried\u00b7lich", "zieht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir aber ruft aus wilder Reih'", "tokens": ["Mir", "a\u00b7ber", "ruft", "aus", "wil\u00b7der", "Reih'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sarazenen Schlachtgeschrei: \u2013", "tokens": ["Der", "Sa\u00b7ra\u00b7ze\u00b7nen", "Schlacht\u00b7ge\u00b7schrei", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht l\u00e4nger darf ich s\u00e4umen,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "darf", "ich", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fahr' wohl, du s\u00fc\u00dfes Tr\u00e4umen.", "tokens": ["Fahr'", "wohl", ",", "du", "s\u00fc\u00b7\u00dfes", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wohlan, ihr Schwaben, frank und frei,", "tokens": ["Wo\u00b7hlan", ",", "ihr", "Schwa\u00b7ben", ",", "frank", "und", "frei", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt auf mit Schild und Schaft!", "tokens": ["Jetzt", "auf", "mit", "Schild", "und", "Schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Heide sp\u00fcre, was es sei", "tokens": ["Der", "Hei\u00b7de", "sp\u00fc\u00b7re", ",", "was", "es", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um deutsche Ritterschaft!", "tokens": ["Um", "deut\u00b7sche", "Rit\u00b7ter\u00b7schaft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und fall' ich hier im W\u00fcstensand, \u2013", "tokens": ["Und", "fall'", "ich", "hier", "im", "W\u00fcs\u00b7ten\u00b7sand", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O gr\u00fc\u00dfet mir mein Heimatland:", "tokens": ["O", "gr\u00fc\u00b7\u00dfet", "mir", "mein", "Hei\u00b7mat\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sagt, treu sei ihm geblieben", "tokens": ["Sagt", ",", "treu", "sei", "ihm", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Heimweh und mein Lieben.", "tokens": ["Mein", "Heim\u00b7weh", "und", "mein", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}