{"textgrid.poem.47601": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[fort, himmlische Wonn']", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fort, himmlische Wonn'", "tokens": ["Fort", ",", "himm\u00b7li\u00b7sche", "Wonn'"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und ewiges Heil!", "tokens": ["Und", "e\u00b7wi\u00b7ges", "Heil", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das irdische hier ist besser!", "tokens": ["Das", "ir\u00b7di\u00b7sche", "hier", "ist", "bes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein einziger Blick,", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ein L\u00e4cheln, ein Ku\u00df,", "tokens": ["Ein", "L\u00e4\u00b7cheln", ",", "ein", "Ku\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Der Liebe von ", "tokens": ["Der", "Lie\u00b7be", "von"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Freiheit Gl\u00fcck", "tokens": ["Der", "Frei\u00b7heit", "Gl\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Es ist edel und gro\u00df;", "tokens": ["Es", "ist", "e\u00b7del", "und", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch wenn ich es recht bedenke,", "tokens": ["Doch", "wenn", "ich", "es", "recht", "be\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Verstrickt mit der Seel'", "tokens": ["Ver\u00b7strickt", "mit", "der", "Seel'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und gefangen zu sein", "tokens": ["Und", "ge\u00b7fan\u00b7gen", "zu", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKZU", "VAINF"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Im Lockenrevier, ist besser. \u2013", "tokens": ["Im", "Lo\u00b7cken\u00b7re\u00b7vier", ",", "ist", "bes\u00b7ser", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "VAFIN", "ADJD", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Weit besser erscheint", "tokens": ["Weit", "bes\u00b7ser", "er\u00b7scheint"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJD", "VVFIN"], "meter": "+---+", "measure": "dactylic.init"}, "line.2": {"text": "Dein geistlicher Trieb,", "tokens": ["Dein", "geist\u00b7li\u00b7cher", "Trieb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Als unsre Begier, doch diese", "tokens": ["Als", "uns\u00b7re", "Be\u00b7gier", ",", "doch", "die\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADV", "PDAT"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ist ohne Geprahl", "tokens": ["Ist", "oh\u00b7ne", "Ge\u00b7prahl"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Anma\u00dfung und Zank,", "tokens": ["An\u00b7ma\u00b7\u00dfung", "und", "Zank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und diese Begier ist besser. \u2013", "tokens": ["Und", "die\u00b7se", "Be\u00b7gier", "ist", "bes\u00b7ser", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch o wie gefehlt", "tokens": ["Doch", "o", "wie", "ge\u00b7fehlt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "FM", "KOKOM", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Rede mit dem", "tokens": ["Die", "Re\u00b7de", "mit", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der mystischem Unsinn opfert!", "tokens": ["Der", "mys\u00b7ti\u00b7schem", "Un\u00b7sinn", "op\u00b7fert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu reden in klar", "tokens": ["Zu", "re\u00b7den", "in", "klar"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Verst\u00e4ndlicher Art.", "tokens": ["Ver\u00b7st\u00e4nd\u00b7li\u00b7cher", "Art", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mit Hammel und Stier, ist besser. \u2013", "tokens": ["Mit", "Ham\u00b7mel", "und", "Stier", ",", "ist", "bes\u00b7ser", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VAFIN", "ADJD", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbo sieh doch, Hafis,", "tokens": ["\u00bb", "o", "sieh", "doch", ",", "Ha\u00b7fis", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "FM", "FM", "ADV", "$,", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wie hier mir am Hals", "tokens": ["Wie", "hier", "mir", "am", "Hals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "APPRART", "NN"], "meter": "----+", "measure": "unknown.measure.single"}, "line.3": {"text": "Das sch\u00f6nste Geschmeide funkelt!", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Ge\u00b7schmei\u00b7de", "fun\u00b7kelt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kein Schmuck in der Welt", "tokens": ["Kein", "Schmuck", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "So k\u00f6stlich er ist,\u00ab", "tokens": ["So", "k\u00f6st\u00b7lich", "er", "ist", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "So sprach sie zu mir, \u00bbist besser.\u00ab \u2013", "tokens": ["So", "sprach", "sie", "zu", "mir", ",", "\u00bb", "ist", "bes\u00b7ser", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "$(", "VAFIN", "ADJD", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbvergleiche, mein Lieb,\u00ab", "tokens": ["\u00bb", "ver\u00b7glei\u00b7che", ",", "mein", "Lieb", ",", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "PPOSAT", "NN", "$,", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Antwortet' ich ihr,", "tokens": ["Ant\u00b7wor\u00b7tet'", "ich", "ihr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbdie poetischen Perlenschn\u00fcre,", "tokens": ["\u00bb", "die", "po\u00b7e\u00b7ti\u00b7schen", "Per\u00b7len\u00b7schn\u00fc\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die dir zum Schmuck", "tokens": ["Die", "dir", "zum", "Schmuck"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein Finger gereiht!", "tokens": ["Mein", "Fin\u00b7ger", "ge\u00b7reiht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Denn selbige Zier ist besser.\u00ab", "tokens": ["Denn", "sel\u00b7bi\u00b7ge", "Zier", "ist", "bes\u00b7ser", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}