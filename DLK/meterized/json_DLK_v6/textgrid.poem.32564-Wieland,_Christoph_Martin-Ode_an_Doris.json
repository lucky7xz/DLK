{"textgrid.poem.32564": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ode an Doris", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erseufzte Stunde, da ich sie wiederseh,", "tokens": ["Er\u00b7seufz\u00b7te", "Stun\u00b7de", ",", "da", "ich", "sie", "wie\u00b7der\u00b7seh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da sich ihr Arm mir z\u00e4rtlich entgegenstreckt,", "tokens": ["Da", "sich", "ihr", "Arm", "mir", "z\u00e4rt\u00b7lich", "ent\u00b7ge\u00b7gen\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Stunde der s\u00fc\u00dfen Freudenschauer,", "tokens": ["Stun\u00b7de", "der", "s\u00fc\u00b7\u00dfen", "Freu\u00b7den\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Eil aus der Liebe Scho\u00df hernieder.", "tokens": ["Eil", "aus", "der", "Lie\u00b7be", "Scho\u00df", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur selten steiget eine der Seligen", "tokens": ["Nur", "sel\u00b7ten", "stei\u00b7get", "ei\u00b7ne", "der", "Se\u00b7li\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00c4therschen Stunden, wie sie der Himmel lebt,", "tokens": ["\u00c4\u00b7ther\u00b7schen", "Stun\u00b7den", ",", "wie", "sie", "der", "Him\u00b7mel", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nieder zur Erde, wo die Menschen,", "tokens": ["Nie\u00b7der", "zur", "Er\u00b7de", ",", "wo", "die", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sich nicht bekannt, die Zeit vertr\u00e4umen.", "tokens": ["Sich", "nicht", "be\u00b7kannt", ",", "die", "Zeit", "ver\u00b7tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADJD", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber dich sendet, goldene Stunde, mir", "tokens": ["A\u00b7ber", "dich", "sen\u00b7det", ",", "gol\u00b7de\u00b7ne", "Stun\u00b7de", ",", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PPER"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der Gottheit Tochter, die ich, von wenigen", "tokens": ["Der", "Got\u00b7theit", "Toch\u00b7ter", ",", "die", "ich", ",", "von", "we\u00b7ni\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "$,", "APPR", "PIAT"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Geh\u00f6rt, den Menschen sang, die Liebe,", "tokens": ["Ge\u00b7h\u00f6rt", ",", "den", "Men\u00b7schen", "sang", ",", "die", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Selber aus ihrer Scho\u00df hernieder.", "tokens": ["Sel\u00b7ber", "aus", "ih\u00b7rer", "Scho\u00df", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "So schweben \u00fcber Liebende Seraphim", "tokens": ["So", "schwe\u00b7ben", "\u00fc\u00b7ber", "Lie\u00b7ben\u00b7de", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Zephyrs\u00fc\u00dfen Stunden der Freude hin,", "tokens": ["Mit", "Ze\u00b7phyr\u00b7s\u00fc\u00b7\u00dfen", "Stun\u00b7den", "der", "Freu\u00b7de", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So, wie ich dich genie\u00dfen werde,", "tokens": ["So", ",", "wie", "ich", "dich", "ge\u00b7nie\u00b7\u00dfen", "wer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PRF", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt dich der J\u00fcngling jenes Erdballs;", "tokens": ["F\u00fchlt", "dich", "der", "J\u00fcng\u00b7ling", "je\u00b7nes", "Erd\u00b7balls", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der dort im Meer unz\u00e4hlicher Sonnen schwimmt,", "tokens": ["Der", "dort", "im", "Meer", "un\u00b7z\u00e4h\u00b7li\u00b7cher", "Son\u00b7nen", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Glanz bedecket, keinem Cassin bemerkt,", "tokens": ["Von", "Glanz", "be\u00b7de\u00b7cket", ",", "kei\u00b7nem", "Cas\u00b7sin", "be\u00b7merkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dir nur sichtbar, dem selbst Eloas", "tokens": ["Dir", "nur", "sicht\u00b7bar", ",", "dem", "selbst", "E\u00b7loas"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "$,", "PRELS", "ADV", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wohnplatz die himmlische Muse zeigte.", "tokens": ["Wohn\u00b7platz", "die", "himm\u00b7li\u00b7sche", "Mu\u00b7se", "zeig\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Was werd ich f\u00fchlen? Doris, was f\u00fchlst du dann?", "tokens": ["Was", "werd", "ich", "f\u00fch\u00b7len", "?", "Do\u00b7ris", ",", "was", "f\u00fchlst", "du", "dann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVINF", "$.", "NE", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was keine Zunge sterblicher S\u00e4nger spricht,", "tokens": ["Was", "kei\u00b7ne", "Zun\u00b7ge", "sterb\u00b7li\u00b7cher", "S\u00e4n\u00b7ger", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was nicht die Seel in seinem Umfang", "tokens": ["Was", "nicht", "die", "Seel", "in", "sei\u00b7nem", "Um\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denken kann, was sie entz\u00fcckt nur f\u00fchlet!", "tokens": ["Den\u00b7ken", "kann", ",", "was", "sie", "ent\u00b7z\u00fcckt", "nur", "f\u00fch\u00b7let", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Kaum wird sie glauben, wenn ihr das Auge sagt,", "tokens": ["Kaum", "wird", "sie", "glau\u00b7ben", ",", "wenn", "ihr", "das", "Au\u00b7ge", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df du ihr nah seist, bis sie vor Freude stumm,", "tokens": ["Da\u00df", "du", "ihr", "nah", "seist", ",", "bis", "sie", "vor", "Freu\u00b7de", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In Umarmungen sanft zerschmolzen,", "tokens": ["In", "Um\u00b7ar\u00b7mun\u00b7gen", "sanft", "zer\u00b7schmol\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Z\u00e4rtlichste Seele, Dich gegenw\u00e4rtig", "tokens": ["Z\u00e4rt\u00b7lichs\u00b7te", "See\u00b7le", ",", "Dich", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJD"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Empfindet, bis die Schauer der Sympathie", "tokens": ["Emp\u00b7fin\u00b7det", ",", "bis", "die", "Schau\u00b7er", "der", "Sym\u00b7pa\u00b7thie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie sanft durchdringen, da\u00df von den Schauern dann", "tokens": ["Sie", "sanft", "durch\u00b7drin\u00b7gen", ",", "da\u00df", "von", "den", "Schau\u00b7ern", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVINF", "$,", "KOUS", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jede Begierde bebt und fr\u00f6hlich", "tokens": ["Je\u00b7de", "Be\u00b7gier\u00b7de", "bebt", "und", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "KON", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ihrer Geliebten entgegenwallet.", "tokens": ["Ih\u00b7rer", "Ge\u00b7lieb\u00b7ten", "ent\u00b7ge\u00b7gen\u00b7wal\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Was f\u00fcr Gedanken, was f\u00fcr Empfindungen,", "tokens": ["Was", "f\u00fcr", "Ge\u00b7dan\u00b7ken", ",", "was", "f\u00fcr", "Emp\u00b7fin\u00b7dun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Mund unnennbar, redst du, o Auge, mir?", "tokens": ["Dem", "Mund", "un\u00b7nenn\u00b7bar", ",", "redst", "du", ",", "o", "Au\u00b7ge", ",", "mir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Himmlisches Aug, was vor Entz\u00fcckung", "tokens": ["Himm\u00b7li\u00b7sches", "Aug", ",", "was", "vor", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Weinst du, mit Blicken der holden Liebe", "tokens": ["Weinst", "du", ",", "mit", "Bli\u00b7cken", "der", "hol\u00b7den", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Auf meine Wangen? Heiliger Augenblick,", "tokens": ["Auf", "mei\u00b7ne", "Wan\u00b7gen", "?", "Hei\u00b7li\u00b7ger", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da ich zuerst dir, Freundin, entgegenkam!", "tokens": ["Da", "ich", "zu\u00b7erst", "dir", ",", "Freun\u00b7din", ",", "ent\u00b7ge\u00b7gen\u00b7kam", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da ich dich liebte! Meines Gl\u00fcckes", "tokens": ["Da", "ich", "dich", "lieb\u00b7te", "!", "Mei\u00b7nes", "Gl\u00fc\u00b7ckes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dieser Stunde Quell sei gesegnet!", "tokens": ["Und", "die\u00b7ser", "Stun\u00b7de", "Quell", "sei", "ge\u00b7seg\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Wenn nun die Arme m\u00fcd von Umarmungen", "tokens": ["Wenn", "nun", "die", "Ar\u00b7me", "m\u00fcd", "von", "Um\u00b7ar\u00b7mun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich ungern lassen, wenn sich die Seelen nun", "tokens": ["Sich", "un\u00b7gern", "las\u00b7sen", ",", "wenn", "sich", "die", "See\u00b7len", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "VVINF", "$,", "KOUS", "PRF", "ART", "NN", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus der Empfindungen s\u00fc\u00dfem Taumel", "tokens": ["Aus", "der", "Emp\u00b7fin\u00b7dun\u00b7gen", "s\u00fc\u00b7\u00dfem", "Tau\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bebend erholen und um sich sehen,", "tokens": ["Be\u00b7bend", "er\u00b7ho\u00b7len", "und", "um", "sich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "APPR", "PRF", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.12": {"line.1": {"text": "Denn blickt ein Auge wundernd das andre an,", "tokens": ["Denn", "blickt", "ein", "Au\u00b7ge", "wun\u00b7dernd", "das", "and\u00b7re", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das volle Herz str\u00f6mt noch von den Lippen nicht,", "tokens": ["Das", "vol\u00b7le", "Herz", "str\u00f6mt", "noch", "von", "den", "Lip\u00b7pen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Stumm, doch voll namenloser Freuden", "tokens": ["Stumm", ",", "doch", "voll", "na\u00b7men\u00b7lo\u00b7ser", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dankt dann der ernste Blick gen Himmel,", "tokens": ["Dankt", "dann", "der", "erns\u00b7te", "Blick", "gen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic