{"textgrid.poem.57915": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "O Jammer, Jammer, h\u00f6ret an", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was soll es bedeuten,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "soll", "es", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df ich so trau-e-rig bin,", "tokens": ["Da\u00df", "ich", "so", "trau\u00b7e\u00b7rig", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Haus in der La-a-vesstra\u00dfe,", "tokens": ["Das", "Haus", "in", "der", "La\u00b7a\u00b7ve\u00b7ss\u00b7tra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das wi-ill mir nicht aus dem Sinn.", "tokens": ["Das", "wi\u00b7\u00b7ill", "mir", "nicht", "aus", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Der la-a-angweilige Kasten,", "tokens": ["Der", "la\u00b7a\u00b7ang\u00b7wei\u00b7li\u00b7ge", "Kas\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das stu-u-umpfsinnige Haus,", "tokens": ["Das", "stu\u00b7u\u00b7umpf\u00b7sin\u00b7ni\u00b7ge", "Haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Halb sieht es wie eine Kase-rne", "tokens": ["Halb", "sieht", "es", "wie", "ei\u00b7ne", "Ka\u00b7se\u00b7\u00b7rne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und halb wie ein Kornspeicher aus.", "tokens": ["Und", "halb", "wie", "ein", "Korn\u00b7spei\u00b7cher", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Den gu-uten Hannoveraner", "tokens": ["Den", "gu\u00b7u\u00b7ten", "Han\u00b7no\u00b7ve\u00b7ra\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ergreift es mit wi-ildem Weh,", "tokens": ["Er\u00b7greift", "es", "mit", "wi\u00b7\u00b7il\u00b7dem", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sieht er an der Silofassade,", "tokens": ["Sieht", "er", "an", "der", "Si\u00b7lo\u00b7fas\u00b7sa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Der \u00f6-\u00f6den, i-in die H\u00f6h.", "tokens": ["Der", "\u00f6\u00b7\u00b7\u00f6\u00b7den", ",", "i\u00b7\u00b7in", "die", "H\u00f6h", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "NE", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und antiberlinisch wird w\u00e4hlen", "tokens": ["Und", "an\u00b7ti\u00b7ber\u00b7li\u00b7nisch", "wird", "w\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das n\u00e4-\u00e4chste Ma-al der Mann;", "tokens": ["Das", "n\u00e4\u00b7\u00e4chs\u00b7te", "Ma\u00b7al", "der", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das ha-at mit seinem Gebau-au-e", "tokens": ["Das", "ha\u00b7at", "mit", "sei\u00b7nem", "Ge\u00b7bau\u00b7au\u00b7\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann wieder der Fiskus getan.", "tokens": ["Dann", "wie\u00b7der", "der", "Fis\u00b7kus", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}