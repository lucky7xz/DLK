{"textgrid.poem.33194": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2152.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um drey rieff er: Ich befehl nun in deine h\u00e4nde, lieber Vater! meine seel; das wars selge ende.", "tokens": ["Um", "drey", "rieff", "er", ":", "Ich", "be\u00b7fehl", "nun", "in", "dei\u00b7ne", "h\u00e4n\u00b7de", ",", "lie\u00b7ber", "Va\u00b7ter", "!", "mei\u00b7ne", "seel", ";", "das", "wars", "sel\u00b7ge", "en\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "$,", "ADV", "NN", "$.", "PPOSAT", "NN", "$.", "PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Diese angeordnete bet- und andachts-stunden zeugen von des Lammes weh seinen wunden-kunden.", "tokens": ["Die\u00b7se", "an\u00b7ge\u00b7ord\u00b7ne\u00b7te", "bet", "und", "an\u00b7dachts\u00b7stun\u00b7den", "zeu\u00b7gen", "von", "des", "Lam\u00b7mes", "weh", "sei\u00b7nen", "wun\u00b7den\u00b7kun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "TRUNC", "KON", "ADJA", "VVFIN", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}}}}