{"textgrid.poem.47887": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sind die Pierinnen", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sind die Pierinnen", "tokens": ["Wie", "sind", "die", "Pie\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nicht mehr auf Pindus Zinnen/", "tokens": ["Nicht", "mehr", "auf", "Pin\u00b7dus", "Zin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NE", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil N\u00fcrnberg lieblich singt?", "tokens": ["Weil", "N\u00fcrn\u00b7berg", "lieb\u00b7lich", "singt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was h\u00f6r ich doch vor S\u00e4iten", "tokens": ["Was", "h\u00f6r", "ich", "doch", "vor", "S\u00e4i\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die klugen H\u00e4nde leiten/", "tokens": ["Die", "klu\u00b7gen", "H\u00e4n\u00b7de", "lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das gantze N\u00f6rdgau springt!", "tokens": ["Das", "gant\u00b7ze", "N\u00f6rd\u00b7gau", "springt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Harsdorff/ Klajus