{"dta.poem.4391": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die glaubigen Atheisten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Atheist darf GOtt nicht ehren, er wird auch solches", "tokens": ["Ein", "A\u00b7theist", "darf", "Gott", "nicht", "eh\u00b7ren", ",", "er", "wird", "auch", "sol\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "NN", "PTKNEG", "VVINF", "$,", "PPER", "VAFIN", "ADV", "PIAT"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "nimmer thun,", "tokens": ["nim\u00b7mer", "thun", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So lange solche Meynungen in seiner Seel\u2019 ungl\u00fccklich", "tokens": ["So", "lan\u00b7ge", "sol\u00b7che", "Mey\u00b7nun\u00b7gen", "in", "sei\u00b7ner", "Seel'", "un\u00b7gl\u00fcck\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "ruhn.", "tokens": ["ruhn", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Allein, wer eine Gottheit glaubt, die ihn, zu Seiner Ehr\u2019,", "tokens": ["Al\u00b7lein", ",", "wer", "ei\u00b7ne", "Got\u00b7theit", "glaubt", ",", "die", "ihn", ",", "zu", "Sei\u00b7ner", "Ehr'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "erschaffen,", "tokens": ["er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und die er weder kennen kann, noch ehren, als in Seinen", "tokens": ["Und", "die", "er", "we\u00b7der", "ken\u00b7nen", "kann", ",", "noch", "eh\u00b7ren", ",", "als", "in", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PRELS", "PPER", "KON", "VVINF", "VMFIN", "$,", "ADV", "VVINF", "$,", "KOUS", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Werken,", "tokens": ["Wer\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "In welchen Sie Sich offenbahrt, und will Sie dennoch", "tokens": ["In", "wel\u00b7chen", "Sie", "Sich", "of\u00b7fen\u00b7bahrt", ",", "und", "will", "Sie", "den\u00b7noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPER", "PRF", "VVFIN", "$,", "KON", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "nicht bemerken,", "tokens": ["nicht", "be\u00b7mer\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Nein, sondern glaubt, es sey genug Sie obenhin nur anzu-", "tokens": ["Nein", ",", "son\u00b7dern", "glaubt", ",", "es", "sey", "ge\u00b7nug", "Sie", "o\u00b7ben\u00b7hin", "nur", "an\u00b7zu"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KON", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "PPER", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "gaffen,", "tokens": ["gaf\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Und meynt, GOtt sey genug geehrt, wenn er nur blo\u00df", "tokens": ["Und", "meynt", ",", "Gott", "sey", "ge\u00b7nug", "ge\u00b7ehrt", ",", "wenn", "er", "nur", "blo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "NN", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "f\u00fcr sich allein,", "tokens": ["f\u00fcr", "sich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Da\u00df er auf Erden m\u00f6ge gl\u00fccklich, und k\u00fcnftig ewig selig", "tokens": ["Da\u00df", "er", "auf", "Er\u00b7den", "m\u00f6\u00b7ge", "gl\u00fcck\u00b7lich", ",", "und", "k\u00fcnf\u00b7tig", "e\u00b7wig", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VMFIN", "ADJD", "$,", "KON", "ADJD", "ADJD", "ADJD"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "seyn,", "tokens": ["seyn", ","], "token_info": ["word", "punct"], "pos": ["VAINF", "$,"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Gesorgt, und die\u00df von Jhm erbeten; der zeiget dieses offen-", "tokens": ["Ge\u00b7sorgt", ",", "und", "die\u00df", "von", "Jhm", "er\u00b7be\u00b7ten", ";", "der", "zei\u00b7get", "die\u00b7ses", "of\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "PDS", "APPR", "PPER", "VVFIN", "$.", "ART", "VVFIN", "PDAT", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "bar,", "tokens": ["bar", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Da\u00df, k\u00f6nnt\u2019 er die\u00df von GOtt nicht hoffen, er w\u00fcrde", "tokens": ["Da\u00df", ",", "k\u00f6nnt'", "er", "die\u00df", "von", "Gott", "nicht", "hof\u00b7fen", ",", "er", "w\u00fcr\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "VMFIN", "PPER", "PDS", "APPR", "NN", "PTKNEG", "VVINF", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "sich, um Jhn zu ehren,", "tokens": ["sich", ",", "um", "Jhn", "zu", "eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "An einen GOtt so wenig kehren,", "tokens": ["An", "ei\u00b7nen", "Gott", "so", "we\u00b7nig", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Als wie der Atheisten Schaar.", "tokens": ["Als", "wie", "der", "A\u00b7theis\u00b7ten", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}