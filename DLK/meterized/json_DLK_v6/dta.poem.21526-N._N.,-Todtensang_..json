{"dta.poem.21526": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Todtensang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Der Nachtwind heult dir den Todtensang \u2014", "tokens": ["Der", "Nacht\u00b7wind", "heult", "dir", "den", "Tod\u00b7ten\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun schlaf, mein Bruder, nun schlaf!", "tokens": ["Nun", "schlaf", ",", "mein", "Bru\u00b7der", ",", "nun", "schlaf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wenn deine Seele auch Flammen trank,", "tokens": ["Und", "wenn", "dei\u00b7ne", "See\u00b7le", "auch", "Flam\u00b7men", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Hieb des Todes, er traf!", "tokens": ["Der", "Hieb", "des", "To\u00b7des", ",", "er", "traf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und wenn deine Seele auch Welten barg,", "tokens": ["Und", "wenn", "dei\u00b7ne", "See\u00b7le", "auch", "Wel\u00b7ten", "barg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und jauchzend zum Lichte sich rang:", "tokens": ["Und", "jauch\u00b7zend", "zum", "Lich\u00b7te", "sich", "rang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Nun liegst du im Grunde, im modernden Sarg \u2014", "tokens": ["Nun", "liegst", "du", "im", "Grun\u00b7de", ",", "im", "mo\u00b7dern\u00b7den", "Sarg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Der Kelch deiner Seele zersprang!", "tokens": ["Der", "Kelch", "dei\u00b7ner", "See\u00b7le", "zer\u00b7sprang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Mit leuchtender Stirn, mit flammender Brust,", "tokens": ["Mit", "leuch\u00b7ten\u00b7der", "Stirn", ",", "mit", "flam\u00b7men\u00b7der", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zog\u2019st du: ein junger Achill!", "tokens": ["Zo\u00b7g'st", "du", ":", "ein", "jun\u00b7ger", "A\u00b7chill", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und warfest die Hallen, wo feiler Wust", "tokens": ["Und", "war\u00b7fest", "die", "Hal\u00b7len", ",", "wo", "fei\u00b7ler", "Wust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die heiligen Bilder befiel \u2014", "tokens": ["Die", "hei\u00b7li\u00b7gen", "Bil\u00b7der", "be\u00b7fiel"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wo lauernder Schlangen giftspeiender Zahn", "tokens": ["Wo", "lau\u00b7ern\u00b7der", "Schlan\u00b7gen", "gift\u00b7spei\u00b7en\u00b7der", "Zahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Zerrissen, was edel und gro\u00df:", "tokens": ["Zer\u00b7ris\u00b7sen", ",", "was", "e\u00b7del", "und", "gro\u00df", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Du warfest sie nieder! Auf siegender Bahn", "tokens": ["Du", "war\u00b7fest", "sie", "nie\u00b7der", "!", "Auf", "sie\u00b7gen\u00b7der", "Bahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Mit heldenhaft markigem Sto\u00df!", "tokens": ["Mit", "hel\u00b7den\u00b7haft", "mar\u00b7ki\u00b7gem", "Sto\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wir jauchzten dir zu in heiliger Gluth", "tokens": ["Wir", "jauchz\u00b7ten", "dir", "zu", "in", "hei\u00b7li\u00b7ger", "Gluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und griffen zum blitzenden Schwert!", "tokens": ["Und", "grif\u00b7fen", "zum", "blit\u00b7zen\u00b7den", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der feigen Seelen neidische Brut,", "tokens": ["Der", "fei\u00b7gen", "See\u00b7len", "nei\u00b7di\u00b7sche", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Von eklem Staube gen\u00e4hrt:", "tokens": ["Von", "ek\u00b7lem", "Stau\u00b7be", "ge\u00b7n\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wir trieben sie aus! Mit gellendem Schrei", "tokens": ["Wir", "trie\u00b7ben", "sie", "aus", "!", "Mit", "gel\u00b7len\u00b7dem", "Schrei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zerstob die zitternde Schaar! \u2026", "tokens": ["Zer\u00b7stob", "die", "zit\u00b7tern\u00b7de", "Schaar", "!", "\u2026"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Doch weiter, nur weiter! Durch Nebel und Mai,", "tokens": ["Doch", "wei\u00b7ter", ",", "nur", "wei\u00b7ter", "!", "Durch", "Ne\u00b7bel", "und", "Mai", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ADV", "$.", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Umflogen von wirbelndem Haar:", "tokens": ["Um\u00b7flo\u00b7gen", "von", "wir\u00b7beln\u00b7dem", "Haar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "So zogen wir hin auf dampfendem Ro\u00df,", "tokens": ["So", "zo\u00b7gen", "wir", "hin", "auf", "damp\u00b7fen\u00b7dem", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wir K\u00e4mpen f\u00fcr Freiheit und Licht! \u2026", "tokens": ["Wir", "K\u00e4m\u00b7pen", "f\u00fcr", "Frei\u00b7heit", "und", "Licht", "!", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da fiel der hirnversengende Sto\u00df,", "tokens": ["Da", "fiel", "der", "hirn\u00b7ver\u00b7sen\u00b7gen\u00b7de", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der\u2019s Herz mir st\u00fcckweis bricht \u2014", "tokens": ["Der's", "Herz", "mir", "st\u00fcck\u00b7weis", "bricht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es schlich der nackte, der fahle Tod", "tokens": ["Es", "schlich", "der", "nack\u00b7te", ",", "der", "fah\u00b7le", "Tod"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zu deinem Herzen sich hin", "tokens": ["Zu", "dei\u00b7nem", "Her\u00b7zen", "sich", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "APZR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Da lagst du im bleichen Morgenroth \u2014", "tokens": ["Da", "lagst", "du", "im", "blei\u00b7chen", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zerbrochen das Schwert und die Br\u00fcnn\u2019. \u2014", "tokens": ["Zer\u00b7bro\u00b7chen", "das", "Schwert", "und", "die", "Br\u00fcnn'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Zerbrochen die lichte, die jauchzende Brust:", "tokens": ["Zer\u00b7bro\u00b7chen", "die", "lich\u00b7te", ",", "die", "jauch\u00b7zen\u00b7de", "Brust", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Fahr\u2019 wohl, mein Bruder, fahr\u2019 wohl!", "tokens": ["Fahr'", "wohl", ",", "mein", "Bru\u00b7der", ",", "fahr'", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "VVIMP", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Verspr\u00fcht die lodernde Kampfeslust \u2014", "tokens": ["Ver\u00b7spr\u00fcht", "die", "lo\u00b7dern\u00b7de", "Kamp\u00b7fes\u00b7lust"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zertr\u00fcmmert das hehre Idol! \u2026", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mert", "das", "heh\u00b7re", "I\u00b7dol", "!", "\u2026"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir sa\u00dfen und sannen in stummer Qual", "tokens": ["Wir", "sa\u00b7\u00dfen", "und", "san\u00b7nen", "in", "stum\u00b7mer", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und starrten auf deinen Leib \u2014", "tokens": ["Und", "starr\u00b7ten", "auf", "dei\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dann gaben wir ihn, das Antlitz fahl,", "tokens": ["Dann", "ga\u00b7ben", "wir", "ihn", ",", "das", "Ant\u00b7litz", "fahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den W\u00fcrmern zum Zeitvertreib \u2026", "tokens": ["Den", "W\u00fcr\u00b7mern", "zum", "Zeit\u00b7ver\u00b7treib", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Sie m\u00f6gen ihn schmausen in k\u00f6stlichem Mahl \u2014", "tokens": ["Sie", "m\u00f6\u00b7gen", "ihn", "schmau\u00b7sen", "in", "k\u00f6st\u00b7li\u00b7chem", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Leb\u2019 wohl, mein Bruder leb\u2019 wohl!", "tokens": ["Leb'", "wohl", ",", "mein", "Bru\u00b7der", "leb'", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wir k\u00e4mpfen, die Faust im blitzenden Stahl,", "tokens": ["Wir", "k\u00e4mp\u00b7fen", ",", "die", "Faust", "im", "blit\u00b7zen\u00b7den", "Stahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fcr der Freiheit leuchtend Symbol! \u2026", "tokens": ["F\u00fcr", "der", "Frei\u00b7heit", "leuch\u00b7tend", "Sym\u00b7bol", "!", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "NN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und pocht auch der Wahnsinn an unser Hirn \u2014", "tokens": ["Und", "pocht", "auch", "der", "Wahn\u00b7sinn", "an", "un\u00b7ser", "Hirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Nur weiter durch Nebel und Nacht:", "tokens": ["Nur", "wei\u00b7ter", "durch", "Ne\u00b7bel", "und", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Dort fressen die W\u00fcrmer die leuchtende Stirn \u2014", "tokens": ["Dort", "fres\u00b7sen", "die", "W\u00fcr\u00b7mer", "die", "leuch\u00b7ten\u00b7de", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Wir rasen d\u00e4monenumlacht! \u2026", "tokens": ["Wir", "ra\u00b7sen", "d\u00e4\u00b7mo\u00b7nen\u00b7um\u00b7lacht", "!", "\u2026"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Noch rinnt in unsern Adern die Gluth,", "tokens": ["Noch", "rinnt", "in", "un\u00b7sern", "A\u00b7dern", "die", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die alles Hohle zerschl\u00e4gt \u2014", "tokens": ["Die", "al\u00b7les", "Hoh\u00b7le", "zer\u00b7schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Noch packt uns wilder Titanenmuth,", "tokens": ["Noch", "packt", "uns", "wil\u00b7der", "Ti\u00b7ta\u00b7nen\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der auf zum Himmel uns tr\u00e4gt!", "tokens": ["Der", "auf", "zum", "Him\u00b7mel", "uns", "tr\u00e4gt", "!"], "token_info": ["word", "word",