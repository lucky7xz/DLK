{"textgrid.poem.57869": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Nummro zwei", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war eine herbstlich k\u00fchle Nacht,", "tokens": ["Es", "war", "ei\u00b7ne", "herbst\u00b7lich", "k\u00fch\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Nordwind pfiff nicht ohne", "tokens": ["Der", "Nord\u00b7wind", "pfiff", "nicht", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und spielte Tarabumdeay", "tokens": ["Und", "spiel\u00b7te", "Ta\u00b7ra\u00b7bum\u00b7de\u00b7ay"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Draht der Telephone.", "tokens": ["Am", "Draht", "der", "Te\u00b7le\u00b7pho\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kam vom Abendschoppen heim", "tokens": ["Ich", "kam", "vom", "A\u00b7bend\u00b7schop\u00b7pen", "heim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Just um die Geisterstunde,", "tokens": ["Just", "um", "die", "Geis\u00b7ter\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Gaslaternen brannten tr\u00fcb,", "tokens": ["Die", "Gas\u00b7la\u00b7ter\u00b7nen", "brann\u00b7ten", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unheimlich heulten zwei Hunde.", "tokens": ["Un\u00b7heim\u00b7lich", "heul\u00b7ten", "zwei", "Hun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "CARD", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Auf einmal bumste es irgendwo,", "tokens": ["Auf", "ein\u00b7mal", "bums\u00b7te", "es", "ir\u00b7gend\u00b7wo", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es donnerte und es krachte;", "tokens": ["Es", "don\u00b7ner\u00b7te", "und", "es", "krach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich dachte verwundert blo\u00df: nanu?", "tokens": ["Ich", "dach\u00b7te", "ver\u00b7wun\u00b7dert", "blo\u00df", ":", "na\u00b7nu", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ADV", "$.", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor Schreck ich sonst nichts dachte.", "tokens": ["Vor", "Schreck", "ich", "sonst", "nichts", "dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich traf einen Schutzmann, den fragte ich:", "tokens": ["Ich", "traf", "ei\u00b7nen", "Schutz\u00b7mann", ",", "den", "frag\u00b7te", "ich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was war denn das f\u00fcr 'n Knallen?", "tokens": ["Was", "war", "denn", "das", "f\u00fcr", "'n", "Knal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbooch nischt, es ist blo\u00df wieder mal", "tokens": ["\u00bb", "o\u00b7och", "nischt", ",", "es", "ist", "blo\u00df", "wie\u00b7der", "mal"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "'ne Br\u00fccke eingefallen.\u00ab", "tokens": ["'ne", "Br\u00fc\u00b7cke", "ein\u00b7ge\u00b7fal\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}