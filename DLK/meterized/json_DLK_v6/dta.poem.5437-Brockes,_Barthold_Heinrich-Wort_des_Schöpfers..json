{"dta.poem.5437": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wort des Sch\u00f6pfers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir finden in der Schrift: da\u00df aller Himmel Pracht", "tokens": ["Wir", "fin\u00b7den", "in", "der", "Schrift", ":", "da\u00df", "al\u00b7ler", "Him\u00b7mel", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "KOUS", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sey durch das ", "tokens": ["Sey", "durch", "das"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Und alle seine Heere,", "tokens": ["Und", "al\u00b7le", "sei\u00b7ne", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Sonnen, Welte, Land und Meere,", "tokens": ["Als", "Son\u00b7nen", ",", "Wel\u00b7te", ",", "Land", "und", "Mee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch seines Mundes Geist. Ist dieses wahr,", "tokens": ["Durch", "sei\u00b7nes", "Mun\u00b7des", "Geist", ".", "Ist", "die\u00b7ses", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "VAFIN", "PDAT", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie es unstreitig ist; so folget dieses klar:", "tokens": ["Wie", "es", "un\u00b7strei\u00b7tig", "ist", ";", "so", "fol\u00b7get", "die\u00b7ses", "klar", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "$.", "ADV", "VVFIN", "PDAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man sieht an einem jeden Ort", "tokens": ["Man", "sieht", "an", "ei\u00b7nem", "je\u00b7den", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch in der Creatur des HErrn, des Sch\u00f6pfers, ", "tokens": ["Auch", "in", "der", "Crea\u00b7tur", "des", "Herrn", ",", "des", "Sch\u00f6p\u00b7fers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Es hat nicht aufgeh\u00f6rt. Sein ", "tokens": ["Es", "hat", "nicht", "auf\u00b7ge\u00b7h\u00f6rt", ".", "Sein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$.", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wie eines Menschen Wort, das, gleich wie ein Geschrey,", "tokens": ["Wie", "ei\u00b7nes", "Men\u00b7schen", "Wort", ",", "das", ",", "gleich", "wie", "ein", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "PDS", "$,", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dahin, vorbey.", "tokens": ["Da\u00b7hin", ",", "vor\u00b7bey", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["PAV", "$,", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Es schallt in Ewigkeit. Wenn ich die\u00df \u00fcberlege", "tokens": ["Es", "schallt", "in", "E\u00b7wig\u00b7keit", ".", "Wenn", "ich", "die\u00df", "\u00fc\u00b7berl\u00b7e\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "KOUS", "PPER", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und Ehrfurcht voll die\u00df grosse ", "tokens": ["Und", "Ehr\u00b7furcht", "voll", "die\u00df", "gros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "PDS", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "So kommen mir", "tokens": ["So", "kom\u00b7men", "mir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Der Creaturen-Heer\u2019, als Lettern, f\u00fcr,", "tokens": ["Der", "Crea\u00b7tu\u00b7ren\u00b7Heer'", ",", "als", "Let\u00b7tern", ",", "f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "NN", "$,", "APPR", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Die GOtt so wunderbar gef\u00fcget,", "tokens": ["Die", "Gott", "so", "wun\u00b7der\u00b7bar", "ge\u00b7f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Worinn der Sinn des Sch\u00f6pfers lieget,", "tokens": ["Wo\u00b7rinn", "der", "Sinn", "des", "Sch\u00f6p\u00b7fers", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und worinn mehr, als man wol, leider! meint,", "tokens": ["Und", "wo\u00b7rinn", "mehr", ",", "als", "man", "wol", ",", "lei\u00b7der", "!", "meint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "$,", "KOUS", "PIS", "ADV", "$,", "ADV", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Der GOttheit Wesen gl\u00e4ntzt und scheint.", "tokens": ["Der", "Got\u00b7theit", "We\u00b7sen", "gl\u00e4ntzt", "und", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Will man denn nun, bey so bestalten Sachen,", "tokens": ["Will", "man", "denn", "nun", ",", "bey", "so", "be\u00b7stal\u00b7ten", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Nicht vom Gewohnheits-Schlaf erwachen?", "tokens": ["Nicht", "vom", "Ge\u00b7wohn\u00b7heits\u00b7Schlaf", "er\u00b7wa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Will man, zu unsers Sch\u00f6pfers Ehren,", "tokens": ["Will", "man", ",", "zu", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Sein ewigs ", "tokens": ["Sein", "e\u00b7wigs"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Will man die sch\u00f6ne Schrift, die wunder-wunder-sch\u00f6n,", "tokens": ["Will", "man", "die", "sch\u00f6\u00b7ne", "Schrift", ",", "die", "wun\u00b7der\u00b7wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nicht besser, als bisher, mit Lust und Andacht sehn?", "tokens": ["Nicht", "bes\u00b7ser", ",", "als", "bis\u00b7her", ",", "mit", "Lust", "und", "An\u00b7dacht", "sehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "ADV", "$,", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Will man den Innhalt nicht verstehn,", "tokens": ["Will", "man", "den", "Inn\u00b7halt", "nicht", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der anders nichts als br\u00fcnst\u2019ge Triebe", "tokens": ["Der", "an\u00b7ders", "nichts", "als", "br\u00fcnst'\u00b7ge", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIS", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Von einer ewig-weis- und ewig m\u00e4cht\u2019gen Liebe?", "tokens": ["Von", "ei\u00b7ner", "e\u00b7wig\u00b7weis", "und", "e\u00b7wig", "m\u00e4cht'\u00b7gen", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}