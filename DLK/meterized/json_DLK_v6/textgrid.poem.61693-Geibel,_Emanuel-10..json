{"textgrid.poem.61693": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm herein, o Nacht, und k\u00fchle", "tokens": ["Komm", "her\u00b7ein", ",", "o", "Nacht", ",", "und", "k\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Gluten, diesen Schmerz!", "tokens": ["Die\u00b7se", "Glu\u00b7ten", ",", "die\u00b7sen", "Schmerz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Wirrsal der Gef\u00fchle", "tokens": ["Aus", "dem", "Wirr\u00b7sal", "der", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie errett' ich nur mein Herz!", "tokens": ["Wie", "er\u00b7rett'", "ich", "nur", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo wir einst so gl\u00fccklich waren,", "tokens": ["Wo", "wir", "einst", "so", "gl\u00fcck\u00b7lich", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich wieder sie gesehn,", "tokens": ["Hab'", "ich", "wie\u00b7der", "sie", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aufs neue, wie vor Jahren,", "tokens": ["Und", "aufs", "neu\u00b7e", ",", "wie", "vor", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist's um meine Ruh' geschehn.", "tokens": ["Ist's", "um", "mei\u00b7ne", "Ruh'", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lodernd aus der Asche steigen", "tokens": ["Lo\u00b7dernd", "aus", "der", "A\u00b7sche", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flammen, die jetzt Frevel sind;", "tokens": ["Flam\u00b7men", ",", "die", "jetzt", "Fre\u00b7vel", "sind", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie ist nicht mehr ihr eigen,", "tokens": ["Denn", "sie", "ist", "nicht", "mehr", "ihr", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und ist so hold und \u2013 blind.", "tokens": ["Ach", ",", "und", "ist", "so", "hold", "und", "\u2013", "blind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VAFIN", "ADV", "ADJD", "KON", "$(", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weil an ihrer Reinheit Bl\u00fcte", "tokens": ["Weil", "an", "ih\u00b7rer", "Rein\u00b7heit", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nie ein tr\u00fcber Hauch ger\u00fchrt,", "tokens": ["Nie", "ein", "tr\u00fc\u00b7ber", "Hauch", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahnt sie nicht in ihrer G\u00fcte,", "tokens": ["Ahnt", "sie", "nicht", "in", "ih\u00b7rer", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welchen Brand sie l\u00e4chelnd sch\u00fcrt.", "tokens": ["Wel\u00b7chen", "Brand", "sie", "l\u00e4\u00b7chelnd", "sch\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Harmlos zeigt sie, kindlich offen,", "tokens": ["Harm\u00b7los", "zeigt", "sie", ",", "kind\u00b7lich", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich begl\u00fcckt, wenn ich erschien \u2013", "tokens": ["Sich", "be\u00b7gl\u00fcckt", ",", "wenn", "ich", "er\u00b7schien", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber ich, ins Herz getroffen,", "tokens": ["A\u00b7ber", "ich", ",", "ins", "Herz", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, was kann ich tun als fliehn!", "tokens": ["Ach", ",", "was", "kann", "ich", "tun", "als", "fliehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VMFIN", "PPER", "VVINF", "KOKOM", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}