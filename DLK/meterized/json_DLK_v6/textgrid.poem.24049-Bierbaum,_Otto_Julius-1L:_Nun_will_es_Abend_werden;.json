{"textgrid.poem.24049": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun will es Abend werden;", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun will es Abend werden;", "tokens": ["Nun", "will", "es", "A\u00b7bend", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der rote Himmelsstrich,", "tokens": ["Der", "ro\u00b7te", "Him\u00b7mels\u00b7strich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Eros mit dem Pfeilgefieder", "tokens": ["Den", "E\u00b7ros", "mit", "dem", "Pfeil\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gemalt zu haben schien, verblich.", "tokens": ["Ge\u00b7malt", "zu", "ha\u00b7ben", "schien", ",", "ver\u00b7blich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es \u00fcberbr\u00e4unt sich leis der Wald;", "tokens": ["Es", "\u00fc\u00b7berb\u00b7r\u00e4unt", "sich", "leis", "der", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die zarten Birkenst\u00e4mmchen blinken", "tokens": ["Die", "zar\u00b7ten", "Bir\u00b7ken\u00b7st\u00e4mm\u00b7chen", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur graulich silbern noch; es lie\u00df", "tokens": ["Nur", "grau\u00b7lich", "sil\u00b7bern", "noch", ";", "es", "lie\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tag die goldene Krone sinken.", "tokens": ["Der", "Tag", "die", "gol\u00b7de\u00b7ne", "Kro\u00b7ne", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Schnell hebt die neidische Nacht sie auf;", "tokens": ["Schnell", "hebt", "die", "nei\u00b7di\u00b7sche", "Nacht", "sie", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch ihre kalten H\u00e4nde eisen", "tokens": ["Doch", "ih\u00b7re", "kal\u00b7ten", "H\u00e4n\u00b7de", "ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gold zu Silber; durch das Schwarz", "tokens": ["Das", "Gold", "zu", "Sil\u00b7ber", ";", "durch", "das", "Schwarz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Endlosen Raums hebts an, zu glei\u00dfen", "tokens": ["End\u00b7lo\u00b7sen", "Raums", "hebts", "an", ",", "zu", "glei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Da rauscht sie feuchteschwer heran.", "tokens": ["Da", "rauscht", "sie", "feuch\u00b7te\u00b7schwer", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Von schwarzem Riesenschwangespann", "tokens": ["Von", "schwar\u00b7zem", "Rie\u00b7sen\u00b7schwan\u00b7ge\u00b7spann"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird durch das Luftmeer sie getragen.", "tokens": ["Wird", "durch", "das", "Luft\u00b7meer", "sie", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lehnt in breitem Muschelwagen.", "tokens": ["Sie", "lehnt", "in", "brei\u00b7tem", "Mu\u00b7schel\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Erst h\u00e4lt sie, still, am Horizont,", "tokens": ["Erst", "h\u00e4lt", "sie", ",", "still", ",", "am", "Ho\u00b7ri\u00b7zont", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der purpurgl\u00fch sich ausgesonnt.", "tokens": ["Der", "pur\u00b7pur\u00b7gl\u00fch", "sich", "aus\u00b7ge\u00b7sonnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann breitet seinen Fittich weit", "tokens": ["Dann", "brei\u00b7tet", "sei\u00b7nen", "Fit\u00b7tich", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der schwarze Schwan, schwimmflugbereit.", "tokens": ["Der", "schwar\u00b7ze", "Schwan", ",", "schwimm\u00b7flug\u00b7be\u00b7reit", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihre Arme hebt die Nacht ...", "tokens": ["Und", "ih\u00b7re", "Ar\u00b7me", "hebt", "die", "Nacht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das All ist dunkel\u00fcberdacht.", "tokens": ["Das", "All", "ist", "dun\u00b7kel\u00b7\u00fc\u00b7berd\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nur noch das Schwanenfittichwehn,", "tokens": ["Nur", "noch", "das", "Schwa\u00b7nen\u00b7fit\u00b7tich\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Br\u00fcsteaufundniedergehn", "tokens": ["Das", "Br\u00fcs\u00b7te\u00b7au\u00b7fund\u00b7nie\u00b7der\u00b7gehn"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der stummen Riesin h\u00f6rt die Welt,", "tokens": ["Der", "stum\u00b7men", "Rie\u00b7sin", "h\u00f6rt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die m\u00fcdebang den Atem h\u00e4lt.", "tokens": ["Die", "m\u00fc\u00b7de\u00b7bang", "den", "A\u00b7tem", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}