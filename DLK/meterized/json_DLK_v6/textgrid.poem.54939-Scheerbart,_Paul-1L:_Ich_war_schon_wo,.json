{"textgrid.poem.54939": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich war schon wo,", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war schon wo,", "tokens": ["Ich", "war", "schon", "wo", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PWAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da ging es w\u00fcste zu;", "tokens": ["Da", "ging", "es", "w\u00fcs\u00b7te", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hatte weder Hemd noch Schuh,", "tokens": ["Ich", "hat\u00b7te", "we\u00b7der", "Hemd", "noch", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur gr\u00fcne Schlangen", "tokens": ["Nur", "gr\u00fc\u00b7ne", "Schlan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "In beiden H\u00e4nden.", "tokens": ["In", "bei\u00b7den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ich konnte mich nicht drehen", "tokens": ["Ich", "konn\u00b7te", "mich", "nicht", "dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und nicht wenden.", "tokens": ["Und", "nicht", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Doch viele Beutelsterne", "tokens": ["Doch", "vie\u00b7le", "Beu\u00b7tel\u00b7ster\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Drehten sich um meine Arme", "tokens": ["Dreh\u00b7ten", "sich", "um", "mei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und sahen aus", "tokens": ["Und", "sa\u00b7hen", "aus"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Wie schlaffe Luftballons.", "tokens": ["Wie", "schlaf\u00b7fe", "Luft\u00b7bal\u00b7lons", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die Schlangen aber sangen.", "tokens": ["Die", "Schlan\u00b7gen", "a\u00b7ber", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}