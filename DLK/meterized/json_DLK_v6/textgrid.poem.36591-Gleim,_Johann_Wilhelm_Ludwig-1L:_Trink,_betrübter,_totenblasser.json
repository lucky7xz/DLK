{"textgrid.poem.36591": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trink, betr\u00fcbter, totenblasser", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trink, betr\u00fcbter, totenblasser", "tokens": ["Trink", ",", "be\u00b7tr\u00fcb\u00b7ter", ",", "to\u00b7ten\u00b7blas\u00b7ser"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wassertrinker, Rebenhasser,", "tokens": ["Was\u00b7ser\u00b7trin\u00b7ker", ",", "Re\u00b7ben\u00b7has\u00b7ser", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trink doch Wein!", "tokens": ["Trink", "doch", "Wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Deine Wangen wirst du f\u00e4rben,", "tokens": ["Dei\u00b7ne", "Wan\u00b7gen", "wirst", "du", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weiser werden, sp\u00e4ter sterben,", "tokens": ["Wei\u00b7ser", "wer\u00b7den", ",", "sp\u00e4\u00b7ter", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00fccklich sein.", "tokens": ["Gl\u00fcck\u00b7lich", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Habt, ihr gro\u00dfen G\u00f6tter! habet", "tokens": ["Habt", ",", "ihr", "gro\u00b7\u00dfen", "G\u00f6t\u00b7ter", "!", "ha\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$.", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr den Trank, den ihr uns gabet,", "tokens": ["F\u00fcr", "den", "Trank", ",", "den", "ihr", "uns", "ga\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Habet Dank!", "tokens": ["Ha\u00b7bet", "Dank", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "O wie dampft er in die Nase!", "tokens": ["O", "wie", "dampft", "er", "in", "die", "Na\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "O wie sprudelt er im Glase!", "tokens": ["O", "wie", "spru\u00b7delt", "er", "im", "Gla\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welch ein Trank!", "tokens": ["Welch", "ein", "Trank", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Alle Sorgen, alle Schmerzen", "tokens": ["Al\u00b7le", "Sor\u00b7gen", ",", "al\u00b7le", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6tet er, und alle Herzen", "tokens": ["T\u00f6\u00b7tet", "er", ",", "und", "al\u00b7le", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht er froh.", "tokens": ["Macht", "er", "froh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Durstig sang zu seinem Preise", "tokens": ["Durs\u00b7tig", "sang", "zu", "sei\u00b7nem", "Prei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses schon der gro\u00dfe Weise,", "tokens": ["Die\u00b7ses", "schon", "der", "gro\u00b7\u00dfe", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Salomo!", "tokens": ["Sa\u00b7lo\u00b7mo", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "O! es m\u00fcssen alle Weisen,", "tokens": ["O", "!", "es", "m\u00fcs\u00b7sen", "al\u00b7le", "Wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O! es mu\u00df ihn jeder preisen,", "tokens": ["O", "!", "es", "mu\u00df", "ihn", "je\u00b7der", "prei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der ihn trinkt!", "tokens": ["Der", "ihn", "trinkt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Finster, gr\u00e4mlich, menschenfeindlich", "tokens": ["Fins\u00b7ter", ",", "gr\u00e4m\u00b7lich", ",", "men\u00b7schen\u00b7feind\u00b7lich"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4\u00dft er keinen! Seht, wie freundlich", "tokens": ["L\u00e4\u00dft", "er", "kei\u00b7nen", "!", "Seht", ",", "wie", "freund\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "$.", "VVFIN", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er mir winkt!", "tokens": ["Er", "mir", "winkt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Siehe, spricht der Rebenhasser,", "tokens": ["Sie\u00b7he", ",", "spricht", "der", "Re\u00b7ben\u00b7has\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie so freundlich da mein Wasser", "tokens": ["Wie", "so", "freund\u00b7lich", "da", "mein", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir auch winkt!", "tokens": ["Mir", "auch", "winkt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ernster Weisheit bleibt ergeben,", "tokens": ["Erns\u00b7ter", "Weis\u00b7heit", "bleibt", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ein Feind vom Saft der Reben", "tokens": ["Wenn", "ein", "Feind", "vom", "Saft", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wasser trinkt.", "tokens": ["Was\u00b7ser", "trinkt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Wasser, immer magst du winken,", "tokens": ["Was\u00b7ser", ",", "im\u00b7mer", "magst", "du", "win\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer zu klug ist, Wein zu trinken,", "tokens": ["Wer", "zu", "klug", "ist", ",", "Wein", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKA", "ADJD", "VAFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinke dich!", "tokens": ["Trin\u00b7ke", "dich", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wasser weg von meinem Tische", "tokens": ["Was\u00b7ser", "weg", "von", "mei\u00b7nem", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du geh\u00f6rest f\u00fcr die Fische,", "tokens": ["Du", "ge\u00b7h\u00f6\u00b7rest", "f\u00fcr", "die", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht f\u00fcr mich!", "tokens": ["Nicht", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}