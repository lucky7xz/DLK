{"dta.poem.352": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Deutliche Liebes-Erkl\u00e4hrung.  \n Aus dem Frantz\u00f6sischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nheit ist gemacht/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "ist", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gleich da man euch gezeuget/", "tokens": ["Gleich", "da", "man", "euch", "ge\u00b7zeu\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PPER", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und die zu Weg\u2019 gebracht/", "tokens": ["Und", "die", "zu", "Weg'", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die mir mein Hertze beuget.", "tokens": ["Die", "mir", "mein", "Hert\u00b7ze", "beu\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lasset mir die Gnade zu/", "tokens": ["Las\u00b7set", "mir", "die", "Gna\u00b7de", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "$("], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Da\u00df ich euch dasselbe thu/", "tokens": ["Da\u00df", "ich", "euch", "das\u00b7sel\u00b7be", "thu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PDAT", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was man eurer Mutter that", "tokens": ["Was", "man", "eu\u00b7rer", "Mut\u00b7ter", "that"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als man euch gemachet hat.", "tokens": ["Als", "man", "euch", "ge\u00b7ma\u00b7chet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}