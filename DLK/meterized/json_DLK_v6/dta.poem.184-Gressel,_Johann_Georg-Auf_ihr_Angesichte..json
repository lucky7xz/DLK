{"dta.poem.184": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihr Angesichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Angesicht dem lichten Himmel gleichet", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "dem", "lich\u00b7ten", "Him\u00b7mel", "glei\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Augen-Paar mit sanfften Strahlen blitzt!", "tokens": ["Das", "Au\u00b7gen\u00b7Paar", "mit", "sanff\u00b7ten", "Strah\u00b7len", "blitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Das Wolcken-Feld den glatten Wangen weichet;", "tokens": ["Das", "Wol\u00b7cken\u00b7Feld", "den", "glat\u00b7ten", "Wan\u00b7gen", "wei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es \u00fcbertrifft/ des Himmels klahres Licht/", "tokens": ["Es", "\u00fc\u00b7bert\u00b7rifft", "/", "des", "Him\u00b7mels", "klah\u00b7res", "Licht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dein Angesicht.", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Purpur-Schein der ungeschminckten Wangen", "tokens": ["Der", "Pur\u00b7pur\u00b7Schein", "der", "un\u00b7ge\u00b7schminck\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der sticht hinweg ", "tokens": ["Der", "sticht", "hin\u00b7weg"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APZR"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Das Abend-Roth verliehret Pracht und Glantz", "tokens": ["Das", "A\u00b7ben\u00b7dRoth", "ver\u00b7lieh\u00b7ret", "Pracht", "und", "Glantz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vor den Rubin/ damit die Lippen prangen.", "tokens": ["Vor", "den", "Ru\u00b7bin", "/", "da\u00b7mit", "die", "Lip\u00b7pen", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$(", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Den Himmel l\u00e4st nicht Obermeister seyn", "tokens": ["Den", "Him\u00b7mel", "l\u00e4st", "nicht", "O\u00b7ber\u00b7meis\u00b7ter", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "NN", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Purpur-Schein.", "tokens": ["Der", "Pur\u00b7pur\u00b7Schein", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}