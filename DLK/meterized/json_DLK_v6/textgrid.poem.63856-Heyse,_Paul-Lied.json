{"textgrid.poem.63856": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wie kann's zur Freude taugen,", "tokens": ["Ach", ",", "wie", "kann's", "zur", "Freu\u00b7de", "tau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VMFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam seines Wegs zu gehn!", "tokens": ["Ein\u00b7sam", "sei\u00b7nes", "Wegs", "zu", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter z\u00e4rtlichen vier Augen", "tokens": ["Un\u00b7ter", "z\u00e4rt\u00b7li\u00b7chen", "vier", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die Welt noch eins so sch\u00f6n.", "tokens": ["Ist", "die", "Welt", "noch", "eins", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PIS", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kann ein Herz doch nicht erwarmen", "tokens": ["Kann", "ein", "Herz", "doch", "nicht", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einzig an der Sonne Glut.", "tokens": ["Ein\u00b7zig", "an", "der", "Son\u00b7ne", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter z\u00e4rtlichen vier Armen", "tokens": ["Un\u00b7ter", "z\u00e4rt\u00b7li\u00b7chen", "vier", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lodert wonnevoll das Blut.", "tokens": ["Lo\u00b7dert", "won\u00b7ne\u00b7voll", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Brandung an den Klippen", "tokens": ["Und", "die", "Bran\u00b7dung", "an", "den", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Lebens h\u00f6rst du nicht,", "tokens": ["Die\u00b7ses", "Le\u00b7bens", "h\u00f6rst", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn mit z\u00e4rtlichen vier Lippen", "tokens": ["Wenn", "mit", "z\u00e4rt\u00b7li\u00b7chen", "vier", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seele stumm zu Seele spricht.", "tokens": ["See\u00b7le", "stumm", "zu", "See\u00b7le", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}