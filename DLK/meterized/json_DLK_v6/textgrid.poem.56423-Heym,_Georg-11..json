{"textgrid.poem.56423": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun stirbt auch er, vom bittren Los bezwungen.", "tokens": ["Nun", "stirbt", "auch", "er", ",", "vom", "bit\u00b7tren", "Los", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf seine Beute st\u00fcrzt ihn Todes Macht.", "tokens": ["Auf", "sei\u00b7ne", "Beu\u00b7te", "st\u00fcrzt", "ihn", "To\u00b7des", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verliebten gleich in s\u00fc\u00dfer Liebesnacht,", "tokens": ["Ver\u00b7lieb\u00b7ten", "gleich", "in", "s\u00fc\u00b7\u00dfer", "Lie\u00b7bes\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Tode halten sie sich eng umschlungen.", "tokens": ["Im", "To\u00b7de", "hal\u00b7ten", "sie", "sich", "eng", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Unz\u00e4hlge Geier schweben auf der Schlacht,", "tokens": ["Un\u00b7z\u00e4hl\u00b7ge", "Gei\u00b7er", "schwe\u00b7ben", "auf", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf jeden Fels der Berge hingeschwungen.", "tokens": ["Auf", "je\u00b7den", "Fels", "der", "Ber\u00b7ge", "hin\u00b7ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sp\u00e4hen sorgsam in die Niederungen.", "tokens": ["Sie", "sp\u00e4\u00b7hen", "sorg\u00b7sam", "in", "die", "Nie\u00b7de\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Schauspiels W\u00e4chter halten stumm sie Wacht.", "tokens": ["Des", "Schau\u00b7spiels", "W\u00e4ch\u00b7ter", "hal\u00b7ten", "stumm", "sie", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ADJD", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie sich die Menge dr\u00e4ngt in die Arenen,", "tokens": ["Wie", "sich", "die", "Men\u00b7ge", "dr\u00e4ngt", "in", "die", "A\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So fliegen neue stets von Meer und Land.", "tokens": ["So", "flie\u00b7gen", "neu\u00b7e", "stets", "von", "Meer", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon grau von ihnen sind der Berge Lehnen.", "tokens": ["Schon", "grau", "von", "ih\u00b7nen", "sind", "der", "Ber\u00b7ge", "Leh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Von Asiens K\u00fcste kamen sie zum Feste,", "tokens": ["Von", "A\u00b7siens", "K\u00fcs\u00b7te", "ka\u00b7men", "sie", "zum", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da sie den Blutgeruch im Wind erkannt,", "tokens": ["Da", "sie", "den", "Blut\u00b7ge\u00b7ruch", "im", "Wind", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der gro\u00dfen Tafeln f\u00fcrchterliche G\u00e4ste.", "tokens": ["Der", "gro\u00b7\u00dfen", "Ta\u00b7feln", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}