{"textgrid.poem.32711": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Daphnens Engel, als sie schlief", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlummre, M\u00e4dchen, schlummre s\u00fc\u00df!", "tokens": ["Schlumm\u00b7re", ",", "M\u00e4d\u00b7chen", ",", "schlumm\u00b7re", "s\u00fc\u00df", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4ume nur von Seligkeiten,", "tokens": ["Tr\u00e4u\u00b7me", "nur", "von", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, in Gottes Paradies,", "tokens": ["Die", ",", "in", "Got\u00b7tes", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Br\u00fcder dir bereiten!", "tokens": ["Mei\u00b7ne", "Br\u00fc\u00b7der", "dir", "be\u00b7rei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter mancher guten That", "tokens": ["Un\u00b7ter", "man\u00b7cher", "gu\u00b7ten", "That"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist der Tag dir hingeflossen,", "tokens": ["Ist", "der", "Tag", "dir", "hin\u00b7ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit Gottgedanken hat", "tokens": ["Und", "mit", "Gott\u00b7ge\u00b7dan\u00b7ken", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich dein Auge zugeschlossen.", "tokens": ["Sich", "dein", "Au\u00b7ge", "zu\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Arme Br\u00fcder speistest du;", "tokens": ["Ar\u00b7me", "Br\u00fc\u00b7der", "speis\u00b7test", "du", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mildertest des Waisen Leiden;", "tokens": ["Mil\u00b7der\u00b7test", "des", "Wai\u00b7sen", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprachst dem Kranken Tr\u00f6stung zu,", "tokens": ["Sprachst", "dem", "Kran\u00b7ken", "Tr\u00f6s\u00b7tung", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und belebtest ihn mit Freuden;", "tokens": ["Und", "be\u00b7leb\u00b7test", "ihn", "mit", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Warst, an stiller Sittsamkeit,", "tokens": ["Warst", ",", "an", "stil\u00b7ler", "Sitt\u00b7sam\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Allen Freundinnen Exempel,", "tokens": ["Al\u00b7len", "Freun\u00b7din\u00b7nen", "Ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und ein Bild der Fr\u00f6mmigkeit", "tokens": ["Und", "ein", "Bild", "der", "Fr\u00f6m\u00b7mig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Gottheit stillem Tempel.", "tokens": ["In", "der", "Got\u00b7theit", "stil\u00b7lem", "Tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jede deiner Stunden ist", "tokens": ["Je\u00b7de", "dei\u00b7ner", "Stun\u00b7den", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Segen allen Erdens\u00f6hnen;", "tokens": ["Se\u00b7gen", "al\u00b7len", "Er\u00b7den\u00b7s\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur Amyntens Auge flie\u00dft", "tokens": ["Nur", "A\u00b7myn\u00b7tens", "Au\u00b7ge", "flie\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NE", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinetwillen noch in Thr\u00e4nen.", "tokens": ["Dei\u00b7net\u00b7wil\u00b7len", "noch", "in", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oft, mit Trauer angef\u00fcllt,", "tokens": ["Oft", ",", "mit", "Trau\u00b7er", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sah ich seine Seele schmachten,", "tokens": ["Sah", "ich", "sei\u00b7ne", "See\u00b7le", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, in Wehmut eingeh\u00fcllt,", "tokens": ["Und", ",", "in", "Weh\u00b7mut", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen Engel ihn betrachten.", "tokens": ["Sei\u00b7nen", "En\u00b7gel", "ihn", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! der Engel ist mein Freund,", "tokens": ["Ach", "!", "der", "En\u00b7gel", "ist", "mein", "Freund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der J\u00fcngling dir ergeben.", "tokens": ["Und", "der", "J\u00fcng\u00b7ling", "dir", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche Wonne! wenn vereint", "tokens": ["Wel\u00b7che", "Won\u00b7ne", "!", "wenn", "ver\u00b7eint"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$.", "KOUS", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir euch leiteten durchs Leben;", "tokens": ["Wir", "euch", "lei\u00b7te\u00b7ten", "durchs", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn, am heiligen Altar,", "tokens": ["Wenn", ",", "am", "hei\u00b7li\u00b7gen", "Al\u00b7tar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Palmen euer Haar umschl\u00e4ngen,", "tokens": ["Pal\u00b7men", "eu\u00b7er", "Haar", "um\u00b7schl\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von aller Engel Schar", "tokens": ["Und", "von", "al\u00b7ler", "En\u00b7gel", "Schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Segenslieder euch erkl\u00e4ngen!", "tokens": ["Se\u00b7gens\u00b7lie\u00b7der", "euch", "er\u00b7kl\u00e4n\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft, im stillen, w\u00fcrdet ihr", "tokens": ["Oft", ",", "im", "stil\u00b7len", ",", "w\u00fcr\u00b7det", "ihr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "APPRART", "ADJA", "$,", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfe Freudenthr\u00e4nen weinen;", "tokens": ["S\u00fc\u00b7\u00dfe", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft, euch dankend, w\u00fcrden wir", "tokens": ["Oft", ",", "euch", "dan\u00b7kend", ",", "w\u00fcr\u00b7den", "wir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PPER", "ADJD", "$,", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In Gesichten euch erscheinen;", "tokens": ["In", "Ge\u00b7sich\u00b7ten", "euch", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freuden aus dem Himmelreich", "tokens": ["Freu\u00b7den", "aus", "dem", "Him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In die fromme Seele strahlen,", "tokens": ["In", "die", "from\u00b7me", "See\u00b7le", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Zukunft Bilder euch", "tokens": ["Und", "der", "Zu\u00b7kunft", "Bil\u00b7der", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus dem Paradiese malen.", "tokens": ["Aus", "dem", "Pa\u00b7ra\u00b7die\u00b7se", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wachend w\u00fcrdest du vom Traum", "tokens": ["Wa\u00b7chend", "w\u00fcr\u00b7dest", "du", "vom", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm die Freuden alle nennen;", "tokens": ["Ihm", "die", "Freu\u00b7den", "al\u00b7le", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Staunend w\u00fcrd' in seinem Traum", "tokens": ["Stau\u00b7nend", "w\u00fcrd'", "in", "sei\u00b7nem", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er den deinigen erkennen,", "tokens": ["Er", "den", "dei\u00b7ni\u00b7gen", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPOSS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Still des Bildes Deutung sich", "tokens": ["Still", "des", "Bil\u00b7des", "Deu\u00b7tung", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In geheimer Brust entschlie\u00dfen,", "tokens": ["In", "ge\u00b7hei\u00b7mer", "Brust", "ent\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, mit Ahnungsthr\u00e4nen, dich", "tokens": ["Und", ",", "mit", "Ah\u00b7nungs\u00b7thr\u00e4\u00b7nen", ",", "dich"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$,", "APPR", "NN", "$,", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unaussprechlich z\u00e4rtlich k\u00fcssen.", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7lich", "z\u00e4rt\u00b7lich", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn dann dir zum zweitenmal", "tokens": ["Wenn", "dann", "dir", "zum", "zwei\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPRART", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine bange Thr\u00e4ne fl\u00f6sse,", "tokens": ["Sei\u00b7ne", "ban\u00b7ge", "Thr\u00e4\u00b7ne", "fl\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von Gottes Thron ein Strahl", "tokens": ["Und", "von", "Got\u00b7tes", "Thron", "ein", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir das Paradies entschl\u00f6sse:", "tokens": ["Dir", "das", "Pa\u00b7ra\u00b7dies", "ent\u00b7schl\u00f6s\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O, dann w\u00fcrd' ich noch als Freund", "tokens": ["O", ",", "dann", "w\u00fcrd'", "ich", "noch", "als", "Freund"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "ADV", "KOUS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00f6stend um den Gatten weilen,", "tokens": ["Tr\u00f6s\u00b7tend", "um", "den", "Gat\u00b7ten", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, wenn hier er ausgeweint,", "tokens": ["Und", ",", "wenn", "hier", "er", "aus\u00b7ge\u00b7weint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Dir mit ihm entgegeneilen!", "tokens": ["Dir", "mit", "ihm", "ent\u00b7ge\u00b7ge\u00b7nei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}