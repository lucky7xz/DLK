{"textgrid.poem.32482": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Auf des Herrn K* Gedanken", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K* unternimmt ein schwer Gesch\u00e4fte,", "tokens": ["K", "*", "un\u00b7ter\u00b7nimmt", "ein", "schwer", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "XY", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Welt zum Unterricht.", "tokens": ["Der", "Welt", "zum", "Un\u00b7ter\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sch\u00e4tzet die lebendgen Kr\u00e4fte,", "tokens": ["Er", "sch\u00e4t\u00b7zet", "die", "le\u00b7bend\u00b7gen", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur seine sch\u00e4tzt er nicht.", "tokens": ["Nur", "sei\u00b7ne", "sch\u00e4tzt", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}