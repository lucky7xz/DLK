{"textgrid.poem.45605": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Edler Wetteifer", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Nord und West war l\u00e4ngst im Streit,", "tokens": ["Der", "Nord", "und", "West", "war", "l\u00e4ngst", "im", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer l\u00e4ndergewaltger und m\u00e4chtiger,", "tokens": ["Wer", "l\u00e4n\u00b7der\u00b7ge\u00b7walt\u00b7ger", "und", "m\u00e4ch\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJA", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Doch friedlicher ist ihr Wettkampf heut,", "tokens": ["Doch", "fried\u00b7li\u00b7cher", "ist", "ihr", "Wett\u00b7kampf", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wer von beiden niedertr\u00e4chtiger.", "tokens": ["Wer", "von", "bei\u00b7den", "nie\u00b7der\u00b7tr\u00e4ch\u00b7ti\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PIAT", "ADJA", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}