ic.di"}, "line.9": {"text": "Und vorsichtig", "tokens": ["Und", "vor\u00b7sich\u00b7tig"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "La\u00df uns bleiben,", "tokens": ["La\u00df", "uns", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Weil wir hier das Bergwerk treiben.", "tokens": ["Weil", "wir", "hier", "das", "Berg\u00b7werk", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schenk uns nur, allerh\u00f6chster Hort!", "tokens": ["Schenk", "uns", "nur", ",", "al\u00b7ler\u00b7h\u00f6chs\u00b7ter", "Hort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Christus hat gef\u00f6rdert dort", "tokens": ["Was", "Chris\u00b7tus", "hat", "ge\u00b7f\u00f6r\u00b7dert", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "VAFIN", "VVPP", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus seiner Leidens-Grube,", "tokens": ["Aus", "sei\u00b7ner", "Lei\u00b7dens\u00b7Gru\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da er zum Lebens-Gange brach,", "tokens": ["Da", "er", "zum", "Le\u00b7bens\u00b7Gan\u00b7ge", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hie\u00df uns alle folgen na