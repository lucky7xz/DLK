{"textgrid.poem.37872": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Romanze vom gro\u00dfen Bergbau der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! richtet Augen, Herz und Sinn", "tokens": ["Auf", "!", "rich\u00b7tet", "Au\u00b7gen", ",", "Herz", "und", "Sinn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu jenen blauen Bergen hin,", "tokens": ["Zu", "je\u00b7nen", "blau\u00b7en", "Ber\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da Gott der Berg-Herr thronet!", "tokens": ["Da", "Gott", "der", "Ber\u00b7gHerr", "thro\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fahrt von der Erde tiefen Bahn", "tokens": ["Fahrt", "von", "der", "Er\u00b7de", "tie\u00b7fen", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "In gr\u00fcnen Hoffnungs-Kleidern an,", "tokens": ["In", "gr\u00fc\u00b7nen", "Hoff\u00b7nungs\u00b7Klei\u00b7dern", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo milder Segen wohnet;", "tokens": ["Wo", "mil\u00b7der", "Se\u00b7gen", "woh\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Betet, tretet", "tokens": ["Be\u00b7tet", ",", "tre\u00b7tet"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Im Gem\u00fcthe", "tokens": ["Im", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Zu der G\u00fcte,", "tokens": ["Zu", "der", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.10": {"text": "Die bescheret,", "tokens": ["Die", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Was den Leib und Geist ern\u00e4hret.", "tokens": ["Was", "den", "Leib", "und", "Geist", "er\u00b7n\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott hat in diesem Erdenball", "tokens": ["Gott", "hat", "in", "die\u00b7sem", "Er\u00b7den\u00b7ball"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So mancher Erze reichen Fall", "tokens": ["So", "man\u00b7cher", "Er\u00b7ze", "rei\u00b7chen", "Fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit weiser Hand verborgen.", "tokens": ["Mit", "wei\u00b7ser", "Hand", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gold, Silber, Kupfer auf sein Wort,", "tokens": ["Gold", ",", "Sil\u00b7ber", ",", "Kup\u00b7fer", "auf", "sein", "Wort", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Streicht in den edlen G\u00e4ngen fort,", "tokens": ["Streicht", "in", "den", "ed\u00b7len", "G\u00e4n\u00b7gen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Menschen zu versorgen,", "tokens": ["Die", "Men\u00b7schen", "zu", "ver\u00b7sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "M\u00e4chtig, pr\u00e4chtig", "tokens": ["M\u00e4ch\u00b7tig", ",", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Durch die Fl\u00f6tzen", "tokens": ["Durch", "die", "Fl\u00f6t\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Hei\u00dft er setzen", "tokens": ["Hei\u00dft", "er", "set\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Die Metallen,", "tokens": ["Die", "Me\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Da\u00df sein Ruhm mu\u00df herrlich schallen.", "tokens": ["Da\u00df", "sein", "Ruhm", "mu\u00df", "herr\u00b7lich", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es sieht so manches rauhe Land", "tokens": ["Es", "sieht", "so", "man\u00b7ches", "rau\u00b7he", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Werken seiner Wunder-Hand", "tokens": ["In", "Wer\u00b7ken", "sei\u00b7ner", "Wun\u00b7der\u00b7Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Macht, Kraft und Weisheit spielen,", "tokens": ["Macht", ",", "Kraft", "und", "Weis\u00b7heit", "spie\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo man kein zartes Bl\u00fcmchen sp\u00fcrt,", "tokens": ["Wo", "man", "kein", "zar\u00b7tes", "Bl\u00fcm\u00b7chen", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Fr\u00fchlings-Gras sich gr\u00fcn auff\u00fchrt,", "tokens": ["Kein", "Fr\u00fch\u00b7lings\u00b7Gras", "sich", "gr\u00fcn", "auf\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df die Natur erzielen,", "tokens": ["Mu\u00df", "die", "Na\u00b7tur", "er\u00b7zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Lichte, dichte", "tokens": ["Lich\u00b7te", ",", "dich\u00b7te"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Berggeschicke", "tokens": ["Berg\u00b7ge\u00b7schi\u00b7cke"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Zum Gel\u00fccke,", "tokens": ["Zum", "Ge\u00b7l\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Die erweisen,", "tokens": ["Die", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Wie man soll den Sch\u00f6pfer preisen.", "tokens": ["Wie", "man", "soll", "den", "Sch\u00f6p\u00b7fer", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Es streicht in diesem Erdenhaus", "tokens": ["Es", "streicht", "in", "die\u00b7sem", "Er\u00b7den\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Erz zu hellen Tage aus", "tokens": ["Im", "Erz", "zu", "hel\u00b7len", "Ta\u00b7ge", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des gro\u00dfen Vaters Liebe,", "tokens": ["Des", "gro\u00b7\u00dfen", "Va\u00b7ters", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wittert vor bei Tag und Nacht,", "tokens": ["Die", "wit\u00b7tert", "vor", "bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus jeden Stollen, Kluft und Schacht;", "tokens": ["Aus", "je\u00b7den", "Stol\u00b7len", ",", "Kluft", "und", "Schacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die weissen Quarzgeschiebe", "tokens": ["Die", "weis\u00b7sen", "Quarz\u00b7ge\u00b7schie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geben eben", "tokens": ["Ge\u00b7ben", "e\u00b7ben"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Wie die G\u00e4nge", "tokens": ["Wie", "die", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Durch die Menge", "tokens": ["Durch", "die", "Men\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Zu erkennen,", "tokens": ["Zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Was wir Vater-G\u00fcte nennen.", "tokens": ["Was", "wir", "Va\u00b7ter\u00b7G\u00fc\u00b7te", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn da sieht ihren milden Gott", "tokens": ["Denn", "da", "sieht", "ih\u00b7ren", "mil\u00b7den", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Armuth nach dem herben Spott,", "tokens": ["Die", "Ar\u00b7muth", "nach", "dem", "her\u00b7ben", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und vielen Z\u00e4hren-Triefen.", "tokens": ["Und", "vie\u00b7len", "Z\u00e4h\u00b7ren\u00b7Trie\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn das Verm\u00f6gen ist verw\u00fcst,", "tokens": ["Wenn", "das", "Ver\u00b7m\u00f6\u00b7gen", "ist", "ver\u00b7w\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und alle Mittel zugeb\u00fc\u00dft,", "tokens": ["Und", "al\u00b7le", "Mit\u00b7tel", "zu\u00b7ge\u00b7b\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kommt aus der schwarzen Tiefen", "tokens": ["Kommt", "aus", "der", "schwar\u00b7zen", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Letzlich, pl\u00f6tzlich", "tokens": ["Letz\u00b7lich", ",", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Reiche Beute", "tokens": ["Rei\u00b7che", "Beu\u00b7te"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "F\u00fcr die Leute,", "tokens": ["F\u00fcr", "die", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Die vertrauen", "tokens": ["Die", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Gott, und gl\u00e4ubig auf ihn bauen.", "tokens": ["Gott", ",", "und", "gl\u00e4u\u00b7big", "auf", "ihn", "bau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum rufen wir auch diesen an,", "tokens": ["Drum", "ru\u00b7fen", "wir", "auch", "die\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PDAT", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der f\u00fcndige Gebirge kann", "tokens": ["Der", "f\u00fcn\u00b7di\u00b7ge", "Ge\u00b7bir\u00b7ge", "kann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er\u00f6ffnen und erhalten;", "tokens": ["Er\u00b7\u00f6ff\u00b7nen", "und", "er\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er wolle mit der Segens-Hand", "tokens": ["Er", "wol\u00b7le", "mit", "der", "Se\u00b7gens\u00b7Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch \u00fcber unser Sachsenland", "tokens": ["Auch", "\u00fc\u00b7ber", "un\u00b7ser", "Sach\u00b7sen\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Forthin gen\u00e4dig walten;", "tokens": ["For\u00b7thin", "ge\u00b7n\u00e4\u00b7dig", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "H\u00f6ren, Lehren,", "tokens": ["H\u00f6\u00b7ren", ",", "Leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Wenn wir sch\u00fcrfen", "tokens": ["Wenn", "wir", "sch\u00fcr\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und bed\u00fcrfen", "tokens": ["Und", "be\u00b7d\u00fcr\u00b7fen"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "H\u00fclf und Rathen,", "tokens": ["H\u00fclf", "und", "Ra\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Sonst ist nichts mit unsern Thaten.", "tokens": ["Sonst", "ist", "nichts", "mit", "un\u00b7sern", "Tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O gro\u00dfer Grundherr aller Welt!", "tokens": ["O", "gro\u00b7\u00dfer", "Grund\u00b7herr", "al\u00b7ler", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil deine Vorsicht uns erh\u00e4lt", "tokens": ["Weil", "dei\u00b7ne", "Vor\u00b7sicht", "uns", "er\u00b7h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch von der Erden Sch\u00e4tzen;", "tokens": ["Auch", "von", "der", "Er\u00b7den", "Sch\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bescheere gutes Erz allhier,", "tokens": ["Be\u00b7schee\u00b7re", "gu\u00b7tes", "Erz", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und la\u00df die G\u00e4nge, Macht und Zier", "tokens": ["Und", "la\u00df", "die", "G\u00e4n\u00b7ge", ",", "Macht", "und", "Zier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In ewge Teufen setzen.", "tokens": ["In", "ew\u00b7ge", "Teu\u00b7fen", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kl\u00fcglich, t\u00fcglich", "tokens": ["Kl\u00fcg\u00b7lich", ",", "t\u00fcg\u00b7lich"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "La\u00df uns bauen", "tokens": ["La\u00df", "uns", "bau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF"], "meter": "+-+-", "measure": 