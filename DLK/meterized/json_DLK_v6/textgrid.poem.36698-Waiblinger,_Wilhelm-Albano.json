{"textgrid.poem.36698": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Albano", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn deine Seele so leicht wie ein Sommervogel der Puppe", "tokens": ["Wenn", "dei\u00b7ne", "See\u00b7le", "so", "leicht", "wie", "ein", "Som\u00b7mer\u00b7vo\u00b7gel", "der", "Pup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Einmal dem l\u00e4stigen Zwang \u00e4rmlicher Fesseln entflohn,", "tokens": ["Ein\u00b7mal", "dem", "l\u00e4s\u00b7ti\u00b7gen", "Zwang", "\u00e4rm\u00b7li\u00b7cher", "Fes\u00b7seln", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dann hin\u00fcber zum See! Die entbundene Psyche verweilet", "tokens": ["Dann", "hin\u00b7\u00fc\u00b7ber", "zum", "See", "!", "Die", "ent\u00b7bun\u00b7de\u00b7ne", "Psy\u00b7che", "ver\u00b7wei\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Nur am Lethe, sie schwelgt nur im Elysium noch.", "tokens": ["Nur", "am", "Le\u00b7the", ",", "sie", "schwelgt", "nur", "im", "E\u00b7ly\u00b7si\u00b7um", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PPER", "VVFIN", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}