{"dta.poem.6542": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So ist der C\u00f6rper Farb' allein das Licht,", "tokens": ["So", "ist", "der", "C\u00f6r\u00b7per", "Fa\u00b7rb'", "al\u00b7lein", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das auf dieselbe sich auf manche Weise bricht.", "tokens": ["Das", "auf", "die\u00b7sel\u00b7be", "sich", "auf", "man\u00b7che", "Wei\u00b7se", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PDAT", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Derselben eigene Beschaffenheiten", "tokens": ["Der\u00b7sel\u00b7ben", "ei\u00b7ge\u00b7ne", "Be\u00b7schaf\u00b7fen\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind blos die Art, wie sie die Theile r\u00fcckwerts schicken,", "tokens": ["Sind", "blos", "die", "Art", ",", "wie", "sie", "die", "Thei\u00b7le", "r\u00fcck\u00b7werts", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sich auf sie alsdann verbreiten,", "tokens": ["So", "sich", "auf", "sie", "als\u00b7dann", "ver\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn helle C\u00f6rper auf sie blicken.", "tokens": ["Wenn", "hel\u00b7le", "C\u00f6r\u00b7per", "auf", "sie", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}