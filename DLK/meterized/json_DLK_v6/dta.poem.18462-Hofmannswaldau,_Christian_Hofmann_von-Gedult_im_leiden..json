{"dta.poem.18462": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gedult im leiden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leid ob es schon ist schwer und ungedult wil kommen/", "tokens": ["Leid", "ob", "es", "schon", "ist", "schwer", "und", "un\u00b7ge\u00b7dult", "wil", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprich nicht da\u00df deine qual die allergr\u00f6ste sey/", "tokens": ["Sprich", "nicht", "da\u00df", "dei\u00b7ne", "qual", "die", "al\u00b7ler\u00b7gr\u00f6s\u00b7te", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein mensch ist auf der welt/ von allem kummer frey:", "tokens": ["Kein", "mensch", "ist", "auf", "der", "welt", "/", "von", "al\u00b7lem", "kum\u00b7mer", "frey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "$(", "APPR", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das gr\u00f6ste leiden wird doch endlich weggenommen.", "tokens": ["Das", "gr\u00f6s\u00b7te", "lei\u00b7den", "wird", "doch", "end\u00b7lich", "weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}