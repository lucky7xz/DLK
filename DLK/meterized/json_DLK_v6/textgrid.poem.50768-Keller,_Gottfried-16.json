{"textgrid.poem.50768": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "16", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon war die letzte Schwalbe fort", "tokens": ["Schon", "war", "die", "letz\u00b7te", "Schwal\u00b7be", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4ngst seit vielen Wochen auch", "tokens": ["Und", "l\u00e4ngst", "seit", "vie\u00b7len", "Wo\u00b7chen", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die letzte Lilie abgedorrt,", "tokens": ["Die", "letz\u00b7te", "Li\u00b7lie", "ab\u00b7ge\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach altem Erdenbrauch.", "tokens": ["Nach", "al\u00b7tem", "Er\u00b7den\u00b7brauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es flimmerte der Buchenhain", "tokens": ["Es", "flim\u00b7mer\u00b7te", "der", "Bu\u00b7chen\u00b7hain"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Rauschgold rot im Abendlicht \u2013", "tokens": ["Wie", "Rauschgold", "rot", "im", "A\u00b7bend\u00b7licht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Herbstsonne gibt gar sondren Schein,", "tokens": ["Herbst\u00b7son\u00b7ne", "gibt", "gar", "sond\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der stets ins Herz mir sticht.", "tokens": ["Der", "stets", "ins", "Herz", "mir", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich traf ", "tokens": ["Ich", "traf"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Nach der allein mein Herz begehrt,", "tokens": ["Nach", "der", "al\u00b7lein", "mein", "Herz", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit wei\u00dfen Kleidern angetan,", "tokens": ["Mit", "wei\u00b7\u00dfen", "Klei\u00b7dern", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom goldnen Schein verkl\u00e4rt.", "tokens": ["Vom", "gold\u00b7nen", "Schein", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie war allein; doch gr\u00fc\u00dft ich sie", "tokens": ["Sie", "war", "al\u00b7lein", ";", "doch", "gr\u00fc\u00dft", "ich", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur ehrfurchtsvoll im Weitergehn,", "tokens": ["Nur", "ehr\u00b7furchts\u00b7voll", "im", "Wei\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil ich sie, seit ich liebte, nie", "tokens": ["Weil", "ich", "sie", ",", "seit", "ich", "lieb\u00b7te", ",", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So still und sch\u00f6n gesehn!", "tokens": ["So", "still", "und", "sch\u00f6n", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch schaut' aus ihrem Angesicht", "tokens": ["Doch", "schaut'", "aus", "ih\u00b7rem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein fremdes Etwas kalt hervor;", "tokens": ["Ein", "frem\u00b7des", "Et\u00b7was", "kalt", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es lag vor ihrer Augen Licht", "tokens": ["Es", "lag", "vor", "ih\u00b7rer", "Au\u00b7gen", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie leichter, dunkler Flor.", "tokens": ["Wie", "leich\u00b7ter", ",", "dunk\u00b7ler", "Flor", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es war, als ob dicht hinter ihr", "tokens": ["Es", "war", ",", "als", "ob", "dicht", "hin\u00b7ter", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ADJD", "APPR", "PPOSAT"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ein Schatten schwebt' im Abendstrahl,", "tokens": ["Ein", "Schat\u00b7ten", "schwebt'", "im", "A\u00b7bend\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der gaukelnd, lachend gegen mir,", "tokens": ["Der", "gau\u00b7kelnd", ",", "la\u00b7chend", "ge\u00b7gen", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "$,", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr folgte durch das Tal.", "tokens": ["Ihr", "folg\u00b7te", "durch", "das", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbmir ist ein Rival aufgewacht!\u00ab", "tokens": ["\u00bb", "mir", "ist", "ein", "Ri\u00b7val", "auf\u00b7ge\u00b7wacht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach ich und sah ins Abendrot,", "tokens": ["Sprach", "ich", "und", "sah", "ins", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis es erlosch und bis die Nacht", "tokens": ["Bis", "es", "er\u00b7losch", "und", "bis", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die kalte Hand mir bot!", "tokens": ["Die", "kal\u00b7te", "Hand", "mir", "bot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}