{"textgrid.poem.67361": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der verschwiegene Name", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit die Liebe mich beseelet,", "tokens": ["Seit", "die", "Lie\u00b7be", "mich", "be\u00b7see\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lass' ich weiden meine Heerde,", "tokens": ["Lass'", "ich", "wei\u00b7den", "mei\u00b7ne", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lass' ich ruhen meine Fl\u00f6te,", "tokens": ["Lass'", "ich", "ru\u00b7hen", "mei\u00b7ne", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis in selbstgew\u00e4hlten T\u00f6nen", "tokens": ["Bis", "in", "selbst\u00b7ge\u00b7w\u00e4hl\u00b7ten", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie nur ", "tokens": ["Sie", "nur"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Nennet nicht, Ihr stillen Th\u00e4ler,", "tokens": ["Nen\u00b7net", "nicht", ",", "Ihr", "stil\u00b7len", "Th\u00e4\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nennet nicht den s\u00fc\u00dfen Namen,", "tokens": ["Nen\u00b7net", "nicht", "den", "s\u00fc\u00b7\u00dfen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den nur schweigend meine Lippe,", "tokens": ["Den", "nur", "schwei\u00b7gend", "mei\u00b7ne", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Den nur meine Fl\u00f6te nennt!", "tokens": ["Den", "nur", "mei\u00b7ne", "Fl\u00f6\u00b7te", "nennt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm, Geliebter, in die Th\u00e4ler,", "tokens": ["Komm", ",", "Ge\u00b7lieb\u00b7ter", ",", "in", "die", "Th\u00e4\u00b7ler", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die schon Deinen Namen kennen,", "tokens": ["Die", "schon", "Dei\u00b7nen", "Na\u00b7men", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihn oft verschwiegen nannten!", "tokens": ["Die", "ihn", "oft", "ver\u00b7schwie\u00b7gen", "nann\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm, hier ist der Sitz der Unschuld,", "tokens": ["Komm", ",", "hier", "ist", "der", "Sitz", "der", "Un\u00b7schuld", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lieb' und Freude wohnen hier.", "tokens": ["Lieb'", "und", "Freu\u00b7de", "woh\u00b7nen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Rufet ihn, Ihr stillen Th\u00e4ler,", "tokens": ["Ru\u00b7fet", "ihn", ",", "Ihr", "stil\u00b7len", "Th\u00e4\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nennet den geliebten Namen,", "tokens": ["Nen\u00b7net", "den", "ge\u00b7lieb\u00b7ten", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den nur schweigend meine Lippe,", "tokens": ["Den", "nur", "schwei\u00b7gend", "mei\u00b7ne", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Den mein Herz nur schweigend nennt!", "tokens": ["Den", "mein", "Herz", "nur", "schwei\u00b7gend", "nennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6r' ich eine Stimme schallen?", "tokens": ["H\u00f6r'", "ich", "ei\u00b7ne", "Stim\u00b7me", "schal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht die Stimme dieser Th\u00e4ler,", "tokens": ["Nicht", "die", "Stim\u00b7me", "die\u00b7ser", "Th\u00e4\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht ein Laut der todten H\u00fcgel!", "tokens": ["Nicht", "ein", "Laut", "der", "tod\u00b7ten", "H\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Horch! \u2013 ", "tokens": ["Horch", "!", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Meinem Herzen freundlich nah.", "tokens": ["Mei\u00b7nem", "Her\u00b7zen", "freund\u00b7lich", "nah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Theon ist's! Ihr H\u00fcgel, schweiget!", "tokens": ["The\u00b7on", "ist's", "!", "Ihr", "H\u00fc\u00b7gel", ",", "schwei\u00b7get", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$.", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Theon nennet meinen Namen,", "tokens": ["The\u00b7on", "nen\u00b7net", "mei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Seinen wird Theano nennen,", "tokens": ["Sei\u00b7nen", "wird", "Thea\u00b7no", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "NE", "VVINF", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.9": {"text": "Wenn der Tod die Lippe schlie\u00dft.", "tokens": ["Wenn", "der", "Tod", "die", "Lip\u00b7pe", "schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}