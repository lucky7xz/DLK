{"dta.poem.10668": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma   an den Rhein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ob wol/ du grosser Rhein/ dir alle Fl\u00fcsse weichen/", "tokens": ["Ob", "wol", "/", "du", "gros\u00b7ser", "Rhein", "/", "dir", "al\u00b7le", "Fl\u00fcs\u00b7se", "wei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$(", "PPER", "ADJA", "NE", "$(", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd deine Fruchtbarkeit die sch\u00f6nste Trauben bringt/", "tokens": ["Vnd", "dei\u00b7ne", "Frucht\u00b7bar\u00b7keit", "die", "sch\u00f6ns\u00b7te", "Trau\u00b7ben", "bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df ich dennoch dir di\u00df kleine Wasser gleichen/", "tokens": ["So", "mu\u00df", "ich", "den\u00b7noch", "dir", "di\u00df", "klei\u00b7ne", "Was\u00b7ser", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "PDS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil Delia hier offt von jhrer Liebe singt/", "tokens": ["Weil", "De\u00b7lia", "hier", "offt", "von", "jhrer", "Lie\u00b7be", "singt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Doch sprichstu/ diese Bach ist kotig/ dick/ vnd tr\u00fcbe/", "tokens": ["Doch", "sprichs\u00b7tu", "/", "die\u00b7se", "Bach", "ist", "ko\u00b7tig", "/", "dick", "/", "vnd", "tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PDAT", "NN", "VAFIN", "ADJD", "$(", "ADJD", "$(", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch wegen Feistigkeit nicht wol zuschawen an/", "tokens": ["Auch", "we\u00b7gen", "Feis\u00b7tig\u00b7keit", "nicht", "wol", "zu\u00b7scha\u00b7wen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKNEG", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df ist es/ edler Rein/ warumb ich sie mehr Liebe/", "tokens": ["Di\u00df", "ist", "es", "/", "ed\u00b7ler", "Rein", "/", "wa\u00b7rumb", "ich", "sie", "mehr", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$(", "ADJA", "NN", "$(", "PWAV", "PPER", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil Delia in jhr sich nicht besehen kan.", "tokens": ["Weil", "De\u00b7lia", "in", "jhr", "sich", "nicht", "be\u00b7se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}