{"textgrid.poem.27590": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Anstand und von Fried und vielen sch\u00f6nen Dingen", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Anstand und von Fried und vielen sch\u00f6nen Dingen", "tokens": ["Von", "An\u00b7stand", "und", "von", "Fried", "und", "vie\u00b7len", "sch\u00f6\u00b7nen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will Fama dieser Zeit ein neues Liedlein singen;", "tokens": ["Will", "Fa\u00b7ma", "die\u00b7ser", "Zeit", "ein", "neu\u00b7es", "Lied\u00b7lein", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PDAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wei\u00df ich nicht, obs neu. Der Anstand ist gar alt;", "tokens": ["Doch", "wei\u00df", "ich", "nicht", ",", "obs", "neu", ".", "Der", "An\u00b7stand", "ist", "gar", "alt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADJD", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Fried' ist auch f\u00fcr l\u00e4ngst gar recht, gar wol bestalt.", "tokens": ["Der", "Fried'", "ist", "auch", "f\u00fcr", "l\u00e4ngst", "gar", "recht", ",", "gar", "wol", "be\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was darff ein Anstand sein, wo nie man noch gestritten?", "tokens": ["Was", "darff", "ein", "An\u00b7stand", "sein", ",", "wo", "nie", "man", "noch", "ge\u00b7strit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VAINF", "$,", "PWAV", "ADV", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da Waffen und ihr Brauch nach dieses Krieges Sitten", "tokens": ["Da", "Waf\u00b7fen", "und", "ihr", "Brauch", "nach", "die\u00b7ses", "Krie\u00b7ges", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "PPOSAT", "NN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gleichwie in einem Spiel nur blo\u00df zum Scherz und Schein", "tokens": ["Gleich\u00b7wie", "in", "ei\u00b7nem", "Spiel", "nur", "blo\u00df", "zum", "Scherz", "und", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und da\u00df sie nicht der Rost zerfre\u00df, in H\u00e4nden sein?", "tokens": ["Und", "da\u00df", "sie", "nicht", "der", "Rost", "zer\u00b7fre\u00df", ",", "in", "H\u00e4n\u00b7den", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was darff ein Anstand sein, wo nie kein Feind sich findet,", "tokens": ["Was", "darff", "ein", "An\u00b7stand", "sein", ",", "wo", "nie", "kein", "Feind", "sich", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VAINF", "$,", "PWAV", "ADV", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der zu bekriegen steht, und wo man sich nur gr\u00fcndet", "tokens": ["Der", "zu", "be\u00b7krie\u00b7gen", "steht", ",", "und", "wo", "man", "sich", "nur", "gr\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKZU", "VVINF", "VVFIN", "$,", "KON", "PWAV", "PIS", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auf Meinung, unser Land nach drau\u00dfgesch\u00f6pfftem Nutz", "tokens": ["Auf", "Mei\u00b7nung", ",", "un\u00b7ser", "Land", "nach", "drau\u00df\u00b7ge\u00b7sch\u00f6pff\u00b7tem", "Nutz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Alsdenn dem lieben Gott zu geben in den Schutz?", "tokens": ["Als\u00b7denn", "dem", "lie\u00b7ben", "Gott", "zu", "ge\u00b7ben", "in", "den", "Schutz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was darff ein Anstand sein, wo man die Krieges-Kinder", "tokens": ["Was", "darff", "ein", "An\u00b7stand", "sein", ",", "wo", "man", "die", "Krie\u00b7ge\u00b7sKin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "VAINF", "$,", "PWAV", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gar glimpf- und g\u00fctlich meint und blo\u00df die feisten Rinder", "tokens": ["Gar", "glimpf", "und", "g\u00fct\u00b7lich", "meint", "und", "blo\u00df", "die", "feis\u00b7ten", "Rin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "TRUNC", "KON", "ADJD", "VVFIN", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sambt ihrer jungen Art um etwa Pferd und Schwein,", "tokens": ["Sambt", "ih\u00b7rer", "jun\u00b7gen", "Art", "um", "et\u00b7wa", "Pferd", "und", "Schwein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "APPR", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schaaf, Hun, Han, Ente, Gans l\u00e4st seine Feinde sein?", "tokens": ["Schaaf", ",", "Hun", ",", "Han", ",", "En\u00b7te", ",", "Gans", "l\u00e4st", "sei\u00b7ne", "Fein\u00b7de", "sein", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VAFIN", "$,", "VVFIN", "$,", "NN", "VVFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Fried' ist lange schon in unsre Gr\u00e4ntzen kommen,", "tokens": ["Der", "Fried'", "ist", "lan\u00b7ge", "schon", "in", "uns\u00b7re", "Gr\u00e4nt\u00b7zen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da jene viel zwar uns, wir ihnen nichts genommen,", "tokens": ["Da", "je\u00b7ne", "viel", "zwar", "uns", ",", "wir", "ih\u00b7nen", "nichts", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ADV", "PPER", "$,", "PPER", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Indem wir uns bem\u00fcht, (o eine feine Kunst!)", "tokens": ["In\u00b7dem", "wir", "uns", "be\u00b7m\u00fcht", ",", "(", "o", "ei\u00b7ne", "fei\u00b7ne", "Kunst", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "$(", "FM", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zu brechen ihren Trotz durch unsre gute Gunst.", "tokens": ["Zu", "bre\u00b7chen", "ih\u00b7ren", "Trotz", "durch", "uns\u00b7re", "gu\u00b7te", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es ist ja Fried' und Ruh im Lande gantz die V\u00f6lle;", "tokens": ["Es", "ist", "ja", "Fried'", "und", "Ruh", "im", "Lan\u00b7de", "gantz", "die", "V\u00f6l\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das Feld h\u00e4lt Sabat-Tag; der Acker liget stille", "tokens": ["Das", "Feld", "h\u00e4lt", "Sa\u00b7bat\u00b7Tag", ";", "der", "A\u00b7cker", "li\u00b7get", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "$.", "ART", "NN", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und duldet nicht wie vor, da\u00df ihm viel Wunden schlug", "tokens": ["Und", "dul\u00b7det", "nicht", "wie", "vor", ",", "da\u00df", "ihm", "viel", "Wun\u00b7den", "schlug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "KOKOM", "APPR", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "De\u00df Bauers frecher Arm und ein tyrannisch Pflug.", "tokens": ["De\u00df", "Bau\u00b7ers", "fre\u00b7cher", "Arm", "und", "ein", "ty\u00b7ran\u00b7nisch", "Pflug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es ist ja Friede da; man darf ja mehr nicht sorgen,", "tokens": ["Es", "ist", "ja", "Frie\u00b7de", "da", ";", "man", "darf", "ja", "mehr", "nicht", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "PTKVZ", "$.", "PIS", "VMFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie jeder Hab und Gut f\u00fcr Dieben h\u00e4lt verborgen", "tokens": ["Wie", "je\u00b7der", "Hab", "und", "Gut", "f\u00fcr", "Die\u00b7ben", "h\u00e4lt", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "KON", "ADJD", "APPR", "PDAT", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "In sicherem Gemach; es bleibt ja Gold und Geld", "tokens": ["In", "si\u00b7che\u00b7rem", "Ge\u00b7mach", ";", "es", "bleibt", "ja", "Gold", "und", "Geld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.28": {"text": "In festem Hause so, wie durch das offen Feld.", "tokens": ["In", "fes\u00b7tem", "Hau\u00b7se", "so", ",", "wie", "durch", "das", "of\u00b7fen", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hierum singt Fama falsch von Anstand und von Friede;", "tokens": ["Hie\u00b7rum", "singt", "Fa\u00b7ma", "falsch", "von", "An\u00b7stand", "und", "von", "Frie\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ihr Sinn sei dieser denn, da\u00df, weil die Welt ist m\u00fcde", "tokens": ["Ihr", "Sinn", "sei", "die\u00b7ser", "denn", ",", "da\u00df", ",", "weil", "die", "Welt", "ist", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PDS", "ADV", "$,", "KOUS", "$,", "KOUS", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der alten deutschen Treu, nur mit Betrieglichkeit", "tokens": ["Der", "al\u00b7ten", "deut\u00b7schen", "Treu", ",", "nur", "mit", "Be\u00b7trieg\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Man habe steten Fried' und Krieg mit Redligheit.", "tokens": ["Man", "ha\u00b7be", "ste\u00b7ten", "Fried'", "und", "Krieg", "mit", "Red\u00b7lig\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJA", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}