{"dta.poem.2755": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Liebes-Dienst/  \n  Hn. W. Z. den 2. Febr. 1681. er wiesen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbtste/ dieser Dienst ist wider Wunsch und Willen/", "tokens": ["Be\u00b7tr\u00b7\u00fcbts\u00b7te", "/", "die\u00b7ser", "Dienst", "ist", "wi\u00b7der", "Wunsch", "und", "Wil\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Feder wird wie Bley in meinen H\u00e4nden schwer/", "tokens": ["Die", "Fe\u00b7der", "wird", "wie", "Bley", "in", "mei\u00b7nen", "H\u00e4n\u00b7den", "schwer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Thr\u00e4nen m\u00fcssen sie an statt der Dint anf\u00fcllen/", "tokens": ["Und", "Thr\u00e4\u00b7nen", "m\u00fcs\u00b7sen", "sie", "an", "statt", "der", "Dint", "an\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPER", "APPR", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! da\u00df ich tr\u00f6sten soll von Trost gantz blo\u00df und leer!", "tokens": ["Ach", "!", "da\u00df", "ich", "tr\u00f6s\u00b7ten", "soll", "von", "Trost", "gantz", "blo\u00df", "und", "leer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "VVINF", "VMFIN", "APPR", "NN", "ADV", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Liebsten blutig Hertz/ der T\u00f6chter heisse Zehren/", "tokens": ["Der", "Liebs\u00b7ten", "blu\u00b7tig", "Hertz", "/", "der", "T\u00f6ch\u00b7ter", "heis\u00b7se", "Zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Anverwandten Weh\u2019 verwirrt mir Sinn und Geist;", "tokens": ["Der", "An\u00b7ver\u00b7wand\u00b7ten", "Weh'", "ver\u00b7wirrt", "mir", "Sinn", "und", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So will mich eigner Schmertz empfindlicher verzehren", "tokens": ["So", "will", "mich", "eig\u00b7ner", "Schmertz", "emp\u00b7find\u00b7li\u00b7cher", "ver\u00b7zeh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der meines Freundes Grab mich itzt bejammern heist.", "tokens": ["Der", "mei\u00b7nes", "Freun\u00b7des", "Grab", "mich", "itzt", "be\u00b7jam\u00b7mern", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "PPER", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein/ di\u00df klag ich nicht da\u00df Menschen m\u00fcssen sterben/", "tokens": ["Al\u00b7lein", "/", "di\u00df", "klag", "ich", "nicht", "da\u00df", "Men\u00b7schen", "m\u00fcs\u00b7sen", "ster\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PDS", "VVFIN", "PPER", "PTKNEG", "KOUS", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es ist der Urtheilspruch de\u00df H\u00f6chsten l\u00e4ngst bekandt;", "tokens": ["Es", "ist", "der", "Ur\u00b7theil\u00b7spruch", "de\u00df", "H\u00f6chs\u00b7ten", "l\u00e4ngst", "be\u00b7kandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was auferstehen soll das mu\u00df zuvor verderben/", "tokens": ["Was", "auf\u00b7er\u00b7ste\u00b7hen", "soll", "das", "mu\u00df", "zu\u00b7vor", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "PDS", "VMFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wir gehn durch Heck und Dorn in jenes Rosen-Land", "tokens": ["Wir", "gehn", "durch", "Heck", "und", "Dorn", "in", "je\u00b7nes", "Ro\u00b7sen\u00b7Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "KON", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nur wenn ein n\u00fctzlich Mann im Wachsthum bester Jahre/", "tokens": ["Nur", "wenn", "ein", "n\u00fctz\u00b7lich", "Mann", "im", "Wach\u00b7sthum", "bes\u00b7ter", "Jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJD", "NN", "APPRART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und r\u00fchmlichen Beruff entf\u00e4llt gemeinem Heil/", "tokens": ["Und", "r\u00fchm\u00b7li\u00b7chen", "Be\u00b7ruff", "ent\u00b7f\u00e4llt", "ge\u00b7mei\u00b7nem", "Heil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenn ein Gewissenhafft Jurist ligt auf der Bahre/", "tokens": ["Wenn", "ein", "Ge\u00b7wis\u00b7sen\u00b7hafft", "Ju\u00b7rist", "ligt", "auf", "der", "Bah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dem Themis anvertraut die Schalen und ihr Beil/", "tokens": ["Dem", "The\u00b7mis", "an\u00b7ver\u00b7traut", "die", "Scha\u00b7len", "und", "ihr", "Beil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So ist nur der Verlust unendlich zu beklagen.", "tokens": ["So", "ist", "nur", "der", "Ver\u00b7lust", "un\u00b7end\u00b7lich", "zu", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So l\u00e4st man ja mit Recht dem Schmertz die Z\u00fcgel lo\u00df;", "tokens": ["So", "l\u00e4st", "man", "ja", "mit", "Recht", "dem", "Schmertz", "die", "Z\u00fc\u00b7gel", "lo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wenn ein wahrer Freund uns wird hinweg getragen/", "tokens": ["Und", "wenn", "ein", "wah\u00b7rer", "Freund", "uns", "wird", "hin\u00b7weg", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist dieser Hintritt nicht ein rechter Hertzens-Sto\u00df?", "tokens": ["Ist", "die\u00b7ser", "Hin\u00b7tritt", "nicht", "ein", "rech\u00b7ter", "Hert\u00b7zens\u00b7Sto\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Herr Zimmerling ist hin! Weint/ keusche Musen/ weinet/", "tokens": ["Herr", "Zim\u00b7mer\u00b7ling", "ist", "hin", "!", "Weint", "/", "keu\u00b7sche", "Mu\u00b7sen", "/", "wei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "$.", "VVFIN", "$(", "ADJA", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Flecht f\u00fcr die Lorbeer euch Cypressen in das Haar/", "tokens": ["Flecht", "f\u00fcr", "die", "Lor\u00b7beer", "euch", "Cyp\u00b7res\u00b7sen", "in", "das", "Haar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Seyd m\u00fchsam da\u00df ihr noch bey seinem Grab erscheinet/", "tokens": ["Seyd", "m\u00fch\u00b7sam", "da\u00df", "ihr", "noch", "bey", "sei\u00b7nem", "Grab", "er\u00b7schei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dieweil er euer Ruhm und Trost und Zierath war!", "tokens": ["Die\u00b7weil", "er", "eu\u00b7er", "Ruhm", "und", "Trost", "und", "Zie\u00b7rath", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es will Astr\u00e4a ihn als einen Priester ehren/", "tokens": ["Es", "will", "A\u00b7str\u00e4a", "ihn", "als", "ei\u00b7nen", "Pries\u00b7ter", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "PPER", "KOUS", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Und sein Ged\u00e4chtn\u00fc\u00df soll in ihrem Tempel stehn/", "tokens": ["Und", "sein", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "soll", "in", "ih\u00b7rem", "Tem\u00b7pel", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der viel den rechten Weg pflag\u2019 in dem Recht zu lehren/", "tokens": ["Der", "viel", "den", "rech\u00b7ten", "Weg", "pflag'", "in", "dem", "Recht", "zu", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Soll in der Nachwelt Mund ein ewig Lob erh\u00f6hn.", "tokens": ["Soll", "in", "der", "Nach\u00b7welt", "Mund", "ein", "e\u00b7wig", "Lob", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Was aber liefer\u2019 ich des Freundes leichten Schatten/", "tokens": ["Was", "a\u00b7ber", "lie\u00b7fer'", "ich", "des", "Freun\u00b7des", "leich\u00b7ten", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dem mich von Jugend auf Minerva zugepaart?", "tokens": ["Dem", "mich", "von", "Ju\u00b7gend", "auf", "Mi\u00b7ner\u00b7va", "zu\u00b7ge\u00b7paart", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "K\u00f6mmt seiner Asche noch mein Liebes-Dienst zu statten/", "tokens": ["K\u00f6mmt", "sei\u00b7ner", "A\u00b7sche", "noch", "mein", "Lie\u00b7bes\u00b7Dienst", "zu", "stat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Trotzt unser B\u00fcndn\u00fc\u00df auch des bleichen Charons-Farth?", "tokens": ["Trotzt", "un\u00b7ser", "B\u00fcnd\u00b7n\u00fc\u00df", "auch", "des", "blei\u00b7chen", "Cha\u00b7rons\u00b7\u00b7F\u00b7arth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.33": {"text": "So schreib ich di\u00df zum Ruhm; ein unbefleckt Gewissen/", "tokens": ["So", "schreib", "ich", "di\u00df", "zum", "Ruhm", ";", "ein", "un\u00b7be\u00b7fleckt", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "APPRART", "NN", "$.", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ein redlich Hertz und Sinn und unverstellter Mund/", "tokens": ["Ein", "red\u00b7lich", "Hertz", "und", "Sinn", "und", "un\u00b7ver\u00b7stell\u00b7ter", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein Christ und fromm Jur ist ist mit ihm hingerissen/", "tokens": ["Ein", "Christ", "und", "fromm", "Jur", "ist", "ist", "mit", "ihm", "hin\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "NN", "VAFIN", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der nicht als wie ein Rohr leicht zu bewegen stund.", "tokens": ["Der", "nicht", "als", "wie", "ein", "Rohr", "leicht", "zu", "be\u00b7we\u00b7gen", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "KOUS", "KOKOM", "ART", "NN", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er wuste da\u00df di\u00df Ampt hochheilig/ voller W\u00fcrde/", "tokens": ["Er", "wus\u00b7te", "da\u00df", "di\u00df", "Ampt", "hoch\u00b7hei\u00b7lig", "/", "vol\u00b7ler", "W\u00fcr\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PDS", "NN", "ADJD", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df die Gerechtigkeit der Erden Bau erh\u00e4lt/", "tokens": ["Da\u00df", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "der", "Er\u00b7den", "Bau", "er\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df es hingegen auch als eine schwere B\u00fcrde/", "tokens": ["Da\u00df", "es", "hin\u00b7ge\u00b7gen", "auch", "als", "ei\u00b7ne", "schwe\u00b7re", "B\u00fcr\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Vertheidigern des Rechts auff ihre Schultern f\u00e4llt;", "tokens": ["Ver\u00b7thei\u00b7di\u00b7gern", "des", "Rechts", "auff", "ih\u00b7re", "Schul\u00b7tern", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was ein Soldat im Krieg das Vaterland zu sch\u00fct