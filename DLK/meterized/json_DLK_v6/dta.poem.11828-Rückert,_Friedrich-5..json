{"dta.poem.11828": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ehre kannst du wol von andern leicht entbehren,", "tokens": ["Der", "Eh\u00b7re", "kannst", "du", "wol", "von", "an\u00b7dern", "leicht", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "APPR", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du dich selber nur zu halten wei\u00dft in Ehren.", "tokens": ["Wenn", "du", "dich", "sel\u00b7ber", "nur", "zu", "hal\u00b7ten", "wei\u00dft", "in", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch will dir Unverstand versagen die Geb\u00fchren,", "tokens": ["Doch", "will", "dir", "Un\u00b7ver\u00b7stand", "ver\u00b7sa\u00b7gen", "die", "Ge\u00b7b\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df ihn nicht deinethalb, la\u00df es ihn seinthalb sp\u00fcren.", "tokens": ["La\u00df", "ihn", "nicht", "dei\u00b7net\u00b7halb", ",", "la\u00df", "es", "ihn", "sein\u00b7thalb", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PAV", "$,", "VVIMP", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "---+-++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Denn jedem Manne ziemt vorm andern, und dem Knaben", "tokens": ["Denn", "je\u00b7dem", "Man\u00b7ne", "ziemt", "vorm", "an\u00b7dern", ",", "und", "dem", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPRART", "ADJA", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ziemt zwiefach Achtung wol vor einem Mann zu haben.", "tokens": ["Ziemt", "zwie\u00b7fach", "Ach\u00b7tung", "wol", "vor", "ei\u00b7nem", "Mann", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Lehre sollst du ihm, weil sie ist heilsam, geben;", "tokens": ["Die", "Leh\u00b7re", "sollst", "du", "ihm", ",", "weil", "sie", "ist", "heil\u00b7sam", ",", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gib sie ihm so alsob es dich nicht angieng' eben.", "tokens": ["Gib", "sie", "ihm", "so", "al\u00b7sob", "es", "dich", "nicht", "an\u00b7gieng'", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}