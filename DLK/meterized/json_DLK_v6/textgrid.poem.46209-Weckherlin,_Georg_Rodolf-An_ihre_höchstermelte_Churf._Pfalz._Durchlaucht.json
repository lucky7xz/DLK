{"textgrid.poem.46209": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An ihre h\u00f6chstermelte Churf. Pfalz. Durchlaucht", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An euch gedenk ich oft, mehr dan an jemand sunst,", "tokens": ["An", "euch", "ge\u00b7denk", "ich", "oft", ",", "mehr", "dan", "an", "je\u00b7mand", "sunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mehr dan sunst jemand auch: doch mein herz zu beweisen", "tokens": ["mehr", "dan", "sunst", "je\u00b7mand", "auch", ":", "doch", "mein", "herz", "zu", "be\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PIS", "ADV", "$.", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "und euch nach euerm wert und meiner treu zu preisen,", "tokens": ["und", "euch", "nach", "eu\u00b7erm", "wert", "und", "mei\u00b7ner", "treu", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "ADJD", "KON", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "verbleibet mir allein der will und nicht die kunst.", "tokens": ["ver\u00b7blei\u00b7bet", "mir", "al\u00b7lein", "der", "will", "und", "nicht", "die", "kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "VMFIN", "KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja solt auch Pallas selbs aus sonderbarer gunst", "tokens": ["Ja", "solt", "auch", "Pal\u00b7las", "selbs", "aus", "son\u00b7der\u00b7ba\u00b7rer", "gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VMFIN", "ADV", "NE", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mit himmelwein und brot mich dr\u00e4nken und mich speisen,", "tokens": ["mit", "him\u00b7mel\u00b7wein", "und", "brot", "mich", "dr\u00e4n\u00b7ken", "und", "mich", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "so kont, allweis, sie doch von mir schier d\u00fcrren greisen,", "tokens": ["so", "kont", ",", "all\u00b7weis", ",", "sie", "doch", "von", "mir", "schier", "d\u00fcr\u00b7ren", "grei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "FM.la", "$,", "PPER", "ADV", "APPR", "PPER", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "numehr so kalt und alt, erwarten schlechte brunst.", "tokens": ["nu\u00b7mehr", "so", "kalt", "und", "alt", ",", "er\u00b7war\u00b7ten", "schlech\u00b7te", "brunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Daher, darf ich schon nicht abdrinken das Pocal,", "tokens": ["Da\u00b7her", ",", "darf", "ich", "schon", "nicht", "ab\u00b7drin\u00b7ken", "das", "Po\u00b7cal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "dan weil es vil zu gro\u00df, ich, drunken, m\u00f6cht stracks toben", "tokens": ["dan", "weil", "es", "vil", "zu", "gro\u00df", ",", "ich", ",", "drun\u00b7ken", ",", "m\u00f6cht", "stracks", "to\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PTKA", "ADJD", "$,", "PPER", "$,", "VVFIN", "$,", "VMFIN", "ADV", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "als doll und voll von sturm, wind, dunder oder strahl:", "tokens": ["als", "doll", "und", "voll", "von", "sturm", ",", "wind", ",", "dun\u00b7der", "o\u00b7der", "strahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "APPR", "ADJD", "$,", "VAFIN", "$,", "ADJA", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So darf doch, gro\u00dfer prinz, da\u00df, euer wert zu loben,", "tokens": ["So", "darf", "doch", ",", "gro\u00b7\u00dfer", "prinz", ",", "da\u00df", ",", "eu\u00b7er", "wert", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "$,", "ADJA", "NN", "$,", "KOUS", "$,", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "ich wider lernen will der Musen musik, zahl,", "tokens": ["ich", "wi\u00b7der", "ler\u00b7nen", "will", "der", "Mu\u00b7sen", "mu\u00b7sik", ",", "zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "VVINF", "VMFIN", "ART", "NN", "VMFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "ma\u00df, kunst und lieblichkeit, ich euch hiemit geloben.", "tokens": ["ma\u00df", ",", "kunst", "und", "lieb\u00b7lich\u00b7keit", ",", "ich", "euch", "hie\u00b7mit", "ge\u00b7lo\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KON", "NN", "$,", "PPER", "PRF", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}