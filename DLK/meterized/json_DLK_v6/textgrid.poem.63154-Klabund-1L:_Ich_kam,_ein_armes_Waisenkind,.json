{"textgrid.poem.63154": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kam, ein armes Waisenkind,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam, ein armes Waisenkind,", "tokens": ["Ich", "kam", ",", "ein", "ar\u00b7mes", "Wai\u00b7sen\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu den Menschen der grossen St\u00e4dte.", "tokens": ["Zu", "den", "Men\u00b7schen", "der", "gros\u00b7sen", "St\u00e4d\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sie sagten, dass ich tiefe Augen h\u00e4tte,", "tokens": ["Sie", "sag\u00b7ten", ",", "dass", "ich", "tie\u00b7fe", "Au\u00b7gen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch war ich den Menschen zu bl\u00f6de gesinnt.", "tokens": ["Doch", "war", "ich", "den", "Men\u00b7schen", "zu", "bl\u00f6\u00b7de", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "APPR", "ADJA", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Mit zwanzig Jahren ohne Lug und Trug", "tokens": ["Mit", "zwan\u00b7zig", "Jah\u00b7ren", "oh\u00b7ne", "Lug", "und", "Trug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hiess es mich gehen zu sch\u00f6nen Frauen.", "tokens": ["Hiess", "es", "mich", "ge\u00b7hen", "zu", "sch\u00f6\u00b7nen", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "PTKZU", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sie nennen es Liebesgrauen.", "tokens": ["Sie", "nen\u00b7nen", "es", "Lie\u00b7bes\u00b7grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch war ich den Frauen nicht sch\u00f6n genug.", "tokens": ["Doch", "war", "ich", "den", "Frau\u00b7en", "nicht", "sch\u00f6n", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Kein Vaterland, in keines Sold,", "tokens": ["Kein", "Va\u00b7ter\u00b7land", ",", "in", "kei\u00b7nes", "Sold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liess ich mich vom Hauptmann werben.", "tokens": ["Liess", "ich", "mich", "vom", "Haupt\u00b7mann", "wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich wollte im Kriege sterben.", "tokens": ["Ich", "woll\u00b7te", "im", "Krie\u00b7ge", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Tod hat mich nicht gewollt.", "tokens": ["Der", "Tod", "hat", "mich", "nicht", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VMPP", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Ward ich zu fr\u00fch geboren, zu sp\u00e4t?", "tokens": ["Ward", "ich", "zu", "fr\u00fch", "ge\u00b7bo\u00b7ren", ",", "zu", "sp\u00e4t", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "VVPP", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Was tu ich auf der Welt noch hier?", "tokens": ["Was", "tu", "ich", "auf", "der", "Welt", "noch", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Leid ist ja so brunnentief. O Ihr,", "tokens": ["Mein", "Leid", "ist", "ja", "so", "brun\u00b7nen\u00b7tief", ".", "O", "Ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "VVFIN", "$.", "NE", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sprecht f\u00fcr den armen Kaspar ein Gebet!", "tokens": ["Sprecht", "f\u00fcr", "den", "ar\u00b7men", "Kas\u00b7par", "ein", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}}}}}