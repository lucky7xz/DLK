{"dta.poem.10276": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Bisweilen ist es mir, als ob ich h\u00f6re             ", "tokens": ["Bis\u00b7wei\u00b7len", "ist", "es", "mir", ",", "als", "ob", "ich", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOKOM", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Trommeln wirbeln und den Ruf der H\u00f6rner.", "tokens": ["Die", "Trom\u00b7meln", "wir\u00b7beln", "und", "den", "Ruf", "der", "H\u00f6r\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und siegestrunken bricht aus tausend Kehlen,", "tokens": ["Und", "sie\u00b7ge\u00b7strun\u00b7ken", "bricht", "aus", "tau\u00b7send", "Keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es klingt zu mir aus ungemess\u2019nen Fernen,", "tokens": ["Es", "klingt", "zu", "mir", "aus", "un\u00b7ge\u00b7mess'\u00b7nen", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein brausend Hurrah jauchzend zu den Sternen.", "tokens": ["Ein", "brau\u00b7send", "Hur\u00b7rah", "jauch\u00b7zend", "zu", "den", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}