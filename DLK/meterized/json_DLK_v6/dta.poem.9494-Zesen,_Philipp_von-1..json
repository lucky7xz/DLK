{"dta.poem.9494": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "007C; Ach sch\u00f6n  ste/ wie kann  so bl\u00e4n  den", "tokens": [";", "Ach", "sch\u00f6n", "ste", "/", "wie", "kann", "so", "bl\u00e4n", "den"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$.", "NN", "ADJD", "VVFIN", "$(", "PWAV", "VMFIN", "ADV", "VVFIN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Sonnen-liechte glantz/", "tokens": ["Der", "Son\u00b7nen\u00b7liech\u00b7te", "glantz", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der dich \u00fcmbgeben gantz?", "tokens": ["Der", "dich", "\u00fcmb\u00b7ge\u00b7ben", "gantz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wie kanstu mein Hertze wenden/", "tokens": ["Wie", "kans\u00b7tu", "mein", "Hert\u00b7ze", "wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du Herrscherin meiner Sinnen?", "tokens": ["Du", "Herr\u00b7sche\u00b7rin", "mei\u00b7ner", "Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie kanstu mich so gewinnen", "tokens": ["Wie", "kans\u00b7tu", "mich", "so", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Durch deines Scepters-macht", "tokens": ["Durch", "dei\u00b7nes", "Scep\u00b7ter\u00b7smacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "zu tag und Nacht?", "tokens": ["zu", "tag", "und", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mein w\u00fcndschen ist/ dich zu schauer/", "tokens": ["Mein", "w\u00fcnd\u00b7schen", "ist", "/", "dich", "zu", "schau\u00b7er", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "$(", "PPER", "PTKA", "ADJD", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du trautes edles Bild/", "tokens": ["Du", "trau\u00b7tes", "ed\u00b7les", "Bild", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mitt s\u00fc\u00dfigkeit erf\u00fcllt;", "tokens": ["Mitt", "s\u00fc\u00b7\u00dfig\u00b7keit", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich will mich mit Dier vertrauen/", "tokens": ["Ich", "will", "mich", "mit", "Dier", "ver\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Will trincken aus deinen fl\u00fcssen/", "tokens": ["Will", "trin\u00b7cken", "aus", "dei\u00b7nen", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Honig uns geben m\u00fcssen;", "tokens": ["Die", "Ho\u00b7nig", "uns", "ge\u00b7ben", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dr\u00fcmb komm/ und la\u00df mich nicht/", "tokens": ["Dr\u00fcmb", "komm", "/", "und", "la\u00df", "mich", "nicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "KON", "VVIMP", "PPER", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Du edles Licht!", "tokens": ["Du", "ed\u00b7les", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}