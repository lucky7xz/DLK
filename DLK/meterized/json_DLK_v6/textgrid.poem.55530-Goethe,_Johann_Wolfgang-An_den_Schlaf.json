{"textgrid.poem.55530": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An den Schlaf", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mit deinem Mohne", "tokens": ["Der", "du", "mit", "dei\u00b7nem", "Moh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Selbst G\u00f6tteraugen zwingst", "tokens": ["Selbst", "G\u00f6t\u00b7ter\u00b7au\u00b7gen", "zwingst"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Bettler oft zum Throne,", "tokens": ["Und", "Bett\u00b7ler", "oft", "zum", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum M\u00e4dchen Sch\u00e4fer bringst,", "tokens": ["Zum", "M\u00e4d\u00b7chen", "Sch\u00e4\u00b7fer", "bringst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vernimm: Kein Traumgespinste", "tokens": ["Ver\u00b7nimm", ":", "Kein", "Traum\u00b7ge\u00b7spins\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVIMP", "$.", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verlang ich heut von dir.", "tokens": ["Ver\u00b7lang", "ich", "heut", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den gr\u00f6\u00dften deiner Dienste,", "tokens": ["Den", "gr\u00f6\u00df\u00b7ten", "dei\u00b7ner", "Diens\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geliebter, leiste mir.", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "leis\u00b7te", "mir", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "An meines M\u00e4dchens Seite", "tokens": ["An", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sitz ich, ihr Aug spricht Lust,", "tokens": ["Sitz", "ich", ",", "ihr", "Aug", "spricht", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und unter neid'scher Seide", "tokens": ["Und", "un\u00b7ter", "nei\u00b7d'\u00b7scher", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Steigt f\u00fchlbar ihre Brust;", "tokens": ["Steigt", "f\u00fchl\u00b7bar", "ih\u00b7re", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Oft hatte meinen K\u00fcssen", "tokens": ["Oft", "hat\u00b7te", "mei\u00b7nen", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie Amor zugebracht,", "tokens": ["Sie", "A\u00b7mor", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dies Gl\u00fcck mu\u00df ich vermissen,", "tokens": ["Dies", "Gl\u00fcck", "mu\u00df", "ich", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die strenge Mutter wacht.", "tokens": ["Die", "stren\u00b7ge", "Mut\u00b7ter", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Am Abend triffst du wieder", "tokens": ["Am", "A\u00b7bend", "triffst", "du", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich dort, o tritt herein,", "tokens": ["Mich", "dort", ",", "o", "tritt", "her\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "FM", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Spr\u00fch Mohn von dem Gefieder,", "tokens": ["Spr\u00fch", "Mohn", "von", "dem", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da schlaf die Mutter ein:", "tokens": ["Da", "schlaf", "die", "Mut\u00b7ter", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bei blassem Lichterscheinen,", "tokens": ["Bei", "blas\u00b7sem", "Lich\u00b7ter\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Lieb Annette warm", "tokens": ["Von", "Lieb", "An\u00b7net\u00b7te", "warm"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sink, wie Mama in deinen,", "tokens": ["Sink", ",", "wie", "Ma\u00b7ma", "in", "dei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "APPR", "PPOSAT", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "In meinen gier'gen Arm.", "tokens": ["In", "mei\u00b7nen", "gier'\u00b7gen", "Arm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}