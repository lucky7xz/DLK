{"textgrid.poem.36721": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Benvenuto Cellini", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerne bekenn' ich, du bist der Uly\u00df der K\u00fcnste, so vielfach", "tokens": ["Ger\u00b7ne", "be\u00b7kenn'", "ich", ",", "du", "bist", "der", "U\u00b7ly\u00df", "der", "K\u00fcns\u00b7te", ",", "so", "viel\u00b7fach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ADV", "ADV"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Trug dich dein guter Humor, Kraft und Genie durch die Welt.", "tokens": ["Trug", "dich", "dein", "gu\u00b7ter", "Hu\u00b7mor", ",", "Kraft", "und", "Ge\u00b7nie", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "L\u00e4ngst schon sperrte die Zeit, die schwarze Zauberin Circe", "tokens": ["L\u00e4ngst", "schon", "sperr\u00b7te", "die", "Zeit", ",", "die", "schwar\u00b7ze", "Zau\u00b7be\u00b7rin", "Cir\u00b7ce"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Deine Genossen im Stall ew'ger Vergessenheit ein,", "tokens": ["Dei\u00b7ne", "Ge\u00b7nos\u00b7sen", "im", "Stall", "ew'\u00b7ger", "Ver\u00b7ges\u00b7sen\u00b7heit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Aber durch manche Charybdis erreichtest du endlich die Heimath,", "tokens": ["A\u00b7ber", "durch", "man\u00b7che", "Cha\u00b7ryb\u00b7dis", "er\u00b7reich\u00b7test", "du", "end\u00b7lich", "die", "Hei\u00b7math", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.6": {"text": "Deine Penelope schlo\u00df dich in die Arme \u2013 die Kunst.", "tokens": ["Dei\u00b7ne", "Pe\u00b7ne\u00b7lo\u00b7pe", "schlo\u00df", "dich", "in", "die", "Ar\u00b7me", "\u2013", "die", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$(", "ART", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}}}}