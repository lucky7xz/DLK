{"dta.poem.11084": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Feuers-gefahr.  \n  C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\nmEin hertze brennt; Ach! wei\u00df jemand,", "tokens": ["mEin", "hert\u00b7ze", "brennt", ";", "Ach", "!", "wei\u00df", "je\u00b7mand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ITJ", "$.", "VVFIN", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie man den angelegten brand", "tokens": ["Wie", "man", "den", "an\u00b7ge\u00b7leg\u00b7ten", "brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch stille w\u00f6rter kan versprechen?", "tokens": ["Durch", "stil\u00b7le", "w\u00f6r\u00b7ter", "kan", "ver\u00b7spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der gebe sich bey zeiten an,", "tokens": ["Der", "ge\u00b7be", "sich", "bey", "zei\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lasse sehen, ob er kan", "tokens": ["Und", "las\u00b7se", "se\u00b7hen", ",", "ob", "er", "kan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVINF", "$,", "KOUS", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des feuers w\u00fcten unterbrechen!", "tokens": ["Des", "feu\u00b7ers", "w\u00fc\u00b7ten", "un\u00b7ter\u00b7bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist ein entlauffner kriegs-knecht hier?", "tokens": ["Ist", "ein", "ent\u00b7lauff\u00b7ner", "kriegs\u00b7knecht", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bringt ihn eilends her zu mir;", "tokens": ["So", "bringt", "ihn", "ei\u00b7lends", "her", "zu", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo nicht? so laufft zur klugen frauen:", "tokens": ["Wo", "nicht", "?", "so", "laufft", "zur", "klu\u00b7gen", "frau\u00b7en", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$.", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn dieses ist ein solches paar,", "tokens": ["Denn", "die\u00b7ses", "ist", "ein", "sol\u00b7ches", "paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf welches man bey der gefahr", "tokens": ["Auf", "wel\u00b7ches", "man", "bey", "der", "ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Offt gl\u00fccklich pfleget viel zu bauen.", "tokens": ["Offt", "gl\u00fcck\u00b7lich", "pfle\u00b7get", "viel", "zu", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch last die kluge frau nur seyn;", "tokens": ["Doch", "last", "die", "klu\u00b7ge", "frau", "nur", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir f\u00e4llt itzund was bessers ein:", "tokens": ["Mir", "f\u00e4llt", "it\u00b7zund", "was", "bes\u00b7sers", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Holt mir an deren statt Belinden!", "tokens": ["Holt", "mir", "an", "de\u00b7ren", "statt", "Be\u00b7lin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDS", "APPR", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Denn diese kostets einen ku\u00df,", "tokens": ["Denn", "die\u00b7se", "kos\u00b7tets", "ei\u00b7nen", "ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wei\u00df ich, da\u00df das feuer mu\u00df", "tokens": ["So", "wei\u00df", "ich", ",", "da\u00df", "das", "feu\u00b7er", "mu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie ein schneller blitz verschwinden.", "tokens": ["Als", "wie", "ein", "schnel\u00b7ler", "blitz", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}