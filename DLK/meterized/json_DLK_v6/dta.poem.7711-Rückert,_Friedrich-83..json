{"dta.poem.7711": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rings um mich her im Haus ein stillgesch\u00e4ftig Regen", "tokens": ["Rings", "um", "mich", "her", "im", "Haus", "ein", "still\u00b7ge\u00b7sch\u00e4f\u00b7tig", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "APZR", "APPRART", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist meinen sinnenden Gedanken nicht entgegen.", "tokens": ["Ist", "mei\u00b7nen", "sin\u00b7nen\u00b7den", "Ge\u00b7dan\u00b7ken", "nicht", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Behaglich f\u00fchlt sich drin der kleine Sch\u00f6pfer Geist,", "tokens": ["Be\u00b7hag\u00b7lich", "f\u00fchlt", "sich", "drin", "der", "klei\u00b7ne", "Sch\u00f6p\u00b7fer", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem gro\u00dfen gleich, um den der Sch\u00f6pfung Einklang kreist.", "tokens": ["Dem", "gro\u00b7\u00dfen", "gleich", ",", "um", "den", "der", "Sch\u00f6p\u00b7fung", "Ein\u00b7klang", "kreist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "KOUI", "ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch ein Geschrei zerrei\u00dft den leichten Sch\u00f6pferwahn,", "tokens": ["Doch", "ein", "Ge\u00b7schrei", "zer\u00b7rei\u00dft", "den", "leich\u00b7ten", "Sch\u00f6p\u00b7fer\u00b7wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und um die Harmonie der Sf\u00e4ren ists gethan.", "tokens": ["Und", "um", "die", "Har\u00b7mo\u00b7nie", "der", "Sf\u00e4\u00b7ren", "ists", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}