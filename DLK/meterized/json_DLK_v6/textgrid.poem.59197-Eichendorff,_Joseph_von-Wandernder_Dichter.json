{"textgrid.poem.59197": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Wandernder Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was das sagen will!", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "das", "sa\u00b7gen", "will", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum tret ich von der Schwelle still,", "tokens": ["Kaum", "tret", "ich", "von", "der", "Schwel\u00b7le", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich schwingt sich eine Lerche auf", "tokens": ["Gleich", "schwingt", "sich", "ei\u00b7ne", "Ler\u00b7che", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jubiliert durchs Blau vorauf.", "tokens": ["Und", "ju\u00b7bi\u00b7liert", "durchs", "Blau", "vor\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Gras ringsum, die Blumen gar", "tokens": ["Das", "Gras", "ring\u00b7sum", ",", "die", "Blu\u00b7men", "gar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stehn mit Juwelen und Perln im Haar,", "tokens": ["Stehn", "mit", "Ju\u00b7we\u00b7len", "und", "Perln", "im", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die schlanken Pappeln, Busch und Saat", "tokens": ["Die", "schlan\u00b7ken", "Pap\u00b7peln", ",", "Busch", "und", "Saat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verneigen sich im gr\u00f6\u00dften Staat.", "tokens": ["Ver\u00b7nei\u00b7gen", "sich", "im", "gr\u00f6\u00df\u00b7ten", "Staat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als Bot voraus das B\u00e4chlein eilt,", "tokens": ["Als", "Bot", "vo\u00b7raus", "das", "B\u00e4ch\u00b7lein", "eilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wo der Wind die Wipfel teilt,", "tokens": ["Und", "wo", "der", "Wind", "die", "Wip\u00b7fel", "teilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Au verstohlen nach mir schaut,", "tokens": ["Die", "Au", "ver\u00b7stoh\u00b7len", "nach", "mir", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4r sie meine liebe Braut.", "tokens": ["Als", "w\u00e4r", "sie", "mei\u00b7ne", "lie\u00b7be", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, komm ich m\u00fcd ins Nachtquartier,", "tokens": ["Ja", ",", "komm", "ich", "m\u00fcd", "ins", "Nacht\u00b7quar\u00b7tier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nachtigall noch vor der T\u00fcr", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "noch", "vor", "der", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir St\u00e4ndchen bringt, Gl\u00fchw\u00fcrmchen bald", "tokens": ["Mir", "St\u00e4nd\u00b7chen", "bringt", ",", "Gl\u00fch\u00b7w\u00fcrm\u00b7chen", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "VVFIN", "$,", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Illuminieren rings den Wald.", "tokens": ["Il\u00b7lu\u00b7mi\u00b7nie\u00b7ren", "rings", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Umsonst! das ist nun einmal so,", "tokens": ["Um\u00b7sonst", "!", "das", "ist", "nun", "ein\u00b7mal", "so", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PDS", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Dichter reist inkognito,", "tokens": ["Kein", "Dich\u00b7ter", "reist", "in\u00b7kog\u00b7ni\u00b7to", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der lust'ge Fr\u00fchling merkt es gleich,", "tokens": ["Der", "lust'\u00b7ge", "Fr\u00fch\u00b7ling", "merkt", "es", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer K\u00f6nig ist in seinem Reich.", "tokens": ["Wer", "K\u00f6\u00b7nig", "ist", "in", "sei\u00b7nem", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}