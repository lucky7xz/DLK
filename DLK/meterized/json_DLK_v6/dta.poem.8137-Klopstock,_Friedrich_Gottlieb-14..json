{"dta.poem.8137": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ernst ist er des Gerichts dunkler Tag;", "tokens": ["Ernst", "ist", "er", "des", "Ge\u00b7richts", "dunk\u00b7ler", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Todesgang und des Sturms Flug eilt des Herrn", "tokens": ["To\u00b7des\u00b7gang", "und", "des", "Sturms", "Flug", "eilt", "des", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gerichtstag! Prophezeihung gegen sie,", "tokens": ["Ge\u00b7richts\u00b7tag", "!", "Pro\u00b7phe\u00b7zei\u00b7hung", "ge\u00b7gen", "sie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bew\u00f6lkt einst, Prophezeihung, wie erf\u00fcllt Gott dich!", "tokens": ["Be\u00b7w\u00f6lkt", "einst", ",", "Pro\u00b7phe\u00b7zei\u00b7hung", ",", "wie", "er\u00b7f\u00fcllt", "Gott", "dich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "$,", "PWAV", "VVPP", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}