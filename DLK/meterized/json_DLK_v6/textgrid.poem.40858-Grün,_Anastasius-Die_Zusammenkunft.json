{"textgrid.poem.40858": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Die Zusammenkunft", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kehrte singend wieder der Fr\u00fchlingsboten Chor,", "tokens": ["Es", "kehr\u00b7te", "sin\u00b7gend", "wie\u00b7der", "der", "Fr\u00fch\u00b7lings\u00b7bo\u00b7ten", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schon guckten frische Halme versch\u00e4mt zum Licht empor.", "tokens": ["Schon", "guck\u00b7ten", "fri\u00b7sche", "Hal\u00b7me", "ver\u00b7sch\u00e4mt", "zum", "Licht", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Max mit dem Baierherzog lustwallt' auf gr\u00fcnender Bahn,", "tokens": ["Max", "mit", "dem", "Bai\u00b7er\u00b7her\u00b7zog", "lust\u00b7wallt'", "auf", "gr\u00fc\u00b7nen\u00b7der", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und so zum treuen Freunde hob er zu sprechen an:", "tokens": ["Und", "so", "zum", "treu\u00b7en", "Freun\u00b7de", "hob", "er", "zu", "spre\u00b7chen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbmein Ludwig, sieh, wie das Leben so sch\u00f6n rings aufersteht,", "tokens": ["\u00bb", "mein", "Lud\u00b7wig", ",", "sieh", ",", "wie", "das", "Le\u00b7ben", "so", "sch\u00f6n", "rings", "auf\u00b7er\u00b7steht", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NE", "$,", "VVFIN", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie um Pal\u00e4st' und H\u00fctten die Freude gesch\u00e4ftig geht!", "tokens": ["Wie", "um", "Pa\u00b7l\u00e4st'", "und", "H\u00fct\u00b7ten", "die", "Freu\u00b7de", "ge\u00b7sch\u00e4f\u00b7tig", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Liebe Boten kehren jetzt wieder ins Land herein,", "tokens": ["Der", "Lie\u00b7be", "Bo\u00b7ten", "keh\u00b7ren", "jetzt", "wie\u00b7der", "ins", "Land", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auch du sollst meiner Liebe ein treuer Bote sein.", "tokens": ["Auch", "du", "sollst", "mei\u00b7ner", "Lie\u00b7be", "ein", "treu\u00b7er", "Bo\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Zieh hin zu Burgunds Maria, bring meinen Gru\u00df ihr dar,", "tokens": ["Zieh", "hin", "zu", "Bur\u00b7gunds", "Ma\u00b7ria", ",", "bring", "mei\u00b7nen", "Gru\u00df", "ihr", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "NE", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dann reiche, mein Vertreter, die Hand ihr am Altar,", "tokens": ["Dann", "rei\u00b7che", ",", "mein", "Ver\u00b7tre\u00b7ter", ",", "die", "Hand", "ihr", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und f\u00fchre sie ins Brautbett \u2013 ei l\u00e4chle, Schalk, mir nicht!", "tokens": ["Und", "f\u00fch\u00b7re", "sie", "ins", "Braut\u00b7bett", "\u2013", "ei", "l\u00e4ch\u00b7le", ",", "Schalk", ",", "mir", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$(", "NE", "VVFIN", "$,", "NN", "$,", "PPER", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Denn erzgewappnet zu liegen erheischet deine Pflicht.", "tokens": ["Denn", "erz\u00b7ge\u00b7wapp\u00b7net", "zu", "lie\u00b7gen", "er\u00b7hei\u00b7schet", "dei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKZU", "VVINF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Gewappnet am rechten Arme, gewappnet am rechten Bein,", "tokens": ["Ge\u00b7wapp\u00b7net", "am", "rech\u00b7ten", "Ar\u00b7me", ",", "ge\u00b7wapp\u00b7net", "am", "rech\u00b7ten", "Bein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das deute: in Kampf und Frieden soll ", "tokens": ["Das", "deu\u00b7te", ":", "in", "Kampf", "und", "Frie\u00b7den", "soll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "APPR", "NN", "KON", "NN", "VMFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und zwischen euch Beiden blitze ein scharfer blanker Degen,", "tokens": ["Und", "zwi\u00b7schen", "euch", "Bei\u00b7den", "blit\u00b7ze", "ein", "schar\u00b7fer", "blan\u00b7ker", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PIS", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das deute: weh dem Frevler, der unsrem Bund entgegen!\u00ab", "tokens": ["Das", "deu\u00b7te", ":", "weh", "dem", "Frev\u00b7ler", ",", "der", "uns\u00b7rem", "Bund", "ent\u00b7ge\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "$.", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Es ritt der Baierherzog frohl\u00e4chelnd seine Bahn,", "tokens": ["Es", "ritt", "der", "Bai\u00b7er\u00b7her\u00b7zog", "froh\u00b7l\u00e4\u00b7chelnd", "sei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und wer ihn sieht, wird fr\u00f6hlich, der Pfaff' und Kriegskumpan,", "tokens": ["Und", "wer", "ihn", "sieht", ",", "wird", "fr\u00f6h\u00b7lich", ",", "der", "Pfaff'", "und", "Kriegs\u00b7kum\u00b7pan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Bauern und die St\u00e4dter freu'n sich in ihrem Sinn,", "tokens": ["Die", "Bau\u00b7ern", "und", "die", "St\u00e4d\u00b7ter", "freu'n", "sich", "in", "ih\u00b7rem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und im Burgunderlande die sch\u00f6ne Herzogin!", "tokens": ["Und", "im", "Bur\u00b7gun\u00b7der\u00b7lan\u00b7de", "die", "sch\u00f6\u00b7ne", "Her\u00b7zo\u00b7gin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Zu Gent vor den Thoren wirbelt einst Staubgew\u00f6lk empor,", "tokens": ["Zu", "Gent", "vor", "den", "Tho\u00b7ren", "wir\u00b7belt", "einst", "Staub\u00b7ge\u00b7w\u00f6lk", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Draus flattert's und blitzt's wie F\u00e4hnlein, wie Panzer und Waffen hervor,", "tokens": ["Draus", "flat\u00b7tert's", "und", "blitzt's", "wie", "F\u00e4hn\u00b7lein", ",", "wie", "Pan\u00b7zer", "und", "Waf\u00b7fen", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "KOKOM", "NN", "$,", "PWAV", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Darinnen scharrt es und stampft es wie muthiger Rosse Huf,", "tokens": ["Da\u00b7rin\u00b7nen", "scharrt", "es", "und", "stampft", "es", "wie", "mut\u00b7hi\u00b7ger", "Ros\u00b7se", "Huf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Dazwischen singt es und klingt es wie jubelnder Krieger Ruf.", "tokens": ["Da\u00b7zwi\u00b7schen", "singt", "es", "und", "klingt", "es", "wie", "ju\u00b7beln\u00b7der", "Krie\u00b7ger", "Ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Jetzt zieht sich durch die Stra\u00dfen der reisige M\u00e4nnertro\u00df,", "tokens": ["Jetzt", "zieht", "sich", "durch", "die", "Stra\u00b7\u00dfen", "der", "rei\u00b7si\u00b7ge", "M\u00e4n\u00b7ner\u00b7tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Neunhundert deutsche Ritter, gewappnet, hoch zu Ro\u00df,", "tokens": ["Neun\u00b7hun\u00b7dert", "deut\u00b7sche", "Rit\u00b7ter", ",", "ge\u00b7wapp\u00b7net", ",", "hoch", "zu", "Ro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "VVPP", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Inmitten ist ein J\u00fcngling auf braunem Hengst zu sehn;", "tokens": ["In\u00b7mit\u00b7ten", "ist", "ein", "J\u00fcng\u00b7ling", "auf", "brau\u00b7nem", "Hengst", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jedoch, ihr Leute, was bleibt ihr All' vor dem Einen stehn?", "tokens": ["Je\u00b7doch", ",", "ihr", "Leu\u00b7te", ",", "was", "bleibt", "ihr", "All'", "vor", "dem", "Ei\u00b7nen", "stehn", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Er reitet wie die Gef\u00e4hrten in schlichtem Waffenglanz,", "tokens": ["Er", "rei\u00b7tet", "wie", "die", "Ge\u00b7f\u00e4hr\u00b7ten", "in", "schlich\u00b7tem", "Waf\u00b7fen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nur nickt im blonden Gelock ihm ein d\u00fcnner Perlenkranz;", "tokens": ["Nur", "nickt", "im", "blon\u00b7den", "Ge\u00b7lock", "ihm", "ein", "d\u00fcn\u00b7ner", "Per\u00b7len\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ist der es, oder die Flamme, die aus dem Aug' ihm f\u00e4hrt,", "tokens": ["Ist", "der", "es", ",", "o\u00b7der", "die", "Flam\u00b7me", ",", "die", "aus", "dem", "Aug'", "ihm", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPER", "$,", "KON", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was Jedem, der vorbeiwallt, vom Haupt die M\u00fctze kehrt?", "tokens": ["Was", "Je\u00b7dem", ",", "der", "vor\u00b7bei\u00b7wallt", ",", "vom", "Haupt", "die", "M\u00fct\u00b7ze", "kehrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "$,", "PRELS", "VVFIN", "$,", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Die Herzogin entgegen den deutschen Rittern zieht,", "tokens": ["Die", "Her\u00b7zo\u00b7gin", "ent\u00b7ge\u00b7gen", "den", "deut\u00b7schen", "Rit\u00b7tern", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihr Antlitz war der Spiegel, drin Sch\u00f6nheit sich besieht,", "tokens": ["Ihr", "Ant\u00b7litz", "war", "der", "Spie\u00b7gel", ",", "drin", "Sch\u00f6n\u00b7heit", "sich", "be\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In ihren Rabenlocken glomm der Demanten Pracht,", "tokens": ["In", "ih\u00b7ren", "Ra\u00b7ben\u00b7lo\u00b7cken", "glomm", "der", "De\u00b7man\u00b7ten", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+----+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wie eine Handvoll Sterne, ges\u00e4't in schwarze Nacht.", "tokens": ["Wie", "ei\u00b7ne", "Hand\u00b7voll", "Ster\u00b7ne", ",", "ge\u00b7s\u00e4't", "in", "schwar\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Sie blickt dem Heldenj\u00fcngling ins Gluthenaug' hinein:", "tokens": ["Sie", "blickt", "dem", "Hel\u00b7den\u00b7j\u00fcng\u00b7ling", "ins", "Glu\u00b7the\u00b7naug'", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbei, wie's da flammt wie Saphire, wie heller Demantschein!\u00ab", "tokens": ["\u00bb", "ei", ",", "wie's", "da", "flammt", "wie", "Sa\u00b7phi\u00b7re", ",", "wie", "hel\u00b7ler", "De\u00b7mant\u00b7schein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "ADV", "VVFIN", "KOKOM", "NE", "$,", "PWAV", 