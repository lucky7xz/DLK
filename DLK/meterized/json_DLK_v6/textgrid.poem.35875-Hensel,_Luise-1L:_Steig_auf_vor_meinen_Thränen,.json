{"textgrid.poem.35875": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steig auf vor meinen Thr\u00e4nen,", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steig auf vor meinen Thr\u00e4nen,", "tokens": ["Steig", "auf", "vor", "mei\u00b7nen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Maria, Meeresstern!", "tokens": ["Ma\u00b7ria", ",", "Mee\u00b7res\u00b7stern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dich suchet all mein Sehnen,", "tokens": ["Dich", "su\u00b7chet", "all", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich, Mutter meines Herrn.", "tokens": ["Dich", ",", "Mut\u00b7ter", "mei\u00b7nes", "Herrn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dir d\u00fcrft' ich Alles klagen,", "tokens": ["Dir", "d\u00fcrft'", "ich", "Al\u00b7les", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was mir das Herz bewegt,", "tokens": ["Was", "mir", "das", "Herz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir alle W\u00fcnsche sagen,", "tokens": ["Dir", "al\u00b7le", "W\u00fcn\u00b7sche", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die heimlich es gehegt.", "tokens": ["Die", "heim\u00b7lich", "es", "ge\u00b7hegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dich, K\u00f6nigin der Hulden,", "tokens": ["Dich", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Hul\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich, Herrin, m\u00f6cht' ich sehn!", "tokens": ["Dich", ",", "Her\u00b7rin", ",", "m\u00f6cht'", "ich", "sehn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir k\u00f6nnt' ich mein Verschulden,", "tokens": ["Dir", "k\u00f6nnt'", "ich", "mein", "Ver\u00b7schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Irren all gestehn.", "tokens": ["Mein", "Ir\u00b7ren", "all", "ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur einmal m\u00f6cht ich weinen", "tokens": ["Nur", "ein\u00b7mal", "m\u00f6cht", "ich", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor Deinem Angesicht;", "tokens": ["Vor", "Dei\u00b7nem", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das w\u00fcrde freundlich scheinen,", "tokens": ["Das", "w\u00fcr\u00b7de", "freund\u00b7lich", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das straft und z\u00fcrnet nicht.", "tokens": ["Das", "straft", "und", "z\u00fcr\u00b7net", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O sel'ge Mutter dr\u00fcben,", "tokens": ["O", "sel'\u00b7ge", "Mut\u00b7ter", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sieh mild herab auf mich!", "tokens": ["Sieh", "mild", "her\u00b7ab", "auf", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df Dich ewig lieben", "tokens": ["Ich", "mu\u00df", "Dich", "e\u00b7wig", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So recht herzinniglich.", "tokens": ["So", "recht", "her\u00b7zin\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es soll ja all mein Streben", "tokens": ["Es", "soll", "ja", "all", "mein", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Deiner Ehre sein;", "tokens": ["Zu", "Dei\u00b7ner", "Eh\u00b7re", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will ja all mein Leben", "tokens": ["Ich", "will", "ja", "all", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur Deinem Dienste weihn.", "tokens": ["Nur", "Dei\u00b7nem", "Diens\u00b7te", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}