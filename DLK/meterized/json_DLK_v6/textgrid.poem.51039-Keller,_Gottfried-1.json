{"textgrid.poem.51039": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo sich drei Gassen kreuzen, krumm und enge,", "tokens": ["Wo", "sich", "drei", "Gas\u00b7sen", "kreu\u00b7zen", ",", "krumm", "und", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "CARD", "NN", "VVINF", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drei Z\u00fcge wallen pl\u00f6tzlich sich entgegen", "tokens": ["Drei", "Z\u00fc\u00b7ge", "wal\u00b7len", "pl\u00f6tz\u00b7lich", "sich", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVINF", "ADJD", "PRF", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schlingen sich, gehemmt auf ihren Wegen,", "tokens": ["Und", "schlin\u00b7gen", "sich", ",", "ge\u00b7hemmt", "auf", "ih\u00b7ren", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu einem Kn\u00e4ul und l\u00e4rmendem Gedr\u00e4nge.", "tokens": ["Zu", "ei\u00b7nem", "Kn\u00e4ul", "und", "l\u00e4r\u00b7men\u00b7dem", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Wachtparad' mit gellen Trommelschl\u00e4gen,", "tokens": ["Die", "Wacht\u00b7pa\u00b7rad'", "mit", "gel\u00b7len", "Trom\u00b7mel\u00b7schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Brautzug kommt mit Geigen und Gepr\u00e4nge,", "tokens": ["Ein", "Braut\u00b7zug", "kommt", "mit", "Gei\u00b7gen", "und", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Leichenzug klagt seine Grabges\u00e4nge:", "tokens": ["Ein", "Lei\u00b7chen\u00b7zug", "klagt", "sei\u00b7ne", "Grab\u00b7ge\u00b7s\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das alles stockt, es kann kein Glied sich regen.", "tokens": ["Das", "al\u00b7les", "stockt", ",", "es", "kann", "kein", "Glied", "sich", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "$,", "PPER", "VMFIN", "PIAT", "NN", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verstummt sind Geiger, Pfaff und Trommelschl\u00e4ger;", "tokens": ["Ver\u00b7stummt", "sind", "Gei\u00b7ger", ",", "Pfaff", "und", "Trom\u00b7mel\u00b7schl\u00e4\u00b7ger", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der dicke Hauptmann flucht, da\u00df niemand weiche,", "tokens": ["Der", "di\u00b7cke", "Haupt\u00b7mann", "flucht", ",", "da\u00df", "nie\u00b7mand", "wei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PIS", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gel\u00e4chter schallet aus dem Freudenzug.", "tokens": ["Ge\u00b7l\u00e4ch\u00b7ter", "schal\u00b7let", "aus", "dem", "Freu\u00b7den\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch oben, auf den Schultern schwarzer Tr\u00e4ger", "tokens": ["Doch", "o\u00b7ben", ",", "auf", "den", "Schul\u00b7tern", "schwar\u00b7zer", "Tr\u00e4\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Starrt in der Mitte kalt und still die Leiche", "tokens": ["Starrt", "in", "der", "Mit\u00b7te", "kalt", "und", "still", "die", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit blinden Augen in den Wolkenflug.", "tokens": ["Mit", "blin\u00b7den", "Au\u00b7gen", "in", "den", "Wol\u00b7ken\u00b7flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}