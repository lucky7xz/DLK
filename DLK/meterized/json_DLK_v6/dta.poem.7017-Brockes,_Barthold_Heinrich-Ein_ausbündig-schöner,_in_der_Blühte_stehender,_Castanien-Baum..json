{"dta.poem.7017": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein ausb\u00fcndig-sch\u00f6ner, in der  \n Bl\u00fchte stehender, Castanien-Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So deucht mich, da\u00df ein Baum, der recht vor meiner Th\u00fcr", "tokens": ["So", "deucht", "mich", ",", "da\u00df", "ein", "Baum", ",", "der", "recht", "vor", "mei\u00b7ner", "Th\u00fcr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gepflantzt war, zu mir sprach: Die pr\u00e4chtige Gestalt,", "tokens": ["Ge\u00b7pflantzt", "war", ",", "zu", "mir", "sprach", ":", "Die", "pr\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "APPR", "PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des hohen Wipffels Breit\u2019 und Dicke,", "tokens": ["Des", "ho\u00b7hen", "Wipf\u00b7fels", "Breit'", "und", "Di\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stellt gleichsam einen gantzen Wald", "tokens": ["Stellt", "gleich\u00b7sam", "ei\u00b7nen", "gant\u00b7zen", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Schatten reich- und gr\u00fcner D\u00e4mm\u2019rung f\u00fcr.", "tokens": ["In", "Schat\u00b7ten", "reich", "und", "gr\u00fc\u00b7ner", "D\u00e4m\u00b7m'\u00b7rung", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "TRUNC", "KON", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Der Zweige Meng\u2019 und zierliches Geschicke,", "tokens": ["Der", "Zwei\u00b7ge", "Meng'", "und", "zier\u00b7li\u00b7ches", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die alle, wie ein halber Bogen,", "tokens": ["Die", "al\u00b7le", ",", "wie", "ein", "hal\u00b7ber", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich von dem Stamm ab-aufwerts zogen,", "tokens": ["Sich", "von", "dem", "Stamm", "ab\u00b7auf\u00b7werts", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von Bl\u00e4ttern gantz umw\u00f6lckt, formirten", "tokens": ["Von", "Bl\u00e4t\u00b7tern", "gantz", "um\u00b7w\u00f6lckt", ",", "for\u00b7mir\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Jm sch\u00f6nsten Wipffel, eine Crone,", "tokens": ["Jm", "sch\u00f6ns\u00b7ten", "Wipf\u00b7fel", ",", "ei\u00b7ne", "Cro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So zierlich, da\u00df auch kaum", "tokens": ["So", "zier\u00b7lich", ",", "da\u00df", "auch", "kaum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Der zierlichste Orange-Baum,", "tokens": ["Der", "zier\u00b7lichs\u00b7te", "O\u00b7ran\u00b7ge\u00b7Baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der in der R\u00fcnde zeigt, was Kunst und Scheere k\u00f6nnen,", "tokens": ["Der", "in", "der", "R\u00fcn\u00b7de", "zeigt", ",", "was", "Kunst", "und", "Schee\u00b7re", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "PWS", "NN", "KON", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sich weigert, ihm den Rang zu g\u00f6nnen.", "tokens": ["Sich", "wei\u00b7gert", ",", "ihm", "den", "Rang", "zu", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}