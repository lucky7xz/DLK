{"textgrid.poem.60498": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Ein Heimatbruder!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wandrer, irrend in der Ferne,", "tokens": ["Der", "Wand\u00b7rer", ",", "ir\u00b7rend", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo fremd das Tier, der Baum, das Kraut,", "tokens": ["Wo", "fremd", "das", "Tier", ",", "der", "Baum", ",", "das", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo fremd die Nacht und ihre Sterne,", "tokens": ["Wo", "fremd", "die", "Nacht", "und", "ih\u00b7re", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo fremd und tot der Menschenlaut,", "tokens": ["Wo", "fremd", "und", "tot", "der", "Men\u00b7schen\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie f\u00fchlt er sich allein, versto\u00dfen,", "tokens": ["Wie", "f\u00fchlt", "er", "sich", "al\u00b7lein", ",", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie jauchzt sein Herz im fremden Land,", "tokens": ["Wie", "jauchzt", "sein", "Herz", "im", "frem\u00b7den", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn pl\u00f6tzlich er den Sprachgenossen,", "tokens": ["Wenn", "pl\u00f6tz\u00b7lich", "er", "den", "Sprach\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den heimatlichen Bruder fand!", "tokens": ["Den", "hei\u00b7mat\u00b7li\u00b7chen", "Bru\u00b7der", "fand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}