{"dta.poem.3302": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Sommerlaube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Die Laube prangt mit jungem Gr\u00fcn:", "tokens": ["Die", "Lau\u00b7be", "prangt", "mit", "jun\u00b7gem", "Gr\u00fcn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es t\u00f6nen ihre dunkeln Buchen", "tokens": ["Es", "t\u00f6\u00b7nen", "ih\u00b7re", "dun\u00b7keln", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von V\u00f6geln, die voll Wollust gl\u00fchn,", "tokens": ["Von", "V\u00f6\u00b7geln", ",", "die", "voll", "Wol\u00b7lust", "gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Fr\u00fchlingstrieben gl\u00fchn und Scherz und Schatten su-", "tokens": ["Von", "Fr\u00fch\u00b7ling\u00b7strie\u00b7ben", "gl\u00fchn", "und", "Scherz", "und", "Schat\u00b7ten", "su"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "KON", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "chen.", "tokens": ["chen", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Soll, was der Wahn Gesch\u00e4fte nennt,", "tokens": ["Soll", ",", "was", "der", "Wahn", "Ge\u00b7sch\u00e4f\u00b7te", "nennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Uns um so sch\u00f6ne Zeit betr\u00fcgen?", "tokens": ["Uns", "um", "so", "sch\u00f6\u00b7ne", "Zeit", "be\u00b7tr\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Freund! wer des Lebens K\u00fcrze kennt,", "tokens": ["Freund", "!", "wer", "des", "Le\u00b7bens", "K\u00fcr\u00b7ze", "kennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der legt es kl\u00fcger an und braucht es zum Vergn\u00fcgen.", "tokens": ["Der", "legt", "es", "kl\u00fc\u00b7ger", "an", "und", "braucht", "es", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Geneu\u00df den feuervollen Wein:", "tokens": ["Ge\u00b7neu\u00df", "den", "feu\u00b7er\u00b7vol\u00b7len", "Wein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beym Weine herrscht vertraulich Scherzen.", "tokens": ["Beym", "Wei\u00b7ne", "herrscht", "ver\u00b7trau\u00b7lich", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Oft ladet Amor sich mit ein,", "tokens": ["Oft", "la\u00b7det", "A\u00b7mor", "sich", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PRF", "APPR", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sein verborgner Pfeil schleicht in die offnen Herzen.", "tokens": ["Und", "sein", "ver\u00b7borg\u00b7ner", "Pfeil", "schleicht", "in", "die", "off\u00b7nen", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der schlaue Gott ist niemals weit;", "tokens": ["Der", "schlau\u00b7e", "Gott", "ist", "nie\u00b7mals", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wittre seine sanften Triebe:", "tokens": ["Ich", "witt\u00b7re", "sei\u00b7ne", "sanf\u00b7ten", "Trie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn gr\u00fcner Lauben Dunkelheit", "tokens": ["Denn", "gr\u00fc\u00b7ner", "Lau\u00b7ben", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist f\u00fcr den Weingott sch\u00f6n, noch sch\u00f6ner f\u00fcr die Liebe.", "tokens": ["Ist", "f\u00fcr", "den", "Wein\u00b7gott", "sch\u00f6n", ",", "noch", "sch\u00f6\u00b7ner", "f\u00fcr", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "$,", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Geliebte Schatten! weicher Klee!", "tokens": ["Ge\u00b7lieb\u00b7te", "Schat\u00b7ten", "!", "wei\u00b7cher", "Klee", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach! w\u00e4re Galathee zugegen!", "tokens": ["Ach", "!", "w\u00e4\u00b7re", "Ga\u00b7la\u00b7thee", "zu\u00b7ge\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! sollt ich, holde Galathee,", "tokens": ["Ach", "!", "sollt", "ich", ",", "hol\u00b7de", "Ga\u00b7la\u00b7thee", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um deinen weissen Hals die Arme br\u00fcnstig legen", "tokens": ["Um", "dei\u00b7nen", "weis\u00b7sen", "Hals", "die", "Ar\u00b7me", "br\u00fcns\u00b7tig", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wo s\u00fcsser Lippen Rosen bl\u00fchn,", "tokens": ["Wo", "s\u00fcs\u00b7ser", "Lip\u00b7pen", "Ro\u00b7sen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer kann sie sehn und nicht verlangen?", "tokens": ["Wer", "kann", "sie", "sehn", "und", "nicht", "ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die jugendlichen K\u00fcsse fliehn", "tokens": ["Die", "ju\u00b7gend\u00b7li\u00b7chen", "K\u00fcs\u00b7se", "fliehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey welkem Reiz vorbey und suchen frische Wangen.", "tokens": ["Bey", "wel\u00b7kem", "Reiz", "vor\u00b7bey", "und", "su\u00b7chen", "fri\u00b7sche", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PTKVZ", "KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ein leblos Auge r\u00fchrt mich nicht;", "tokens": ["Ein", "leb\u00b7los", "Au\u00b7ge", "r\u00fchrt", "mich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein bl\u00f6des Kind wird mich gewinnen,", "tokens": ["Kein", "bl\u00f6\u00b7des", "Kind", "wird", "mich", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das reizt, solang der Mund nicht spricht,", "tokens": ["Das", "reizt", ",", "so\u00b7lang", "der", "Mund", "nicht", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eine Venus ist, doch ohne Charitinnen.", "tokens": ["Und", "ei\u00b7ne", "Ve\u00b7nus", "ist", ",", "doch", "oh\u00b7ne", "Cha\u00b7ri\u00b7tin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}