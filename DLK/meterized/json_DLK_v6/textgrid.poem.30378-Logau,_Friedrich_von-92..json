{"textgrid.poem.30378": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Babel worden sch\u00f6ne T\u00f6chter auff freyem Marckte feil gestellt;", "tokens": ["Zu", "Ba\u00b7bel", "wor\u00b7den", "sch\u00f6\u00b7ne", "T\u00f6ch\u00b7ter", "auff", "frey\u00b7em", "Marck\u00b7te", "feil", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAPP", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Die ungestalten aber namen zur Mitgifft so gel\u00f6stes Geld.", "tokens": ["Die", "un\u00b7ge\u00b7stal\u00b7ten", "a\u00b7ber", "na\u00b7men", "zur", "Mit\u00b7gifft", "so", "ge\u00b7l\u00f6s\u00b7tes", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "NN", "APPRART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+--++--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Wann dieses heute noch bey Tage solt ebenm\u00e4ssig auch geschehn,", "tokens": ["Wann", "die\u00b7ses", "heu\u00b7te", "noch", "bey", "Ta\u00b7ge", "solt", "e\u00b7ben\u00b7m\u00e4s\u00b7sig", "auch", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "ADV", "APPR", "NN", "VMFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+--++--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "So wer es gut f\u00fcr solche Freyer, die nur auff schn\u00f6de M\u00fcntze sehn.", "tokens": ["So", "wer", "es", "gut", "f\u00fcr", "sol\u00b7che", "Frey\u00b7er", ",", "die", "nur", "auff", "schn\u00f6\u00b7de", "M\u00fcnt\u00b7ze", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "ADJD", "APPR", "PIAT", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Ich aber, wann ich diesem Brauche nach Willen solte pflichten bey,", "tokens": ["Ich", "a\u00b7ber", ",", "wann", "ich", "die\u00b7sem", "Brau\u00b7che", "nach", "Wil\u00b7len", "sol\u00b7te", "pflich\u00b7ten", "bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAV", "PPER", "PDAT", "NN", "APPR", "NN", "VMFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "So meint ich, da\u00df allhier das geben viel seliger als nehmen sey.", "tokens": ["So", "meint", "ich", ",", "da\u00df", "all\u00b7hier", "das", "ge\u00b7ben", "viel", "se\u00b7li\u00b7ger", "als", "neh\u00b7men", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "PDS", "VVFIN", "ADV", "ADJD", "KOKOM", "VVINF", "VAFIN", "$."], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}