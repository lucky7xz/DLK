{"textgrid.poem.64609": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Pharao w\u00e4hlt einer der Poeten,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Pharao w\u00e4hlt einer der Poeten,", "tokens": ["Das", "Pha\u00b7rao", "w\u00e4hlt", "ei\u00b7ner", "der", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Zeit gedankenlos zu t\u00f6dten;", "tokens": ["Die", "Zeit", "ge\u00b7dan\u00b7ken\u00b7los", "zu", "t\u00f6d\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der andre, der auf Vortheil sinnt,", "tokens": ["Der", "and\u00b7re", ",", "der", "auf", "Vor\u00b7theil", "sinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4hlt L'hombre, wo sein Witz gewinnt.", "tokens": ["W\u00e4hlt", "L'\u00b7hom\u00b7bre", ",", "wo", "sein", "Witz", "ge\u00b7winnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Wenn sie es noch wie der, den sie verachten,", "tokens": ["Wenn", "sie", "es", "noch", "wie", "der", ",", "den", "sie", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "KOKOM", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der unersch\u00f6pfte ", "tokens": ["Der", "un\u00b7er\u00b7sch\u00f6pf\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der Mann versteht doch wirklich mehr,", "tokens": ["Der", "Mann", "ver\u00b7steht", "doch", "wirk\u00b7lich", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn vortheilhaft f\u00fcr sich, und doch gedankenleer", "tokens": ["Denn", "vor\u00b7theil\u00b7haft", "f\u00fcr", "sich", ",", "und", "doch", "ge\u00b7dan\u00b7ken\u00b7leer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PRF", "$,", "KON", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die lange Zeit sich zu vertreiben:", "tokens": ["Die", "lan\u00b7ge", "Zeit", "sich", "zu", "ver\u00b7trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Was thut er wohl? F\u00fcr zwanzig Pressen schreiben.", "tokens": ["Was", "thut", "er", "wohl", "?", "F\u00fcr", "zwan\u00b7zig", "Pres\u00b7sen", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}