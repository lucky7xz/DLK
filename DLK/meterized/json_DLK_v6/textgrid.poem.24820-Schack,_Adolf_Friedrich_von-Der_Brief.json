{"textgrid.poem.24820": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Brief", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts ist mir von dir geblieben", "tokens": ["Nichts", "ist", "mir", "von", "dir", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Als der Brief, den du geschrieben,", "tokens": ["Als", "der", "Brief", ",", "den", "du", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meines Lebens h\u00f6chstes Gut;", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "h\u00f6chs\u00b7tes", "Gut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag das Auge mir erblinden,", "tokens": ["Mag", "das", "Au\u00b7ge", "mir", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00f6stung kann ich einzig finden,", "tokens": ["Tr\u00f6s\u00b7tung", "kann", "ich", "ein\u00b7zig", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn es auf dem Blatte ruht.", "tokens": ["Wenn", "es", "auf", "dem", "Blat\u00b7te", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann erstehn mir sel'ge Stunden", "tokens": ["Dann", "er\u00b7stehn", "mir", "sel'\u00b7ge", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Wonnen, die geschwunden,", "tokens": ["Mit", "den", "Won\u00b7nen", ",", "die", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder aus der Totengruft;", "tokens": ["Wie\u00b7der", "aus", "der", "To\u00b7ten\u00b7gruft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und um meine wehmuttrunkne", "tokens": ["Und", "um", "mei\u00b7ne", "weh\u00b7mut\u00b7trunk\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seele hauchen lang versunkne", "tokens": ["See\u00b7le", "hau\u00b7chen", "lang", "ver\u00b7sunk\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lenze ihren Bl\u00fctenduft.", "tokens": ["Len\u00b7ze", "ih\u00b7ren", "Bl\u00fc\u00b7ten\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ueber mir im Abendwinde", "tokens": ["Ue\u00b7ber", "mir", "im", "Ab\u00b7end\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauscht das Wipfellaub der Linde", "tokens": ["Rauscht", "das", "Wip\u00b7fel\u00b7laub", "der", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So wie ehmals wiederum,", "tokens": ["So", "wie", "eh\u00b7mals", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wir Arm in Arm gelegen", "tokens": ["Als", "wir", "Arm", "in", "Arm", "ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nur mit des Herzens Schl\u00e4gen", "tokens": ["Und", "nur", "mit", "des", "Her\u00b7zens", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zwiesprach hielten, wonnestumm.", "tokens": ["Zwie\u00b7sprach", "hiel\u00b7ten", ",", "won\u00b7nes\u00b7tumm", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und dann ist mir, auf dem Blatte", "tokens": ["Und", "dann", "ist", "mir", ",", "auf", "dem", "Blat\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ruhe neben mir dein Schatte", "tokens": ["Ru\u00b7he", "ne\u00b7ben", "mir", "dein", "Schat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "In dem blassen D\u00e4mmerlicht;", "tokens": ["In", "dem", "blas\u00b7sen", "D\u00e4m\u00b7mer\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O, an ihm im langen, langen", "tokens": ["O", ",", "an", "ihm", "im", "lan\u00b7gen", ",", "lan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "APPR", "PPER", "APPRART", "ADJA", "$,", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Kusse soll mein Mund noch hangen,", "tokens": ["Kus\u00b7se", "soll", "mein", "Mund", "noch", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn im Tod mein Auge bricht.", "tokens": ["Wenn", "im", "Tod", "mein", "Au\u00b7ge", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}