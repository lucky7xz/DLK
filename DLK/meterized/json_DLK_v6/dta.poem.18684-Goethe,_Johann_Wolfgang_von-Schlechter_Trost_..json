{"dta.poem.18684": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Schlechter Trost .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Mitternachts weint\u2019 und schluchtzt\u2019 ich,", "tokens": ["Mit\u00b7ter\u00b7nachts", "weint'", "und", "schluchtzt'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Weil ich dein entbehrte.", "tokens": ["Weil", "ich", "dein", "ent\u00b7behr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da kamen Nachtgespenster", "tokens": ["Da", "ka\u00b7men", "Nacht\u00b7ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ich sch\u00e4mte mich.", "tokens": ["Und", "ich", "sch\u00e4m\u00b7te", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nachtgespenster, sagt ich,", "tokens": ["Nacht\u00b7ge\u00b7spens\u00b7ter", ",", "sagt", "ich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schluchzend und weinend", "tokens": ["Schluch\u00b7zend", "und", "wei\u00b7nend"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Feindet ihr mich, dem ihr sonst", "tokens": ["Fein\u00b7det", "ihr", "mich", ",", "dem", "ihr", "sonst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PRELS", "PPER", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Schlafendem vor\u00fcberzogt.", "tokens": ["Schla\u00b7fen\u00b7dem", "vor\u00b7\u00fc\u00b7berz\u00b7ogt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Grosse G\u00fcter vermiss\u2019 ich.", "tokens": ["Gros\u00b7se", "G\u00fc\u00b7ter", "ver\u00b7miss'", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Denkt nicht schlimmer von mir", "tokens": ["Denkt", "nicht", "schlim\u00b7mer", "von", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADJD", "APPR", "PPER"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Den ihr sonst weise nanntet,", "tokens": ["Den", "ihr", "sonst", "wei\u00b7se", "nann\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Grosses Uebel betrifft ihn! \u2014", "tokens": ["Gros\u00b7ses", "Ue\u00b7bel", "be\u00b7tr\u00b7ifft", "ihn", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Und die Nachtgespenster", "tokens": ["Und", "die", "Nacht\u00b7ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Mit langen Gesichtern", "tokens": ["Mit", "lan\u00b7gen", "Ge\u00b7sich\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Zogen vorbey,", "tokens": ["Zo\u00b7gen", "vor\u00b7bey", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Ob ich weise oder th\u00f6rig", "tokens": ["Ob", "ich", "wei\u00b7se", "o\u00b7der", "th\u00f6\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "V\u00f6llig unbek\u00fcmmert.", "tokens": ["V\u00f6l\u00b7lig", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}