{"dta.poem.12400": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der angeschossene Kukuk .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6r' eine wunderliche Stimm:               ", "tokens": ["Ich", "h\u00f6r'", "ei\u00b7ne", "wun\u00b7der\u00b7li\u00b7che", "Stimm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Von Fern im Echo ich vernimm:", "tokens": ["Von", "Fern", "im", "E\u00b7cho", "ich", "ver\u00b7nimm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So oft ich diese Stimm anh\u00f6r,", "tokens": ["So", "oft", "ich", "die\u00b7se", "Stimm", "an\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PDAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht mirs allmal noch Freude mehr:", "tokens": ["Macht", "mirs", "all\u00b7mal", "noch", "Freu\u00b7de", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "ADV", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Kukuk! Kukuk! Kukuk!", "tokens": ["Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Den Vogel mu\u00df ich treffen an,", "tokens": ["Den", "Vo\u00b7gel", "mu\u00df", "ich", "tref\u00b7fen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Weil er so lieblich singen kann,", "tokens": ["Weil", "er", "so", "lieb\u00b7lich", "sin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sollt ich den Wald auf alle Seit", "tokens": ["Sollt", "ich", "den", "Wald", "auf", "al\u00b7le", "Seit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und auch die B\u00fcsche auslaufen heut,", "tokens": ["Und", "auch", "die", "B\u00fc\u00b7sche", "aus\u00b7lau\u00b7fen", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Kukuk! Kukuk! Kukuk!", "tokens": ["Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Was schau ich dort im gr\u00fcnen Gras?", "tokens": ["Was", "schau", "ich", "dort", "im", "gr\u00fc\u00b7nen", "Gras", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ist es ein Fuchs oder ists ein Has?", "tokens": ["Ist", "es", "ein", "Fuchs", "o\u00b7der", "ists", "ein", "Has", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NE", "KON", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ich wei\u00df nicht soll ich schie\u00dfen drein,", "tokens": ["Ich", "wei\u00df", "nicht", "soll", "ich", "schie\u00b7\u00dfen", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Oder soll ichs noch lassen seyn?", "tokens": ["O\u00b7der", "soll", "ichs", "noch", "las\u00b7sen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kukuk! Kukuk! Kukuk!", "tokens": ["Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ich bin zwar ein gut J\u00e4gersmann,", "tokens": ["Ich", "bin", "zwar", "ein", "gut", "J\u00e4\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und traue mich doch nicht heran,", "tokens": ["Und", "trau\u00b7e", "mich", "doch", "nicht", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So ein gar junges sch\u00f6nes Thier", "tokens": ["So", "ein", "gar", "jun\u00b7ges", "sch\u00f6\u00b7nes", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hab ich noch nicht getroffen hier.", "tokens": ["Hab", "ich", "noch", "nicht", "ge\u00b7trof\u00b7fen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "VVPP", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kukuk! Kukuk! Kukuk!", "tokens": ["Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Weil nun das Schie\u00dfen J\u00e4gers Brauch,", "tokens": ["Weil", "nun", "das", "Schie\u00b7\u00dfen", "J\u00e4\u00b7gers", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So will ich endlich schie\u00dfen auch,", "tokens": ["So", "will", "ich", "end\u00b7lich", "schie\u00b7\u00dfen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Mein B\u00fcchsen die sind schon geladen,", "tokens": ["Mein", "B\u00fcch\u00b7sen", "die", "sind", "schon", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df dirs nicht m\u00f6g am Leben schaden,", "tokens": ["Da\u00df", "dirs", "nicht", "m\u00f6g", "am", "Le\u00b7ben", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kukuk! Kukuk! Kukuk!", "tokens": ["Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Nun liegst du Vogel getroffen hier,", "tokens": ["Nun", "liegst", "du", "Vo\u00b7gel", "ge\u00b7trof\u00b7fen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "VVPP", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Komm immerfort in mein Revier,", "tokens": ["Komm", "im\u00b7mer\u00b7fort", "in", "mein", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So oft ich dich im Wald erblick,", "tokens": ["So", "oft", "ich", "dich", "im", "Wald", "er\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So schie\u00df ich dich im Augenblick.", "tokens": ["So", "schie\u00df", "ich", "dich", "im", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kukuk! Kukuk! Kukuk!", "tokens": ["Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Der Vogel hat mich recht erfreut,", "tokens": ["Der", "Vo\u00b7gel", "hat", "mich", "recht", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ums Pulver ist mirs gar nicht leid,", "tokens": ["Ums", "Pul\u00b7ver", "ist", "mirs", "gar", "nicht", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "NE", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kukuk!", "tokens": ["Ku\u00b7kuk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wenn ich ihn nur vermerken thue,", "tokens": ["Wenn", "ich", "ihn", "nur", "ver\u00b7mer\u00b7ken", "thue", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So schrey ich'm den Namen zu:", "tokens": ["So", "schrey", "ich'm", "den", "Na\u00b7men", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Kukuk! Kukuk! Kukuk!", "tokens": ["Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!", "Ku\u00b7kuk", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}