{"textgrid.poem.43223": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du h\u00f6rtest nicht mein th\u00f6richtes Herz", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du h\u00f6rtest nicht mein th\u00f6richtes Herz", "tokens": ["Du", "h\u00f6r\u00b7test", "nicht", "mein", "th\u00f6\u00b7rich\u00b7tes", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aufst\u00f6hnen und trotzig jammern,", "tokens": ["Auf\u00b7st\u00f6h\u00b7nen", "und", "trot\u00b7zig", "jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du sahst mich nicht des Gitters Erz", "tokens": ["Du", "sahst", "mich", "nicht", "des", "Git\u00b7ters", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dumpfem Zorn umklammern.", "tokens": ["In", "dum\u00b7pfem", "Zorn", "um\u00b7klam\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sah Dich lachen, ich lauschte dort,", "tokens": ["Ich", "sah", "Dich", "la\u00b7chen", ",", "ich", "lauschte", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein Dieb vor Deiner Th\u00fcre,", "tokens": ["Wie", "ein", "Dieb", "vor", "Dei\u00b7ner", "Th\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich wollte nur ein wahres Wort", "tokens": ["Ich", "woll\u00b7te", "nur", "ein", "wah\u00b7res", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr all' Deine falschen Schw\u00fcre.", "tokens": ["F\u00fcr", "all'", "Dei\u00b7ne", "fal\u00b7schen", "Schw\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O sage mir, was Dich von mir trieb", "tokens": ["O", "sa\u00b7ge", "mir", ",", "was", "Dich", "von", "mir", "trieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "+----+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und mich so ruhlos machte,", "tokens": ["Und", "mich", "so", "ruh\u00b7los", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Damit ich nimmer Dich, mein ", "tokens": ["Da\u00b7mit", "ich", "nim\u00b7mer", "Dich", ",", "mein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "$,", "PPOSAT"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und nimmer mich selbst \u2013 ", "tokens": ["Und", "nim\u00b7mer", "mich", "selbst", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}