{"textgrid.poem.41978": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, wer l\u00f6set das R\u00e4thsel mir wohl der bangen Erwartung,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wer l\u00f6set das R\u00e4thsel mir wohl der bangen Erwartung,", "tokens": ["Ach", ",", "wer", "l\u00f6\u00b7set", "das", "R\u00e4th\u00b7sel", "mir", "wohl", "der", "ban\u00b7gen", "Er\u00b7war\u00b7tung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ART", "NN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Scheidet den ewigen Streit zwischen dem Dunkel und Licht?", "tokens": ["Schei\u00b7det", "den", "e\u00b7wi\u00b7gen", "Streit", "zwi\u00b7schen", "dem", "Dun\u00b7kel", "und", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Hurtiger schafft mir den Geist und die lauschenden Sinne die Hoffnung,", "tokens": ["Hur\u00b7ti\u00b7ger", "schafft", "mir", "den", "Geist", "und", "die", "lau\u00b7schen\u00b7den", "Sin\u00b7ne", "die", "Hoff\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und doch t\u00e4uscht sie den Geist, t\u00e4uschet die Sinne mir stets.", "tokens": ["Und", "doch", "t\u00e4uscht", "sie", "den", "Geist", ",", "t\u00e4u\u00b7schet", "die", "Sin\u00b7ne", "mir", "stets", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Seh' ich ein wei\u00dfes Gewand hinflattern, so ruft mir die Sehnsucht", "tokens": ["Seh'", "ich", "ein", "wei\u00b7\u00dfes", "Ge\u00b7wand", "hin\u00b7flat\u00b7tern", ",", "so", "ruft", "mir", "die", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-+--+-+++", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Leis' in das Ohr: Sieh' da, siehe, das Liebchen erscheint!", "tokens": ["Leis'", "in", "das", "Ohr", ":", "Sieh'", "da", ",", "sie\u00b7he", ",", "das", "Lieb\u00b7chen", "er\u00b7scheint", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "VVIMP", "ADV", "$,", "VVIMP", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+---+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Und doch gleichen die Grazien nur in der schlanken Gestalt dir,", "tokens": ["Und", "doch", "glei\u00b7chen", "die", "Gra\u00b7zi\u00b7en", "nur", "in", "der", "schlan\u00b7ken", "Ge\u00b7stalt", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und nur Idalia w\u00e4hnt s\u00fc\u00dfer zu l\u00e4cheln wie du.", "tokens": ["Und", "nur", "I\u00b7da\u00b7lia", "w\u00e4hnt", "s\u00fc\u00b7\u00dfer", "zu", "l\u00e4\u00b7cheln", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VVFIN", "ADJD", "PTKZU", "VVFIN", "KOKOM", "PPER", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Nahet ein Schritt zum Gemach sich heran, stets ist er mir dein Schritt,", "tokens": ["Na\u00b7het", "ein", "Schritt", "zum", "Ge\u00b7mach", "sich", "he\u00b7ran", ",", "stets", "ist", "er", "mir", "dein", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PRF", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.10": {"text": "Und doch schwebte der West nimmer so leise wie du.", "tokens": ["Und", "doch", "schweb\u00b7te", "der", "West", "nim\u00b7mer", "so", "lei\u00b7se", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Jeglicher Ton, der mein horchendes Ohr trifft, scheinet mir dein Ton,", "tokens": ["Jeg\u00b7li\u00b7cher", "Ton", ",", "der", "mein", "hor\u00b7chen\u00b7des", "Ohr", "trifft", ",", "schei\u00b7net", "mir", "dein", "Ton", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.12": {"text": "Und doch t\u00e4usch' ich mich nie, Musen, in eurem Gesang!", "tokens": ["Und", "doch", "t\u00e4usch'", "ich", "mich", "nie", ",", "Mu\u00b7sen", ",", "in", "eu\u00b7rem", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Wenn du mich lockst mit dem schmeichelnden Wort, wenn die reizende Wange", "tokens": ["Wenn", "du", "mich", "lockst", "mit", "dem", "schmei\u00b7cheln\u00b7den", "Wort", ",", "wenn", "die", "rei\u00b7zen\u00b7de", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Sch\u00fcchtern ins d\u00e4mmernde Roth k\u00fcnftiger K\u00fcsse sich taucht,", "tokens": ["Sch\u00fcch\u00b7tern", "ins", "d\u00e4m\u00b7mern\u00b7de", "Roth", "k\u00fcnf\u00b7ti\u00b7ger", "K\u00fcs\u00b7se", "sich", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Ach, dann bin ich besiegt! Treu w\u00e4hn' ich die Schw\u00fcre der Treue,", "tokens": ["Ach", ",", "dann", "bin", "ich", "be\u00b7siegt", "!", "Treu", "w\u00e4hn'", "ich", "die", "Schw\u00fc\u00b7re", "der", "Treu\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$.", "NN", "VVIMP", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-------+--+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Und doch wei\u00df ich zu gut, Schmeichlerin, da\u00df du betr\u00fcgst!", "tokens": ["Und", "doch", "wei\u00df", "ich", "zu", "gut", ",", "Schmeich\u00b7le\u00b7rin", ",", "da\u00df", "du", "be\u00b7tr\u00fcgst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKA", "ADJD", "$,", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}