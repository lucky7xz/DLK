{"textgrid.poem.38758": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Laue L\u00fcfte", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laue L\u00fcfte", "tokens": ["Lau\u00b7e", "L\u00fcf\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Spielen lind,", "tokens": ["Spie\u00b7len", "lind", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Blumend\u00fcfte", "tokens": ["Blu\u00b7men\u00b7d\u00fcf\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Tr\u00e4gt der Wind,", "tokens": ["Tr\u00e4gt", "der", "Wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "R\u00f6thlich sich die B\u00e4ume kr\u00e4useln,", "tokens": ["R\u00f6th\u00b7lich", "sich", "die", "B\u00e4u\u00b7me", "kr\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieblich W\u00e4hnen", "tokens": ["Lieb\u00b7lich", "W\u00e4h\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Z\u00e4rtlich Sehnen", "tokens": ["Z\u00e4rt\u00b7lich", "Seh\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "In den Wipfeln, abw\u00e4rts durch die Bl\u00e4tter s\u00e4useln.", "tokens": ["In", "den", "Wip\u00b7feln", ",", "ab\u00b7w\u00e4rts", "durch", "die", "Bl\u00e4t\u00b7ter", "s\u00e4u\u00b7seln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Rufst du mich,", "tokens": ["Rufst", "du", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "S\u00fc\u00dfes Klingen?", "tokens": ["S\u00fc\u00b7\u00dfes", "Klin\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ach! geheimni\u00dfvolles Singen,", "tokens": ["Ach", "!", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7les", "Sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist nicht fremd, ich kenne dich!", "tokens": ["Bist", "nicht", "fremd", ",", "ich", "ken\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Tauben", "tokens": ["Wie", "die", "Tau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Z\u00e4rtlich lachen, girren, kosen,", "tokens": ["Z\u00e4rt\u00b7lich", "la\u00b7chen", ",", "gir\u00b7ren", ",", "ko\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Also mir im bangen Herzen", "tokens": ["Al\u00b7so", "mir", "im", "ban\u00b7gen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schlagen Fitt'ge Lust und Schmerzen;", "tokens": ["Schla\u00b7gen", "Fitt'\u00b7ge", "Lust", "und", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zu den dunkeln D\u00e4mmerlauben,", "tokens": ["Zu", "den", "dun\u00b7keln", "D\u00e4m\u00b7mer\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Zu den Blumenbeeten, Rosen", "tokens": ["Zu", "den", "Blu\u00b7men\u00b7bee\u00b7ten", ",", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wandl' ich, ruf' ich, schau' umher \u2013", "tokens": ["Wandl'", "ich", ",", "ruf'", "ich", ",", "schau'", "um\u00b7her", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und die ganze Welt ist leer.", "tokens": ["Und", "die", "gan\u00b7ze", "Welt", "ist", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In die dichte Einsamkeit", "tokens": ["In", "die", "dich\u00b7te", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trag' ich meiner Thr\u00e4nen Brand;", "tokens": ["Trag'", "ich", "mei\u00b7ner", "Thr\u00e4\u00b7nen", "Brand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! kein Baum thut mir bekannt,", "tokens": ["Ach", "!", "kein", "Baum", "thut", "mir", "be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Setz' mich an des Bronnens Rand:", "tokens": ["Setz'", "mich", "an", "des", "Bron\u00b7nens", "Rand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vogel wild die T\u00f6ne schreit,", "tokens": ["Vo\u00b7gel", "wild", "die", "T\u00f6\u00b7ne", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Echo hallt,", "tokens": ["E\u00b7cho", "hallt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Hirschlein springt im dunkeln Wald.", "tokens": ["Hirsc\u00b7hlein", "springt", "im", "dun\u00b7keln", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und es brau\u00dft herauf, herunter,", "tokens": ["Und", "es", "brau\u00dft", "her\u00b7auf", ",", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Waldstrom klingt durch seine Kl\u00fcfte,", "tokens": ["Wald\u00b7strom", "klingt", "durch", "sei\u00b7ne", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine jungen Wellen springen", "tokens": ["Sei\u00b7ne", "jun\u00b7gen", "Wel\u00b7len", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Felsenstufen munter,", "tokens": ["Auf", "den", "Fel\u00b7sen\u00b7stu\u00b7fen", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Adler schwingt sich durch die L\u00fcfte: \u2013", "tokens": ["Ad\u00b7ler", "schwingt", "sich", "durch", "die", "L\u00fcf\u00b7te", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Thr\u00e4nen, Rufen, Klagen, Singen,", "tokens": ["Thr\u00e4\u00b7nen", ",", "Ru\u00b7fen", ",", "Kla\u00b7gen", ",", "Sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00f6nnt ihn nicht zur\u00fcck mir zwingen?", "tokens": ["K\u00f6nnt", "ihn", "nicht", "zu\u00b7r\u00fcck", "mir", "zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PTKVZ", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Garten, Berge, W\u00e4lder weit", "tokens": ["Gar\u00b7ten", ",", "Ber\u00b7ge", ",", "W\u00e4l\u00b7der", "weit"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sind mir Grab und Einsamkeit.", "tokens": ["Sind", "mir", "Grab", "und", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}