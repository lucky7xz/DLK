{"textgrid.poem.52726": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Gelehrte", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tausend Schmerzen in junger Brust", "tokens": ["Tau\u00b7send", "Schmer\u00b7zen", "in", "jun\u00b7ger", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Schon so tief so fest gegraben!", "tokens": ["Schon", "so", "tief", "so", "fest", "ge\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War das ein Leben voll Lebenslust,", "tokens": ["War", "das", "ein", "Le\u00b7ben", "voll", "Le\u00b7bens\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wies gl\u00fcckliche Menschen haben?", "tokens": ["Wies", "gl\u00fcck\u00b7li\u00b7che", "Men\u00b7schen", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Sorgen um Zukunft, klein und gemein,", "tokens": ["Die", "Sor\u00b7gen", "um", "Zu\u00b7kunft", ",", "klein", "und", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ewiges Sehen auf mein und dein \u2013", "tokens": ["E\u00b7wi\u00b7ges", "Se\u00b7hen", "auf", "mein", "und", "dein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "KON", "PPOSAT", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Das sollt mein Fr\u00fchlingsleben sein?", "tokens": ["Das", "sollt", "mein", "Fr\u00fch\u00b7lings\u00b7le\u00b7ben", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Fluch dir, h\u00f6hnische Wissenschaft,", "tokens": ["O", "Fluch", "dir", ",", "h\u00f6h\u00b7ni\u00b7sche", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was hab ich mich dir ergeben!", "tokens": ["Was", "hab", "ich", "mich", "dir", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "PPER", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du Gespenst ohne Bl\u00fcth, ohne Saft und Kraft,", "tokens": ["Du", "Ge\u00b7spenst", "oh\u00b7ne", "Bl\u00fcth", ",", "oh\u00b7ne", "Saft", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Elendes Gelehrtenleben!", "tokens": ["E\u00b7len\u00b7des", "Ge\u00b7lehr\u00b7ten\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir kommt entgegen kein Menschenkind,", "tokens": ["Mir", "kommt", "ent\u00b7ge\u00b7gen", "kein", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn ich nicht such und schau mich blind,", "tokens": ["Wenn", "ich", "nicht", "such", "und", "schau", "mich", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ob ich lebendige Wesen find.", "tokens": ["Ob", "ich", "le\u00b7ben\u00b7di\u00b7ge", "We\u00b7sen", "find", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Was Alles ich hatte Freud und Gl\u00fcck,", "tokens": ["Was", "Al\u00b7les", "ich", "hat\u00b7te", "Freud", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mu\u00dft ich mir selber erringen,", "tokens": ["Mu\u00dft", "ich", "mir", "sel\u00b7ber", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Erjagt ich den frohen Augenblick,", "tokens": ["Er\u00b7jagt", "ich", "den", "fro\u00b7hen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mu\u00dft ich ihn bannen und zwingen.", "tokens": ["Mu\u00dft", "ich", "ihn", "ban\u00b7nen", "und", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Nie harmlos gab ich mich hin der Lust,", "tokens": ["Nie", "harm\u00b7los", "gab", "ich", "mich", "hin", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "$,"], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Wann kehrte mir Heiterkeit ein zur Brust,", "tokens": ["Wann", "kehr\u00b7te", "mir", "Hei\u00b7ter\u00b7keit", "ein", "zur", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "ART", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Da ichs nicht wollt und nicht gewu\u00dft?", "tokens": ["Da", "ichs", "nicht", "wollt", "und", "nicht", "ge\u00b7wu\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VMFIN", "KON", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchl es erst, o ewig zu sp\u00e4t,", "tokens": ["Ich", "f\u00fchl", "es", "erst", ",", "o", "e\u00b7wig", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "FM", "ADJD", "PTKA", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie bitter der Kelch der Reue,", "tokens": ["Wie", "bit\u00b7ter", "der", "Kelch", "der", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und h\u00e4tte ich noch ein einzig Gebet,", "tokens": ["Und", "h\u00e4t\u00b7te", "ich", "noch", "ein", "ein\u00b7zig", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es w\u00e4re um Liebe und Treue!", "tokens": ["Es", "w\u00e4\u00b7re", "um", "Lie\u00b7be", "und", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wohl kannt ich einmal eine sch\u00f6ne Frau,", "tokens": ["Wohl", "kannt", "ich", "ein\u00b7mal", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In ihren Augen stund oft der Thau;", "tokens": ["In", "ih\u00b7ren", "Au\u00b7gen", "stund", "oft", "der", "Thau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Ich \u2013 trug meinen Wissenseifer zur Schau.", "tokens": ["Ich", "\u2013", "trug", "mei\u00b7nen", "Wis\u00b7sen\u00b7sei\u00b7fer", "zur", "Schau", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Jetzt grau vor Weisheit und Verdru\u00df", "tokens": ["Jetzt", "grau", "vor", "Weis\u00b7heit", "und", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinschwank ich einsam zur Grube,", "tokens": ["Hin\u00b7schwank", "ich", "ein\u00b7sam", "zur", "Gru\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Wehmuth ist mein Lebensgenu\u00df,", "tokens": ["Die", "Weh\u00b7muth", "ist", "mein", "Le\u00b7bens\u00b7ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "\u00bbbedauern\u00ab darf mich ein Bube.", "tokens": ["\u00bb", "be\u00b7dau\u00b7ern", "\u00ab", "darf", "mich", "ein", "Bu\u00b7be", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$(", "VMFIN", "PRF", "ART", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Statt bl\u00fchender Kinder liebende Schaar", "tokens": ["Statt", "bl\u00fc\u00b7hen\u00b7der", "Kin\u00b7der", "lie\u00b7ben\u00b7de", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Z\u00e4hl ich meine Werke Jahr f\u00fcr Jahr,", "tokens": ["Z\u00e4hl", "ich", "mei\u00b7ne", "Wer\u00b7ke", "Jahr", "f\u00fcr", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und seh, da\u00df ich Andern n\u00fctzlich war.", "tokens": ["Und", "seh", ",", "da\u00df", "ich", "An\u00b7dern", "n\u00fctz\u00b7lich", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADJA", "ADJD", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "O schafft eine sch\u00f6ne, menschliche Zeit,", "tokens": ["O", "schafft", "ei\u00b7ne", "sch\u00f6\u00b7ne", ",", "menschli\u00b7che", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ohne Heuchelei der Tugend,", "tokens": ["Oh\u00b7ne", "Heu\u00b7che\u00b7lei", "der", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Gegenwart der Gerechtigkeit,", "tokens": ["Ei\u00b7ne", "Ge\u00b7gen\u00b7wart", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Einen Fr\u00fchling unserer Jugend!", "tokens": ["Ei\u00b7nen", "Fr\u00fch\u00b7ling", "un\u00b7se\u00b7rer", "Ju\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wo die Zeit, darin sie knospen mag?", "tokens": ["Wo", "die", "Zeit", ",", "da\u00b7rin", "sie", "knos\u00b7pen", "mag", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Ihr werft das Kind mit Einem Schlag", "tokens": ["Ihr", "werft", "das", "Kind", "mit", "Ei\u00b7nem", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In den arbeitsschw\u00fclen Sommertag.", "tokens": ["In", "den", "ar\u00b7beits\u00b7schw\u00fc\u00b7len", "Som\u00b7mer\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}