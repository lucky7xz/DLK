{"textgrid.poem.43146": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Mein Lied", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einschneidend ist mein Lied und peinlich,", "tokens": ["Ein\u00b7schnei\u00b7dend", "ist", "mein", "Lied", "und", "pein\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So frostig wie die Winternacht,", "tokens": ["So", "fros\u00b7tig", "wie", "die", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es h\u00e4tte sonst nach mir wahrscheinlich", "tokens": ["Es", "h\u00e4t\u00b7te", "sonst", "nach", "mir", "wahr\u00b7schein\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Manch' Th\u00f6rin Aehnliches gebracht;", "tokens": ["Man\u00b7ch'", "Th\u00f6\u00b7rin", "A\u00b7ehn\u00b7li\u00b7ches", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In Versen rauh und lebensfeindlich,", "tokens": ["In", "Ver\u00b7sen", "rauh", "und", "le\u00b7bens\u00b7feind\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ich geweint, geflucht, gelacht,", "tokens": ["Wie", "ich", "ge\u00b7weint", ",", "ge\u00b7flucht", ",", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So derb-unk\u00fcnstlich, geistig-kleinlich,", "tokens": ["So", "der\u00b7bun\u00b7k\u00fcnst\u00b7lich", ",", "geis\u00b7tig\u00b7klein\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So tief gef\u00fchlt und \u2013 seicht gemacht.", "tokens": ["So", "tief", "ge\u00b7f\u00fchlt", "und", "\u2013", "seicht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}