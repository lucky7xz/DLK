{"dta.poem.11375": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria von der hoffnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hoffnung macht doch nicht zu schanden,", "tokens": ["Hoff\u00b7nung", "macht", "doch", "nicht", "zu", "schan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob sie gleich geduld begehrt:", "tokens": ["Ob", "sie", "gleich", "ge\u00b7duld", "be\u00b7gehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn das b\u00f6se \u00fcberstanden,", "tokens": ["Wenn", "das", "b\u00f6\u00b7se", "\u00fc\u00b7bers\u00b7tan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird man auch des guten werth.", "tokens": ["Wird", "man", "auch", "des", "gu\u00b7ten", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ART", "ADJA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Strecken sich die langen eichen", "tokens": ["Stre\u00b7cken", "sich", "die", "lan\u00b7gen", "ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach behauen mehr empor:", "tokens": ["Nach", "be\u00b7hau\u00b7en", "mehr", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf den zarten rosen-str\u00e4uchen", "tokens": ["Auf", "den", "zar\u00b7ten", "ro\u00b7sen\u00b7str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00e4chst der dorn den rosen vor:", "tokens": ["W\u00e4chst", "der", "dorn", "den", "ro\u00b7sen", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und wer Canaan will sehen,", "tokens": ["Und", "wer", "Ca\u00b7naan", "will", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mu\u00df erst durch die w\u00fcsten gehen.", "tokens": ["Mu\u00df", "erst", "durch", "die", "w\u00fcs\u00b7ten", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heists gleich offt: Ach wir ertrincken!", "tokens": ["Heists", "gleich", "offt", ":", "Ach", "wir", "er\u00b7trin\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$.", "ITJ", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun geht alle hoffnung drauf!", "tokens": ["Nun", "geht", "al\u00b7le", "hoff\u00b7nung", "drauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn das schiff will untersincken,", "tokens": ["Wenn", "das", "schiff", "will", "un\u00b7ter\u00b7sin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wachet erst der helffer auf.", "tokens": ["Wa\u00b7chet", "erst", "der", "helf\u00b7fer", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rettung kommt doch stets zu rechte,", "tokens": ["Ret\u00b7tung", "kommt", "doch", "stets", "zu", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn es uns am schlimmsten geht:", "tokens": ["Wenn", "es", "uns", "am", "schlimms\u00b7ten", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gleich als wenn die l\u00e4ngsten n\u00e4chte,", "tokens": ["Gleich", "als", "wenn", "die", "l\u00e4ngs\u00b7ten", "n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "KOUS", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die sonn am h\u00f6chsten steht:", "tokens": ["Und", "die", "sonn", "am", "h\u00f6chs\u00b7ten", "steht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Denn ein starcker wetter-regen", "tokens": ["Denn", "ein", "star\u00b7cker", "wet\u00b7ter\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Pflegt am ersten sich zu legen.", "tokens": ["Pflegt", "am", "ers\u00b7ten", "sich", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fr\u00fchlings-fieber \u00fcberwunden,", "tokens": ["Fr\u00fch\u00b7lings\u00b7fie\u00b7ber", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seynd den leibern gar gesund.", "tokens": ["Seynd", "den", "lei\u00b7bern", "gar", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die wunden einst verbunden,", "tokens": ["Wenn", "die", "wun\u00b7den", "einst", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lacht der vorbethr\u00e4nte mund.", "tokens": ["Lacht", "der", "vor\u00b7be\u00b7thr\u00e4n\u00b7te", "mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wills das gl\u00fcck gleich lange machen,", "tokens": ["Wills", "das", "gl\u00fcck", "gleich", "lan\u00b7ge", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh\u2019 es uns zu h\u00fclfe zieht;", "tokens": ["Eh'", "es", "uns", "zu", "h\u00fcl\u00b7fe", "zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hoffnung ist ein traum im wachen,", "tokens": ["Hoff\u00b7nung", "ist", "ein", "traum", "im", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPRART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo man lauter himmel sieht.", "tokens": ["Wo", "man", "lau\u00b7ter", "him\u00b7mel", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Hoffnung, gl\u00fccklicher zu werden,", "tokens": ["Hoff\u00b7nung", ",", "gl\u00fcck\u00b7li\u00b7cher", "zu", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Tilgt die gr\u00f6sse der beschwerden.", "tokens": ["Tilgt", "die", "gr\u00f6s\u00b7se", "der", "be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum, o seele! la\u00df das klagen", "tokens": ["Drum", ",", "o", "see\u00b7le", "!", "la\u00df", "das", "kla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "FM", "FM", "$.", "VVIMP", "PDS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey der widerw\u00e4rtigkeit!", "tokens": ["Bey", "der", "wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der wird heilen, der geschlagen,", "tokens": ["Der", "wird", "hei\u00b7len", ",", "der", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVINF", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Harrc nur noch kurtze zeit!", "tokens": ["Harrc", "nur", "noch", "kurt\u00b7ze", "zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn das gl\u00fcck einst ausgeschlafen,", "tokens": ["Wenn", "das", "gl\u00fcck", "einst", "aus\u00b7ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alsdenn wird es munter seyn.", "tokens": ["Als\u00b7denn", "wird", "es", "mun\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "David bleibt nicht bey den schafen:", "tokens": ["Da\u00b7vid", "bleibt", "nicht", "bey", "den", "scha\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "ART", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf den sturm folgt sonnen-schein.", "tokens": ["Auf", "den", "sturm", "folgt", "son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und wer warten kan und hoffen,", "tokens": ["Und", "wer", "war\u00b7ten", "kan", "und", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VMFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dem steht gl\u00fcck und himmel offen.", "tokens": ["Dem", "steht", "gl\u00fcck", "und", "him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}