{"textgrid.poem.65747": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "21", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht jene Muse hab' ich mir erw\u00e4hlt,", "tokens": ["Nicht", "je\u00b7ne", "Mu\u00b7se", "hab'", "ich", "mir", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die aus gemalten Reizen Lieder saugt,", "tokens": ["Die", "aus", "ge\u00b7mal\u00b7ten", "Rei\u00b7zen", "Lie\u00b7der", "saugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Selbst nur zur F\u00e4rbung braucht des Himmels Zelt,", "tokens": ["Selbst", "nur", "zur", "F\u00e4r\u00b7bung", "braucht", "des", "Him\u00b7mels", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In seine Sch\u00f6nheit alles Sch\u00f6ne taucht;", "tokens": ["In", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "al\u00b7les", "Sch\u00f6\u00b7ne", "taucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die nur zusammenh\u00e4uft hochtrabende Vergleiche", "tokens": ["Die", "nur", "zu\u00b7sam\u00b7men\u00b7h\u00e4uft", "hoch\u00b7tra\u00b7ben\u00b7de", "Ver\u00b7glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Mit Sonn' und Mond, dem k\u00f6stlichsten Gestein", "tokens": ["Mit", "Sonn'", "und", "Mond", ",", "dem", "k\u00f6st\u00b7lichs\u00b7ten", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.7": {"text": "In Erd' und See, mit Florens j\u00fcngstem Zweige,", "tokens": ["In", "Erd'", "und", "See", ",", "mit", "Flo\u00b7rens", "j\u00fcngs\u00b7tem", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und was nur Seltnes hegt des Himmels W\u00f6lbung ein.", "tokens": ["Und", "was", "nur", "Selt\u00b7nes", "hegt", "des", "Him\u00b7mels", "W\u00f6l\u00b7bung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NE", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O la\u00df mich, treu in Lieb', auch treu nur schreiben!", "tokens": ["O", "la\u00df", "mich", ",", "treu", "in", "Lieb'", ",", "auch", "treu", "nur", "schrei\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJD", "APPR", "NN", "$,", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dann glaube mir: so reizend ist mein Freund", "tokens": ["Dann", "glau\u00b7be", "mir", ":", "so", "rei\u00b7zend", "ist", "mein", "Freund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wie je ein Mutterkind, wenn er die goldnen Scheiben", "tokens": ["Wie", "je", "ein", "Mut\u00b7ter\u00b7kind", ",", "wenn", "er", "die", "gold\u00b7nen", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Sternenplanes gleich nicht \u00fcberscheint.", "tokens": ["Des", "Ster\u00b7nen\u00b7pla\u00b7nes", "gleich", "nicht", "\u00fc\u00b7bersc\u00b7heint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mehr sage, wer mit Worten abzuspeisen:", "tokens": ["Mehr", "sa\u00b7ge", ",", "wer", "mit", "Wor\u00b7ten", "ab\u00b7zu\u00b7spei\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ich, der ich nichts verkaufe, mag nichts preisen.", "tokens": ["Ich", ",", "der", "ich", "nichts", "ver\u00b7kau\u00b7fe", ",", "mag", "nichts", "prei\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,", "VMFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}