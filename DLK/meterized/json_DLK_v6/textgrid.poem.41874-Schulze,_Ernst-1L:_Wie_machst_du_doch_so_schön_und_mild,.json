{"textgrid.poem.41874": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie machst du doch so sch\u00f6n und mild,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie machst du doch so sch\u00f6n und mild,", "tokens": ["Wie", "machst", "du", "doch", "so", "sch\u00f6n", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Liebe, die dein Arm umfangen!", "tokens": ["O", "Lie\u00b7be", ",", "die", "dein", "Arm", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie freundlich lacht dein holdes Bild", "tokens": ["Wie", "freund\u00b7lich", "lacht", "dein", "hol\u00b7des", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Blick und Mund, auf Stirn und Wangen!", "tokens": ["In", "Blick", "und", "Mund", ",", "auf", "Stirn", "und", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ist ihr Herz so weich und weit,", "tokens": ["Wie", "ist", "ihr", "Herz", "so", "weich", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie selten ist ihr Auge tr\u00fcbe!", "tokens": ["Wie", "sel\u00b7ten", "ist", "ihr", "Au\u00b7ge", "tr\u00fc\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie schm\u00fcckst du Thr\u00e4nen selbst und Leid!", "tokens": ["Wie", "schm\u00fcckst", "du", "Thr\u00e4\u00b7nen", "selbst", "und", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie machst du doch so sch\u00f6n, o Liebe!", "tokens": ["Wie", "machst", "du", "doch", "so", "sch\u00f6n", ",", "o", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur Eine flieht und meidet dich", "tokens": ["Nur", "Ei\u00b7ne", "flieht", "und", "mei\u00b7det", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bebt vor deinen s\u00fc\u00dfen Sorgen.", "tokens": ["Und", "bebt", "vor", "dei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl ist sie sch\u00f6n genug f\u00fcr sich", "tokens": ["Wohl", "ist", "sie", "sch\u00f6n", "ge\u00b7nug", "f\u00fcr", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und braucht nicht fremden Schmuck zu borgen;", "tokens": ["Und", "braucht", "nicht", "frem\u00b7den", "Schmuck", "zu", "bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch mich ergreift ein tiefer Schmerz,", "tokens": ["Doch", "mich", "er\u00b7greift", "ein", "tie\u00b7fer", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Seh' ich so hold dich gl\u00fchn und bl\u00fchen,", "tokens": ["Seh'", "ich", "so", "hold", "dich", "gl\u00fchn", "und", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und leise seufzt mein traurend Herz:", "tokens": ["Und", "lei\u00b7se", "seufzt", "mein", "trau\u00b7rend", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie darf die Eine doch dich fliehen?", "tokens": ["Wie", "darf", "die", "Ei\u00b7ne", "doch", "dich", "flie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}