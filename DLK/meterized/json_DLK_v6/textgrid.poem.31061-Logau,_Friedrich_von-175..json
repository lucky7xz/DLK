{"textgrid.poem.31061": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "175.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man meint, die Welt sey gar zu neu; sie habe nichts vom alten.", "tokens": ["Man", "meint", ",", "die", "Welt", "sey", "gar", "zu", "neu", ";", "sie", "ha\u00b7be", "nichts", "vom", "al\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$.", "PPER", "VAFIN", "PIS", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ich sage nein; man mu\u00df die Zeit nur recht zusammen halten.", "tokens": ["Ich", "sa\u00b7ge", "nein", ";", "man", "mu\u00df", "die", "Zeit", "nur", "recht", "zu\u00b7sam\u00b7men", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKANT", "$.", "PIS", "VMFIN", "ART", "NN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die alte Welt hat ihre Witz in Fabeln eingerichtet;", "tokens": ["Die", "al\u00b7te", "Welt", "hat", "ih\u00b7re", "Witz", "in", "Fa\u00b7beln", "ein\u00b7ge\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "O, was die neue Welt uns sagt, ist mehrentheils ertichtet.", "tokens": ["O", ",", "was", "die", "neu\u00b7e", "Welt", "uns", "sagt", ",", "ist", "meh\u00b7ren\u00b7theils", "er\u00b7tich\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}