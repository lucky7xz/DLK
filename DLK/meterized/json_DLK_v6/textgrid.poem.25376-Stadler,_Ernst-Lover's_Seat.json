{"textgrid.poem.25376": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Lover's Seat", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Abend sind wir steile", "tokens": ["Im", "A\u00b7bend", "sind", "wir", "stei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "gr\u00fcnbebuschte D\u00fcnenwege hingeschritten.", "tokens": ["gr\u00fcn\u00b7be\u00b7buschte", "D\u00fc\u00b7nen\u00b7we\u00b7ge", "hin\u00b7ge\u00b7schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Du ruhst an mich gedr\u00e4ngt.", "tokens": ["Du", "ruhst", "an", "mich", "ge\u00b7dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Kreideklippe schwingt ihr schimmerndes Gefieder", "tokens": ["Die", "Krei\u00b7de\u00b7klip\u00b7pe", "schwingt", "ihr", "schim\u00b7mern\u00b7des", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u00fcber tiefem Meere.", "tokens": ["\u00fc\u00b7ber", "tie\u00b7fem", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hier, wo der Fels", "tokens": ["Hier", ",", "wo", "der", "Fels"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "in j\u00e4her Todesgier ins Leere", "tokens": ["in", "j\u00e4\u00b7her", "To\u00b7des\u00b7gier", "ins", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hin\u00fcberlehnt, sind einst zwei Liebende", "tokens": ["Hin\u00b7\u00fc\u00b7ber\u00b7lehnt", ",", "sind", "einst", "zwei", "Lie\u00b7ben\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VAFIN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "ins weiche blaue Bett geglitten.", "tokens": ["ins", "wei\u00b7che", "blau\u00b7e", "Bett", "ge\u00b7glit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fern t\u00f6nt die Brandung.", "tokens": ["Fern", "t\u00f6nt", "die", "Bran\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zwischen K\u00fcssen lausch ich der Legende,", "tokens": ["Zwi\u00b7schen", "K\u00fcs\u00b7sen", "lausch", "ich", "der", "Le\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die lachend mir dein Mund", "tokens": ["Die", "la\u00b7chend", "mir", "dein", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "in den ergl\u00fchten Sommerabend spricht.", "tokens": ["in", "den", "er\u00b7gl\u00fch\u00b7ten", "Som\u00b7mer\u00b7a\u00b7bend", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch tief mich beugend", "tokens": ["Doch", "tief", "mich", "beu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "seh' ich wie im Gl\u00fcck erstarren dein Gesicht", "tokens": ["seh'", "ich", "wie", "im", "Gl\u00fcck", "er\u00b7star\u00b7ren", "dein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "APPRART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Und dumpfe Schwermut", "tokens": ["Und", "dump\u00b7fe", "Schwer\u00b7mut"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "hinter deinen Wimpern warten und das nahe Ende.", "tokens": ["hin\u00b7ter", "dei\u00b7nen", "Wim\u00b7pern", "war\u00b7ten", "und", "das", "na\u00b7he", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}