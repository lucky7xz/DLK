{"textgrid.poem.66545": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leeres Reden, Kommen, Gehen,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leeres Reden, Kommen, Gehen,", "tokens": ["Lee\u00b7res", "Re\u00b7den", ",", "Kom\u00b7men", ",", "Ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaales L\u00e4cheln, Lachen auch,", "tokens": ["Schaa\u00b7les", "L\u00e4\u00b7cheln", ",", "La\u00b7chen", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles mu\u00dftest du verstehen,", "tokens": ["Al\u00b7les", "mu\u00df\u00b7test", "du", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heuchelnd nach des Tages Brauch!", "tokens": ["Heu\u00b7chelnd", "nach", "des", "Ta\u00b7ges", "Brauch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unergr\u00fcndet mu\u00df es bleiben,", "tokens": ["Un\u00b7er\u00b7gr\u00fcn\u00b7det", "mu\u00df", "es", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glatt und tr\u00fcgrisch wie die Welt,", "tokens": ["Glatt", "und", "tr\u00fcg\u00b7risch", "wie", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn dein Wesen ihrem Treiben", "tokens": ["Wenn", "dein", "We\u00b7sen", "ih\u00b7rem", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Widerwillig ward gesellt.", "tokens": ["Wi\u00b7der\u00b7wil\u00b7lig", "ward", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein erst, wenn der Tag zerstoben,", "tokens": ["Dein", "erst", ",", "wenn", "der", "Tag", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist, was dir die Seel' umfa\u00dft,", "tokens": ["Ist", ",", "was", "dir", "die", "Seel'", "um\u00b7fa\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein des Gl\u00fccks, der Schmerzen Toben,", "tokens": ["Dein", "des", "Gl\u00fccks", ",", "der", "Schmer\u00b7zen", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein geliebter Sorgen Last.", "tokens": ["Dein", "ge\u00b7lieb\u00b7ter", "Sor\u00b7gen", "Last", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}