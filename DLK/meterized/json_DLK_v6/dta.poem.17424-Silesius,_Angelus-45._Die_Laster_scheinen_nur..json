{"dta.poem.17424": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "45.  Die Laster scheinen nur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Laster gehn bekleidt/ die Tugend stehet Blo\u00df/", "tokens": ["Die", "Las\u00b7ter", "gehn", "be\u00b7kleidt", "/", "die", "Tu\u00b7gend", "ste\u00b7het", "Blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$(", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ist warhafftiglich/ ien\u2019 aber scheinen gro\u00df.", "tokens": ["Die", "ist", "war\u00b7haff\u00b7tig\u00b7lich", "/", "ien'", "a\u00b7ber", "schei\u00b7nen", "gro\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$(", "ADV", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}