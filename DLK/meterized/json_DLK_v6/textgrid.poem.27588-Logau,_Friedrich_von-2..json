{"textgrid.poem.27588": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lebt nun, liebes Paar, lebt zwischen Krieg und morden", "tokens": ["So", "lebt", "nun", ",", "lie\u00b7bes", "Paar", ",", "lebt", "zwi\u00b7schen", "Krieg", "und", "mor\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dennoch s\u00fcsser Ruh und in dem sch\u00f6nen Orden", "tokens": ["In", "den\u00b7noch", "s\u00fcs\u00b7ser", "Ruh", "und", "in", "dem", "sch\u00f6\u00b7nen", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der lieben Einigkeit; lebt, da\u00df de\u00df Gl\u00fcckes neiden", "tokens": ["Der", "lie\u00b7ben", "Ei\u00b7nig\u00b7keit", ";", "lebt", ",", "da\u00df", "de\u00df", "Gl\u00fc\u00b7ckes", "nei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "$,", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df Euch und euer Thun stets fliehen und vermeiden!", "tokens": ["Mu\u00df", "Euch", "und", "eu\u00b7er", "Thun", "stets", "flie\u00b7hen", "und", "ver\u00b7mei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KON", "PPOSAT", "NN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So w\u00fcntschen etwas gilt, so woll auch di\u00df Gott geben,", "tokens": ["So", "w\u00fcnt\u00b7schen", "et\u00b7was", "gilt", ",", "so", "woll", "auch", "di\u00df", "Gott", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "$,", "ADV", "ADV", "ADV", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ihr, wenn ihr seid tod, noch lange m\u00f6get leben,", "tokens": ["Da\u00df", "ihr", ",", "wenn", "ihr", "seid", "tod", ",", "noch", "lan\u00b7ge", "m\u00f6\u00b7get", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "VAFIN", "NN", "$,", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Viel S\u00f6hne, da\u00df man denn nach euch, dem Vater, nennet,", "tokens": ["Viel", "S\u00f6h\u00b7ne", ",", "da\u00df", "man", "denn", "nach", "euch", ",", "dem", "Va\u00b7ter", ",", "nen\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "PIS", "ADV", "APPR", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So viel der T\u00f6chter auch nach euch, der Mutter, kennet.", "tokens": ["So", "viel", "der", "T\u00f6ch\u00b7ter", "auch", "nach", "euch", ",", "der", "Mut\u00b7ter", ",", "ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "APPR", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}