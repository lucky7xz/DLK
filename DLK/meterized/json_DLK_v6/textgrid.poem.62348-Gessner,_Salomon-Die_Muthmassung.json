{"textgrid.poem.62348": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "Die Muthmassung", "genre": "verse", "period": "N.A.", "pub_year": 1750, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bin ich? wie! wo schlief ich dann,", "tokens": ["Wo", "bin", "ich", "?", "wie", "!", "wo", "schlief", "ich", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "PWAV", "$.", "PWAV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die dunkle Nacht hindurch?", "tokens": ["Die", "dunk\u00b7le", "Nacht", "hin\u00b7durch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier lieg ich unter diesem Baum,", "tokens": ["Hier", "lieg", "ich", "un\u00b7ter", "die\u00b7sem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier ich, mein Becher \u2013 \u2013 dort im Gras,", "tokens": ["Hier", "ich", ",", "mein", "Be\u00b7cher", "\u2013", "\u2013", "dort", "im", "Gras", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NN", "$(", "$(", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort liegt mein Kranz, die Fl\u00f6te da,", "tokens": ["Dort", "liegt", "mein", "Kranz", ",", "die", "Fl\u00f6\u00b7te", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald glaub ich, da\u00df ich gestern hier,", "tokens": ["Bald", "glaub", "ich", ",", "da\u00df", "ich", "ge\u00b7stern", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Im starken Rausche sank.", "tokens": ["Im", "star\u00b7ken", "Rau\u00b7sche", "sank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}