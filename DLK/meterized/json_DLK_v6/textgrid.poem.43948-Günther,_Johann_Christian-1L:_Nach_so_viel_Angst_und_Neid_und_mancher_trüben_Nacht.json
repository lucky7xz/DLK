{"textgrid.poem.43948": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach so viel Angst und Neid und mancher tr\u00fcben Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach so viel Angst und Neid und mancher tr\u00fcben Nacht", "tokens": ["Nach", "so", "viel", "Angst", "und", "Neid", "und", "man\u00b7cher", "tr\u00fc\u00b7ben", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "KON", "NN", "KON", "PIAT", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ersah ich wiederum des Gl\u00fcckes Morgenr\u00f6the.", "tokens": ["Er\u00b7sah", "ich", "wie\u00b7de\u00b7rum", "des", "Gl\u00fc\u00b7ckes", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf, Musen, auf und sucht die lang entri\u00dfne Fl\u00f6the,", "tokens": ["Auf", ",", "Mu\u00b7sen", ",", "auf", "und", "sucht", "die", "lang", "ent\u00b7ri\u00df\u00b7ne", "Fl\u00f6\u00b7the", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "APPR", "KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die uns in Schweidniz einst den Abend kurz gemacht!", "tokens": ["Die", "uns", "in", "Schweid\u00b7niz", "einst", "den", "A\u00b7bend", "kurz", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr habt mit mir geweint, ihr sollt auch mit mir singen", "tokens": ["Ihr", "habt", "mit", "mir", "ge\u00b7weint", ",", "ihr", "sollt", "auch", "mit", "mir", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,", "PPER", "VMFIN", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Lenchens Gegenwart mit Treu und Lust umringen.", "tokens": ["Und", "Len\u00b7chens", "Ge\u00b7gen\u00b7wart", "mit", "Treu", "und", "Lust", "um\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach Kind, ach liebstes Kind, ach, k\u00f6nt es m\u00f6glich seyn,", "tokens": ["Ach", "Kind", ",", "ach", "liebs\u00b7tes", "Kind", ",", "ach", ",", "k\u00f6nt", "es", "m\u00f6g\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "XY", "ADJA", "NN", "$,", "ITJ", "$,", "VVFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies mein getreues Herz im Blute zu erblicken,", "tokens": ["Dies", "mein", "ge\u00b7treu\u00b7es", "Herz", "im", "Blu\u00b7te", "zu", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Jauchzen m\u00fcste dich noch halb so scharf entz\u00fccken;", "tokens": ["Sein", "Jauch\u00b7zen", "m\u00fcs\u00b7te", "dich", "noch", "halb", "so", "scharf", "ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "ADJD", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So viel hier Tropfen gehn, so viel auch W\u00fcntsche schreyn,", "tokens": ["So", "viel", "hier", "Trop\u00b7fen", "gehn", ",", "so", "viel", "auch", "W\u00fcnt\u00b7sche", "schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "VVINF", "$,", "ADV", "ADV", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dir mit geschickter Hand und tausend Freudenz\u00e4hren", "tokens": ["Dir", "mit", "ge\u00b7schick\u00b7ter", "Hand", "und", "tau\u00b7send", "Freu\u00b7den\u00b7z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "KON", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Wollust \u00fcber dir nachdr\u00fccklich zu erkl\u00e4ren.", "tokens": ["Die", "Wol\u00b7lust", "\u00fc\u00b7ber", "dir", "nach\u00b7dr\u00fcck\u00b7lich", "zu", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich hatte mich nunmehr des Gl\u00fcckes l\u00e4ngst verziehn,", "tokens": ["Ich", "hat\u00b7te", "mich", "nun\u00b7mehr", "des", "Gl\u00fc\u00b7ckes", "l\u00e4ngst", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch einmahl auf der Welt mein Lenchen zu umfangen,", "tokens": ["Noch", "ein\u00b7mahl", "auf", "der", "Welt", "mein", "Len\u00b7chen", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich ward in fremder Luft von Freunden hintergangen", "tokens": ["Ich", "ward", "in", "frem\u00b7der", "Luft", "von", "Freun\u00b7den", "hin\u00b7ter\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und muste blos und arm bald hier- bald dorthin fliehn;", "tokens": ["Und", "mus\u00b7te", "blos", "und", "arm", "bald", "hier", "bald", "dor\u00b7thin", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "KON", "ADJD", "ADV", "TRUNC", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Tr\u00fcbsahl machte mich durch L\u00e4ng und Gr\u00f6\u00dfe m\u00fcrbe,", "tokens": ["Die", "Tr\u00fcb\u00b7sahl", "mach\u00b7te", "mich", "durch", "L\u00e4ng", "und", "Gr\u00f6\u00b7\u00dfe", "m\u00fcr\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So da\u00df ich ofters sprach: Ach, g\u00e4be Gott, ich st\u00fcrbe!", "tokens": ["So", "da\u00df", "ich", "of\u00b7ters", "sprach", ":", "Ach", ",", "g\u00e4\u00b7be", "Gott", ",", "ich", "st\u00fcr\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVFIN", "$.", "ITJ", "$,", "VVFIN", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es w\u00e4r auch bald geschehn: die Kr\u00e4fte fielen hin,", "tokens": ["Es", "w\u00e4r", "auch", "bald", "ge\u00b7schehn", ":", "die", "Kr\u00e4f\u00b7te", "fie\u00b7len", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Fieber grif mich an und warf mich auf das Bette;", "tokens": ["Das", "Fie\u00b7ber", "grif", "mich", "an", "und", "warf", "mich", "auf", "das", "Bet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da w\u00fcntscht ich, da\u00df ich nur dein Abschiedsm\u00e4ulchen h\u00e4tte;", "tokens": ["Da", "w\u00fcnt\u00b7scht", "ich", ",", "da\u00df", "ich", "nur", "dein", "Ab\u00b7schieds\u00b7m\u00e4ul\u00b7chen", "h\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Doch sprach ich: Da ich schon dazu versehen bin,", "tokens": ["Doch", "sprach", "ich", ":", "Da", "ich", "schon", "da\u00b7zu", "ver\u00b7se\u00b7hen", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "KOUS", "PPER", "ADV", "PAV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So las doch nur, mein Gott, nebst viel- und wahrem Seegen", "tokens": ["So", "las", "doch", "nur", ",", "mein", "Gott", ",", "nebst", "viel", "und", "wah\u00b7rem", "See\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Alter, so mir fehlt, zu Lenchens Jahren legen!", "tokens": ["Das", "Al\u00b7ter", ",", "so", "mir", "fehlt", ",", "zu", "Len\u00b7chens", "Jah\u00b7ren", "le\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gott hat mich noch so lieb und will dir, werthes Herz,", "tokens": ["Gott", "hat", "mich", "noch", "so", "lieb", "und", "will", "dir", ",", "wert\u00b7hes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Leben durch mein Grab noch nicht so elend machen.", "tokens": ["Das", "Le\u00b7ben", "durch", "mein", "Grab", "noch", "nicht", "so", "e\u00b7lend", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verbanne deinen Gram, fang an, aufs neu zu lachen,", "tokens": ["Ver\u00b7ban\u00b7ne", "dei\u00b7nen", "Gram", ",", "fang", "an", ",", "aufs", "neu", "zu", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,", "APPRART", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verkl\u00e4re Blick und Mund mit Freundligkeit und Scherz,", "tokens": ["Ver\u00b7kl\u00e4\u00b7re", "Blick", "und", "Mund", "mit", "Freund\u00b7lig\u00b7keit", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Damit mir, wenn ich dich in nechstem Tage spreche,", "tokens": ["Da\u00b7mit", "mir", ",", "wenn", "ich", "dich", "in", "nechs\u00b7tem", "Ta\u00b7ge", "spre\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein Unmuth alle Lust nicht wider Willen schw\u00e4che.", "tokens": ["Dein", "Un\u00b7muth", "al\u00b7le", "Lust", "nicht", "wi\u00b7der", "Wil\u00b7len", "schw\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dies ist der vierte Herbst, seit dem ich dich entbehrt;", "tokens": ["Dies", "ist", "der", "vier\u00b7te", "Herbst", ",", "seit", "dem", "ich", "dich", "ent\u00b7behrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hab ich in der Zeit vor Ungemach erlidten!", "tokens": ["Was", "hab", "ich", "in", "der", "Zeit", "vor", "Un\u00b7ge\u00b7mach", "er\u00b7lid\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hat man nicht auf mich vor Creuze zugeschnidten!", "tokens": ["Was", "hat", "man", "nicht", "auf", "mich", "vor", "Creu\u00b7ze", "zu\u00b7ge\u00b7schnid\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "PTKNEG", "APPR", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Welch Arbeit hat mir nicht der Glieder Marck verzehrt!", "tokens": ["Welch", "Ar\u00b7beit", "hat", "mir", "nicht", "der", "Glie\u00b7der", "Marck", "ver\u00b7zehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was hat man mir vor Schimpf statt Wohlthat zugeme\u00dfen!", "tokens": ["Was", "hat", "man", "mir", "vor", "Schimpf", "statt", "Wohlt\u00b7hat", "zu\u00b7ge\u00b7me\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "PPER", "APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gnug! Da ich Lenchen seh, sey alles gern verge\u00dfen.", "tokens": ["Gnug", "!", "Da", "ich", "Len\u00b7chen", "seh", ",", "sey", "al\u00b7les", "gern", "ver\u00b7ge\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "PPER", "NN", "VVFIN", "$,", "VAFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ach aber, was vor Furcht verringert mir die Lust?", "tokens": ["Ach", "a\u00b7ber", ",", "was", "vor", "Furcht", "ver\u00b7rin\u00b7gert", "mir", "die", "Lust", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "PRELS", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach, kr\u00e4h ich auch zu fr\u00fch? Ach, werd ich auch betrogen?", "tokens": ["Ach", ",", "kr\u00e4h", "ich", "auch", "zu", "fr\u00fch", "?", "Ach", ",", "werd", "ich", "auch", "be\u00b7tro\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$.", "ITJ", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer weis, ist nicht dein Schwur mit Zeit und Wind verflogen?", "tokens": ["Wer", "weis", ",", "ist", "nicht", "dein", "Schwur", "mit", "Zeit", "und", "Wind", "ver\u00b7flo\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "$,", "VAFIN", "PTKNEG", "PPOSAT", "VVFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer weis, steht G\u00fcnther noch in jener Schwanenbrust?", "tokens": ["Wer", "weis", ",", "steht", "G\u00fcn\u00b7ther", "noch", "in", "je\u00b7ner", "Schwa\u00b7nen\u00b7brust", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "$,", "VVFIN", "NE", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vielleicht war meine Noth und langes Au\u00dfenbleiben", "tokens": ["Viel\u00b7leicht", "war", "mei\u00b7ne", "Noth", "und", "lan\u00b7ges", "Au\u00b7\u00dfen\u00b7blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So m\u00e4chtig, Lenchens Herz in andre Brunst zu treiben?", "tokens": ["So", "m\u00e4ch\u00b7tig", ",", "Len\u00b7chens", "Herz", "in", "and\u00b7re", "Brunst", "zu", "trei\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NE", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Dies glaub ich doch wohl nicht. Nein, falscher Argwohn, fleuch!", "tokens": ["Dies", "glaub", "ich", "doch", "wohl", "nicht", ".", "Nein", ",", "fal\u00b7scher", "Arg\u00b7wohn", ",", "fleuch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$.", "PTKANT", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ist mir zu genau mit Wort und Fleisch verbunden;", "tokens": ["Sie", "ist", "mir", "zu", "ge\u00b7nau", "mit", "Wort", "und", "Fleisch", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKA", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich habe sie gepr\u00fcft und allzeit rein befunden,", "tokens": ["Ich", "ha\u00b7be", "sie", "ge\u00b7pr\u00fcft", "und", "all\u00b7zeit", "rein", "be\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und darum hof ich auch ein irdisch Himmelreich,", "tokens": ["Und", "da\u00b7rum", "hof", "ich", "auch", "ein", "ir\u00b7disch", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn endlich Gott und Zeit die Sehnsucht stillen wollen", "tokens": ["Wenn", "end\u00b7lich", "Gott", "und", "Zeit", "die", "Sehn\u00b7sucht", "stil\u00b7len", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und unsre Glieder sich in Myrthen paaren sollen.", "tokens": ["Und", "uns\u00b7re", "Glie\u00b7der", "sich", "in", "Myr\u00b7then", "paa\u00b7ren", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Man lacht uns beiderseits, geliebter Engel, aus,", "tokens": ["Man", "lacht", "uns", "bei\u00b7der\u00b7seits", ",", "ge\u00b7lieb\u00b7ter", "En\u00b7gel", ",", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum ich armes Kind dich armes Kind erwehle;", "tokens": ["Wa\u00b7rum", "ich", "ar\u00b7mes", "Kind", "dich", "ar\u00b7mes", "Kind", "er\u00b7weh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man meint, wo Liebe nicht die g\u00fcldnen Ringe zehle,", "tokens": ["Man", "meint", ",", "wo", "Lie\u00b7be", "nicht", "die", "g\u00fcld\u00b7nen", "Rin\u00b7ge", "zeh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da komme nach und nach der Mangel in das Haus.", "tokens": ["Da", "kom\u00b7me", "nach", "und", "nach", "der", "Man\u00b7gel", "in", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch las dich, treues Herz, den blinden Wahn nicht irren;", "tokens": ["Doch", "las", "dich", ",", "treu\u00b7es", "Herz", ",", "den", "blin\u00b7den", "Wahn", "nicht", "ir\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gott kan den Rechnungsschlu\u00df der Sp\u00f6tter leicht verwirren.", "tokens": ["Gott", "kan", "den", "Rech\u00b7nungs\u00b7schlu\u00df", "der", "Sp\u00f6t\u00b7ter", "leicht", "ver\u00b7wir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.10": {"line.1": {"text": "Ich hab es oft gesagt und sag es noch einmahl:", "tokens": ["Ich", "hab", "es", "oft", "ge\u00b7sagt", "und", "sag", "es", "noch", "ein\u00b7mahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wollte, bliebe mir kein be\u00dfer Gl\u00fcck auf Erden,", "tokens": ["Ich", "woll\u00b7te", ",", "blie\u00b7be", "mir", "kein", "be\u00b7\u00dfer", "Gl\u00fcck", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bey Salz und Brodt mit dir in H\u00fctten seelig werden", "tokens": ["Bey", "Salz", "und", "Brodt", "mit", "dir", "in", "H\u00fct\u00b7ten", "see\u00b7lig", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "APPR", "PPER", "APPR", "NN", "ADJD", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und halt ein gro\u00dfes Gut im Lieben nur vor Qual.", "tokens": ["Und", "halt", "ein", "gro\u00b7\u00dfes", "Gut", "im", "Lie\u00b7ben", "nur", "vor", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Flei\u00df wird endlich auch nach so viel na\u00dfen Tagen", "tokens": ["Mein", "Flei\u00df", "wird", "end\u00b7lich", "auch", "nach", "so", "viel", "na\u00b7\u00dfen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Ruhm und Anmuth bl\u00fchn und reife Fr\u00fcchte tragen.", "tokens": ["Mit", "Ruhm", "und", "An\u00b7muth", "bl\u00fchn", "und", "rei\u00b7fe", "Fr\u00fcch\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Gedencke nur zur\u00fcck und sieh die Schwester an;", "tokens": ["Ge\u00b7den\u00b7cke", "nur", "zu\u00b7r\u00fcck", "und", "sieh", "die", "Schwes\u00b7ter", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "KON", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wie ich prophezeit, so ist es auch ergangen.", "tokens": ["So", "wie", "ich", "pro\u00b7phe\u00b7zeit", ",", "so", "ist", "es", "auch", "er\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hilft ihr aller Prast von Kleidern, Perl- und Spangen,", "tokens": ["Was", "hilft", "ihr", "al\u00b7ler", "Prast", "von", "Klei\u00b7dern", ",", "Per\u00b7l", "und", "Span\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "NN", "APPR", "NN", "$,", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn kein geruhig Herz davon genie\u00dfen kan?", "tokens": ["Wenn", "kein", "ge\u00b7ru\u00b7hig", "Herz", "da\u00b7von", "ge\u00b7nie\u00b7\u00dfen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJD", "NN", "PAV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr Ku\u00df ist lauter Gift, ihr Ehbett eine H\u00f6lle,", "tokens": ["Ihr", "Ku\u00df", "ist", "lau\u00b7ter", "Gift", ",", "ihr", "Eh\u00b7bett", "ei\u00b7ne", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wo ihr Mann nur weicht, f\u00fcllt Schimpf und Groll die Stelle.", "tokens": ["Und", "wo", "ihr", "Mann", "nur", "weicht", ",", "f\u00fcllt", "Schimpf", "und", "Groll", "die", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "VVFIN", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Nur bitt ich, trau nechst Gott sonst keiner Seel als mir!", "tokens": ["Nur", "bitt", "ich", ",", "trau", "nechst", "Gott", "sonst", "kei\u00b7ner", "Seel", "als", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "NN", "ADV", "PIAT", "NN", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist mein Schaz und Ruhm, dich will ich auch besch\u00fczen.", "tokens": ["Du", "bist", "mein", "Schaz", "und", "Ruhm", ",", "dich", "will", "ich", "auch", "be\u00b7sch\u00fc\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Las fahren, was nicht bleibt, las Tadler Pfeile schnizen;", "tokens": ["Las", "fah\u00b7ren", ",", "was", "nicht", "bleibt", ",", "las", "Tad\u00b7ler", "Pfei\u00b7le", "schni\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "PRELS", "PTKNEG", "VVFIN", "$,", "VVFIN", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Blutsfreund ist so nah, er schadet mir und dir;", "tokens": ["Kein", "Bluts\u00b7freund", "ist", "so", "nah", ",", "er", "scha\u00b7det", "mir", "und", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gott r\u00e4che mit Gedult und Ablas ihre S\u00fcnden,", "tokens": ["Gott", "r\u00e4\u00b7che", "mit", "Ge\u00b7dult", "und", "Ab\u00b7las", "ih\u00b7re", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir werden unsern Herd ohn ihren Vorschu\u00df finden.", "tokens": ["Wir", "wer\u00b7den", "un\u00b7sern", "Herd", "ohn", "ih\u00b7ren", "Vor\u00b7schu\u00df", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Ach, breite zum Voraus Hand, Lippen, Brust und Arm,", "tokens": ["Ach", ",", "brei\u00b7te", "zum", "Vo\u00b7raus", "Hand", ",", "Lip\u00b7pen", ",", "Brust", "und", "Arm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "APPRART", "NN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich komm und zitter schon vor Unruh u