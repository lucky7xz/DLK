{"textgrid.poem.38573": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Natur", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Saht ihr, in stiller Sommernacht, den Mond", "tokens": ["Saht", "ihr", ",", "in", "stil\u00b7ler", "Som\u00b7mer\u00b7nacht", ",", "den", "Mond"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch melancholische Zypressen schaun,", "tokens": ["Durch", "me\u00b7lan\u00b7cho\u00b7li\u00b7sche", "Zyp\u00b7res\u00b7sen", "schaun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wann ringsumher die feiernde Natur", "tokens": ["Wann", "rings\u00b7um\u00b7her", "die", "fei\u00b7ern\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Schlummer sank und kaum zu athmen schien,", "tokens": ["In", "Schlum\u00b7mer", "sank", "und", "kaum", "zu", "ath\u00b7men", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jedes Herz in s\u00fcsser Wehmuth schmolz?", "tokens": ["Und", "je\u00b7des", "Herz", "in", "s\u00fcs\u00b7ser", "Weh\u00b7muth", "schmolz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Saht ihr, vom goldnen Abendd\u00e4mmrungslicht", "tokens": ["Saht", "ihr", ",", "vom", "gold\u00b7nen", "A\u00b7bend\u00b7d\u00e4mm\u00b7rungs\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sanft angestralt, in stiller Majest\u00e4t,", "tokens": ["Sanft", "an\u00b7ge\u00b7stralt", ",", "in", "stil\u00b7ler", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Helveziens beeiste Gipfel gl\u00fchn?", "tokens": ["Hel\u00b7ve\u00b7zi\u00b7ens", "be\u00b7eis\u00b7te", "Gip\u00b7fel", "gl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Saht ihr, wie dort vom schroffen Fels der Rhein,", "tokens": ["Saht", "ihr", ",", "wie", "dort", "vom", "schrof\u00b7fen", "Fels", "der", "Rhein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "APPRART", "ADJA", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Gleich immerdonnernden Gewittern, sich", "tokens": ["Gleich", "im\u00b7mer\u00b7don\u00b7nern\u00b7den", "Ge\u00b7wit\u00b7tern", ",", "sich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "In hochgeth\u00fcrmte Schaumgebirge st\u00fcrzt?", "tokens": ["In", "hoch\u00b7get\u00b7h\u00fcrm\u00b7te", "Schaum\u00b7ge\u00b7bir\u00b7ge", "st\u00fcrzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ha! selbst der hundertj\u00e4hr'gen Eiche Stamm", "tokens": ["Ha", "!", "selbst", "der", "hun\u00b7dert\u00b7j\u00e4hr'\u00b7gen", "Ei\u00b7che", "Stamm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ist seinen Riesenwogen hier ein Spiel!", "tokens": ["Ist", "sei\u00b7nen", "Rie\u00b7sen\u00b7wo\u00b7gen", "hier", "ein", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Saht ihr, vom Sturm emp\u00f6rt, den Ozean,", "tokens": ["Saht", "ihr", ",", "vom", "Sturm", "em\u00b7p\u00f6rt", ",", "den", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Mit ungez\u00e4hmter Wuth, bald himmelw\u00e4rts", "tokens": ["Mit", "un\u00b7ge\u00b7z\u00e4hm\u00b7ter", "Wuth", ",", "bald", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.16": {"text": "Verschlagne Flotten schleudern, bald hinab", "tokens": ["Ver\u00b7schlag\u00b7ne", "Flot\u00b7ten", "schleu\u00b7dern", ",", "bald", "hin\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "$,", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Zur schwarzen Tiefe st\u00fcrzen, donnernd sich", "tokens": ["Zur", "schwar\u00b7zen", "Tie\u00b7fe", "st\u00fcr\u00b7zen", ",", "don\u00b7nernd", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,", "ADJD", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Noch einmal heben, und die Leichen dann", "tokens": ["Noch", "ein\u00b7mal", "he\u00b7ben", ",", "und", "die", "Lei\u00b7chen", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "$,", "KON", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Hochbrandend schmettern an das Felsgestad?", "tokens": ["Hoch\u00b7bran\u00b7dend", "schmet\u00b7tern", "an", "das", "Fels\u00b7ge\u00b7stad", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Saht ihr dies alles, so beschw\u00f6r' ich euch,", "tokens": ["Saht", "ihr", "dies", "al\u00b7les", ",", "so", "be\u00b7schw\u00f6r'", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PIS", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "O Dichterlinge! bei den Grazien", "tokens": ["O", "Dich\u00b7ter\u00b7lin\u00b7ge", "!", "bei", "den", "Gra\u00b7zi\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und Musen! bei des M\u00e4oniden Geist!", "tokens": ["Und", "Mu\u00b7sen", "!", "bei", "des", "M\u00e4o\u00b7ni\u00b7den", "Geist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Bei jenen H\u00f6h'n, die Klopstocks Genius", "tokens": ["Bei", "je\u00b7nen", "H\u00f6h'n", ",", "die", "Klops\u00b7tocks", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Zuerst erschwebte! bei dem Harfenklang", "tokens": ["Zu\u00b7erst", "er\u00b7schweb\u00b7te", "!", "bei", "dem", "Har\u00b7fen\u00b7klang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Von Fingals Barden! bei Petrarkas Quell!", "tokens": ["Von", "Fin\u00b7gals", "Bar\u00b7den", "!", "bei", "Pe\u00b7trar\u00b7kas", "Quell", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$.", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Beim Lorbeerbaum der Maros Grab umrauscht!", "tokens": ["Beim", "Lor\u00b7beer\u00b7baum", "der", "Ma\u00b7ros", "Grab", "um\u00b7rauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Bei jenem Paradies der Feeerei", "tokens": ["Bei", "je\u00b7nem", "Pa\u00b7ra\u00b7dies", "der", "Fe\u00b7e\u00b7e\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.28": {"text": "Wo einst Rinaldos Heldenkraft erlag!", "tokens": ["Wo", "einst", "Ri\u00b7nal\u00b7dos", "Hel\u00b7den\u00b7kraft", "er\u00b7lag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Bei Miltons Lichtgru\u00df! bei dem d\u00fcstern Flor", "tokens": ["Bei", "Mil\u00b7tons", "Licht\u00b7gru\u00df", "!", "bei", "dem", "d\u00fcs\u00b7tern", "Flor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Um Dantes Nachtst\u00fcck: Ugolinos Tod!", "tokens": ["Um", "Dan\u00b7tes", "Nacht\u00b7st\u00fcck", ":", "U\u00b7go\u00b7li\u00b7nos", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Bei Hamlets Seyn und Nichtseyn! beim Ergu\u00df", "tokens": ["Bei", "Ham\u00b7lets", "Seyn", "und", "Nich\u00b7tseyn", "!", "beim", "Er\u00b7gu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "NE", "KON", "NN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Des Vaterherzens an Narzissas Gruft!", "tokens": ["Des", "Va\u00b7ter\u00b7her\u00b7zens", "an", "Nar\u00b7zis\u00b7sas", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Bei Wielands rosenfarbner Zauberwelt!", "tokens": ["Bei", "Wie\u00b7lands", "ro\u00b7sen\u00b7farb\u00b7ner", "Zau\u00b7ber\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Bei Uzens Sonnenflug, bei Allem was", "tokens": ["Bei", "U\u00b7zens", "Son\u00b7nen\u00b7flug", ",", "bei", "Al\u00b7lem", "was"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "APPR", "PIS", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Dem Dichter heilig ist, beschw\u00f6r' ich euch:", "tokens": ["Dem", "Dich\u00b7ter", "hei\u00b7lig", "ist", ",", "be\u00b7schw\u00f6r'", "ich", "euch", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Entweihet nicht das Allerheiligste", "tokens": ["Ent\u00b7wei\u00b7het", "nicht", "das", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.37": {"text": "Der g\u00f6ttlichen Natur, in Red' und Sang,", "tokens": ["Der", "g\u00f6tt\u00b7li\u00b7chen", "Na\u00b7tur", ",", "in", "Red'", "und", "Sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Durch leeres Wortgesch\u00e4um von Seelensturm,", "tokens": ["Durch", "lee\u00b7res", "Wort\u00b7ge\u00b7sch\u00e4um", "von", "See\u00b7len\u00b7sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Von Schwung und Allkraft, Drang und Hochgef\u00fchl!", "tokens": ["Von", "Schwung", "und", "All\u00b7kraft", ",", "Drang", "und", "Hoch\u00b7ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Denn wisset, es verschm\u00e4ht die G\u00f6ttliche", "tokens": ["Denn", "wis\u00b7set", ",", "es", "ver\u00b7schm\u00e4ht", "die", "G\u00f6tt\u00b7li\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.41": {"text": "Der Dichterlinge Kainsopfer, winkt", "tokens": ["Der", "Dich\u00b7ter\u00b7lin\u00b7ge", "Kain\u00b7sop\u00b7fer", ",", "winkt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.42": {"text": "Dem Sturm der Zeit, lautz\u00fcrnend, zu verwehn", "tokens": ["Dem", "Sturm", "der", "Zeit", ",", "laut\u00b7z\u00fcr\u00b7nend", ",", "zu", "ver\u00b7wehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Den schwarzen Dampf, der ihr ein Gr\u00e4uel ist!", "tokens": ["Den", "schwar\u00b7zen", "Dampf", ",", "der", "ihr", "ein", "Gr\u00e4u\u00b7el", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}