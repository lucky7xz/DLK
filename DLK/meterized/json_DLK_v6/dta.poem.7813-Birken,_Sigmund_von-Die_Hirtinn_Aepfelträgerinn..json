{"dta.poem.7813": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hirtinn Aepfeltr\u00e4gerinn.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00f6ne Hirtinn gieng/ trug \u00fcber ihrem", "tokens": ["Ein", "sch\u00f6\u00b7ne", "Hir\u00b7tinn", "gieng", "/", "trug", "\u00fc\u00b7ber", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "voll Aepfel einen Korb. Mit beyder Hand", "tokens": ["voll", "A\u00b7e\u00b7pfel", "ei\u00b7nen", "Korb", ".", "Mit", "bey\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "ART", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "die liebe Last empor/ die sie vom Ast geraubt.", "tokens": ["die", "lie\u00b7be", "Last", "em\u00b7por", "/", "die", "sie", "vom", "Ast", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$(", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als der verliebte West mit ihrem Kleide", "tokens": ["Als", "der", "ver\u00b7lieb\u00b7te", "West", "mit", "ih\u00b7rem", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "fiel das ihr oberhalb vom weissen Busen ab:", "tokens": ["fiel", "das", "ihr", "o\u00b7ber\u00b7halb", "vom", "weis\u00b7sen", "Bu\u00b7sen", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPER", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da sich dann bald ein paar Milch-sch\u00f6ner", "tokens": ["da", "sich", "dann", "bald", "ein", "paar", "Milch\u00b7sch\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Sch\u00e4fer/ deme Lust die zarte Bl\u00f6sse gab/", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "/", "de\u00b7me", "Lust", "die", "zar\u00b7te", "Bl\u00f6s\u00b7se", "gab", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sprach/ indem sein Gesicht er auf sie schies-", "tokens": ["sprach", "/", "in\u00b7dem", "sein", "Ge\u00b7sicht", "er", "auf", "sie", "schies"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "KOUS", "PPOSAT", "NN", "PPER", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich mag der Aepfel nicht/ die Die gebrochen", "tokens": ["Ich", "mag", "der", "A\u00b7e\u00b7pfel", "nicht", "/", "die", "Die", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "$(", "ART", "ART", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die mag ich/ die ich dort am Baum noch han-", "tokens": ["die", "mag", "ich", "/", "die", "ich", "dort", "am", "Baum", "noch", "han"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "$(", "PRELS", "PPER", "ADV", "APPRART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}