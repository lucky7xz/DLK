{"textgrid.poem.63959": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Hymnus an die Dummheit", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dummheit, erhabene G\u00f6ttin,", "tokens": ["Dumm\u00b7heit", ",", "er\u00b7ha\u00b7be\u00b7ne", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Unsere Patronin,", "tokens": ["Un\u00b7se\u00b7re", "Pat\u00b7ro\u00b7nin", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die du auf goldenem Throne,", "tokens": ["Die", "du", "auf", "gol\u00b7de\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf niedriger Stirne die blitzende Krone,", "tokens": ["Auf", "nied\u00b7ri\u00b7ger", "Stir\u00b7ne", "die", "blit\u00b7zen\u00b7de", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Stumpfsinnig erhabenes L\u00e4cheln", "tokens": ["Stumpf\u00b7sin\u00b7nig", "er\u00b7ha\u00b7be\u00b7nes", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Auf breitem, nichtssagendem Antlitz \u2013", "tokens": ["Auf", "brei\u00b7tem", ",", "nichts\u00b7sa\u00b7gen\u00b7dem", "Ant\u00b7litz", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "K\u00f6niglich sitzest:", "tokens": ["K\u00f6\u00b7nig\u00b7lich", "sit\u00b7zest", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Siehe herab mit der Milde Miene", "tokens": ["Sie\u00b7he", "her\u00b7ab", "mit", "der", "Mil\u00b7de", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Auf deine treuen, dir nach-", "tokens": ["Auf", "dei\u00b7ne", "treu\u00b7en", ",", "dir", "nach"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "PPER", "TRUNC"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Dummenden Kinder,", "tokens": ["Dum\u00b7men\u00b7den", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Verjage aus dem Land", "tokens": ["Ver\u00b7ja\u00b7ge", "aus", "dem", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die Dichter und K\u00fcnstler und Denker,", "tokens": ["Die", "Dich\u00b7ter", "und", "K\u00fcnst\u00b7ler", "und", "Den\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Unsere Ver\u00e4chter,", "tokens": ["Un\u00b7se\u00b7re", "Ver\u00b7\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Vernichte die B\u00fccher, \u2013 Traumbuch und Rechenknecht,", "tokens": ["Ver\u00b7nich\u00b7te", "die", "B\u00fc\u00b7cher", ",", "\u2013", "Traum\u00b7buch", "und", "Re\u00b7chen\u00b7knecht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "$(", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Briefsteller und Lacherbsen verschonend,", "tokens": ["Brief\u00b7stel\u00b7ler", "und", "La\u00b7cherb\u00b7sen", "ver\u00b7scho\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+++--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und wir bringen ein Eselchen dir,", "tokens": ["Und", "wir", "brin\u00b7gen", "ein", "E\u00b7sel\u00b7chen", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Dein Lieblingstier,", "tokens": ["Dein", "Lieb\u00b7lings\u00b7tier", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Dein mildes, sanftes, ohrenaufsteigendes", "tokens": ["Dein", "mil\u00b7des", ",", "sanf\u00b7tes", ",", "oh\u00b7ren\u00b7auf\u00b7stei\u00b7gen\u00b7des"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "$,", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Lieblingstier", "tokens": ["Lieb\u00b7lings\u00b7tier"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Eine goldene Krippe daf\u00fcr", "tokens": ["Ei\u00b7ne", "gol\u00b7de\u00b7ne", "Krip\u00b7pe", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "Und ein purpurnes Laken von Disteln.", "tokens": ["Und", "ein", "pur\u00b7pur\u00b7nes", "La\u00b7ken", "von", "Dis\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}