{"textgrid.poem.39269": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[als Gott der Ew'ge niederw\u00e4rts gestiegen]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Gott der Ew'ge niederw\u00e4rts gestiegen,", "tokens": ["Als", "Gott", "der", "Ew'\u00b7ge", "nie\u00b7der\u00b7w\u00e4rts", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Hauch geformt den Geist, den Himmelssprossen,", "tokens": ["Sein", "Hauch", "ge\u00b7formt", "den", "Geist", ",", "den", "Him\u00b7mels\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da hat er Flammenmut ihm eingegossen,", "tokens": ["Da", "hat", "er", "Flam\u00b7men\u00b7mut", "ihm", "ein\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Flammenmut, zu k\u00e4mpfen und zu siegen.", "tokens": ["Den", "Flam\u00b7men\u00b7mut", ",", "zu", "k\u00e4mp\u00b7fen", "und", "zu", "sie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbnicht magst du,\u00ab rief er, \u00bbzagend unterliegen,", "tokens": ["\u00bb", "nicht", "magst", "du", ",", "\u00ab", "rief", "er", ",", "\u00bb", "za\u00b7gend", "un\u00b7ter\u00b7lie\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "VMFIN", "PPER", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Weh des Ungl\u00fccks dr\u00e4uend eingeschlossen,", "tokens": ["Vom", "Weh", "des", "Un\u00b7gl\u00fccks", "dr\u00e4u\u00b7end", "ein\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fahren sollst du, wie auf Sonnenrossen,", "tokens": ["Und", "fah\u00b7ren", "sollst", "du", ",", "wie", "auf", "Son\u00b7nen\u00b7ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Nacht und Kampf, die grimm sich an dich schmiegen.\u00ab", "tokens": ["Durch", "Nacht", "und", "Kampf", ",", "die", "grimm", "sich", "an", "dich", "schmie\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "ADJD", "PRF", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbnicht vor den M\u00e4cht'gen sollst du knieend flehen;", "tokens": ["\u00bb", "nicht", "vor", "den", "M\u00e4cht'\u00b7gen", "sollst", "du", "kni\u00b7e\u00b7end", "fle\u00b7hen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPR", "ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das Aug' hoch hebend zu des \u00c4thers Weiten,", "tokens": ["Das", "Aug'", "hoch", "he\u00b7bend", "zu", "des", "\u00c4\u00b7thers", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sollst frei du vor dem Gott der Freiheit stehen.\u00ab", "tokens": ["Sollst", "frei", "du", "vor", "dem", "Gott", "der", "Frei\u00b7heit", "ste\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbnicht mag ich kriechend ", "tokens": ["\u00bb", "nicht", "mag", "ich", "krie\u00b7chend"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PTKNEG", "VMFIN", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Denn nur der Freie mag zum Himmel schreiten.\u00ab", "tokens": ["Denn", "nur", "der", "Frei\u00b7e", "mag", "zum", "Him\u00b7mel", "schrei\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}