{"textgrid.poem.42392": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du gingst so vorschnell aus der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gingst so vorschnell aus der Welt", "tokens": ["Du", "gingst", "so", "vor\u00b7schnell", "aus", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und liessest so das Leben stehn,", "tokens": ["Und", "lies\u00b7sest", "so", "das", "Le\u00b7ben", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie, wenn sie M\u00fcdigkeit bef\u00e4llt,", "tokens": ["Wie", ",", "wenn", "sie", "M\u00fc\u00b7dig\u00b7keit", "be\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kinder abends schlafen gehn.", "tokens": ["Die", "Kin\u00b7der", "a\u00b7bends", "schla\u00b7fen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du schwebst mir vor so licht und wert.", "tokens": ["Du", "schwebst", "mir", "vor", "so", "licht", "und", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch harte Mannesk\u00e4mpfe blieb", "tokens": ["Durch", "har\u00b7te", "Man\u00b7nes\u00b7k\u00e4mp\u00b7fe", "blieb"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf deinen Lippen, unversehrt,", "tokens": ["Auf", "dei\u00b7nen", "Lip\u00b7pen", ",", "un\u00b7ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Knabenl\u00e4cheln zart und lieb.", "tokens": ["Ein", "Kna\u00b7ben\u00b7l\u00e4\u00b7cheln", "zart", "und", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Walde liegt so tief der Schnee,", "tokens": ["Im", "Wal\u00b7de", "liegt", "so", "tief", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie liegt er hoch auf deinem Grab!", "tokens": ["Wie", "liegt", "er", "hoch", "auf", "dei\u00b7nem", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist dahingegangen, eh", "tokens": ["Du", "bist", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen", ",", "eh"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PAV", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Schicksal dir Gen\u00fcge gab.", "tokens": ["Dein", "Schick\u00b7sal", "dir", "Ge\u00b7n\u00fc\u00b7ge", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Weiss wer, wo sich der Wind erhebt?", "tokens": ["Weiss", "wer", ",", "wo", "sich", "der", "Wind", "er\u00b7hebt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kennt wer der Wanderwolke Fahrt?", "tokens": ["Kennt", "wer", "der", "Wan\u00b7der\u00b7wol\u00b7ke", "Fahrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du schweiftest fern und bist entschwebt", "tokens": ["Du", "schweif\u00b7test", "fern", "und", "bist", "ent\u00b7schwebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Windesweise, Wolkenart.", "tokens": ["Nach", "Win\u00b7des\u00b7wei\u00b7se", ",", "Wol\u00b7ken\u00b7art", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}