{"textgrid.poem.45278": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ihr seid wohl Minister dem Namen nach]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid wohl Minister dem Namen nach,", "tokens": ["Ihr", "seid", "wohl", "Mi\u00b7nis\u00b7ter", "dem", "Na\u00b7men", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Doch die Sache liegt annoch im weiten,", "tokens": ["Doch", "die", "Sa\u00b7che", "liegt", "an\u00b7noch", "im", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wenn ich einen zum Oberstallmeister mach,", "tokens": ["Wenn", "ich", "ei\u00b7nen", "zum", "O\u00b7ber\u00b7stall\u00b7meis\u00b7ter", "mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "So kann er darum noch nicht reiten.", "tokens": ["So", "kann", "er", "da\u00b7rum", "noch", "nicht", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PAV", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}