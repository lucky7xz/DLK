{"dta.poem.8852": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00fcnscht' ich, da\u00df die Liebesleiter keine h\u00f6h're Sprossen", "tokens": ["W\u00fcnscht'", "ich", ",", "da\u00df", "die", "Lie\u00b7be\u00b7slei\u00b7ter", "kei\u00b7ne", "h\u00f6h'\u00b7re", "Spros\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Denn was m\u00fc\u00dfte Der empfinden, der an deinen Lippen", "tokens": ["Denn", "was", "m\u00fc\u00df\u00b7te", "Der", "emp\u00b7fin\u00b7den", ",", "der", "an", "dei\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "PDS", "VVFIN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Diese sch\u00f6nen, keuschen Formen jugendlich umschlossen", "tokens": ["Die\u00b7se", "sch\u00f6\u00b7nen", ",", "keu\u00b7schen", "For\u00b7men", "ju\u00b7gend\u00b7lich", "um\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "$,", "ADJA", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Freudetrunken dir am Busen w\u00fcrd' ich br\u00fcnstig weinen", "tokens": ["Freu\u00b7de\u00b7trun\u00b7ken", "dir", "am", "Bu\u00b7sen", "w\u00fcrd'", "ich", "br\u00fcns\u00b7tig", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Wenn ich nicht, doch nicht aus Freude, Thr\u00e4nen schon", "tokens": ["Wenn", "ich", "nicht", ",", "doch", "nicht", "aus", "Freu\u00b7de", ",", "Thr\u00e4\u00b7nen", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "$,", "ADV", "PTKNEG", "APPR", "NN", "$,", "NN", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Wenn ich nun erk\u00fchnt mich h\u00e4tte, leise dir die Hand", "tokens": ["Wenn", "ich", "nun", "er\u00b7k\u00fchnt", "mich", "h\u00e4t\u00b7te", ",", "lei\u00b7se", "dir", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPER", "VAFIN", "$,", "ADJD", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Gar zu gerne m\u00f6cht' ich wissen, ob es dich verdrossen", "tokens": ["Gar", "zu", "ger\u00b7ne", "m\u00f6cht'", "ich", "wis\u00b7sen", ",", "ob", "es", "dich", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "W\u00fcnschen nicht, wir sollen wagen; denn wie leicht ist's,", "tokens": ["W\u00fcn\u00b7schen", "nicht", ",", "wir", "sol\u00b7len", "wa\u00b7gen", ";", "denn", "wie", "leicht", "ist's", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "VMFIN", "VVINF", "$.", "KON", "PWAV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.9": {"text": "Fliegen w\u00fcrd' ich, wenn ich Fl\u00fcgel, schwimmen, wenn", "tokens": ["Flie\u00b7gen", "w\u00fcrd'", "ich", ",", "wenn", "ich", "Fl\u00fc\u00b7gel", ",", "schwim\u00b7men", ",", "wenn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "NN", "$,", "VVFIN", "$,", "KOUS"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Sittenzwang und Formelwesen h\u00e4tten l\u00e4ngst die Welt", "tokens": ["Sit\u00b7ten\u00b7zwang", "und", "For\u00b7mel\u00b7we\u00b7sen", "h\u00e4t\u00b7ten", "l\u00e4ngst", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Wenn sich nicht Gesang zuweilen durch die Welt ergossen", "tokens": ["Wenn", "sich", "nicht", "Ge\u00b7sang", "zu\u00b7wei\u00b7len", "durch", "die", "Welt", "er\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PTKNEG", "NN", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}