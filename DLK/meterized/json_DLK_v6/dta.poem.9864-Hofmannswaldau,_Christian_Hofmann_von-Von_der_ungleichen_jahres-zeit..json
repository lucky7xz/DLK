{"dta.poem.9864": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der ungleichen jahres-zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.71", "nl:0.14", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje? soll die welt in ruhe stehen/", "tokens": ["Wie", "?", "soll", "die", "welt", "in", "ru\u00b7he", "ste\u00b7hen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil die verst\u00e4ndige natur", "tokens": ["Weil", "die", "ver\u00b7st\u00e4n\u00b7di\u00b7ge", "na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die werck in ihrer grossen uhr", "tokens": ["Die", "werck", "in", "ih\u00b7rer", "gros\u00b7sen", "uhr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einander l\u00e4st zu wider gehen?", "tokens": ["Ein\u00b7an\u00b7der", "l\u00e4st", "zu", "wi\u00b7der", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erregt nicht eine jahres-zeit", "tokens": ["Er\u00b7regt", "nicht", "ei\u00b7ne", "jah\u00b7res\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vier mahl auf erden neuen streit?", "tokens": ["Vier", "mahl", "auf", "er\u00b7den", "neu\u00b7en", "streit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Des fr\u00fchlings angenehme sonne", "tokens": ["Des", "fr\u00fch\u00b7lings", "an\u00b7ge\u00b7neh\u00b7me", "son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zerschmeltzt des kalten winters ei\u00df:", "tokens": ["Zer\u00b7schmeltzt", "des", "kal\u00b7ten", "win\u00b7ters", "ei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Des d\u00fcrren sommers heissen schwei\u00df", "tokens": ["Des", "d\u00fcr\u00b7ren", "som\u00b7mers", "heis\u00b7sen", "schwei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vertreibt des k\u00fchlen herbstes wonne:", "tokens": ["Ver\u00b7treibt", "des", "k\u00fch\u00b7len", "herbs\u00b7tes", "won\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Giebt jedes monats unterscheid", "tokens": ["Giebt", "je\u00b7des", "mo\u00b7nats", "un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dem felde nicht ein ander kleid?", "tokens": ["Dem", "fel\u00b7de", "nicht", "ein", "an\u00b7der", "kleid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ART", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Jhr sterblichen/ schaut an die gaben/", "tokens": ["Ihr", "sterb\u00b7li\u00b7chen", "/", "schaut", "an", "die", "ga\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$(", "VVFIN", "APPR", "ART", "VVFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "Die uns der lentz und sommer giebt/", "tokens": ["Die", "uns", "der", "lentz", "und", "som\u00b7mer", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "KON", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Was man im herbst und winter liebt/", "tokens": ["Was", "man", "im", "herbst", "und", "win\u00b7ter", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPRART", "VVFIN", "KON", "NE", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wer soll den prei\u00df des vorzugs haben?", "tokens": ["Wer", "soll", "den", "prei\u00df", "des", "vor\u00b7zugs", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ART", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Des lentzes gra\u00df? des sommers klee?", "tokens": ["Des", "lent\u00b7zes", "gra\u00df", "?", "des", "som\u00b7mers", "klee", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Des herbsies wein? des winters schnee?", "tokens": ["Des", "herb\u00b7sies", "wein", "?", "des", "win\u00b7ters", "schnee", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Des fr\u00fchlings sch\u00f6ne stirn und wangen?", "tokens": ["Des", "fr\u00fch\u00b7lings", "sch\u00f6\u00b7ne", "stirn", "und", "wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Des sommers warme hand und brust?", "tokens": ["Des", "som\u00b7mers", "war\u00b7me", "hand", "und", "brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Des herbstes mund- und augen-lust?", "tokens": ["Des", "herbs\u00b7tes", "mun\u00b7d", "und", "au\u00b7gen\u00b7lust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Des winters wild- und vogel-fangen?", "tokens": ["Des", "win\u00b7ters", "wild", "und", "vo\u00b7gel\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die leibes-st\u00e4rck und liebes macht?", "tokens": ["Die", "lei\u00b7bes\u00b7st\u00e4rck", "und", "lie\u00b7bes", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der kurtze tag/ die lange nacht?", "tokens": ["Der", "kurt\u00b7ze", "tag", "/", "die", "lan\u00b7ge", "nacht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ich kan den gr\u00fcnen lentz nicht hassen:", "tokens": ["Ich", "kan", "den", "gr\u00fc\u00b7nen", "lentz", "nicht", "has\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ja mir gef\u00e4llt des sommers frucht:", "tokens": ["Ja", "mir", "ge\u00b7f\u00e4llt", "des", "som\u00b7mers", "frucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Was man im herbst auf b\u00e4umen sucht:", "tokens": ["Was", "man", "im", "herbst", "auf", "b\u00e4u\u00b7men", "sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPRART", "ADV", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Was man im winter soll umfassen.", "tokens": ["Was", "man", "im", "win\u00b7ter", "soll", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPRART", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "So bringet mir vergn\u00fcgsamkeit", "tokens": ["So", "brin\u00b7get", "mir", "ver\u00b7gn\u00fcg\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ein iedes theil der jahres-zeit.", "tokens": ["Ein", "ie\u00b7des", "theil", "der", "jah\u00b7res\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}