{"dta.poem.16303": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In K\u00f6nigshallen tritt man unbeschuhter ein,", "tokens": ["In", "K\u00f6\u00b7nigs\u00b7hal\u00b7len", "tritt", "man", "un\u00b7be\u00b7schuh\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie sind ausgelegt mit k\u00f6stlichem Gestein.", "tokens": ["Weil", "sie", "sind", "aus\u00b7ge\u00b7legt", "mit", "k\u00f6st\u00b7li\u00b7chem", "Ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O sieh, der Morgen hat mit thauigem Geschmeide", "tokens": ["O", "sieh", ",", "der", "Mor\u00b7gen", "hat", "mit", "thau\u00b7i\u00b7gem", "Ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ART", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Belegt die Gottes Flur; komm und den Fu\u00df entkleide!", "tokens": ["Be\u00b7legt", "die", "Got\u00b7tes", "Flur", ";", "komm", "und", "den", "Fu\u00df", "ent\u00b7klei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$.", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer in des Maien Thau fr\u00fchmorgens wandeln mag,", "tokens": ["Wer", "in", "des", "Mai\u00b7en", "Thau", "fr\u00fch\u00b7mor\u00b7gens", "wan\u00b7deln", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "ADV", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlt sich von unten auf gest\u00e4rkt den ganzen Tag.", "tokens": ["F\u00fchlt", "sich", "von", "un\u00b7ten", "auf", "ge\u00b7st\u00e4rkt", "den", "gan\u00b7zen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADV", "APPR", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Froh f\u00fchle, da\u00df der Herr im Thau den Fu\u00df dir wasche;", "tokens": ["Froh", "f\u00fch\u00b7le", ",", "da\u00df", "der", "Herr", "im", "Thau", "den", "Fu\u00df", "dir", "wa\u00b7sche", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Setz' ihn auf S\u00fcndenschmutz nie noch auf Kummerasche!", "tokens": ["Setz'", "ihn", "auf", "S\u00fcn\u00b7den\u00b7schmutz", "nie", "noch", "auf", "Kum\u00b7me\u00b7ra\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}}}}