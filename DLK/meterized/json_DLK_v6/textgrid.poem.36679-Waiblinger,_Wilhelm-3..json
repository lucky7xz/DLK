{"textgrid.poem.36679": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich's aber nur gestehe,", "tokens": ["Da\u00df", "ich's", "a\u00b7ber", "nur", "ge\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Lieder heitre Weise,", "tokens": ["Die\u00b7ser", "Lie\u00b7der", "heit\u00b7re", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses schlichte Spiel der Leier", "tokens": ["Die\u00b7ses", "schlich\u00b7te", "Spiel", "der", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruft dich auf zum Wiederklange.", "tokens": ["Ruft", "dich", "auf", "zum", "Wie\u00b7der\u00b7klan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei's da\u00df in gewicht'gem Ernste", "tokens": ["Sei's", "da\u00df", "in", "ge\u00b7wicht'\u00b7gem", "Erns\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du ein Dichterwort mir sagest,", "tokens": ["Du", "ein", "Dich\u00b7ter\u00b7wort", "mir", "sa\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder da\u00df die Fr\u00f6hlichkeit", "tokens": ["O\u00b7der", "da\u00df", "die", "Fr\u00f6h\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieses Himmels dich beseele!", "tokens": ["Die\u00b7ses", "Him\u00b7mels", "dich", "be\u00b7see\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar verschwieg'ne Zauberh\u00fcter", "tokens": ["Zwar", "ver\u00b7schwieg'\u00b7ne", "Zau\u00b7ber\u00b7h\u00fc\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn an deines Herzens Pforten,", "tokens": ["Stehn", "an", "dei\u00b7nes", "Her\u00b7zens", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nur wen'gen l\u00e4\u00dft du's \u00f6ffnen,", "tokens": ["Und", "nur", "wen'\u00b7gen", "l\u00e4\u00dft", "du's", "\u00f6ff\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wen'ge schauen in die Kammern,", "tokens": ["Wen'\u00b7ge", "schau\u00b7en", "in", "die", "Kam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo in flammendem Gebl\u00fcte,", "tokens": ["Wo", "in", "flam\u00b7men\u00b7dem", "Ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00f6pfrischer Natur geheiligt,", "tokens": ["Sch\u00f6pf\u00b7ri\u00b7scher", "Na\u00b7tur", "ge\u00b7hei\u00b7ligt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich des Goldes Lieblichkeit", "tokens": ["Sich", "des", "Gol\u00b7des", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus der Erde Schoo\u00df gerungen.", "tokens": ["Aus", "der", "Er\u00b7de", "Schoo\u00df", "ge\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch beim duftigen Elysium", "tokens": ["Doch", "beim", "duf\u00b7ti\u00b7gen", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Des Gianicolo, den s\u00fc\u00dfen", "tokens": ["Des", "Gi\u00b7a\u00b7ni\u00b7co\u00b7lo", ",", "den", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Veilchenauen vom Pamfili,", "tokens": ["Veil\u00b7chen\u00b7au\u00b7en", "vom", "Pam\u00b7fi\u00b7li", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NE", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Bei des Pantheons Gew\u00f6lbe,", "tokens": ["Bei", "des", "Pan\u00b7the\u00b7ons", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bei des Forums Tempel-S\u00e4ulen,", "tokens": ["Bei", "des", "Fo\u00b7rums", "Tem\u00b7pel\u00b7S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja bei Roms Titanenbilde,", "tokens": ["Ja", "bei", "Roms", "Ti\u00b7ta\u00b7nen\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn's in holder Rosengluth", "tokens": ["Wenn's", "in", "hol\u00b7der", "Ro\u00b7sen\u00b7gluth"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unaussprechlich herrlich leuchtet,", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7lich", "herr\u00b7lich", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oder kann's dich mehr bezaubern,", "tokens": ["O\u00b7der", "kann's", "dich", "mehr", "be\u00b7zau\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei Parthenopes Gestade,", "tokens": ["Bei", "Par\u00b7the\u00b7no\u00b7pes", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Bei den Felsen von Sorrento,", "tokens": ["Bei", "den", "Fel\u00b7sen", "von", "Sor\u00b7ren\u00b7to", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Bei der Einsamkeit von Capri,", "tokens": ["Bei", "der", "Ein\u00b7sam\u00b7keit", "von", "Cap\u00b7ri", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bei dem Anblick all' des Landes,", "tokens": ["Bei", "dem", "An\u00b7blick", "all'", "des", "Lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "All' des Meeres auf dem Gipfel", "tokens": ["All'", "des", "Mee\u00b7res", "auf", "dem", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Des Vulkans beschw\u00f6r' ich dich,", "tokens": ["Des", "Vul\u00b7kans", "be\u00b7schw\u00f6r'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Mir ein Gegenlied zu singen.", "tokens": ["Mir", "ein", "Ge\u00b7gen\u00b7lied", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gern verwehe dann das meine,", "tokens": ["Gern", "ver\u00b7we\u00b7he", "dann", "das", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist's ja doch nur leicht und munter,", "tokens": ["Ist's", "ja", "doch", "nur", "leicht", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie's die V\u00f6gel in Hesperien", "tokens": ["Wie's", "die", "V\u00f6\u00b7gel", "in", "Hes\u00b7pe\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "NE"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Auch an Wintermorgen singen.", "tokens": ["Auch", "an", "Win\u00b7ter\u00b7mor\u00b7gen", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Entz\u00fccken aber will ich", "tokens": ["Mit", "Ent\u00b7z\u00fc\u00b7cken", "a\u00b7ber", "will", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VMFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann der Nachtigall verstummen,", "tokens": ["Dann", "der", "Nach\u00b7ti\u00b7gall", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im tiefsten Lorbeerhain", "tokens": ["Und", "im", "tiefs\u00b7ten", "Lor\u00b7beer\u00b7hain"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihren sel'gen T\u00f6nen lauschen.", "tokens": ["Ih\u00b7ren", "sel'\u00b7gen", "T\u00f6\u00b7nen", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}