{"textgrid.poem.26849": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du nicht das L\u00e4mmlein h\u00fcten?", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du nicht das L\u00e4mmlein h\u00fcten?", "tokens": ["Willst", "du", "nicht", "das", "L\u00e4mm\u00b7lein", "h\u00fc\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4mmlein ist so fromm und sanft,", "tokens": ["L\u00e4mm\u00b7lein", "ist", "so", "fromm", "und", "sanft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "N\u00e4hrt sich von des Grases Bl\u00fcten,", "tokens": ["N\u00e4hrt", "sich", "von", "des", "Gra\u00b7ses", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielend an des Baches Ranft.", "tokens": ["Spie\u00b7lend", "an", "des", "Ba\u00b7ches", "Ranft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbmutter, Mutter, la\u00df mich gehen,", "tokens": ["\u00bb", "mut\u00b7ter", ",", "Mut\u00b7ter", ",", "la\u00df", "mich", "ge\u00b7hen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jagen nach des Berges H\u00f6hen!\u00ab", "tokens": ["Ja\u00b7gen", "nach", "des", "Ber\u00b7ges", "H\u00f6\u00b7hen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Willst du nicht die Herde locken", "tokens": ["Willst", "du", "nicht", "die", "Her\u00b7de", "lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit des Hornes munterm Klang?", "tokens": ["Mit", "des", "Hor\u00b7nes", "mun\u00b7term", "Klang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblich t\u00f6nt der Schall der Glocken", "tokens": ["Lieb\u00b7lich", "t\u00f6nt", "der", "Schall", "der", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Waldes Lustgesang.", "tokens": ["In", "des", "Wal\u00b7des", "Lust\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbmutter, Mutter, la\u00df mich gehen,", "tokens": ["\u00bb", "mut\u00b7ter", ",", "Mut\u00b7ter", ",", "la\u00df", "mich", "ge\u00b7hen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schweifen auf den wilden H\u00f6hen!\u00ab", "tokens": ["Schwei\u00b7fen", "auf", "den", "wil\u00b7den", "H\u00f6\u00b7hen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Willst du nicht der Bl\u00fcmlein warten,", "tokens": ["Willst", "du", "nicht", "der", "Bl\u00fcm\u00b7lein", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die im Beete freundlich stehn?", "tokens": ["Die", "im", "Bee\u00b7te", "freund\u00b7lich", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drau\u00dfen ladet dich kein Garten,", "tokens": ["Drau\u00b7\u00dfen", "la\u00b7det", "dich", "kein", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wild ists auf den wilden H\u00f6hn!", "tokens": ["Wild", "ists", "auf", "den", "wil\u00b7den", "H\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbla\u00df die Bl\u00fcmlein, la\u00df sie bl\u00fchen!", "tokens": ["\u00bb", "la\u00df", "die", "Bl\u00fcm\u00b7lein", ",", "la\u00df", "sie", "bl\u00fc\u00b7hen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "$,", "VVIMP", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mutter, Mutter, la\u00df mich ziehen!\u00ab", "tokens": ["Mut\u00b7ter", ",", "Mut\u00b7ter", ",", "la\u00df", "mich", "zie\u00b7hen", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVIMP", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und der Knabe ging zu jagen,", "tokens": ["Und", "der", "Kna\u00b7be", "ging", "zu", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es treibt und rei\u00dft ihn fort,", "tokens": ["Und", "es", "treibt", "und", "rei\u00dft", "ihn", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rastlos fort mit blindem Wagen", "tokens": ["Rast\u00b7los", "fort", "mit", "blin\u00b7dem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Berges finstern Ort,", "tokens": ["An", "des", "Ber\u00b7ges", "fins\u00b7tern", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vor ihm her mit Windesschnelle", "tokens": ["Vor", "ihm", "her", "mit", "Win\u00b7des\u00b7schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flieht die zitternde Gazelle.", "tokens": ["Flieht", "die", "zit\u00b7tern\u00b7de", "Ga\u00b7zel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Auf der Felsen nackte Rippen", "tokens": ["Auf", "der", "Fel\u00b7sen", "nack\u00b7te", "Rip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klettert sie mit leichtem Schwung,", "tokens": ["Klet\u00b7tert", "sie", "mit", "leich\u00b7tem", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den Ri\u00df geborstner Klippen", "tokens": ["Durch", "den", "Ri\u00df", "ge\u00b7borst\u00b7ner", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gt sie der gewagte Sprung,", "tokens": ["Tr\u00e4gt", "sie", "der", "ge\u00b7wag\u00b7te", "Sprung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber hinter ihr verwogen", "tokens": ["A\u00b7ber", "hin\u00b7ter", "ihr", "ver\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Folgt er mit dem Todesbogen.", "tokens": ["Folgt", "er", "mit", "dem", "To\u00b7des\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jetzo auf den schroffen Zinken", "tokens": ["Jet\u00b7zo", "auf", "den", "schrof\u00b7fen", "Zin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4ngt sie, auf dem h\u00f6chsten Grat,", "tokens": ["H\u00e4ngt", "sie", ",", "auf", "dem", "h\u00f6chs\u00b7ten", "Grat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Felsen j\u00e4h versinken", "tokens": ["Wo", "die", "Fel\u00b7sen", "j\u00e4h", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verschwunden ist der Pfad.", "tokens": ["Und", "ver\u00b7schwun\u00b7den", "ist", "der", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter sich die steile H\u00f6he,", "tokens": ["Un\u00b7ter", "sich", "die", "stei\u00b7le", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hinter sich des Feindes N\u00e4he.", "tokens": ["Hin\u00b7ter", "sich", "des", "Fein\u00b7des", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit des Jammers stummen Blicken", "tokens": ["Mit", "des", "Jam\u00b7mers", "stum\u00b7men", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fleht sie zu dem harten Mann,", "tokens": ["Fleht", "sie", "zu", "dem", "har\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fleht umsonst, denn loszudr\u00fccken", "tokens": ["Fleht", "um\u00b7sonst", ",", "denn", "los\u00b7zu\u00b7dr\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Legt er schon den Bogen an.", "tokens": ["Legt", "er", "schon", "den", "Bo\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pl\u00f6tzlich aus der Felsenspalte", "tokens": ["Pl\u00f6tz\u00b7lich", "aus", "der", "Fel\u00b7sen\u00b7spal\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tritt der Geist, der Bergesalte.", "tokens": ["Tritt", "der", "Geist", ",", "der", "Ber\u00b7ge\u00b7sal\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und mit seinen G\u00f6tterh\u00e4nden", "tokens": ["Und", "mit", "sei\u00b7nen", "G\u00f6t\u00b7ter\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fctzt er das gequ\u00e4lte Tier.", "tokens": ["Sch\u00fctzt", "er", "das", "ge\u00b7qu\u00e4l\u00b7te", "Tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbmu\u00dft du Tod und Jammer senden\u00ab,", "tokens": ["\u00bb", "mu\u00dft", "du", "Tod", "und", "Jam\u00b7mer", "sen\u00b7den", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruft er, \u00bbbis herauf zu mir?", "tokens": ["Ruft", "er", ",", "\u00bb", "bis", "her\u00b7auf", "zu", "mir", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Raum f\u00fcr alle hat die Erde,", "tokens": ["Raum", "f\u00fcr", "al\u00b7le", "hat", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was verfolgst du meine Herde?\u00ab", "tokens": ["Was", "ver\u00b7folgst", "du", "mei\u00b7ne", "Her\u00b7de", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}