{"dta.poem.23659": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Denn ein K\u00fcnstler, was gew\u00f6nn' er,", "tokens": ["Denn", "ein", "K\u00fcnst\u00b7ler", ",", "was", "ge\u00b7w\u00f6nn'", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4tt' er nicht auch seine G\u00f6nner?", "tokens": ["H\u00e4tt'", "er", "nicht", "auch", "sei\u00b7ne", "G\u00f6n\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der K\u00fcnstler mu\u00df auf der Erde leben,", "tokens": ["Der", "K\u00fcnst\u00b7ler", "mu\u00df", "auf", "der", "Er\u00b7de", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch ist sein ganzes Ringen und Streben", "tokens": ["Doch", "ist", "sein", "gan\u00b7zes", "Rin\u00b7gen", "und", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Euch auf der Erde den Himmel zu geben.", "tokens": ["Euch", "auf", "der", "Er\u00b7de", "den", "Him\u00b7mel", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Er m\u00f6chte lieber im Himmel schweben,", "tokens": ["Er", "m\u00f6ch\u00b7te", "lie\u00b7ber", "im", "Him\u00b7mel", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als unten an der Erde kleben \u2014", "tokens": ["Als", "un\u00b7ten", "an", "der", "Er\u00b7de", "kle\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch mu\u00df er nun mal auf der Erde leben.", "tokens": ["Doch", "mu\u00df", "er", "nun", "mal", "auf", "der", "Er\u00b7de", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn's euch nun freut, wie der K\u00fcnstler waltet und schaltet,", "tokens": ["Wenn's", "euch", "nun", "freut", ",", "wie", "der", "K\u00fcnst\u00b7ler", "wal\u00b7tet", "und", "schal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wenn euch freut was er in Worten und T\u00f6nen entfaltet,", "tokens": ["Wenn", "euch", "freut", "was", "er", "in", "Wor\u00b7ten", "und", "T\u00f6\u00b7nen", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PWS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Und zu seelenvollen Bildern gestaltet,", "tokens": ["Und", "zu", "see\u00b7len\u00b7vol\u00b7len", "Bil\u00b7dern", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "So m\u00f6gt ihr eure Freude zur Erscheinung bringen,", "tokens": ["So", "m\u00f6gt", "ihr", "eu\u00b7re", "Freu\u00b7de", "zur", "Er\u00b7schei\u00b7nung", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und lasst anmuthiglich eure Meinung klingen,", "tokens": ["Und", "lasst", "an\u00b7mut\u00b7hig\u00b7lich", "eu\u00b7re", "Mei\u00b7nung", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Und vergleicht nicht erst mit der Bilderei des Thalers", "tokens": ["Und", "ver\u00b7gleicht", "nicht", "erst", "mit", "der", "Bil\u00b7de\u00b7rei", "des", "Tha\u00b7lers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "die Schilderei des Malers", "tokens": ["die", "Schil\u00b7de\u00b7rei", "des", "Ma\u00b7lers"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und mit der Moneten Singsang", "tokens": ["Und", "mit", "der", "Mo\u00b7ne\u00b7ten", "Sings\u00b7ang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.9": {"text": "der Poeten Klingklang!", "tokens": ["der", "Po\u00b7et\u00b7en", "Kling\u00b7klang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Denn das ist mir nun einmal klar", "tokens": ["Denn", "das", "ist", "mir", "nun", "ein\u00b7mal", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Seit manchem Jahr und bleibt auch wahr", "tokens": ["Seit", "man\u00b7chem", "Jahr", "und", "bleibt", "auch", "wahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Heut' und immerdar:", "tokens": ["Heut'", "und", "im\u00b7mer\u00b7dar", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Alle wahre Kunst", "tokens": ["Al\u00b7le", "wah\u00b7re", "Kunst"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Ohne wahre Gunst", "tokens": ["Oh\u00b7ne", "wah\u00b7re", "Gunst"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "M\u00fcht sich f\u00fcrwahr umsunst.", "tokens": ["M\u00fcht", "sich", "f\u00fcr\u00b7wahr", "um\u00b7sunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "Drum lasst uns alle das Glas erheben:", "tokens": ["Drum", "lasst", "uns", "al\u00b7le", "das", "Glas", "er\u00b7he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "sie sollen leben!", "tokens": ["sie", "sol\u00b7len", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}