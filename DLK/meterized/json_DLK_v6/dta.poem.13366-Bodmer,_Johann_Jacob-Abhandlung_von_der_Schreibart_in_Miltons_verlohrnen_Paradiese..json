{"dta.poem.13366": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Abhandlung  \n von der Schreibart in Miltons  \n verlohrnen Paradiese.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du schwinge selbst vielmehr des Geistes Kr\u00e4fte los,", "tokens": ["Du", "schwin\u00b7ge", "selbst", "viel\u00b7mehr", "des", "Geis\u00b7tes", "Kr\u00e4f\u00b7te", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht ewig f\u00fcr die Zeit, nicht f\u00fcr die Erde gro\u00df,", "tokens": ["Nicht", "e\u00b7wig", "f\u00fcr", "die", "Zeit", ",", "nicht", "f\u00fcr", "die", "Er\u00b7de", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und h\u00f6hrer Dinge werth.", "tokens": ["Und", "h\u00f6h\u00b7rer", "Din\u00b7ge", "werth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}