{"textgrid.poem.45899": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von K\u00f6llen war ein Edelknecht", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von K\u00f6llen war ein Edelknecht", "tokens": ["Von", "K\u00f6l\u00b7len", "war", "ein", "E\u00b7del\u00b7knecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um Kundschaft ausgegangen", "tokens": ["Um", "Kund\u00b7schaft", "aus\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Vater lag durch Engelbrecht", "tokens": ["Sein", "Va\u00b7ter", "lag", "durch", "En\u00b7gel\u00b7brecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Bischof hart gefangen.", "tokens": ["Den", "Bi\u00b7schof", "hart", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er sucht durchs Land wohl manchen Tag", "tokens": ["Er", "sucht", "durchs", "Land", "wohl", "man\u00b7chen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sucht in schweren Sorgen,", "tokens": ["Er", "sucht", "in", "schwe\u00b7ren", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Liebchen ihm im Sinne lag,", "tokens": ["Sein", "Lieb\u00b7chen", "ihm", "im", "Sin\u00b7ne", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der h\u00e4tt' er es verborgen.", "tokens": ["Der", "h\u00e4tt'", "er", "es", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gar traurig er am Bronnen lag", "tokens": ["Gar", "trau\u00b7rig", "er", "am", "Bron\u00b7nen", "lag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Busch und gr\u00fcnen Hecken,", "tokens": ["In", "Busch", "und", "gr\u00fc\u00b7nen", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da h\u00f6rt er schallen Hufesschlag,", "tokens": ["Da", "h\u00f6rt", "er", "schal\u00b7len", "Hu\u00b7fes\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eilt sich zu verstecken.", "tokens": ["Und", "eilt", "sich", "zu", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er sah wohl einen frohen Mann", "tokens": ["Er", "sah", "wohl", "ei\u00b7nen", "fro\u00b7hen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Born sein R\u00f6\u00dflein lenken", "tokens": ["Zum", "Born", "sein", "R\u00f6\u00df\u00b7lein", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein andrer ritt betr\u00fcbt heran,", "tokens": ["Ein", "an\u00b7drer", "ritt", "be\u00b7tr\u00fcbt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der t\u00e4t die Augen senken.", "tokens": ["Der", "t\u00e4t", "die", "Au\u00b7gen", "sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O froher Mann der eine sprach", "tokens": ["O", "fro\u00b7her", "Mann", "der", "ei\u00b7ne", "sprach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "ART", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mag dich nur erfreuen", "tokens": ["Was", "mag", "dich", "nur", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Betr\u00fcbter Mann der Frohe sprach", "tokens": ["Be\u00b7tr\u00fcb\u00b7ter", "Mann", "der", "Fro\u00b7he", "sprach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott woll' dir Trost verleihen.", "tokens": ["Gott", "woll'", "dir", "Trost", "ver\u00b7lei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Herr Gottschalk der getreue Mann", "tokens": ["Herr", "Gott\u00b7schalk", "der", "ge\u00b7treu\u00b7e", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Geht frei in unsren Landen", "tokens": ["Geht", "frei", "in", "un\u00b7sren", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch wunderbare H\u00fclf' entrann", "tokens": ["Durch", "wun\u00b7der\u00b7ba\u00b7re", "H\u00fcl\u00b7f'", "ent\u00b7rann"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Er aus des Bischofs Banden.", "tokens": ["Er", "aus", "des", "Bi\u00b7schofs", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er hatte eine kleine Maus", "tokens": ["Er", "hat\u00b7te", "ei\u00b7ne", "klei\u00b7ne", "Maus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Kerker zahm gezogen,", "tokens": ["Im", "Ker\u00b7ker", "zahm", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie gieng als Gastfreund ein und aus,", "tokens": ["Sie", "gieng", "als", "Gast\u00b7freund", "ein", "und", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und war dem Herrn gewogen.", "tokens": ["Und", "war", "dem", "Herrn", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die harte Rinde, die sie nagt", "tokens": ["Die", "har\u00b7te", "Rin\u00b7de", ",", "die", "sie", "nagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4nkt er im Lampen\u00f6le", "tokens": ["Tr\u00e4nkt", "er", "im", "Lam\u00b7pe\u00b7n\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und wenn er Lebewohl gesagt", "tokens": ["Und", "wenn", "er", "Le\u00b7be\u00b7wohl", "ge\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kehrt sie nach ihrer H\u00f6hle.", "tokens": ["Kehrt", "sie", "nach", "ih\u00b7rer", "H\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und wenn er traurig niederkniet", "tokens": ["Und", "wenn", "er", "trau\u00b7rig", "nie\u00b7der\u00b7kniet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt den Morgensegen", "tokens": ["Und", "singt", "den", "Mor\u00b7gen\u00b7se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So t\u00f6nt ihm auch ihr frommes Lied", "tokens": ["So", "t\u00f6nt", "ihm", "auch", "ihr", "from\u00b7mes", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihrem Haus entgegen.", "tokens": ["Aus", "ih\u00b7rem", "Haus", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch einst sein treuer Freund entlief", "tokens": ["Doch", "einst", "sein", "treu\u00b7er", "Freund", "ent\u00b7lief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollte nicht mehr kehren", "tokens": ["Und", "woll\u00b7te", "nicht", "mehr", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie Herr Gottschalk lockt' und rief,", "tokens": ["Und", "wie", "Herr", "Gott\u00b7schalk", "lockt'", "und", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "NE", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das M\u00e4uslein wollt' nicht h\u00f6ren.", "tokens": ["Das", "M\u00e4us\u00b7lein", "wollt'", "nicht", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Bei Mittagsbrot und Abendbrot", "tokens": ["Bei", "Mit\u00b7tags\u00b7brot", "und", "A\u00b7bend\u00b7brot"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blieb unbenagt die Rinde:", "tokens": ["Blieb", "un\u00b7be\u00b7nagt", "die", "Rin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er grub nach ihr, ob mausetot,", "tokens": ["Er", "grub", "nach", "ihr", ",", "ob", "mau\u00b7se\u00b7tot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KOUS", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wohl die Treue finde.", "tokens": ["Er", "wohl", "die", "Treu\u00b7e", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und in der Erde eingescharrt", "tokens": ["Und", "in", "der", "Er\u00b7de", "ein\u00b7ge\u00b7scharrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fand Mei\u00dfel er und Feilen,", "tokens": ["Fand", "Mei\u00b7\u00dfel", "er", "und", "Fei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "KON", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Womit er seine Bande hart", "tokens": ["Wo\u00b7mit", "er", "sei\u00b7ne", "Ban\u00b7de", "hart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar leichtlich konnt' zerteilen.", "tokens": ["Gar", "leicht\u00b7lich", "konnt'", "zer\u00b7tei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Nun geht er frei, der fromme Mann", "tokens": ["Nun", "geht", "er", "frei", ",", "der", "from\u00b7me", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wird sein Schwert bald r\u00fchren,", "tokens": ["Und", "wird", "sein", "Schwert", "bald", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihm schlie\u00dfen sich die Freunde an,", "tokens": ["Ihm", "schlie\u00b7\u00dfen", "sich", "die", "Freun\u00b7de", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das soll der Bischof sp\u00fcren.", "tokens": ["Das", "soll", "der", "Bi\u00b7schof", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der andre sprach mein Schwesterlein,", "tokens": ["Der", "and\u00b7re", "sprach", "mein", "Schwes\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das liegt gar hart gefangen", "tokens": ["Das", "liegt", "gar", "hart", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und selbst das treue M\u00e4uslein dein", "tokens": ["Und", "selbst", "das", "treu\u00b7e", "M\u00e4us\u00b7lein", "dein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6nnt' nicht zu ihr gelangen.", "tokens": ["K\u00f6nnt'", "nicht", "zu", "ihr", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Der falsche Knecht, die Liebe brach", "tokens": ["Der", "fal\u00b7sche", "Knecht", ",", "die", "Lie\u00b7be", "brach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihres Herzens Kammer,", "tokens": ["In", "ih\u00b7res", "Her\u00b7zens", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihm stiegen die Gesellen nach", "tokens": ["Ihm", "stie\u00b7gen", "die", "Ge\u00b7sel\u00b7len", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Leid und b\u00f6ser Jammer.", "tokens": ["Das", "Leid", "und", "b\u00f6\u00b7ser", "Jam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter":