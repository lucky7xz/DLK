{"textgrid.poem.65283": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Monds\u00fcchtige", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bleicher Mann da droben,", "tokens": ["Du", "blei\u00b7cher", "Mann", "da", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Siehst wieder so m\u00fcrrisch aus:", "tokens": ["Siehst", "wie\u00b7der", "so", "m\u00fcr\u00b7risch", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist wohl recht unzufrieden", "tokens": ["Bist", "wohl", "recht", "un\u00b7zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit deinem luftigen Haus?", "tokens": ["Mit", "dei\u00b7nem", "luf\u00b7ti\u00b7gen", "Haus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "H\u00f6r', Freund, wir wollen tauschen:", "tokens": ["H\u00f6r'", ",", "Freund", ",", "wir", "wol\u00b7len", "tau\u00b7schen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich geh' und r\u00e4ume dir", "tokens": ["Ich", "geh'", "und", "r\u00e4u\u00b7me", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr diesen k\u00fchlen Abend", "tokens": ["F\u00fcr", "die\u00b7sen", "k\u00fch\u00b7len", "A\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein warmes Lager hier.", "tokens": ["Mein", "war\u00b7mes", "La\u00b7ger", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Daf\u00fcr sollst du mich heben", "tokens": ["Da\u00b7f\u00fcr", "sollst", "du", "mich", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "In deinen Mond hinauf,", "tokens": ["In", "dei\u00b7nen", "Mond", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich mit ihm wandeln lassen", "tokens": ["Mich", "mit", "ihm", "wan\u00b7deln", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den hellen Himmelslauf.", "tokens": ["Den", "hel\u00b7len", "Him\u00b7mels\u00b7lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Will auch auf deiner Warte", "tokens": ["Will", "auch", "auf", "dei\u00b7ner", "War\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ganz m\u00e4uschenstille stehn,", "tokens": ["Ganz", "m\u00e4u\u00b7schen\u00b7stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nach der b\u00f6sen Erde", "tokens": ["Und", "nach", "der", "b\u00f6\u00b7sen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht viel herunter sehn.", "tokens": ["Nicht", "viel", "her\u00b7un\u00b7ter", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Will keinen Dieb verrathen,", "tokens": ["Will", "kei\u00b7nen", "Dieb", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will st\u00f6ren kein liebendes Paar:", "tokens": ["Will", "st\u00f6\u00b7ren", "kein", "lie\u00b7ben\u00b7des", "Paar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nur Eines m\u00f6cht' ich sehen,", "tokens": ["Nur", "Ei\u00b7nes", "m\u00f6cht'", "ich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und das recht hell und klar.", "tokens": ["Und", "das", "recht", "hell", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dir, Mond, will ich's vertrauen:", "tokens": ["Dir", ",", "Mond", ",", "will", "ich's", "ver\u00b7trau\u00b7en", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ist die Liebste mein,", "tokens": ["Es", "ist", "die", "Liebs\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ich beschauen m\u00f6chte", "tokens": ["Die", "ich", "be\u00b7schau\u00b7en", "m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deinem goldnen Schein.", "tokens": ["In", "dei\u00b7nem", "gold\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie wohnet in der Ferne,", "tokens": ["Sie", "woh\u00b7net", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blickt oft empor zu dir:", "tokens": ["Blickt", "oft", "em\u00b7por", "zu", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du guckst im Weltget\u00fcmmel", "tokens": ["Du", "guckst", "im", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl kaum einmal nach ihr.", "tokens": ["Wohl", "kaum", "ein\u00b7mal", "nach", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Ich wollt' sie besser finden,", "tokens": ["Ich", "wollt'", "sie", "bes\u00b7ser", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich kenn' ihr Fensterlein;", "tokens": ["Ich", "kenn'", "ihr", "Fens\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch Laden, Glas und Gitter", "tokens": ["Durch", "La\u00b7den", ",", "Glas", "und", "Git\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schl\u00fcpft' ich zu ihr hinein.", "tokens": ["Schl\u00fcpft'", "ich", "zu", "ihr", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Hinein in ihre Kammer", "tokens": ["Hin\u00b7ein", "in", "ih\u00b7re", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit aller Strahlen Fluth! \u2013", "tokens": ["Mit", "al\u00b7ler", "Strah\u00b7len", "Fluth", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ist der Mond geblieben?", "tokens": ["Wo", "ist", "der", "Mond", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Himmel auf Erden ruht.", "tokens": ["Der", "Him\u00b7mel", "auf", "Er\u00b7den", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}