{"textgrid.poem.55244": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Den Originalen", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Quidam sagt: \u00bbIch bin von keiner Schule!", "tokens": ["Ein", "Qui\u00b7dam", "sagt", ":", "\u00bb", "Ich", "bin", "von", "kei\u00b7ner", "Schu\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Meister lebt, mit dem ich buhle;", "tokens": ["Kein", "Meis\u00b7ter", "lebt", ",", "mit", "dem", "ich", "buh\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch bin ich weit davon entfernt,", "tokens": ["Auch", "bin", "ich", "weit", "da\u00b7von", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich von Toten was gelernt.\u00ab", "tokens": ["Da\u00df", "ich", "von", "To\u00b7ten", "was", "ge\u00b7lernt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PWS", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das hei\u00dft, wenn ich ihn recht verstand:", "tokens": ["Das", "hei\u00dft", ",", "wenn", "ich", "ihn", "recht", "ver\u00b7stand", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin ein Narr auf eigne Hand.", "tokens": ["Ich", "bin", "ein", "Narr", "auf", "eig\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}