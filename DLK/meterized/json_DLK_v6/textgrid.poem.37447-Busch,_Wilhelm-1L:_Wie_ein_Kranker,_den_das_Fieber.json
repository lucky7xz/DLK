{"textgrid.poem.37447": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ein Kranker, den das Fieber", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Kranker, den das Fieber", "tokens": ["Wie", "ein", "Kran\u00b7ker", ",", "den", "das", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00df gemacht und aufgeregt,", "tokens": ["Hei\u00df", "ge\u00b7macht", "und", "auf\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich her\u00fcber und hin\u00fcber", "tokens": ["Sich", "her\u00b7\u00fc\u00b7ber", "und", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die andre Seite legt \u2013", "tokens": ["Auf", "die", "and\u00b7re", "Sei\u00b7te", "legt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So die Welt. Vor Ha\u00df und Hader", "tokens": ["So", "die", "Welt", ".", "Vor", "Ha\u00df", "und", "Ha\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sie niemals noch geruht.", "tokens": ["Hat", "sie", "nie\u00b7mals", "noch", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immerfort durch jede Ader", "tokens": ["Im\u00b7mer\u00b7fort", "durch", "je\u00b7de", "A\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tobt das alte S\u00fcnderblut.", "tokens": ["Tobt", "das", "al\u00b7te", "S\u00fcn\u00b7der\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}