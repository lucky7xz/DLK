{"textgrid.poem.58508": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein B\u00e4chlein reiner Sucht la\u00df nicht zerteilet werden,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein B\u00e4chlein reiner Sucht la\u00df nicht zerteilet werden,", "tokens": ["Dein", "B\u00e4ch\u00b7lein", "rei\u00b7ner", "Sucht", "la\u00df", "nicht", "zer\u00b7tei\u00b7let", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVIMP", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst hat's nicht Kraft zum Lauf, versiegt in Sand und Erden;", "tokens": ["Sonst", "hat's", "nicht", "Kraft", "zum", "Lauf", ",", "ver\u00b7siegt", "in", "Sand", "und", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "APPRART", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr Gott sei alle dein Begier,", "tokens": ["F\u00fcr", "Gott", "sei", "al\u00b7le", "dein", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann gibt er herzlich gern sich dir!", "tokens": ["Dann", "gibt", "er", "herz\u00b7lich", "gern", "sich", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "PRF", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}