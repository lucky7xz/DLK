{"dta.poem.2062": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Esel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00fclf am Zahnfleisch, st\u00e4rkt und heilt es. Endlich wird", "tokens": ["H\u00fclf", "am", "Zahn\u00b7fleisch", ",", "st\u00e4rkt", "und", "heilt", "es", ".", "End\u00b7lich", "wird"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPRART", "NN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "$.", "ADV", "VAFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "auch seine Haut,", "tokens": ["auch", "sei\u00b7ne", "Haut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Drauf zu schreiben, sehr gen\u00fctzt. Merkt denn, wie der", "tokens": ["Drauf", "zu", "schrei\u00b7ben", ",", "sehr", "ge\u00b7n\u00fctzt", ".", "Merkt", "denn", ",", "wie", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "PTKZU", "VVINF", "$,", "ADV", "VVPP", "$.", "VVFIN", "ADV", "$,", "PWAV", "ART"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Esel auch", "tokens": ["E\u00b7sel", "auch"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Uns zum Nutz formiret sey, in so mancherley Ge-", "tokens": ["Uns", "zum", "Nutz", "for\u00b7mi\u00b7ret", "sey", ",", "in", "so", "man\u00b7cher\u00b7ley", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,", "APPR", "ADV", "PIAT", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "brauch,", "tokens": ["brauch", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und, wenn ihr euch sein bedient, denket, wenn ihr bil-", "tokens": ["Und", ",", "wenn", "ihr", "euch", "sein", "be\u00b7dient", ",", "den\u00b7ket", ",", "wenn", "ihr", "bil"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "PPOSAT", "VVPP", "$,", "VVFIN", "$,", "KOUS", "PPOSAT", "TRUNC"], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "lig denket,", "tokens": ["lig", "den\u00b7ket", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Da\u00df auch er euch einen Sch\u00f6pfer zeige, der ihn euch ge-", "tokens": ["Da\u00df", "auch", "er", "euch", "ei\u00b7nen", "Sch\u00f6p\u00b7fer", "zei\u00b7ge", ",", "der", "ihn", "euch", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "schenket.", "tokens": ["schen\u00b7ket", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}