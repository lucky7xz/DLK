{"textgrid.poem.64505": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "129. Zweymal eilf Uhr", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um eilf Uhr in der Nacht k\u00f6nnt ihr zum ", "tokens": ["Um", "eilf", "Uhr", "in", "der", "Nacht", "k\u00f6nnt", "ihr", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "APPRART"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und er wird euch bey ihm zu bleiben flehn:", "tokens": ["Und", "er", "wird", "euch", "bey", "ihm", "zu", "blei\u00b7ben", "flehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Um eilf Uhr Vormittag d\u00fcrft ihr soviel nicht wagen,", "tokens": ["Um", "eilf", "Uhr", "Vor\u00b7mit\u00b7tag", "d\u00fcrft", "ihr", "so\u00b7viel", "nicht", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr m\u00f6chtet ihn sonst aus dem Bette jagen.", "tokens": ["Ihr", "m\u00f6ch\u00b7tet", "ihn", "sonst", "aus", "dem", "Bet\u00b7te", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}