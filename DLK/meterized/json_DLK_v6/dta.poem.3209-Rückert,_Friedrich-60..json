{"dta.poem.3209": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In meiner Einsamkeit da kann ich ohne Schaden,", "tokens": ["In", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", "da", "kann", "ich", "oh\u00b7ne", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wen ich am liebsten will, bei mir zu Gaste laden, \u2014", "tokens": ["Wen", "ich", "am", "liebs\u00b7ten", "will", ",", "bei", "mir", "zu", "Gas\u00b7te", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "PTKA", "ADJD", "VMFIN", "$,", "APPR", "PPER", "APPR", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht unvertr\u00e4gliche Gesellschaft so gemischt,", "tokens": ["Nicht", "un\u00b7ver\u00b7tr\u00e4g\u00b7li\u00b7che", "Ge\u00b7sell\u00b7schaft", "so", "ge\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie streitende Gericht' auf einmal aufgetischt, \u2014", "tokens": ["Wie", "strei\u00b7ten\u00b7de", "Ge\u00b7richt'", "auf", "ein\u00b7mal", "auf\u00b7ge\u00b7tischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADV", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht so unleidlicher Gesichter Schofel, Pafel,", "tokens": ["Nicht", "so", "un\u00b7leid\u00b7li\u00b7cher", "Ge\u00b7sich\u00b7ter", "Scho\u00b7fel", ",", "Pa\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJA", "NN", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Womit die E\u00dflust mir benimmt die Gastwirthstafel, \u2014", "tokens": ["Wo\u00b7mit", "die", "E\u00df\u00b7lust", "mir", "be\u00b7nimmt", "die", "Gast\u00b7wirth\u00b7sta\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht Hof- und Staatslivreen, der Uniform Unformen,", "tokens": ["Nicht", "Hof", "und", "Staats\u00b7liv\u00b7re\u00b7en", ",", "der", "U\u00b7ni\u00b7form", "Un\u00b7for\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "TRUNC", "KON", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von meinem Ideal enorm abnorme Normen; \u2014", "tokens": ["Von", "mei\u00b7nem", "I\u00b7deal", "en\u00b7orm", "ab\u00b7nor\u00b7me", "Nor\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Weisen alter Zeit, die mir vom Ruhm genannten,", "tokens": ["Die", "Wei\u00b7sen", "al\u00b7ter", "Zeit", ",", "die", "mir", "vom", "Ruhm", "ge\u00b7nann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die in L\u00e4ndern weit geahnten, unbekannten;", "tokens": ["Und", "die", "in", "L\u00e4n\u00b7dern", "weit", "ge\u00b7ahn\u00b7ten", ",", "un\u00b7be\u00b7kann\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "ADJD", "VVPP", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und alle Lieben mir und Abgeschiedenen;", "tokens": ["Und", "al\u00b7le", "Lie\u00b7ben", "mir", "und", "Ab\u00b7ge\u00b7schie\u00b7de\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wie labt das Mienenspiel mich der Zufriedenen!", "tokens": ["Wie", "labt", "das", "Mie\u00b7nen\u00b7spiel", "mich", "der", "Zu\u00b7frie\u00b7de\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Unterhaltung kreis't, die nicht in Pausen stockt,", "tokens": ["Die", "Un\u00b7ter\u00b7hal\u00b7tung", "kreis't", ",", "die", "nicht", "in", "Pau\u00b7sen", "stockt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PTKNEG", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ew'ger Fr\u00fchlingshauch aus Bl\u00fcthen Bl\u00fcthen lockt.", "tokens": ["Wie", "ew'\u00b7ger", "Fr\u00fch\u00b7lings\u00b7hauch", "aus", "Bl\u00fc\u00b7then", "Bl\u00fc\u00b7then", "lockt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sie reden nicht, was heut' der Tag zu reden beut,", "tokens": ["Sie", "re\u00b7den", "nicht", ",", "was", "heut'", "der", "Tag", "zu", "re\u00b7den", "beut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ADV", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie reden, was das Herz der Ewigkeit erfreut,", "tokens": ["Sie", "re\u00b7den", ",", "was", "das", "Herz", "der", "E\u00b7wig\u00b7keit", "er\u00b7freut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Nicht Spekulation und Aktien-Eisenbahn,", "tokens": ["Nicht", "Spe\u00b7ku\u00b7la\u00b7ti\u00b7on", "und", "Ak\u00b7ti\u00b7en\u00b7Ei\u00b7sen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Feuerversicherung, Stadtschuldentilgungsplan.", "tokens": ["Feu\u00b7er\u00b7ver\u00b7si\u00b7che\u00b7rung", ",", "Stadt\u00b7schul\u00b7den\u00b7til\u00b7gungs\u00b7plan", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.10": {"line.1": {"text": "Hoch \u00fcber Qualm und Koth, irdischem Drang und Noth,", "tokens": ["Hoch", "\u00fc\u00b7ber", "Qualm", "und", "Koth", ",", "ir\u00b7di\u00b7schem", "Drang", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Himmel geht ein Weg durch Morgenabendroth.", "tokens": ["Am", "Him\u00b7mel", "geht", "ein", "Weg", "durch", "Mor\u00b7gen\u00b7a\u00b7ben\u00b7droth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Und wann ich zugelauscht, und mit darein getauscht", "tokens": ["Und", "wann", "ich", "zu\u00b7ge\u00b7lauscht", ",", "und", "mit", "da\u00b7rein", "ge\u00b7tauscht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVPP", "$,", "KON", "APPR", "PAV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein W\u00f6rtchen, schweig' ich satt von Duft und wohlberauscht.", "tokens": ["Ein", "W\u00f6rt\u00b7chen", ",", "schweig'", "ich", "satt", "von", "Duft", "und", "wohl\u00b7be\u00b7rauscht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Und wie ich winke, gehn beiseit die frommen Sch\u00e4fchen,", "tokens": ["Und", "wie", "ich", "win\u00b7ke", ",", "gehn", "bei\u00b7seit", "die", "from\u00b7men", "Sch\u00e4f\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und geben gerne Raum mir f\u00fcr ein Mittagschl\u00e4fchen.", "tokens": ["Und", "ge\u00b7ben", "ger\u00b7ne", "Raum", "mir", "f\u00fcr", "ein", "Mit\u00b7tag\u00b7schl\u00e4f\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}