{"dta.poem.5843": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Regungen, so man in den Saphiernen H\u00f6hen", "tokens": ["Die", "Re\u00b7gun\u00b7gen", ",", "so", "man", "in", "den", "Sa\u00b7phier\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vom Anfang immerfort in grossen Kreysen sp\u00fchrt,", "tokens": ["Vom", "An\u00b7fang", "im\u00b7mer\u00b7fort", "in", "gros\u00b7sen", "Krey\u00b7sen", "sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sind auf derselben Art im Firmament vollf\u00fchrt,", "tokens": ["Sind", "auf", "der\u00b7sel\u00b7ben", "Art", "im", "Fir\u00b7ma\u00b7ment", "voll\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als die, so wir allhier vor Augen t\u00e4glich sehen.", "tokens": ["Als", "die", ",", "so", "wir", "all\u00b7hier", "vor", "Au\u00b7gen", "t\u00e4g\u00b7lich", "se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PPER", "ADV", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.i