{"textgrid.poem.45117": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Schleswig-Holsteinische Frage", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt mit Macht gesungen,", "tokens": ["Ihr", "habt", "mit", "Macht", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Europa hat gepfiffen.", "tokens": ["Eu\u00b7ro\u00b7pa", "hat", "ge\u00b7pfif\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man hat noch nicht errungen,", "tokens": ["Man", "hat", "noch", "nicht", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was h\u00f6chstens man begriffen.", "tokens": ["Was", "h\u00f6chs\u00b7tens", "man", "be\u00b7grif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}