{"textgrid.poem.38864": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Wi\u00dfbegierge k\u00fcnstlich Gl\u00e4ser schleifen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Wi\u00dfbegierge k\u00fcnstlich Gl\u00e4ser schleifen,", "tokens": ["Wie", "Wi\u00df\u00b7be\u00b7gier\u00b7ge", "k\u00fcnst\u00b7lich", "Gl\u00e4\u00b7ser", "schlei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich Sonne, Mond und Sterne nah zu bringen,", "tokens": ["Sich", "Son\u00b7ne", ",", "Mond", "und", "Ster\u00b7ne", "nah", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "$,", "NN", "KON", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00fchn in ein weit entlegnes Land zu dringen,", "tokens": ["K\u00fchn", "in", "ein", "weit", "ent\u00b7leg\u00b7nes", "Land", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJD", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verwegen durch das Firmament zu streifen;", "tokens": ["Ver\u00b7we\u00b7gen", "durch", "das", "Fir\u00b7ma\u00b7ment", "zu", "strei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kann denn so ferne Frucht dem Forschen reifen?", "tokens": ["Kann", "denn", "so", "fer\u00b7ne", "Frucht", "dem", "For\u00b7schen", "rei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihnen, Sterblichen, es darf gelingen,", "tokens": ["Da\u00df", "ih\u00b7nen", ",", "Sterb\u00b7li\u00b7chen", ",", "es", "darf", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich stolzen Flugs zum Himmel aufzuschwingen,", "tokens": ["Sich", "stol\u00b7zen", "Flugs", "zum", "Him\u00b7mel", "auf\u00b7zu\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Lauf der ew'gen Lichter zu ergreifen?", "tokens": ["Den", "Lauf", "der", "ew'\u00b7gen", "Lich\u00b7ter", "zu", "er\u00b7grei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So dient, mein Heinrich, mir dein Grab zum Rohre,", "tokens": ["So", "dient", ",", "mein", "Hein\u00b7rich", ",", "mir", "dein", "Grab", "zum", "Roh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NE", "$,", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Erde hindert nicht den muthgen Seher,", "tokens": ["Die", "Er\u00b7de", "hin\u00b7dert", "nicht", "den", "muth\u00b7gen", "Se\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht das tr\u00fcb' plutonische Gew\u00e4sser;", "tokens": ["Und", "nicht", "das", "tr\u00fcb'", "plu\u00b7to\u00b7ni\u00b7sche", "Ge\u00b7w\u00e4s\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Seitdem du eingingst durch die dunkeln Thore,", "tokens": ["Seit\u00b7dem", "du", "ein\u00b7gingst", "durch", "die", "dun\u00b7keln", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "F\u00fchl' ich durch Erd' und Grab und Tod dich n\u00e4her,", "tokens": ["F\u00fchl'", "ich", "durch", "Erd'", "und", "Grab", "und", "Tod", "dich", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN", "KON", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie zeigen heller deinen Geist und gr\u00f6\u00dfer.", "tokens": ["Sie", "zei\u00b7gen", "hel\u00b7ler", "dei\u00b7nen", "Geist", "und", "gr\u00f6\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}