{"textgrid.poem.32895": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trotzt auf den Vorzug nur, entfernt Nationen!", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trotzt auf den Vorzug nur, entfernt Nationen!", "tokens": ["Trotzt", "auf", "den", "Vor\u00b7zug", "nur", ",", "ent\u00b7fernt", "Na\u00b7ti\u00b7o\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$,", "ADJD", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nein, Deutschlands Klugheit lob ich mir:", "tokens": ["Nein", ",", "Deutschlands", "Klug\u00b7heit", "lob", "ich", "mir", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die in S\u00fcd und West, in Nord und Osten wohnen,", "tokens": ["Und", "die", "in", "S\u00fcd", "und", "West", ",", "in", "Nord", "und", "Os\u00b7ten", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "KON", "NN", "$,", "APPR", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind halb so weise nicht, als wir.", "tokens": ["Sind", "halb", "so", "wei\u00b7se", "nicht", ",", "als", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der leere Franzmann pfeift, und schneidet Capriolen;", "tokens": ["Der", "lee\u00b7re", "Franz\u00b7mann", "pfeift", ",", "und", "schnei\u00b7det", "Cap\u00b7ri\u00b7o\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der r\u00f6mische Castrate singt;", "tokens": ["Der", "r\u00f6\u00b7mi\u00b7sche", "Cas\u00b7tra\u00b7te", "singt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Britte l\u00e4\u00dft am Strang sich Miltons Teufel holen,", "tokens": ["Der", "Brit\u00b7te", "l\u00e4\u00dft", "am", "Strang", "sich", "Mil\u00b7tons", "Teu\u00b7fel", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PRF", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Deutsche, was thut der? er trinkt!", "tokens": ["Der", "Deut\u00b7sche", ",", "was", "thut", "der", "?", "er", "trinkt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "VVFIN", "ART", "$.", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}