{"dta.poem.84": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als die sch\u00f6ne T\u00fcrckin/ da er mit ihr bekand  \n worden/ kurtz darauf wegzog.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wil meine Sonne schon die schwartzen Schatten k\u00fcssen/", "tokens": ["Wil", "mei\u00b7ne", "Son\u00b7ne", "schon", "die", "schwart\u00b7zen", "Schat\u00b7ten", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und h\u00fclt sie ihren Glantz in dunckle Wolcken ein?", "tokens": ["Und", "h\u00fclt", "sie", "ih\u00b7ren", "Glantz", "in", "dunck\u00b7le", "Wol\u00b7cken", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mu\u00df die verha\u00dfte Nacht mit ihren Finsternissen/", "tokens": ["Mu\u00df", "die", "ver\u00b7ha\u00df\u00b7te", "Nacht", "mit", "ih\u00b7ren", "Fins\u00b7ter\u00b7nis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf einem kurtzen Tag so bald verhanden seyn?", "tokens": ["Auf", "ei\u00b7nem", "kurt\u00b7zen", "Tag", "so", "bald", "ver\u00b7han\u00b7den", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja! ", "tokens": ["Ja", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Die schwartze Finsterni\u00df verdunckelt seine Lufft/", "tokens": ["Die", "schwart\u00b7ze", "Fins\u00b7ter\u00b7ni\u00df", "ver\u00b7dun\u00b7ckelt", "sei\u00b7ne", "Lufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Sonne meiner Lust/ die ziehet um die Erden/", "tokens": ["Die", "Son\u00b7ne", "mei\u00b7ner", "Lust", "/", "die", "zie\u00b7het", "um", "die", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und durch entwandtem Schein st\u00f6st sie mich in die Grufft.", "tokens": ["Und", "durch", "ent\u00b7wand\u00b7tem", "Schein", "st\u00f6st", "sie", "mich", "in", "die", "Grufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sehe mit Verdru\u00df den Freuden-Stern versincken/", "tokens": ["Ich", "se\u00b7he", "mit", "Ver\u00b7dru\u00df", "den", "Freu\u00b7den\u00b7S\u00b7tern", "ver\u00b7sin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Die kummer-volle Nacht mit Traum-Gespensten dreut/", "tokens": ["Die", "kum\u00b7mer\u00b7vol\u00b7le", "Nacht", "mit", "Traum\u00b7Ge\u00b7spens\u00b7ten", "dreut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es l\u00e4st ein Jrwisch-Licht der Traurigkeit sich blincken:", "tokens": ["Es", "l\u00e4st", "ein", "Jr\u00b7wischLicht", "der", "Trau\u00b7rig\u00b7keit", "sich", "blin\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und ein ", "tokens": ["Und", "ein"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Es k\u00fc\u00dft mit ", "tokens": ["Es", "k\u00fc\u00dft", "mit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Ich mu\u00df wie ", "tokens": ["Ich", "mu\u00df", "wie"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Nun meine Sonne fort/ ich sterbend untergeh.", "tokens": ["Nun", "mei\u00b7ne", "Son\u00b7ne", "fort", "/", "ich", "ster\u00b7bend", "un\u00b7ter\u00b7geh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$(", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Barlottens Abzug raubt der Seelen ihre Freude/", "tokens": ["Bar\u00b7lot\u00b7tens", "Ab\u00b7zug", "raubt", "der", "See\u00b7len", "ih\u00b7re", "Freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mein klahrer Himmel wird zu einer Trauer-Lufft/", "tokens": ["Mein", "klah\u00b7rer", "Him\u00b7mel", "wird", "zu", "ei\u00b7ner", "Trau\u00b7e\u00b7rLufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich gehe jetzt im Boy an statt der gr\u00fcnen Seide/", "tokens": ["Ich", "ge\u00b7he", "jetzt", "im", "Boy", "an", "statt", "der", "gr\u00fc\u00b7nen", "Sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und mein Gesicht umgiebt ein Sorgen-voller Dufft.", "tokens": ["Und", "mein", "Ge\u00b7sicht", "um\u00b7giebt", "ein", "Sor\u00b7gen\u00b7vol\u00b7ler", "Dufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.22": {"text": "Wer wei\u00df/ ob ich mein Licht jemahlen wieder seh?", "tokens": ["Wer", "wei\u00df", "/", "ob", "ich", "mein", "Licht", "je\u00b7mah\u00b7len", "wie\u00b7der", "seh", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich mu\u00df vor Angst entselt in langen N\u00e4chten leben/", "tokens": ["Ich", "mu\u00df", "vor", "Angst", "ent\u00b7selt", "in", "lan\u00b7gen", "N\u00e4ch\u00b7ten", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es zeigt sich sonst kein Licht als der Gedancken-Schnee.", "tokens": ["Es", "zeigt", "sich", "sonst", "kein", "Licht", "als", "der", "Ge\u00b7dan\u00b7cken\u00b7Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PIAT", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mein mattes Hertze mu\u00df verderben und erfrieren/", "tokens": ["Mein", "mat\u00b7tes", "Hert\u00b7ze", "mu\u00df", "ver\u00b7der\u00b7ben", "und", "er\u00b7frie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "VMFIN", "VVFIN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der kalte Winter greiffts mit vieler Unlust an.", "tokens": ["Der", "kal\u00b7te", "Win\u00b7ter", "greiffts", "mit", "vie\u00b7ler", "Un\u00b7lust", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich mu\u00df/ ach herber Schmertz! Diejenige verliehren/", "tokens": ["Ich", "mu\u00df", "/", "ach", "her\u00b7ber", "Schmertz", "!", "Die\u00b7je\u00b7ni\u00b7ge", "ver\u00b7lieh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "XY", "ADJA", "NN", "$.", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Von der ich nur allein das Leben haben kan.", "tokens": ["Von", "der", "ich", "nur", "al\u00b7lein", "das", "Le\u00b7ben", "ha\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "ART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie! wenn des Winters Grim\u0303 der Sonnen-Strahlen mindert", "tokens": ["Wie", "!", "wenn", "des", "Win\u00b7ters", "Grim\u0303", "der", "Son\u00b7nen\u00b7Strah\u00b7len", "min\u00b7dert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "ART", "NN", "NE", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das bunte Blumen-Feld ein Todten-Kleid anzieht/", "tokens": ["Das", "bun\u00b7te", "Blu\u00b7men\u00b7Feld", "ein", "Tod\u00b7ten\u00b7Kleid", "an\u00b7zieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So hat auch dein Verlust die Sommer-Zeit verhindert/", "tokens": ["So", "hat", "auch", "dein", "Ver\u00b7lust", "die", "Som\u00b7mer\u00b7Zeit", "ver\u00b7hin\u00b7dert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Liebe ist erstarrt/ die Hoffnung steht verbl\u00fcht.", "tokens": ["Die", "Lie\u00b7be", "ist", "er\u00b7starrt", "/", "die", "Hoff\u00b7nung", "steht", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Zwar l\u00e4st die Liebe sich abwesend wol ernehren/", "tokens": ["Zwar", "l\u00e4st", "die", "Lie\u00b7be", "sich", "ab\u00b7we\u00b7send", "wol", "er\u00b7neh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Doch es ist nur ein Traum/ sie kan nicht lange stehn/", "tokens": ["Doch", "es", "ist", "nur", "ein", "Traum", "/", "sie", "kan", "nicht", "lan\u00b7ge", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "$(", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wenn man den Gegen-Stand nicht t\u00e4glich kan verehren/", "tokens": ["Wenn", "man", "den", "Ge\u00b7gen\u00b7Stand", "nicht", "t\u00e4g\u00b7lich", "kan", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PTKNEG", "ADJD", "VMFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So mu\u00df die Liebes-Glut ohn Nahrungs-Oehl vergehn.", "tokens": ["So", "mu\u00df", "die", "Lie\u00b7bes\u00b7Glut", "ohn", "Nah\u00b7rungs\u00b7Oehl", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Zeit verkehret viel sie \u00e4ndert die Gestalten/", "tokens": ["Die", "Zeit", "ver\u00b7keh\u00b7ret", "viel", "sie", "\u00e4n\u00b7dert", "die", "Ge\u00b7stal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein weit entferntes Hertz der Meisterin vergi\u00dft/", "tokens": ["Ein", "weit", "ent\u00b7fern\u00b7tes", "Hertz", "der", "Meis\u00b7te\u00b7rin", "ver\u00b7gi\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Mit der Abwesenheit mu\u00df Lieb\u2019 und Gunst erkalten/", "tokens": ["Mit", "der", "Ab\u00b7we\u00b7sen\u00b7heit", "mu\u00df", "Lieb'", "und", "Gunst", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.40": {"text": "Sie macht/ da\u00df deren Glut nicht immerw\u00e4hrend ist.", "tokens": ["Sie", "macht", "/", "da\u00df", "de\u00b7ren", "Glut", "nicht", "im\u00b7mer\u00b7w\u00e4h\u00b7rend", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PRELAT", "NN", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich f\u00fcrchte nicht umsonst annehmliche ", "tokens": ["Ich", "f\u00fcrch\u00b7te", "nicht", "um\u00b7sonst", "an\u00b7nehm\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.42": {"text": "Da\u00df meine Liebes-Glut sich in sich selbst verzehrt;", "tokens": ["Da\u00df", "mei\u00b7ne", "Lie\u00b7bes\u00b7Glut", "sich", "in", "sich", "selbst", "ver\u00b7zehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und da\u00df die schnelle Zeit die Neigungen ausrotte/", "tokens": ["Und", "da\u00df", "die", "schnel\u00b7le", "Zeit", "die", "Nei\u00b7gun\u00b7gen", "aus\u00b7rot\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Weil man abwesend nicht der Liebsten Pracht verehrt.", "tokens": ["Weil", "man", "ab\u00b7we\u00b7send", "nicht", "der", "Liebs\u00b7ten", "Pracht", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PTKNEG", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die Wurtzel ist nicht vest ins Hertze eingesencket/", "tokens": ["Die", "Wurt\u00b7zel", "ist", "nicht", "vest", "ins", "Hert\u00b7ze", "ein\u00b7ge\u00b7sen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN