{"textgrid.poem.26179": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Wir sa\u00dfen auf den engen Weinbergstufen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun hat der Weinberg seinen gr\u00fcnen Bl\u00e4tterbausch.", "tokens": ["Nun", "hat", "der", "Wein\u00b7berg", "sei\u00b7nen", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter\u00b7bausch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Luft geht um die Rebenbl\u00fct' und sucht,", "tokens": ["Die", "Luft", "geht", "um", "die", "Re\u00b7ben\u00b7bl\u00fct'", "und", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Statt bei der Traubenfrucht, schon bei der Bl\u00fcte ihren Rausch.", "tokens": ["Statt", "bei", "der", "Trau\u00b7ben\u00b7frucht", ",", "schon", "bei", "der", "Bl\u00fc\u00b7te", "ih\u00b7ren", "Rausch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}}, "stanza.2": {"line.1": {"text": "Wir sa\u00dfen auf den engen Weinbergstufen", "tokens": ["Wir", "sa\u00b7\u00dfen", "auf", "den", "en\u00b7gen", "Wein\u00b7berg\u00b7stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dachten nicht an K\u00fcfer, nicht an Kufen.", "tokens": ["Und", "dach\u00b7ten", "nicht", "an", "K\u00fc\u00b7fer", ",", "nicht", "an", "Ku\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weinseligkeit kam ohne Wein uns ungerufen.", "tokens": ["Wein\u00b7se\u00b7lig\u00b7keit", "kam", "oh\u00b7ne", "Wein", "uns", "un\u00b7ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PPER", "ADJD", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Es war der Weinberg unser zugemauert Haus,", "tokens": ["Es", "war", "der", "Wein\u00b7berg", "un\u00b7ser", "zu\u00b7ge\u00b7mau\u00b7ert", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unsere Augen sahn als Fenster hell hinaus,", "tokens": ["Und", "un\u00b7se\u00b7re", "Au\u00b7gen", "sahn", "als", "Fens\u00b7ter", "hell", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KOUS", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wie den Trinkern ging der Durst nicht aus.", "tokens": ["Und", "wie", "den", "Trin\u00b7kern", "ging", "der", "Durst", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}