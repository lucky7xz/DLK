{"textgrid.poem.58327": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "365. Der Herr ist meine St\u00e4rke", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich nicht immer bleib' in Jesus eingekehrt,", "tokens": ["Wenn", "ich", "nicht", "im\u00b7mer", "bleib'", "in", "Je\u00b7sus", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So werd' ich unvermerkt durchs mind'ste Ding gest\u00f6rt;", "tokens": ["So", "werd'", "ich", "un\u00b7ver\u00b7merkt", "durchs", "min\u00b7d'\u00b7ste", "Ding", "ge\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In Jesus bin ich stark \u2013 wenn Sinne und Gedanken", "tokens": ["In", "Je\u00b7sus", "bin", "ich", "stark", "\u2013", "wenn", "Sin\u00b7ne", "und", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ADJD", "$(", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In seiner Liebe stehn, so geh' ich ohne Wanken.", "tokens": ["In", "sei\u00b7ner", "Lie\u00b7be", "stehn", ",", "so", "geh'", "ich", "oh\u00b7ne", "Wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}