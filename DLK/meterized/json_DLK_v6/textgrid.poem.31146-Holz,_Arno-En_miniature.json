{"textgrid.poem.31146": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "En miniature", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Farbenfunkelnd in ihr Goldhaar hatte", "tokens": ["Far\u00b7ben\u00b7fun\u00b7kelnd", "in", "ihr", "Gold\u00b7haar", "hat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ein Libellenweibchen sich verirrt.", "tokens": ["Ein", "Li\u00b7bel\u00b7len\u00b7weib\u00b7chen", "sich", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Eins \u2013 zwei \u2013 drei Sekunden liess es dort", "tokens": ["Eins", "\u2013", "zwei", "\u2013", "drei", "Se\u00b7kun\u00b7den", "liess", "es", "dort"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "CARD", "$(", "CARD", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zierlich seine Fl\u00fcgelchen vibrieren,", "tokens": ["Zier\u00b7lich", "sei\u00b7ne", "Fl\u00fc\u00b7gel\u00b7chen", "vi\u00b7brie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Klappte sie dann schillernd wieder auf", "tokens": ["Klapp\u00b7te", "sie", "dann", "schil\u00b7lernd", "wie\u00b7der", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ADV", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und?", "tokens": ["Und", "?"], "token_info": ["word", "punct"], "pos": ["KON", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Fragt das Schilfrohr, wo es dann geblieben!", "tokens": ["Fragt", "das", "Schilf\u00b7rohr", ",", "wo", "es", "dann", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "L\u00e4chelnd \u00fcber das naive Thierchen,", "tokens": ["L\u00e4\u00b7chelnd", "\u00fc\u00b7ber", "das", "nai\u00b7ve", "Thier\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Das Frisuren noch f\u00fcr Blumen hielt,", "tokens": ["Das", "Fri\u00b7su\u00b7ren", "noch", "f\u00fcr", "Blu\u00b7men", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Band sie jetzt ihr kugelrundes Str\u00e4usschen", "tokens": ["Band", "sie", "jetzt", "ihr", "ku\u00b7gel\u00b7run\u00b7des", "Str\u00e4us\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Regelrecht mit einem Halm zusammen.", "tokens": ["Re\u00b7gel\u00b7recht", "mit", "ei\u00b7nem", "Halm", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Blank aus ihrem kleinen Goldreif blitzte", "tokens": ["Blank", "aus", "ih\u00b7rem", "klei\u00b7nen", "Gold\u00b7reif", "blitz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In die schwarzen Augen ihr die Sonne,", "tokens": ["In", "die", "schwar\u00b7zen", "Au\u00b7gen", "ihr", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und auf ihrem weissen Nacken liess", "tokens": ["Und", "auf", "ih\u00b7rem", "weis\u00b7sen", "Na\u00b7cken", "liess"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Blau der Flieder seine Bl\u00fcthen zittern.", "tokens": ["Blau", "der", "Flie\u00b7der", "sei\u00b7ne", "Bl\u00fc\u00b7then", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "So, jetzt noch dies B\u00fcndelchen Reseda,", "tokens": ["So", ",", "jetzt", "noch", "dies", "B\u00fcn\u00b7del\u00b7chen", "Re\u00b7se\u00b7da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "PDS", "NN", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Jetzt dies Veilchen, jetzt dies Tausendsch\u00f6nchen,", "tokens": ["Jetzt", "dies", "Veil\u00b7chen", ",", "jetzt", "dies", "Tau\u00b7send\u00b7sch\u00f6n\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "$,", "ADV", "PDS", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und \u2013 der alte G\u00e4rtnerjakob soll sich wundern!", "tokens": ["Und", "\u2013", "der", "al\u00b7te", "G\u00e4rt\u00b7ner\u00b7ja\u00b7kob", "soll", "sich", "wun\u00b7dern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ART", "ADJA", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sich ihr Morgenr\u00f6ckchen sorglich sch\u00fcrzend,", "tokens": ["Sich", "ihr", "Mor\u00b7gen\u00b7r\u00f6ck\u00b7chen", "sorg\u00b7lich", "sch\u00fcr\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dass der Thau nicht seinen Saum zern\u00e4sse,", "tokens": ["Dass", "der", "Thau", "nicht", "sei\u00b7nen", "Saum", "zer\u00b7n\u00e4s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Strich sie sich noch einmal \u00fcbers Sch\u00fcrzchen,", "tokens": ["Strich", "sie", "sich", "noch", "ein\u00b7mal", "\u00fc\u00b7bers", "Sch\u00fcrz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PRF", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Stippte dann die Blumen in den Springquell,", "tokens": ["Stipp\u00b7te", "dann", "die", "Blu\u00b7men", "in", "den", "Spring\u00b7quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Den der L\u00f6wenkopf ins Becken spie,", "tokens": ["Den", "der", "L\u00f6\u00b7wen\u00b7kopf", "ins", "Be\u00b7cken", "spie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Und die beiden kleinen Atlassch\u00fchchen,", "tokens": ["Und", "die", "bei\u00b7den", "klei\u00b7nen", "At\u00b7las\u00b7sch\u00fch\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Knallroth wie zwei Herrgottsk\u00e4ferchen,", "tokens": ["Knall\u00b7roth", "wie", "zwei", "Herr\u00b7gotts\u00b7k\u00e4\u00b7fer\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Trippelten, tripp-trapp, um die Bosketts", "tokens": ["Trip\u00b7pel\u00b7ten", ",", "tripp\u00b7trapp", ",", "um", "die", "Bos\u00b7ketts"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "$,", "KOUI", "ART", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Durch das sonnige Kastanienw\u00e4ldchen", "tokens": ["Durch", "das", "son\u00b7ni\u00b7ge", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7w\u00e4ld\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Auf das alte, graue Schlossthor zu.", "tokens": ["Auf", "das", "al\u00b7te", ",", "grau\u00b7e", "Schlos\u00b7sthor", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Doch der Weg bis dahin ist noch weit.", "tokens": ["Doch", "der", "Weg", "bis", "da\u00b7hin", "ist", "noch", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "PAV", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "So weit, dass das weisse Th\u00fcrmchen dort", "tokens": ["So", "weit", ",", "dass", "das", "weis\u00b7se", "Th\u00fcrm\u00b7chen", "dort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur noch wie ein Punkt durch die Allee blitzt.", "tokens": ["Nur", "noch", "wie", "ein", "Punkt", "durch", "die", "Al\u00b7lee", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Und sie spitzt ihr kirschrothrundes M\u00e4ulchen,", "tokens": ["Und", "sie", "spitzt", "ihr", "kirsc\u00b7hro\u00b7thrun\u00b7des", "M\u00e4ul\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dreht dem Faun, der marmorn sie durchs Buschwerk", "tokens": ["Dreht", "dem", "Faun", ",", "der", "mar\u00b7morn", "sie", "durchs", "Busc\u00b7hwerk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kollegialisch wie ein Nymphlein angrinst,", "tokens": ["Kol\u00b7le\u00b7gi\u00b7a\u00b7lisch", "wie", "ein", "Nymp\u00b7hlein", "an\u00b7grinst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Resolut ein aufgewipptes N\u00e4schen,", "tokens": ["Re\u00b7so\u00b7lut", "ein", "auf\u00b7ge\u00b7wipp\u00b7tes", "N\u00e4\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lacht laut auf und f\u00e4ngt ein altes Liedchen,", "tokens": ["Lacht", "laut", "auf", "und", "f\u00e4ngt", "ein", "al\u00b7tes", "Lied\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Das vielleicht mal ihrer Amme einfiel,", "tokens": ["Das", "viel\u00b7leicht", "mal", "ih\u00b7rer", "Am\u00b7me", "ein\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Als der Mondschein sie nicht schlafen liess,", "tokens": ["Als", "der", "Mond\u00b7schein", "sie", "nicht", "schla\u00b7fen", "liess", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und das heut ihr wieder wie ein Schw\u00e4lblein", "tokens": ["Und", "das", "heut", "ihr", "wie\u00b7der", "wie", "ein", "Schw\u00e4lb\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "PPER", "ADV", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Neckisch durch den kleinen, krausen Sinn schiesst,", "tokens": ["Ne\u00b7ckisch", "durch", "den", "klei\u00b7nen", ",", "krau\u00b7sen", "Sinn", "schiesst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Leise vor sich hinzusummen an:", "tokens": ["Lei\u00b7se", "vor", "sich", "hin\u00b7zu\u00b7sum\u00b7men", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbach wenn ich es doch nur w\u00fcsste, w\u00fcsste,", "tokens": ["\u00bb", "ach", "wenn", "ich", "es", "doch", "nur", "w\u00fcss\u00b7te", ",", "w\u00fcss\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ein Liebster seine Liebste k\u00fcsste!\u00ab", "tokens": ["Wie", "ein", "Liebs\u00b7ter", "sei\u00b7ne", "Liebs\u00b7te", "k\u00fcss\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbw\u00f6lklein, das dort um das T\u00e4nnlein flattert,", "tokens": ["\u00bb", "w\u00f6lk\u00b7lein", ",", "das", "dort", "um", "das", "T\u00e4nn\u00b7lein", "flat\u00b7tert", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "V\u00f6glein, das dort um das Nestlein girrt,", "tokens": ["V\u00f6\u00b7glein", ",", "das", "dort", "um", "das", "Nes\u00b7tlein", "girrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und du B\u00e4umlein, das so weiss dort bl\u00fcht,", "tokens": ["Und", "du", "B\u00e4um\u00b7lein", ",", "das", "so", "weiss", "dort", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "$,", "PRELS", "ADV", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Sag mir doch, wo schl\u00e4gt das Herz des Fr\u00fchlings?", "tokens": ["Sag", "mir", "doch", ",", "wo", "schl\u00e4gt", "das", "Herz", "des", "Fr\u00fch\u00b7lings", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Fl\u00f6tet es die Nachtigall ins Mondlicht,", "tokens": ["Fl\u00f6\u00b7tet", "es", "die", "Nach\u00b7ti\u00b7gall", "ins", "Mond\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wiegt's der Apfelbaum in seinen Bl\u00fcthen,", "tokens": ["Wiegt's", "der", "Ap\u00b7fel\u00b7baum", "in", "sei\u00b7nen", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Oder jauchzt's mir in der ei