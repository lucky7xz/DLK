{"textgrid.poem.35269": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die feindliche Erde", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weiche Bartlose sprachen unter sich von dem Ekel am Weibe.", "tokens": ["Wei\u00b7che", "Bart\u00b7lo\u00b7se", "spra\u00b7chen", "un\u00b7ter", "sich", "von", "dem", "E\u00b7kel", "am", "Wei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PRF", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "In steinernen Museen schritten sanft die ausgeschlafenen Kenner.", "tokens": ["In", "stei\u00b7ner\u00b7nen", "Mu\u00b7se\u00b7en", "schrit\u00b7ten", "sanft", "die", "aus\u00b7ge\u00b7schla\u00b7fe\u00b7nen", "Ken\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "In hei\u00dfen Redaktionen schrieb man die Lebensl\u00e4ufe ber\u00fchmter M\u00e4nner.", "tokens": ["In", "hei\u00b7\u00dfen", "Re\u00b7dak\u00b7ti\u00b7o\u00b7nen", "schrieb", "man", "die", "Le\u00b7bens\u00b7l\u00e4u\u00b7fe", "be\u00b7r\u00fchm\u00b7ter", "M\u00e4n\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Die H\u00e4user glotzten wie die Fre\u00dfz\u00e4hne an einem ungeheuren, g\u00e4hnenden Jahrmarkts-Ringer.", "tokens": ["Die", "H\u00e4u\u00b7ser", "glotz\u00b7ten", "wie", "die", "Fre\u00df\u00b7z\u00e4h\u00b7ne", "an", "ei\u00b7nem", "un\u00b7ge\u00b7heu\u00b7ren", ",", "g\u00e4h\u00b7nen\u00b7den", "Jahr\u00b7markt\u00b7sRin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+---+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Die Erde faulte l\u00e4nglich auf zur wimmelnden himmlischen Birne.", "tokens": ["Die", "Er\u00b7de", "faul\u00b7te", "l\u00e4ng\u00b7lich", "auf", "zur", "wim\u00b7meln\u00b7den", "himm\u00b7li\u00b7schen", "Bir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der Himmel rollte herum dunkel funkelnd im schwarzen hohlen Oval.", "tokens": ["Der", "Him\u00b7mel", "roll\u00b7te", "he\u00b7rum", "dun\u00b7kel", "fun\u00b7kelnd", "im", "schwar\u00b7zen", "hoh\u00b7len", "O\u00b7val", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "VVPP", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Das Licht war eingesogen in stampfende Kessel und Telegraphenstrahl.", "tokens": ["Das", "Licht", "war", "ein\u00b7ge\u00b7so\u00b7gen", "in", "stamp\u00b7fen\u00b7de", "Kes\u00b7sel", "und", "Te\u00b7le\u00b7gra\u00b7phen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Der Lampenschein strich klein durch die Stra\u00dfen wie Wurmaugen nachts im Korn.", "tokens": ["Der", "Lam\u00b7pen\u00b7schein", "strich", "klein", "durch", "die", "Stra\u00b7\u00dfen", "wie", "Wur\u00b7mau\u00b7gen", "nachts", "im", "Korn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "APPR", "ART", "NN", "KOKOM", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Das Licht war fort von der kleinen Brde, niemand sa\u00df in der Sonne oder blickte zum mondlichen Horn.", "tokens": ["Das", "Licht", "war", "fort", "von", "der", "klei\u00b7nen", "Br\u00b7de", ",", "nie\u00b7mand", "sa\u00df", "in", "der", "Son\u00b7ne", "o\u00b7der", "blick\u00b7te", "zum", "mond\u00b7li\u00b7chen", "Horn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,", "PIS", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+--+-+-+--+--+", "measure": "iambic.octa.plus.relaxed"}}}}}