{"dta.poem.16317": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wem ein Geliebtes stirbt, dem ist es wie ein Traum,", "tokens": ["Wem", "ein", "Ge\u00b7lieb\u00b7tes", "stirbt", ",", "dem", "ist", "es", "wie", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ersten Tage kommt er zu sich selber kaum.", "tokens": ["Die", "ers\u00b7ten", "Ta\u00b7ge", "kommt", "er", "zu", "sich", "sel\u00b7ber", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie ers ertragen soll, kann er sich selbst nicht fragen;", "tokens": ["Wie", "ers", "er\u00b7tra\u00b7gen", "soll", ",", "kann", "er", "sich", "selbst", "nicht", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VMFIN", "$,", "VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und wenn er sich besinnt, so hat ers schon ertragen.", "tokens": ["Und", "wenn", "er", "sich", "be\u00b7sinnt", ",", "so", "hat", "ers", "schon", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "$,", "ADV", "VAFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}