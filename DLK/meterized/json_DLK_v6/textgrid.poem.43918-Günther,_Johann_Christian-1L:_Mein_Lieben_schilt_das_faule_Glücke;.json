{"textgrid.poem.43918": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Lieben schilt das faule Gl\u00fccke;", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieben schilt das faule Gl\u00fccke;", "tokens": ["Mein", "Lie\u00b7ben", "schilt", "das", "fau\u00b7le", "Gl\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Hofnung spricht: Gedulde dich.", "tokens": ["Die", "Hof\u00b7nung", "spricht", ":", "Ge\u00b7dul\u00b7de", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel giebt zwar saure Blicke,", "tokens": ["Der", "Him\u00b7mel", "giebt", "zwar", "sau\u00b7re", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein er z\u00fcrnt nicht ewiglich;", "tokens": ["Al\u00b7lein", "er", "z\u00fcrnt", "nicht", "e\u00b7wig\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er kan durch unverhoftes Lachen", "tokens": ["Er", "kan", "durch", "un\u00b7ver\u00b7hof\u00b7tes", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Freude desto s\u00fc\u00dfer machen.", "tokens": ["Die", "Freu\u00b7de", "des\u00b7to", "s\u00fc\u00b7\u00dfer", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, wenn nur auch die Jugend s\u00e4umte!", "tokens": ["Ja", ",", "wenn", "nur", "auch", "die", "Ju\u00b7gend", "s\u00e4um\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Allein sie streicht so schnell vorbey.", "tokens": ["Al\u00b7lein", "sie", "streicht", "so", "schnell", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn mir nur nicht st\u00fcndlich tr\u00e4umte,", "tokens": ["Und", "wenn", "mir", "nur", "nicht", "st\u00fcnd\u00b7lich", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie schwer ein einsam Leben sey!", "tokens": ["Wie", "schwer", "ein", "ein\u00b7sam", "Le\u00b7ben", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man kan doch bey so b\u00f6sen Tagen", "tokens": ["Man", "kan", "doch", "bey", "so", "b\u00f6\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Kummer ohne Trost kaum tragen.", "tokens": ["Den", "Kum\u00b7mer", "oh\u00b7ne", "Trost", "kaum", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn ich noch mehr wi\u00dfen k\u00f6nte,", "tokens": ["Und", "wenn", "ich", "noch", "mehr", "wi\u00b7\u00dfen", "k\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor welch ich aufgehoben bin", "tokens": ["Vor", "welch", "ich", "auf\u00b7ge\u00b7ho\u00b7ben", "bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ob ihr Herz auch redlich brennte,", "tokens": ["Und", "ob", "ihr", "Herz", "auch", "red\u00b7lich", "brenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn reine Treu sucht gleichen Sinn,", "tokens": ["Denn", "rei\u00b7ne", "Treu", "sucht", "glei\u00b7chen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wollte mich zum voraus \u00fcben,", "tokens": ["Ich", "woll\u00b7te", "mich", "zum", "vo\u00b7raus", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie als mein Theil geschickt zu lieben.", "tokens": ["Sie", "als", "mein", "Theil", "ge\u00b7schickt", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "PPOSAT", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Herz, verwirf dies eitle Sorgen,", "tokens": ["Mein", "Herz", ",", "ver\u00b7wirf", "dies", "eit\u00b7le", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es macht dich doch nur grillenvoll;", "tokens": ["Es", "macht", "dich", "doch", "nur", "gril\u00b7len\u00b7voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du weist, die Vorsicht geht verborgen", "tokens": ["Du", "weist", ",", "die", "Vor\u00b7sicht", "geht", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und will nicht, da\u00df man gr\u00fcbeln soll,", "tokens": ["Und", "will", "nicht", ",", "da\u00df", "man", "gr\u00fc\u00b7beln", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit man nicht durch blinde R\u00e4ncke", "tokens": ["Da\u00b7mit", "man", "nicht", "durch", "blin\u00b7de", "R\u00e4n\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich selbst aus \u00dcbereilung kr\u00e4ncke.", "tokens": ["Sich", "selbst", "aus", "\u00dc\u00b7be\u00b7rei\u00b7lung", "kr\u00e4n\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich trau, o Himmel, deiner G\u00fcte,", "tokens": ["Ich", "trau", ",", "o", "Him\u00b7mel", ",", "dei\u00b7ner", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf diese kommt mein Wohlseyn an;", "tokens": ["Auf", "die\u00b7se", "kommt", "mein", "Wohl\u00b7seyn", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verbinde mir ein klug Gem\u00fcthe,", "tokens": ["Ver\u00b7bin\u00b7de", "mir", "ein", "klug", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das treu und z\u00e4rtlich k\u00fc\u00dfen kan", "tokens": ["Das", "treu", "und", "z\u00e4rt\u00b7lich", "k\u00fc\u00b7\u00dfen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "KON", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und das mich, wenn ich auch veralte,", "tokens": ["Und", "das", "mich", ",", "wenn", "ich", "auch", "ver\u00b7al\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Lust und Unruh wohl verhalte.", "tokens": ["In", "Lust", "und", "Un\u00b7ruh", "wohl", "ver\u00b7hal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}