{"dta.poem.15968": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wag' es wenn du's vermagst, von beiden Lebenssf\u00e4ren", "tokens": ["Wag'", "es", "wenn", "du's", "ver\u00b7magst", ",", "von", "bei\u00b7den", "Le\u00b7bens\u00b7sf\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "KOUS", "PIS", "VVFIN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die hier f\u00fcr Schein, die dort f\u00fcr Wahrheit zu erkl\u00e4ren!", "tokens": ["Die", "hier", "f\u00fcr", "Schein", ",", "die", "dort", "f\u00fcr", "Wahr\u00b7heit", "zu", "er\u00b7kl\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und sieh die Wirklichkeit f\u00fcr einen Schatten an,", "tokens": ["Und", "sieh", "die", "Wirk\u00b7lich\u00b7keit", "f\u00fcr", "ei\u00b7nen", "Schat\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dort vom fernen Licht sich streckt zu dir heran!", "tokens": ["Der", "dort", "vom", "fer\u00b7nen", "Licht", "sich", "streckt", "zu", "dir", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "PRF", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dagegen la\u00df nur auch dem andern seinen Glauben,", "tokens": ["Da\u00b7ge\u00b7gen", "la\u00df", "nur", "auch", "dem", "an\u00b7dern", "sei\u00b7nen", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der diese Wirklichkeit sich nicht will lassen rauben,", "tokens": ["Der", "die\u00b7se", "Wirk\u00b7lich\u00b7keit", "sich", "nicht", "will", "las\u00b7sen", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "PRF", "PTKNEG", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und selbst das Ewige f\u00fcr einen Schatten h\u00e4lt,", "tokens": ["Und", "selbst", "das", "E\u00b7wi\u00b7ge", "f\u00fcr", "ei\u00b7nen", "Schat\u00b7ten", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der von dem Sinnlichen hinaus ins Leere f\u00e4llt.", "tokens": ["Der", "von", "dem", "Sinn\u00b7li\u00b7chen", "hin\u00b7aus", "ins", "Lee\u00b7re", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APZR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du kannst den Schatten hier nicht leugnen, der dich neckt,", "tokens": ["Du", "kannst", "den", "Schat\u00b7ten", "hier", "nicht", "leug\u00b7nen", ",", "der", "dich", "neckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und er dort jenen nicht, der ihm ein Grauen weckt.", "tokens": ["Und", "er", "dort", "je\u00b7nen", "nicht", ",", "der", "ihm", "ein", "Grau\u00b7en", "weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PDS", "PTKNEG", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ihr theilet beide gleich die Welt in Licht und Schatten,", "tokens": ["Ihr", "thei\u00b7let", "bei\u00b7de", "gleich", "die", "Welt", "in", "Licht", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tauscht die Namen nur, wer will's euch nicht gestatten?", "tokens": ["Und", "tauscht", "die", "Na\u00b7men", "nur", ",", "wer", "will's", "euch", "nicht", "ge\u00b7stat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,", "PWS", "VMFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}