{"textgrid.poem.64375": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "98. Die Schwalbe und die Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Progne und Philomele sind Schwestern; die eine verk\u00fcndet", "tokens": ["Prog\u00b7ne", "und", "Phi\u00b7lo\u00b7me\u00b7le", "sind", "Schwes\u00b7tern", ";", "die", "ei\u00b7ne", "ver\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "NN", "$.", "ART", "PIS", "VVFIN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Uns den Sommer, und ist jedem willkommen und lieb;", "tokens": ["Uns", "den", "Som\u00b7mer", ",", "und", "ist", "je\u00b7dem", "will\u00b7kom\u00b7men", "und", "lieb", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "KON", "VAFIN", "PIAT", "ADJA", "KON", "ADJD", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aber willkommner ist uns und heil'ger, siebenmal lieber", "tokens": ["A\u00b7ber", "will\u00b7komm\u00b7ner", "ist", "uns", "und", "heil'\u00b7ger", ",", "sie\u00b7ben\u00b7mal", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "VAFIN", "PPER", "KON", "ADJA", "$,", "ADV", "ADV"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Philomele! wen r\u00fchrt nicht Philomelens Gesang?", "tokens": ["Phi\u00b7lo\u00b7me\u00b7le", "!", "wen", "r\u00fchrt", "nicht", "Phi\u00b7lo\u00b7me\u00b7lens", "Ge\u00b7sang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VVFIN", "PTKNEG", "NE", "NN", "$."], "meter": "--+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Nichts verk\u00fcndet sie uns, doch lauschen wir, nimmer get\u00e4uschet,", "tokens": ["Nichts", "ver\u00b7k\u00fcn\u00b7det", "sie", "uns", ",", "doch", "lau\u00b7schen", "wir", ",", "nim\u00b7mer", "ge\u00b7t\u00e4u\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Ihrem s\u00fc\u00dfen Gesang, welcher uns Thr\u00e4nen entlockt.", "tokens": ["Ih\u00b7rem", "s\u00fc\u00b7\u00dfen", "Ge\u00b7sang", ",", "wel\u00b7cher", "uns", "Thr\u00e4\u00b7nen", "ent\u00b7lockt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "NN", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Thr\u00e4ne der Sehnsucht, entfallen dem Aug', und dem Herzen entfallen,", "tokens": ["Thr\u00e4\u00b7ne", "der", "Sehn\u00b7sucht", ",", "ent\u00b7fal\u00b7len", "dem", "Aug'", ",", "und", "dem", "Her\u00b7zen", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Bist mir erquickend, wie Tau auf dem versengten Gefild!", "tokens": ["Bist", "mir", "er\u00b7qui\u00b7ckend", ",", "wie", "Tau", "auf", "dem", "ver\u00b7seng\u00b7ten", "Ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Zarter Ahndungen bebender Glanz, du schimmerst mir sch\u00f6ner,", "tokens": ["Zar\u00b7ter", "Ahn\u00b7dun\u00b7gen", "be\u00b7ben\u00b7der", "Glanz", ",", "du", "schim\u00b7merst", "mir", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-++-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Als des erwachenden Tags Purpur in zitterndem Tau.", "tokens": ["Als", "des", "er\u00b7wa\u00b7chen\u00b7den", "Tags", "Pur\u00b7pur", "in", "zit\u00b7tern\u00b7dem", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Singe mir Sehnsucht ins Herz, geliebte S\u00e4ngerin! singe,", "tokens": ["Sin\u00b7ge", "mir", "Sehn\u00b7sucht", "ins", "Herz", ",", "ge\u00b7lieb\u00b7te", "S\u00e4n\u00b7ge\u00b7rin", "!", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPRART", "NN", "$,", "ADJA", "NN", "$.", "VVFIN", "$,"], "meter": "+---+-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Philomele, mir Ruh, Ruhe der Ahndung ins Herz!", "tokens": ["Phi\u00b7lo\u00b7me\u00b7le", ",", "mir", "Ruh", ",", "Ru\u00b7he", "der", "Ahn\u00b7dung", "ins", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NN", "$,", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Ahndung ist unsre Weisheit hienieden, und unsere Wonne", "tokens": ["Ahn\u00b7dung", "ist", "uns\u00b7re", "Weis\u00b7heit", "hien\u00b7ie\u00b7den", ",", "und", "un\u00b7se\u00b7re", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Sehnsucht, doch kennen wir den, welcher die Sehnsucht uns lie\u00df!", "tokens": ["Sehn\u00b7sucht", ",", "doch", "ken\u00b7nen", "wir", "den", ",", "wel\u00b7cher", "die", "Sehn\u00b7sucht", "uns", "lie\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+++-+", "measure": "unknown.measure.octa.plus"}, "line.15": {"text": "Sehnsucht ist Morgenr\u00f6te; noch weilet unter dem Himmel", "tokens": ["Sehn\u00b7sucht", "ist", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ";", "noch", "wei\u00b7let", "un\u00b7ter", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "NN", "$.", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Unsre Sonne \u2013 sie kommt! Himmel und Erde! sie kommt!", "tokens": ["Uns\u00b7re", "Son\u00b7ne", "\u2013", "sie", "kommt", "!", "Him\u00b7mel", "und", "Er\u00b7de", "!", "sie", "kommt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "$.", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Heil dir, Herrliche! Tritt aus deinem heiligen Osten!", "tokens": ["Heil", "dir", ",", "Herr\u00b7li\u00b7che", "!", "Tritt", "aus", "dei\u00b7nem", "hei\u00b7li\u00b7gen", "Os\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$.", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Wahrheit strahlet dein Licht! Lieb' ist der Herrlichen Glut.", "tokens": ["Wahr\u00b7heit", "strah\u00b7let", "dein", "Licht", "!", "Lieb'", "ist", "der", "Herr\u00b7li\u00b7chen", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}