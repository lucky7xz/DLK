{"textgrid.poem.27609": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was man glaubt; ich wei\u00df nicht, wem man trauet.", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "man", "glaubt", ";", "ich", "wei\u00df", "nicht", ",", "wem", "man", "trau\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich h\u00e4tt' ein hohes Schlo\u00df, Herr Br\u00e4utigam, gebauet", "tokens": ["Ich", "h\u00e4tt'", "ein", "ho\u00b7hes", "Schlo\u00df", ",", "Herr", "Br\u00e4u\u00b7ti\u00b7gam", ",", "ge\u00b7bau\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "NN", "NE", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff eurer Worte Grund, als wie auff Fels und Stein;", "tokens": ["Auff", "eu\u00b7rer", "Wor\u00b7te", "Grund", ",", "als", "wie", "auff", "Fels", "und", "Stein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "KOUS", "KOKOM", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Seh' aber, da\u00df die Welt nur wil betrogen seyn", "tokens": ["Seh'", "a\u00b7ber", ",", "da\u00df", "die", "Welt", "nur", "wil", "be\u00b7tro\u00b7gen", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "VMFIN", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ich mit sambt der Welt. Ihr sprecht: ihr seyd ergetzet,", "tokens": ["Und", "ich", "mit", "sambt", "der", "Welt", ".", "Ihr", "sprecht", ":", "ihr", "seyd", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df euch de\u00df Himmels Gunst f\u00fcr Augen hat gesetzet", "tokens": ["Da\u00df", "euch", "de\u00df", "Him\u00b7mels", "Gunst", "f\u00fcr", "Au\u00b7gen", "hat", "ge\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPR", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den s\u00fcssen Hochzeit-Tag, und meinet doch die Nacht,", "tokens": ["Den", "s\u00fcs\u00b7sen", "Hoch\u00b7zeit\u00b7Tag", ",", "und", "mei\u00b7net", "doch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die euch zum Vater weiht, die Braut zur Mutter macht.", "tokens": ["Die", "euch", "zum", "Va\u00b7ter", "weiht", ",", "die", "Braut", "zur", "Mut\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil schwartzes ihr nun meint und weisses dennoch nennet,", "tokens": ["Weil", "schwart\u00b7zes", "ihr", "nun", "meint", "und", "weis\u00b7ses", "den\u00b7noch", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PPER", "ADV", "VVFIN", "KON", "ADJA", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "So sey euch, merket drauff, zur Strafe zuerkennet,", "tokens": ["So", "sey", "euch", ",", "mer\u00b7ket", "drauff", ",", "zur", "Stra\u00b7fe", "zu\u00b7er\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVFIN", "PTKVZ", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df, wenn ihr meint, es soll das erst ein S\u00f6hnlein sein,", "tokens": ["Da\u00df", ",", "wenn", "ihr", "meint", ",", "es", "soll", "das", "erst", "ein", "S\u00f6hn\u00b7lein", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PDS", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wird es E E E, wie Mutter Eva, schrein.", "tokens": ["So", "wird", "es", "E", "E", "E", ",", "wie", "Mut\u00b7ter", "E\u00b7va", ",", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "NN", "NN", "$,", "PWAV", "NN", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}