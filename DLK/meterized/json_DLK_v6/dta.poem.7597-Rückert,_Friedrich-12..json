{"dta.poem.7597": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh dort den Baum, der nie im Sonnenbrand ermattet,", "tokens": ["Sieh", "dort", "den", "Baum", ",", "der", "nie", "im", "Son\u00b7nen\u00b7brand", "er\u00b7mat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er als Sonnenschirm den eignen Fu\u00df beschattet.", "tokens": ["Weil", "er", "als", "Son\u00b7nen\u00b7schirm", "den", "eig\u00b7nen", "Fu\u00df", "be\u00b7schat\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er h\u00e4lt den Boden k\u00fchl und feucht worauf er steht,", "tokens": ["Er", "h\u00e4lt", "den", "Bo\u00b7den", "k\u00fchl", "und", "feucht", "wo\u00b7rauf", "er", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woraus der Wurzel Saft in alle Zweige geht.", "tokens": ["Wo\u00b7raus", "der", "Wur\u00b7zel", "Saft", "in", "al\u00b7le", "Zwei\u00b7ge", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Wurzel ist bedacht, die Kraft zu wenden oben", "tokens": ["Die", "Wur\u00b7zel", "ist", "be\u00b7dacht", ",", "die", "Kraft", "zu", "wen\u00b7den", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Wipfel zu, von dem ihr Schutzdach wird gewoben.", "tokens": ["Dem", "Wip\u00b7fel", "zu", ",", "von", "dem", "ihr", "Schutz\u00b7dach", "wird", "ge\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "ART", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Wipfel aber ringt stets dichter sich zu falten,", "tokens": ["Der", "Wip\u00b7fel", "a\u00b7ber", "ringt", "stets", "dich\u00b7ter", "sich", "zu", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um frisch den Nahrungsquell der Wurzel zu erhalten.", "tokens": ["Um", "frisch", "den", "Nah\u00b7rungs\u00b7quell", "der", "Wur\u00b7zel", "zu", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So ist ein M\u00e4nnerstamm, der wechselnd sich besch\u00fctzt;", "tokens": ["So", "ist", "ein", "M\u00e4n\u00b7ner\u00b7stamm", ",", "der", "wech\u00b7selnd", "sich", "be\u00b7sch\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So jeder einzle Mann, der seine Kr\u00e4fte n\u00fctzt.", "tokens": ["So", "je\u00b7der", "einz\u00b7le", "Mann", ",", "der", "sei\u00b7ne", "Kr\u00e4f\u00b7te", "n\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}