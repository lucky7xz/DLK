{"textgrid.poem.54649": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "63. Auff dieselbige", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr die ihr ewre Zeit von erstem Tag an zehlet/", "tokens": ["Ihr", "die", "ihr", "ew\u00b7re", "Zeit", "von", "ers\u00b7tem", "Tag", "an", "zeh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlachet meine Nacht des Lebens Anfang nicht:", "tokens": ["Ver\u00b7la\u00b7chet", "mei\u00b7ne", "Nacht", "des", "Le\u00b7bens", "An\u00b7fang", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob ihr gebohren seyd bey hellem Sonnen Licht/", "tokens": ["Ob", "ihr", "ge\u00b7boh\u00b7ren", "seyd", "bey", "hel\u00b7lem", "Son\u00b7nen", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat meine Lebens Stund doch Gott ihn selbst erwehlet.", "tokens": ["Hat", "mei\u00b7ne", "Le\u00b7bens", "Stund", "doch", "Gott", "ihn", "selbst", "er\u00b7weh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "ADV", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}