{"textgrid.poem.56254": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Ein franz\u00f6sisches Lied", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und sitz ich am Tische beim Glase Wein,", "tokens": ["Und", "sitz", "ich", "am", "Ti\u00b7sche", "beim", "Gla\u00b7se", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPRART", "NN", "APPRART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Trink aus!", "tokens": ["Trink", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und stimmen auch wacker die Freunde mit ein,", "tokens": ["Und", "stim\u00b7men", "auch", "wa\u00b7cker", "die", "Freun\u00b7de", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ART", "NN", "APPR", "ART", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Trink aus!", "tokens": ["Trink", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "So geht mir zu Herzen das Heil der Welt:", "tokens": ["So", "geht", "mir", "zu", "Her\u00b7zen", "das", "Heil", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "'s ist gar zu erb\u00e4rmlich damit auch bestellt,", "tokens": ["'s", "ist", "gar", "zu", "er\u00b7b\u00e4rm\u00b7lich", "da\u00b7mit", "auch", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADJD", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Trink aus, trink aus, trink aus!", "tokens": ["Trink", "aus", ",", "trink", "aus", ",", "trink", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es treiben's die Leute zu kraus!", "tokens": ["Es", "trei\u00b7ben's", "die", "Leu\u00b7te", "zu", "kraus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ich sollte nur tragen der Herrschaft Last,", "tokens": ["Ich", "soll\u00b7te", "nur", "tra\u00b7gen", "der", "Herr\u00b7schaft", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Trink aus!", "tokens": ["Trink", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Es st\u00fcnde bald anders und besser fast.", "tokens": ["Es", "st\u00fcn\u00b7de", "bald", "an\u00b7ders", "und", "bes\u00b7ser", "fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "ADJD", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Trink aus!", "tokens": ["Trink", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Die Presse zuerst und die Wahlen frei,", "tokens": ["Die", "Pres\u00b7se", "zu\u00b7erst", "und", "die", "Wah\u00b7len", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die Presse, sie dient mir als Polizei.", "tokens": ["Die", "Pres\u00b7se", ",", "sie", "dient", "mir", "als", "Po\u00b7li\u00b7zei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Trink aus, trink aus, trink aus!", "tokens": ["Trink", "aus", ",", "trink", "aus", ",", "trink", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es treiben's die Leute zu kraus!", "tokens": ["Es", "trei\u00b7ben's", "die", "Leu\u00b7te", "zu", "kraus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wann erst in dem Hause Vertrauen besteht,", "tokens": ["Wann", "erst", "in", "dem", "Hau\u00b7se", "Ver\u00b7trau\u00b7en", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Trink aus!", "tokens": ["Trink", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Geht alles von selbst, was nimmer sonst geht.", "tokens": ["Geht", "al\u00b7les", "von", "selbst", ",", "was", "nim\u00b7mer", "sonst", "geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADV", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Trink aus!", "tokens": ["Trink", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wir schaffen uns bald vor den M\u00f6nchen Ruh,", "tokens": ["Wir", "schaf\u00b7fen", "uns", "bald", "vor", "den", "M\u00f6n\u00b7chen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wir schicken die frommsten dem Chaves zu,", "tokens": ["Wir", "schi\u00b7cken", "die", "fromms\u00b7ten", "dem", "Cha\u00b7ves", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Trink aus, trink aus, trink aus!", "tokens": ["Trink", "aus", ",", "trink", "aus", ",", "trink", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es treiben's die Leute zu kraus!", "tokens": ["Es", "trei\u00b7ben's", "die", "Leu\u00b7te", "zu", "kraus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Es m\u00f6gen die St\u00e4dte verwalten sodann \u2013", "tokens": ["Es", "m\u00f6\u00b7gen", "die", "St\u00e4d\u00b7te", "ver\u00b7wal\u00b7ten", "so\u00b7dann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Trink aus!", "tokens": ["Trink", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die eignen Gesch\u00e4fte, es geht sie nur an,", "tokens": ["Die", "eig\u00b7nen", "Ge\u00b7sch\u00e4f\u00b7te", ",", "es", "geht", "sie", "nur", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Trink aus!", "tokens": ["Trink", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Regieren nur wenig, das wenige gut,", "tokens": ["Re\u00b7gie\u00b7ren", "nur", "we\u00b7nig", ",", "das", "we\u00b7ni\u00b7ge", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "$,", "PRELS", "PIS", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Das hab ich der Ruhe halber geruht,", "tokens": ["Das", "hab", "ich", "der", "Ru\u00b7he", "hal\u00b7ber", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "NN", "APPO", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Trink aus, trink aus, trink aus!", "tokens": ["Trink", "aus", ",", "trink", "aus", ",", "trink", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es trieben's die Leute zu kraus!", "tokens": ["Es", "trie\u00b7ben's", "die", "Leu\u00b7te", "zu", "kraus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und merkt euch, ihr Freunde, wie trefflich es schafft!", "tokens": ["Und", "merkt", "euch", ",", "ihr", "Freun\u00b7de", ",", "wie", "treff\u00b7lich", "es", "schafft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Trink aus!", "tokens": ["Trink", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die Liebe der V\u00f6lker, da lieget die Kraft,", "tokens": ["Die", "Lie\u00b7be", "der", "V\u00f6l\u00b7ker", ",", "da", "lie\u00b7get", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Trink aus!", "tokens": ["Trink", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wie klingen die Gl\u00e4ser in heiliger Lust,", "tokens": ["Wie", "klin\u00b7gen", "die", "Gl\u00e4\u00b7ser", "in", "hei\u00b7li\u00b7ger", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wie schallt das Gebet mir aus jeglicher Brust,", "tokens": ["Wie", "schallt", "das", "Ge\u00b7bet", "mir", "aus", "jeg\u00b7li\u00b7cher", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Trink aus, trink aus, trink aus!", "tokens": ["Trink", "aus", ",", "trink", "aus", ",", "trink", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der K\u00f6nig hoch, und sein Haus!", "tokens": ["Der", "K\u00f6\u00b7nig", "hoch", ",", "und", "sein", "Haus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Sind aber die Gl\u00e4ser und Flaschen erst leer,", "tokens": ["Sind", "a\u00b7ber", "die", "Gl\u00e4\u00b7ser", "und", "Fla\u00b7schen", "erst", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zu Bett!", "tokens": ["Zu", "Bett", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Dann werden der Kopf und die Zunge mir schwer,", "tokens": ["Dann", "wer\u00b7den", "der", "Kopf", "und", "die", "Zun\u00b7ge", "mir", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zu Bett!", "tokens": ["Zu", "Bett", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Mein Weib wird mich schelten, mein Herrschen ist aus,", "tokens": ["Mein", "Weib", "wird", "mich", "schel\u00b7ten", ",", "mein", "Herr\u00b7schen", "ist", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ich schleiche mich leise, ganz leise nach Haus,", "tokens": ["Ich", "schlei\u00b7che", "mich", "lei\u00b7se", ",", "ganz", "lei\u00b7se", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Zu Bett, zu Bett, zu Bett!", "tokens": ["Zu", "Bett", ",", "zu", "Bett", ",", "zu", "Bett", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df sie den Pantoffel nicht h\u00e4tt!", "tokens": ["Da\u00df", "sie", "den", "Pan\u00b7tof\u00b7fel", "nicht", "h\u00e4tt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}