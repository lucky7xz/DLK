{"textgrid.poem.59601": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Betet, M\u00e4nner! \u2013 Denn ein J\u00fcngling kniet \u2013", "tokens": ["Be\u00b7tet", ",", "M\u00e4n\u00b7ner", "!", "\u2013", "Denn", "ein", "J\u00fcng\u00b7ling", "kniet", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "$(", "KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df sein Herz, sein Eisen heilig werde!", "tokens": ["Da\u00df", "sein", "Herz", ",", "sein", "Ei\u00b7sen", "hei\u00b7lig", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "K\u00fcsse, Knabe, fr\u00f6hlich diese Erde,", "tokens": ["K\u00fcs\u00b7se", ",", "Kna\u00b7be", ",", "fr\u00f6h\u00b7lich", "die\u00b7se", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn sie ist der Freiheit heil'ges Land.", "tokens": ["Denn", "sie", "ist", "der", "Frei\u00b7heit", "heil'\u00b7ges", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Willst du seinen Namen h\u00f6ren?", "tokens": ["Willst", "du", "sei\u00b7nen", "Na\u00b7men", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00fche bei dem Klang der Ehren!", "tokens": ["Gl\u00fc\u00b7he", "bei", "dem", "Klang", "der", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Betet, M\u00e4nner! \u2013 Denn ein J\u00fcngling kniet \u2013", "tokens": ["Be\u00b7tet", ",", "M\u00e4n\u00b7ner", "!", "\u2013", "Denn", "ein", "J\u00fcng\u00b7ling", "kniet", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "$(", "KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Macht den Klang unsterblich seinen Ohren!", "tokens": ["Macht", "den", "Klang", "uns\u00b7terb\u00b7lich", "sei\u00b7nen", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Deutscher J\u00fcngling, frei bist du geboren,", "tokens": ["Deut\u00b7scher", "J\u00fcng\u00b7ling", ",", "frei", "bist", "du", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Freiheit sei dein Glanz, dein h\u00f6chstes Gut!", "tokens": ["Frei\u00b7heit", "sei", "dein", "Glanz", ",", "dein", "h\u00f6chs\u00b7tes", "Gut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ihr sollst du dein ganzes Leben,", "tokens": ["Ihr", "sollst", "du", "dein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ihr den letzten Atem geben,", "tokens": ["Ihr", "den", "letz\u00b7ten", "A\u00b7tem", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihr dein bestes Herzensblut!", "tokens": ["Ihr", "dein", "bes\u00b7tes", "Her\u00b7zens\u00b7blut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Betet, M\u00e4nner! \u2013 Denn ein J\u00fcngling kniet \u2013", "tokens": ["Be\u00b7tet", ",", "M\u00e4n\u00b7ner", "!", "\u2013", "Denn", "ein", "J\u00fcng\u00b7ling", "kniet", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "$(", "KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und er hat den h\u00f6chsten Schwur geschworen.", "tokens": ["Und", "er", "hat", "den", "h\u00f6chs\u00b7ten", "Schwur", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hier und dort sei ihm das Heil verloren,", "tokens": ["Hier", "und", "dort", "sei", "ihm", "das", "Heil", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn er diese Worte jemals schw\u00e4cht!", "tokens": ["Wenn", "er", "die\u00b7se", "Wor\u00b7te", "je\u00b7mals", "schw\u00e4cht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Erd' und Himmel sollen zeugen!", "tokens": ["Erd'", "und", "Him\u00b7mel", "sol\u00b7len", "zeu\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dienen m\u00fcss' er dann dem Feigen", "tokens": ["Die\u00b7nen", "m\u00fcss'", "er", "dann", "dem", "Fei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und erzittern vor dem Knecht!", "tokens": ["Und", "er\u00b7zit\u00b7tern", "vor", "dem", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Betet, M\u00e4nner! \u2013 Denn ein J\u00fcngling kniet \u2013", "tokens": ["Be\u00b7tet", ",", "M\u00e4n\u00b7ner", "!", "\u2013", "Denn", "ein", "J\u00fcng\u00b7ling", "kniet", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "$(", "KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sch\u00f6nes Eisen, du, der Freien Freude,", "tokens": ["Sch\u00f6\u00b7nes", "Ei\u00b7sen", ",", "du", ",", "der", "Frei\u00b7en", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schmuck der Tapfern, k\u00f6stlichstes Geschmeide,", "tokens": ["Schmuck", "der", "Tap\u00b7fern", ",", "k\u00f6st\u00b7lichs\u00b7tes", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das der Hammer aus Metallen schlug!", "tokens": ["Das", "der", "Ham\u00b7mer", "aus", "Me\u00b7tal\u00b7len", "schlug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Werde, ritterlicher Degen,", "tokens": ["Wer\u00b7de", ",", "rit\u00b7ter\u00b7li\u00b7cher", "De\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutschem Lande Ruhm und Segen!", "tokens": ["Deut\u00b7schem", "Lan\u00b7de", "Ruhm", "und", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Werde Deutschlands Feinden Fluch!", "tokens": ["Wer\u00b7de", "Deutschlands", "Fein\u00b7den", "Fluch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Betet, M\u00e4nner! \u2013 Denn ein J\u00fcngling kniet \u2013", "tokens": ["Be\u00b7tet", ",", "M\u00e4n\u00b7ner", "!", "\u2013", "Denn", "ein", "J\u00fcng\u00b7ling", "kniet", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "$(", "KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Eisen, k\u00f6nnte Untreu diesen sch\u00e4nden,", "tokens": ["Ei\u00b7sen", ",", "k\u00f6nn\u00b7te", "Un\u00b7treu", "die\u00b7sen", "sch\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "APPR", "PDAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dann emp\u00f6re dich in seinen H\u00e4nden,", "tokens": ["Dann", "em\u00b7p\u00f6\u00b7re", "dich", "in", "sei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sto\u00df in seine Brust geschwindsten Tod!", "tokens": ["Sto\u00df", "in", "sei\u00b7ne", "Brust", "ge\u00b7schwinds\u00b7ten", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Dulde nimmer, Schwert der Ehren,", "tokens": ["Dul\u00b7de", "nim\u00b7mer", ",", "Schwert", "der", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df Verr\u00e4ter bei dir schw\u00f6ren!", "tokens": ["Da\u00df", "Ver\u00b7r\u00e4\u00b7ter", "bei", "dir", "schw\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dulde nimmer Sklavennot!", "tokens": ["Dul\u00b7de", "nim\u00b7mer", "Skla\u00b7ven\u00b7not", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Betet, M\u00e4nner! \u2013 Denn ein J\u00fcngling kniet \u2013", "tokens": ["Be\u00b7tet", ",", "M\u00e4n\u00b7ner", "!", "\u2013", "Denn", "ein", "J\u00fcng\u00b7ling", "kniet", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "$(", "KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Steh nun auf, umg\u00fcrtet mit dem Stahle!", "tokens": ["Steh", "nun", "auf", ",", "um\u00b7g\u00fcr\u00b7tet", "mit", "dem", "Stah\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Steh nun auf! Es schaun vom Himmelssaale", "tokens": ["Steh", "nun", "auf", "!", "Es", "schaun", "vom", "Him\u00b7mels\u00b7saa\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deine Ahnen fr\u00f6hlich auf dein Fest,", "tokens": ["Dei\u00b7ne", "Ah\u00b7nen", "fr\u00f6h\u00b7lich", "auf", "dein", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Segnen deine Waffenweihe,", "tokens": ["Seg\u00b7nen", "dei\u00b7ne", "Waf\u00b7fen\u00b7wei\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Machen dich f\u00fcr Pflicht und Treue", "tokens": ["Ma\u00b7chen", "dich", "f\u00fcr", "Pflicht", "und", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ehrenfest und eisenfest.", "tokens": ["Eh\u00b7ren\u00b7fest", "und", "ei\u00b7sen\u00b7fest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Betet, M\u00e4nner, heiligstes Gebet!", "tokens": ["Be\u00b7tet", ",", "M\u00e4n\u00b7ner", ",", "hei\u00b7ligs\u00b7tes", "Ge\u00b7bet", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Gott im h\u00f6chsten Himmel gebe Segen", "tokens": ["Gott", "im", "h\u00f6chs\u00b7ten", "Him\u00b7mel", "ge\u00b7be", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Diesem freien Mann und seinem Degen,", "tokens": ["Die\u00b7sem", "frei\u00b7en", "Mann", "und", "sei\u00b7nem", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df er Blitz in deutschen Schlachten sei!", "tokens": ["Da\u00df", "er", "Blitz", "in", "deut\u00b7schen", "Schlach\u00b7ten", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Gott beh\u00fcte unsre Lande,", "tokens": ["Gott", "be\u00b7h\u00fc\u00b7te", "uns\u00b7re", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Seelen vor der Schande!", "tokens": ["Uns\u00b7re", "See\u00b7len", "vor", "der", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott erhalte Deutschland frei!", "tokens": ["Gott", "er\u00b7hal\u00b7te", "Deutschland", "frei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}