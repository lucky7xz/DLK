{"textgrid.poem.45665": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich preise dich, und ohne dich zu kennen!", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich preise dich, und ohne dich zu kennen!", "tokens": ["Ich", "prei\u00b7se", "dich", ",", "und", "oh\u00b7ne", "dich", "zu", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das m\u00f6chte mancher vorschnell nennen,", "tokens": ["Das", "m\u00f6ch\u00b7te", "man\u00b7cher", "vor\u00b7schnell", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und h\u00e4tte doch, wei\u00df Gott, wie weit gefehlt \u2013", "tokens": ["Und", "h\u00e4t\u00b7te", "doch", ",", "wei\u00df", "Gott", ",", "wie", "weit", "ge\u00b7fehlt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$,", "VVFIN", "NN", "$,", "PWAV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich kenne den, der dich gew\u00e4hlt.", "tokens": ["Ich", "ken\u00b7ne", "den", ",", "der", "dich", "ge\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}