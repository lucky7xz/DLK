{"dta.poem.13093": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thalia schaute mich, ich sie bedachtsam an,", "tokens": ["Tha\u00b7lia", "schau\u00b7te", "mich", ",", "ich", "sie", "be\u00b7dacht\u00b7sam", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPER", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Als ob ein jedes sich auf diesen Spruch besann.", "tokens": ["Als", "ob", "ein", "je\u00b7des", "sich", "auf", "die\u00b7sen", "Spruch", "be\u00b7sann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "PIAT", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich sagte: diese prahlt zwar sehr mit ihren Bildern;", "tokens": ["Ich", "sag\u00b7te", ":", "die\u00b7se", "prahlt", "zwar", "sehr", "mit", "ih\u00b7ren", "Bil\u00b7dern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDS", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein was wurde sie zum Ruhm des F\u00fcrstens schildern?", "tokens": ["Al\u00b7lein", "was", "wur\u00b7de", "sie", "zum", "Ruhm", "des", "F\u00fcrs\u00b7tens", "schil\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "285Thalia sprach hierauf: \u201d Vielleicht beschreibt sie noch", "tokens": ["sprach", "hier\u00b7auf", ":", "\"", "Viel\u00b7leicht", "be\u00b7schreibt", "sie", "noch"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "$.", "$(", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u201emehr als was Pr\u00e4chtiges und hebt es Himmel hoch.", "tokens": ["\u201e", "mehr", "als", "was", "Pr\u00e4ch\u00b7ti\u00b7ges", "und", "hebt", "es", "Him\u00b7mel", "hoch", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PIS", "NN", "KON", "VVFIN", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201esie r\u00fchmt sich ohne dem, sie sey dorther gekommen;", "tokens": ["\u201e", "sie", "r\u00fchmt", "sich", "oh\u00b7ne", "dem", ",", "sie", "sey", "dor\u00b7ther", "ge\u00b7kom\u00b7men", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "APPR", "ART", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201edie Kunst sey von dem Licht der Sonnen hergenommen.", "tokens": ["\u201e", "die", "Kunst", "sey", "von", "dem", "Licht", "der", "Son\u00b7nen", "her\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}