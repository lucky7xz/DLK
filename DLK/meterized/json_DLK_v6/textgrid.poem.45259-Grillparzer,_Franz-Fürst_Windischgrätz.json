{"textgrid.poem.45259": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcrst Windischgr\u00e4tz", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trotz Worten, gro\u00df und emphatischen,", "tokens": ["Trotz", "Wor\u00b7ten", ",", "gro\u00df", "und", "em\u00b7pha\u00b7ti\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bleibst du gebannt im Donauwinkel,", "tokens": ["Bleibst", "du", "ge\u00b7bannt", "im", "Do\u00b7nau\u00b7win\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Steif macht nebst aristokratischen", "tokens": ["Steif", "macht", "nebst", "a\u00b7ris\u00b7to\u00b7kra\u00b7ti\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dich auch noch Gamaschend\u00fcnkel.", "tokens": ["Dich", "auch", "noch", "Ga\u00b7ma\u00b7schen\u00b7d\u00fcn\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}