{"textgrid.poem.35498": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Auf Tr\u00fcmmern", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auszog ich: den Muskel gestrafft, den Blick", "tokens": ["Aus\u00b7zog", "ich", ":", "den", "Mus\u00b7kel", "ge\u00b7strafft", ",", "den", "Blick"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ART", "NN", "VVPP", "$,", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So blitzend, so leuchtend, so helle! ...", "tokens": ["So", "blit\u00b7zend", ",", "so", "leuch\u00b7tend", ",", "so", "hel\u00b7le", "!", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVPP", "$,", "ADV", "ADJA", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Heimkehr' ich \u2013 den Traum \u2013 den Traum von Gl\u00fcck", "tokens": ["Heim\u00b7kehr'", "ich", "\u2013", "den", "Traum", "\u2013", "den", "Traum", "von", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "$(", "ART", "NN", "$(", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Verschlang der ", "tokens": ["Ver\u00b7schlang", "der"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Auf Tr\u00fcmmern lehn' ich ... Es schert mich kaum \u2013", "tokens": ["Auf", "Tr\u00fcm\u00b7mern", "lehn'", "ich", "...", "Es", "schert", "mich", "kaum", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus Tr\u00fcmmern bau' ich aufs neue \u2013", "tokens": ["Aus", "Tr\u00fcm\u00b7mern", "bau'", "ich", "aufs", "neu\u00b7e", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor\u00fcber der Jugend Morgenrotstraum \u2013", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "der", "Ju\u00b7gend", "Mor\u00b7gen\u00b7rots\u00b7traum", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}