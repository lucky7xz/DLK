{"dta.poem.18728": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Gesteht\u2019s! Die Dichter des Orients", "tokens": ["Ge\u00b7steht's", "!", "Die", "Dich\u00b7ter", "des", "O\u00b7rients"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind gr\u00f6sser als wir des Occidents.", "tokens": ["Sind", "gr\u00f6s\u00b7ser", "als", "wir", "des", "Oc\u00b7ci\u00b7dents", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Worin wir sie aber v\u00f6llig erreichen,", "tokens": ["Wo\u00b7rin", "wir", "sie", "a\u00b7ber", "v\u00f6l\u00b7lig", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ist im Hass auf unsres Gleichen.", "tokens": ["Das", "ist", "im", "Hass", "auf", "uns\u00b7res", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}