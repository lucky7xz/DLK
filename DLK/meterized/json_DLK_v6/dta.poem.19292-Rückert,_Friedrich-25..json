{"dta.poem.19292": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu jeder Stund' ist dir, was du bedarfst gereicht;", "tokens": ["Zu", "je\u00b7der", "Stund'", "ist", "dir", ",", "was", "du", "be\u00b7darfst", "ge\u00b7reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "$,", "PWS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ergreif es nur, da\u00df es nicht ungenutzt entweicht.", "tokens": ["Er\u00b7greif", "es", "nur", ",", "da\u00df", "es", "nicht", "un\u00b7ge\u00b7nutzt", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erkenne immermehr allsehender Vorsicht Walten", "tokens": ["Er\u00b7ken\u00b7ne", "im\u00b7mer\u00b7mehr", "all\u00b7se\u00b7hen\u00b7der", "Vor\u00b7sicht", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In dem, was Bl\u00f6dere f\u00fcr blinden Zufall halten.", "tokens": ["In", "dem", ",", "was", "Bl\u00f6\u00b7de\u00b7re", "f\u00fcr", "blin\u00b7den", "Zu\u00b7fall", "hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexand