{"textgrid.poem.66387": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Schulmeistertrost", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe viele Sorgen,", "tokens": ["Ich", "ha\u00b7be", "vie\u00b7le", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Leben wird vom Morgen", "tokens": ["Mein", "Le\u00b7ben", "wird", "vom", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis in die sp\u00e4te Nacht", "tokens": ["Bis", "in", "die", "sp\u00e4\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Lehren zugebracht.", "tokens": ["Mit", "Leh\u00b7ren", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Viel M\u00e4gdlein und viel Knaben", "tokens": ["Viel", "M\u00e4gd\u00b7lein", "und", "viel", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf seiner Seele haben,", "tokens": ["Auf", "sei\u00b7ner", "See\u00b7le", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist wahrlich eine Pflicht", "tokens": ["Ist", "wahr\u00b7lich", "ei\u00b7ne", "Pflicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von dr\u00fcckendem Gewicht.", "tokens": ["Von", "dr\u00fc\u00b7cken\u00b7dem", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch thu' ich es mit Freuden;", "tokens": ["Doch", "thu'", "ich", "es", "mit", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn Christi Sch\u00e4flein weiden", "tokens": ["Denn", "Chris\u00b7ti", "Sch\u00e4f\u00b7lein", "wei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf kleebes\u00e4ter Trift", "tokens": ["Auf", "klee\u00b7be\u00b7s\u00e4\u00b7ter", "Trift"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Macht selig nach der Schrift.", "tokens": ["Macht", "se\u00b7lig", "nach", "der", "Schrift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Die gro\u00dfen starken Geister", "tokens": ["Die", "gro\u00b7\u00dfen", "star\u00b7ken", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Besch\u00e4mt oft ein Schulmeister,", "tokens": ["Be\u00b7sch\u00e4mt", "oft", "ein", "Schul\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der in dem Hirtenamt", "tokens": ["Der", "in", "dem", "Hir\u00b7ten\u00b7amt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von reinem Eifer flammt.", "tokens": ["Von", "rei\u00b7nem", "Ei\u00b7fer", "flammt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Kinder Herz regieren", "tokens": ["Der", "Kin\u00b7der", "Herz", "re\u00b7gie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sie zur Tugend f\u00fchren", "tokens": ["Und", "sie", "zur", "Tu\u00b7gend", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch treuen Unterricht,", "tokens": ["Durch", "treu\u00b7en", "Un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Welch eine s\u00fc\u00dfe Pflicht!", "tokens": ["Welch", "ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das Lesen, Rechnen, Schreiben", "tokens": ["Das", "Le\u00b7sen", ",", "Rech\u00b7nen", ",", "Schrei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit k\u00fcnft'gen B\u00fcrgern treiben,", "tokens": ["Mit", "k\u00fcnft'\u00b7gen", "B\u00fcr\u00b7gern", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sie mit Bildners Hand", "tokens": ["Und", "sie", "mit", "Bild\u00b7ners", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bereiten f\u00fcr das Land;", "tokens": ["Be\u00b7rei\u00b7ten", "f\u00fcr", "das", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wenn mit stillem Schm\u00e4hen", "tokens": ["Und", "wenn", "mit", "stil\u00b7lem", "Schm\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Menschen auf uns sehen,", "tokens": ["Die", "Men\u00b7schen", "auf", "uns", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fcr verdienten Lohn", "tokens": ["Und", "f\u00fcr", "ver\u00b7dien\u00b7ten", "Lohn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Oft geben Spott und Hohn;", "tokens": ["Oft", "ge\u00b7ben", "Spott", "und", "Hohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dies leiden ohne Kr\u00e4nken", "tokens": ["Dies", "lei\u00b7den", "oh\u00b7ne", "Kr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und still im Herzen denken:", "tokens": ["Und", "still", "im", "Her\u00b7zen", "den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich dulde gern die Schmach", "tokens": ["Ich", "dul\u00b7de", "gern", "die", "Schmach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dem gr\u00f6\u00dften Lehrer nach \u2013", "tokens": ["Dem", "gr\u00f6\u00df\u00b7ten", "Leh\u00b7rer", "nach", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dies ist Schulmeistersw\u00fcrde;", "tokens": ["Dies", "ist", "Schul\u00b7meis\u00b7ters\u00b7w\u00fcr\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drum trag' ich meine B\u00fcrde", "tokens": ["Drum", "trag'", "ich", "mei\u00b7ne", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meinen Hirtenstab", "tokens": ["Und", "mei\u00b7nen", "Hir\u00b7ten\u00b7stab"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Geduldig bis ins Grab.", "tokens": ["Ge\u00b7dul\u00b7dig", "bis", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn ich die Orgel spiele", "tokens": ["Wenn", "ich", "die", "Or\u00b7gel", "spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll g\u00f6ttlicher Gef\u00fchle,", "tokens": ["Voll", "g\u00f6tt\u00b7li\u00b7cher", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und die Gemeinde singt,", "tokens": ["Und", "die", "Ge\u00b7mein\u00b7de", "singt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df mir's im Herzen klingt;", "tokens": ["Da\u00df", "mir's", "im", "Her\u00b7zen", "klingt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wenn Gottes Huld mir l\u00e4chelt", "tokens": ["Wenn", "Got\u00b7tes", "Huld", "mir", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Himmelsluft mich f\u00e4chelt,", "tokens": ["Und", "Him\u00b7mel\u00b7sluft", "mich", "f\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Rinnt von der Stirne hei\u00df", "tokens": ["Rinnt", "von", "der", "Stir\u00b7ne", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Herunter mir der Schwei\u00df:", "tokens": ["Her\u00b7un\u00b7ter", "mir", "der", "Schwei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So f\u00fchl' ich s\u00fc\u00dfen Frieden;", "tokens": ["So", "f\u00fchl'", "ich", "s\u00fc\u00b7\u00dfen", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und will ich auch erm\u00fcden,", "tokens": ["Und", "will", "ich", "auch", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So denk' ich an den Lohn,", "tokens": ["So", "denk'", "ich", "an", "den", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Uns beigelegt am Thron.", "tokens": ["Uns", "bei\u00b7ge\u00b7legt", "am", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sing' ich mit meinen Knaben:", "tokens": ["Sing'", "ich", "mit", "mei\u00b7nen", "Kna\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbla\u00dft uns den Leib begraben!\u00ab", "tokens": ["\u00bb", "la\u00dft", "uns", "den", "Leib", "be\u00b7gra\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vor eines Christen Grab,", "tokens": ["Vor", "ei\u00b7nes", "Chris\u00b7ten", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So blick' ich stumm hinab;", "tokens": ["So", "blick'", "ich", "stumm", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und seufz': Hier will ich schlafen", "tokens": ["Und", "seuf\u00b7z'", ":", "Hier", "will", "ich", "schla\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einst unter meinen Schafen,", "tokens": ["Einst", "un\u00b7ter", "mei\u00b7nen", "Scha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ach, nach kurzer Ruh',", "tokens": ["Und", "ach", ",", "nach", "kur\u00b7zer", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erl\u00f6ser, weckst uns du!", "tokens": ["Er\u00b7l\u00f6\u00b7ser", ",", "weckst", "uns", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}