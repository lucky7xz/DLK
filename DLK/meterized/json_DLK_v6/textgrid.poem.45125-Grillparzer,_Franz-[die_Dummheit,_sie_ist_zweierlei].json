{"textgrid.poem.45125": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die Dummheit, sie ist zweierlei]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Dummheit, sie ist zweierlei,", "tokens": ["Die", "Dumm\u00b7heit", ",", "sie", "ist", "zwei\u00b7er\u00b7lei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die tiefe und die seichte,", "tokens": ["Die", "tie\u00b7fe", "und", "die", "seich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die seichte f\u00fchlt sich frank und frei", "tokens": ["Die", "seich\u00b7te", "f\u00fchlt", "sich", "frank", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4lt sich gern ans Leichte.", "tokens": ["Und", "h\u00e4lt", "sich", "gern", "ans", "Leich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie jauchzt bei Lanner und bei Strau\u00df,", "tokens": ["Sie", "jauchzt", "bei", "Lan\u00b7ner", "und", "bei", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beklatscht die welsche Kehle,", "tokens": ["Be\u00b7k