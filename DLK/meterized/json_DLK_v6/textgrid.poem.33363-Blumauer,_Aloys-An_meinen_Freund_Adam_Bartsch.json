{"textgrid.poem.33363": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Freund Adam Bartsch", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund! hie\u00df ich Adam, so wie du,", "tokens": ["Freund", "!", "hie\u00df", "ich", "A\u00b7dam", ",", "so", "wie", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "NN", "$,", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich h\u00f6rte gern den Sp\u00f6ttern zu,", "tokens": ["Ich", "h\u00f6r\u00b7te", "gern", "den", "Sp\u00f6t\u00b7tern", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die \u00fcber Namen spassen,", "tokens": ["Die", "\u00fc\u00b7ber", "Na\u00b7men", "spas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sagen, es sei dumm gethan,", "tokens": ["Und", "sa\u00b7gen", ",", "es", "sei", "dumm", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Zum Namenstag von Jedermann", "tokens": ["Zum", "Na\u00b7mens\u00b7tag", "von", "Je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PIS"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Sich gratuliren lassen.", "tokens": ["Sich", "gra\u00b7tu\u00b7li\u00b7ren", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Sie meinen, es w\u00e4r' ein's, ob man", "tokens": ["Sie", "mei\u00b7nen", ",", "es", "w\u00e4r'", "ein's", ",", "ob", "man"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PIS", "$,", "KOUS", "PIS"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Longinus, Christoph, Kilian,", "tokens": ["Lon\u00b7gi\u00b7nus", ",", "Chris\u00b7toph", ",", "Ki\u00b7li\u00b7an", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Paul, oder Thomas hiesse;", "tokens": ["Paul", ",", "o\u00b7der", "Tho\u00b7mas", "hies\u00b7se", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NE", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ich aber wei\u00df, da\u00df von den Herr'n", "tokens": ["Ich", "a\u00b7ber", "wei\u00df", ",", "da\u00df", "von", "den", "Herr'n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich mancher seinen Namen gern", "tokens": ["Sich", "man\u00b7cher", "sei\u00b7nen", "Na\u00b7men", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "PIAT", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Vom Leibe schneiden liesse.", "tokens": ["Vom", "Lei\u00b7be", "schnei\u00b7den", "lies\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gesetzt, du hiessest Kilian,", "tokens": ["Ge\u00b7setzt", ",", "du", "hies\u00b7sest", "Ki\u00b7li\u00b7an", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein M\u00e4dchen Ursel: w\u00e4rst du dann", "tokens": ["Dein", "M\u00e4d\u00b7chen", "Ur\u00b7sel", ":", "w\u00e4rst", "du", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "$.", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht wahrlich zu beklagen?", "tokens": ["Nicht", "wahr\u00b7lich", "zu", "be\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denk nur, du m\u00fc\u00dftest in dem Schwung", "tokens": ["Denk", "nur", ",", "du", "m\u00fc\u00df\u00b7test", "in", "dem", "Schwung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der innigsten Begeisterung:", "tokens": ["Der", "in\u00b7nigs\u00b7ten", "Be\u00b7geis\u00b7te\u00b7rung", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Ach, liebste Ursel! \u2013 sagen.", "tokens": ["Ach", ",", "liebs\u00b7te", "Ur\u00b7sel", "!", "\u2013", "sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$.", "$(", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und h\u00f6rte sie's gef\u00e4llig an,", "tokens": ["Und", "h\u00f6r\u00b7te", "sie's", "ge\u00b7f\u00e4l\u00b7lig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seufzte: Ach mein Kilian!", "tokens": ["Und", "seufz\u00b7te", ":", "Ach", "mein", "Ki\u00b7li\u00b7an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ITJ", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprich, m\u00fc\u00dftest du nicht lachen?", "tokens": ["Sprich", ",", "m\u00fc\u00df\u00b7test", "du", "nicht", "la\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00fcrde nicht die Kleinigkeit", "tokens": ["Und", "w\u00fcr\u00b7de", "nicht", "die", "Klei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den allersch\u00f6nsten Liebesstreit", "tokens": ["Den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Lie\u00b7bes\u00b7streit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Possenspiele machen?", "tokens": ["Zum", "Pos\u00b7sen\u00b7spie\u00b7le", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und dann erst die Ungl\u00fccklichen,", "tokens": ["Und", "dann", "erst", "die", "Un\u00b7gl\u00fcck\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die einst Gregor den Siebenten", "tokens": ["Die", "einst", "Gre\u00b7gor", "den", "Sie\u00b7ben\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "NE", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Zum Heiligen bekamen:", "tokens": ["Zum", "Hei\u00b7li\u00b7gen", "be\u00b7ka\u00b7men", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sag, hiessen wohl die Armen gern", "tokens": ["Sag", ",", "hies\u00b7sen", "wohl", "die", "Ar\u00b7men", "gern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor aller Welt anjetzt den Herr'n", "tokens": ["Vor", "al\u00b7ler", "Welt", "an\u00b7jetzt", "den", "Herr'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dem verpappten Namen?", "tokens": ["Mit", "dem", "ver\u00b7papp\u00b7ten", "Na\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "D'rum freue deines Namens dich!", "tokens": ["D'\u00b7rum", "freu\u00b7e", "dei\u00b7nes", "Na\u00b7mens", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der wird aus dem Kalender sich", "tokens": ["Der", "wird", "aus", "dem", "Ka\u00b7len\u00b7der", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "PRF"], "meter": "-+--+-++", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zu keiner Zeit verlieren:", "tokens": ["Zu", "kei\u00b7ner", "Zeit", "ver\u00b7lie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich, du und aller Menschentro\u00df,", "tokens": ["Ich", ",", "du", "und", "al\u00b7ler", "Men\u00b7schen\u00b7tro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir m\u00fc\u00dften ja als vaterlos", "tokens": ["Wir", "m\u00fc\u00df\u00b7ten", "ja", "als", "va\u00b7ter\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dagegen protestiren.", "tokens": ["Da\u00b7ge\u00b7gen", "pro\u00b7tes\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVINF", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}}}}}