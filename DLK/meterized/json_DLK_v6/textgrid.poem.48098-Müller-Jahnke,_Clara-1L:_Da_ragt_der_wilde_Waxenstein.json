{"textgrid.poem.48098": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da ragt der wilde Waxenstein", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ragt der wilde Waxenstein", "tokens": ["Da", "ragt", "der", "wil\u00b7de", "Wa\u00b7xens\u00b7tein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "hoch in der L\u00fcfte zitterndes Blau; \u2013", "tokens": ["hoch", "in", "der", "L\u00fcf\u00b7te", "zit\u00b7tern\u00b7des", "Blau", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "zur H\u00f6lle soll dort der Eingang sein,", "tokens": ["zur", "H\u00f6l\u00b7le", "soll", "dort", "der", "Ein\u00b7gang", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "so sagte mir eine alte Frau.", "tokens": ["so", "sag\u00b7te", "mir", "ei\u00b7ne", "al\u00b7te", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich bin ihr begegnet im tiefen Tann,", "tokens": ["Ich", "bin", "ihr", "be\u00b7geg\u00b7net", "im", "tie\u00b7fen", "Tann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "dahin mich vertrieben die Mittagsglut.", "tokens": ["da\u00b7hin", "mich", "ver\u00b7trie\u00b7ben", "die", "Mit\u00b7tags\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Sie trug den kecken Tirolerhut", "tokens": ["Sie", "trug", "den", "ke\u00b7cken", "Ti\u00b7ro\u00b7ler\u00b7hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und sah mich mit lauernden Blicken an.", "tokens": ["und", "sah", "mich", "mit", "lau\u00b7ern\u00b7den", "Bli\u00b7cken", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbder Eingang zur H\u00f6lle reizt mich schier.", "tokens": ["\u00bb", "der", "Ein\u00b7gang", "zur", "H\u00f6l\u00b7le", "reizt", "mich", "schier", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Ich war im Himmel in dieser Nacht;", "tokens": ["Ich", "war", "im", "Him\u00b7mel", "in", "die\u00b7ser", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "seine selige W\u00e4rme behagte mir, \u2013", "tokens": ["sei\u00b7ne", "se\u00b7li\u00b7ge", "W\u00e4r\u00b7me", "be\u00b7hag\u00b7te", "mir", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "nun bin ich l\u00fcstern nach feuriger Pracht,", "tokens": ["nun", "bin", "ich", "l\u00fcs\u00b7tern", "nach", "feu\u00b7ri\u00b7ger", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "nach der ewigen Glut f\u00fcr Seele und Leib, \u2013", "tokens": ["nach", "der", "e\u00b7wi\u00b7gen", "Glut", "f\u00fcr", "See\u00b7le", "und", "Leib", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,", "$("], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "wo geht der Weg in die H\u00f6lle, Weib?\u00ab", "tokens": ["wo", "geht", "der", "Weg", "in", "die", "H\u00f6l\u00b7le", ",", "Weib", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und sie wies mit der d\u00fcrren vertrockneten Hand", "tokens": ["Und", "sie", "wies", "mit", "der", "d\u00fcr\u00b7ren", "ver\u00b7trock\u00b7ne\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "auf des Waxen steiltrotzige Felsenwand,", "tokens": ["auf", "des", "Wa\u00b7xen", "steil\u00b7trot\u00b7zi\u00b7ge", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "der in steinerner Ruhe, ein Warner, stand.", "tokens": ["der", "in", "stei\u00b7ner\u00b7ner", "Ru\u00b7he", ",", "ein", "War\u00b7ner", ",", "stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Um seine H\u00f6hen kein Lebenshauch,", "tokens": ["Um", "sei\u00b7ne", "H\u00f6\u00b7hen", "kein", "Le\u00b7bens\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "auf seinen Schroffen nicht Gras noch Strauch,", "tokens": ["auf", "sei\u00b7nen", "Schrof\u00b7fen", "nicht", "Gras", "noch", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "die Wache hielten hier Tod und Graun.", "tokens": ["die", "Wa\u00b7che", "hiel\u00b7ten", "hier", "Tod", "und", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und ich ging den Weg, denn ich wollte schaun.", "tokens": ["Und", "ich", "ging", "den", "Weg", ",", "denn", "ich", "woll\u00b7te", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich ging ihn sicher und stieg empor", "tokens": ["Ich", "ging", "ihn", "si\u00b7cher", "und", "stieg", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und stand vor der H\u00f6lle granitnem Tor.", "tokens": ["und", "stand", "vor", "der", "H\u00f6l\u00b7le", "gra\u00b7nit\u00b7nem", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Dreimal schlug ich mit starker Hand", "tokens": ["Drei\u00b7mal", "schlug", "ich", "mit", "star\u00b7ker", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "an die lockende, klingende, brennende Wand.", "tokens": ["an", "die", "lo\u00b7cken\u00b7de", ",", "klin\u00b7gen\u00b7de", ",", "bren\u00b7nen\u00b7de", "Wand", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Dreimal t\u00f6nte der Widerhall", "tokens": ["Drei\u00b7mal", "t\u00f6n\u00b7te", "der", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "aus der schwindelnden Tiefe wie Glockenschall.", "tokens": ["aus", "der", "schwin\u00b7deln\u00b7den", "Tie\u00b7fe", "wie", "Glo\u00b7cken\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KOKOM", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Dreimal klang er von Horst und Riff", "tokens": ["Drei\u00b7mal", "klang", "er", "von", "Horst", "und", "Riff"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "aus der schwindelnden H\u00f6he wie Geierpfiff.", "tokens": ["aus", "der", "schwin\u00b7deln\u00b7den", "H\u00f6\u00b7he", "wie", "Gei\u00b7er\u00b7pfiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KOKOM", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Als er das erstemal verhallt,", "tokens": ["Als", "er", "das", "ers\u00b7te\u00b7mal", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sah ich in dem Stein einen schmalen Spalt.", "tokens": ["sah", "ich", "in", "dem", "Stein", "ei\u00b7nen", "schma\u00b7len", "Spalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Und als er verklungen das drittemal,", "tokens": ["Und", "als", "er", "ver\u00b7klun\u00b7gen", "das", "drit\u00b7te\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "lag mir vor den Augen das H\u00f6llental.", "tokens": ["lag", "mir", "vor", "den", "Au\u00b7gen", "das", "H\u00f6l\u00b7len\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Kein loderndes Licht, keine flammende Schau,", "tokens": ["Kein", "lo\u00b7dern\u00b7des", "Licht", ",", "kei\u00b7ne", "flam\u00b7men\u00b7de", "Schau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "stahlhart die W\u00e4nde und glimmerblau.", "tokens": ["stahl\u00b7hart", "die", "W\u00e4n\u00b7de", "und", "glim\u00b7mer\u00b7blau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Von den H\u00f6hen fiel es wie Silber wei\u00df,", "tokens": ["Von", "den", "H\u00f6\u00b7hen", "fiel", "es", "wie", "Sil\u00b7ber", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "KOKOM", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und im Fallen erstarrte die Flut zu Eis.", "tokens": ["und", "im", "Fal\u00b7len", "er\u00b7starr\u00b7te", "die", "Flut", "zu", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.13": {"line.1": {"text": "Aus dem sch\u00e4umenden Schlund, wo der Wildbach tost,", "tokens": ["Aus", "dem", "sch\u00e4u\u00b7men\u00b7den", "Schlund", ",", "wo", "der", "Wild\u00b7bach", "tost", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "reckte die kn\u00f6cherne Hand der Frost.", "tokens": ["reck\u00b7te", "die", "kn\u00f6\u00b7cher\u00b7ne", "Hand", "der", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Und was er streifte, ward blinkend Eis;", "tokens": ["Und", "was", "er", "streif\u00b7te", ",", "ward", "blin\u00b7kend", "Eis", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "in der Wurzel erstarb das Edelwei\u00df.", "tokens": ["in", "der", "Wur\u00b7zel", "er\u00b7starb", "das", "E\u00b7del\u00b7wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.15": {"line.1": {"text": "In den toten Tiefen lag Schnee und Schnee,", "tokens": ["In", "den", "to\u00b7ten", "Tie\u00b7fen", "lag", "Schnee", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und mir fror das Blut, und mein Herz tat weh.", "tokens": ["und", "mir", "fror", "das", "Blut", ",", "und", "mein", "Herz", "tat", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Ein Schauder durchkroch meinen warmen Leib:", "tokens": ["Ein", "Schau\u00b7der", "durch\u00b7kroch", "mei\u00b7nen", "war\u00b7men", "Leib", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbdas ist die H\u00f6lle! Wahr sprachst du, Weib!", "tokens": ["\u00bb", "das", "ist", "die", "H\u00f6l\u00b7le", "!", "Wahr", "sprachst", "du", ",", "Weib", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$.", "NE", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.17": {"line.1": {"text": "Das ist das Ziel unsrer Pilgerbahn, \u2013", "tokens": ["Das", "ist", "das", "Ziel", "uns\u00b7rer", "Pil\u00b7ger\u00b7bahn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und die ewige ", "tokens": ["und", "die", "e\u00b7wi\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.18": {"line.1": {"text": "Mich packte der Schwindel. Mit sinkendem Blick", "tokens": ["Mich", "pack\u00b7te", "der", "Schwin\u00b7del", ".", "Mit", "sin\u00b7ken\u00b7dem", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "mit tastenden Schritten nur fand ich 