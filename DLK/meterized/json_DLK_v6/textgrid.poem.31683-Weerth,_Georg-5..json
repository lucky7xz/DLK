{"textgrid.poem.31683": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Myrten wolln wir schm\u00fccken", "tokens": ["Mit", "Myr\u00b7ten", "wolln", "wir", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Deine Stirne, so rein und licht,", "tokens": ["Dei\u00b7ne", "Stir\u00b7ne", ",", "so", "rein", "und", "licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und wollen dir K\u00fcsse dr\u00fccken", "tokens": ["Und", "wol\u00b7len", "dir", "K\u00fcs\u00b7se", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In dein liebes Angesicht.", "tokens": ["In", "dein", "lie\u00b7bes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wolln die H\u00e4nde falten", "tokens": ["Und", "wolln", "die", "H\u00e4n\u00b7de", "fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sprechen ein klein Gebet;", "tokens": ["Und", "spre\u00b7chen", "ein", "klein", "Ge\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wollen dich lieb behalten,", "tokens": ["Und", "wol\u00b7len", "dich", "lieb", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis die Sonne untergeht.", "tokens": ["Bis", "die", "Son\u00b7ne", "un\u00b7ter\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}