{"textgrid.poem.63940": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der L\u00f6we und der Esel", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebeuget unter schwere Lasten,", "tokens": ["Ge\u00b7beu\u00b7get", "un\u00b7ter", "schwe\u00b7re", "Las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mager von den vielen Fasten,", "tokens": ["Und", "ma\u00b7ger", "von", "den", "vie\u00b7len", "Fas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ging einst ein Esel \u00fcber Feld.", "tokens": ["Ging", "einst", "ein", "E\u00b7sel", "\u00fc\u00b7ber", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn sah der Thiere F\u00fcrst, der Held,", "tokens": ["Ihn", "sah", "der", "Thie\u00b7re", "F\u00fcrst", ",", "der", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der so gepries'ne L\u00f6we, gehn,", "tokens": ["Der", "so", "ge\u00b7pries'\u00b7ne", "L\u00f6\u00b7we", ",", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sprach zu ihm mit bitterm Schm\u00e4hn:", "tokens": ["Und", "sprach", "zu", "ihm", "mit", "bit\u00b7term", "Schm\u00e4hn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weich' aus, du niedertr\u00e4chtig Thier!", "tokens": ["Weich'", "aus", ",", "du", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "Thier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Man siehet seine Schand' an dir!", "tokens": ["Man", "sie\u00b7het", "sei\u00b7ne", "Schand'", "an", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du schleppest, wie man's haben will,", "tokens": ["Du", "schlep\u00b7pest", ",", "wie", "man's", "ha\u00b7ben", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIS", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und schweigst zu jeder Drohung still!", "tokens": ["Und", "schweigst", "zu", "je\u00b7der", "Dro\u00b7hung", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ruhm habt ihr noch allein von mir;", "tokens": ["Ruhm", "habt", "ihr", "noch", "al\u00b7lein", "von", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich bin des Thierreichs Schmuck und Zier,", "tokens": ["Ich", "bin", "des", "Thier\u00b7reichs", "Schmuck", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Denn mich und meine Tapferkeit", "tokens": ["Denn", "mich", "und", "mei\u00b7ne", "Tap\u00b7fer\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "R\u00fchmt man auf Erden weit und breit.", "tokens": ["R\u00fchmt", "man", "auf", "Er\u00b7den", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mit Demuth hub der Esel an:", "tokens": ["Mit", "De\u00b7muth", "hub", "der", "E\u00b7sel", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und was hat Gutes sie gethan,", "tokens": ["Und", "was", "hat", "Gu\u00b7tes", "sie", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die so ger\u00fchmte Tapferkeit?", "tokens": ["Die", "so", "ge\u00b7r\u00fchm\u00b7te", "Tap\u00b7fer\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Den Wald verheert, das Vieh zerstreut!", "tokens": ["Den", "Wald", "ver\u00b7heert", ",", "das", "Vieh", "zer\u00b7streut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ich n\u00fctze Menschen sp\u00e4t und fr\u00fch,", "tokens": ["Ich", "n\u00fct\u00b7ze", "Men\u00b7schen", "sp\u00e4t", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und du, Held, F\u00fcrst? zerrei\u00dfest sie!", "tokens": ["Und", "du", ",", "Held", ",", "F\u00fcrst", "?", "zer\u00b7rei\u00b7\u00dfest", "sie", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}