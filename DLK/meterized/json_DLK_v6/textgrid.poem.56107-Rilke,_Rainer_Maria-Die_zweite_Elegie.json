{"textgrid.poem.56107": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die zweite Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jeder Engel ist schrecklich. Und dennoch, weh mir,", "tokens": ["Je\u00b7der", "En\u00b7gel", "ist", "schreck\u00b7lich", ".", "Und", "den\u00b7noch", ",", "weh", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$.", "KON", "ADV", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-++-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "ansing ich euch, fast t\u00f6dliche V\u00f6gel der Seele,", "tokens": ["an\u00b7sing", "ich", "euch", ",", "fast", "t\u00f6d\u00b7li\u00b7che", "V\u00f6\u00b7gel", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADV", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wissend um euch. Wohin sind die Tage Tobiae,", "tokens": ["wis\u00b7send", "um", "euch", ".", "Wo\u00b7hin", "sind", "die", "Ta\u00b7ge", "To\u00b7bi\u00b7ae", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$.", "PWAV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-++-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "da der Strahlendsten einer stand an der einfachen Haust\u00fcr,", "tokens": ["da", "der", "Strah\u00b7lends\u00b7ten", "ei\u00b7ner", "stand", "an", "der", "ein\u00b7fa\u00b7chen", "Haus\u00b7t\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "zur Reise ein wenig verkleidet und schon nicht mehr furchtbar;", "tokens": ["zur", "Rei\u00b7se", "ein", "we\u00b7nig", "ver\u00b7klei\u00b7det", "und", "schon", "nicht", "mehr", "furcht\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "PIS", "VVPP", "KON", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+--+--+--+-+--", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "(j\u00fcngling dem J\u00fcngling, wie er neugierig hinaussah).", "tokens": ["(", "j\u00fcng\u00b7ling", "dem", "J\u00fcng\u00b7ling", ",", "wie", "er", "neu\u00b7gie\u00b7rig", "hin\u00b7aus\u00b7sah", ")", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$(", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Tr\u00e4te der Erzengel jetzt, der gef\u00e4hrliche, hinter den Sternen", "tokens": ["Tr\u00e4\u00b7te", "der", "Er\u00b7zen\u00b7gel", "jetzt", ",", "der", "ge\u00b7f\u00e4hr\u00b7li\u00b7che", ",", "hin\u00b7ter", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "APPR", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "eines Schrittes nur nieder und herw\u00e4rts:", "tokens": ["ei\u00b7nes", "Schrit\u00b7tes", "nur", "nie\u00b7der", "und", "her\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "KON", "ADV", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "hochaufschlagend erschl\u00fcg uns das eigene Herz. Wer seid ihr?", "tokens": ["hoch\u00b7auf\u00b7schla\u00b7gend", "er\u00b7schl\u00fcg", "uns", "das", "ei\u00b7ge\u00b7ne", "Herz", ".", "Wer", "seid", "ihr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "PWS", "VAFIN", "PPER", "$."], "meter": "-+---+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Fr\u00fche Gegl\u00fcckte, ihr Verw\u00f6hnten der Sch\u00f6pfung,", "tokens": ["Fr\u00fc\u00b7he", "Ge\u00b7gl\u00fcck\u00b7te", ",", "ihr", "Ver\u00b7w\u00f6hn\u00b7ten", "der", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "H\u00f6henz\u00fcge, morgenr\u00f6tliche Grate", "tokens": ["H\u00f6\u00b7hen\u00b7z\u00fc\u00b7ge", ",", "mor\u00b7gen\u00b7r\u00f6t\u00b7li\u00b7che", "Gra\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "aller Erschaffung, \u2013 Pollen der bl\u00fchenden Gottheit,", "tokens": ["al\u00b7ler", "Er\u00b7schaf\u00b7fung", ",", "\u2013", "Pol\u00b7len", "der", "bl\u00fc\u00b7hen\u00b7den", "Got\u00b7theit", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "$(", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Gelenke des Lichtes, G\u00e4nge, Treppen, Throne,", "tokens": ["Ge\u00b7len\u00b7ke", "des", "Lich\u00b7tes", ",", "G\u00e4n\u00b7ge", ",", "Trep\u00b7pen", ",", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "R\u00e4ume aus Wesen, Schilde aus Wonne, Tumulte", "tokens": ["R\u00e4u\u00b7me", "aus", "We\u00b7sen", ",", "Schil\u00b7de", "aus", "Won\u00b7ne", ",", "Tu\u00b7mul\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "st\u00fcrmisch entz\u00fcckten Gef\u00fchls und pl\u00f6tzlich, einzeln,", "tokens": ["st\u00fcr\u00b7misch", "ent\u00b7z\u00fcck\u00b7ten", "Ge\u00b7f\u00fchls", "und", "pl\u00f6tz\u00b7lich", ",", "ein\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "ADJD", "$,", "ADJD", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "wiedersch\u00f6pfen zur\u00fcck in das eigene Antlitz.", "tokens": ["wie\u00b7der\u00b7sch\u00f6p\u00b7fen", "zu\u00b7r\u00fcck", "in", "das", "ei\u00b7ge\u00b7ne", "Ant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Denn wir, wo wir f\u00fchlen, verfl\u00fcchtigen; ach wir", "tokens": ["Denn", "wir", ",", "wo", "wir", "f\u00fch\u00b7len", ",", "ver\u00b7fl\u00fcch\u00b7ti\u00b7gen", ";", "ach", "wir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVPP", "$.", "XY", "PPER"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "atmen uns aus und dahin; von Holzglut zu Holzglut", "tokens": ["at\u00b7men", "uns", "aus", "und", "da\u00b7hin", ";", "von", "Holz\u00b7glut", "zu", "Holz\u00b7glut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "PAV", "$.", "APPR", "NN", "APPR", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "geben wir schw\u00e4chern Geruch. Da sagt uns wohl einer:", "tokens": ["ge\u00b7ben", "wir", "schw\u00e4\u00b7chern", "Ge\u00b7ruch", ".", "Da", "sagt", "uns", "wohl", "ei\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PIS", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "ja, du gehst mir ins Blut, dieses Zimmer, der Fr\u00fchling", "tokens": ["ja", ",", "du", "gehst", "mir", "ins", "Blut", ",", "die\u00b7ses", "Zim\u00b7mer", ",", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "PDAT", "NN", "$,", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "f\u00fcllt sich mit dir... Was hilfts, er kann uns nicht halten,", "tokens": ["f\u00fcllt", "sich", "mit", "dir", "...", "Was", "hilfts", ",", "er", "kann", "uns", "nicht", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$(", "PWS", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "wir schwinden in ihm und um ihn. Und jene, die sch\u00f6n sind,", "tokens": ["wir", "schwin\u00b7den", "in", "ihm", "und", "um", "ihn", ".", "Und", "je\u00b7ne", ",", "die", "sch\u00f6n", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KON", "APPR", "PPER", "$.", "KON", "PDS", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "o wer h\u00e4lt sie zur\u00fcck? Unaufh\u00f6rlich steht Anschein", "tokens": ["o", "wer", "h\u00e4lt", "sie", "zu\u00b7r\u00fcck", "?", "Un\u00b7auf\u00b7h\u00f6r\u00b7lich", "steht", "An\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM", "PWS", "VVFIN", "PPER", "PTKVZ", "$.", "ADJD", "VVFIN", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "auf in ihrem Gesicht und geht fort. Wie Tau von dem Fr\u00fchgras", "tokens": ["auf", "in", "ih\u00b7rem", "Ge\u00b7sicht", "und", "geht", "fort", ".", "Wie", "Tau", "von", "dem", "Fr\u00fch\u00b7gras"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PTKVZ", "$.", "PWAV", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "hebt sich das Unsre von uns, wie die Hitze von einem", "tokens": ["hebt", "sich", "das", "Uns\u00b7re", "von", "uns", ",", "wie", "die", "Hit\u00b7ze", "von", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "PPER", "$,", "PWAV", "ART", "NN", "APPR", "ART"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.10": {"text": "hei\u00dfen Gericht. O L\u00e4cheln, wohin? O Aufschaun:", "tokens": ["hei\u00b7\u00dfen", "Ge\u00b7richt", ".", "O", "L\u00e4\u00b7cheln", ",", "wo\u00b7hin", "?", "O", "Auf\u00b7schaun", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "NN", "$,", "PWAV", "$.", "NE", "NN", "$."], "meter": "+--+-+--+++-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "neue, warme, entgehende Welle des Herzens \u2013;", "tokens": ["neu\u00b7e", ",", "war\u00b7me", ",", "ent\u00b7ge\u00b7hen\u00b7de", "Wel\u00b7le", "des", "Her\u00b7zens", "\u2013", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "ART", "NN", "$(", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "weh mir: wir ", "tokens": ["weh", "mir", ":", "wir"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "PPER", "$.", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "in den wir uns l\u00f6sen, nach uns? Fangen die Engel", "tokens": ["in", "den", "wir", "uns", "l\u00f6\u00b7sen", ",", "nach", "uns", "?", "Fan\u00b7gen", "die", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVINF", "$,", "APPR", "PPER", "$.", "NN", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "wirklich nur Ihriges auf, ihnen Entstr\u00f6mtes,", "tokens": ["wirk\u00b7lich", "nur", "Ih\u00b7ri\u00b7ges", "auf", ",", "ih\u00b7nen", "Ent\u00b7str\u00f6m\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PIS", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "oder ist manchmal, wie aus Versehen, ein wenig", "tokens": ["o\u00b7der", "ist", "manch\u00b7mal", ",", "wie", "aus", "Ver\u00b7se\u00b7hen", ",", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "$,", "PWAV", "APPR", "NN", "$,", "ART", "PIS"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.16": {"text": "unseres Wesens dabei? Sind wir in ihre", "tokens": ["un\u00b7se\u00b7res", "We\u00b7sens", "da\u00b7bei", "?", "Sind", "wir", "in", "ih\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PAV", "$.", "VAFIN", "PPER", "APPR", "PPOSAT"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.17": {"text": "Z\u00fcge soviel nur gemischt wie das Vage in die Gesichter", "tokens": ["Z\u00fc\u00b7ge", "so\u00b7viel", "nur", "ge\u00b7mischt", "wie", "das", "Va\u00b7ge", "in", "die", "Ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "VVPP", "KOKOM", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+--+-++-+-", "measure": "dactylic.tri.plus"}, "line.18": {"text": "schwangerer Frauen? Sie merken es nicht in dem Wirbel", "tokens": ["schwan\u00b7ge\u00b7rer", "Frau\u00b7en", "?", "Sie", "mer\u00b7ken", "es", "nicht", "in", "dem", "Wir\u00b7bel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN"], "meter": "+--+-+---+--+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "ihrer R\u00fcckkehr zu sich. (Wie sollten sie's merken.)", "tokens": ["ih\u00b7rer", "R\u00fcck\u00b7kehr", "zu", "sich", ".", "(", "Wie", "soll\u00b7ten", "sie's", "mer\u00b7ken", ".", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PRF", "$.", "$(", "PWAV", "VMFIN", "PIS", "VVINF", "$.", "$("], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Liebende k\u00f6nnten, verst\u00fcnden sie's, in der Nachtluft", "tokens": ["Lie\u00b7ben\u00b7de", "k\u00f6nn\u00b7ten", ",", "ver\u00b7st\u00fcn\u00b7den", "sie's", ",", "in", "der", "Nacht\u00b7luft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+--+-+--++", "measure": "dactylic.di.plus"}, "line.2": {"text": "wunderlich reden. Denn es scheint, da\u00df uns alles", "tokens": ["wun\u00b7der\u00b7lich", "re\u00b7den", ".", "Denn", "es", "scheint", ",", "da\u00df", "uns", "al\u00b7les"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVINF", "$.", "KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PIS"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "verheimlicht. Siehe, die B\u00e4ume ", "tokens": ["ver\u00b7heim\u00b7licht", ".", "Sie\u00b7he", ",", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVPP", "$.", "VVIMP", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die wir bewohnen, bestehn noch. Wir nur", "tokens": ["die", "wir", "be\u00b7woh\u00b7nen", ",", "be\u00b7stehn", "noch", ".", "Wir", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVINF", "$,", "VVFIN", "ADV", "$.", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ziehen allem vorbei wie ein luftiger Austausch.", "tokens": ["zie\u00b7hen", "al\u00b7lem", "vor\u00b7bei", "wie", "ein", "luf\u00b7ti\u00b7ger", "Aus\u00b7tausch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Und alles ist einig, uns zu verschweigen,