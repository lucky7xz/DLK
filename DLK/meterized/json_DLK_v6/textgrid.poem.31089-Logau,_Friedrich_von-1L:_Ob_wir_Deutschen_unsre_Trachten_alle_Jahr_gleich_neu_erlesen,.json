{"textgrid.poem.31089": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob wir Deutschen unsre Trachten alle Jahr gleich neu erlesen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob wir Deutschen unsre Trachten alle Jahr gleich neu erlesen,", "tokens": ["Ob", "wir", "Deut\u00b7schen", "uns\u00b7re", "Trach\u00b7ten", "al\u00b7le", "Jahr", "gleich", "neu", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PPOSAT", "NN", "PIAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dennoch ist noch nimmer keine nur ein Jahr durch recht gewesen;", "tokens": ["Den\u00b7noch", "ist", "noch", "nim\u00b7mer", "kei\u00b7ne", "nur", "ein", "Jahr", "durch", "recht", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PIAT", "ADV", "ART", "NN", "APPR", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Abends f\u00fcr dem j\u00fcngsten Tage, was wir damals, wil ich gl\u00e4uben,", "tokens": ["A\u00b7bends", "f\u00fcr", "dem", "j\u00fcng\u00b7sten", "Ta\u00b7ge", ",", "was", "wir", "da\u00b7mals", ",", "wil", "ich", "gl\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Werden zu der Tracht erwehlen, wird ja m\u00fcssen endlich bleiben.", "tokens": ["Wer\u00b7den", "zu", "der", "Tracht", "er\u00b7weh\u00b7len", ",", "wird", "ja", "m\u00fcs\u00b7sen", "end\u00b7lich", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVINF", "$,", "VAFIN", "ADV", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}}}}