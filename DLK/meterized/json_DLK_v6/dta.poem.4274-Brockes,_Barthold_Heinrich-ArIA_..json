{"dta.poem.4274": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ArIA .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erde bebt, der Boden sch\u00fcttert,", "tokens": ["Die", "Er\u00b7de", "bebt", ",", "der", "Bo\u00b7den", "sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Acker untern F\u00fcssen zittert", "tokens": ["Der", "A\u00b7cker", "un\u00b7tern", "F\u00fcs\u00b7sen", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor des Getraydes Segens-Last.", "tokens": ["Vor", "des", "Ge\u00b7tray\u00b7des", "Se\u00b7gens\u00b7Last", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns sey, bey diesen frohen Zeiten,", "tokens": ["Uns", "sey", ",", "bey", "die\u00b7sen", "fro\u00b7hen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Undankbarkeit und Gram verha\u00dft;", "tokens": ["Un\u00b7dank\u00b7bar\u00b7keit", "und", "Gram", "ver\u00b7ha\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Man sp\u00fchre nichts, als Fr\u00f6hlichkeiten!", "tokens": ["Man", "sp\u00fch\u00b7re", "nichts", ",", "als", "Fr\u00f6h\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der frohe Fuhrmann jauchzt und singt,", "tokens": ["Der", "fro\u00b7he", "Fuhr\u00b7mann", "jauchzt", "und", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kurze Knall der Geissel klingt,", "tokens": ["Der", "kur\u00b7ze", "Knall", "der", "Geis\u00b7sel", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis er das Korn zum Scheun-Thor bringt,", "tokens": ["Bis", "er", "das", "Korn", "zum", "Scheun\u00b7Thor", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wodurch es sich, mit M\u00fche, dringt.", "tokens": ["Wo\u00b7durch", "es", "sich", ",", "mit", "M\u00fc\u00b7he", ",", "dringt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "$,", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es m\u00fcssen sich die Garben beugen,", "tokens": ["Es", "m\u00fcs\u00b7sen", "sich", "die", "Gar\u00b7ben", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dieweil das Thor f\u00fcr sie zu klein,", "tokens": ["Die\u00b7weil", "das", "Thor", "f\u00fcr", "sie", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sie so hoch geladen seyn,", "tokens": ["Und", "sie", "so", "hoch", "ge\u00b7la\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum m\u00fcssen sie sich zischend neigen.", "tokens": ["Drum", "m\u00fcs\u00b7sen", "sie", "sich", "zi\u00b7schend", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dann schw\u00e4rzet sich des Tages Schein,", "tokens": ["Dann", "schw\u00e4r\u00b7zet", "sich", "des", "Ta\u00b7ges", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es nimmt ein angenehmer Schatten", "tokens": ["Es", "nimmt", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Das ganze Vorwerk pl\u00f6tzlich ein,", "tokens": ["Das", "gan\u00b7ze", "Vor\u00b7werk", "pl\u00f6tz\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Pferde zieh\u2019n, und stehen kaum,", "tokens": ["Die", "Pfer\u00b7de", "zieh'n", ",", "und", "ste\u00b7hen", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als man den angezw\u00e4ngten Baum,", "tokens": ["Als", "man", "den", "an\u00b7ge\u00b7zw\u00e4ng\u00b7ten", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den sie ganz krumm gezogen hatten,", "tokens": ["Den", "sie", "ganz", "krumm", "ge\u00b7zo\u00b7gen", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Erl\u00f6set und die Freyheit schenkt.", "tokens": ["Er\u00b7l\u00f6\u00b7set", "und", "die", "Frey\u00b7heit", "schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Worauf sodenn ein muntrer Knecht mit H\u00e4nden an das", "tokens": ["Wo\u00b7rauf", "so\u00b7denn", "ein", "mun\u00b7trer", "Knecht", "mit", "H\u00e4n\u00b7den", "an", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seil sich henket,", "tokens": ["Seil", "sich", "hen\u00b7ket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und oben auf den Wagen schwenket,", "tokens": ["Und", "o\u00b7ben", "auf", "den", "Wa\u00b7gen", "schwen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einer Gabel, die sich kr\u00fcmmt,", "tokens": ["Mit", "ei\u00b7ner", "Ga\u00b7bel", ",", "die", "sich", "kr\u00fcmmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Garben in die H\u00f6he hebt,", "tokens": ["Die", "Gar\u00b7ben", "in", "die", "H\u00f6\u00b7he", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wodurch er sich herunter gr\u00e4bt,", "tokens": ["Wo\u00b7durch", "er", "sich", "her\u00b7un\u00b7ter", "gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APZR", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sie dahin zu reichen strebet,", "tokens": ["Und", "sie", "da\u00b7hin", "zu", "rei\u00b7chen", "stre\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo man sie von der Gabel nimmt,", "tokens": ["Wo", "man", "sie", "von", "der", "Ga\u00b7bel", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie da, woselbst sie hin bestimmt,", "tokens": ["Sie", "da", ",", "wo\u00b7selbst", "sie", "hin", "be\u00b7stimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auf den erhabnen Balken tr\u00e4gt,", "tokens": ["Auf", "den", "er\u00b7hab\u00b7nen", "Bal\u00b7ken", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und sie daselbst in Ordnung legt.", "tokens": ["Und", "sie", "da\u00b7selbst", "in", "Ord\u00b7nung", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}