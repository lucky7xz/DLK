{"textgrid.poem.34911": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Geoffroy Rud\u00e8l und Melisande von Tripoli", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Schlosse Blay erblickt man", "tokens": ["In", "dem", "Schlos\u00b7se", "Blay", "er\u00b7blickt", "man"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Tapete an den W\u00e4nden,", "tokens": ["Die", "Ta\u00b7pe\u00b7te", "an", "den", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So die Gr\u00e4fin Tripolis", "tokens": ["So", "die", "Gr\u00e4\u00b7fin", "Tri\u00b7po\u00b7lis"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst gestickt mit klugen H\u00e4nden.", "tokens": ["Einst", "ge\u00b7stickt", "mit", "klu\u00b7gen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihre ganze Seele stickte", "tokens": ["Ih\u00b7re", "gan\u00b7ze", "See\u00b7le", "stick\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie hinein, und Liebestr\u00e4ne", "tokens": ["Sie", "hin\u00b7ein", ",", "und", "Lie\u00b7be\u00b7str\u00e4\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat gefeit das seidne Bildwerk,", "tokens": ["Hat", "ge\u00b7feit", "das", "seid\u00b7ne", "Bild\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welches darstellt jene Szene:", "tokens": ["Wel\u00b7ches", "dar\u00b7stellt", "je\u00b7ne", "Sze\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie die Gr\u00e4fin den Rud\u00e8l,", "tokens": ["Wie", "die", "Gr\u00e4\u00b7fin", "den", "Ru\u00b7d\u00e8l", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.2": {"text": "Sterbend sah am Strande liegen,", "tokens": ["Ster\u00b7bend", "sah", "am", "Stran\u00b7de", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Urbild ihrer Sehnsucht", "tokens": ["Und", "das", "Ur\u00b7bild", "ih\u00b7rer", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich erkannt in seinen Z\u00fcgen.", "tokens": ["Gleich", "er\u00b7kannt", "in", "sei\u00b7nen", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch Rud\u00e8l hat hier zum ersten", "tokens": ["Auch", "Ru\u00b7d\u00e8l", "hat", "hier", "zum", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VAFIN", "ADV", "APPRART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und zum letzten Mal erblicket", "tokens": ["Und", "zum", "letz\u00b7ten", "Mal", "er\u00b7bli\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Wirklichkeit die Dame,", "tokens": ["In", "der", "Wirk\u00b7lich\u00b7keit", "die", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihn oft im Traum entz\u00fccket.", "tokens": ["Die", "ihn", "oft", "im", "Traum", "ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00dcber ihn beugt sich die Gr\u00e4fin,", "tokens": ["\u00dc\u00b7ber", "ihn", "beugt", "sich", "die", "Gr\u00e4\u00b7fin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "H\u00e4lt ihn liebevoll umschlungen,", "tokens": ["H\u00e4lt", "ihn", "lie\u00b7be\u00b7voll", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fc\u00dft den todesbleichen Mund,", "tokens": ["K\u00fc\u00dft", "den", "to\u00b7des\u00b7blei\u00b7chen", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der so sch\u00f6n ihr Lob gesungen!", "tokens": ["Der", "so", "sch\u00f6n", "ihr", "Lob", "ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! der Ku\u00df des Willkomms wurde", "tokens": ["Ach", "!", "der", "Ku\u00df", "des", "Will\u00b7komms", "wur\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch zugleich der Ku\u00df des Scheidens,", "tokens": ["Auch", "zu\u00b7gleich", "der", "Ku\u00df", "des", "Schei\u00b7dens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so leerten sie den Kelch", "tokens": ["Und", "so", "leer\u00b7ten", "sie", "den", "Kelch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6chster Lust und tiefsten Leidens.", "tokens": ["H\u00f6chs\u00b7ter", "Lust", "und", "tiefs\u00b7ten", "Lei\u00b7dens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In dem Schlosse Blay alln\u00e4chtlich", "tokens": ["In", "dem", "Schlos\u00b7se", "Blay", "all\u00b7n\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gibt's ein Rauschen, Knistern, Beben,", "tokens": ["Gibt's", "ein", "Rau\u00b7schen", ",", "Knis\u00b7tern", ",", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Figuren der Tapete", "tokens": ["Die", "Fi\u00b7gu\u00b7ren", "der", "Ta\u00b7pe\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "--++-+--", "measure": "anapaest.init"}, "line.4": {"text": "Fangen pl\u00f6tzlich an zu leben.", "tokens": ["Fan\u00b7gen", "pl\u00f6tz\u00b7lich", "an", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Troubadour und Dame sch\u00fctteln", "tokens": ["Trou\u00b7ba\u00b7dour", "und", "Da\u00b7me", "sch\u00fct\u00b7teln"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verschlafnen Schattenglieder,", "tokens": ["Die", "ver\u00b7schlaf\u00b7nen", "Schat\u00b7ten\u00b7glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treten aus der Wand und wandeln", "tokens": ["Tre\u00b7ten", "aus", "der", "Wand", "und", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die S\u00e4le auf und nieder.", "tokens": ["Durch", "die", "S\u00e4\u00b7le", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Trautes Fl\u00fcstern, sanftes T\u00e4ndeln,", "tokens": ["Trau\u00b7tes", "Fl\u00fcs\u00b7tern", ",", "sanf\u00b7tes", "T\u00e4n\u00b7deln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wehmuts\u00fc\u00dfe Heimlichkeiten,", "tokens": ["Weh\u00b7mut\u00b7s\u00fc\u00b7\u00dfe", "Heim\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und postume Galantrie", "tokens": ["Und", "pos\u00b7tu\u00b7me", "Ga\u00b7lan\u00b7trie"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Minnesanges Zeiten:", "tokens": ["Aus", "des", "Min\u00b7ne\u00b7san\u00b7ges", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbgeoffroy! Mein totes Herz", "tokens": ["\u00bb", "ge\u00b7off\u00b7roy", "!", "Mein", "to\u00b7tes", "Herz"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVPP", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wird erw\u00e4rmt von deiner Stimme,", "tokens": ["Wird", "er\u00b7w\u00e4rmt", "von", "dei\u00b7ner", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den l\u00e4ngst erloschnen Kohlen", "tokens": ["In", "den", "l\u00e4ngst", "er\u00b7lo\u00b7schnen", "Koh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchl ich wieder ein Geglimme!\u00ab", "tokens": ["F\u00fchl", "ich", "wie\u00b7der", "ein", "Ge\u00b7glim\u00b7me", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbmelisande! Gl\u00fcck und Blume!", "tokens": ["\u00bb", "me\u00b7li\u00b7san\u00b7de", "!", "Gl\u00fcck", "und", "Blu\u00b7me", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich dir ins Auge sehe,", "tokens": ["Wenn", "ich", "dir", "ins", "Au\u00b7ge", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leb ich auf \u2013 gestorben ist", "tokens": ["Leb", "ich", "auf", "\u2013", "ge\u00b7stor\u00b7ben", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "$(", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur mein Erdenleid und \u2013 wehe.\u00ab", "tokens": ["Nur", "mein", "Er\u00b7den\u00b7leid", "und", "\u2013", "we\u00b7he", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "$(", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbgeoffroy! Wir liebten uns", "tokens": ["\u00bb", "ge\u00b7off\u00b7roy", "!", "Wir", "lieb\u00b7ten", "uns"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVPP", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einst im Traume, und jetzunder", "tokens": ["Einst", "im", "Trau\u00b7me", ",", "und", "je\u00b7tzun\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieben wir uns gar im Tode \u2013", "tokens": ["Lie\u00b7ben", "wir", "uns", "gar", "im", "To\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "PPER", "PRF", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott Amour tat dieses Wunder!\u00ab", "tokens": ["Gott", "A\u00b7mour", "tat", "die\u00b7ses", "Wun\u00b7der", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "VVFIN", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "\u00bbmelisande! Was ist Traum?", "tokens": ["\u00bb", "me\u00b7li\u00b7san\u00b7de", "!", "Was", "ist", "Traum", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "PWS", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ist Tod? Nur eitel T\u00f6ne.", "tokens": ["Was", "ist", "Tod", "?", "Nur", "ei\u00b7tel", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "ADV", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Liebe nur ist Wahrheit,", "tokens": ["In", "der", "Lie\u00b7be", "nur", "ist", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dich lieb ich, ewig Sch\u00f6ne.\u00ab", "tokens": ["Und", "dich", "lieb", "ich", ",", "e\u00b7wig", "Sch\u00f6\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "\u00bbgeoffroy! Wie traulich ist es", "tokens": ["\u00bb", "ge\u00b7off\u00b7roy", "!", "Wie", "trau\u00b7lich", "ist", "es"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "$.", "PWAV", "ADJD", "VAFIN", "PPER"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Hier im stillen Mondscheinsaale,", "tokens": ["Hier", "im", "stil\u00b7len", "Mond\u00b7schein\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6chte nicht mehr drau\u00dfen wandeln", "tokens": ["M\u00f6ch\u00b7te", "nicht", "mehr", "drau\u00b7\u00dfen", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Tages Sonnenstrahle.\u00ab", "tokens": ["In", "des", "Ta\u00b7ges", "Son\u00b7nen\u00b7strah\u00b7le", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbmelisande! teure N\u00e4rrin,", "tokens": ["\u00bb", "me\u00b7li\u00b7san\u00b7de", "!", "teu\u00b7re", "N\u00e4r\u00b7rin", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bist selber Licht und Sonne,", "tokens": ["Du", "bist", "sel\u00b7ber", "Licht", "und", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wo du wandelst, bl\u00fcht der Fr\u00fchling,", "tokens": ["Wo", "du", "wan\u00b7delst", ",", "bl\u00fcht", "der", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprossen Lieb' und Maienwonne!\u00ab", "tokens": ["Spros\u00b7sen", "Lieb'", "und", "Mai\u00b7en\u00b7won\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Also kosen, also wandeln", "tokens": ["Al\u00b7so", "ko\u00b7sen", ",", "al\u00b7so", "wan\u00b7deln"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene z\u00e4rtlichen Gespenster", "tokens": ["Je\u00b7ne", "z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf und ab, derweil das Mondlicht", "tokens": ["Auf", "und", "ab", ",", "der\u00b7weil", "das", "Mond\u00b7licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "KON", "PTKVZ", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauschet durch die Bogenfenster.", "tokens": ["Lau\u00b7schet", "durch"