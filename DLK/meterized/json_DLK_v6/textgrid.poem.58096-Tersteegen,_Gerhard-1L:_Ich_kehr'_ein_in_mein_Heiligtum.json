{"textgrid.poem.58096": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kehr' ein in mein Heiligtum", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kehr' ein in mein Heiligtum", "tokens": ["Ich", "kehr'", "ein", "in", "mein", "Hei\u00b7lig\u00b7tum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bleib' in Liebesehrfurcht stumm;", "tokens": ["Und", "bleib'", "in", "Lie\u00b7be\u00b7sehr\u00b7furcht", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In 's Geistes stiller Dunkelheit,", "tokens": ["In", "'s", "Geis\u00b7tes", "stil\u00b7ler", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Von Kreatur und Sinnen weit,", "tokens": ["Von", "Kre\u00b7a\u00b7tur", "und", "Sin\u00b7nen", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da wohnet Gottes Klarheit,", "tokens": ["Da", "woh\u00b7net", "Got\u00b7tes", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da schaut man Gott in Wahrheit.", "tokens": ["Da", "schaut", "man", "Gott", "in", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}