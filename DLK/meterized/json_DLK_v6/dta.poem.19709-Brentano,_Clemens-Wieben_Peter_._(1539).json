{"dta.poem.19709": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wieben Peter . (1539)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Will jy h\u00f6ren en nie Gedicht,               ", "tokens": ["Will", "jy", "h\u00f6\u00b7ren", "en", "nie", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wat kortelich is uthgericht,", "tokens": ["Wat", "kor\u00b7te\u00b7lich", "is", "uth\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darvan will ick jy singen,", "tokens": ["Dar\u00b7van", "will", "ick", "jy", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "En Mann is Wieben Peter genandt,", "tokens": ["En", "Mann", "is", "Wie\u00b7ben", "Pe\u00b7ter", "ge\u00b7nandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "FM", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "De Dithmarscher wolde he dwingen.", "tokens": ["De", "Dith\u00b7mar\u00b7scher", "wol\u00b7de", "he", "dwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "He t\u00f6g wol ut sin Vaterland,", "tokens": ["He", "t\u00f6g", "wol", "ut", "sin", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darup het he gerovet und gebrant,", "tokens": ["Da\u00b7rup", "het", "he", "ge\u00b7ro\u00b7vet", "und", "ge\u00b7brant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NE", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Gewalt veel Gudes genahmen,", "tokens": ["Mit", "Ge\u00b7walt", "veel", "Gu\u00b7des", "ge\u00b7nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NE", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Etliche gefangen und weggef\u00f6hrt,", "tokens": ["Et\u00b7li\u00b7che", "ge\u00b7fan\u00b7gen", "und", "weg\u00b7ge\u00b7f\u00f6hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Is nu to Uhtdracht kamen.", "tokens": ["Is", "nu", "to", "Uht\u00b7dracht", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "He hefft sick Hans Pommerening genannt,", "tokens": ["He", "hefft", "sick", "Hans", "Pom\u00b7me\u00b7re\u00b7ning", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "NE", "NE", "VVPP", "$,"], "meter": "-+-----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Hefft Schaepstette sulvest abgebrannt,", "tokens": ["Hefft", "Schaeps\u00b7tet\u00b7te", "sul\u00b7vest", "ab\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit sienen Broder und Knechten,", "tokens": ["Mit", "sie\u00b7nen", "Bro\u00b7der", "und", "Knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dat wareden de Acht and Vertig gewahr,", "tokens": ["Dat", "wa\u00b7re\u00b7den", "de", "Acht", "and", "Ver\u00b7tig", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "CARD", "PIS", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "De Sacke m\u00fcste he verfechten.", "tokens": ["De", "Sa\u00b7cke", "m\u00fcs\u00b7te", "he", "ver\u00b7fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Darna wart he gefangen fzon,", "tokens": ["Dar\u00b7na", "wart", "he", "ge\u00b7fan\u00b7gen", "fzon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dat man em szolde geven sin Lohn,", "tokens": ["Dat", "man", "em", "szol\u00b7de", "ge\u00b7ven", "sin", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Na sinen Verdenst und Rechte,", "tokens": ["Na", "si\u00b7nen", "Ver\u00b7denst", "und", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "To Rendsburg ward he gefunden lo\u00df", "tokens": ["To", "Rends\u00b7burg", "ward", "he", "ge\u00b7fun\u00b7den", "lo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VAFIN", "NE", "VVPP", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Van adelichem Geschlechte.", "tokens": ["Van", "a\u00b7de\u00b7li\u00b7chem", "Ge\u00b7schlech\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Idt wahrde nicht gar lange Tydt", "tokens": ["Idt", "wahr\u00b7de", "nicht", "gar", "lan\u00b7ge", "Tydt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PTKNEG", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "T\u00f6g he in d\u00fctschen Lande wiht", "tokens": ["T\u00f6g", "he", "in", "d\u00fct\u00b7schen", "Lan\u00b7de", "wiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Na Carol dem R\u00f6mischen Kaiser,", "tokens": ["Na", "Ca\u00b7rol", "dem", "R\u00f6\u00b7mi\u00b7schen", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ammer sine Mandata to hahlen dar,", "tokens": ["Am\u00b7mer", "si\u00b7ne", "Man\u00b7da\u00b7ta", "to", "hah\u00b7len", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "FM.it", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Ungl\u00fcck war sine Reise.", "tokens": ["Un\u00b7gl\u00fcck", "war", "si\u00b7ne", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Den Acht und Vertig is Badeschop gekohmen,", "tokens": ["Den", "Acht", "und", "Ver\u00b7tig", "is", "Ba\u00b7de\u00b7schop", "ge\u00b7koh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "KON", "ADJD", "FM", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wieben Peter hedde Knechte angenohmen", "tokens": ["Wie\u00b7ben", "Pe\u00b7ter", "hed\u00b7de", "Knech\u00b7te", "an\u00b7ge\u00b7noh\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "To Jevern in Frehen Lande,", "tokens": ["To", "Je\u00b7vern", "in", "Fre\u00b7hen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darmit wolde he up de Dithmarscher nehmen,", "tokens": ["Dar\u00b7mit", "wol\u00b7de", "he", "up", "de", "Dith\u00b7mar\u00b7scher", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "FM", "FM", "FM", "FM", "FM", "VVINF", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und dohn enen weh und bange.", "tokens": ["Und", "dohn", "e\u00b7nen", "weh", "und", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}