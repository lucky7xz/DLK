{"textgrid.poem.48721": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. Antwort auf Herrn Tycho von Jessen sein mir \u00fcbersendetes Sonnet", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesetzt, das doch nicht ist, was deine milde Gunst", "tokens": ["Ge\u00b7setzt", ",", "das", "doch", "nicht", "ist", ",", "was", "dei\u00b7ne", "mil\u00b7de", "Gunst"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PRELS", "ADV", "PTKNEG", "VAFIN", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von meiner Poesie, dem Kinderwerke richtet,", "tokens": ["von", "mei\u00b7ner", "Poe\u00b7sie", ",", "dem", "Kin\u00b7der\u00b7wer\u00b7ke", "rich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "sie sei die, wie du sprichst, ich habe was gedichtet,", "tokens": ["sie", "sei", "die", ",", "wie", "du", "sprichst", ",", "ich", "ha\u00b7be", "was", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es etwah Lobes wert und schmecke nach der Kunst,", "tokens": ["Es", "et\u00b7wah", "Lo\u00b7bes", "wert", "und", "schme\u00b7cke", "nach", "der", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "befahr dich nichts f\u00fcr mir. Mein' ungeschickte Dunst", "tokens": ["be\u00b7fahr", "dich", "nichts", "f\u00fcr", "mir", ".", "Mein'", "un\u00b7ge\u00b7schick\u00b7te", "Dunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "PPER", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "d\u00e4mpft deine Lohe nicht, du hast dich mehr verpflichtet,", "tokens": ["d\u00e4mpft", "dei\u00b7ne", "Lo\u00b7he", "nicht", ",", "du", "hast", "dich", "mehr", "ver\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie sehr du ihre Glut hiermit auch fast vernichtet,", "tokens": ["wie", "sehr", "du", "ih\u00b7re", "Glut", "hier\u00b7mit", "auch", "fast", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN", "PAV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df dein Gem\u00fcte brennt von einer sondern Brunst.", "tokens": ["da\u00df", "dein", "Ge\u00b7m\u00fc\u00b7te", "brennt", "von", "ei\u00b7ner", "son\u00b7dern", "Brunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Setz' an, Freund, fahre fort. Die deutschen Kastalinnen,", "tokens": ["Setz'", "an", ",", "Freund", ",", "fah\u00b7re", "fort", ".", "Die", "deut\u00b7schen", "Kas\u00b7ta\u00b7lin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mein Ruhm, so viel des ist, bed\u00fcrfen solcher Sinnen,", "tokens": ["mein", "Ruhm", ",", "so", "viel", "des", "ist", ",", "be\u00b7d\u00fcr\u00b7fen", "sol\u00b7cher", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADV", "ART", "VAFIN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die deinen \u00e4hnlich sind. Zu dem ist dir bewu\u00dft,", "tokens": ["die", "dei\u00b7nen", "\u00e4hn\u00b7lich", "sind", ".", "Zu", "dem", "ist", "dir", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "VAFIN", "$.", "APPR", "ART", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "da\u00df unser Lorbeerbaum steht Allen in die Mitten.", "tokens": ["da\u00df", "un\u00b7ser", "Lor\u00b7beer\u00b7baum", "steht", "Al\u00b7len", "in", "die", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Brich ab und nim dir selbst, leg hin die bl\u00f6den Sitten.", "tokens": ["Brich", "ab", "und", "nim", "dir", "selbst", ",", "leg", "hin", "die", "bl\u00f6\u00b7den", "Sit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVIMP", "PPER", "ADV", "$,", "NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du selbst bist dir der Weg zu unsers Pindus Lust.", "tokens": ["Du", "selbst", "bist", "dir", "der", "Weg", "zu", "un\u00b7sers", "Pin\u00b7dus", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}