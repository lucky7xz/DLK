{"textgrid.poem.60906": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier im antiken Gew\u00f6lb, wo rings noch Scherben von Urnen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier im antiken Gew\u00f6lb, wo rings noch Scherben von Urnen", "tokens": ["Hier", "im", "an\u00b7ti\u00b7ken", "Ge\u00b7w\u00f6lb", ",", "wo", "rings", "noch", "Scher\u00b7ben", "von", "Ur\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "PWAV", "ADV", "ADV", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Stehn in den Nischen umher, keltert der Bauer den Wein:", "tokens": ["Stehn", "in", "den", "Ni\u00b7schen", "um\u00b7her", ",", "kel\u00b7tert", "der", "Bau\u00b7er", "den", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Unsere Gr\u00e4ber beleuchtet, o Freund, kein sonniger Strahl einst,", "tokens": ["Un\u00b7se\u00b7re", "Gr\u00e4\u00b7ber", "be\u00b7leuch\u00b7tet", ",", "o", "Freund", ",", "kein", "son\u00b7ni\u00b7ger", "Strahl", "einst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "FM", "NN", "$,", "PIAT", "ADJA", "NN", "ADV", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "K\u00fcnftigen werden sie nie dienen zu s\u00fc\u00dfem Gebrauch!", "tokens": ["K\u00fcnf\u00b7ti\u00b7gen", "wer\u00b7den", "sie", "nie", "die\u00b7nen", "zu", "s\u00fc\u00b7\u00dfem", "Ge\u00b7brauch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVINF", "PTKZU", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Modergeruch nur hauchen sie aus, die blo\u00df der Verwesung,", "tokens": ["Mo\u00b7der\u00b7ge\u00b7ruch", "nur", "hau\u00b7chen", "sie", "aus", ",", "die", "blo\u00df", "der", "Ver\u00b7we\u00b7sung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "ADV", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Blo\u00df dem Gew\u00fcrm schmachvoll unter der Erde geweiht.", "tokens": ["Blo\u00df", "dem", "Ge\u00b7w\u00fcrm", "schmach\u00b7voll", "un\u00b7ter", "der", "Er\u00b7de", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}