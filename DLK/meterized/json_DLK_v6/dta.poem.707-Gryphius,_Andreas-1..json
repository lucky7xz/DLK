{"dta.poem.707": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcsses Kind/ der V\u00e4tter hoffen: ", "tokens": ["S\u00fcs\u00b7ses", "Kind", "/", "der", "V\u00e4t\u00b7ter", "hof\u00b7fen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kind der Menschen L\u00f6segeldt/", "tokens": ["Kind", "der", "Men\u00b7schen", "L\u00f6\u00b7se\u00b7geldt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht der Himmel nunmehr offen?", "tokens": ["Steht", "der", "Him\u00b7mel", "nun\u00b7mehr", "of\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liefert ", "tokens": ["Lie\u00b7fert"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Heyland wirst du nun gebohren ", "tokens": ["Hey\u00b7land", "wirst", "du", "nun", "ge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu erl\u00f6sen was verlohren? ", "tokens": ["Zu", "er\u00b7l\u00f6\u00b7sen", "was", "ver\u00b7loh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PWS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Den eh\u2019 ewig angebrochen", "tokens": ["Den", "eh'", "e\u00b7wig", "an\u00b7ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "GoTT jhm ewig gleich gebahr:", "tokens": ["GoTT", "jhm", "e\u00b7wig", "gleich", "ge\u00b7bahr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wortt das GOTT hat au\u00dfgesprochen", "tokens": ["Wortt", "das", "GoTT", "hat", "au\u00df\u00b7ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das im anfang war/ vnd wahr;", "tokens": ["Das", "im", "an\u00b7fang", "war", "/", "vnd", "wahr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "VAFIN", "$(", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "GoTT das ", "tokens": ["GoTT", "das"], "token_info": ["word", "word"], "pos": ["NE", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Wunder! GOtt soll ", "tokens": ["Wun\u00b7der", "!", "Gott", "soll"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "VMFIN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Heiligkeit der h\u00f6chsten G\u00fctte! ", "tokens": ["Hei\u00b7lig\u00b7keit", "der", "h\u00f6chs\u00b7ten", "G\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! verl\u00e4st du deinen Thron!", "tokens": ["Ach", "!", "ver\u00b7l\u00e4st", "du", "dei\u00b7nen", "Thron", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie entsetzt sich mein Gem\u00fctte! ", "tokens": ["Wie", "ent\u00b7setzt", "sich", "mein", "Ge\u00b7m\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird GOtt eines Menschen Sohn?", "tokens": ["Wird", "Gott", "ei\u00b7nes", "Men\u00b7schen", "Sohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Den nichts was er schuff kan schlissen: ", "tokens": ["Den", "nichts", "was", "er", "schuff", "kan", "schlis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Kan die zarte Jungfer k\u00fcssen. ", "tokens": ["Kan", "die", "zar\u00b7te", "Jung\u00b7fer", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! Er komm\u2019t/ er wird gebohren", "tokens": ["Ach", "!", "Er", "kom\u00b7m't", "/", "er", "wird", "ge\u00b7boh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$(", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil der bleiche Monden wacht", "tokens": ["Weil", "der", "blei\u00b7che", "Mon\u00b7den", "wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dem Liecht sein Liecht verlohren/", "tokens": ["Vor", "dem", "Liecht", "sein", "Liecht", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt verh\u00fcll\u2019t mit schwartzer Nacht.", "tokens": ["Kommt", "ver\u00b7h\u00fcll't", "mit", "schwart\u00b7zer", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den viel tausend Jahr begehret:", "tokens": ["Den", "viel", "tau\u00b7send", "Jahr", "be\u00b7ge\u00b7hret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wird eh\u2019 als man meynt beschehret.", "tokens": ["Wird", "eh'", "als", "man", "meynt", "be\u00b7scheh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "KOUS", "PIS", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch er wird/ den alle kennen", "tokens": ["Doch", "er", "wird", "/", "den", "al\u00b7le", "ken\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$(", "ART", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht von seinem Volck\u2019 erkannt.", "tokens": ["Nicht", "von", "sei\u00b7nem", "Volck'", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die Welt sein Hau\u00df kan nennen ", "tokens": ["Der", "die", "Welt", "sein", "Hau\u00df", "kan", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wird in einen Stall verbannt/", "tokens": ["Wird", "in", "ei\u00b7nen", "Stall", "ver\u00b7bannt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der der Erden grund beweget", "tokens": ["Der", "der", "Er\u00b7den", "grund", "be\u00b7we\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird auff d\u00fcrres Hew geleget. ", "tokens": ["Wird", "auff", "d\u00fcr\u00b7res", "Hew", "ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "NE", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dem der Donner zu gebotte", "tokens": ["Dem", "der", "Don\u00b7ner", "zu", "ge\u00b7bot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem der Blitz zu dinste steht ", "tokens": ["Dem", "der", "Blitz", "zu", "dins\u00b7te", "steht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der an Macht dem h\u00f6chsten GOtte", "tokens": ["Der", "an", "Macht", "dem", "h\u00f6chs\u00b7ten", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Al\u00df an wesen gleiche geht", "tokens": ["Al\u00df", "an", "we\u00b7sen", "glei\u00b7che", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PWAT", "ADJA", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der was ist vnd ward/ gebawet", "tokens": ["Der", "was", "ist", "vnd", "ward", "/", "ge\u00b7ba\u00b7wet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PIS", "VAFIN", "KON", "VAFIN", "$(", "VVPP"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wird hier al\u00df ein Kind geschawet.", "tokens": ["Wird", "hier", "al\u00df", "ein", "Kind", "ge\u00b7scha\u00b7wet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Kan der ", "tokens": ["Kan", "der"], "token_info": ["word", "word"], "pos": ["VMFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Kan die Jungfraw Mutter seyn", "tokens": ["Kan", "die", "Jung\u00b7fraw", "Mut\u00b7ter", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "NN", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tritt di\u00df Kind der Drachen K\u00f6pffe", "tokens": ["Tritt", "di\u00df", "Kind", "der", "Dra\u00b7chen", "K\u00f6pf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd de\u00df Sathans scheytel eyn? ", "tokens": ["Vnd", "de\u00df", "Sa\u00b7thans", "schey\u00b7tel", "eyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird die Wei\u00dfheit selbst zum Kinde?", "tokens": ["Wird", "die", "Wei\u00df\u00b7heit", "selbst", "zum", "Kin\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4gt die Vnschuld meine S\u00fcnde:", "tokens": ["Tr\u00e4gt", "die", "Vn\u00b7schuld", "mei\u00b7ne", "S\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Jrr' ich? nein! ich schaw den Himmel ", "tokens": ["Jrr'", "ich", "?", "nein", "!", "ich", "schaw", "den", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "PTKANT", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst mit frewden schwanger gehn?", "tokens": ["Selbst", "mit", "frew\u00b7den", "schwan\u00b7ger", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd mit jauchtzendem get\u00fcmmel", "tokens": ["Vnd", "mit", "jaucht\u00b7zen\u00b7dem", "ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Engel vmb mich stehn!", "tokens": ["Tau\u00b7send", "En\u00b7gel", "vmb", "mich", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Engel/ die zu Ehren singen ", "tokens": ["En\u00b7gel", "/", "die", "zu", "Eh\u00b7ren", "sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "ART", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem der vn\u00df wil Frieden bringen.", "tokens": ["Dem", "der", "vn\u00df", "wil", "Frie\u00b7den", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Alles frolock\u2019t! alles lachet!", "tokens": ["Al\u00b7les", "fro\u00b7lock't", "!", "al\u00b7les", "la\u00b7chet", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur mein hochbetr\u00fcbtes Hertz; ", "tokens": ["Nur", "mein", "hoch\u00b7be\u00b7tr\u00fcb\u00b7tes", "Hertz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das im jammer fe", "tokens": ["Das", "im", "jam\u00b7mer", "fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPRART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Das der Marter-volle Schmertz/", "tokens": ["Das", "der", "Mar\u00b7ter\u00b7vol\u00b7le", "Schmertz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit stets newen Geisseln plaget/", "tokens": ["Mit", "stets", "ne\u00b7wen", "Geis\u00b7seln", "pla\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Schmacht bey dieser Frewd vnd zaget. ", "tokens": ["Schmacht", "bey", "die\u00b7ser", "Frewd", "vnd", "za\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Augen/ die jhr alles sehet/", "tokens": ["Au\u00b7gen", "/", "die", "jhr", "al\u00b7les", "se\u00b7het", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "PIS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht was meine Seele sch\u00e4tzt:", "tokens": ["Seht", "was", "mei\u00b7ne", "See\u00b7le", "sch\u00e4tzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schawt wie mich der ", "tokens": ["Schawt", "wie", "mich", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "PPER", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Schawt/ wie mich die Welt verletzt/ ", "tokens": ["Schawt", "/", "wie", "mich", "die", "Welt", "ver\u00b7letzt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "PPER", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaw\u2019t wie mich die Nacht erschrecket", "tokens": ["Scha\u00b7w't", "wie", "mich", "die", "Nacht", "er\u00b7schre\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd mit trawrigkeit verdecket.", "tokens": ["Vnd", "mit", "traw