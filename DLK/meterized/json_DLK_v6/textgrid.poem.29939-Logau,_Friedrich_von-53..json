{"textgrid.poem.29939": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verstand, den ieder hat, helt ieder lieb und wehrt;", "tokens": ["Ver\u00b7stand", ",", "den", "ie\u00b7der", "hat", ",", "helt", "ie\u00b7der", "lieb", "und", "wehrt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "VAFIN", "$,", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zustand, den er hat, wird anders stets begehrt,", "tokens": ["Der", "Zu\u00b7stand", ",", "den", "er", "hat", ",", "wird", "an\u00b7ders", "stets", "be\u00b7gehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da jener, wie mich d\u00fcnckt, doch mehr als der verkehrt.", "tokens": ["Da", "je\u00b7ner", ",", "wie", "mich", "d\u00fcnckt", ",", "doch", "mehr", "als", "der", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "PIS", "KOKOM", "ART", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}