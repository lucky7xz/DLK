{"textgrid.poem.63315": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Bauz", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bauz schwingt zierlich den Zylinder,", "tokens": ["Bauz", "schwingt", "zier\u00b7lich", "den", "Zy\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bauz entstellt sich hiermit vor.", "tokens": ["Bauz", "ent\u00b7stellt", "sich", "hier\u00b7mit", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bauz hat 45 Kinder", "tokens": ["Bauz", "hat", "45", "Kin\u00b7der"], "token_info": ["word", "word", "number", "word"], "pos": ["NN", "VAFIN", "CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und nen Bruch im Wasserrohr.", "tokens": ["Und", "nen", "Bruch", "im", "Was\u00b7ser\u00b7rohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bauz ist ohne alle Frage,", "tokens": ["Bauz", "ist", "oh\u00b7ne", "al\u00b7le", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bauz ist geradezu direkt,", "tokens": ["Bauz", "ist", "ge\u00b7ra\u00b7de\u00b7zu", "di\u00b7rekt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bauz macht jede Nacht zum Tage,", "tokens": ["Bauz", "macht", "je\u00b7de", "Nacht", "zum", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bauz hat einen Schlauchdefekt.", "tokens": ["Bauz", "hat", "ei\u00b7nen", "Schlauch\u00b7de\u00b7fekt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bauz ist jeder Krone Gipfel,", "tokens": ["Bauz", "ist", "je\u00b7der", "Kro\u00b7ne", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bauz ist jedes \u00c4rmels Loch,", "tokens": ["Bauz", "ist", "je\u00b7des", "\u00c4r\u00b7mels", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bauz ist auf dem I das Tipfel,", "tokens": ["Bauz", "ist", "auf", "dem", "I", "das", "Tip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "CARD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bauz kroch, wo noch keiner kroch.", "tokens": ["Bauz", "kroch", ",", "wo", "noch", "kei\u00b7ner", "kroch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ADV", "PIS", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Bauz ist wiederum hingegen,", "tokens": ["Bauz", "ist", "wie\u00b7de\u00b7rum", "hin\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bauz ist zwecks zu dem behuf,", "tokens": ["Bauz", "ist", "zwecks", "zu", "dem", "be\u00b7huf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bauz ist andernteils deswegen,", "tokens": ["Bauz", "ist", "an\u00b7dern\u00b7teils", "des\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bauz ist ohne Widerruf!", "tokens": ["Bauz", "ist", "oh\u00b7ne", "Wi\u00b7der\u00b7ruf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}