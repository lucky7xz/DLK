{"textgrid.poem.55123": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Mahomets-Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht den Felsenquell,", "tokens": ["Seht", "den", "Fel\u00b7sen\u00b7quell", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Freudehell,", "tokens": ["Freu\u00b7de\u00b7hell", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wie ein Sternenblick;", "tokens": ["Wie", "ein", "Ster\u00b7nen\u00b7blick", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "\u00dcber Wolken", "tokens": ["\u00dc\u00b7ber", "Wol\u00b7ken"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "N\u00e4hrten seine Jugend", "tokens": ["N\u00e4hr\u00b7ten", "sei\u00b7ne", "Ju\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Gute Geister", "tokens": ["Gu\u00b7te", "Geis\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Zwischen Klippen im Geb\u00fcsch.", "tokens": ["Zwi\u00b7schen", "Klip\u00b7pen", "im", "Ge\u00b7b\u00fcsch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "J\u00fcnglingfrisch", "tokens": ["J\u00fcng\u00b7ling\u00b7frisch"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Tanzt er aus der Wolke", "tokens": ["Tanzt", "er", "aus", "der", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auf die Marmorfelsen nieder,", "tokens": ["Auf", "die", "Mar\u00b7mor\u00b7fel\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jauchzet wieder", "tokens": ["Jauch\u00b7zet", "wie\u00b7der"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nach dem Himmel.", "tokens": ["Nach", "dem", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Durch die Gipfelg\u00e4nge", "tokens": ["Durch", "die", "Gip\u00b7fel\u00b7g\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jagt er bunten Kieseln nach,", "tokens": ["Jagt", "er", "bun\u00b7ten", "Kie\u00b7seln", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit fr\u00fchem F\u00fchrertritt", "tokens": ["Und", "mit", "fr\u00fc\u00b7hem", "F\u00fch\u00b7rer\u00b7tritt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rei\u00dft er seine Bruderquellen", "tokens": ["Rei\u00dft", "er", "sei\u00b7ne", "Bru\u00b7der\u00b7quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit sich fort.", "tokens": ["Mit", "sich", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Drunten werden in dem Tal", "tokens": ["Drun\u00b7ten", "wer\u00b7den", "in", "dem", "Tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter seinem Fu\u00dftritt Blumen,", "tokens": ["Un\u00b7ter", "sei\u00b7nem", "Fu\u00df\u00b7tritt", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Wiese", "tokens": ["Und", "die", "Wie\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Lebt von seinem Hauch.", "tokens": ["Lebt", "von", "sei\u00b7nem", "Hauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Doch ihn h\u00e4lt kein Schattental,", "tokens": ["Doch", "ihn", "h\u00e4lt", "kein", "Schat\u00b7ten\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine Blumen,", "tokens": ["Kei\u00b7ne", "Blu\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die ihm seine Knie umschlingen,", "tokens": ["Die", "ihm", "sei\u00b7ne", "Knie", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm mit Liebesaugen schmeicheln:", "tokens": ["Ihm", "mit", "Lie\u00b7be\u00b7sau\u00b7gen", "schmei\u00b7cheln", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach der Ebne dringt sein Lauf", "tokens": ["Nach", "der", "Eb\u00b7ne", "dringt", "sein", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlangenwandelnd.", "tokens": ["Schlan\u00b7gen\u00b7wan\u00b7delnd", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "B\u00e4che schmiegen", "tokens": ["B\u00e4\u00b7che", "schmie\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sich gesellig an. Nun tritt er", "tokens": ["Sich", "ge\u00b7sel\u00b7lig", "an", ".", "Nun", "tritt", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Ebne silberprangend,", "tokens": ["In", "die", "Eb\u00b7ne", "sil\u00b7ber\u00b7pran\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Ebne prangt mit ihm,", "tokens": ["Und", "die", "Eb\u00b7ne", "prangt", "mit", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Fl\u00fcsse von der Ebne", "tokens": ["Und", "die", "Fl\u00fcs\u00b7se", "von", "der", "Eb\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die B\u00e4che von den Bergen", "tokens": ["Und", "die", "B\u00e4\u00b7che", "von", "den", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jauchzen ihm und rufen: \u00bbBruder!", "tokens": ["Jauch\u00b7zen", "ihm", "und", "ru\u00b7fen", ":", "\u00bb", "Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVINF", "$.", "$(", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bruder, nimm die Br\u00fcder mit,", "tokens": ["Bru\u00b7der", ",", "nimm", "die", "Br\u00fc\u00b7der", "mit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Mit zu deinem alten Vater,", "tokens": ["Mit", "zu", "dei\u00b7nem", "al\u00b7ten", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Zu dem ew'gen Ozean,", "tokens": ["Zu", "dem", "ew'\u00b7gen", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Der mit ausgespannten Armen", "tokens": ["Der", "mit", "aus\u00b7ge\u00b7spann\u00b7ten", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Unser wartet,", "tokens": ["Un\u00b7ser", "war\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Die sich, ach! vergebens \u00f6ffnen,", "tokens": ["Die", "sich", ",", "ach", "!", "ver\u00b7ge\u00b7bens", "\u00f6ff\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "ITJ", "$.", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Seine Sehnenden zu fassen;", "tokens": ["Sei\u00b7ne", "Seh\u00b7nen\u00b7den", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Denn uns fri\u00dft in \u00f6der W\u00fcste", "tokens": ["Denn", "uns", "fri\u00dft", "in", "\u00f6\u00b7der", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Gier'ger Sand; die Sonne droben", "tokens": ["Gier'\u00b7ger", "Sand", ";", "die", "Son\u00b7ne", "dro\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Saugt an unserm Blut; ein H\u00fcgel", "tokens": ["Saugt", "an", "un\u00b7serm", "Blut", ";", "ein", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Hemmet uns zum Teiche! Bruder,", "tokens": ["Hem\u00b7met", "uns", "zum", "Tei\u00b7che", "!", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Nimm die Br\u00fcder von der Ebne,", "tokens": ["Nimm", "die", "Br\u00fc\u00b7der", "von", "der", "Eb\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Nimm die Br\u00fcder von den Bergen", "tokens": ["Nimm", "die", "Br\u00fc\u00b7der", "von", "den", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Mit, zu deinem Vater mit!\u00ab", "tokens": ["Mit", ",", "zu", "dei\u00b7nem", "Va\u00b7ter", "mit", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbkommt ihr alle!\u00ab", "tokens": ["\u00bb", "kommt", "ihr", "al\u00b7le", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "PIS", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und nun schwillt er", "tokens": ["Und", "nun", "schwillt", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Herrlicher; ein ganz Geschlechte", "tokens": ["Herr\u00b7li\u00b7cher", ";", "ein", "ganz", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADV", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Tr\u00e4gt den F\u00fcrsten hoch empor!", "tokens": ["Tr\u00e4gt", "den", "F\u00fcrs\u00b7ten", "hoch", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und im rollenden Triumphe", "tokens": ["Und", "im", "rol\u00b7len\u00b7den", "Tri\u00b7um\u00b7phe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Gibt er L\u00e4ndern Namen, St\u00e4dte", "tokens": ["Gibt", "er", "L\u00e4n\u00b7dern", "Na\u00b7men", ",", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "NN", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden unter seinem Fu\u00df.", "tokens": ["Wer\u00b7den", "un\u00b7ter", "sei\u00b7nem", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Unaufhaltsam rauscht er weiter,", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "rauscht", "er", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft der T\u00fcrme Flammengipfel,", "tokens": ["L\u00e4\u00dft", "der", "T\u00fcr\u00b7me", "Flam\u00b7men\u00b7gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Marmorh\u00e4user, eine Sch\u00f6pfung", "tokens": ["Mar\u00b7mor\u00b7h\u00e4u\u00b7ser", ",", "ei\u00b7ne", "Sch\u00f6p\u00b7fung"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner F\u00fclle, hinter sich.", "tokens": ["Sei\u00b7ner", "F\u00fcl\u00b7le", ",", "hin\u00b7ter", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Zedernh\u00e4user tr\u00e4gt der Atlas", "tokens": ["Ze\u00b7dern\u00b7h\u00e4u\u00b7ser", "tr\u00e4gt", "der", "At\u00b7las"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Auf den Riesenschultern: sausend", "tokens": ["Auf", "den", "Rie\u00b7sen\u00b7schul\u00b7tern", ":", "sau\u00b7send"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$.", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wehen \u00fcber seinem Haupte", "tokens": ["We\u00b7hen", "\u00fc\u00b7ber", "sei\u00b7nem", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Flaggen durch die L\u00fcfte,", "tokens": ["Tau\u00b7send", "Flag\u00b7gen", "durch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeugen seiner Herrlichkeit.", "tokens": ["Zeu\u00b7gen", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und so tr\u00e4gt er seine Br\u00fcder,", "tokens": ["Und", "so", "tr\u00e4gt", "er", "sei\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Sch\u00e4tze, seine Kinder", "tokens": ["Sei\u00b7ne", "Sch\u00e4t\u00b7ze", ",", "sei\u00b7ne", "Kin\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem erwartenden Erzeuger", "tokens": ["Dem", "er\u00b7war\u00b7ten\u00b7den", "Er\u00b7zeu\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freudebrausend an das Herz.", "tokens": ["Freu\u00b7de\u00b7brau\u00b7send", "an", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}