{"dta.poem.21666": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Es ist ein Kampf \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Kampf, kein and\u2019rer kommt ihm gleich \u2014", "tokens": ["Es", "ist", "ein", "Kampf", ",", "kein", "an\u00b7d'\u00b7rer", "kommt", "ihm", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PIAT", "PIS", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht, wenn in offner Feldschlacht du die Brust", "tokens": ["Nicht", ",", "wenn", "in", "off\u00b7ner", "Feld\u00b7schlacht", "du", "die", "Brust"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "KOUS", "APPR", "ADJA", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Feind entgegenwirfst, die panzerlose,", "tokens": ["Dem", "Feind", "ent\u00b7ge\u00b7gen\u00b7wirfst", ",", "die", "pan\u00b7zer\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tausend Schwerter blitzen auf dich ein", "tokens": ["Und", "tau\u00b7send", "Schwer\u00b7ter", "blit\u00b7zen", "auf", "dich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVFIN", "APPR", "PRF", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und flammen Tod und klirren laut Verderben \u2014", "tokens": ["Und", "flam\u00b7men", "Tod", "und", "klir\u00b7ren", "laut", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht, wenn in Asiens dichtem Rohrgestr\u00fcpp", "tokens": ["Nicht", ",", "wenn", "in", "A\u00b7siens", "dich\u00b7tem", "Rohr\u00b7ge\u00b7str\u00fcpp"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "KOUS", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem gelben Dschungeltiger Du begegnest,", "tokens": ["Dem", "gel\u00b7ben", "Dschun\u00b7gel\u00b7ti\u00b7ger", "Du", "be\u00b7geg\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df arggeschlitzte Augen schnell ersp\u00e4hn,", "tokens": ["Da\u00df", "arg\u00b7ge\u00b7schlitz\u00b7te", "Au\u00b7gen", "schnell", "er\u00b7sp\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie sprungschnell er des Nackens Weichen dir", "tokens": ["Wie", "sprungsc\u00b7hnell", "er", "des", "Na\u00b7ckens", "Wei\u00b7chen", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zerfleischen wird, des Todes armen Sohn,", "tokens": ["Zer\u00b7flei\u00b7schen", "wird", ",", "des", "To\u00b7des", "ar\u00b7men", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und du die Flinte nach dem Hirn ihm richtest,", "tokens": ["Und", "du", "die", "Flin\u00b7te", "nach", "dem", "Hirn", "ihm", "rich\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Den Dolch bereit, falls deine Kugel irrt, \u2014", "tokens": ["Den", "Dolch", "be\u00b7reit", ",", "falls", "dei\u00b7ne", "Ku\u00b7gel", "irrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Auch nicht, wenn du auf winzig schmalem Kahn,", "tokens": ["Auch", "nicht", ",", "wenn", "du", "auf", "win\u00b7zig", "schma\u00b7lem", "Kahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der lecken Nu\u00df, von himmelhohen Wogen", "tokens": ["Der", "le\u00b7cken", "Nu\u00df", ",", "von", "him\u00b7mel\u00b7ho\u00b7hen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "In tiefen Wasserschlund geschleudert wirst,", "tokens": ["In", "tie\u00b7fen", "Was\u00b7ser\u00b7schlund", "ge\u00b7schleu\u00b7dert", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Es kocht das Meer und brandet wild emp\u00f6rt,", "tokens": ["Es", "kocht", "das", "Meer", "und", "bran\u00b7det", "wild", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Du aber k\u00e4mpfst verzweifelt mit den Fluthen \u2014", "tokens": ["Du", "a\u00b7ber", "k\u00e4mpfst", "ver\u00b7zwei\u00b7felt", "mit", "den", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Es ist ein Ringen f\u00fcrchterlicher Art,", "tokens": ["Es", "ist", "ein", "Rin\u00b7gen", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Zerrei\u00dfend deines Herzens straffste Fasern", "tokens": ["Zer\u00b7rei\u00b7\u00dfend", "dei\u00b7nes", "Her\u00b7zens", "straffs\u00b7te", "Fa\u00b7sern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und dein Gehirn mit Sturmeswuth durchtosend,", "tokens": ["Und", "dein", "Ge\u00b7hirn", "mit", "Stur\u00b7mes\u00b7wuth", "durch\u00b7to\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Liegst du im Kampf mit deiner ", "tokens": ["Liegst", "du", "im", "Kampf", "mit", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Auf st\u00fcrmt sie aus des Busens tiefsten Sch\u00e4chten,", "tokens": ["Auf", "st\u00fcrmt", "sie", "aus", "des", "Bu\u00b7sens", "tiefs\u00b7ten", "Sch\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Springt auf dich ein, gr\u00e4bt ihre Pranken fest,", "tokens": ["Springt", "auf", "dich", "ein", ",", "gr\u00e4bt", "ih\u00b7re", "Pran\u00b7ken", "fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Saugt aus dein Blut, durchbohrt dich schwertesscharf,", "tokens": ["Saugt", "aus", "dein", "Blut", ",", "durch\u00b7bohrt", "dich", "schwer\u00b7tes\u00b7scharf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und kennt kein Ruh\u2019n, kein Zaudern, kein Ermatten,", "tokens": ["Und", "kennt", "kein", "Ruh'n", ",", "kein", "Zau\u00b7dern", ",", "kein", "Er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Stets neu geboren wie der Gorgo Haupt \u2014", "tokens": ["Stets", "neu", "ge\u00b7bo\u00b7ren", "wie", "der", "Gor\u00b7go", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KOKOM", "ART", "NE", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und doch, um Gott, verliere nicht den Muth,", "tokens": ["Und", "doch", ",", "um", "Gott", ",", "ver\u00b7lie\u00b7re", "nicht", "den", "Muth", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "NN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Verzweifle nicht, wenn dir auch heimlich graust,", "tokens": ["Ver\u00b7zweif\u00b7le", "nicht", ",", "wenn", "dir", "auch", "heim\u00b7lich", "graust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Raff\u2019 stets dich auf zu wahrhaft k\u00fchnem Schlage", "tokens": ["Raff'", "stets", "dich", "auf", "zu", "wahr\u00b7haft", "k\u00fch\u00b7nem", "Schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und schlag\u2019 so lange, bis das wilde Weib", "tokens": ["Und", "schlag'", "so", "lan\u00b7ge", ",", "bis", "das", "wil\u00b7de", "Weib"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Besiegt zu deinen F\u00fc\u00dfen niederbricht,", "tokens": ["Be\u00b7siegt", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "nie\u00b7der\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Und du befreit dein Haupt gen Himmel richtest \u2014", "tokens": ["Und", "du", "be\u00b7freit", "dein", "Haupt", "gen", "Him\u00b7mel", "rich\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Ein Sieger, dem kein and'rer Sieger gleich!             ", "tokens": ["Ein", "Sie\u00b7ger", ",", "dem", "kein", "an\u00b7d'\u00b7rer", "Sie\u00b7ger", "gleich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "ADJA", "NN", "ADV", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}}}}