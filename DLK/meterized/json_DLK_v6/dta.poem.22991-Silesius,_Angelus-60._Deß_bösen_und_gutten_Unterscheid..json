{"dta.poem.22991": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "60. De\u00df b\u00f6sen und gutten Unterscheid.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Jrrliecht ist der b\u00f6\u00df\u2019: ein gutter Mensch ein stern:", "tokens": ["Ein", "Jrr\u00b7liecht", "ist", "der", "b\u00f6\u00df'", ":", "ein", "gut\u00b7ter", "Mensch", "ein", "stern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "NN", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er brennet von sich selbst/ der leuchtet von dem Herrn.", "tokens": ["Er", "bren\u00b7net", "von", "sich", "selbst", "/", "der", "leuch\u00b7tet", "von", "dem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "$(", "ART", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}