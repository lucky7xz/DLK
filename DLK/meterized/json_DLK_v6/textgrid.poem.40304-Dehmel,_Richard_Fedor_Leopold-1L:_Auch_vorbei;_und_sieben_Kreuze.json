{"textgrid.poem.40304": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch vorbei; und sieben Kreuze", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch vorbei; und sieben Kreuze", "tokens": ["Auch", "vor\u00b7bei", ";", "und", "sie\u00b7ben", "Kreu\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "KON", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hinter Jede! mein Gel\u00fcst ging irr.", "tokens": ["hin\u00b7ter", "Je\u00b7de", "!", "mein", "Ge\u00b7l\u00fcst", "ging", "irr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$.", "PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber \u2013 ich brauche tiefere Reize:", "tokens": ["A\u00b7ber", "\u2013", "ich", "brau\u00b7che", "tie\u00b7fe\u00b7re", "Rei\u00b7ze", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dich: komm, liebe dich vor mir!", "tokens": ["Dich", ":", "komm", ",", "lie\u00b7be", "dich", "vor", "mir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dich nur, Dich nur: deine genossenen Blicke", "tokens": ["Dich", "nur", ",", "Dich", "nur", ":", "dei\u00b7ne", "ge\u00b7nos\u00b7se\u00b7nen", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PPER", "ADV", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und deine bittende Scham und deine treuen", "tokens": ["und", "dei\u00b7ne", "bit\u00b7ten\u00b7de", "Scham", "und", "dei\u00b7ne", "treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "H\u00e4nde lieb'ich ... ja, entz\u00fccke", "tokens": ["H\u00e4n\u00b7de", "lie\u00b7b'\u00b7ich", "...", "ja", ",", "ent\u00b7z\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ADJD", "$(", "PTKANT", "$,", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "mich mit Deinen Rasereien!", "tokens": ["mich", "mit", "Dei\u00b7nen", "Ra\u00b7se\u00b7rei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oh Du! wenn die Knospen deiner m\u00fcden", "tokens": ["Oh", "Du", "!", "wenn", "die", "Knos\u00b7pen", "dei\u00b7ner", "m\u00fc\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "$.", "KOUS", "ART", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Br\u00fcste unter deinen tastenden Fingern", "tokens": ["Br\u00fcs\u00b7te", "un\u00b7ter", "dei\u00b7nen", "tas\u00b7ten\u00b7den", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "wieder schwellen, wie in j\u00fcngern", "tokens": ["wie\u00b7der", "schwel\u00b7len", ",", "wie", "in", "j\u00fcn\u00b7gern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "PWAV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4chten ... oh du, keinen Frieden", "tokens": ["N\u00e4ch\u00b7ten", "...", "oh", "du", ",", "kei\u00b7nen", "Frie\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$(", "FM", "PPER", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "lie\u00df mir's: meine eigenen Freuden", "tokens": ["lie\u00df", "mir's", ":", "mei\u00b7ne", "ei\u00b7ge\u00b7nen", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "sind mir Schaum, der bitter ist!", "tokens": ["sind", "mir", "Schaum", ",", "der", "bit\u00b7ter", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "aber Du, wenn Du so st\u00f6hnst und gl\u00fchst,", "tokens": ["a\u00b7ber", "Du", ",", "wenn", "Du", "so", "st\u00f6hnst", "und", "gl\u00fchst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "will ich mich an Deiner Wildheit weiden:", "tokens": ["will", "ich", "mich", "an", "Dei\u00b7ner", "Wild\u00b7heit", "wei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "wie du gleich verlassnen Br\u00e4uten", "tokens": ["wie", "du", "gleich", "ver\u00b7lass\u00b7nen", "Br\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "deine Sehnsucht nach mir stilltest,", "tokens": ["dei\u00b7ne", "Sehn\u00b7sucht", "nach", "mir", "still\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn du tief in deinen Heimlichkeiten", "tokens": ["wenn", "du", "tief", "in", "dei\u00b7nen", "Heim\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "mit berauschten Armen w\u00fchltest \u2013", "tokens": ["mit", "be\u00b7rauschten", "Ar\u00b7men", "w\u00fchl\u00b7test", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "w\u00fchlst ... stillst ... Seele, bricht dein Blick?", "tokens": ["w\u00fchlst", "...", "stillst", "...", "See\u00b7le", ",", "bricht", "dein", "Blick", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "oh du: la\u00df mich diesen Blick genie\u00dfen,", "tokens": ["oh", "du", ":", "la\u00df", "mich", "die\u00b7sen", "Blick", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "VVIMP", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "dies Verr\u00f6cheln von Lippen bis zu F\u00fc\u00dfen,", "tokens": ["dies", "Ver\u00b7r\u00f6\u00b7cheln", "von", "Lip\u00b7pen", "bis", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "recke dich nicht so starr zur\u00fcck \u2013 \u2013", "tokens": ["re\u00b7cke", "dich", "nicht", "so", "starr", "zu\u00b7r\u00fcck", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Menschen? bist du satt der Kuhnatur?! \u2013", "tokens": ["Men\u00b7schen", "?", "bist", "du", "satt", "der", "Kuh\u00b7na\u00b7tur", "?!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und wir fliehen, wir begreifen", "tokens": ["Und", "wir", "flie\u00b7hen", ",", "wir", "be\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "den Triumph der Unnatur ...", "tokens": ["den", "Tri\u00b7umph", "der", "Un\u00b7na\u00b7tur", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}