{"textgrid.poem.31759": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Pfingstlied", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie herzten sich und sie k\u00fc\u00dften sich", "tokens": ["Sie", "herz\u00b7ten", "sich", "und", "sie", "k\u00fc\u00df\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "PPER", "VVFIN", "PRF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit liebevoller Geb\u00e4rde.", "tokens": ["Mit", "lie\u00b7be\u00b7vol\u00b7ler", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der junge Herr Fr\u00fchling wonniglich,", "tokens": ["Der", "jun\u00b7ge", "Herr", "Fr\u00fch\u00b7ling", "won\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der besuchte die alte Frau Erde.", "tokens": ["Der", "be\u00b7such\u00b7te", "die", "al\u00b7te", "Frau", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er ist der guten, ehrlichen Frau", "tokens": ["Er", "ist", "der", "gu\u00b7ten", ",", "ehr\u00b7li\u00b7chen", "Frau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mit eins an den Hals gesprungen,", "tokens": ["Mit", "eins", "an", "den", "Hals", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df bis hinauf in den Himmel blau", "tokens": ["Da\u00df", "bis", "hin\u00b7auf", "in", "den", "Him\u00b7mel", "blau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur Lust und Jubel erklungen.", "tokens": ["Nur", "Lust", "und", "Ju\u00b7bel", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbmein Sohn, es freut mich, da\u00df du hier!", "tokens": ["\u00bb", "mein", "Sohn", ",", "es", "freut", "mich", ",", "da\u00df", "du", "hier", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lang w\u00e4hrte des Winters Tosen.", "tokens": ["Lang", "w\u00e4hr\u00b7te", "des", "Win\u00b7ters", "To\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Meine Felder brauchen die goldne Zier,", "tokens": ["Mei\u00b7ne", "Fel\u00b7der", "brau\u00b7chen", "die", "gold\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Meine G\u00e4rten Lilien und Rosen.", "tokens": ["Mei\u00b7ne", "G\u00e4r\u00b7ten", "Li\u00b7li\u00b7en", "und", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Verstummt sind all meine Nachtigalln,", "tokens": ["Ver\u00b7stummt", "sind", "all", "mei\u00b7ne", "Nach\u00b7ti\u00b7galln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seit ich dich verloren hatte;", "tokens": ["Seit", "ich", "dich", "ver\u00b7lo\u00b7ren", "hat\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum schm\u00fccke den V\u00f6geln die gr\u00fcnen Halln", "tokens": ["Drum", "schm\u00fc\u00b7cke", "den", "V\u00f6\u00b7geln", "die", "gr\u00fc\u00b7nen", "Halln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und den Hirschen die blumige Matte.", "tokens": ["Und", "den", "Hir\u00b7schen", "die", "blu\u00b7mi\u00b7ge", "Mat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Ich habe so oft an dich gedacht,", "tokens": ["Ich", "ha\u00b7be", "so", "oft", "an", "dich", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn es st\u00fcrmte wilder und wilder;", "tokens": ["Wenn", "es", "st\u00fcrm\u00b7te", "wil\u00b7der", "und", "wil\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "KON", "ADJA", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch sprich, was hast du mir mitgebracht", "tokens": ["Doch", "sprich", ",", "was", "hast", "du", "mir", "mit\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWS", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fcr die lieblichen Menschenbilder?\u00ab", "tokens": ["F\u00fcr", "die", "lieb\u00b7li\u00b7chen", "Men\u00b7schen\u00b7bil\u00b7der", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbf\u00fcr die Menschenbilder?\u00ab versetzte da", "tokens": ["\u00bb", "f\u00fcr", "die", "Men\u00b7schen\u00b7bil\u00b7der", "?", "\u00ab", "ver\u00b7setz\u00b7te", "da"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "$.", "$(", "VVFIN", "ADV"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der junge Herr Fr\u00fchling stutzend \u2013", "tokens": ["Der", "jun\u00b7ge", "Herr", "Fr\u00fch\u00b7ling", "stut\u00b7zend", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In die Tasche griff er behend: \u00bbVoil\u00e0!", "tokens": ["In", "die", "Ta\u00b7sche", "griff", "er", "be\u00b7hend", ":", "\u00bb", "Voil\u00e0", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVPP", "$.", "$(", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Revolutionen ein Dutzend.\u00ab", "tokens": ["Re\u00b7vo\u00b7lu\u00b7ti\u00b7o\u00b7nen", "ein", "Dut\u00b7zend", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}