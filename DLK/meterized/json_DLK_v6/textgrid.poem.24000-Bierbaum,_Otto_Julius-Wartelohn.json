{"textgrid.poem.24000": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Wartelohn", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgenjunge Herrlichkeit,", "tokens": ["Mor\u00b7gen\u00b7jun\u00b7ge", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hell die Welt und frisch der Wind,", "tokens": ["Hell", "die", "Welt", "und", "frisch", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wartend klopft mein Herz geschwind \u2013:", "tokens": ["War\u00b7tend", "klopft", "mein", "Herz", "ge\u00b7schwind", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "VVPP", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Minute schon \u00fcber der Zeit!", "tokens": ["Ei\u00b7ne", "Mi\u00b7nu\u00b7te", "schon", "\u00fc\u00b7ber", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Ach, wie oft schon sagt ichs, Kind:", "tokens": ["Ach", ",", "wie", "oft", "schon", "sagt", "ichs", ",", "Kind", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "ADV", "VVFIN", "PIS", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "P\u00fcnktlichkeit!", "tokens": ["P\u00fcnkt\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und ich sp\u00e4he augenweit,", "tokens": ["Und", "ich", "sp\u00e4\u00b7he", "au\u00b7gen\u00b7weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ich schaue fast mich blind,", "tokens": ["Und", "ich", "schau\u00b7e", "fast", "mich", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ist das M\u00e4del nicht gescheidt?", "tokens": ["Ist", "das", "M\u00e4\u00b7del", "nicht", "ge\u00b7scheidt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Zehn Minuten schon \u00fcber der Zeit!", "tokens": ["Zehn", "Mi\u00b7nu\u00b7ten", "schon", "\u00fc\u00b7ber", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Soll ich eine Ewigkeit", "tokens": ["Soll", "ich", "ei\u00b7ne", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Warten und sehnen!? \u2013 Langsam rinnt", "tokens": ["War\u00b7ten", "und", "seh\u00b7nen", "!?", "\u2013", "Lang\u00b7sam", "rinnt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "KON", "VVINF", "$.", "$(", "ADJD", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Der Minuten Folge, breit", "tokens": ["Der", "Mi\u00b7nu\u00b7ten", "Fol\u00b7ge", ",", "breit"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Wie ein Theerstromm. \u2013 Zeit, oh Zeit!", "tokens": ["Wie", "ein", "Theer\u00b7stromm", ".", "\u2013", "Zeit", ",", "oh", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$.", "$(", "NN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Deine Minuten wie Stunden sind! ...", "tokens": ["Dei\u00b7ne", "Mi\u00b7nu\u00b7ten", "wie", "Stun\u00b7den", "sind", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "NN", "VAFIN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Sieh, da flattert ihr blaues Kleid,", "tokens": ["Sieh", ",", "da", "flat\u00b7tert", "ihr", "blau\u00b7es", "Kleid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Flattert im Wind!", "tokens": ["Flat\u00b7tert", "im", "Wind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Alles Warten ist verwunden,", "tokens": ["Al\u00b7les", "War\u00b7ten", "ist", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Hat sich Mund auf Mund gefunden,", "tokens": ["Hat", "sich", "Mund", "auf", "Mund", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Blick in Blick sich eingesenkt.", "tokens": ["Blick", "in", "Blick", "sich", "ein\u00b7ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Dehnten jetzt sich die ", "tokens": ["Dehn\u00b7ten", "jetzt", "sich", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PRF", "ART"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "W\u00e4rs kein Umstand, der uns kr\u00e4nkt,", "tokens": ["W\u00e4rs", "kein", "Um\u00b7stand", ",", "der", "uns", "kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Da der Wind mit leisem Neigen", "tokens": ["Da", "der", "Wind", "mit", "lei\u00b7sem", "Nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Ein Panier aus Fr\u00fchlingszweigen", "tokens": ["Ein", "Pa\u00b7nier", "aus", "Fr\u00fch\u00b7lings\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Ueber unsren K\u00fcssen schwenkt.", "tokens": ["Ue\u00b7ber", "un\u00b7sren", "K\u00fcs\u00b7sen", "schwenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}