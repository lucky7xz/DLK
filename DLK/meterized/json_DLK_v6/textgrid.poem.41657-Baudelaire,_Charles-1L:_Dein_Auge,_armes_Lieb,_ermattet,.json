{"textgrid.poem.41657": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Auge, armes Lieb, ermattet,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Auge, armes Lieb, ermattet,", "tokens": ["Dein", "Au\u00b7ge", ",", "ar\u00b7mes", "Lieb", ",", "er\u00b7mat\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wimpern senkend ruhe lang", "tokens": ["Die", "Wim\u00b7pern", "sen\u00b7kend", "ru\u00b7he", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von m\u00fcder Anmut \u00fcberschattet,", "tokens": ["Von", "m\u00fc\u00b7der", "An\u00b7mut", "\u00fc\u00b7bersc\u00b7hat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun dich die Freude ganz bezwang.", "tokens": ["Nun", "dich", "die", "Freu\u00b7de", "ganz", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Springbrunn, der im Hofe fl\u00fcstert,", "tokens": ["Der", "Spring\u00b7brunn", ",", "der", "im", "Ho\u00b7fe", "fl\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns Tag und Nacht sein Murmeln schenkt,", "tokens": ["Uns", "Tag", "und", "Nacht", "sein", "Mur\u00b7meln", "schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bleibt dem Entz\u00fccken hold verschwistert,", "tokens": ["Bleibt", "dem", "Ent\u00b7z\u00fc\u00b7cken", "hold", "ver\u00b7schwis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "In das die Liebe mich versenkt.", "tokens": ["In", "das", "die", "Lie\u00b7be", "mich", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Strahl, der schlank sich hebend", "tokens": ["Der", "Strahl", ",", "der", "schlank", "sich", "he\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Garben bl\u00fcht,", "tokens": ["In", "Gar\u00b7ben", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Drin Ph\u00f6be Flimmer webend", "tokens": ["Drin", "Ph\u00f6\u00b7be", "Flim\u00b7mer", "we\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sanft ergl\u00fcht,", "tokens": ["So", "sanft", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "F\u00e4llt, Tr\u00e4nen niederbebend,", "tokens": ["F\u00e4llt", ",", "Tr\u00e4\u00b7nen", "nie\u00b7der\u00b7be\u00b7bend", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Duft verspr\u00fcht.", "tokens": ["Im", "Duft", "ver\u00b7spr\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So schwingt, entz\u00fcndet von dem Funken", "tokens": ["So", "schwingt", ",", "ent\u00b7z\u00fcn\u00b7det", "von", "dem", "Fun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Lust, die deinen Busen schwellt,", "tokens": ["Der", "Lust", ",", "die", "dei\u00b7nen", "Bu\u00b7sen", "schwellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich deine Seele k\u00fchn und trunken", "tokens": ["Sich", "dei\u00b7ne", "See\u00b7le", "k\u00fchn", "und", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ferner Himmel Lichtgezelt.", "tokens": ["Zu", "fer\u00b7ner", "Him\u00b7mel", "Licht\u00b7ge\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann sinkt sie im Vergehen wieder", "tokens": ["Dann", "sinkt", "sie", "im", "Ver\u00b7ge\u00b7hen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In einer Flut von bangem Schmerz,", "tokens": ["In", "ei\u00b7ner", "Flut", "von", "ban\u00b7gem", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die unsichtbaren Pfads hernieder", "tokens": ["Die", "un\u00b7sicht\u00b7ba\u00b7ren", "Pfads", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hinabtropft in mein tiefstes Herz.", "tokens": ["Hin\u00b7ab\u00b7tropft", "in", "mein", "tiefs\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Strahl, der schlank sich hebend", "tokens": ["Der", "Strahl", ",", "der", "schlank", "sich", "he\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Garben bl\u00fcht,", "tokens": ["In", "Gar\u00b7ben", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Drin Ph\u00f6be Flimmer webend", "tokens": ["Drin", "Ph\u00f6\u00b7be", "Flim\u00b7mer", "we\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sanft ergl\u00fcht,", "tokens": ["So", "sanft", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "F\u00e4llt, Tr\u00e4nen niederbebend,", "tokens": ["F\u00e4llt", ",", "Tr\u00e4\u00b7nen", "nie\u00b7der\u00b7be\u00b7bend", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Duft verspr\u00fcht.", "tokens": ["Im", "Duft", "ver\u00b7spr\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "O du, so sch\u00f6n in n\u00e4chtgen Schatten,", "tokens": ["O", "du", ",", "so", "sch\u00f6n", "in", "n\u00e4cht\u00b7gen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie s\u00fc\u00df h\u00f6rt, \u00fcber dich geneigt,", "tokens": ["Wie", "s\u00fc\u00df", "h\u00f6rt", ",", "\u00fc\u00b7ber", "dich", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Klage sich, die ohn Ermatten", "tokens": ["Die", "Kla\u00b7ge", "sich", ",", "die", "ohn", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Marmorbecken weinend steigt!", "tokens": ["Vom", "Mar\u00b7mor\u00b7be\u00b7cken", "wei\u00b7nend", "steigt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mond, heilges Dunkel, Wasserschauer,", "tokens": ["Mond", ",", "heil\u00b7ges", "Dun\u00b7kel", ",", "Was\u00b7ser\u00b7schau\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Gezweig, aus dem ein Rauschen quillt \u2013", "tokens": ["Ge\u00b7zweig", ",", "aus", "dem", "ein", "Rau\u00b7schen", "quillt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die reine Schwermut eurer Trauer", "tokens": ["Die", "rei\u00b7ne", "Schwer\u00b7mut", "eu\u00b7rer", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist meiner Liebe Spiegelbild.", "tokens": ["Ist", "mei\u00b7ner", "Lie\u00b7be", "Spie\u00b7gel\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Strahl, der schlank sich hebend", "tokens": ["Der", "Strahl", ",", "der", "schlank", "sich", "he\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Garben bl\u00fcht,", "tokens": ["In", "Gar\u00b7ben", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Drin Ph\u00f6be Flimmer webend", "tokens": ["Drin", "Ph\u00f6\u00b7be", "Flim\u00b7mer", "we\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sanft ergl\u00fcht,", "tokens": ["So", "sanft", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "F\u00e4llt, Tr\u00e4nen niederbebend,", "tokens": ["F\u00e4llt", ",", "Tr\u00e4\u00b7nen", "nie\u00b7der\u00b7be\u00b7bend", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Duft verspr\u00fcht.", "tokens": ["Im", "Duft", "ver\u00b7spr\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}