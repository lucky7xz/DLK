{"textgrid.poem.40996": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Des Landschaftsmaler Karl D\u00f6rrs Tod", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er, der in mondbestrahlten Bildern", "tokens": ["Er", ",", "der", "in", "mond\u00b7bes\u00b7trahl\u00b7ten", "Bil\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Natur in ihrem Liebesreiz", "tokens": ["Na\u00b7tur", "in", "ih\u00b7rem", "Lie\u00b7bes\u00b7reiz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So klar, so wahr gewu\u00dft zu schildern,", "tokens": ["So", "klar", ",", "so", "wahr", "ge\u00b7wu\u00dft", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der aussah wie ein Sohn der Schweiz,", "tokens": ["Der", "aus\u00b7sah", "wie", "ein", "Sohn", "der", "Schweiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lag n\u00e4chtlich einst in stiller Kammer,", "tokens": ["Lag", "n\u00e4cht\u00b7lich", "einst", "in", "stil\u00b7ler", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll eines wunderhellen Traums,", "tokens": ["Voll", "ei\u00b7nes", "wun\u00b7der\u00b7hel\u00b7len", "Traums", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gestreift vom m\u00fcden Leib den Jammer", "tokens": ["Ge\u00b7streift", "vom", "m\u00fc\u00b7den", "Leib", "den", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Dufte eines Bl\u00fctenbaums.", "tokens": ["Im", "Duf\u00b7te", "ei\u00b7nes", "Bl\u00fc\u00b7ten\u00b7baums", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Fischerhaus auf moos'gem Steine", "tokens": ["Ein", "Fi\u00b7scher\u00b7haus", "auf", "moos'\u00b7gem", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stand nah bei eines Baches Fall,", "tokens": ["Stand", "nah", "bei", "ei\u00b7nes", "Ba\u00b7ches", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber ihm aus dunklem Haine", "tokens": ["Und", "\u00fc\u00b7ber", "ihm", "aus", "dunk\u00b7lem", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sang ihren Schmerz die Nachtigall.", "tokens": ["Sang", "ih\u00b7ren", "Schmerz", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie schwieg und pl\u00f6tzlich ward es helle,", "tokens": ["Sie", "schwieg", "und", "pl\u00f6tz\u00b7lich", "ward", "es", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "VAFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herschwebte eine Lichtgestalt,", "tokens": ["Her\u00b7schweb\u00b7te", "ei\u00b7ne", "Licht\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Silberblick ward Bacheswelle,", "tokens": ["Zum", "Sil\u00b7berb\u00b7lick", "ward", "Ba\u00b7ches\u00b7wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Goldflu\u00df ward der dunkle Wald.", "tokens": ["Zum", "Gold\u00b7flu\u00df", "ward", "der", "dunk\u00b7le", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da lag der Tr\u00e4umer, Gottesfrieden", "tokens": ["Da", "lag", "der", "Tr\u00e4u\u00b7mer", ",", "Got\u00b7tes\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner warmen K\u00fcnstlerbrust,", "tokens": ["In", "sei\u00b7ner", "war\u00b7men", "K\u00fcnst\u00b7ler\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach: \u00bbWie sch\u00f6n ist's doch hienieden!", "tokens": ["Und", "sprach", ":", "\u00bb", "Wie", "sch\u00f6n", "ist's", "doch", "hien\u00b7ie\u00b7den", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PWAV", "ADJD", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das bild' ich morgen nach voll Lust.\u00ab", "tokens": ["Das", "bild'", "ich", "mor\u00b7gen", "nach", "voll", "Lust", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbdu klares Herz!\u00ab sprach die Erscheinung", "tokens": ["\u00bb", "du", "kla\u00b7res", "Herz", "!", "\u00ab", "sprach", "die", "Er\u00b7schei\u00b7nung"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "ADJA", "NN", "$.", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "(der Mond war es in voller Pracht),", "tokens": ["(", "der", "Mond", "war", "es", "in", "vol\u00b7ler", "Pracht", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbschweb' auf zu sel'ger Geister Einung,", "tokens": ["\u00bb", "schweb'", "auf", "zu", "sel'\u00b7ger", "Geis\u00b7ter", "Ei\u00b7nung", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hin, wo dir ew'ger Fr\u00fchling lacht.", "tokens": ["Hin", ",", "wo", "dir", "ew'\u00b7ger", "Fr\u00fch\u00b7ling", "lacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In dir war Wahrheit, war die Treue,", "tokens": ["In", "dir", "war", "Wahr\u00b7heit", ",", "war", "die", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein ganzes Wesen war Natur.", "tokens": ["Dein", "gan\u00b7zes", "We\u00b7sen", "war", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stirb, schwerer Leib! und la\u00df ins Freie", "tokens": ["Stirb", ",", "schwe\u00b7rer", "Leib", "!", "und", "la\u00df", "ins", "Frei\u00b7e"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$.", "KON", "VVIMP", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den treuen Sohn von Wald und Flur.\u00ab", "tokens": ["Den", "treu\u00b7en", "Sohn", "von", "Wald", "und", "Flur", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Morgen kam \u2013 des K\u00fcnstlers Kammer", "tokens": ["Der", "Mor\u00b7gen", "kam", "\u2013", "des", "K\u00fcnst\u00b7lers", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er\u00f6ffnete wie sonst sich nicht;", "tokens": ["Er\u00b7\u00f6ff\u00b7ne\u00b7te", "wie", "sonst", "sich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADV", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Freund drang ein und fand \u2013 o Jammer!", "tokens": ["Ein", "Freund", "drang", "ein", "und", "fand", "\u2013", "o", "Jam\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "$(", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur seine H\u00fclle, ihn doch nicht.", "tokens": ["Nur", "sei\u00b7ne", "H\u00fcl\u00b7le", ",", "ihn", "doch", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er hatte keinen Tod gesehen,", "tokens": ["Er", "hat\u00b7te", "kei\u00b7nen", "Tod", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er f\u00fchlte keiner Krankheit Pein;", "tokens": ["Er", "f\u00fchl\u00b7te", "kei\u00b7ner", "Krank\u00b7heit", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es nahm den Liebling ohne Wehen", "tokens": ["Es", "nahm", "den", "Lieb\u00b7ling", "oh\u00b7ne", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Natur zu sich im Mondenschein.", "tokens": ["Na\u00b7tur", "zu", "sich", "im", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}