{"textgrid.poem.55164": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wecke den Amor nicht auf! Noch schl\u00e4ft der liebliche Knabe;", "tokens": ["We\u00b7cke", "den", "A\u00b7mor", "nicht", "auf", "!", "Noch", "schl\u00e4ft", "der", "lieb\u00b7li\u00b7che", "Kna\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "PTKNEG", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Geh, vollbring dein Gesch\u00e4ft, wie es der Tag dir gebeut!", "tokens": ["Geh", ",", "voll\u00b7bring", "dein", "Ge\u00b7sch\u00e4ft", ",", "wie", "es", "der", "Tag", "dir", "ge\u00b7beut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "So der Zeit bedienet sich klug die sorgliche Mutter,", "tokens": ["So", "der", "Zeit", "be\u00b7die\u00b7net", "sich", "klug", "die", "sorg\u00b7li\u00b7che", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn ihr Kn\u00e4bchen entschl\u00e4ft, denn es erwacht nur zu bald.", "tokens": ["Wenn", "ihr", "Kn\u00e4b\u00b7chen", "ent\u00b7schl\u00e4ft", ",", "denn", "es", "er\u00b7wacht", "nur", "zu", "bald", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADV", "APPR", "ADV", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}}}}}