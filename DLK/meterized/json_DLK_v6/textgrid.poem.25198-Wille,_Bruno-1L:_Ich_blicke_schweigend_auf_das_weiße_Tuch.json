{"textgrid.poem.25198": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich blicke schweigend auf das wei\u00dfe Tuch", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich blicke schweigend auf das wei\u00dfe Tuch", "tokens": ["Ich", "bli\u00b7cke", "schwei\u00b7gend", "auf", "das", "wei\u00b7\u00dfe", "Tuch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und tippe sinnend mit dem Tafelmesser;", "tokens": ["Und", "tip\u00b7pe", "sin\u00b7nend", "mit", "dem", "Ta\u00b7fel\u00b7mes\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weingl\u00e4ser klirren, eine Dame lacht,", "tokens": ["Wein\u00b7gl\u00e4\u00b7ser", "klir\u00b7ren", ",", "ei\u00b7ne", "Da\u00b7me", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die beiden Diplomaten reden wichtig,", "tokens": ["Die", "bei\u00b7den", "Dip\u00b7lo\u00b7ma\u00b7ten", "re\u00b7den", "wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Seidenroben duften nach Parf\u00fcm.", "tokens": ["Und", "Sei\u00b7den\u00b7ro\u00b7ben", "duf\u00b7ten", "nach", "Par\u00b7f\u00fcm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch \u00fcber die Terrasse weht ein Hauch", "tokens": ["Doch", "\u00fc\u00b7ber", "die", "Ter\u00b7ras\u00b7se", "weht", "ein", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Aus waldiger Bergesschlucht so k\u00fchl und rein;", "tokens": ["Aus", "wal\u00b7di\u00b7ger", "Ber\u00b7ges\u00b7schlucht", "so", "k\u00fchl", "und", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Tief atmend schlage ich die Augen auf.", "tokens": ["Tief", "at\u00b7mend", "schla\u00b7ge", "ich", "die", "Au\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da \u00fcbergipfelt sich der krause Wald", "tokens": ["Da", "\u00fc\u00b7berg\u00b7ip\u00b7felt", "sich", "der", "krau\u00b7se", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Berg hinan, da lagern gr\u00fcne Matten", "tokens": ["Den", "Berg", "hi\u00b7nan", ",", "da", "la\u00b7gern", "gr\u00fc\u00b7ne", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Felsgeh\u00e4ngen, und mit schroffem Stolz", "tokens": ["An", "Fels\u00b7ge\u00b7h\u00e4n\u00b7gen", ",", "und", "mit", "schrof\u00b7fem", "Stolz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erhebt der Riese himmelan sein Haupt.", "tokens": ["Er\u00b7hebt", "der", "Rie\u00b7se", "him\u00b7me\u00b7lan", "sein", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbentz\u00fcckend!\u00ab lispelt meine Tafeldame,", "tokens": ["\u00bb", "ent\u00b7z\u00fc\u00b7ckend", "!", "\u00ab", "lis\u00b7pelt", "mei\u00b7ne", "Ta\u00b7fel\u00b7da\u00b7me", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "$(", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gouvernante.", "tokens": ["Die", "Gou\u00b7ver\u00b7nan\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "\u00ab", "tokens": ["\u00ab"], "token_info": ["punct"], "pos": ["$("]}, "line.4": {"text": "Ich mu\u00df es stets betonen, Herr Minister,", "tokens": ["Ich", "mu\u00df", "es", "stets", "be\u00b7to\u00b7nen", ",", "Herr", "Mi\u00b7nis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erh\u00f6hen wir den Schutzzoll! Unser Staat,", "tokens": ["Er\u00b7h\u00f6\u00b7hen", "wir", "den", "Schutz\u00b7zoll", "!", "Un\u00b7ser", "Staat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verlassen Sie Sich drauf, wird ausgesogen.", "tokens": ["Ver\u00b7las\u00b7sen", "Sie", "Sich", "drauf", ",", "wird", "aus\u00b7ge\u00b7so\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PAV", "$,", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das einzige Rettungsmittel ist mein Antrag.\u00ab", "tokens": ["Das", "ein\u00b7zi\u00b7ge", "Ret\u00b7tungs\u00b7mit\u00b7tel", "ist", "mein", "An\u00b7trag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wie offen blickt das Deputirtenauge \u2013", "tokens": ["Wie", "of\u00b7fen", "blickt", "das", "De\u00b7pu\u00b7tir\u00b7ten\u00b7au\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nur blitzt es heimlich drin: \u00bbWenn es gelingt,", "tokens": ["Nur", "blitzt", "es", "heim\u00b7lich", "drin", ":", "\u00bb", "Wenn", "es", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "$(", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "O k\u00f6stlicher Profit!\u00ab", "tokens": ["O", "k\u00f6st\u00b7li\u00b7cher", "Pro\u00b7fit", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Vogel kreischt und schl\u00e4gt mit starkem Fittig", "tokens": ["Ein", "Vo\u00b7gel", "kreischt", "und", "schl\u00e4gt", "mit", "star\u00b7kem", "Fit\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wiegt sich sp\u00e4hend \u00fcber Wald und Schlucht;", "tokens": ["Und", "wiegt", "sich", "sp\u00e4\u00b7hend", "\u00fc\u00b7ber", "Wald", "und", "Schlucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dumpf tost der Gie\u00dfbach zwischen Felsgebl\u00f6ck,", "tokens": ["Dumpf", "tost", "der", "Gie\u00df\u00b7bach", "zwi\u00b7schen", "Fels\u00b7ge\u00b7bl\u00f6ck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und eine Wolke schattet.", "tokens": ["Und", "ei\u00b7ne", "Wol\u00b7ke", "schat\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Geil grinsend dr\u00fcckt die alte Excellenz", "tokens": ["Geil", "grin\u00b7send", "dr\u00fcckt", "die", "al\u00b7te", "Ex\u00b7cel\u00b7lenz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Patschhand seiner Dame an den Wei\u00dfbart.", "tokens": ["Die", "Patschhand", "sei\u00b7ner", "Da\u00b7me", "an", "den", "Wei\u00df\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Gn\u00e4dige l\u00e4chelt wie ein Kind \u2013 und denkt:", "tokens": ["Die", "Gn\u00e4\u00b7di\u00b7ge", "l\u00e4\u00b7chelt", "wie", "ein", "Kind", "\u2013", "und", "denkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$(", "KON", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u00bbhat erst mein Mann die Stelle in der Tasche,", "tokens": ["\u00bb", "hat", "erst", "mein", "Mann", "die", "Stel\u00b7le", "in", "der", "Ta\u00b7sche", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann, Herr Protektor ... warte, alter Ekel!\u00ab", "tokens": ["Dann", ",", "Herr", "Pro\u00b7tek\u00b7tor", "...", "war\u00b7te", ",", "al\u00b7ter", "E\u00b7kel", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "NN", "NN", "$(", "VVFIN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u00bbentz\u00fcckend!\u00ab lispelt meine Tafeldame", "tokens": ["\u00bb", "ent\u00b7z\u00fc\u00b7ckend", "!", "\u00ab", "lis\u00b7pelt", "mei\u00b7ne", "Ta\u00b7fel\u00b7da\u00b7me"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVPP", "$.", "$(", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch ihre falschen Z\u00e4hne; \u00bbo Natur!\u00ab", "tokens": ["Durch", "ih\u00b7re", "fal\u00b7schen", "Z\u00e4h\u00b7ne", ";", "\u00bb", "o", "Na\u00b7tur", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$(", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und blickt hinan zum Angesicht des Berges, \u2013", "tokens": ["Und", "blickt", "hi\u00b7nan", "zum", "An\u00b7ge\u00b7sicht", "des", "Ber\u00b7ges", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das sich verfinstert und in Wolken h\u00fcllt.", "tokens": ["Das", "sich", "ver\u00b7fins\u00b7tert", "und", "in", "Wol\u00b7ken", "h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "VVPP", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nur auf der Matte ruht noch goldnes Licht;", "tokens": ["Nur", "auf", "der", "Mat\u00b7te", "ruht", "noch", "gold\u00b7nes", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das l\u00e4chelt mich wehm\u00fctig an. Ich schlage", "tokens": ["Das", "l\u00e4\u00b7chelt", "mich", "weh\u00b7m\u00fc\u00b7tig", "an", ".", "Ich", "schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Augen nieder auf das wei\u00dfe Tuch", "tokens": ["Die", "Au\u00b7gen", "nie\u00b7der", "auf", "das", "wei\u00b7\u00dfe", "Tuch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und tippe sinnend mit dem Tafelmesser.", "tokens": ["Und", "tip\u00b7pe", "sin\u00b7nend", "mit", "dem", "Ta\u00b7fel\u00b7mes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}