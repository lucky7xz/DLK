{"textgrid.poem.66514": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Odenwalde der Fr\u00fchlingswind", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Odenwalde der Fr\u00fchlingswind", "tokens": ["Vom", "O\u00b7den\u00b7wal\u00b7de", "der", "Fr\u00fch\u00b7lings\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Senkt seine Schwingen zu Thal,", "tokens": ["Senkt", "sei\u00b7ne", "Schwin\u00b7gen", "zu", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein gr\u00fcnes Netz um die Buchen sich spinnt", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Netz", "um", "die", "Bu\u00b7chen", "sich", "spinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im w\u00e4rmenden Sonnenstrahl.", "tokens": ["Im", "w\u00e4r\u00b7men\u00b7den", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nun hinaus auf des Lebens erquickender Spur,", "tokens": ["Nun", "hin\u00b7aus", "auf", "des", "Le\u00b7bens", "er\u00b7qui\u00b7cken\u00b7der", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APZR", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-----+--+--+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Um zu schl\u00fcrfen den duftenden Schaum!", "tokens": ["Um", "zu", "schl\u00fcr\u00b7fen", "den", "duf\u00b7ten\u00b7den", "Schaum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Zur Rechten des Rheins goldprangende Flur,", "tokens": ["Zur", "Rech\u00b7ten", "des", "Rheins", "gold\u00b7pran\u00b7gen\u00b7de", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zur Linken der Berge Saum.", "tokens": ["Zur", "Lin\u00b7ken", "der", "Ber\u00b7ge", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Burgen und Oertlein wie Perlen sich reih'n,", "tokens": ["Die", "Bur\u00b7gen", "und", "O\u00b7ert\u00b7lein", "wie", "Per\u00b7len", "sich", "reih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KOKOM", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und die B\u00e4che wie Silber spr\u00fchn,", "tokens": ["Und", "die", "B\u00e4\u00b7che", "wie", "Sil\u00b7ber", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KOKOM", "NN", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Es kleidet der Alte, der Frankenstein,", "tokens": ["Es", "klei\u00b7det", "der", "Al\u00b7te", ",", "der", "Fran\u00b7ken\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sich immer noch waidmannsgr\u00fcn.", "tokens": ["Sich", "im\u00b7mer", "noch", "waid\u00b7manns\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da drunten Seeheim, leuchtend im Thal,", "tokens": ["Da", "drun\u00b7ten", "See\u00b7heim", ",", "leuch\u00b7tend", "im", "Thal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und Jugenheim, strahlend in's Land,", "tokens": ["Und", "Ju\u00b7gen\u00b7heim", ",", "strah\u00b7lend", "in's", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Gott gr\u00fc\u00df euch, ihr traulichen Heim allzumal,", "tokens": ["Gott", "gr\u00fc\u00df", "euch", ",", "ihr", "trau\u00b7li\u00b7chen", "Heim", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Bis an des Neckars Strand!", "tokens": ["Bis", "an", "des", "Ne\u00b7ckars", "Strand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was manch Jahrhundert von S\u00fcd und Nord", "tokens": ["Was", "manch", "Jahr\u00b7hun\u00b7dert", "von", "S\u00fcd", "und", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus der Wanderstra\u00dfe hier zog,", "tokens": ["Aus", "der", "Wan\u00b7der\u00b7stra\u00b7\u00dfe", "hier", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "An Bergen und Th\u00e4lern vor\u00fcber und fort,", "tokens": ["An", "Ber\u00b7gen", "und", "Th\u00e4\u00b7lern", "vor\u00b7\u00fc\u00b7ber", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zieht heut noch, das V\u00f6lkergewog.", "tokens": ["Zieht", "heut", "noch", ",", "das", "V\u00f6l\u00b7ker\u00b7ge\u00b7wog", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Doch wollt' es uns wieder von Westen her", "tokens": ["Doch", "wollt'", "es", "uns", "wie\u00b7der", "von", "Wes\u00b7ten", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "APPR", "NN", "APZR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Mit Drohn und Gel\u00fcsten zum Rhein,", "tokens": ["Mit", "Drohn", "und", "Ge\u00b7l\u00fcs\u00b7ten", "zum", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Der Weg soll zur\u00fcck ihm mit kundiger Wehr", "tokens": ["Der", "Weg", "soll", "zu\u00b7r\u00fcck", "ihm", "mit", "kun\u00b7di\u00b7ger", "Wehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Gewiesen f\u00fcr alle Zeit sein!", "tokens": ["Ge\u00b7wie\u00b7sen", "f\u00fcr", "al\u00b7le", "Zeit", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Was dr\u00fcben des Fremden Gel\u00fcst verdarb", "tokens": ["Was", "dr\u00fc\u00b7ben", "des", "Frem\u00b7den", "Ge\u00b7l\u00fcst", "ver\u00b7darb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sei verbannt heut aus unserem Reim!", "tokens": ["Sei", "ver\u00b7bannt", "heut", "aus", "un\u00b7se\u00b7rem", "Reim", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wir singen dem Gl\u00fcck nur, das uns erwarb", "tokens": ["Wir", "sin\u00b7gen", "dem", "Gl\u00fcck", "nur", ",", "das", "uns", "er\u00b7warb"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein einziges sonniges Heim.", "tokens": ["Ein", "ein\u00b7zi\u00b7ges", "son\u00b7ni\u00b7ges", "Heim", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ihr aber seid uns von Herzen gegr\u00fc\u00dft,", "tokens": ["Ihr", "a\u00b7ber", "seid", "uns", "von", "Her\u00b7zen", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ihr Vogesengipfel im Blau!", "tokens": ["Ihr", "Vo\u00b7ge\u00b7sen\u00b7gip\u00b7fel", "im", "Blau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Du sch\u00f6ner Strom, der in Freiheit flie\u00dft", "tokens": ["Du", "sch\u00f6\u00b7ner", "Strom", ",", "der", "in", "Frei\u00b7heit", "flie\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Durch den prangenden deutschen Gau!", "tokens": ["Durch", "den", "pran\u00b7gen\u00b7den", "deut\u00b7schen", "Gau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Drum hinaus auf die Berg', und hinaus zu Thal,", "tokens": ["Drum", "hin\u00b7aus", "auf", "die", "Ber\u00b7g'", ",", "und", "hin\u00b7aus", "zu", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APZR", "APPR", "ART", "NN", "$,", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo uns die Rast ist bescheert!", "tokens": ["Und", "wo", "uns", "die", "Rast", "ist", "be\u00b7scheert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Gesungen, geklungen, wir haben die Wahl,", "tokens": ["Ge\u00b7sun\u00b7gen", ",", "ge\u00b7klun\u00b7gen", ",", "wir", "ha\u00b7ben", "die", "Wahl", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und was nur das Herz begehrt!", "tokens": ["Und", "was", "nur", "das", "Herz", "be\u00b7gehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob Diesen verbindet ein heimisches Band,", "tokens": ["Ob", "Die\u00b7sen", "ver\u00b7bin\u00b7det", "ein", "hei\u00b7mi\u00b7sches", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ob Jenen die Ferne gebracht,", "tokens": ["Ob", "Je\u00b7nen", "die", "Fer\u00b7ne", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Wir preisen das herrliche deutsche Land", "tokens": ["Wir", "prei\u00b7sen", "das", "herr\u00b7li\u00b7che", "deut\u00b7sche", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "In wieder erstandener Pracht!", "tokens": ["In", "wie\u00b7der", "er\u00b7stan\u00b7de\u00b7ner", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}