{"textgrid.poem.66996": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leben will dich \u00fcberlisten.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leben will dich \u00fcberlisten.", "tokens": ["Le\u00b7ben", "will", "dich", "\u00fc\u00b7berl\u00b7is\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollst mit edlern Listen lohnen.", "tokens": ["Sollst", "mit", "ed\u00b7lern", "Lis\u00b7ten", "loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Adler lasse nisten", "tokens": ["Dei\u00b7ne", "Ad\u00b7ler", "las\u00b7se", "nis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lachend in des Lebens Kronen!", "tokens": ["La\u00b7chend", "in", "des", "Le\u00b7bens", "Kro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}