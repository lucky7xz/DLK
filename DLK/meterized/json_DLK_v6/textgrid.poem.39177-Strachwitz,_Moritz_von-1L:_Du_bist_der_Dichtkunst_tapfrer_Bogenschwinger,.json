{"textgrid.poem.39177": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist der Dichtkunst tapfrer Bogenschwinger,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist der Dichtkunst tapfrer Bogenschwinger,", "tokens": ["Du", "bist", "der", "Dicht\u00b7kunst", "tapf\u00b7rer", "Bo\u00b7gen\u00b7schwin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der rastlos seine goldnen Pfeile sendet,", "tokens": ["Der", "rast\u00b7los", "sei\u00b7ne", "gold\u00b7nen", "Pfei\u00b7le", "sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der endlich trotzig sich verblutend endet", "tokens": ["Der", "end\u00b7lich", "trot\u00b7zig", "sich", "ver\u00b7blu\u00b7tend", "en\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "PRF", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als der Philister g\u00f6ttlicher Bezwinger.", "tokens": ["Als", "der", "Phi\u00b7lis\u00b7ter", "g\u00f6tt\u00b7li\u00b7cher", "Be\u00b7zwin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun schlummre sanft, Du kampfesm\u00fcder Ringer,", "tokens": ["Nun", "schlumm\u00b7re", "sanft", ",", "Du", "kamp\u00b7fes\u00b7m\u00fc\u00b7der", "Rin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Nord und S\u00fcden Ruhm und Preis gespendet;", "tokens": ["Dem", "Nord", "und", "S\u00fc\u00b7den", "Ruhm", "und", "Preis", "ge\u00b7spen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es sei Dein Haupt der Heimat zugewendet,", "tokens": ["Es", "sei", "Dein", "Haupt", "der", "Hei\u00b7mat", "zu\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du melodieenvoller Rhythmenschlinger!", "tokens": ["Du", "me\u00b7lo\u00b7di\u00b7e\u00b7en\u00b7vol\u00b7ler", "Rhyth\u00b7menschlin\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-++-", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Und ob die Vatererde Du gemieden", "tokens": ["Und", "ob", "die", "Va\u00b7te\u00b7rer\u00b7de", "Du", "ge\u00b7mie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im \u00dcberma\u00dfe Deiner Zorngedanken,", "tokens": ["Im", "\u00dc\u00b7berm\u00b7a\u00b7\u00dfe", "Dei\u00b7ner", "Zorn\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Reicht sie die Rechte doch ins Grab zum Frieden.", "tokens": ["Reicht", "sie", "die", "Rech\u00b7te", "doch", "ins", "Grab", "zum", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dahin, wo ums Grab sich Lorbeern ranken,", "tokens": ["Und", "da\u00b7hin", ",", "wo", "ums", "Grab", "sich", "Lor\u00b7beern", "ran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "PWAV", "APPRART", "NN", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sei auch der deutsche Eichenkranz beschieden,", "tokens": ["Sei", "auch", "der", "deut\u00b7sche", "Ei\u00b7chen\u00b7kranz", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Dein verbleib' er ewig, ohne Wanken!", "tokens": ["Und", "Dein", "ver\u00b7bleib'", "er", "e\u00b7wig", ",", "oh\u00b7ne", "Wan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PPER", "ADJD", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}