{"textgrid.poem.44275": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[die Liebe, sagt man sonst, kan alles in der Welt]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe, sagt man sonst, kan alles in der Welt,", "tokens": ["Die", "Lie\u00b7be", ",", "sagt", "man", "sonst", ",", "kan", "al\u00b7les", "in", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PIS", "ADV", "$,", "VMFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe, deren Bau durch keine Zeit zerf\u00e4llt,", "tokens": ["Die", "Lie\u00b7be", ",", "de\u00b7ren", "Bau", "durch", "kei\u00b7ne", "Zeit", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Liebe, so die Macht des Goldes \u00fcberwindet,", "tokens": ["Die", "Lie\u00b7be", ",", "so", "die", "Macht", "des", "Gol\u00b7des", "\u00fc\u00b7berw\u00b7in\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Gro\u00dfmuth niederschl\u00e4gt und selbst die Wei\u00dfheit bindet,", "tokens": ["Die", "Gro\u00df\u00b7muth", "nie\u00b7der\u00b7schl\u00e4gt", "und", "selbst", "die", "Wei\u00df\u00b7heit", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Wei\u00dfheit und den Tod. Die Liebe kan zwar viel,", "tokens": ["Die", "Wei\u00df\u00b7heit", "und", "den", "Tod", ".", "Die", "Lie\u00b7be", "kan", "zwar", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "ART", "NN", "VMFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur etwas kan sie nicht; denn da mein m\u00fcder Kiel,", "tokens": ["Nur", "et\u00b7was", "kan", "sie", "nicht", ";", "denn", "da", "mein", "m\u00fc\u00b7der", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "PTKNEG", "$.", "KON", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der noch von Briefen schwizt, an seine Pflicht gedencket,", "tokens": ["Der", "noch", "von", "Brie\u00b7fen", "schwizt", ",", "an", "sei\u00b7ne", "Pflicht", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weis ich so gut als er noch nicht, woran es hencket,", "tokens": ["Weis", "ich", "so", "gut", "als", "er", "noch", "nicht", ",", "wo\u00b7ran", "es", "hen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df weder Kraft noch Geist nach meinen W\u00fcntschen fliest.", "tokens": ["Da\u00df", "we\u00b7der", "Kraft", "noch", "Geist", "nach", "mei\u00b7nen", "W\u00fcnt\u00b7schen", "fliest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "NN", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ihr, die der Musen Quell bis an den Gurt begie\u00dft,", "tokens": ["Ihr", ",", "die", "der", "Mu\u00b7sen", "Quell", "bis", "an", "den", "Gurt", "be\u00b7gie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sagt, wo ich bitten darf, was vor ein scharf Gestirne", "tokens": ["Sagt", ",", "wo", "ich", "bit\u00b7ten", "darf", ",", "was", "vor", "ein", "scharf", "Ge\u00b7stir\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,", "PRELS", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit mir und meiner Faust an diesem Tage z\u00fcrne.", "tokens": ["Mit", "mir", "und", "mei\u00b7ner", "Faust", "an", "die\u00b7sem", "Ta\u00b7ge", "z\u00fcr\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Liebe, wie gesagt, thut Wunder in der Welt,", "tokens": ["Die", "Lie\u00b7be", ",", "wie", "ge\u00b7sagt", ",", "thut", "Wun\u00b7der", "in", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VVPP", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Macht gro\u00dfe Thoren klug, verlacht Gewalt und Geld", "tokens": ["Macht", "gro\u00b7\u00dfe", "Tho\u00b7ren", "klug", ",", "ver\u00b7lacht", "Ge\u00b7walt", "und", "Geld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ADJD", "$,", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und sizt haupts\u00e4chlich da, wo Redner und Poeten", "tokens": ["Und", "sizt", "haupt\u00b7s\u00e4ch\u00b7lich", "da", ",", "wo", "Red\u00b7ner", "und", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nach angebohrner Art bey Wein und Lust err\u00f6then.", "tokens": ["Nach", "an\u00b7ge\u00b7bohr\u00b7ner", "Art", "bey", "Wein", "und", "Lust", "er\u00b7r\u00f6\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie kommt es aber jezt, da\u00df selbst die Liebe schweigt", "tokens": ["Wie", "kommt", "es", "a\u00b7ber", "jezt", ",", "da\u00df", "selbst", "die", "Lie\u00b7be", "schweigt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und mir, so sehr ich schrey, nicht eine Gunst erzeigt,", "tokens": ["Und", "mir", ",", "so", "sehr", "ich", "schrey", ",", "nicht", "ei\u00b7ne", "Gunst", "er\u00b7zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "ADV", "PPER", "ADJD", "$,", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mir, der ich mich allein auf ihren Trost verspize", "tokens": ["Mir", ",", "der", "ich", "mich", "al\u00b7lein", "auf", "ih\u00b7ren", "Trost", "ver\u00b7spi\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und mehr als unsre Braut bey ihrem Reihen schwize?", "tokens": ["Und", "mehr", "als", "uns\u00b7re", "Braut", "bey", "ih\u00b7rem", "Rei\u00b7hen", "schwi\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich seh nun endlich wohl, was dies mein Zaudern heist:", "tokens": ["Ich", "seh", "nun", "end\u00b7lich", "wohl", ",", "was", "dies", "mein", "Zau\u00b7dern", "heist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$,", "PWS", "PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Ha\u00df der Poesie versagt mir H\u00fclf und Geist,", "tokens": ["Der", "Ha\u00df", "der", "Poe\u00b7sie", "ver\u00b7sagt", "mir", "H\u00fclf", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Der Phoebus h\u00f6rt mich nicht, er kehrt mir gar den R\u00fccken,", "tokens": ["Der", "Phoe\u00b7bus", "h\u00f6rt", "mich", "nicht", ",", "er", "kehrt", "mir", "gar", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als sollt ich seinen Zorn auch hinterw\u00e4rts erblicken.", "tokens": ["Als", "sollt", "ich", "sei\u00b7nen", "Zorn", "auch", "hin\u00b7ter\u00b7w\u00e4rts", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die Musen stehn und fliehn. Flieht, Musen, immerhin,", "tokens": ["Die", "Mu\u00b7sen", "stehn", "und", "fliehn", ".", "Flieht", ",", "Mu\u00b7sen", ",", "im\u00b7mer\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$.", "VVFIN", "$,", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Ursach eurer Flucht ist, da\u00df ich fl\u00fcchtig bin", "tokens": ["Die", "Ur\u00b7sach", "eu\u00b7rer", "Flucht", "ist", ",", "da\u00df", "ich", "fl\u00fcch\u00b7tig", "bin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und, wie ihr etwan scherzt, aus eurem edlen Orden", "tokens": ["Und", ",", "wie", "ihr", "et\u00b7wan", "scherzt", ",", "aus", "eu\u00b7rem", "ed\u00b7len", "Or\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Beym fl\u00fcchtigen Mercur zum Mammelucken worden.", "tokens": ["Beym", "fl\u00fcch\u00b7ti\u00b7gen", "Mer\u00b7cur", "zum", "Mam\u00b7me\u00b7lu\u00b7cken", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ja ja, da liegt der Hund; helft, Musen, wem ihr wollt,", "tokens": ["Ja", "ja", ",", "da", "liegt", "der", "Hund", ";", "helft", ",", "Mu\u00b7sen", ",", "wem", "ihr", "wollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "VVFIN", "$,", "NN", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich schw\u00f6re, da\u00df ihr mich gleichwohl nicht hemmen sollt,", "tokens": ["Ich", "schw\u00f6\u00b7re", ",", "da\u00df", "ihr", "mich", "gleich\u00b7wohl", "nicht", "hem\u00b7men", "sollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dies angefangne Blat als ein gewi\u00dfes Zeichen", "tokens": ["Dies", "an\u00b7ge\u00b7fang\u00b7ne", "Blat", "als", "ein", "ge\u00b7wi\u00b7\u00dfes", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Verwandter Redligkeit der werthen Braut zu reichen.", "tokens": ["Ver\u00b7wand\u00b7ter", "Red\u00b7lig\u00b7keit", "der", "wert\u00b7hen", "Braut", "zu", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Liebe schimpf ich nicht, denn da\u00df sie mich verl\u00e4st,", "tokens": ["Die", "Lie\u00b7be", "schimpf", "ich", "nicht", ",", "denn", "da\u00df", "sie", "mich", "ver\u00b7l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das macht, vergn\u00fcgte Braut, dein angenehmstes Fest.", "tokens": ["Das", "macht", ",", "ver\u00b7gn\u00fcg\u00b7te", "Braut", ",", "dein", "an\u00b7ge\u00b7nehms\u00b7tes", "Fest", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Bey diesem hat sie schon so viel zu thun bekommen,", "tokens": ["Bey", "die\u00b7sem", "hat", "sie", "schon", "so", "viel", "zu", "thun", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "ADV", "ADV", "PIS", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df ihre Sorgfalt nicht mein Schreyn in Acht genommen.", "tokens": ["Da\u00df", "ih\u00b7re", "Sorg\u00b7falt", "nicht", "mein", "Schreyn", "in", "Acht", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "APPR", "CARD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du siehst, vergn\u00fcgtes Paar, das Blat ist ziemlich voll,", "tokens": ["Du", "siehst", ",", "ver\u00b7gn\u00fcg\u00b7tes", "Paar", ",", "das", "Blat", "ist", "ziem\u00b7lich", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bevor ich selber weis, womit ich dienen soll.", "tokens": ["Be\u00b7vor", "ich", "sel\u00b7ber", "weis", ",", "wo\u00b7mit", "ich", "die\u00b7nen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ists mit galanter Kunst, so werd ich nichts erzwingen,", "tokens": ["Ists", "mit", "ga\u00b7lan\u00b7ter", "Kunst", ",", "so", "werd", "ich", "nichts", "er\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ists mit der Redligkeit, so will ich be\u00dfer singen.", "tokens": ["Ists", "mit", "der", "Red\u00b7lig\u00b7keit", ",", "so", "will", "ich", "be\u00b7\u00dfer", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Du bist es allerdings, gepriesne Redligkeit,", "tokens": ["Du", "bist", "es", "al\u00b7ler\u00b7dings", ",", "ge\u00b7pri\u00b7es\u00b7ne", "Red\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Die einzig und allein den Worten Kraft verleiht", "tokens": ["Die", "ein\u00b7zig", "und", "al\u00b7lein", "den", "Wor\u00b7ten", "Kraft", "ver\u00b7leiht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADV", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und die mich nach Geb\u00fchr mit sanfter St\u00e4rcke treibet,", "tokens": ["Und", "die", "mich", "nach", "Ge\u00b7b\u00fchr", "mit", "sanf\u00b7ter", "St\u00e4r\u00b7cke", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PRF", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da\u00df meine Schuldigkeit die frohen Zeilen schreibet.", "tokens": ["Da\u00df", "mei\u00b7ne", "Schul\u00b7dig\u00b7keit", "die", "fro\u00b7hen", "Zei\u00b7len", "schrei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Blutfreundschaft und Natur thun mehr als Wiz und Flei\u00df,", "tokens": ["Blut\u00b7freund\u00b7schaft", "und", "Na\u00b7tur", "thun", "mehr", "als", "Wiz", "und", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "ADV", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und, Vetter, deine Glut macht meine Feder hei\u00df,", "tokens": ["Und", ",", "Vet\u00b7ter", ",", "dei\u00b7ne", "Glut", "macht", "mei\u00b7ne", "Fe\u00b7der", "hei\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Bey wohlgetrofner Wahl die Silben einzurichten.", "tokens": ["Bey", "wohl\u00b7ge\u00b7trof\u00b7ner", "Wahl", "die", "Sil\u00b7ben", "ein\u00b7zu\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Die Warheit cr\u00f6nt den Vers, du weist, ich kan nicht dichten,", "tokens": ["Die", "War\u00b7heit", "cr\u00f6nt", "den", "Vers", ",", "du", "weist", ",", "ich", "kan", "nicht", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Daher vergn\u00fcgt es mich, da\u00df dies, was dich vergn\u00fcgt,", "tokens": ["Da\u00b7her", "ver\u00b7gn\u00fcgt", "es", "mich", ",", "da\u00df", "dies", ",", "was", "dich", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PDS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "(wie lange wird es noch?) in deinen Armen liegt", "tokens": ["(", "wie", "lan\u00b7ge", "wird", "es", "noch", "?", ")", "in", "dei\u00b7nen", "Ar\u00b7men", "liegt"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADV", "VAFIN", "PPER", "ADV", "$.", "$(", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und da\u00df ein ehrlich Herz, nach dem du starck gerungen,", "tokens": ["Und", "da\u00df", "ein", "ehr\u00b7lich", "Herz", ",", "nach", "dem", "du", "starck", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Die Freyheit deiner Brust so unverhoft bezwungen.", "tokens": ["Die", "Frey\u00b7heit", "dei\u00b7ner", "Brust", "so", "un\u00b7ver\u00b7hoft", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Gieb Achtung, sieh dich um (doch nur ins Ohr gesagt),", "tokens": ["Gieb", "Ach\u00b7tung", ",", "sieh", "dich", "um", "(", "doch", "nur", "ins", "Ohr", "ge\u00b7sagt", ")", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "NN", "$,", "VVIMP", "PPER", "APPR", "$(", "ADV", "ADV", "APPRART", "NN", "VVPP", "$(", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Was der und jener Freund in seiner Eh beklagt,", "tokens": ["Was", "der", "und", "je\u00b7ner", "Freund", "in", "sei\u00b7ner", "Eh", "be\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "KON", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Die, weil sie nur der Zwang der Mode nach geschlo\u00dfen,", "tokens": ["Die", ",", "weil", "sie", "nur", "der", "Zwang", "der", "Mo\u00b7de", "nach", "ge\u00b7schlo\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Gem\u00fcther paart und trennt. Was sieht man nicht vor Po\u00dfen,", "tokens": ["Ge\u00b7m\u00fc\u00b7ther", "paart", "und", "trennt", ".", "Was", "sieht", "man", "nicht", "vor", "Po\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "$.", "PWS", "VVFIN", "PIS", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Wenn bald ein Kuppelweib, bald eine tolle Magd", "tokens": ["Wenn", "bald", "ein", "Kup\u00b7pel\u00b7weib", ",", "bald", "ei\u00b7ne", "tol\u00b7le", "Magd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Sich blos aus Eigennuz zum Heiratsstiften wagt", "tokens": ["Sich", "blos", "aus", "Ei\u00b7gen\u00b7nuz", "zum", "Hei\u00b7rats\u00b7stif\u00b7ten", "wagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Und wenn wir insgeheim einander f\u00e4lschlich k\u00fc\u00dfen,", "tokens": ["Und", "wenn", "wir", "ins\u00b7ge\u00b7heim", "ein\u00b7an\u00b7der", "f\u00e4lschlich", "k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.60": {"text": "Da\u00df wir drauf ofentlich die Schande paaren m\u00fc\u00dfen.", "tokens": ["Da\u00df", "wir", "drauf", "o\u00b7fent\u00b7lich", "die", "Schan\u00b7de", "paa\u00b7ren", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ADJD", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.61": {"text": "Der Trieb, der dich erhizt, kommt aus der h\u00f6chsten Hand,", "tokens": ["Der", "Trieb", ",", "der", "dich", "er\u00b7hizt", ",", "kommt", "aus", "der", "h\u00f6chs\u00b7ten", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Die Vorsicht, nicht ein Mensch, kn\u00fcpft blos das feste Band,", "tokens": ["Die", "Vor\u00b7sicht", ",", "nicht", "ein", "Mensch", ",", "kn\u00fcpft", "blos", "das", "fes\u00b7te", "Band", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Das Seelen gleicher Art zum Lebensbaume f\u00fchret", "tokens": ["Das", "See\u00b7len", "glei\u00b7cher", "Art", "zum", "Le\u00b7bens\u00b7bau\u00b7me", "f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Und als ein s\u00fc\u00dfes Joch viel minder dr\u00fcckt als zieret.", "tokens": ["Und", "als", "ein", "s\u00fc\u00b7\u00dfes", "Joch", "viel", "min\u00b7der", "dr\u00fcckt", "als", "zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Ich la\u00dfe, wie du siehst, den Reimen freyen Lauf", "tokens": ["Ich", "la\u00b7\u00dfe", ",", "wie", "du", "siehst", ",", "den", "Rei\u00b7men", "frey\u00b7en", "Lauf"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Und halte dich gewis in Scherz und K\u00fc\u00dfen auf.", "tokens": ["Und", "hal\u00b7te", "dich", "ge\u00b7wis", "in", "Scherz", "und", "K\u00fc\u00b7\u00dfen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Doch scherz und k\u00fc\u00dfe nur, da der, so alles f\u00fchret,", "tokens": ["Doch", "scherz", "und", "k\u00fc\u00b7\u00dfe", "nur", ",", "da", "der", ",", "so", "al\u00b7les", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "ADV", "$,", "KOUS", "ART", "$,", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Dich und dein ander Herz mit gleichem Zuge r\u00fchret.", "tokens": ["Dich", "und", "dein", "an\u00b7der", "Herz", "mit", "glei\u00b7chem", "Zu\u00b7ge", "r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "ADJD", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.69": {"text": "Mein Buch, mein Contoir ist freylich kein Parna\u00df,", "tokens": ["Mein", "Buch", ",", "mein", "Con\u00b7to\u00b7ir", "ist", "frey\u00b7lich", "kein", "Par\u00b7na\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Der Phoebus lockt mich oft, doch ists nur allzeit Spa\u00df,", "tokens": ["Der", "Phoe\u00b7bus", "lockt", "mich", "oft", ",", "doch", "ists", "nur", "all\