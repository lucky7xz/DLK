{"dta.poem.8142": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Der Messias.  \n Achtzehnter Gesang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Adam sank zu den F\u00fc\u00dfen des Mittlers nieder, und fleht' ihm:", "tokens": ["A\u00b7dam", "sank", "zu", "den", "F\u00fc\u00b7\u00dfen", "des", "Mitt\u00b7lers", "nie\u00b7der", ",", "und", "fleht'", "ihm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Hab\u2019 ich Gnade vor dir gefunden; so la\u00df, o Messias,", "tokens": ["Hab'", "ich", "Gna\u00b7de", "vor", "dir", "ge\u00b7fun\u00b7den", ";", "so", "la\u00df", ",", "o", "Mes\u00b7si\u00b7as", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "APPR", "PPER", "VVPP", "$.", "ADV", "VVFIN", "$,", "FM", "FM", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Einige Blicke mich thun in die Folgen deiner Vers\u00f6hnung.", "tokens": ["Ei\u00b7ni\u00b7ge", "Bli\u00b7cke", "mich", "thun", "in", "die", "Fol\u00b7gen", "dei\u00b7ner", "Ver\u00b7s\u00f6h\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Adam, im Weltgerichte vollend\u2019 ich es Alles. Entferne", "tokens": ["A\u00b7dam", ",", "im", "Welt\u00b7ge\u00b7rich\u00b7te", "vollend'", "ich", "es", "Al\u00b7les", ".", "Ent\u00b7fer\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "APPRART", "NN", "VVFIN", "PPER", "PPER", "PIS", "$.", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Dich in jene Schatten der Cedern. Du sollst von der Tage", "tokens": ["Dich", "in", "je\u00b7ne", "Schat\u00b7ten", "der", "Ce\u00b7dern", ".", "Du", "sollst", "von", "der", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PDAT", "NN", "ART", "NN", "$.", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Letztem dort der milderen Schimmer einige sehen.", "tokens": ["Letz\u00b7tem", "dort", "der", "mil\u00b7de\u00b7ren", "Schim\u00b7mer", "ei\u00b7ni\u00b7ge", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "ADJA", "NN", "PIS", "VVINF", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Adam gieng in die Schatten der Cedern, und Schlummer, wie", "tokens": ["A\u00b7dam", "gieng", "in", "die", "Schat\u00b7ten", "der", "Ce\u00b7dern", ",", "und", "Schlum\u00b7mer", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "NN", "$,", "PWAV"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "ehmals", "tokens": ["eh\u00b7mals"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "In dem ruhigen Schoo\u00dfe des Paradieses, befiel ihn,", "tokens": ["In", "dem", "ru\u00b7hi\u00b7gen", "Schoo\u00b7\u00dfe", "des", "Pa\u00b7ra\u00b7die\u00b7ses", ",", "be\u00b7fiel", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und er sah ein Gesicht. Er kam, von Erstaunen belastet,", "tokens": ["Und", "er", "sah", "ein", "Ge\u00b7sicht", ".", "Er", "kam", ",", "von", "Er\u00b7stau\u00b7nen", "be\u00b7las\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Langsamen Schrittes, zur\u00fcck zu den Auferstandnen, und Engeln.", "tokens": ["Lang\u00b7sa\u00b7men", "Schrit\u00b7tes", ",", "zu\u00b7r\u00fcck", "zu", "den", "Auf\u00b7er\u00b7stand\u00b7nen", ",", "und", "En\u00b7geln", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,", "KON", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Sie umschwebten den Vater der Menschen, mit sanftem Verlangen,", "tokens": ["Sie", "um\u00b7schweb\u00b7ten", "den", "Va\u00b7ter", "der", "Men\u00b7schen", ",", "mit", "sanf\u00b7tem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Von dem milderem Schimmer des letzten Tages zu h\u00f6ren.", "tokens": ["Von", "dem", "mil\u00b7de\u00b7rem", "Schim\u00b7mer", "des", "letz\u00b7ten", "Ta\u00b7ges", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Adam setzte sich nieder auf einer der H\u00f6hen; sie setzten", "tokens": ["A\u00b7dam", "setz\u00b7te", "sich", "nie\u00b7der", "auf", "ei\u00b7ner", "der", "H\u00f6\u00b7hen", ";", "sie", "setz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "PTKVZ", "APPR", "ART", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Sich an dem Fusse des H\u00fcgels vor Christus Begnadigtem nieder.", "tokens": ["Sich", "an", "dem", "Fus\u00b7se", "des", "H\u00fc\u00b7gels", "vor", "Chris\u00b7tus", "Be\u00b7gna\u00b7dig\u00b7tem", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Einst am Tage des Herrn, als, auf der kommenden D\u00e4mm\u2019rung", "tokens": ["Einst", "am", "Ta\u00b7ge", "des", "Herrn", ",", "als", ",", "auf", "der", "kom\u00b7men\u00b7den", "D\u00e4m\u00b7m'\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,", "KOUS", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+---+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Fl\u00fcgel, vor mir die einsamen freudigen Stunden vorbeyflohn,", "tokens": ["Fl\u00fc\u00b7gel", ",", "vor", "mir", "die", "ein\u00b7sa\u00b7men", "freu\u00b7di\u00b7gen", "Stun\u00b7den", "vor\u00b7bey\u00b7flohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "ART", "ADJA", "ADJA", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Und ich forschete; kam die heilige Sionitin", "tokens": ["Und", "ich", "for\u00b7sche\u00b7te", ";", "kam", "die", "hei\u00b7li\u00b7ge", "Si\u00b7o\u00b7ni\u00b7tin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Gegen mich her. So war mir noch nie die Prophetin erschienen,", "tokens": ["Ge\u00b7gen", "mich", "her", ".", "So", "war", "mir", "noch", "nie", "die", "Pro\u00b7phe\u00b7tin", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "So viel Ewigkeit hatte noch niemals ihr Antlitz getragen!", "tokens": ["So", "viel", "E\u00b7wig\u00b7keit", "hat\u00b7te", "noch", "nie\u00b7mals", "ihr", "Ant\u00b7litz", "ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Und sie sang mir Adams Gesicht. Sie selber verstummte", "tokens": ["Und", "sie", "sang", "mir", "A\u00b7dams", "Ge\u00b7sicht", ".", "Sie", "sel\u00b7ber", "ver\u00b7stumm\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NE", "NN", "$.", "PPER", "ADV", "VVFIN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Oft, da sie sang. Die Wange gl\u00fcht\u2019 ihr, es drang zusehends", "tokens": ["Oft", ",", "da", "sie", "sang", ".", "Die", "Wan\u00b7ge", "gl\u00fcht'", "ihr", ",", "es", "drang", "zu\u00b7se\u00b7hends"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "In ihr gl\u00fchendes Angesicht schnelle Bl\u00e4\u00dfe. Die Lippe", "tokens": ["In", "ihr", "gl\u00fc\u00b7hen\u00b7des", "An\u00b7ge\u00b7sicht", "schnel\u00b7le", "Bl\u00e4\u00b7\u00dfe", ".", "Die", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$.", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Rufte stammelnde Donner, und ernst her blickt\u2019 ihr das Auge.", "tokens": ["Ruf\u00b7te", "stam\u00b7meln\u00b7de", "Don\u00b7ner", ",", "und", "ernst", "her", "blickt'", "ihr", "das", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "KON", "ADJD", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Fast entsank die Harfe der starrenden Hand, und die Krone", "tokens": ["Fast", "ent\u00b7sank", "die", "Har\u00b7fe", "der", "star\u00b7ren\u00b7den", "Hand", ",", "und", "die", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Bebt\u2019 um ihr fliegendes Haar. Dann erhub  sie sich wieder, dann", "tokens": ["Bebt'", "um", "ihr", "flie\u00b7gen\u00b7des", "Haar", ".", "Dann", "er\u00b7hub", "sie", "sich", "wie\u00b7der", ",", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "ADV"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.12": {"text": "kam ihr", "tokens": ["kam", "ihr"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Jedes L\u00e4cheln der ewigen Ruh in ihr Antlitz herunter.", "tokens": ["Je\u00b7des", "L\u00e4\u00b7cheln", "der", "e\u00b7wi\u00b7gen", "Ruh", "in", "ihr", "Ant\u00b7litz", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Dann mit hundert Fl\u00fcgeln gefl\u00fcgelt, mit Schwingen des Sturmes,", "tokens": ["Dann", "mit", "hun\u00b7dert", "Fl\u00fc\u00b7geln", "ge\u00b7fl\u00fc\u00b7gelt", ",", "mit", "Schwin\u00b7gen", "des", "Stur\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "VVPP", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.15": {"text": "Stiegen die Erstgebohrnen der Seele, die wahrsten Gedanken", "tokens": ["Stie\u00b7gen", "die", "Erst\u00b7ge\u00b7bohr\u00b7nen", "der", "See\u00b7le", ",", "die", "wahrs\u00b7ten", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Auf zu Gott. So sah sie mein Aug\u2019, und starrt\u2019 in die Nacht hin.", "tokens": ["Auf", "zu", "Gott", ".", "So", "sah", "sie", "mein", "Aug'", ",", "und", "starrt'", "in", "die", "Nacht", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.17": {"text": "Mit der Linken ber\u00fchrt\u2019 ich die Erde, mein Grab; und die Rechte", "tokens": ["Mit", "der", "Lin\u00b7ken", "be\u00b7r\u00fchrt'", "ich", "die", "Er\u00b7de", ",", "mein", "Grab", ";", "und", "die", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$.", "KON", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Hub ich gegen den Himmel empor. Der Erde Bewohner,", "tokens": ["Hub", "ich", "ge\u00b7gen", "den", "Him\u00b7mel", "em\u00b7por", ".", "Der", "Er\u00b7de", "Be\u00b7woh\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.19": {"text": "Oder des Grabes, was ich vermag, das will ich euch singen.", "tokens": ["O\u00b7der", "des", "Gra\u00b7bes", ",", "was", "ich", "ver\u00b7mag", ",", "das", "will", "ich", "euch", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Tausend Gedanken erflog mein Geist nicht; zu tausenden fehlt mir", "tokens": ["Tau\u00b7send", "Ge\u00b7dan\u00b7ken", "er\u00b7flog", "mein", "Geist", "nicht", ";", "zu", "tau\u00b7sen\u00b7den", "fehlt", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$.", "PTKZU", "VVINF", "VVFIN", "PPER"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.21": {"text": "Stimm\u2019 und Gesang; und tausendmal tausend verbarg sie dem H\u00f6rer.", "tokens": ["Stimm'", "und", "Ge\u00b7sang", ";", "und", "tau\u00b7send\u00b7mal", "tau\u00b7send", "ver\u00b7barg", "sie", "dem", "H\u00f6\u00b7rer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$.", "KON", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}}, "stanza.5": {"line.1": {"text": "Adam begann. So str\u00f6mten die Lippen des Erstgeschaffnen:", "tokens": ["A\u00b7dam", "be\u00b7gann", ".", "So", "str\u00f6m\u00b7ten", "die", "Lip\u00b7pen", "des", "Erst\u00b7ge\u00b7schaff\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Schnell, der Cherub denket so schnell, so wurd\u2019 ich gef\u00fchret", "tokens": ["Schnell", ",", "der", "Che\u00b7rub", "den\u00b7ket", "so", "schnell", ",", "so", "wurd'", "ich", "ge\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ART", "NE", "VVFIN", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Unter die Schaarenheere der auferstandenen Todten.", "tokens": ["Un\u00b7ter", "die", "Schaa\u00b7ren\u00b7hee\u00b7re", "der", "auf\u00b7er\u00b7stan\u00b7de\u00b7nen", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Grenzlos war das Gefild der Auferstehung. Sie warens", "tokens": ["Grenz\u00b7los", "war", "das", "Ge\u00b7fild", "der", "Auf\u00b7er\u00b7ste\u00b7hung", ".", "Sie", "wa\u00b7rens"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "NE"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Alle meine Kinder! O ewiger Vater der Wesen!", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Kin\u00b7der", "!", "O", "e\u00b7wi\u00b7ger", "Va\u00b7ter", "der", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$.", "NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Welch ein Anschaun war es! und welches das Anschaun dessen,", "tokens": ["Welch", "ein", "An\u00b7schaun", "war", "es", "!", "und", "wel\u00b7ches", "das", "An\u00b7schaun", "des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "PPER", "$.", "KON", "PWS", "ART", "NN", "PDS", "$,"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Der auf dem Throne sa\u00df, die Kinder Adams zu richten!", "tokens": ["Der", "auf", "dem", "Thro\u00b7ne", "sa\u00df", ",", "die", "Kin\u00b7der", "A\u00b7dams", "zu", "rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "V\u00e4ter des Mittlers, und ihr, o Engel! wie m\u00e4chtig empfand ich,", "tokens": ["V\u00e4\u00b7ter", "des", "Mitt\u00b7lers", ",", "und", "ihr", ",", "o", "En\u00b7gel", "!", "wie", "m\u00e4ch\u00b7tig", "emp\u00b7fand", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "PPER", "$,", "FM", "NE", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Was die Unsterblichkeit sey! Das Alles erblickt\u2019 ich, und lebte!", "tokens": ["Was", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "sey", "!", "Das", "Al\u00b7les", "er\u00b7blickt'", "ich", ",", "und", "leb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$.", "ART", "PIS", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$."], "meter": "--+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Siehe, der Tag wird kommen, dann werdet ihr alle das Heer sehn,", "tokens": ["Sie\u00b7he", ",", "der", "Tag", "wird", "kom\u00b7men", ",", "dann", "wer\u00b7det", "ihr", "al\u00b7le", "das", "Heer", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ART", "NN", "VAFIN", "VVINF", "$,", "ADV", "VAFIN", "PPER", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.