{"textgrid.poem.28560": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein wolbenamtes Volck sind gleichwol Hurenkinder.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein wolbenamtes Volck sind gleichwol Hurenkinder.", "tokens": ["Ein", "wol\u00b7ben\u00b7am\u00b7tes", "Volck", "sind", "gleich\u00b7wol", "Hu\u00b7ren\u00b7kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey Bauren heist man sie zwar so nichts desto minder,", "tokens": ["Bey", "Bau\u00b7ren", "heist", "man", "sie", "zwar", "so", "nichts", "des\u00b7to", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PPER", "ADV", "ADV", "PIS", "ADV", "ADV", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bey B\u00fcrgern besser noch Banckhart und im Geschlechte", "tokens": ["Bey", "B\u00fcr\u00b7gern", "bes\u00b7ser", "noch", "Ban\u00b7ck\u00b7hart", "und", "im", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "ADV", "NN", "KON", "APPRART", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Edlen Bastarten und Beyschlag, auch Un\u00e4chte", "tokens": ["Der", "Ed\u00b7len", "Bas\u00b7tar\u00b7ten", "und", "Bey\u00b7schlag", ",", "auch", "Un\u00b7\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,", "ADV", "NN"], "meter": "-+--+--+---+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Bey F\u00fcrst- und K\u00f6nigen. Mumm wil sich zwar bekennen,", "tokens": ["Bey", "F\u00fcr\u00b7st", "und", "K\u00f6\u00b7ni\u00b7gen", ".", "Mumm", "wil", "sich", "zwar", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$.", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Wohin man immer wil, wann Bauern ihn nicht nennen.", "tokens": ["Wo\u00b7hin", "man", "im\u00b7mer", "wil", ",", "wann", "Bau\u00b7ern", "ihn", "nicht", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VMFIN", "$,", "PWAV", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}}}}