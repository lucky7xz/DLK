{"textgrid.poem.35558": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie scheint doch alles Werdende so krank!", "tokens": ["Wie", "scheint", "doch", "al\u00b7les", "Wer\u00b7den\u00b7de", "so", "krank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Ein Fieberhauch um einen Weiler kreist;", "tokens": ["Ein", "Fie\u00b7ber\u00b7hauch", "um", "ei\u00b7nen", "Wei\u00b7ler", "kreist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch aus Gezweigen winkt ein sanfter Geist", "tokens": ["Doch", "aus", "Ge\u00b7zwei\u00b7gen", "winkt", "ein", "sanf\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00f6ffnet das Gem\u00fcte weit und bang.", "tokens": ["Und", "\u00f6ff\u00b7net", "das", "Ge\u00b7m\u00fc\u00b7te", "weit", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein bl\u00fchender Ergu\u00df verrinnt sehr sacht", "tokens": ["Ein", "bl\u00fc\u00b7hen\u00b7der", "Er\u00b7gu\u00df", "ver\u00b7rinnt", "sehr", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Ungebornes pflegt der eignen Ruh.", "tokens": ["Und", "Un\u00b7ge\u00b7bor\u00b7nes", "pflegt", "der", "eig\u00b7nen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebenden bl\u00fchn ihren Sternen zu", "tokens": ["Die", "Lie\u00b7ben\u00b7den", "bl\u00fchn", "ih\u00b7ren", "Ster\u00b7nen", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKZU"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und s\u00fc\u00dfer flie\u00dft ihr Odem durch die Nacht.", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "flie\u00dft", "ihr", "O\u00b7dem", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So schmerzlich gut und wahrhaft ist, was lebt;", "tokens": ["So", "schmerz\u00b7lich", "gut", "und", "wahr\u00b7haft", "ist", ",", "was", "lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADV", "VAFIN", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und leise r\u00fchrt dich an ein alter Stein:", "tokens": ["Und", "lei\u00b7se", "r\u00fchrt", "dich", "an", "ein", "al\u00b7ter", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wahrlich! Ich werde immer bei euch sein.", "tokens": ["Wahr\u00b7lich", "!", "Ich", "wer\u00b7de", "im\u00b7mer", "bei", "euch", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "O Mund! der durch die Silberweide bebt.", "tokens": ["O", "Mund", "!", "der", "durch", "die", "Sil\u00b7ber\u00b7wei\u00b7de", "bebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}