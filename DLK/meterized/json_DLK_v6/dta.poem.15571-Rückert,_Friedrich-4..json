{"dta.poem.15571": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sowahr du hier die Welt nur kannst im Zwielicht sehn,", "tokens": ["So\u00b7wahr", "du", "hier", "die", "Welt", "nur", "kannst", "im", "Zwie\u00b7licht", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADV", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sowahr wird sie dir dort im vollen Glanze stehn.", "tokens": ["So\u00b7wahr", "wird", "sie", "dir", "dort", "im", "vol\u00b7len", "Glan\u00b7ze", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was also bist du aufs Unm\u00f6gliche beflissen,", "tokens": ["Was", "al\u00b7so", "bist", "du", "aufs", "Un\u00b7m\u00f6g\u00b7li\u00b7che", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umsonst zu forschen, was du einst von selbst wirst wissen?", "tokens": ["Um\u00b7sonst", "zu", "for\u00b7schen", ",", "was", "du", "einst", "von", "selbst", "wirst", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PWS", "PPER", "ADV", "APPR", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weil Trieb nach Wahrheit nur die B\u00fcrgschaft ist des Wahren.", "tokens": ["Weil", "Trieb", "nach", "Wahr\u00b7heit", "nur", "die", "B\u00fcrg\u00b7schaft", "ist", "des", "Wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "ADV", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur was du suchtest hier, das wirst du dort erfahren.", "tokens": ["Nur", "was", "du", "such\u00b7test", "hier", ",", "das", "wirst", "du", "dort", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "ADV", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}