{"dta.poem.12628": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XvI.  \n  Mischmasch.  \n   \n  Ne perdas operam: qui se mirantur, in illos  \n Virus habe, nos h\u00e6c novimus esse nihil.  \n  martialis ,  Lib. XIII. Epigr.  2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wein-Trunk erh\u00e4lt.", "tokens": ["Der", "Wein\u00b7Trunk", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Das lehrten die Welt", "tokens": ["Das", "lehr\u00b7ten", "die", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Druiden und Barden und Magi.", "tokens": ["Dru\u00b7i\u00b7den", "und", "Bar\u00b7den", "und", "Ma\u00b7gi", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NE", "$."], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie hatten auch Recht;", "tokens": ["Sie", "hat\u00b7ten", "auch", "Recht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Das findet wer zecht", "tokens": ["Das", "fin\u00b7det", "wer", "zecht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PWS", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Freund! trinke getreu;", "tokens": ["Freund", "!", "trin\u00b7ke", "ge\u00b7treu", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADJD", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So wollen wir zwey", "tokens": ["So", "wol\u00b7len", "wir", "zwey"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "CARD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Verschreib mir vom Rhein", "tokens": ["Ver\u00b7schreib", "mir", "vom", "Rhein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "F\u00fcrtreflichern Wein,", "tokens": ["F\u00fcr\u00b7tre\u00b7fli\u00b7chern", "Wein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "O dieser verj\u00fcngt", "tokens": ["O", "die\u00b7ser", "ver\u00b7j\u00fcngt"], "token_info": ["word", "word", "word"], "pos": ["NE", "PDS", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Den der ihn besingt:", "tokens": ["Den", "der", "ihn", "be\u00b7singt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auch ist er, mein Freund,", "tokens": ["Auch", "ist", "er", ",", "mein", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der Liebe nicht feind,", "tokens": ["Der", "Lie\u00b7be", "nicht", "feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Man lehret uns zwar,", "tokens": ["Man", "leh\u00b7ret", "uns", "zwar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Vielleicht ists auch wahr:", "tokens": ["Viel\u00b7leicht", "ists", "auch", "wahr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dem sey wie ihm sey,", "tokens": ["Dem", "sey", "wie", "ihm", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "So bleibt es dabey:", "tokens": ["So", "bleibt", "es", "da\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Es lebe dein Kind!", "tokens": ["Es", "le\u00b7be", "dein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie ist es gesinnt?", "tokens": ["Wie", "ist", "es", "ge\u00b7sinnt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du seufzest ja hier,", "tokens": ["Du", "seuf\u00b7zest", "ja", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Als sagtest du mir:", "tokens": ["Als", "sag\u00b7test", "du", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Doch Rhein-Wein und Scherz", "tokens": ["Doch", "Rhein\u00b7Wein", "und", "Scherz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NN"], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Erfrischen das Herz,", "tokens": ["Er\u00b7fri\u00b7schen", "das", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dein Gram wird ertr\u00e4nkt,", "tokens": ["Dein", "Gram", "wird", "er\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wenn Phyllis dich kr\u00e4nkt,", "tokens": ["Wenn", "Phyl\u00b7lis", "dich", "kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}