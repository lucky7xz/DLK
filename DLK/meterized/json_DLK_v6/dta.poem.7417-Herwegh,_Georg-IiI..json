{"dta.poem.7417": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus H\u00fctten einzig kommt das Heil der Welt,", "tokens": ["Aus", "H\u00fct\u00b7ten", "ein\u00b7zig", "kommt", "das", "Heil", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im h\u00e4rnen Mantel predigt der Prophete \u2014", "tokens": ["Im", "h\u00e4r\u00b7nen", "Man\u00b7tel", "pre\u00b7digt", "der", "Pro\u00b7phe\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So ward auch Blei, und nicht das Gold, bestellt,", "tokens": ["So", "ward", "auch", "Blei", ",", "und", "nicht", "das", "Gold", ",", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$,", "KON", "PTKNEG", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df tausendz\u00fcngig jede Wahrheit rede.", "tokens": ["Da\u00df", "tau\u00b7send\u00b7z\u00fcn\u00b7gig", "je\u00b7de", "Wahr\u00b7heit", "re\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein b\u00f6ser Geist der Tiefe haust im Gold,", "tokens": ["Ein", "b\u00f6\u00b7ser", "Geist", "der", "Tie\u00b7fe", "haust", "im", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es ist ein Knecht und gibt sich gern in Sold;", "tokens": ["Es", "ist", "ein", "Knecht", "und", "gibt", "sich", "gern", "in", "Sold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "PRF", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie Porzia, fa\u00dft das Beste man in Blei,", "tokens": ["Wie", "Por\u00b7zia", ",", "fa\u00dft", "das", "Bes\u00b7te", "man", "in", "Blei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "VVFIN", "ART", "NN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und reimt man drauf, so reimt man immer: Frei!", "tokens": ["Und", "reimt", "man", "drauf", ",", "so", "reimt", "man", "im\u00b7mer", ":", "Frei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PAV", "$,", "ADV", "VVFIN", "PIS", "ADV", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das schwere Blei wird in des Meisters Hand", "tokens": ["Das", "schwe\u00b7re", "Blei", "wird", "in", "des", "Meis\u00b7ters", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Elfengeister luftiges Gewand;", "tokens": ["Der", "El\u00b7fen\u00b7geis\u00b7ter", "luf\u00b7ti\u00b7ges", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Er l\u00e4\u00dft es nicht als Todeskugel fliegen,", "tokens": ["Er", "l\u00e4\u00dft", "es", "nicht", "als", "To\u00b7des\u00b7ku\u00b7gel", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Er f\u00fchret es als Wort von Sieg zu Siegen,", "tokens": ["Er", "f\u00fch\u00b7ret", "es", "als", "Wort", "von", "Sieg", "zu", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und wo die beste Waffe fehlt von Erz,", "tokens": ["Und", "wo", "die", "bes\u00b7te", "Waf\u00b7fe", "fehlt", "von", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da trifft ein Wort des rechten Mannes Herz;", "tokens": ["Da", "trifft", "ein", "Wort", "des", "rech\u00b7ten", "Man\u00b7nes", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Es zittert nicht vor des Tyrannen Miene \u2014", "tokens": ["Es", "zit\u00b7tert", "nicht", "vor", "des", "Ty\u00b7ran\u00b7nen", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Was will die Flocke gegen die Lawine?", "tokens": ["Was", "will", "die", "Flo\u00b7cke", "ge\u00b7gen", "die", "La\u00b7wi\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Kein Censor f\u00e4llt der Wahrheit in die Z\u00fcgel,", "tokens": ["Kein", "Cen\u00b7sor", "f\u00e4llt", "der", "Wahr\u00b7heit", "in", "die", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Er hat nur Federn, doch die Wahrheit Fl\u00fcgel.", "tokens": ["Er", "hat", "nur", "Fe\u00b7dern", ",", "doch", "die", "Wahr\u00b7heit", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}