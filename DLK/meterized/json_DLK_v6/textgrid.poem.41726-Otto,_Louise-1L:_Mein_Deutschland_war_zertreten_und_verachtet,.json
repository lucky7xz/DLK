{"textgrid.poem.41726": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Deutschland war zertreten und verachtet,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Deutschland war zertreten und verachtet,", "tokens": ["Mein", "Deutschland", "war", "zer\u00b7tre\u00b7ten", "und", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zerrissen und ein Spielball fremder M\u00e4chte,", "tokens": ["Zer\u00b7ris\u00b7sen", "und", "ein", "Spiel\u00b7ball", "frem\u00b7der", "M\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das deutsche Volk von Sklaverei umnachtet", "tokens": ["Das", "deut\u00b7sche", "Volk", "von", "Skla\u00b7ve\u00b7rei", "um\u00b7nach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selber seine F\u00fcrsten waren Knechte,", "tokens": ["Und", "sel\u00b7ber", "sei\u00b7ne", "F\u00fcrs\u00b7ten", "wa\u00b7ren", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da that es not aus seinem Schlaf es wecken,", "tokens": ["Da", "that", "es", "not", "aus", "sei\u00b7nem", "Schlaf", "es", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da that ein Wort voll deutschen Sinnes not,", "tokens": ["Da", "that", "ein", "Wort", "voll", "deut\u00b7schen", "Sin\u00b7nes", "not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da war es Zeit ihm seine Schmach entdecken", "tokens": ["Da", "war", "es", "Zeit", "ihm", "sei\u00b7ne", "Schmach", "ent\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und jede k\u00fcnft'ge die ihm droht.", "tokens": ["Und", "je\u00b7de", "k\u00fcnft'\u00b7ge", "die", "ihm", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da ri\u00df Verzweiflung st\u00fcrmisch in die Saiten", "tokens": ["Da", "ri\u00df", "Ver\u00b7zwei\u00b7flung", "st\u00fcr\u00b7misch", "in", "die", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Harfen, die an deutschen Eichen hangen.", "tokens": ["Der", "Har\u00b7fen", ",", "die", "an", "deut\u00b7schen", "Ei\u00b7chen", "han\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da galt's mit Worten, wie mit Thaten streiten", "tokens": ["Da", "galt's", "mit", "Wor\u00b7ten", ",", "wie", "mit", "Tha\u00b7ten", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "PWAV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auch die Dichter k\u00e4mpften wie sie sangen.", "tokens": ["Und", "auch", "die", "Dich\u00b7ter", "k\u00e4mpf\u00b7ten", "wie", "sie", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "KOKOM", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und deutsche Kraft galt's in der Jugend n\u00e4hren:", "tokens": ["Und", "deut\u00b7sche", "Kraft", "galt's", "in", "der", "Ju\u00b7gend", "n\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Volk stand auf \u2013 \u00bbder Sturm bricht los \u2013\u00ab brach los!", "tokens": ["Das", "Volk", "stand", "auf", "\u2013", "\u00bb", "der", "Sturm", "bricht", "los", "\u2013", "\u00ab", "brach", "los", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$(", "$(", "ART", "NN", "VVFIN", "PTKVZ", "$(", "$(", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das war das Werk den deutschen Sinn zu n\u00e4hren", "tokens": ["Das", "war", "das", "Werk", "den", "deut\u00b7schen", "Sinn", "zu", "n\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und deutsche Kraft wuchs riesengro\u00df!", "tokens": ["Und", "deut\u00b7sche", "Kraft", "wuchs", "rie\u00b7sen\u00b7gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Tapfren viele sind im Kampf geblieben", "tokens": ["Der", "Tapf\u00b7ren", "vie\u00b7le", "sind", "im", "Kampf", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der deutschen Jugend F\u00fchrer fehlen viele,", "tokens": ["Der", "deut\u00b7schen", "Ju\u00b7gend", "F\u00fch\u00b7rer", "feh\u00b7len", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch fehlt darum nicht unser br\u00fcnstig Lieben", "tokens": ["Doch", "fehlt", "da\u00b7rum", "nicht", "un\u00b7ser", "br\u00fcns\u00b7tig", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "PTKNEG", "PPOSAT", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr sie, die uns gezeigt den Weg zum Ziele;", "tokens": ["F\u00fcr", "sie", ",", "die", "uns", "ge\u00b7zeigt", "den", "Weg", "zum", "Zie\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "VVPP", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr sie, die aus der Hand die Fahne geben,", "tokens": ["F\u00fcr", "sie", ",", "die", "aus", "der", "Hand", "die", "Fah\u00b7ne", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Damit die Jugend unsrer neuen Zeit", "tokens": ["Da\u00b7mit", "die", "Ju\u00b7gend", "uns\u00b7rer", "neu\u00b7en", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie selber lerne triumphierend heben", "tokens": ["Sie", "sel\u00b7ber", "ler\u00b7ne", "tri\u00b7um\u00b7phie\u00b7rend", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu neuer Siege Herrlichkeit.", "tokens": ["Zu", "neu\u00b7er", "Sie\u00b7ge", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So rauschte jetzt, gleich wie durch m\u00e4cht'ge Eichen", "tokens": ["So", "rauschte", "jetzt", ",", "gleich", "wie", "durch", "m\u00e4cht'\u00b7ge", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADV", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Abendwind erinnrungsfl\u00fcsternd geht,", "tokens": ["Ein", "Ab\u00b7end\u00b7wind", "e\u00b7rinn\u00b7rungs\u00b7fl\u00fcs\u00b7ternd", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dein Name aus den Bl\u00e4ttern, aus den Zweigen", "tokens": ["Dein", "Na\u00b7me", "aus", "den", "Bl\u00e4t\u00b7tern", ",", "aus", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Darin der Geist der neuen Zeiten weht", "tokens": ["Da\u00b7rin", "der", "Geist", "der", "neu\u00b7en", "Zei\u00b7ten", "weht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und an dem stillen Abend Deines Lebens", "tokens": ["Und", "an", "dem", "stil\u00b7len", "A\u00b7bend", "Dei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bezeugt es Dir die deutsche Nation:", "tokens": ["Be\u00b7zeugt", "es", "Dir", "die", "deut\u00b7sche", "Na\u00b7tion", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Du lebtest nicht, f\u00fcr sie auch nicht vergebens,", "tokens": ["Du", "leb\u00b7test", "nicht", ",", "f\u00fcr", "sie", "auch", "nicht", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "APPR", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Deutsche ist auch jetzt dein Sohn.", "tokens": ["Der", "Deut\u00b7sche", "ist", "auch", "jetzt", "dein", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum f\u00fchlt ich stolzer jetzt das Herz mir schlagen", "tokens": ["Drum", "f\u00fchlt", "ich", "stol\u00b7zer", "jetzt", "das", "Herz", "mir", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ich Dein v\u00e4terliches Wort empfangen;", "tokens": ["Als", "ich", "Dein", "v\u00e4\u00b7ter\u00b7li\u00b7ches", "Wort", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wolltest mir den Segen nicht versagen", "tokens": ["Du", "woll\u00b7test", "mir", "den", "Se\u00b7gen", "nicht", "ver\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu meines Strebens br\u00fcnstigem Verlangen;", "tokens": ["Zu", "mei\u00b7nes", "Stre\u00b7bens", "br\u00fcns\u00b7ti\u00b7gem", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gleich wie dem Epheu an der S\u00e4ule Fu\u00dfe,", "tokens": ["Gleich", "wie", "dem", "E\u00b7pheu", "an", "der", "S\u00e4u\u00b7le", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die eines Tempels Hallen tr\u00e4gt und schm\u00fcckt,", "tokens": ["Die", "ei\u00b7nes", "Tem\u00b7pels", "Hal\u00b7len", "tr\u00e4gt", "und", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vor seinem Eingang mit lebendgem Gru\u00dfe", "tokens": ["Vor", "sei\u00b7nem", "Ein\u00b7gang", "mit", "le\u00b7bend\u00b7gem", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die hohe Eiche fl\u00fcsternd nickt.", "tokens": ["Die", "ho\u00b7he", "Ei\u00b7che", "fl\u00fcs\u00b7ternd", "nickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der deuschen Einheit und dem deutschen Sinne", "tokens": ["Der", "deu\u00b7schen", "Ein\u00b7heit", "und", "dem", "deut\u00b7schen", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist solch ein heilger Tempel aufgerichtet.", "tokens": ["Ist", "solch", "ein", "heil\u00b7ger", "Tem\u00b7pel", "auf\u00b7ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du gr\u00fc\u00dft als mark'ge Eiche seine Zinne,", "tokens": ["Du", "gr\u00fc\u00dft", "als", "mark'\u00b7ge", "Ei\u00b7che", "sei\u00b7ne", "Zin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lustig will ich um die S\u00e4ulen steigen", "tokens": ["Und", "lus\u00b7tig", "will", "ich", "um", "die", "S\u00e4u\u00b7len", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und fr\u00f6hlich spielen mit dem gr\u00fcnen Kranz:", "tokens": ["Und", "fr\u00f6h\u00b7lich", "spie\u00b7len", "mit", "dem", "gr\u00fc\u00b7nen", "Kranz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den deutschen ", "tokens": ["Den", "deut\u00b7schen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Des deutschen Vaterlandes Glanz.", "tokens": ["Des", "deut\u00b7schen", "Va\u00b7ter\u00b7lan\u00b7des", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du wollst dein Volk als deutsche M\u00e4nner sehen", "tokens": ["Du", "wollst", "dein", "Volk", "als", "deut\u00b7sche", "M\u00e4n\u00b7ner", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KOUS", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und deutsche M\u00e4nner sind dem Volk erstanden \u2013", "tokens": ["Und", "deut\u00b7sche", "M\