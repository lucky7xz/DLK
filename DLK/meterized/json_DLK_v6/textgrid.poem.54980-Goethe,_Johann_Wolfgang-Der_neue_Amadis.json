{"textgrid.poem.54980": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Amadis", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich noch ein Knabe war,", "tokens": ["Als", "ich", "noch", "ein", "Kna\u00b7be", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sperrte man mich ein;", "tokens": ["Sperr\u00b7te", "man", "mich", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und so sa\u00df ich manches Jahr", "tokens": ["Und", "so", "sa\u00df", "ich", "man\u00b7ches", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber mir allein,", "tokens": ["\u00dc\u00b7ber", "mir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wie in Mutterleib.", "tokens": ["Wie", "in", "Mut\u00b7ter\u00b7leib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch du warst mein Zeitvertreib,", "tokens": ["Doch", "du", "warst", "mein", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldne Phantasie,", "tokens": ["Gold\u00b7ne", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich ward ein warmer Held,", "tokens": ["Und", "ich", "ward", "ein", "war\u00b7mer", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Prinz Pipi,", "tokens": ["Wie", "der", "Prinz", "Pi\u00b7pi", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und durchzog die Welt.", "tokens": ["Und", "durch\u00b7zog", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Baute manch kristallen Schlo\u00df", "tokens": ["Bau\u00b7te", "manch", "kris\u00b7tal\u00b7len", "Schlo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zerst\u00f6rt es auch,", "tokens": ["Und", "zer\u00b7st\u00f6rt", "es", "auch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Warf mein blinkendes Gescho\u00df", "tokens": ["Warf", "mein", "blin\u00b7ken\u00b7des", "Ge\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drachen durch den Bauch,", "tokens": ["Dra\u00b7chen", "durch", "den", "Bauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ja, ich war ein Mann!", "tokens": ["Ja", ",", "ich", "war", "ein", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ritterlich befreit ich dann", "tokens": ["Rit\u00b7ter\u00b7lich", "be\u00b7freit", "ich", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Prinzessin Fisch;", "tokens": ["Die", "Prin\u00b7zes\u00b7sin", "Fisch", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie war gar zu obligeant,", "tokens": ["Sie", "war", "gar", "zu", "ob\u00b7li\u00b7ge\u00b7ant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "F\u00fchrte mich zu Tisch,", "tokens": ["F\u00fchr\u00b7te", "mich", "zu", "Tisch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und ich war galant.", "tokens": ["Und", "ich", "war", "ga\u00b7lant", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und ihr Ku\u00df war G\u00f6tterbrot,", "tokens": ["Und", "ihr", "Ku\u00df", "war", "G\u00f6t\u00b7ter\u00b7brot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fchend wie der Wein.", "tokens": ["Gl\u00fc\u00b7hend", "wie", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach! ich liebte fast mich tot!", "tokens": ["Ach", "!", "ich", "lieb\u00b7te", "fast", "mich", "tot", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ADV", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rings mit Sonnenschein", "tokens": ["Rings", "mit", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "War sie emailliert.", "tokens": ["War", "sie", "e\u00b7mail\u00b7liert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ach! wer hat sie mir entf\u00fchrt?", "tokens": ["Ach", "!", "wer", "hat", "sie", "mir", "ent\u00b7f\u00fchrt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hielt kein Zauberband", "tokens": ["Hielt", "kein", "Zau\u00b7ber\u00b7band"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sie zur\u00fcck vom schnellen Fliehn?", "tokens": ["Sie", "zu\u00b7r\u00fcck", "vom", "schnel\u00b7len", "Fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt, wo ist ihr Land?", "tokens": ["Sagt", ",", "wo", "ist", "ihr", "Land", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wo der Weg dahin?", "tokens": ["Wo", "der", "Weg", "da\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PAV", "$."], "meter": "--+-+", "measure": "anapaest.init"}}}}}