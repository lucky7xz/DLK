{"textgrid.poem.42608": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der R\u00e4uber", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst am sch\u00f6nen Fr\u00fchlingstage", "tokens": ["Einst", "am", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7lings\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt der R\u00e4uber vor den Wald.", "tokens": ["Tritt", "der", "R\u00e4u\u00b7ber", "vor", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh! den hohlen Pfad hernieder", "tokens": ["Sieh", "!", "den", "hoh\u00b7len", "Pfad", "her\u00b7nie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt ein schlankes M\u00e4dchen bald.", "tokens": ["Kommt", "ein", "schlan\u00b7kes", "M\u00e4d\u00b7chen", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbtr\u00fcgst du statt der Maienglocken\u00ab,", "tokens": ["\u00bb", "tr\u00fcgst", "du", "statt", "der", "Mai\u00b7en\u00b7glo\u00b7cken", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spricht des Waldes k\u00fchner Sohn,", "tokens": ["Spricht", "des", "Wal\u00b7des", "k\u00fch\u00b7ner", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbin dem Korb den Schmuck des K\u00f6nigs,", "tokens": ["\u00bb", "in", "dem", "Korb", "den", "Schmuck", "des", "K\u00f6\u00b7nigs", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Frei doch z\u00f6gest du davon.\u00ab", "tokens": ["Frei", "doch", "z\u00f6\u00b7gest", "du", "da\u00b7von", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PAV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Lange folgen seine Blicke", "tokens": ["Lan\u00b7ge", "fol\u00b7gen", "sei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der geliebten Wallerin.", "tokens": ["Der", "ge\u00b7lieb\u00b7ten", "Wal\u00b7le\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Durch die Wiesengr\u00fcnde wandelt", "tokens": ["Durch", "die", "Wie\u00b7sen\u00b7gr\u00fcn\u00b7de", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sie zu stillen D\u00f6rfern hin,", "tokens": ["Sie", "zu", "stil\u00b7len", "D\u00f6r\u00b7fern", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Bis der G\u00e4rten reiche Bl\u00fcte", "tokens": ["Bis", "der", "G\u00e4r\u00b7ten", "rei\u00b7che", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "H\u00fcllt die liebliche Gestalt.", "tokens": ["H\u00fcllt", "die", "lieb\u00b7li\u00b7che", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Doch der R\u00e4uber kehret wieder", "tokens": ["Doch", "der", "R\u00e4u\u00b7ber", "keh\u00b7ret", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "In den finstern Tannenwald.", "tokens": ["In", "den", "fins\u00b7tern", "Tan\u00b7nen\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}