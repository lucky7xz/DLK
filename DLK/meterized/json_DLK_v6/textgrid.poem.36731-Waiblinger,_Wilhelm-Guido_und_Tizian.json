{"textgrid.poem.36731": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Guido und Tizian", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tte Tizian's Pinsel die Seele Guido's gef\u00fchret,", "tokens": ["H\u00e4t\u00b7te", "Ti\u00b7zian's", "Pin\u00b7sel", "die", "See\u00b7le", "Gui\u00b7do's", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "ART", "NN", "NE", "VVPP", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "S\u00e4h' ich Vollendung im Bund geist'ger und sinnlicher Kraft.", "tokens": ["S\u00e4h'", "ich", "Vol\u00b7len\u00b7dung", "im", "Bund", "geist'\u00b7ger", "und", "sinn\u00b7li\u00b7cher", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPRART", "NN", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}}}}}