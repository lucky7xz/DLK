{"textgrid.poem.38330": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "11. Erziehung durch Ahndung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkst du nicht, Maria, mehr an die ausgestandnen Schmerzen,", "tokens": ["Denkst", "du", "nicht", ",", "Ma\u00b7ria", ",", "mehr", "an", "die", "aus\u00b7ge\u00b7stand\u00b7nen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "NE", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Als das kleine Jesulein in dir ein Gestalt gewann?", "tokens": ["Als", "das", "klei\u00b7ne", "Je\u00b7su\u00b7lein", "in", "dir", "ein", "Ge\u00b7stalt", "ge\u00b7wann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "O wie sollt ich ihn nicht drum tausendmal im Glauben herzen,", "tokens": ["O", "wie", "sollt", "ich", "ihn", "nicht", "drum", "tau\u00b7send\u00b7mal", "im", "Glau\u00b7ben", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "PPER", "PTKNEG", "PAV", "ADV", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Da er nun zusehens w\u00e4chst, mir zum Br\u00e4utgam und zum Mann.", "tokens": ["Da", "er", "nun", "zu\u00b7se\u00b7hens", "w\u00e4chst", ",", "mir", "zum", "Br\u00e4ut\u00b7gam", "und", "zum", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "PPER", "APPRART", "NE", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Hat Johannes nicht vor Freud, schon im Mutterleib gesprungen,", "tokens": ["Hat", "Jo\u00b7han\u00b7nes", "nicht", "vor", "Freud", ",", "schon", "im", "Mut\u00b7ter\u00b7leib", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "APPR", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Spielt er nicht zum voraus schon, eh er noch kam an das Licht;", "tokens": ["Spielt", "er", "nicht", "zum", "vo\u00b7raus", "schon", ",", "eh", "er", "noch", "kam", "an", "das", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Haben wir nicht seine Freund oft sein Hochzeitslied gesungen,", "tokens": ["Ha\u00b7ben", "wir", "nicht", "sei\u00b7ne", "Freund", "oft", "sein", "Hoch\u00b7zeits\u00b7lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Hat man mir mit Fingern da dieses Kind gezeiget nicht.", "tokens": ["Hat", "man", "mir", "mit", "Fin\u00b7gern", "da", "die\u00b7ses", "Kind", "ge\u00b7zei\u00b7get", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "APPR", "NN", "ADV", "PDAT", "NN", "VVPP", "PTKNEG", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nun liegt mir dies Kind im Schoo\u00df! Nun hab ich das Lamm vor Augen,", "tokens": ["Nun", "liegt", "mir", "dies", "Kind", "im", "Schoo\u00df", "!", "Nun", "hab", "ich", "das", "Lamm", "vor", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "NN", "APPRART", "NN", "$.", "ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schaue, wie es mir zur Lust treibt so manches s\u00fc\u00dfe Spiel;", "tokens": ["Schau\u00b7e", ",", "wie", "es", "mir", "zur", "Lust", "treibt", "so", "man\u00b7ches", "s\u00fc\u00b7\u00dfe", "Spiel", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PPER", "APPRART", "NN", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Ist dies nicht mein Freund, der pflegt meiner Mutter Brust zu saugen,", "tokens": ["Ist", "dies", "nicht", "mein", "Freund", ",", "der", "pflegt", "mei\u00b7ner", "Mut\u00b7ter", "Brust", "zu", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ist er nicht mein Salomon, den ich niemals k\u00fc\u00df zu viel.", "tokens": ["Ist", "er", "nicht", "mein", "Sa\u00b7lo\u00b7mon", ",", "den", "ich", "nie\u00b7mals", "k\u00fc\u00df", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Ja er ists, und was ich will, kann ich in dem Kindlein finden,", "tokens": ["Ja", "er", "ists", ",", "und", "was", "ich", "will", ",", "kann", "ich", "in", "dem", "Kin\u00b7dlein", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "$,", "KON", "PWS", "PPER", "VMFIN", "$,", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Kind und Br\u00e4utigam zugleich hei\u00dft und ist er in der That;", "tokens": ["Kind", "und", "Br\u00e4u\u00b7ti\u00b7gam", "zu\u00b7gleich", "hei\u00dft", "und", "ist", "er", "in", "der", "That", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "ADV", "VVFIN", "KON", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+----+-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Denn die zarte Liebe kann auch wohl Kinder ehlich binden,", "tokens": ["Denn", "die", "zar\u00b7te", "Lie\u00b7be", "kann", "auch", "wohl", "Kin\u00b7der", "eh\u00b7lich", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "ADV", "ADV", "NN", "ADJD", "VVINF", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df in Unschuld als sich selbst, eins das andre lieber hat.", "tokens": ["Da\u00df", "in", "Un\u00b7schuld", "als", "sich", "selbst", ",", "eins", "das", "and\u00b7re", "lie\u00b7ber", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KOUS", "PRF", "ADV", "$,", "PIS", "ART", "ADJA", "ADV", "VAFIN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}}}}}