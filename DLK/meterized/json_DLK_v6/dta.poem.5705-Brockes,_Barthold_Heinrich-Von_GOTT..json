{"dta.poem.5705": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er mu\u00df allein die Ehre haben.", "tokens": ["Er", "mu\u00df", "al\u00b7lein", "die", "Eh\u00b7re", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bald ", "tokens": ["So", "bald"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Da\u00df in der Welt so Licht, als Ordnung herrschen sollte,", "tokens": ["Da\u00df", "in", "der", "Welt", "so", "Licht", ",", "als", "Ord\u00b7nung", "herr\u00b7schen", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "NN", "$,", "KOUS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}