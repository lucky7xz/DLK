{"textgrid.poem.66952": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Byzanz", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbzu deinen F\u00fc\u00dfen wir uns weihn", "tokens": ["\u00bb", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "wir", "uns", "weihn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Leib und Seel' aufs Neue\u00ab,", "tokens": ["Mit", "Leib", "und", "Seel'", "aufs", "Neu\u00b7e", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "ADJA", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Griechen nannten's \u03c0\u03c1\u03bf\u03c3\u03ba\u03c5\u03bd\u03b5\u1fd6\u03bd,", "tokens": ["Die", "Grie\u00b7chen", "nann\u00b7ten's", "\u03c0\u03c1\u03bf\u03c3\u03ba\u03c5\u03bd\u03b5\u1fd6\u03bd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Deutschen nennen's \u2013 Treue.", "tokens": ["Die", "Deut\u00b7schen", "nen\u00b7nen's", "\u2013", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}