{"textgrid.poem.51135": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Apostatenmarsch", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bum! Bum! Bim, bam, bum!", "tokens": ["Bum", "!", "Bum", "!", "Bim", ",", "bam", ",", "bum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$,", "VVFIN", "$,", "XY", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Schn\u00fcrt den Sack und kehrt links um!", "tokens": ["Schn\u00fcrt", "den", "Sack", "und", "kehrt", "links", "um", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abgeweidet ist die Matte,", "tokens": ["Ab\u00b7ge\u00b7wei\u00b7det", "ist", "die", "Mat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spute dich, du Wanderratte,", "tokens": ["Spu\u00b7te", "dich", ",", "du", "Wan\u00b7der\u00b7rat\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hungern ist kein Gaudium!", "tokens": ["Hun\u00b7gern", "ist", "kein", "Gau\u00b7di\u00b7um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dreht die Fahne, d\u00e4mpft die Trommel:", "tokens": ["Dreht", "die", "Fah\u00b7ne", ",", "d\u00e4mpft", "die", "Trom\u00b7mel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bum! Bum! Bim, bam, bum!", "tokens": ["Bum", "!", "Bum", "!", "Bim", ",", "bam", ",", "bum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$,", "VVFIN", "$,", "XY", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sind wir nicht ein sch\u00f6ner Zug,", "tokens": ["Sind", "wir", "nicht", "ein", "sch\u00f6\u00b7ner", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Galgenfroher Rabenflug?", "tokens": ["Gal\u00b7gen\u00b7fro\u00b7her", "Ra\u00b7ben\u00b7flug", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter uns die guten Tr\u00f6pfe", "tokens": ["Hin\u00b7ter", "uns", "die", "gu\u00b7ten", "Tr\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehn und brechen sich die K\u00f6pfe", "tokens": ["Stehn", "und", "bre\u00b7chen", "sich", "die", "K\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob dem lustigen Betrug.", "tokens": ["Ob", "dem", "lus\u00b7ti\u00b7gen", "Be\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dreht die Fahne, d\u00e4mpft die Trommel:", "tokens": ["Dreht", "die", "Fah\u00b7ne", ",", "d\u00e4mpft", "die", "Trom\u00b7mel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bum! Bum! Bim, bam, bum!", "tokens": ["Bum", "!", "Bum", "!", "Bim", ",", "bam", ",", "bum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$,", "VVFIN", "$,", "XY", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hohn und schriller Pfeifenklang", "tokens": ["Hohn", "und", "schril\u00b7ler", "Pfei\u00b7fen\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Folgen uns den Weg entlang;", "tokens": ["Fol\u00b7gen", "uns", "den", "Weg", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weiter, weiter in dem Kote!", "tokens": ["Wei\u00b7ter", ",", "wei\u00b7ter", "in", "dem", "Ko\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00dfe s\u00fc\u00dfe Gnadenbrote", "tokens": ["Wei\u00b7\u00dfe", "s\u00fc\u00b7\u00dfe", "Gna\u00b7den\u00b7bro\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lohnen uns den sauren Gang!", "tokens": ["Loh\u00b7nen", "uns", "den", "sau\u00b7ren", "Gang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dreht die Fahne, d\u00e4mpft die Trommel:", "tokens": ["Dreht", "die", "Fah\u00b7ne", ",", "d\u00e4mpft", "die", "Trom\u00b7mel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bum! Bum! Bim, bam, bum!", "tokens": ["Bum", "!", "Bum", "!", "Bim", ",", "bam", ",", "bum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$,", "VVFIN", "$,", "XY", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aus dem Busen rei\u00dft das Herz,", "tokens": ["Aus", "dem", "Bu\u00b7sen", "rei\u00dft", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werft es fluchend hinterw\u00e4rts!", "tokens": ["Werft", "es", "flu\u00b7chend", "hin\u00b7ter\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfaffenk\u00fcch und Keller k\u00fchle,", "tokens": ["Pfaf\u00b7fen\u00b7k\u00fcch", "und", "Kel\u00b7ler", "k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sp\u00fcle weg die Hochgef\u00fchle,", "tokens": ["Sp\u00fc\u00b7le", "weg", "die", "Hoch\u00b7ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ei, es war nur Bubenscherz!", "tokens": ["Ei", ",", "es", "war", "nur", "Bu\u00b7ben\u00b7scherz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dreht die Fahne, d\u00e4mpft die Trommel:", "tokens": ["Dreht", "die", "Fah\u00b7ne", ",", "d\u00e4mpft", "die", "Trom\u00b7mel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bum! Bum! Bim, bam, bum!", "tokens": ["Bum", "!", "Bum", "!", "Bim", ",", "bam", ",", "bum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$,", "VVFIN", "$,", "XY", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nieder mit dem Jungfernkranz!", "tokens": ["Nie\u00b7der", "mit", "dem", "Jung\u00b7fern\u00b7kranz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ausgel\u00f6scht der Ehre Glanz!", "tokens": ["Aus\u00b7ge\u00b7l\u00f6scht", "der", "Eh\u00b7re", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ausgepfiffen jede Wahrheit!", "tokens": ["Aus\u00b7ge\u00b7pfif\u00b7fen", "je\u00b7de", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Angeschw\u00e4rzt der Sonne Klarheit!", "tokens": ["An\u00b7ge\u00b7schw\u00e4rzt", "der", "Son\u00b7ne", "Klar\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Staub mit dem Popanz!", "tokens": ["In", "den", "Staub", "mit", "dem", "Po\u00b7panz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dreht die Fahne, d\u00e4mpft die Trommel:", "tokens": ["Dreht", "die", "Fah\u00b7ne", ",", "d\u00e4mpft", "die", "Trom\u00b7mel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bum! Bum! Bim, bam, bum!", "tokens": ["Bum", "!", "Bum", "!", "Bim", ",", "bam", ",", "bum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$,", "VVFIN", "$,", "XY", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Judas starb den dummsten Tod \u2013", "tokens": ["Ju\u00b7das", "starb", "den", "dumms\u00b7ten", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00e4me dich, Ischariot!", "tokens": ["Sch\u00e4\u00b7me", "dich", ",", "I\u00b7scha\u00b7ri\u00b7ot", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$."], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Magst du zappeln! Unsereiner", "tokens": ["Magst", "du", "zap\u00b7peln", "!", "Un\u00b7ser\u00b7ei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwimmt mit W\u00fcrde stets als reiner", "tokens": ["Schwimmt", "mit", "W\u00fcr\u00b7de", "stets", "als", "rei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ADV", "KOUS", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Goldfisch durch das Blut so rot!", "tokens": ["Gold\u00b7fisch", "durch", "das", "Blut", "so", "rot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dreht die Fahne, d\u00e4mpft die Trommel:", "tokens": ["Dreht", "die", "Fah\u00b7ne", ",", "d\u00e4mpft", "die", "Trom\u00b7mel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bum! Bum! Bim, bam, bum!", "tokens": ["Bum", "!", "Bum", "!", "Bim", ",", "bam", ",", "bum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$,", "VVFIN", "$,", "XY", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}