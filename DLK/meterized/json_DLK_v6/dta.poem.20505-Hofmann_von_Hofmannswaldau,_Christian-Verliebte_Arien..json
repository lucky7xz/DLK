{"dta.poem.20505": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sey tausendmahl/ o sch\u00f6nstes kind/ willkommen/", "tokens": ["Sey", "tau\u00b7send\u00b7mahl", "/", "o", "sch\u00f6ns\u00b7tes", "kind", "/", "will\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "$(", "FM", "ADJA", "NN", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Luci\u2019gen bist dus oder bist dus nicht?", "tokens": ["Lu\u00b7ci'\u00b7gen", "bist", "dus", "o\u00b7der", "bist", "dus", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "KON", "VAFIN", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo hat sich denn mein gl\u00fccke hergenommen/", "tokens": ["Wo", "hat", "sich", "denn", "mein", "gl\u00fc\u00b7cke", "her\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df mir dein licht bey stiller nacht anbricht/", "tokens": ["Da\u00df", "mir", "dein", "licht", "bey", "stil\u00b7ler", "nacht", "an\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erw\u00fcnschtes gl\u00fcck soll ich denn mein vergn\u00fcgen", "tokens": ["Er\u00b7w\u00fcnschtes", "gl\u00fcck", "soll", "ich", "denn", "mein", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "ADV", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Noch heute/ schatz/ in deinen armen liegen?", "tokens": ["Noch", "heu\u00b7te", "/", "schatz", "/", "in", "dei\u00b7nen", "ar\u00b7men", "lie\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ADJD", "$(", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Entweichet nun ihr m\u00fcden liebes-sorgen?", "tokens": ["Ent\u00b7wei\u00b7chet", "nun", "ihr", "m\u00fc\u00b7den", "lie\u00b7bes\u00b7sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr findet doch bey mir mehr keinen platz/", "tokens": ["Ihr", "fin\u00b7det", "doch", "bey", "mir", "mehr", "kei\u00b7nen", "platz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ADV", "PIAT", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Vor sehnt ich mich mit schmertzen nach dem morgen/", "tokens": ["Vor", "sehnt", "ich", "mich", "mit", "schmert\u00b7zen", "nach", "dem", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PRF", "APPR", "VVFIN", "APPR", "ART", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Itzt bringt die nacht mir meinen liebsten schatz/", "tokens": ["Itzt", "bringt", "die", "nacht", "mir", "mei\u00b7nen", "liebs\u00b7ten", "schatz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und l\u00e4st mir zum vergn\u00fcgten liebes-raube", "tokens": ["Und", "l\u00e4st", "mir", "zum", "ver\u00b7gn\u00fcg\u00b7ten", "lie\u00b7bes\u00b7rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein liebstes m\u00e4dgen/ meine turtel-taube.", "tokens": ["Mein", "liebs\u00b7tes", "m\u00e4d\u00b7gen", "/", "mei\u00b7ne", "tur\u00b7tel\u00b7tau\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach s\u00e4ume nicht/ mein kind/ mich zuvergn\u00fcgen/", "tokens": ["Ach", "s\u00e4u\u00b7me", "nicht", "/", "mein", "kind", "/", "mich", "zu\u00b7ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKNEG", "$(", "PPOSAT", "NN", "$(", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Komm her du ausbund aller meiner lust/", "tokens": ["Komm", "her", "du", "aus\u00b7bund", "al\u00b7ler", "mei\u00b7ner", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VVFIN", "PIAT", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mich vergn\u00fcgt in deinen armen liegen/", "tokens": ["La\u00df", "mich", "ver\u00b7gn\u00fcgt", "in", "dei\u00b7nen", "ar\u00b7men", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und g\u00f6nne mir die seiden-weiche brust/", "tokens": ["Und", "g\u00f6n\u00b7ne", "mir", "die", "sei\u00b7den\u00b7wei\u00b7che", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es stehet schon in deiner hand mein leben/", "tokens": ["Es", "ste\u00b7het", "schon", "in", "dei\u00b7ner", "hand", "mein", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was mir und dir vergn\u00fcgte lust kan geben.", "tokens": ["Was", "mir", "und", "dir", "ver\u00b7gn\u00fcg\u00b7te", "lust", "kan", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KON", "PPER", "VVFIN", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun f\u00fchl ich recht das liebende vers\u00fcssen/", "tokens": ["Nun", "f\u00fchl", "ich", "recht", "das", "lie\u00b7ben\u00b7de", "ver\u00b7s\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein geist ist ausser mir/ ich wei\u00df nicht wo/", "tokens": ["Mein", "geist", "ist", "aus\u00b7ser", "mir", "/", "ich", "wei\u00df", "nicht", "wo", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "$(", "PPER", "VVFIN", "PTKNEG", "PWAV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie wird dir doch/ mein kind/ bey meinen k\u00fcssen?", "tokens": ["Wie", "wird", "dir", "doch", "/", "mein", "kind", "/", "bey", "mei\u00b7nen", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohl/ liebstes kind/ es ist mir eben so/", "tokens": ["Wohl", "/", "liebs\u00b7tes", "kind", "/", "es", "ist", "mir", "e\u00b7ben", "so", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJA", "NN", "$(", "PPER", "VAFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sencke schon die matten augen-lieder/", "tokens": ["Ich", "sen\u00b7cke", "schon", "die", "mat\u00b7ten", "au\u00b7gen\u00b7lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vergn\u00fcgt auff deine zarten br\u00fcste nieder.", "tokens": ["Ver\u00b7gn\u00fcgt", "auff", "dei\u00b7ne", "zar\u00b7ten", "br\u00fcs\u00b7te", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wiltu mir nicht/ mein kind/ noch eins verstatten/", "tokens": ["Wil\u00b7tu", "mir", "nicht", "/", "mein", "kind", "/", "noch", "eins", "ver\u00b7stat\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$(", "PPOSAT", "NN", "$(", "ADV", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich dich darff ", "tokens": ["Da\u00df", "ich", "dich", "darff"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wo bleibt die nacht mit ihren dunckeln schatten/", "tokens": ["Wo", "bleibt", "die", "nacht", "mit", "ih\u00b7ren", "dun\u00b7ckeln", "schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun seh\u2019 ich erst da\u00df ich betrogen bin/", "tokens": ["Nun", "seh'", "ich", "erst", "da\u00df", "ich", "be\u00b7tro\u00b7gen", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein blosser traum der hat mich dir entzogen/", "tokens": ["Ein", "blos\u00b7ser", "traum", "der", "hat", "mich", "dir", "ent\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "VAFIN", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mich zugleich vergn\u00fcgt und auch betrogen.", "tokens": ["Und", "mich", "zu\u00b7gleich", "ver\u00b7gn\u00fcgt", "und", "auch", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}