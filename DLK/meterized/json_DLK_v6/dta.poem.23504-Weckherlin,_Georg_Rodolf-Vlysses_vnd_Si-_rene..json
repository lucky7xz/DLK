{"dta.poem.23504": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vlysses vnd Si-  \n rene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Vlysses beth\u00f6r dich nicht/", "tokens": ["O", "Vlys\u00b7ses", "be\u00b7th\u00f6r", "dich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das du ein lob bringest zuwegen;", "tokens": ["Das", "du", "ein", "lob", "brin\u00b7gest", "zu\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dan solches ja nur ein gedicht/", "tokens": ["Dan", "sol\u00b7ches", "ja", "nur", "ein", "ge\u00b7dicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd in andrer leut k\u00f6pf gelegen;", "tokens": ["Vnd", "in", "an\u00b7drer", "leut", "k\u00f6pf", "ge\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auch nur erfunden/ vnsern muht", "tokens": ["Auch", "nur", "er\u00b7fun\u00b7den", "/", "vn\u00b7sern", "muht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vmb frewd vnd friden zu betriegen/", "tokens": ["Vmb", "frewd", "vnd", "fri\u00b7den", "zu", "be\u00b7trie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd f\u00fcr ruh (vnser bestes gut)", "tokens": ["Vnd", "f\u00fcr", "ruh", "(", "vn\u00b7ser", "bes\u00b7tes", "gut", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$(", "PPOSAT", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit m\u00fch vnd streit vns zu vern\u00fcegen.", "tokens": ["Mit", "m\u00fch", "vnd", "streit", "vns", "zu", "ver\u00b7n\u00fce\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}