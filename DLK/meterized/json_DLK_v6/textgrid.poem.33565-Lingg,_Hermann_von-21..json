{"textgrid.poem.33565": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00fctend jagen Sturm und Schlossen", "tokens": ["W\u00fc\u00b7tend", "ja\u00b7gen", "Sturm", "und", "Schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Berge Tannennacht,", "tokens": ["Durch", "der", "Ber\u00b7ge", "Tan\u00b7nen\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fchsam mit den m\u00fcden Rossen", "tokens": ["M\u00fch\u00b7sam", "mit", "den", "m\u00fc\u00b7den", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zieht durchs Tal ein Wagen sacht.", "tokens": ["Zieht", "durchs", "Tal", "ein", "Wa\u00b7gen", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit den Nebelwolken ringend", "tokens": ["Mit", "den", "Ne\u00b7bel\u00b7wol\u00b7ken", "rin\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Taucht der blasse Mond hervor,", "tokens": ["Taucht", "der", "blas\u00b7se", "Mond", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Posthorn, lustig klingend,", "tokens": ["Und", "ein", "Post\u00b7horn", ",", "lus\u00b7tig", "klin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6net aus der Schlucht empor.", "tokens": ["T\u00f6\u00b7net", "aus", "der", "Schlucht", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blase nur die sch\u00f6nsten St\u00fccke!", "tokens": ["Bla\u00b7se", "nur", "die", "sch\u00f6ns\u00b7ten", "St\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgen, guter Postillon,", "tokens": ["Mor\u00b7gen", ",", "gu\u00b7ter", "Pos\u00b7til\u00b7lon", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Hab' ich mit dem Tagesblicke", "tokens": ["Hab'", "ich", "mit", "dem", "Ta\u00b7ges\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Liebsten Briefe schon!", "tokens": ["Mei\u00b7ner", "Liebs\u00b7ten", "Brie\u00b7fe", "schon", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}