{"textgrid.poem.58346": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "384. Bei allem Tun in einem ruhn!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tue dies und das, ich gehe hin und her,", "tokens": ["Ich", "tue", "dies", "und", "das", ",", "ich", "ge\u00b7he", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "KON", "PDS", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Geist bleibt unbewegt in seinem Ruhpunkt stehen,", "tokens": ["Mein", "Geist", "bleibt", "un\u00b7be\u00b7wegt", "in", "sei\u00b7nem", "Ruh\u00b7punkt", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Herren Angesicht im Grunde schauet er", "tokens": ["Des", "Her\u00b7ren", "An\u00b7ge\u00b7sicht", "im", "Grun\u00b7de", "schau\u00b7et", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nimmt sich des nicht an, was drau\u00dfen mag geschehen.", "tokens": ["Und", "nimmt", "sich", "des", "nicht", "an", ",", "was", "drau\u00b7\u00dfen", "mag", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "PTKNEG", "PTKVZ", "$,", "PRELS", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}