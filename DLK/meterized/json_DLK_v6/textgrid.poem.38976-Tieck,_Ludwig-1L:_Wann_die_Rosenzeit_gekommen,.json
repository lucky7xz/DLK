{"textgrid.poem.38976": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann die Rosenzeit gekommen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann die Rosenzeit gekommen,", "tokens": ["Wann", "die", "Ro\u00b7sen\u00b7zeit", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt um sie die warme Luft,", "tokens": ["Spielt", "um", "sie", "die", "war\u00b7me", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihnen ist die Furcht benommen,", "tokens": ["Ih\u00b7nen", "ist", "die", "Furcht", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie ergie\u00dfen s\u00fc\u00dfen Duft.", "tokens": ["Sie", "er\u00b7gie\u00b7\u00dfen", "s\u00fc\u00b7\u00dfen", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Winde buhlen mit den Rosen", "tokens": ["Win\u00b7de", "buh\u00b7len", "mit", "den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Willig bricht die Knospe los,", "tokens": ["Wil\u00b7lig", "bricht", "die", "Knos\u00b7pe", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eilt entgegen s\u00fc\u00dfem Kosen,", "tokens": ["Eilt", "ent\u00b7ge\u00b7gen", "s\u00fc\u00b7\u00dfem", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oeffnet lachend ihren Schoos.", "tokens": ["Oeff\u00b7net", "la\u00b7chend", "ih\u00b7ren", "Schoos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zarte Arme zum umarmen,", "tokens": ["Zar\u00b7te", "Ar\u00b7me", "zum", "um\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lippen f\u00fcr den s\u00fc\u00dfen Ku\u00df,", "tokens": ["Lip\u00b7pen", "f\u00fcr", "den", "s\u00fc\u00b7\u00dfen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Busen daran zu erwarmen,", "tokens": ["Bu\u00b7sen", "da\u00b7ran", "zu", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leib zum herrlichsten Genu\u00df.", "tokens": ["Leib", "zum", "herr\u00b7lichs\u00b7ten", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Rosen, Lilien, sind gestreuet", "tokens": ["Ro\u00b7sen", ",", "Li\u00b7li\u00b7en", ",", "sind", "ge\u00b7streu\u00b7et"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "VAFIN", "VVPP"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Auf den wunders\u00fc\u00dfen Leib,", "tokens": ["Auf", "den", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfen", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Liebe Gunst erfreuet", "tokens": ["Und", "der", "Lie\u00b7be", "Gunst", "er\u00b7freu\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00e4utigam und junges Weib.", "tokens": ["Br\u00e4u\u00b7ti\u00b7gam", "und", "jun\u00b7ges", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}