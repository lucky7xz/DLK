, "wei\u00b7sen", "G\u00fc\u00b7te", "Wun\u00b7der\u00b7wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jhr Zeugnis heist auch allgemein:", "tokens": ["Ihr", "Zeug\u00b7nis", "heist", "auch", "all\u00b7ge\u00b7mein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und wenn wir uns nur selbst ansehen,", "tokens": ["Und", "wenn", "wir", "uns", "nur", "selbst", "an\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem aufmerksamen Blik;", "tokens": ["Mit", "ei\u00b7nem", "auf\u00b7merk\u00b7sa\u00b7men", "Blik", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00df ein jeder eingestehen,", "tokens": ["So", "mu\u00df", "ein", "je\u00b7der", "ein\u00b7ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein K\u00f6rper sey ein Meisterst\u00fck.", "tokens": ["Sein", "K\u00f6r\u00b7per", "sey", "ein", "Meis\u00b7ter\u00b7st\u00fck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein jedes Theil, Glied und Gelenke,", "tokens": ["Ein", "je\u00b7des", "Theil", ",", "Glied", "und", "Ge\u00b7len\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das sagt mir, wenn ich nur bedenke,", "tokens": ["Das", "sagt", "mir", ",", "wenn", "ich", "nur", "be\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie k\u00fcnstlich das, was gros und fein,", "tokens": ["Wie", "k\u00fcnst\u00b7lich", "das", ",", "was", "gros", "und", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PDS", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Erweg ich, wie im Leib, die Seele,", "tokens": ["Er\u00b7weg", "ich", ",", "wie", "im", "Leib", ",", "die", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein denkend geistig Wesen wohnt;", "tokens": ["Ein", "den\u00b7kend", "geis\u00b7tig", "We\u00b7sen", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Betracht ich wie sie in der H\u00f6le", "tokens": ["Be\u00b7tracht", "ich", "wie", "sie", "in", "der", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des K\u00f6rpers, als unsichtbahr thront,", "tokens": ["Des", "K\u00f6r\u00b7pers", ",", "als", "un\u00b7sicht\u00b7bahr", "thront", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Bemerk ich was vor Eigenschaften,", "tokens": ["Be\u00b7merk", "ich", "was", "vor", "Ei\u00b7gen\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In den Verstand und Willen haften;", "tokens": ["In", "den", "Ver\u00b7stand", "und", "Wil\u00b7len", "haf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So mu\u00df ich ", "tokens": ["So", "mu\u00df", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}}, "stanza.13": {"line.1": {"text": "Bedenk ich, wie durch unsre Sinnen,", "tokens": ["Be\u00b7denk", "ich", ",", "wie", "durch", "uns\u00b7re", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Dinge die von aussen sind,", "tokens": ["Die", "Din\u00b7ge", "die", "von", "aus\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wie durch zarte R\u00f6hren rinnen,", "tokens": ["Als", "wie", "durch", "zar\u00b7te", "R\u00f6h\u00b7ren", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie wir sie so bald, geschwind,", "tokens": ["Und", "wie", "wir", "sie", "so", "bald", ",", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "ADV", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn sie derselben Kunstwerk r\u00fchren,", "tokens": ["Wenn", "sie", "der\u00b7sel\u00b7ben", "Kunst\u00b7werk", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihrem Bild im Geiste sp\u00fcren:", "tokens": ["Mit", "ih\u00b7rem", "Bild", "im", "Geis\u00b7te", "sp\u00fc\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So f\u00fchlen wir, bey Lust und Pein:", "tokens": ["So", "f\u00fch\u00b7len", "wir", ",", "bey", "Lust", "und", "Pein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ja! du bist gros in allen Werken,", "tokens": ["Ja", "!", "du", "bist", "gros", "in", "al\u00b7len", "Wer\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die deine weise Hand gemacht,", "tokens": ["Die", "dei\u00b7ne", "wei\u00b7se", "Hand", "ge\u00b7macht", ","], "t