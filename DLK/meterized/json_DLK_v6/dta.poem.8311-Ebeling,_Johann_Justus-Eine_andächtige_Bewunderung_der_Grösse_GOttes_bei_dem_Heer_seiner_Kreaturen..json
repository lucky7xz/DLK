{"dta.poem.8311": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Eine and\u00e4chtige Bewunderung  \n der Gr\u00f6sse GOttes bei dem Heer  \n seiner Kreaturen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich auf Fl\u00fcgeln der Gedanken", "tokens": ["Wenn", "ich", "auf", "Fl\u00fc\u00b7geln", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich schwinge durch die ganze Welt,", "tokens": ["Mich", "schwin\u00b7ge", "durch", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erwege ihre breite Schranken,", "tokens": ["Er\u00b7we\u00b7ge", "ih\u00b7re", "brei\u00b7te", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was dieser Umkreis in sich h\u00e4lt,", "tokens": ["Was", "die\u00b7ser", "Um\u00b7kreis", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Bedenke was vor grosse Heere", "tokens": ["Be\u00b7den\u00b7ke", "was", "vor", "gros\u00b7se", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jm Himmel, L\u00fcften, Erd und Meere", "tokens": ["Jm", "Him\u00b7mel", ",", "L\u00fcf\u00b7ten", ",", "Erd", "und", "Mee\u00b7re"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Mein Geist erstaunt und wird entz\u00fckket,", "tokens": ["Mein", "Geist", "er\u00b7staunt", "und", "wird", "ent\u00b7z\u00fck\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er die Himmels Veste sieht,", "tokens": ["Wenn", "er", "die", "Him\u00b7mels", "Ves\u00b7te", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch ein Ferne-Glas erblikket,", "tokens": ["Und", "durch", "ein", "Fer\u00b7ne\u00b7Glas", "er\u00b7blik\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was in der blauen Tieffe gl\u00fcht,", "tokens": ["Was", "in", "der", "blau\u00b7en", "Tief\u00b7fe", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da schau ich Millionen Sonnen,", "tokens": ["Da", "schau", "ich", "Mil\u00b7lion\u00b7en", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Darum ein Wolken-Flor gesponnen", "tokens": ["Da\u00b7rum", "ein", "Wol\u00b7ken\u00b7Flor", "ge\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und denke bei der Lichter Schein,", "tokens": ["Und", "den\u00b7ke", "bei", "der", "Lich\u00b7ter", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kaum merk ich, da\u00df der Luftkreis schwim-\nmet,", "tokens": ["Kaum", "merk", "ich", ",", "da\u00df", "der", "Luft\u00b7kreis", "schwim", "met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "TRUNC", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dadurch der Sonnen Feuer-Strahl,", "tokens": ["Da\u00b7durch", "der", "Son\u00b7nen", "Feu\u00b7er\u00b7Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Sternen Heer, zur Nachtzeit glimmet;", "tokens": ["Das", "Ster\u00b7nen", "Heer", ",", "zur", "Nacht\u00b7zeit", "glim\u00b7met", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So frag ich dabei allemahl,", "tokens": ["So", "frag", "ich", "da\u00b7bei", "al\u00b7le\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob diese regen Himmels-Lasten", "tokens": ["Ob", "die\u00b7se", "re\u00b7gen", "Him\u00b7mels\u00b7Las\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf starken Pfeilern etwan rasten?", "tokens": ["Auf", "star\u00b7ken", "Pfei\u00b7lern", "et\u00b7wan", "ras\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sch\u00f6pfer tr\u00e4get sie allein,", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "tr\u00e4\u00b7get", "sie", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich denke nach, wie unsre Erde,", "tokens": ["Ich", "den\u00b7ke", "nach", ",", "wie", "uns\u00b7re", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der kleine Ball, den ", "tokens": ["Der", "klei\u00b7ne", "Ball", ",", "den"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nebst allen durch ein w\u00fcrkend Werde,", "tokens": ["Nebst", "al\u00b7len", "durch", "ein", "w\u00fcr\u00b7kend", "Wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "APPR", "ART", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihren Nichts sich dargestellt,", "tokens": ["Aus", "ih\u00b7ren", "Nichts", "sich", "dar\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was vor viele Kreaturen,", "tokens": ["Und", "was", "vor", "vie\u00b7le", "Kre\u00b7a\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf Bergen, Ebnen, Th\u00e4lern, Fluren,", "tokens": ["Auf", "Ber\u00b7gen", ",", "Eb\u00b7nen", ",", "Th\u00e4\u00b7lern", ",", "Flu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es sagt mir beides gros und klein,", "tokens": ["Es", "sagt", "mir", "bei\u00b7des", "gros", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Berge schr\u00f6kliche Ger\u00fcste,", "tokens": ["Der", "Ber\u00b7ge", "schr\u00f6k\u00b7li\u00b7che", "Ge\u00b7r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der schroffen Felsen steile H\u00f6hn,", "tokens": ["Der", "schrof\u00b7fen", "Fel\u00b7sen", "stei\u00b7le", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wie der Erden S\u00e4ugebr\u00fcste,", "tokens": ["Die", "wie", "der", "Er\u00b7den", "S\u00e4u\u00b7ge\u00b7br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Reiche der Natur zu sehn,", "tokens": ["Jm", "Rei\u00b7che", "der", "Na\u00b7tur", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihre majest\u00e4tschen H\u00fcgel", "tokens": ["Und", "ih\u00b7re", "ma\u00b7jes\u00b7t\u00e4t\u00b7schen", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind alle lauter Wunder Spiegel,", "tokens": ["Sind", "al\u00b7le", "lau\u00b7ter", "Wun\u00b7der", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie stimmen darin \u00fcberein:", "tokens": ["Sie", "stim\u00b7men", "da\u00b7rin", "\u00fc\u00b7be\u00b7re\u00b7in", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Will ich die Augen zu den Meeren,", "tokens": ["Will", "ich", "die", "Au\u00b7gen", "zu", "den", "Mee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Woraus die Fl\u00fcsse sich herdrehn,", "tokens": ["Wo\u00b7raus", "die", "Fl\u00fcs\u00b7se", "sich", "her\u00b7drehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In der Betrachtung weiter kehren", "tokens": ["In", "der", "Be\u00b7trach\u00b7tung", "wei\u00b7ter", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So seh ich Wunderfluthen gehn,", "tokens": ["So", "seh", "ich", "Wun\u00b7der\u00b7flut\u00b7hen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die in den Uffern brausend wallen,", "tokens": ["Die", "in", "den", "Uf\u00b7fern", "brau\u00b7send", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und murmeln an der Klippen Stein,", "tokens": ["Und", "mur\u00b7meln", "an", "der", "Klip\u00b7pen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was vor ein ruderndes Gewimmel,", "tokens": ["Was", "vor", "ein", "ru\u00b7dern\u00b7des", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Fischen hegt See, Flus und Teich,", "tokens": ["Von", "Fi\u00b7schen", "hegt", "See", ",", "Flus", "und", "Teich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was vor webende Get\u00fcmmel,", "tokens": ["Und", "was", "vor", "we\u00b7ben\u00b7de", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Von Thieren sind im trocknen Reich", "tokens": ["Von", "Thie\u00b7ren", "sind", "im", "trock\u00b7nen", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Erde, in den tieffen Gr\u00fcnden,", "tokens": ["Der", "Er\u00b7de", ",", "in", "den", "tief\u00b7fen", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Wiesen, Feldern, Wald zu finden!", "tokens": ["In", "Wie\u00b7sen", ",", "Fel\u00b7dern", ",", "Wald", "zu", "fin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie schnattern, singen, br\u00fcllen, schrein,", "tokens": ["Sie", "schnat\u00b7tern", ",", "sin\u00b7gen", ",", "br\u00fcl\u00b7len", ",", "schrein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er ist es der viel tausend Fr\u00fcchte,", "tokens": ["Er", "ist", "es", "der", "viel", "tau\u00b7send", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Reiche der Natur gemacht,", "tokens": ["In", "Rei\u00b7che", "der", "Na\u00b7tur", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und manche niedliche Gerichte,", "tokens": ["Und", "man\u00b7che", "nied\u00b7li\u00b7che", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Vieh und Menschen ausgedacht:", "tokens": ["F\u00fcr", "Vieh", "und", "Men\u00b7schen", "aus\u00b7ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er kr\u00f6nt die Felder mit den Seegen,", "tokens": ["Er", "kr\u00f6nt", "die", "Fel\u00b7der", "mit", "den", "See\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was Wiesen, G\u00e4rten in sich hegen,", "tokens": ["Was", "Wie\u00b7sen", ",", "G\u00e4r\u00b7ten", "in", "sich", "he\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mu\u00df alles blos durch ihm gedein,", "tokens": ["Mu\u00df", "al\u00b7les", "blos", "durch", "ihm", "ge\u00b7dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er schaft zu jeden Jahreszeiten,", "tokens": ["Er", "schaft", "zu", "je\u00b7den", "Jah\u00b7res\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Menschen Kindern alles an,", "tokens": ["Den", "Men\u00b7schen", "Kin\u00b7dern", "al\u00b7les", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was sie mit frohen Liebligkeiten", "tokens": ["Was", "sie", "mit", "fro\u00b7hen", "Lieb\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erg\u00f6tzen und erquikken kan:", "tokens": ["Er\u00b7g\u00f6t\u00b7zen", "und", "er\u00b7quik\u00b7ken", "kan", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er l\u00e4\u00dft der G\u00fcte Wunderquellen,", "tokens": ["Er", "l\u00e4\u00dft", "der", "G\u00fc\u00b7te", "Wun\u00b7der\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jm gr\u00f6\u00dften Ueberflusse schwellen,", "tokens": ["Jm", "gr\u00f6\u00df\u00b7ten", "Ue\u00b7ber\u00b7flus\u00b7se", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Menschen Herze zu erfreun,", "tokens": ["Der", "Men\u