{"textgrid.poem.64865": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbghasel! was magst du wohl, so fragt' ich lange, sein?", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbghasel! was magst du wohl, so fragt' ich lange, sein?", "tokens": ["\u00bb", "gha\u00b7sel", "!", "was", "magst", "du", "wohl", ",", "so", "fragt'", "ich", "lan\u00b7ge", ",", "sein", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$.", "PWS", "VMFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll ich, dein Wesen zu ergr\u00fcnden, bange sein?\u00ab \u2013", "tokens": ["Soll", "ich", ",", "dein", "We\u00b7sen", "zu", "er\u00b7gr\u00fcn\u00b7den", ",", "ban\u00b7ge", "sein", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "ADJD", "VAINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So fragend hab ich dich erkannt und du kannst nun", "tokens": ["So", "fra\u00b7gend", "hab", "ich", "dich", "er\u00b7kannt", "und", "du", "kannst", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "PRF", "VVPP", "KON", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein dunkles R\u00e4thsel mehr dem Dichterdrange sein.", "tokens": ["Kein", "dunk\u00b7les", "R\u00e4th\u00b7sel", "mehr", "dem", "Dich\u00b7ter\u00b7dran\u00b7ge", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df nunmehr: du sollst, aus tiefster Brust gehaucht,", "tokens": ["Ich", "wei\u00df", "nun\u00b7mehr", ":", "du", "sollst", ",", "aus", "tiefs\u00b7ter", "Brust", "ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VMFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein frischer West um gl\u00fch'nde Liebeswange sein,", "tokens": ["Ein", "fri\u00b7scher", "West", "um", "gl\u00fch'n\u00b7de", "Lie\u00b7bes\u00b7wan\u00b7ge", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du sollst dem sch\u00f6nen Freund, zu dessen Preis du t\u00f6nst,", "tokens": ["Du", "sollst", "dem", "sch\u00f6\u00b7nen", "Freund", ",", "zu", "des\u00b7sen", "Preis", "du", "t\u00f6nst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein f\u00fcrstlich reicher Schmuck, womit er prange, sein.", "tokens": ["Ein", "f\u00fcrst\u00b7lich", "rei\u00b7cher", "Schmuck", ",", "wo\u00b7mit", "er", "pran\u00b7ge", ",", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sollst seines edlen Schatzes k\u00f6stlichster Demant,", "tokens": ["Sollst", "sei\u00b7nes", "ed\u00b7len", "Schat\u00b7zes", "k\u00f6st\u00b7lichs\u00b7ter", "De\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Und 0seines k\u00f6niglichen Purpurs Spange sein,", "tokens": ["Und", "0sei\u00b7nes", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Pur\u00b7purs", "Span\u00b7ge", "sein", ","], "token_info": ["word", "number_compound", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ein unzerrei\u00dfbar Netz, aus laut'rem Gold gef\u00fcgt,", "tokens": ["Ein", "un\u00b7zer\u00b7rei\u00df\u00b7bar", "Netz", ",", "aus", "laut'\u00b7rem", "Gold", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In das sich Reiz und Huld f\u00fcr stets verfange, sein!", "tokens": ["In", "das", "sich", "Reiz", "und", "Huld", "f\u00fcr", "stets", "ver\u00b7fan\u00b7ge", ",", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NN", "KON", "NN", "APPR", "ADV", "VVFIN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du sollst, Ghasel! mit deinem s\u00fc\u00dfen Echospiel", "tokens": ["Du", "sollst", ",", "Gha\u00b7sel", "!", "mit", "dei\u00b7nem", "s\u00fc\u00b7\u00dfen", "E\u00b7cho\u00b7spiel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "NN", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein Wiederhall vom ew'gen Liebesklange sein!", "tokens": ["Ein", "Wie\u00b7der\u00b7hall", "vom", "ew'\u00b7gen", "Lie\u00b7bes\u00b7klan\u00b7ge", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}