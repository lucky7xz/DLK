{"textgrid.poem.42411": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wie die Jahre lasten auf den Toten!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie die Jahre lasten auf den Toten!", "tokens": ["O", "wie", "die", "Jah\u00b7re", "las\u00b7ten", "auf", "den", "To\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ungeheure Grabesdunkelheit", "tokens": ["Die", "un\u00b7ge\u00b7heu\u00b7re", "Gra\u00b7bes\u00b7dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um ihre H\u00e4upter tausendfach verdichtet,", "tokens": ["Um", "ih\u00b7re", "H\u00e4up\u00b7ter", "tau\u00b7send\u00b7fach", "ver\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Erde \u00fcber ihnen aufgeschichtet", "tokens": ["Die", "Er\u00b7de", "\u00fc\u00b7ber", "ih\u00b7nen", "auf\u00b7ge\u00b7schich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und alles Erz und Stein darauf errichtet,", "tokens": ["Und", "al\u00b7les", "Erz", "und", "Stein", "da\u00b7rauf", "er\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wiegt nicht so schwer auf ihnen wie die Zeit.", "tokens": ["Wiegt", "nicht", "so", "schwer", "auf", "ih\u00b7nen", "wie", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "APPR", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zuerst vom Reich des Lebens abgeschnitten,", "tokens": ["Zu\u00b7erst", "vom", "Reich", "des", "Le\u00b7bens", "ab\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind sie in unsern Kreis noch eingebannt.", "tokens": ["Sind", "sie", "in", "un\u00b7sern", "Kreis", "noch", "ein\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schweben schattenhaft vor unsern Sinnen", "tokens": ["Sie", "schwe\u00b7ben", "schat\u00b7ten\u00b7haft", "vor", "un\u00b7sern", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und haben teil an all unserm Beginnen", "tokens": ["Und", "ha\u00b7ben", "teil", "an", "all", "un\u00b7serm", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und halten uns bei Tag und Nacht tief innen", "tokens": ["Und", "hal\u00b7ten", "uns", "bei", "Tag", "und", "Nacht", "tief", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ADJD", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit ungebrochner Wesenskraft umspannt.", "tokens": ["Mit", "un\u00b7ge\u00b7broch\u00b7ner", "We\u00b7sens\u00b7kraft", "um\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Allm\u00e4hlich aber bricht in sich zusammen", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "a\u00b7ber", "bricht", "in", "sich", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das lebensgleiche Trugbild ihrer Macht.", "tokens": ["Das", "le\u00b7bens\u00b7glei\u00b7che", "Trug\u00b7bild", "ih\u00b7rer", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf ihren Gr\u00e4bern welken und verbleichen", "tokens": ["Auf", "ih\u00b7ren", "Gr\u00e4\u00b7bern", "wel\u00b7ken", "und", "ver\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebesgaben und die Namenszeichen,", "tokens": ["Die", "Lie\u00b7bes\u00b7ga\u00b7ben", "und", "die", "Na\u00b7mens\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und aus dem Sinn der Lebenden entweichen", "tokens": ["Und", "aus", "dem", "Sinn", "der", "Le\u00b7ben\u00b7den", "ent\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Schatten immer tiefer in die Nacht.", "tokens": ["Die", "Schat\u00b7ten", "im\u00b7mer", "tie\u00b7fer", "in", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Weitab verflattern fr\u00fchere Geschicke,", "tokens": ["Weit\u00b7ab", "ver\u00b7flat\u00b7tern", "fr\u00fc\u00b7he\u00b7re", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erkennbar kaum dem Blick und abgeschw\u00e4cht:", "tokens": ["Er\u00b7kenn\u00b7bar", "kaum", "dem", "Blick", "und", "ab\u00b7ge\u00b7schw\u00e4cht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Vater, Mutter nennst du so die Namen,", "tokens": ["Von", "Va\u00b7ter", ",", "Mut\u00b7ter", "nennst", "du", "so", "die", "Na\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie du von Ahnen sprichst, die vor dir kamen,", "tokens": ["Wie", "du", "von", "Ah\u00b7nen", "sprichst", ",", "die", "vor", "dir", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sp\u00e4ter Enkelspross aus deinem Samen", "tokens": ["Und", "sp\u00e4\u00b7ter", "En\u00b7kel\u00b7spross", "aus", "dei\u00b7nem", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist dir ein fremdgeartetes Geschlecht.", "tokens": ["Ist", "dir", "ein", "fremd\u00b7ge\u00b7ar\u00b7te\u00b7tes", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Die Nachgebornen eilen von den St\u00e4tten,", "tokens": ["Die", "Nach\u00b7ge\u00b7bor\u00b7nen", "ei\u00b7len", "von", "den", "St\u00e4t\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich versammelt ihrer V\u00e4ter Staub.", "tokens": ["Wo", "sich", "ver\u00b7sam\u00b7melt", "ih\u00b7rer", "V\u00e4\u00b7ter", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie ziehen ihres Wegs dahin und ahnen", "tokens": ["Sie", "zie\u00b7hen", "ih\u00b7res", "Wegs", "da\u00b7hin", "und", "ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PAV", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht, was sie treibt die selbstgew\u00e4hlten Bahnen", "tokens": ["Nicht", ",", "was", "sie", "treibt", "die", "selbst\u00b7ge\u00b7w\u00e4hl\u00b7ten", "Bah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sind in ihrem Innersten den Manen", "tokens": ["Und", "sind", "in", "ih\u00b7rem", "In\u00b7ners\u00b7ten", "den", "Ma\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Urv\u00e4terlichen Blutes kalt und taub.", "tokens": ["Ur\u00b7v\u00e4\u00b7ter\u00b7li\u00b7chen", "Blu\u00b7tes", "kalt", "und", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O wie die Jahre lasten auf den Toten!", "tokens": ["O", "wie", "die", "Jah\u00b7re", "las\u00b7ten", "auf", "den", "To\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ungeheure Grabesdunkelheit", "tokens": ["Die", "un\u00b7ge\u00b7heu\u00b7re", "Gra\u00b7bes\u00b7dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um ihre H\u00e4upter tausendfach verdichtet,", "tokens": ["Um", "ih\u00b7re", "H\u00e4up\u00b7ter", "tau\u00b7send\u00b7fach", "ver\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Erde \u00fcber ihnen aufgeschichtet", "tokens": ["Die", "Er\u00b7de", "\u00fc\u00b7ber", "ih\u00b7nen", "auf\u00b7ge\u00b7schich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und alles Erz und Stein darauf errichtet,", "tokens": ["Und", "al\u00b7les", "Erz", "und", "Stein", "da\u00b7rauf", "er\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wiegt nicht so schwer auf ihnen wie die Zeit.", "tokens": ["Wiegt", "nicht", "so", "schwer", "auf", "ih\u00b7nen", "wie", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "APPR", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}