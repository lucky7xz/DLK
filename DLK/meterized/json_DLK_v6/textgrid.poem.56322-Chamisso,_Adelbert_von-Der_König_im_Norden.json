{"textgrid.poem.56322": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00f6nig im Norden", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein K\u00f6nig im Norden", "tokens": ["Es", "war", "ein", "K\u00f6\u00b7nig", "im", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar stolz, gewaltig und reich;", "tokens": ["Gar", "stolz", ",", "ge\u00b7wal\u00b7tig", "und", "reich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihm gleich ist keiner geworden,", "tokens": ["Ihm", "gleich", "ist", "kei\u00b7ner", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PIS", "VAPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und nie wird einer ihm gleich.", "tokens": ["Und", "nie", "wird", "ei\u00b7ner", "ihm", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und als es galt zu sterben,", "tokens": ["Und", "als", "es", "galt", "zu", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er sa\u00df am \u00f6den Meer,", "tokens": ["Er", "sa\u00df", "am", "\u00f6\u00b7den", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schlichen herbei seine Erben,", "tokens": ["Es", "schli\u00b7chen", "her\u00b7bei", "sei\u00b7ne", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wolf, die Eule, der B\u00e4r.", "tokens": ["Der", "Wolf", ",", "die", "Eu\u00b7le", ",", "der", "B\u00e4r", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Da sprach er zum zottigen B\u00e4ren:", "tokens": ["Da", "sprach", "er", "zum", "zot\u00b7ti\u00b7gen", "B\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "\u00bbdir la\u00df ich Forst und Wald;", "tokens": ["\u00bb", "dir", "la\u00df", "ich", "Forst", "und", "Wald", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Jagdherr wird dich st\u00f6ren", "tokens": ["Kein", "Jagd\u00b7herr", "wird", "dich", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im luftigen Aufenthalt.\u00ab", "tokens": ["Im", "luf\u00b7ti\u00b7gen", "Auf\u00b7ent\u00b7halt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und weiter sprach er zur Eule:", "tokens": ["Und", "wei\u00b7ter", "sprach", "er", "zur", "Eu\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbich lasse sonder Zahl", "tokens": ["\u00bb", "ich", "las\u00b7se", "son\u00b7der", "Zahl"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir Burgen und St\u00e4dte, verteile", "tokens": ["Dir", "Bur\u00b7gen", "und", "St\u00e4d\u00b7te", ",", "ver\u00b7tei\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "NN", "KON", "NN", "$,", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie deinen T\u00f6chtern zumal.\u00ab", "tokens": ["Sie", "dei\u00b7nen", "T\u00f6ch\u00b7tern", "zu\u00b7mal", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Und sprach zum Wolfe desgleichen:", "tokens": ["Und", "sprach", "zum", "Wol\u00b7fe", "des\u00b7glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbdir la\u00df ich ein stilles Feld,", "tokens": ["\u00bb", "dir", "la\u00df", "ich", "ein", "stil\u00b7les", "Feld", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit Leichen und aber Leichen,", "tokens": ["Mit", "Lei\u00b7chen", "und", "a\u00b7ber", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So weit ich geherrscht, bestellt.\u00ab", "tokens": ["So", "weit", "ich", "ge\u00b7herrscht", ",", "be\u00b7stellt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$,", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und wie er solches gesprochen,", "tokens": ["Und", "wie", "er", "sol\u00b7ches", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PIAT", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So streckt' er sich aus zur Ruh, \u2013", "tokens": ["So", "streckt'", "er", "sich", "aus", "zur", "Ruh", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "APPRART", "NN", "$,", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Sturm ist angebrochen,", "tokens": ["Ein", "Sturm", "ist", "an\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der deckte mit Schlo\u00dfen ihn zu.", "tokens": ["Der", "deck\u00b7te", "mit", "Schlo\u00b7\u00dfen", "ihn", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}