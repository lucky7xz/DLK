{"textgrid.poem.44212": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[crispin und Choerilus, die Latt und Wurm beschwert]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Crispin und Choerilus, die Latt und Wurm beschwert,", "tokens": ["Cris\u00b7pin", "und", "Choe\u00b7ri\u00b7lus", ",", "die", "Latt", "und", "Wurm", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verleumden mich an allen Orten", "tokens": ["Ver\u00b7leum\u00b7den", "mich", "an", "al\u00b7len", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit L\u00fcgen, Spott und groben Worten.", "tokens": ["Mit", "L\u00fc\u00b7gen", ",", "Spott", "und", "gro\u00b7ben", "Wor\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erz\u00fcrnt ich mich nun erst, so w\u00e4r ich alles werth.", "tokens": ["Er\u00b7z\u00fcrnt", "ich", "mich", "nun", "erst", ",", "so", "w\u00e4r", "ich", "al\u00b7les", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "$,", "ADV", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}