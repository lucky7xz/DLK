{"textgrid.poem.35169": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Dank", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fiel ein Thau wohl \u00fcber Nacht", "tokens": ["Es", "fiel", "ein", "Thau", "wohl", "\u00fc\u00b7ber", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rings auf die durstig matten Auen,", "tokens": ["Rings", "auf", "die", "durs\u00b7tig", "mat\u00b7ten", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und fr\u00fch war in der Sonne Pracht", "tokens": ["Und", "fr\u00fch", "war", "in", "der", "Son\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Sch\u00f6pfers Lob und Preis zu schauen.", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Lob", "und", "Preis", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein diamantnes Leuchten spr\u00fchte", "tokens": ["Ein", "di\u00b7a\u00b7mant\u00b7nes", "Leuch\u00b7ten", "spr\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Strauch zu Strauch, von Halm zu Halm,", "tokens": ["Von", "Strauch", "zu", "Strauch", ",", "von", "Halm", "zu", "Halm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und von Milliarden Perlen gl\u00fchte", "tokens": ["Und", "von", "Mil\u00b7li\u00b7ar\u00b7den", "Per\u00b7len", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zu ihm empor ein Dankespsalm.", "tokens": ["Zu", "ihm", "em\u00b7por", "ein", "Dan\u00b7ke\u00b7spsalm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun aber sendet Tag und Nacht", "tokens": ["Nun", "a\u00b7ber", "sen\u00b7det", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vater seinen Segen nieder,", "tokens": ["Der", "Va\u00b7ter", "sei\u00b7nen", "Se\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hat der Segen Gl\u00fcck gebracht,", "tokens": ["Und", "hat", "der", "Se\u00b7gen", "Gl\u00fcck", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo bleiben dann die Dankeslieder?", "tokens": ["Wo", "blei\u00b7ben", "dann", "die", "Dan\u00b7kes\u00b7lie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es hat der Mensch so viel zu sagen,", "tokens": ["Es", "hat", "der", "Mensch", "so", "viel", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch Dank an Gott, den sagt er nicht.", "tokens": ["Doch", "Dank", "an", "Gott", ",", "den", "sagt", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "ART", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O, m\u00f6chte er den Thau doch fragen,", "tokens": ["O", ",", "m\u00f6ch\u00b7te", "er", "den", "Thau", "doch", "fra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der lehrte ihm die Dankespflicht!", "tokens": ["Der", "lehr\u00b7te", "ihm", "die", "Dan\u00b7kes\u00b7pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}