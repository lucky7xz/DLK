{"textgrid.poem.49795": {"metadata": {"author": {"name": "Abschatz, Hans A\u00dfmann von", "birth": "N.A.", "death": "N.A."}, "title": "[k\u00f6nte sich ein krancker Mutt]", "genre": "verse", "period": "N.A.", "pub_year": 1672, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nte sich ein krancker Mutt", "tokens": ["K\u00f6n\u00b7te", "sich", "ein", "kran\u00b7cker", "Mutt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Bande machen lo\u00df/", "tokens": ["Sei\u00b7ner", "Ban\u00b7de", "ma\u00b7chen", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn das Hertz zu wehe thut/", "tokens": ["Wenn", "das", "Hertz", "zu", "we\u00b7he", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKA", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So s\u00e4\u00df in des Gl\u00fcckes Scho\u00df", "tokens": ["So", "s\u00e4\u00df", "in", "des", "Gl\u00fc\u00b7ckes", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wer empfindt der Liebe Glutt.", "tokens": ["Wer", "emp\u00b7findt", "der", "Lie\u00b7be", "Glutt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber weil der Sternen Schlu\u00df", "tokens": ["A\u00b7ber", "weil", "der", "Ster\u00b7nen", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Selten wieder machet frey", "tokens": ["Sel\u00b7ten", "wie\u00b7der", "ma\u00b7chet", "frey"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den mit Lieb' umstrickten Fu\u00df/", "tokens": ["Den", "mit", "Lieb'", "um\u00b7strick\u00b7ten", "Fu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebt in harter Sclaverey", "tokens": ["Lebt", "in", "har\u00b7ter", "Scla\u00b7ve\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer der Liebe dienen mu\u00df.", "tokens": ["Wer", "der", "Lie\u00b7be", "die\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}