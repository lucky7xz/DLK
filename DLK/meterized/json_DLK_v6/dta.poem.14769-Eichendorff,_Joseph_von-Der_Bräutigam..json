{"dta.poem.14769": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Br\u00e4utigam.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin? Im Mondenschimmer", "tokens": ["Wo\u00b7hin", "?", "Im", "Mon\u00b7den\u00b7schim\u00b7mer"], "token_info": ["word", "punct", "word", "word"], "pos": ["PWAV", "$.", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bleich der Wald schon steht. \u2014", "tokens": ["So", "bleich", "der", "Wald", "schon", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leis rauscht die Nacht \u2014 frag' nimmer,", "tokens": ["Leis", "rauscht", "die", "Nacht", "frag'", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$(", "VVIMP", "ADV", "$,"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wo Lieb' zu Ende geht!", "tokens": ["Wo", "Lieb'", "zu", "En\u00b7de", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}