{"textgrid.poem.32893": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich war bey Chloen ganz allein,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war bey Chloen ganz allein,", "tokens": ["Ich", "war", "bey", "Chloen", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ADV", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und k\u00fcssen wollt ich sie:", "tokens": ["Und", "k\u00fcs\u00b7sen", "wollt", "ich", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jedoch sie sprach: sie w\u00fcrde schreyn,", "tokens": ["Je\u00b7doch", "sie", "sprach", ":", "sie", "w\u00fcr\u00b7de", "schreyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es sey vergebne M\u00fch!", "tokens": ["Es", "sey", "ver\u00b7geb\u00b7ne", "M\u00fch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch wagt ich es, und k\u00fc\u00dfte sie,", "tokens": ["Doch", "wagt", "ich", "es", ",", "und", "k\u00fc\u00df\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie oft? f\u00e4llt mir nicht ein!", "tokens": ["Wie", "oft", "?", "f\u00e4llt", "mir", "nicht", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und schrie sie nicht? Ja wohl, sie schrie \u2013 \u2013", "tokens": ["Und", "schrie", "sie", "nicht", "?", "Ja", "wohl", ",", "sie", "schrie", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$.", "PTKANT", "ADV", "$,", "PPER", "VVFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch lange hinter drein.", "tokens": ["Doch", "lan\u00b7ge", "hin\u00b7ter", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}