{"textgrid.poem.35676": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "39. Der Rebenspro\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fruchtschwer an Lesbos sonnigen H\u00f6hn erwuchs", "tokens": ["Frucht\u00b7schwer", "an", "Les\u00b7bos", "son\u00b7ni\u00b7gen", "H\u00f6hn", "er\u00b7wuchs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "ADJA", "NN", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein hehrer Weinstock, welcher Ambrosia,", "tokens": ["Ein", "heh\u00b7rer", "Wein\u00b7stock", ",", "wel\u00b7cher", "A\u00b7mbro\u00b7sia", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Voll Hochgef\u00fchls und Hochgesanges,", "tokens": ["Voll", "Hoch\u00b7ge\u00b7f\u00fchls", "und", "Hoch\u00b7ge\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zeitigte, durch Dionysos Obhut,", "tokens": ["Zei\u00b7tig\u00b7te", ",", "durch", "Di\u00b7o\u00b7ny\u00b7sos", "Ob\u00b7hut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NE", "NE", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der rohen Tiersinn z\u00e4hmte zu Menschlichkeit.", "tokens": ["Der", "ro\u00b7hen", "Tier\u00b7sinn", "z\u00e4hm\u00b7te", "zu", "Menschlich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Anstaunensw\u00fcrdig, mitten im Tempelhain,", "tokens": ["An\u00b7stau\u00b7nens\u00b7w\u00fcr\u00b7dig", ",", "mit\u00b7ten", "im", "Tem\u00b7pel\u00b7hain", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dichtlaubig, schwer von reifem Purpur,", "tokens": ["Dicht\u00b7lau\u00b7big", ",", "schwer", "von", "rei\u00b7fem", "Pur\u00b7pur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stand der ambrosische Lebensweinbaum.", "tokens": ["Stand", "der", "am\u00b7bro\u00b7si\u00b7sche", "Le\u00b7bens\u00b7wein\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier trank Arion schmelzenden Zauberhall,", "tokens": ["Hier", "trank", "A\u00b7rion", "schmel\u00b7zen\u00b7den", "Zau\u00b7ber\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit Nymph' und Satyr schw\u00e4rmend im Hain; es trank", "tokens": ["Mit", "Nym\u00b7ph'", "und", "Sa\u00b7tyr", "schw\u00e4r\u00b7mend", "im", "Hain", ";", "es", "trank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "APPRART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sturmlauten Freiheitsschwung Alk\u00e4os,", "tokens": ["Sturm\u00b7lau\u00b7ten", "Frei\u00b7heits\u00b7schwung", "Al\u00b7k\u00e4\u00b7os", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Brautmelodieen die entz\u00fcckte Sappho.", "tokens": ["Braut\u00b7me\u00b7lo\u00b7di\u00b7e\u00b7en", "die", "ent\u00b7z\u00fcck\u00b7te", "Sap\u00b7pho", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Zwar ach! verhallt sind ihr Ges\u00e4ng' in Nacht:", "tokens": ["Zwar", "ach", "!", "ver\u00b7hallt", "sind", "ihr", "Ge\u00b7s\u00e4ng'", "in", "Nacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "VVPP", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch weht' in Flaccus lebende Harmonie", "tokens": ["Doch", "weht'", "in", "Flac\u00b7cus", "le\u00b7ben\u00b7de", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nachhall; und sanft um tote Rollen", "tokens": ["Nach\u00b7hall", ";", "und", "sanft", "um", "to\u00b7te", "Rol\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "T\u00f6nt in den Schlacken Vesuvs ihr Lispel.", "tokens": ["T\u00f6nt", "in", "den", "Schla\u00b7cken", "Ve\u00b7suvs", "ihr", "Lis\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Mir trug Ly\u00e4os, mir der begeisternden", "tokens": ["Mir", "trug", "Ly\u00b7\u00e4\u00b7os", ",", "mir", "der", "be\u00b7geis\u00b7tern\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$,", "PPER", "ART", "ADJA"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weinrebe Spr\u00f6\u00dfling; als, dem Verst\u00fcrmten gleich", "tokens": ["Wein\u00b7re\u00b7be", "Spr\u00f6\u00df\u00b7ling", ";", "als", ",", "dem", "Ver\u00b7st\u00fcrm\u00b7ten", "gleich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KOUS", "$,", "ART", "NN", "ADV"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Auf \u00f6dem Eiland, ich mit Sehnsucht", "tokens": ["Auf", "\u00f6\u00b7dem", "Ei\u00b7land", ",", "ich", "mit", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wandte den Blick zur Hellenenheimat.", "tokens": ["Wand\u00b7te", "den", "Blick", "zur", "Hel\u00b7le\u00b7nen\u00b7hei\u00b7mat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Schamhaft ergl\u00fchend, nahm ich den heiligen", "tokens": ["Scham\u00b7haft", "er\u00b7gl\u00fc\u00b7hend", ",", "nahm", "ich", "den", "hei\u00b7li\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Rebscho\u00df, und hegt' ihn, nahe dem Nordgestirn,", "tokens": ["Reb\u00b7scho\u00df", ",", "und", "hegt'", "ihn", ",", "na\u00b7he", "dem", "Nord\u00b7ges\u00b7tirn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Abwehrend Luft und Ungeschlachtheit,", "tokens": ["Ab\u00b7weh\u00b7rend", "Luft", "und", "Un\u00b7ge\u00b7schlacht\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter dem Glas in erkargter Sonne.", "tokens": ["Un\u00b7ter", "dem", "Glas", "in", "er\u00b7karg\u00b7ter", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Vom Trieb der Gottheit, siehe, beschleuniget,", "tokens": ["Vom", "Trieb", "der", "Got\u00b7theit", ",", "sie\u00b7he", ",", "be\u00b7schleu\u00b7ni\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVIMP", "$,", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stieg Rankenwaldung \u00fcbergew\u00f6lbt, mich bald", "tokens": ["Stieg", "Ran\u00b7ken\u00b7wal\u00b7dung", "\u00fc\u00b7ber\u00b7ge\u00b7w\u00f6lbt", ",", "mich", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "VVPP", "$,", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Bl\u00fcte, bald mit gr\u00fcnem Herling,", "tokens": ["Mit", "Bl\u00fc\u00b7te", ",", "bald", "mit", "gr\u00fc\u00b7nem", "Her\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald mit ger\u00f6teter Traub' umschwebend.", "tokens": ["Bald", "mit", "ge\u00b7r\u00f6\u00b7te\u00b7ter", "Traub'", "um\u00b7schwe\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Im s\u00fc\u00dfen Anhauch tr\u00e4um' ich, der Zeit entflohn,", "tokens": ["Im", "s\u00fc\u00b7\u00dfen", "An\u00b7hauch", "tr\u00e4um'", "ich", ",", "der", "Zeit", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wettkampf mit altert\u00fcmlichem Hochgesang.", "tokens": ["Wett\u00b7kampf", "mit", "al\u00b7ter\u00b7t\u00fcm\u00b7li\u00b7chem", "Hoch\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wer lauter ist, der koste freundlich,", "tokens": ["Wer", "lau\u00b7ter", "ist", ",", "der", "kos\u00b7te", "freund\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob die Ambrosiafrucht gereift sei.", "tokens": ["Ob", "die", "A\u00b7mbro\u00b7si\u00b7a\u00b7frucht", "ge\u00b7reift", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}