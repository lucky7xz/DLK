{"textgrid.poem.66327": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Froschkritik", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sang in 'nem Busch 'ne Nachtigall;", "tokens": ["Sang", "in", "'nem", "Busch", "'ne", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wunderlieblich war ihr Schall", "tokens": ["So", "wun\u00b7der\u00b7lieb\u00b7lich", "war", "ihr", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wie der 'rausgezogne Ton", "tokens": ["Als", "wie", "der", "'r\u00b7aus\u00b7ge\u00b7zo\u00b7gne", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus Meister ", "tokens": ["Aus", "Meis\u00b7ter"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Es war 'n Sumpf nicht weit davon,", "tokens": ["Es", "war", "'n", "Sumpf", "nicht", "weit", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drin lag 'ne ganze Legion", "tokens": ["Drin", "lag", "'ne", "gan\u00b7ze", "Le\u00b7gi\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Fr\u00f6schen; und die h\u00f6rten all'", "tokens": ["Von", "Fr\u00f6\u00b7schen", ";", "und", "die", "h\u00f6r\u00b7ten", "all'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "KON", "ART", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Wundersang der Nachtigall.", "tokens": ["Den", "Wun\u00b7der\u00b7sang", "der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da war ein hochstudirter Frosch,", "tokens": ["Da", "war", "ein", "hoch\u00b7stu\u00b7dir\u00b7ter", "Frosch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit runzlicher Stirn und breiter Gosch,", "tokens": ["Mit", "runz\u00b7li\u00b7cher", "Stirn", "und", "brei\u00b7ter", "Gosch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Hatte die edle Musikam,", "tokens": ["Hat\u00b7te", "die", "ed\u00b7le", "Mu\u00b7si\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.12": {"text": "Den Kontrapunkt, die Algebram", "tokens": ["Den", "Kon\u00b7tra\u00b7punkt", ",", "die", "Al\u00b7ge\u00b7bram"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "In manchem Sumpf und Weiher studirt,", "tokens": ["In", "man\u00b7chem", "Sumpf", "und", "Wei\u00b7her", "stu\u00b7dirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Und orgelte, wie sich's geb\u00fchrt.", "tokens": ["Und", "or\u00b7gel\u00b7te", ",", "wie", "sich's", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Doch weil er war gar kalter Natur,", "tokens": ["Doch", "weil", "er", "war", "gar", "kal\u00b7ter", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Der h\u00f6rte auch die Nachtigall", "tokens": ["Der", "h\u00f6r\u00b7te", "auch", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und sprach: \u00bbIhr Br\u00fcder, h\u00f6rt einmal,", "tokens": ["Und", "sprach", ":", "\u00bb", "Ihr", "Br\u00fc\u00b7der", ",", "h\u00f6rt", "ein\u00b7mal", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wie singt das Thier so abgeschmackt,", "tokens": ["Wie", "singt", "das", "Thier", "so", "ab\u00b7ge\u00b7schmackt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Macht falsche Quinten, h\u00e4lt keinen Takt,", "tokens": ["Macht", "fal\u00b7sche", "Quin\u00b7ten", ",", "h\u00e4lt", "kei\u00b7nen", "Takt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Weicht nicht in k\u00fcnstlicher Modulation", "tokens": ["Weicht", "nicht", "in", "k\u00fcnst\u00b7li\u00b7cher", "Mo\u00b7du\u00b7la\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Aus einem Ton in den andern Ton.", "tokens": ["Aus", "ei\u00b7nem", "Ton", "in", "den", "an\u00b7dern", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "In ihrem ekeln di \u2013 di \u2013 di", "tokens": ["In", "ih\u00b7rem", "e\u00b7keln", "di", "\u2013", "di", "\u2013", "di"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$(", "NE", "$(", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und duk, duk, duk \u2013 steckt ihre ganze Melodie.", "tokens": ["Und", "duk", ",", "duk", ",", "duk", "\u2013", "steckt", "ih\u00b7re", "gan\u00b7ze", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "NE", "$,", "NE", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Magister Frosch lacht drob so laut,", "tokens": ["Ma\u00b7gis\u00b7ter", "Frosch", "lacht", "drob", "so", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Da\u00df ihm beinah' zerplatzt die Haut,", "tokens": ["Da\u00df", "ihm", "bei\u00b7nah'", "zer\u00b7platzt", "die", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und sprach: Kameraden, wi\u00dft ihr was?", "tokens": ["Und", "sprach", ":", "Ka\u00b7me\u00b7ra\u00b7den", ",", "wi\u00dft", "ihr", "was", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$,", "VVFIN", "PPER", "PIS", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.27": {"text": "Eine Fuge klingt doch ba\u00df;", "tokens": ["Ei\u00b7ne", "Fu\u00b7ge", "klingt", "doch", "ba\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Wollen's singen im Sopran, Alt und Tenor,", "tokens": ["Wol\u00b7len's", "sin\u00b7gen", "im", "Sop\u00b7ran", ",", "Alt", "und", "Te\u00b7nor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,", "ADJD", "KON", "NE", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Ich orgle euch das Thema vor.\u00ab", "tokens": ["Ich", "org\u00b7le", "euch", "das", "The\u00b7ma", "vor", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Nun ging's an ein scheu\u00dflich Gequack", "tokens": ["Nun", "ging's", "an", "ein", "scheu\u00df\u00b7lich", "Ge\u00b7quack"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJD", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.31": {"text": "Im wahren antiken Geschmack", "tokens": ["Im", "wah\u00b7ren", "an\u00b7ti\u00b7ken", "Ge\u00b7schmack"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Mit Bund und Motu contrario;", "tokens": ["Mit", "Bund", "und", "Mo\u00b7tu", "cont\u00b7ra\u00b7rio", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der Frosch hielt Tasto solo;", "tokens": ["Der", "Frosch", "hielt", "Tas\u00b7to", "so\u00b7lo", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.34": {"text": "Unaufgel\u00f6st in der Fuge ganz", "tokens": ["Un\u00b7auf\u00b7ge\u00b7l\u00f6st", "in", "der", "Fu\u00b7ge", "ganz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Folgt Dissonanz auf Dissonanz.", "tokens": ["Folgt", "Dis\u00b7so\u00b7nanz", "auf", "Dis\u00b7so\u00b7nanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Nach mancher halsbrechenden Modulation", "tokens": ["Nach", "man\u00b7cher", "hals\u00b7bre\u00b7chen\u00b7den", "Mo\u00b7du\u00b7la\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.37": {"text": "Kam endlich doch der letzte Ton.", "tokens": ["Kam", "end\u00b7lich", "doch", "der", "letz\u00b7te", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Die Fledermaus und der Uhu", "tokens": ["Die", "Fle\u00b7der\u00b7maus", "und", "der", "U\u00b7hu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "H\u00f6rten dem Froschconcerte zu;", "tokens": ["H\u00f6r\u00b7ten", "dem", "Fro\u00b7schcon\u00b7cer\u00b7te", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.40": {"text": "Waren drob gar lustig und froh,", "tokens": ["Wa\u00b7ren", "drob", "gar", "lus\u00b7tig", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.41": {"text": "Und schrieen laut: Bravissimo!", "tokens": ["Und", "schri\u00b7een", "laut", ":", "Bra\u00b7vis\u00b7si\u00b7mo", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ein J\u00fcngling voll Empfindsamkeit,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "voll", "Emp\u00b7find\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Gelockt von sanfter Abendzeit,", "tokens": ["Ge\u00b7lockt", "von", "sanf\u00b7ter", "A\u00b7bend\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Kam aus dem nahen Rosenthal,", "tokens": ["Kam", "aus", "dem", "na\u00b7hen", "Ro\u00b7sen\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "H\u00f6rte das Lied der Nachtigall,", "tokens": ["H\u00f6r\u00b7te", "das", "Lied", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Und weint' und sah zum Himmel auf;", "tokens": ["Und", "weint'", "und", "sah", "zum", "Him\u00b7mel", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und als die Fr\u00f6sche fugirten drauf,", "tokens": ["Und", "als", "die", "Fr\u00f6\u00b7sche", "fu\u00b7gir\u00b7ten", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.48": {"text": "Da warf er Steine in den Teich", "tokens": ["Da", "warf", "er", "Stei\u00b7ne", "in", "den", "Teich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Und schrie: Der Henker hole euch!", "tokens": ["Und", "schrie", ":", "Der", "Hen\u00b7ker", "ho\u00b7le", "euch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "\u00bbhum! sprach der Kritikus unter'm Gew\u00e4sser,", "tokens": ["\u00bb", "hum", "!", "sprach", "der", "Kri\u00b7ti\u00b7kus", "un\u00b7ter'm", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.51": {"text": "Der Kerl versteht's nicht besser!", "tokens": ["Der", "Kerl", "ver\u00b7steht's", "nicht", "bes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}