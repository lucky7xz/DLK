{"textgrid.poem.53341": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[ihr Schatten in der stillen Nacht]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Schatten in der stillen Nacht,", "tokens": ["Ihr", "Schat\u00b7ten", "in", "der", "stil\u00b7len", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die jhr vieleicht an wilder Schlacht", "tokens": ["Die", "jhr", "vie\u00b7leicht", "an", "wil\u00b7der", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch, wie im Leben, tragt Gefallen", "tokens": ["Noch", ",", "wie", "im", "Le\u00b7ben", ",", "tragt", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd gern seht, wenn das Todten-Meer", "tokens": ["Vnd", "gern", "seht", ",", "wenn", "das", "Tod\u00b7ten\u00b7Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vermehrt durch Helden ewer Heer", "tokens": ["Ver\u00b7mehrt", "durch", "Hel\u00b7den", "e\u00b7wer", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die hie ber\u00fchmt gewest f\u00fcr allen:", "tokens": ["Die", "hie", "be\u00b7r\u00fchmt", "ge\u00b7west", "f\u00fcr", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt schicken wir euch durch den Tod", "tokens": ["Jetzt", "schi\u00b7cken", "wir", "euch", "durch", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den hochverdienten Wallenrodt,", "tokens": ["Den", "hoch\u00b7ver\u00b7dien\u00b7ten", "Wal\u00b7len\u00b7rodt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den edlen Mann zwar durch die Ahnen", "tokens": ["Den", "ed\u00b7len", "Mann", "zwar", "durch", "die", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Krieges-Thaten aber mehr,", "tokens": ["Durch", "Krie\u00b7ge\u00b7sTha\u00b7ten", "a\u00b7ber", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der einig suchte Ruhm vnd Ehr", "tokens": ["Der", "ei\u00b7nig", "such\u00b7te", "Ruhm", "vnd", "Ehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Streit der Blutig-rohten Fahnen.", "tokens": ["Im", "Streit", "der", "Blu\u00b7tig\u00b7roh\u00b7ten", "Fah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Seyd froh vnd nehmt Ihn zu euch ein,", "tokens": ["Seyd", "froh", "vnd", "nehmt", "Ihn", "zu", "euch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lasst jhn euch hoch willkommen seyn.", "tokens": ["Lasst", "jhn", "euch", "hoch", "will\u00b7kom\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht da\u00df jhm werde Platz gegeben", "tokens": ["Geht", "da\u00df", "jhm", "wer\u00b7de", "Platz", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PPER", "VAFIN", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Hercules vnd Theseus sind,", "tokens": ["Wo", "Her\u00b7cu\u00b7les", "vnd", "The\u00b7seus", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NE", "VAFIN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Vnd wo Philippus grosses Kind,", "tokens": ["Vnd", "wo", "Phil\u00b7ip\u00b7pus", "gros\u00b7ses", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Pericles vnd Pompejus schweben.", "tokens": ["Pe\u00b7ri\u00b7cles", "vnd", "Pom\u00b7pe\u00b7jus", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Was? gebt jhm eine Stell' allhie", "tokens": ["Was", "?", "gebt", "jhm", "ei\u00b7ne", "Stell'", "all\u00b7hie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "$.", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sich zum Samen Isai,", "tokens": ["Wo", "sich", "zum", "Sa\u00b7men", "I\u00b7sai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPRART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Schild vnd Scepter der Ebreer,", "tokens": ["Dem", "Schild", "vnd", "Scep\u00b7ter", "der", "E\u00b7breer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schar der Gottes-Helden thut,", "tokens": ["Die", "Schar", "der", "Got\u00b7tes\u00b7Hel\u00b7den", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Voraus die jhr gabt ewer Blut", "tokens": ["Vo\u00b7raus", "die", "jhr", "gabt", "e\u00b7wer", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr das Gesetz, jhr Asmoneer.", "tokens": ["F\u00fcr", "das", "Ge\u00b7setz", ",", "jhr", "As\u00b7mo\u00b7neer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er wird in einer langen Reih'", "tokens": ["Er", "wird", "in", "ei\u00b7ner", "lan\u00b7gen", "Reih'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch seiner strengen Waffen Trew", "tokens": ["Euch", "sei\u00b7ner", "stren\u00b7gen", "Waf\u00b7fen", "Trew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdienst vnd hohen Flei\u00df erzehlen,", "tokens": ["Ver\u00b7dienst", "vnd", "ho\u00b7hen", "Flei\u00df", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd welches zu beschreiben dir,", "tokens": ["Vnd", "wel\u00b7ches", "zu", "be\u00b7schrei\u00b7ben", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "PTKZU", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Maro, es an Kunst vnd Zier", "tokens": ["O", "Ma\u00b7ro", ",", "es", "an", "Kunst", "vnd", "Zier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Vnd sch\u00f6nen Worten m\u00f6chte fehlen.", "tokens": ["Vnd", "sch\u00f6\u00b7nen", "Wor\u00b7ten", "m\u00f6ch\u00b7te", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wird sprechen: stracks von Jugend auff", "tokens": ["Wird", "spre\u00b7chen", ":", "stracks", "von", "Ju\u00b7gend", "auff"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "VVINF", "$.", "ADV", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Enthielte mich der Tugend Lauff,", "tokens": ["Ent\u00b7hiel\u00b7te", "mich", "der", "Tu\u00b7gend", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich war noch kaum von sechszehn Jahren,", "tokens": ["Ich", "war", "noch", "kaum", "von", "sechs\u00b7zehn", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als mich die Krieges-Lust schon fieng,", "tokens": ["Als", "mich", "die", "Krie\u00b7ge\u00b7sLust", "schon", "fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Vnd, wo noch spielte, stund vnd gieng,", "tokens": ["Vnd", ",", "wo", "noch", "spiel\u00b7te", ",", "stund", "vnd", "gieng", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "VVFIN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Lantz vnd Helm mein Leben waren.", "tokens": ["Nur", "Lantz", "vnd", "Helm", "mein", "Le\u00b7ben", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Trummel-Schlag, das Feld-Geschrey", "tokens": ["Der", "Trum\u00b7mel\u00b7Schlag", ",", "das", "Feld\u00b7Ge\u00b7schrey"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War meine liebste Melodey.", "tokens": ["War", "mei\u00b7ne", "liebs\u00b7te", "Me\u00b7lo\u00b7dey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Hertz im Leibe pflag zu springen", "tokens": ["Mein", "Hertz", "im", "Lei\u00b7be", "pflag", "zu", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sah ich ein Ritterliches Pferd,", "tokens": ["Sah", "ich", "ein", "Rit\u00b7ter\u00b7li\u00b7ches", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Leit-Gestirn ein blanckes Schwerd,", "tokens": ["Mein", "Leit\u00b7Ge\u00b7stirn", "ein", "blan\u00b7ckes", "Schwerd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd h\u00f6rt ich wo die Kugeln singen.", "tokens": ["Vnd", "h\u00f6rt", "ich", "wo", "die", "Ku\u00b7geln", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mein Preussen vnd mein Vater-Gut", "tokens": ["Mein", "Preus\u00b7sen", "vnd", "mein", "Va\u00b7ter\u00b7Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War ein Gef\u00e4ngni\u00df meinem Muth,", "tokens": ["War", "ein", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "mei\u00b7nem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sagte: gute Nacht, o Pregel!", "tokens": ["Ich", "sag\u00b7te", ":", "gu\u00b7te", "Nacht", ",", "o", "Pre\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADJA", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du leschest doch nicht meinen Brand,", "tokens": ["Du", "le\u00b7schest", "doch", "nicht", "mei\u00b7nen", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nimm du mich auff, o Niederland,", "tokens": ["Nimm", "du", "mich", "auff", ",", "o", "Nie\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "PTKVZ", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir eil ich zu mit vollem Segel.", "tokens": ["Dir", "eil", "ich", "zu", "mit", "vol\u00b7lem", "Se\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So kam ich in der Waffen Feld.", "tokens": ["So", "kam", "ich", "in", "der", "Waf\u00b7fen", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sah' in ordnung die Gezelt'", "tokens": ["Ich", "sah'", "in", "ord\u00b7nung", "die", "Ge\u00b7zelt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd den beschlossnen Feind von fernen,", "tokens": ["Vnd", "den", "be\u00b7schloss\u00b7nen", "Feind", "von", "fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jedes Heer verschantzte sich,", "tokens": ["Ein", "je\u00b7des", "Heer", "ver\u00b7schantz\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die\u00df, sprach ich: ist ein Thun vor dich,", "tokens": ["Die\u00df", ",", "sprach", "ich", ":", "ist", "ein", "Thun", "vor", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "$.", "VAFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hie kan dein Flei\u00df was guttes lernen.", "tokens": ["Hie", "kan", "dein", "Flei\u00df", "was", "gut\u00b7tes", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PWS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Graff Wilhelm der Nassawen Liecht", "tokens": ["Graff", "Wil\u00b7helm", "der", "Nas\u00b7sa\u00b7wen", "Liecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Verwarff auch meine Jugend nicht,", "tokens": ["Ver