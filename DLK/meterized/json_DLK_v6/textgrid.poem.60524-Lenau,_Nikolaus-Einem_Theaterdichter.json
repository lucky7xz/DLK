{"textgrid.poem.60524": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Einem Theaterdichter", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Niedrung schmilzt der Schnee,", "tokens": ["In", "der", "Nie\u00b7drung", "schmilzt", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Gebirge schneit es;", "tokens": ["Im", "Ge\u00b7bir\u00b7ge", "schneit", "es", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ob der Schwarm in Tr\u00e4nen steh", "tokens": ["Ob", "der", "Schwarm", "in", "Tr\u00e4\u00b7nen", "steh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber all dein Breites,", "tokens": ["\u00dc\u00b7ber", "all", "dein", "Brei\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Uns wird k\u00fchl, langweilig weh,", "tokens": ["Uns", "wird", "k\u00fchl", ",", "lang\u00b7wei\u00b7lig", "weh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringst du nichts Gescheites.", "tokens": ["Bringst", "du", "nichts", "Ge\u00b7schei\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}