{"dta.poem.10903": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sah man, von bejahrten St\u00e4mmen, und von Bl\u00e4ttern, die\nFigur", "tokens": ["Sah", "man", ",", "von", "be\u00b7jahr\u00b7ten", "St\u00e4m\u00b7men", ",", "und", "von", "Bl\u00e4t\u00b7tern", ",", "die", "Fi\u00b7gur"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "APPR", "ADJA", "NN", "$,", "KON", "APPR", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Je nat\u00fcrlicher gebildet? eine thierische Natur,", "tokens": ["Je", "na\u00b7t\u00fcr\u00b7li\u00b7cher", "ge\u00b7bil\u00b7det", "?", "ei\u00b7ne", "thie\u00b7ri\u00b7sche", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Gleichsam lebend, blo\u00df in Strichen? Nein, vom Thier - und", "tokens": ["Gleich\u00b7sam", "le\u00b7bend", ",", "blo\u00df", "in", "Stri\u00b7chen", "?", "Nein", ",", "vom", "Thier", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "ADV", "APPR", "NN", "$.", "PTKANT", "$,", "APPRART", "NN", "$(", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Sah man nimmer die Copie, dem Original so gleich.", "tokens": ["Sah", "man", "nim\u00b7mer", "die", "Co\u00b7pie", ",", "dem", "O\u00b7rig\u00b7i\u00b7nal", "so", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "In dem Aug, in allen Sehnen, ja in Muskeln, durch die", "tokens": ["In", "dem", "Aug", ",", "in", "al\u00b7len", "Seh\u00b7nen", ",", "ja", "in", "Mus\u00b7keln", ",", "durch", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PIAT", "NN", "$,", "ADV", "APPR", "NN", "$,", "APPR", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Wird an diesem Hirsch, wie sanft ihm das Streifen thut, ge-", "tokens": ["Wird", "an", "die\u00b7sem", "Hirsch", ",", "wie", "sanft", "ihm", "das", "Strei\u00b7fen", "thut", ",", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "$,", "PWAV", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,", "TRUNC"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ja, es zeiget \u00fcberall die so k\u00fcnstliche Copie,", "tokens": ["Ja", ",", "es", "zei\u00b7get", "\u00fc\u00b7be\u00b7rall", "die", "so", "k\u00fcnst\u00b7li\u00b7che", "Co\u00b7pie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "In nur schwarzen Linien, Farben, Haltung, Harmonie.", "tokens": ["In", "nur", "schwar\u00b7zen", "Li\u00b7ni\u00b7en", ",", "Far\u00b7ben", ",", "Hal\u00b7tung", ",", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Wie das Urbild so vortrefflich, wie die Welt so wundersch\u00f6n,", "tokens": ["Wie", "das", "Ur\u00b7bild", "so", "vor\u00b7treff\u00b7lich", ",", "wie", "die", "Welt", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Davon kann man hier, im Abdruck, einen sch\u00f6nen Schatten", "tokens": ["Da\u00b7von", "kann", "man", "hier", ",", "im", "Ab\u00b7druck", ",", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PIS", "ADV", "$,", "APPRART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Ja noch mehr, in wahren W\u00e4ldern, wie es mir zum \u00f6ftern", "tokens": ["Ja", "noch", "mehr", ",", "in", "wah\u00b7ren", "W\u00e4l\u00b7dern", ",", "wie", "es", "mir", "zum", "\u00f6f\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "PWAV", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Scheint der gr\u00fcne Schatten schwarz, hier der schwarze Schat-", "tokens": ["Scheint", "der", "gr\u00fc\u00b7ne", "Schat\u00b7ten", "schwarz", ",", "hier", "der", "schwar\u00b7ze", "Schat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "$,", "ADV", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Ja es scheint die\u00df edle Thier, nicht zu stehn, nein, sich zu regen,", "tokens": ["Ja", "es", "scheint", "die\u00df", "ed\u00b7le", "Thier", ",", "nicht", "zu", "stehn", ",", "nein", ",", "sich", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PDS", "ADJA", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$,", "PTKANT", "$,", "PRF", "APPR", "ADJA", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Und die Bl\u00e4tter an den Zweigen, durchs Geh\u00f6rn, sich zu be-", "tokens": ["Und", "die", "Bl\u00e4t\u00b7ter", "an", "den", "Zwei\u00b7gen", ",", "durchs", "Ge\u00b7h\u00f6rn", ",", "sich", "zu", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$,", "PRF", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}