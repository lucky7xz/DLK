{"dta.poem.15748": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An alter Poesie verbl\u00fchten Blumenbeeten", "tokens": ["An", "al\u00b7ter", "Poe\u00b7sie", "ver\u00b7bl\u00fch\u00b7ten", "Blu\u00b7men\u00b7bee\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die silbenstechenden Ausleger der Poeten", "tokens": ["Die", "sil\u00b7ben\u00b7ste\u00b7chen\u00b7den", "Aus\u00b7le\u00b7ger", "der", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-+----", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Erwecken mir halb k\u00fchl im Busen und halb schw\u00fcl", "tokens": ["Er\u00b7we\u00b7cken", "mir", "halb", "k\u00fchl", "im", "Bu\u00b7sen", "und", "halb", "schw\u00fcl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "ADJD", "APPRART", "NN", "KON", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Stolz und Trauer ein gemischtes Mitgef\u00fchl:", "tokens": ["Aus", "Stolz", "und", "Trau\u00b7er", "ein", "ge\u00b7mischtes", "Mit\u00b7ge\u00b7f\u00fchl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Stolz, da\u00df ein leichtes Wort zu solchen Ehren kam,", "tokens": ["Stolz", ",", "da\u00df", "ein", "leich\u00b7tes", "Wort", "zu", "sol\u00b7chen", "Eh\u00b7ren", "kam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Trauer, da\u00df die Lust der Welt solch Ende nahm;", "tokens": ["Und", "Trau\u00b7er", ",", "da\u00df", "die", "Lust", "der", "Welt", "solch", "En\u00b7de", "nahm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df diese Blumen, die mit Duft und Glanze neu", "tokens": ["Da\u00df", "die\u00b7se", "Blu\u00b7men", ",", "die", "mit", "Duft", "und", "Glan\u00b7ze", "neu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einst Herzen labten, nun sind solcher Ochsen Heu.", "tokens": ["Einst", "Her\u00b7zen", "lab\u00b7ten", ",", "nun", "sind", "sol\u00b7cher", "Och\u00b7sen", "Heu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Auf, Lieder, la\u00dft uns frisch der frischen Welt gefallen,", "tokens": ["Auf", ",", "Lie\u00b7der", ",", "la\u00dft", "uns", "frisch", "der", "fri\u00b7schen", "Welt", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "VVIMP", "PPER", "ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh wir verdorrt zum Raub dem d\u00fcrren Vieh heimfallen.", "tokens": ["Eh", "wir", "ver\u00b7dorrt", "zum", "Raub", "dem", "d\u00fcr\u00b7ren", "Vieh", "heim\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}