{"textgrid.poem.63713": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Rosens\u00fcnden", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese flatterhaften Rosen,", "tokens": ["Die\u00b7se", "flat\u00b7ter\u00b7haf\u00b7ten", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir tags Laurella gab,", "tokens": ["Die", "mir", "tags", "Lau\u00b7rel\u00b7la", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fliegen, die gewissenlosen,", "tokens": ["Flie\u00b7gen", ",", "die", "ge\u00b7wis\u00b7sen\u00b7lo\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachts zu Grazia hinab.", "tokens": ["Nachts", "zu", "Gra\u00b7zia", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Steckt euch denn, ihr B\u00f6sewichter,", "tokens": ["Steckt", "euch", "denn", ",", "ihr", "B\u00f6\u00b7se\u00b7wich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So die Kuppelei im Blut,", "tokens": ["So", "die", "Kup\u00b7pe\u00b7lei", "im", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Da\u00df ihr doppelt eurem Dichter", "tokens": ["Da\u00df", "ihr", "dop\u00b7pelt", "eu\u00b7rem", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure Liebesdienste tut?", "tokens": ["Eu\u00b7re", "Lie\u00b7bes\u00b7diens\u00b7te", "tut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder lie\u00df sich billig finden", "tokens": ["O\u00b7der", "lie\u00df", "sich", "bil\u00b7lig", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euer Beichtiger, der Lenz?", "tokens": ["Eu\u00b7er", "Beich\u00b7ti\u00b7ger", ",", "der", "Lenz", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gab er euch f\u00fcr Rosens\u00fcnden", "tokens": ["Gab", "er", "euch", "f\u00fcr", "Ro\u00b7sen\u00b7s\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im voraus die Indulgenz?", "tokens": ["Im", "vo\u00b7raus", "die", "In\u00b7dul\u00b7genz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}