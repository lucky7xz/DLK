{"textgrid.poem.26517": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wein' dich aus an meiner Brust,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wein' dich aus an meiner Brust,", "tokens": ["O", "wein'", "dich", "aus", "an", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df in dein Herz mich seh'n;", "tokens": ["La\u00df", "in", "dein", "Herz", "mich", "seh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4rst du noch so schuldbewu\u00dft:", "tokens": ["Und", "w\u00e4rst", "du", "noch", "so", "schuld\u00b7be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kann dich ganz versteh'n.", "tokens": ["Ich", "kann", "dich", "ganz", "ver\u00b7steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn nennen kannst du mir kein Leid,", "tokens": ["Denn", "nen\u00b7nen", "kannst", "du", "mir", "kein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das nicht schon traf auch mich;", "tokens": ["Das", "nicht", "schon", "traf", "auch", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und schuldig war auch ich.", "tokens": ["Und", "schul\u00b7dig", "war", "auch", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "In der Besch\u00e4mung Roth \u2013", "tokens": ["In", "der", "Be\u00b7sch\u00e4\u00b7mung", "Roth", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verloren hab' ich mich genannt", "tokens": ["Ver\u00b7lo\u00b7ren", "hab'", "ich", "mich", "ge\u00b7nannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir erhofft den Tod.", "tokens": ["Und", "mir", "er\u00b7hofft", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "D'rum wein' dich aus an meiner Brust,", "tokens": ["D'\u00b7rum", "wein'", "dich", "aus", "an", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich kann dich ganz versteh'n,", "tokens": ["Ich", "kann", "dich", "ganz", "ver\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4rst du noch so schuldbewu\u00dft:", "tokens": ["Und", "w\u00e4rst", "du", "noch", "so", "schuld\u00b7be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Getr\u00f6stet wirst du geh'n!", "tokens": ["Ge\u00b7tr\u00f6s\u00b7tet", "wirst", "du", "geh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}