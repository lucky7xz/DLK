{"textgrid.poem.36483": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Adler flog zur Sonne, pr\u00e4chtig hell,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Adler flog zur Sonne, pr\u00e4chtig hell,", "tokens": ["Ein", "Ad\u00b7ler", "flog", "zur", "Son\u00b7ne", ",", "pr\u00e4ch\u00b7tig", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Rabe sah ihn fliegen.", "tokens": ["Ein", "Ra\u00b7be", "sah", "ihn", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ei! sprach der Rabe da, den denk' ich einzukriegen!", "tokens": ["Ei", "!", "sprach", "der", "Ra\u00b7be", "da", ",", "den", "denk'", "ich", "ein\u00b7zu\u00b7krie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der fliegt doch eben nicht so schnell!", "tokens": ["Der", "fliegt", "doch", "e\u00b7ben", "nicht", "so", "schnell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Rabe flog; ein Trieb nach Ehre, tief empfunden,", "tokens": ["Der", "Ra\u00b7be", "flog", ";", "ein", "Trieb", "nach", "Eh\u00b7re", ",", "tief", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War Schuld, da\u00df er so k\u00fchn den Wettflug unternahm!", "tokens": ["War", "Schuld", ",", "da\u00df", "er", "so", "k\u00fchn", "den", "Wett\u00b7flug", "un\u00b7ter\u00b7nahm", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein der Adler war schon seinem Blick entschwunden,", "tokens": ["Al\u00b7lein", "der", "Ad\u00b7ler", "war", "schon", "sei\u00b7nem", "Blick", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als er mit seinem Flug bis an die Wolken kam.", "tokens": ["Als", "er", "mit", "sei\u00b7nem", "Flug", "bis", "an", "die", "Wol\u00b7ken", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar durft er, k\u00fchn den Weg des Adlers nachzugehn,", "tokens": ["Zwar", "durft", "er", ",", "k\u00fchn", "den", "Weg", "des", "Ad\u00b7lers", "nach\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur sch\u00e4rfer in die Sonne sehn;", "tokens": ["Nur", "sch\u00e4r\u00b7fer", "in", "die", "Son\u00b7ne", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein, er fand f\u00fcr gut, bei Zeiten umzukehren.", "tokens": ["Al\u00b7lein", ",", "er", "fand", "f\u00fcr", "gut", ",", "bei", "Zei\u00b7ten", "um\u00b7zu\u00b7keh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "APPR", "ADJD", "$,", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn unsre Flieger doch klug, wie der Rabe, w\u00e4ren!", "tokens": ["Wenn", "uns\u00b7re", "Flie\u00b7ger", "doch", "klug", ",", "wie", "der", "Ra\u00b7be", ",", "w\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}