{"textgrid.poem.35109": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckkehr zum Glauben", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gr\u00fc\u00dfe dich. Du warst als heller Stern", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", ".", "Du", "warst", "als", "hel\u00b7ler", "Stern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An meinem Himmel leuchtend aufgegangen.", "tokens": ["An", "mei\u00b7nem", "Him\u00b7mel", "leuch\u00b7tend", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Licht, es zeigte mir den Weg zum Herrn,", "tokens": ["Dein", "Licht", ",", "es", "zeig\u00b7te", "mir", "den", "Weg", "zum", "Herrn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An dessen Thron der Engel Ch\u00f6re klangen.", "tokens": ["An", "des\u00b7sen", "Thron", "der", "En\u00b7gel", "Ch\u00f6\u00b7re", "klan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich folgte dir gehorsam, hoch begl\u00fcckt", "tokens": ["Ich", "folg\u00b7te", "dir", "ge\u00b7hor\u00b7sam", ",", "hoch", "be\u00b7gl\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und lie\u00df mir als dein dankbar staunend Eigen,", "tokens": ["Und", "lie\u00df", "mir", "als", "dein", "dank\u00b7bar", "stau\u00b7nend", "Ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "PPOSAT", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dem Kreis der Erde mehr und mehr entr\u00fcckt,", "tokens": ["Dem", "Kreis", "der", "Er\u00b7de", "mehr", "und", "mehr", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Allmacht und der Liebe Wunder zeigen.", "tokens": ["Der", "All\u00b7macht", "und", "der", "Lie\u00b7be", "Wun\u00b7der", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich gr\u00fc\u00dfe dich, wie ich dich einst gegr\u00fc\u00dft", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "wie", "ich", "dich", "einst", "ge\u00b7gr\u00fc\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An jedem Tag, den mir der Herr gegeben.", "tokens": ["An", "je\u00b7dem", "Tag", ",", "den", "mir", "der", "Herr", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vielleicht, vielleicht hab ich genug geb\u00fc\u00dft", "tokens": ["Viel\u00b7leicht", ",", "viel\u00b7leicht", "hab", "ich", "ge\u00b7nug", "ge\u00b7b\u00fc\u00dft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und darf nun wieder f\u00fcr und durch dich leben.", "tokens": ["Und", "darf", "nun", "wie\u00b7der", "f\u00fcr", "und", "durch", "dich", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "APPR", "KON", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich konnte, was mich leise dir entzog,", "tokens": ["Ich", "konn\u00b7te", ",", "was", "mich", "lei\u00b7se", "dir", "ent\u00b7zog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit meinem schwachen Auge nicht erkennen;", "tokens": ["Mit", "mei\u00b7nem", "schwa\u00b7chen", "Au\u00b7ge", "nicht", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nun aber wei\u00df ich, da\u00df es mich betrog,", "tokens": ["Nun", "a\u00b7ber", "wei\u00df", "ich", ",", "da\u00df", "es", "mich", "be\u00b7trog", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und lasse mich nicht wieder von dir trennen.", "tokens": ["Und", "las\u00b7se", "mich", "nicht", "wie\u00b7der", "von", "dir", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich gr\u00fc\u00dfe dich. Du bringst die Klarheit mir,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", ".", "Du", "bringst", "die", "Klar\u00b7heit", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nun darf die Genesung ich erwarten.", "tokens": ["Und", "nun", "darf", "die", "Ge\u00b7ne\u00b7sung", "ich", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Es schl\u00e4gt mein Puls von Neuem auf zu dir,", "tokens": ["Es", "schl\u00e4gt", "mein", "Puls", "von", "Neu\u00b7em", "auf", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ich, ich lach der Zweifel, die mich narrten.", "tokens": ["Und", "ich", ",", "ich", "lach", "der", "Zwei\u00b7fel", ",", "die", "mich", "narr\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich gr\u00fc\u00dfe dich so froh, so dankerf\u00fcllt;", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", "so", "froh", ",", "so", "dan\u00b7ker\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich konnte irren, doch du mu\u00dftest siegen,", "tokens": ["Ich", "konn\u00b7te", "ir\u00b7ren", ",", "doch", "du", "mu\u00df\u00b7test", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und ob die Brandung hinter mir noch br\u00fcllt,", "tokens": ["Und", "ob", "die", "Bran\u00b7dung", "hin\u00b7ter", "mir", "noch", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du warst der Anker \u2013 \u2013 ich bin ihr entstiegen.", "tokens": ["Du", "warst", "der", "An\u00b7ker", "\u2013", "\u2013", "ich", "bin", "ihr", "ent\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "$(", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}