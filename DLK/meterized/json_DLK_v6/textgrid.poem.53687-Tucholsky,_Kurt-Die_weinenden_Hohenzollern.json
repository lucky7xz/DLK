{"textgrid.poem.53687": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die weinenden Hohenzollern", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sitzen in den Niederlanden", "tokens": ["Sie", "sit\u00b7zen", "in", "den", "Nie\u00b7der\u00b7lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und gucken in die blaue Luft.", "tokens": ["und", "gu\u00b7cken", "in", "die", "blau\u00b7e", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Alte mit den hohen Granden,", "tokens": ["Der", "Al\u00b7te", "mit", "den", "ho\u00b7hen", "Gran\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der Junge in der Tenniskluft.", "tokens": ["der", "Jun\u00b7ge", "in", "der", "Ten\u00b7nis\u00b7kluft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer fuhr denn \u2013 t\u00f6ff-t\u00f6ff-t\u00f6ff \u2013 nach Holland,", "tokens": ["Wer", "fuhr", "denn", "\u2013", "t\u00f6ff\u00b7t\u00f6ff\u00b7t\u00f6ff", "\u2013", "nach", "Hol\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$(", "NE", "$(", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "woraus man heut sich traurig sehnt?", "tokens": ["wo\u00b7raus", "man", "heut", "sich", "trau\u00b7rig", "sehnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie klagen, ihre Welt sei Moll-Land . . .", "tokens": ["Sie", "kla\u00b7gen", ",", "ih\u00b7re", "Welt", "sei", "Moll\u00b7Land", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "NE", "$.", "$.", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Vater hat jeweent, Willy hat jeweent \u2013", "tokens": ["Va\u00b7ter", "hat", "je\u00b7weent", ",", "Wil\u00b7ly", "hat", "je\u00b7weent", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,", "NE", "VAFIN", "ADV", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Alle ham se jeweent!", "tokens": ["Al\u00b7le", "ham", "se", "je\u00b7weent", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "FM", "FM", "FM", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Das geht nun seit vier langen Jahren.", "tokens": ["Das", "geht", "nun", "seit", "vier", "lan\u00b7gen", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es trieft das Schmalz. Die Z\u00e4hre rinnt:", "tokens": ["Es", "trieft", "das", "Schmalz", ".", "Die", "Z\u00e4h\u00b7re", "rinnt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbder biedere Greis in Silberhaaren \u2013", "tokens": ["\u00bb", "der", "bie\u00b7de\u00b7re", "Greis", "in", "Sil\u00b7ber\u00b7haa\u00b7ren", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "das arme, so verfolgte Kind . . . \u00ab", "tokens": ["das", "ar\u00b7me", ",", "so", "ver\u00b7folg\u00b7te", "Kind", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "ADJA", "NN", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und selbst im Kino bl\u00fcht die Lilie.", "tokens": ["Und", "selbst", "im", "Ki\u00b7no", "bl\u00fcht", "die", "Li\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Das Fridericus-Auge tr\u00e4nt . . .", "tokens": ["Das", "Fri\u00b7de\u00b7ri\u00b7cus\u00b7Au\u00b7ge", "tr\u00e4nt", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das liegt nun mal in der Familie . . .", "tokens": ["Das", "liegt", "nun", "mal", "in", "der", "Fa\u00b7mi\u00b7lie", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Vater hat jeweent, Willy hat jeweent \u2013", "tokens": ["Va\u00b7ter", "hat", "je\u00b7weent", ",", "Wil\u00b7ly", "hat", "je\u00b7weent", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,", "NE", "VAFIN", "ADV", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Alle ham se jeweent!", "tokens": ["Al\u00b7le", "ham", "se", "je\u00b7weent", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "FM", "FM", "FM", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sie schreiben Fibeln f\u00fcr die Kleinen \u2013", "tokens": ["Sie", "schrei\u00b7ben", "Fi\u00b7beln", "f\u00fcr", "die", "Klei\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "drin steht: \u00bbIch hab es nicht gewollt!\u00ab", "tokens": ["drin", "steht", ":", "\u00bb", "Ich", "hab", "es", "nicht", "ge\u00b7wollt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PPER", "PTKNEG", "VMPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Krone fiel. Wer wird denn weinen!", "tokens": ["Die", "Kro\u00b7ne", "fiel", ".", "Wer", "wird", "denn", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ganze Geld kam nachgerollt.", "tokens": ["Das", "gan\u00b7ze", "Geld", "kam", "nach\u00b7ge\u00b7rollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ewig Gestern \u2013 nie ein Morgen.", "tokens": ["Ein", "e\u00b7wig", "Ge\u00b7stern", "\u2013", "nie", "ein", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$(", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Gott, die Welt hat andre Sorgen!", "tokens": ["Mein", "Gott", ",", "die", "Welt", "hat", "and\u00b7re", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es tr\u00e4gt ein Volk die schwersten Lasten . . .", "tokens": ["Es", "tr\u00e4gt", "ein", "Volk", "die", "schwers\u00b7ten", "Las\u00b7ten", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Melodien, dem Kitsch entlehnt,", "tokens": ["Mit", "Me\u00b7lo\u00b7dien", ",", "dem", "Kitsch", "ent\u00b7lehnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "drehn die an ihrem Leierkasten:", "tokens": ["drehn", "die", "an", "ih\u00b7rem", "Lei\u00b7er\u00b7kas\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.10": {"text": "Vater hat jeweent, Willy hat jeweent \u2013", "tokens": ["Va\u00b7ter", "hat", "je\u00b7weent", ",", "Wil\u00b7ly", "hat", "je\u00b7weent", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,", "NE", "VAFIN", "ADV", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Alle ham se jeweent!", "tokens": ["Al\u00b7le", "ham", "se", "je\u00b7weent", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "FM", "FM", "FM", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}