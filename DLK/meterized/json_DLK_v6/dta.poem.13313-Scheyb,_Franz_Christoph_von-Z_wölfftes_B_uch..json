{"dta.poem.13313": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas h\u00e4lt der Erde Rund? der Sterne Krei\u00df? die Welt?", "tokens": ["\u201e", "was", "h\u00e4lt", "der", "Er\u00b7de", "Rund", "?", "der", "Ster\u00b7ne", "Krei\u00df", "?", "die", "Welt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "NN", "ADJD", "$.", "ART", "NN", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "360\u201dDes Wercks Zusammenhang, in den es GOtt gestellt.", "tokens": ["\"", "Des", "Wercks", "Zu\u00b7sam\u00b7men\u00b7hang", ",", "in", "den", "es", "Gott", "ge\u00b7stellt", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewie h\u00f6rtet ihr, was ich hier zum erwegen bringe,", "tokens": ["\u201e", "wie", "h\u00f6r\u00b7tet", "ihr", ",", "was", "ich", "hier", "zum", "er\u00b7we\u00b7gen", "brin\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewann nicht der Krei\u00df der Luft durch mein Gespr\u00e4ch erkl\u00fcnge?", "tokens": ["\u201e", "wann", "nicht", "der", "Krei\u00df", "der", "Luft", "durch", "mein", "Ge\u00b7spr\u00e4ch", "er\u00b7kl\u00fcn\u00b7ge", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PTKNEG", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ebef\u00e4nde sich des Meers Umgrenzung lo\u00dfged\u00e4mmt,", "tokens": ["\u201e", "be\u00b7f\u00e4n\u00b7de", "sich", "des", "Meers", "Um\u00b7gren\u00b7zung", "lo\u00df\u00b7ge\u00b7d\u00e4mmt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PRF", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eso w\u00e4r das Vaterland und wir mit ihm verschwemmt;", "tokens": ["\u201e", "so", "w\u00e4r", "das", "Va\u00b7ter\u00b7land", "und", "wir", "mit", "ihm", "ver\u00b7schwemmt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "KON", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "365\u201dWas w\u00fcrde den Bezirck des Felds mit Thau befeuchten,", "tokens": ["\"", "Was", "w\u00fcr\u00b7de", "den", "Be\u00b7zirck", "des", "Felds", "mit", "Thau", "be\u00b7feuch\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewann nur der Sonne Schein sollt unabwechselnd leuchten?", "tokens": ["\u201e", "wann", "nur", "der", "Son\u00b7ne", "Schein", "sollt", "un\u00b7ab\u00b7wech\u00b7selnd", "leuch\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "ART", "NN", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eh\u00e4tt GOtt die K\u00f6rper leicht und keinen schwer verschafft,", "tokens": ["\u201e", "h\u00e4tt", "Gott", "die", "K\u00f6r\u00b7per", "leicht", "und", "kei\u00b7nen", "schwer", "ver\u00b7schafft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "ART", "NN", "ADJD", "KON", "PIAT", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewas w\u00e4r der Erde Ball? des Undings Eigenschaft.", "tokens": ["\u201e", "was", "w\u00e4r", "der", "Er\u00b7de", "Ball", "?", "des", "Un\u00b7dings", "Ei\u00b7gen\u00b7schaft", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "NN", "NN", "$.", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201egesezt, wir w\u00fc\u00dften nichts von Kleinheit oder Gr\u00f6sse;", "tokens": ["\u201e", "ge\u00b7sezt", ",", "wir", "w\u00fc\u00df\u00b7ten", "nichts", "von", "Klein\u00b7heit", "o\u00b7der", "Gr\u00f6s\u00b7se", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "PPER", "VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "370\u201dWas w\u00e4r das, so den Werth der Tugend in sich schl\u00f6sse?", "tokens": ["\"", "Was", "w\u00e4r", "das", ",", "so", "den", "Werth", "der", "Tu\u00b7gend", "in", "sich", "schl\u00f6s\u00b7se", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PDS", "$,", "ADV", "ART", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}