{"textgrid.poem.25986": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die junge K\u00f6nigin", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die junge K\u00f6nigin", "tokens": ["Die", "jun\u00b7ge", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Betrachtet ihre glei\u00dfen Schuh.", "tokens": ["Be\u00b7trach\u00b7tet", "ih\u00b7re", "glei\u00b7\u00dfen", "Schuh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie geht mit Unruh durch die S\u00e4le,", "tokens": ["Sie", "geht", "mit", "Un\u00b7ruh", "durch", "die", "S\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit wei\u00dfen Wangen den langen Tag,", "tokens": ["Mit", "wei\u00b7\u00dfen", "Wan\u00b7gen", "den", "lan\u00b7gen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und nichts vermag sie zu vers\u00f6hnen.", "tokens": ["Und", "nichts", "ver\u00b7mag", "sie", "zu", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Leben scheint ihr schon vergangen,", "tokens": ["Ihr", "Le\u00b7ben", "scheint", "ihr", "schon", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr L\u00e4cheln und der Jugend Verlangen.", "tokens": ["Ihr", "L\u00e4\u00b7cheln", "und", "der", "Ju\u00b7gend", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Hofdamen und Hofherrn gehen", "tokens": ["Hof\u00b7da\u00b7men", "und", "Hof\u00b7herrn", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Im Schweigen ihr nach", "tokens": ["Im", "Schwei\u00b7gen", "ihr", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Durch alle T\u00fcren, die offen stehen.", "tokens": ["Durch", "al\u00b7le", "T\u00fc\u00b7ren", ",", "die", "of\u00b7fen", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Auf glatten Dielen sp\u00fcren", "tokens": ["Auf", "glat\u00b7ten", "Die\u00b7len", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Sie Windhauch, von Schatten", "tokens": ["Sie", "Wind\u00b7hauch", ",", "von", "Schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Unsichtbaren Reigen.", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7ren", "Rei\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Im Dielenholze ert\u00f6nen von Geigen seufzende Weisen,", "tokens": ["Im", "Die\u00b7len\u00b7hol\u00b7ze", "er\u00b7t\u00f6\u00b7nen", "von", "Gei\u00b7gen", "seuf\u00b7zen\u00b7de", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Von leisen Geigen, die keiner sieht,", "tokens": ["Von", "lei\u00b7sen", "Gei\u00b7gen", ",", "die", "kei\u00b7ner", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Die wie unter Schmerzen aufst\u00f6hnen.", "tokens": ["Die", "wie", "un\u00b7ter", "Schmer\u00b7zen", "auf\u00b7st\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "Nachts gucken die Lichter der Kerzen und des Mondes zu,", "tokens": ["Nachts", "gu\u00b7cken", "die", "Lich\u00b7ter", "der", "Ker\u00b7zen", "und", "des", "Mon\u00b7des", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Wie Gesichter, die zucken.", "tokens": ["Wie", "Ge\u00b7sich\u00b7ter", ",", "die", "zu\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Die junge K\u00f6nigin aber geht ohne Ruh,", "tokens": ["Die", "jun\u00b7ge", "K\u00f6\u00b7ni\u00b7gin", "a\u00b7ber", "geht", "oh\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Bis ihre Augen einmal Alain trafen,", "tokens": ["Bis", "ih\u00b7re", "Au\u00b7gen", "ein\u00b7mal", "A\u00b7lain", "tra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Des Hofes Dichter, im blauen Mondschein eingeschlafen am Altan.", "tokens": ["Des", "Ho\u00b7fes", "Dich\u00b7ter", ",", "im", "blau\u00b7en", "Mond\u00b7schein", "ein\u00b7ge\u00b7schla\u00b7fen", "am", "Al\u00b7tan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.22": {"text": "Die K\u00f6nigin sieht nicht, da\u00df gar nicht sch\u00f6n, fast h\u00e4\u00dflich der S\u00e4ngersmann.", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "sieht", "nicht", ",", "da\u00df", "gar", "nicht", "sch\u00f6n", ",", "fast", "h\u00e4\u00df\u00b7lich", "der", "S\u00e4n\u00b7gers\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "PTKNEG", "ADJD", "$,", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.23": {"text": "Die K\u00f6nigin stund und beugt sich und k\u00fc\u00dft des Schlafenden Mund.", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "stund", "und", "beugt", "sich", "und", "k\u00fc\u00dft", "des", "Schla\u00b7fen\u00b7den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "PRF", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und das Gefolgt sieht's mit an, entsetzt.", "tokens": ["Und", "das", "Ge\u00b7folgt", "sieht's", "mit", "an", ",", "ent\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "APPR", "PTKVZ", "$,", "VVPP", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Keiner sich ihr Gel\u00fcste erkl\u00e4ren kann.", "tokens": ["Kei\u00b7ner", "sich", "ihr", "Ge\u00b7l\u00fcs\u00b7te", "er\u00b7kl\u00e4\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "\u00bbich k\u00fc\u00dfte den Mund, der mir wohlgetan", "tokens": ["\u00bb", "ich", "k\u00fc\u00df\u00b7te", "den", "Mund", ",", "der", "mir", "wohl\u00b7ge\u00b7tan"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Mit sch\u00f6nsten Worten, mit tugendhaften, klug erkoren,", "tokens": ["Mit", "sch\u00f6ns\u00b7ten", "Wor\u00b7ten", ",", "mit", "tu\u00b7gend\u00b7haf\u00b7ten", ",", "klug", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Kein Mund in meinem K\u00f6nigreich", "tokens": ["Kein", "Mund", "in", "mei\u00b7nem", "K\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Tat's diesem schlafenden Munde gleich.", "tokens": ["Tat's", "die\u00b7sem", "schla\u00b7fen\u00b7den", "Mun\u00b7de", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Doch wer eines Dichters Seele k\u00fc\u00dft, der stirbt daran.", "tokens": ["Doch", "wer", "ei\u00b7nes", "Dich\u00b7ters", "See\u00b7le", "k\u00fc\u00dft", ",", "der", "stirbt", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PAV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.31": {"text": "Schaudernd f\u00fchlt sich Unsterblichkeit f\u00fcr Sterbliche und t\u00f6dlich an.", "tokens": ["Schau\u00b7dernd", "f\u00fchlt", "sich", "U\u00b7nsterb\u00b7lich\u00b7keit", "f\u00fcr", "Sterb\u00b7li\u00b7che", "und", "t\u00f6d\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "NN", "APPR", "NN", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.32": {"text": "Wie h\u00e4\u00dflich, Herrn und Damen, euer L\u00e4cheln ist!", "tokens": ["Wie", "h\u00e4\u00df\u00b7lich", ",", "Herrn", "und", "Da\u00b7men", ",", "eu\u00b7er", "L\u00e4\u00b7cheln", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein Leben, ach, gottlob, da\u00df du nur sterblich bist!\u00ab", "tokens": ["Mein", "Le\u00b7ben", ",", "ach", ",", "gott\u00b7lob", ",", "da\u00df", "du", "nur", "sterb\u00b7lich", "bist", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$,", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die junge, junge K\u00f6nigin,", "tokens": ["Die", "jun\u00b7ge", ",", "jun\u00b7ge", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Sie spricht es hin, \u2013", "tokens": ["Sie", "spricht", "es", "hin", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Starb und lag l\u00e4chelnd bleich.", "tokens": ["Starb", "und", "lag", "l\u00e4\u00b7chelnd", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJD", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Und tot schien sie die Gl\u00fccklichste im K\u00f6nigreich.", "tokens": ["Und", "tot", "schien", "sie", "die", "Gl\u00fcck\u00b7lichs\u00b7te", "im", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}