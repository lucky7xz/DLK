{"textgrid.poem.64866": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bewahrst mein ernstumflossnes Bild du im Gedanken nicht?", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bewahrst mein ernstumflossnes Bild du im Gedanken nicht?", "tokens": ["Be\u00b7wahrst", "mein", "erns\u00b7tum\u00b7floss\u00b7nes", "Bild", "du", "im", "Ge\u00b7dan\u00b7ken", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PPER", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Bricht meine Liebe deiner Brust umeis'te Schranken nicht? \u2013", "tokens": ["Bricht", "mei\u00b7ne", "Lie\u00b7be", "dei\u00b7ner", "Brust", "um\u00b7eis'\u00b7te", "Schran\u00b7ken", "nicht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "ADJA", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ich denke fast, es mu\u00df gescheh'n, denn wer, o sage, wer", "tokens": ["Ich", "den\u00b7ke", "fast", ",", "es", "mu\u00df", "ge\u00b7scheh'n", ",", "denn", "wer", ",", "o", "sa\u00b7ge", ",", "wer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "VVPP", "$,", "KON", "PWS", "$,", "FM", "FM", "$,", "PWS"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Erbarmte meiner Seele sich, der t\u00f6dtlich kranken, nicht?", "tokens": ["Er\u00b7barm\u00b7te", "mei\u00b7ner", "See\u00b7le", "sich", ",", "der", "t\u00f6dt\u00b7lich", "kran\u00b7ken", ",", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PRF", "$,", "PRELS", "ADJD", "VVINF", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Das Meer der Zeit verschlang die Lust, die Hoffnung und das Heil,", "tokens": ["Das", "Meer", "der", "Zeit", "ver\u00b7schlang", "die", "Lust", ",", "die", "Hoff\u00b7nung", "und", "das", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Nur meine Leiden standen fest, nur sie versanken nicht!", "tokens": ["Nur", "mei\u00b7ne", "Lei\u00b7den", "stan\u00b7den", "fest", ",", "nur", "sie", "ver\u00b7san\u00b7ken", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Als Felsen ragen sie empor im w\u00fcsten Ocean;", "tokens": ["Als", "Fel\u00b7sen", "ra\u00b7gen", "sie", "em\u00b7por", "im", "w\u00fcs\u00b7ten", "O\u00b7cean", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.8": {"text": "Des Lebens Brandung rauscht drum her, allein sie wanken nicht.", "tokens": ["Des", "Le\u00b7bens", "Bran\u00b7dung", "rauscht", "drum", "her", ",", "al\u00b7lein", "sie", "wan\u00b7ken", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PAV", "PTKVZ", "$,", "ADV", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Doch sinnend weilet gern mein Geist auf ihrer h\u00f6chsten H\u00f6h,", "tokens": ["Doch", "sin\u00b7nend", "wei\u00b7let", "gern", "mein", "Geist", "auf", "ih\u00b7rer", "h\u00f6chs\u00b7ten", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Von Eb'nen k\u00f6nnt' er sich so leicht zum Himmel ranken nicht.", "tokens": ["Von", "Eb'\u00b7nen", "k\u00f6nnt'", "er", "sich", "so", "leicht", "zum", "Him\u00b7mel", "ran\u00b7ken", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Als Brautgeschenk gabst du mir Schmerz, der f\u00fchrt zu Gott hinan,", "tokens": ["Als", "Braut\u00b7ge\u00b7schenk", "gabst", "du", "mir", "Schmerz", ",", "der", "f\u00fchrt", "zu", "Gott", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PPER", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Wie sollte ich f\u00fcr solch Geschenk dir innigst danken nicht?", "tokens": ["Wie", "soll\u00b7te", "ich", "f\u00fcr", "solch", "Ge\u00b7schenk", "dir", "in\u00b7nigst", "dan\u00b7ken", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PIAT", "NN", "PPER", "ADJD", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}