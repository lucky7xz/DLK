{"dta.poem.18222": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber des Kirchhofs trauernde Gruft,", "tokens": ["Ue\u00b7ber", "des", "Kirch\u00b7hofs", "trau\u00b7ern\u00b7de", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und gleich verwehten Bl\u00fcthenflocken", "tokens": ["Und", "gleich", "ver\u00b7weh\u00b7ten", "Bl\u00fc\u00b7then\u00b7flo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verschwamm ihr Klang in der Morgenluft.", "tokens": ["Ver\u00b7schwamm", "ihr", "Klang", "in", "der", "Mor\u00b7gen\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich aber riefen sie in die Weite", "tokens": ["Mich", "a\u00b7ber", "rie\u00b7fen", "sie", "in", "die", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Und lie\u00dfen mich nicht im dumpfen Haus,", "tokens": ["Und", "lie\u00b7\u00dfen", "mich", "nicht", "im", "dum\u00b7pfen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und unter der Osterlieder Geleite", "tokens": ["Und", "un\u00b7ter", "der", "Os\u00b7ter\u00b7lie\u00b7der", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zog ich die Stra\u00dfen zum Thore hinaus.", "tokens": ["Zog", "ich", "die", "Stra\u00b7\u00dfen", "zum", "Tho\u00b7re", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Weit hinter mir im Morgend\u00e4mmer", "tokens": ["Weit", "hin\u00b7ter", "mir", "im", "Mor\u00b7gen\u00b7d\u00e4m\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich das Gem\u00e4uer der Stadt verlor,", "tokens": ["Sich", "das", "Ge\u00b7m\u00e4u\u00b7er", "der", "Stadt", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und selbst das Pochen der Eisenh\u00e4mmer", "tokens": ["Und", "selbst", "das", "Po\u00b7chen", "der", "Ei\u00b7sen\u00b7h\u00e4m\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Traf nur ged\u00e4mpft noch an mein Ohr.", "tokens": ["Traf", "nur", "ge\u00b7d\u00e4mpft", "noch", "an", "mein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch dehnte sich immer weiter und weiter", "tokens": ["Doch", "dehn\u00b7te", "sich", "im\u00b7mer", "wei\u00b7ter", "und", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "KON", "ADV"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Vor meinen Blicken der sonnige Gau,", "tokens": ["Vor", "mei\u00b7nen", "Bli\u00b7cken", "der", "son\u00b7ni\u00b7ge", "Gau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und jauchzend auf t\u00f6nender Himmelsleiter", "tokens": ["Und", "jauch\u00b7zend", "auf", "t\u00f6\u00b7nen\u00b7der", "Him\u00b7mels\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Schwang sich die Lerche ins Aetherblau.", "tokens": ["Schwang", "sich", "die", "Ler\u00b7che", "ins", "A\u00b7e\u00b7ther\u00b7blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Da stand ich denn nun am Waldesrande", "tokens": ["Da", "stand", "ich", "denn", "nun", "am", "Wal\u00b7des\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit meinen Gedanken so ganz allein", "tokens": ["Mit", "mei\u00b7nen", "Ge\u00b7dan\u00b7ken", "so", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und sah tief unter mir die Lande", "tokens": ["Und", "sah", "tief", "un\u00b7ter", "mir", "die", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liegen im flimmernden Sonnenschein.", "tokens": ["Lie\u00b7gen", "im", "flim\u00b7mern\u00b7den", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und als dann den letzten Zweifel zu rauben,", "tokens": ["Und", "als", "dann", "den", "letz\u00b7ten", "Zwei\u00b7fel", "zu", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein Sch\u00e4fer noch blies auf seiner Schalmei,", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "noch", "blies", "auf", "sei\u00b7ner", "Schal\u00b7mei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da wollte ich es selbst nicht glauben,", "tokens": ["Da", "woll\u00b7te", "ich", "es", "selbst", "nicht", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df Tod die L\u00f6sung des R\u00e4thsels sei.", "tokens": ["Da\u00df", "Tod", "die", "L\u00f6\u00b7sung", "des", "R\u00e4th\u00b7sels", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Da schien mir alles verweht und vergangen,", "tokens": ["Da", "schien", "mir", "al\u00b7les", "ver\u00b7weht", "und", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was ich betrauerte winterlang;", "tokens": ["Was", "ich", "be\u00b7trau\u00b7er\u00b7te", "win\u00b7ter\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und alle Saiten des Herzens klangen", "tokens": ["Und", "al\u00b7le", "Sai\u00b7ten", "des", "Her\u00b7zens", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zusammen im Auferstehungsgesang.", "tokens": ["Zu\u00b7sam\u00b7men", "im", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O, solche Seelenkl\u00e4nge dringen", "tokens": ["O", ",", "sol\u00b7che", "See\u00b7len\u00b7kl\u00e4n\u00b7ge", "drin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weit h\u00f6her noch in die Himmel empor,", "tokens": ["Weit", "h\u00f6\u00b7her", "noch", "in", "die", "Him\u00b7mel", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als je auf seinen Flatterschwingen", "tokens": ["Als", "je", "auf", "sei\u00b7nen", "Flat\u00b7ter\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Vogel sich in der Luft verlor!", "tokens": ["Ein", "Vo\u00b7gel", "sich", "in", "der", "Luft", "ver\u00b7lor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ja, Fest der Ostern, nun warst du gezogen", "tokens": ["Ja", ",", "Fest", "der", "Os\u00b7tern", ",", "nun", "warst", "du", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auch endlich in diese ver\u00f6dete Brust;", "tokens": ["Auch", "end\u00b7lich", "in", "die\u00b7se", "ver\u00b7\u00f6\u00b7de\u00b7te", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und dies Herz, das so oft schon das Leben betrogen,", "tokens": ["Und", "dies", "Herz", ",", "das", "so", "oft", "schon", "das", "Le\u00b7ben", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "$,", "PRELS", "ADV", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Erzitterte wieder von s\u00fc\u00dfer Lust", "tokens": ["Er\u00b7zit\u00b7ter\u00b7te", "wie\u00b7der", "von", "s\u00fc\u00b7\u00dfer", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und schl\u00e4gt nun der hohen Feier entgegen,", "tokens": ["Und", "schl\u00e4gt", "nun", "der", "ho\u00b7hen", "Fei\u00b7er", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die \u00fcber die Erde zu gie\u00dfen verhei\u00dft", "tokens": ["Die", "\u00fc\u00b7ber", "die", "Er\u00b7de", "zu", "gie\u00b7\u00dfen", "ver\u00b7hei\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Den herrlichsten aller himmlischen Segen,", "tokens": ["Den", "herr\u00b7lichs\u00b7ten", "al\u00b7ler", "himm\u00b7li\u00b7schen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den welterl\u00f6senden, heiligen Geist.", "tokens": ["Den", "wel\u00b7ter\u00b7l\u00f6\u00b7sen\u00b7den", ",", "hei\u00b7li\u00b7gen", "Geist", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Der heilige Geist ist die ewige Liebe,", "tokens": ["Der", "hei\u00b7li\u00b7ge", "Geist", "ist", "die", "e\u00b7wi\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Gott in die Herzen der Menschen gesenkt,", "tokens": ["Die", "Gott", "in", "die", "Her\u00b7zen", "der", "Men\u00b7schen", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und die mit jedem Ostertriebe", "tokens": ["Und", "die", "mit", "je\u00b7dem", "Os\u00b7ter\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von neuem sich zum Lichte dr\u00e4ngt.", "tokens": ["Von", "neu\u00b7em", "sich", "zum", "Lich\u00b7te", "dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie schwebt herab vom Himmelssaale", "tokens": ["Sie", "schwebt", "her\u00b7ab", "vom", "Him\u00b7mels\u00b7saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Jedem, der an sie noch glaubt \u2014", "tokens": ["Zu", "Je\u00b7dem", ",", "der", "an", "sie", "noch", "glaubt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "O neige, neige die goldene Schaale", "tokens": ["O", "nei\u00b7ge", ",", "nei\u00b7ge", "die", "gol\u00b7de\u00b7ne", "Schaa\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Auch hier auf dieses Beterhaupt!", "tokens": ["Auch", "hier", "auf", "die\u00b7ses", "Be\u00b7ter\u00b7haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}