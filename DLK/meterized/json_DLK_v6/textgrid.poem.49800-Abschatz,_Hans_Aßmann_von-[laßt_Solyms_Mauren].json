+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sieh an das Sehnen", "tokens": ["Sieh", "an", "das", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den Zorn zu stillen/", "tokens": ["Den", "Zorn", "zu", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die bittern Thr\u00e4nen", "tokens": ["Die", "bit\u00b7tern", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Um deinet willen.", "tokens": ["Um", "dei\u00b7net", "wil\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Die bangen Stunden", "tokens": ["Die", "ban\u00b7gen", "Stun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sieht man sich mehren:", "tokens": ["Sieht", "man", "sich", "meh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es wird gebunden", "tokens": ["Es", "wird", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Herr der Ehren.", "tokens": ["Der", "Herr", "der", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Das Mord-Geschlechte", "tokens": ["Das", "Mord\u00b7Ge\u00b7schlech\u00b7te"], "token_info"