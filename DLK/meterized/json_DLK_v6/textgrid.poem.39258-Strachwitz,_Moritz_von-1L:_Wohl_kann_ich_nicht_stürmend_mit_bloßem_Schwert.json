{"textgrid.poem.39258": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl kann ich nicht st\u00fcrmend mit blo\u00dfem Schwert", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl kann ich nicht st\u00fcrmend mit blo\u00dfem Schwert", "tokens": ["Wohl", "kann", "ich", "nicht", "st\u00fcr\u00b7mend", "mit", "blo\u00b7\u00dfem", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Aufdonnern im Kampfesflug,", "tokens": ["Auf\u00b7don\u00b7nern", "im", "Kamp\u00b7fes\u00b7flug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch kann ich singen der Helden Wert", "tokens": ["Doch", "kann", "ich", "sin\u00b7gen", "der", "Hel\u00b7den", "Wert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ART", "NN", "NE"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Im wirbelnden Windeszug.", "tokens": ["Im", "wir\u00b7beln\u00b7den", "Win\u00b7des\u00b7zug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch will ich spotten dem Minnedank", "tokens": ["Doch", "will", "ich", "spot\u00b7ten", "dem", "Min\u00b7ne\u00b7dank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im brausenden Liedergeroll,", "tokens": ["Im", "brau\u00b7sen\u00b7den", "Lie\u00b7der\u00b7ge\u00b7roll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch will ich singen und sagen frank,", "tokens": ["Doch", "will", "ich", "sin\u00b7gen", "und", "sa\u00b7gen", "frank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie das mutige Herz mir schwoll.", "tokens": ["Wie", "das", "mu\u00b7ti\u00b7ge", "Herz", "mir", "schwoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}