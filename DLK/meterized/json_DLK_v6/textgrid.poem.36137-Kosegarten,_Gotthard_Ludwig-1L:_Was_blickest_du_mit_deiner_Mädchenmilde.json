{"textgrid.poem.36137": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was blickest du mit deiner M\u00e4dchenmilde", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was blickest du mit deiner M\u00e4dchenmilde", "tokens": ["Was", "bli\u00b7ckest", "du", "mit", "dei\u00b7ner", "M\u00e4d\u00b7chen\u00b7mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich an, und fragst, was meinen Kummer schafft?", "tokens": ["Mich", "an", ",", "und", "fragst", ",", "was", "mei\u00b7nen", "Kum\u00b7mer", "schafft", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KON", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was meinen Kummer schafft, das heilt nicht deine Milde,", "tokens": ["Was", "mei\u00b7nen", "Kum\u00b7mer", "schafft", ",", "das", "heilt", "nicht", "dei\u00b7ne", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das heilet keine Erdenkraft.", "tokens": ["Das", "hei\u00b7let", "kei\u00b7ne", "Er\u00b7den\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Busen tief, da steht mit Demantkeilen", "tokens": ["Im", "Bu\u00b7sen", "tief", ",", "da", "steht", "mit", "De\u00b7mant\u00b7kei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "$,", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wunde eingebohrt, die mir das Herze bricht.", "tokens": ["Die", "Wun\u00b7de", "ein\u00b7ge\u00b7bohrt", ",", "die", "mir", "das", "Her\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie blutet, t\u00f6dlich! \u2013 ach! sie heilen kann nur Eine,", "tokens": ["Sie", "blu\u00b7tet", ",", "t\u00f6d\u00b7lich", "!", "\u2013", "ach", "!", "sie", "hei\u00b7len", "kann", "nur", "Ei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "$.", "$(", "ITJ", "$.", "PPER", "VVINF", "VMFIN", "ADV", "ART", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und diese Eine will es nicht.", "tokens": ["Und", "die\u00b7se", "Ei\u00b7ne", "will", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PIS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du wolltest wol, und \u2013 \u2013 ja! du kannst! Im Busen", "tokens": ["Du", "woll\u00b7test", "wol", ",", "und", "\u2013", "\u2013", "ja", "!", "du", "kannst", "!", "Im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "$,", "KON", "$(", "$(", "PTKANT", "$.", "PPER", "VMFIN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerrei\u00df den Faden nur, daran mein Herz noch hangt,", "tokens": ["Zer\u00b7rei\u00df", "den", "Fa\u00b7den", "nur", ",", "da\u00b7ran", "mein", "Herz", "noch", "hangt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "PAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und r\u00f6chelt dann die\u00df Herz, und dr\u00e4ngt zum blut'gen Busen", "tokens": ["Und", "r\u00f6\u00b7chelt", "dann", "die\u00df", "Herz", ",", "und", "dr\u00e4ngt", "zum", "blut'\u00b7gen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PDS", "NN", "$,", "KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Heraus, und zuckt, und bebt \u2013 so wiss', da\u00df es dir dankt!!", "tokens": ["He\u00b7raus", ",", "und", "zuckt", ",", "und", "bebt", "\u2013", "so", "wiss'", ",", "da\u00df", "es", "dir", "dankt", "!!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$(", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}