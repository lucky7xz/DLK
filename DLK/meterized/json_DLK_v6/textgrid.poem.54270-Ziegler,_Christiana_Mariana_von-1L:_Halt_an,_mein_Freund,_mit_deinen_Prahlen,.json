{"textgrid.poem.54270": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Halt an, mein Freund, mit deinen Prahlen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halt an, mein Freund, mit deinen Prahlen,", "tokens": ["Halt", "an", ",", "mein", "Freund", ",", "mit", "dei\u00b7nen", "Prah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es rechnet Herr Prudentius", "tokens": ["Es", "rech\u00b7net", "Herr", "Pru\u00b7den\u00b7ti\u00b7us"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "NE"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Viel tausend tausend auszuzahlen,", "tokens": ["Viel", "tau\u00b7send", "tau\u00b7send", "aus\u00b7zu\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "CARD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wovon er j\u00e4hrlich leben mu\u00df,", "tokens": ["Wo\u00b7von", "er", "j\u00e4hr\u00b7lich", "le\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da seine Renten doch, h\u00f6rt man den Wind nicht wehen,", "tokens": ["Da", "sei\u00b7ne", "Ren\u00b7ten", "doch", ",", "h\u00f6rt", "man", "den", "Wind", "nicht", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,", "VVFIN", "PIS", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "In finster Groschen nur bestehen.", "tokens": ["In", "fins\u00b7ter", "Gro\u00b7schen", "nur", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Spricht dann und wann ein Gast mit ein,", "tokens": ["Spricht", "dann", "und", "wann", "ein", "Gast", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "PWAV", "ART", "NN", "APPR", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So mu\u00df der Kinder Pathen-Geld,", "tokens": ["So", "mu\u00df", "der", "Kin\u00b7der", "Pa\u00b7then\u00b7Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Weil Schmahlhan\u00df K\u00fcch und Tisch bestellt,", "tokens": ["Weil", "Schmahl\u00b7han\u00df", "K\u00fcch", "und", "Tisch", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vor di\u00dfmahl Koch und Kellner seyn,", "tokens": ["Vor", "di\u00df\u00b7mahl", "Koch", "und", "Kell\u00b7ner", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NE", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und reicht es noch nicht zu, so mu\u00df bey Fre\u00df- und Sauffen", "tokens": ["Und", "reicht", "es", "noch", "nicht", "zu", ",", "so", "mu\u00df", "bey", "Fre\u00df", "und", "Sauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,", "ADV", "VMFIN", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Caffee-Topff, der Beine satt,", "tokens": ["Der", "Caf\u00b7fee\u00b7Topff", ",", "der", "Bei\u00b7ne", "satt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und eins noch mehr als Menschen hat,", "tokens": ["Und", "eins", "noch", "mehr", "als", "Men\u00b7schen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADV", "KOUS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Nach der Ebr\u00e4er Schule lauffen.", "tokens": ["Nach", "der", "Eb\u00b7r\u00e4\u00b7er", "Schu\u00b7le", "lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Wiewohl wenn man recht mit Bedacht", "tokens": ["Wie\u00b7wohl", "wenn", "man", "recht", "mit", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "ADV", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.16": {"text": "Das Hau\u00df, so du bewohnst, betracht,", "tokens": ["Das", "Hau\u00df", ",", "so", "du", "be\u00b7wohnst", ",", "be\u00b7tracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So kan der Wahn uns leicht verf\u00fchren,", "tokens": ["So", "kan", "der", "Wahn", "uns", "leicht", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Es mu\u00df ein grosses Thier in selbigen logieren,", "tokens": ["Es", "mu\u00df", "ein", "gros\u00b7ses", "Thier", "in", "sel\u00b7bi\u00b7gen", "lo\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn jeder Winckel ist mit Menschen angeh\u00e4ufft,", "tokens": ["Denn", "je\u00b7der", "Win\u00b7ckel", "ist", "mit", "Men\u00b7schen", "an\u00b7ge\u00b7h\u00e4ufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das immer eins ans andre l\u00e4ufft,", "tokens": ["Das", "im\u00b7mer", "eins", "ans", "and\u00b7re", "l\u00e4ufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIS", "APPRART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und solcher Zulauff steigt von Tag zu Tage h\u00f6her;", "tokens": ["Und", "sol\u00b7cher", "Zu\u00b7lauff", "steigt", "von", "Tag", "zu", "Ta\u00b7ge", "h\u00f6\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Allein es sind nur Manich\u00e4er.", "tokens": ["Al\u00b7lein", "es", "sind", "nur", "Ma\u00b7nic\u00b7h\u00e4\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}