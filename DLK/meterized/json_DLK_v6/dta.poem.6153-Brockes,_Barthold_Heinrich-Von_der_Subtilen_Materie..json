{"dta.poem.6153": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Subtilen Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sah\u2019 er, so voller Kunst, den sch\u00f6nen Bau der Welt;", "tokens": ["Sah'", "er", ",", "so", "vol\u00b7ler", "Kunst", ",", "den", "sch\u00f6\u00b7nen", "Bau", "der", "Welt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat er sich ihn, als wenn er lebte,", "tokens": ["Hat", "er", "sich", "ihn", ",", "als", "wenn", "er", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PPER", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und durch den Himmels-Stoff beseelt sey, vorgestellt.", "tokens": ["Und", "durch", "den", "Him\u00b7mels\u00b7Stoff", "be\u00b7seelt", "sey", ",", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch sonder so gar weit zu gehen,", "tokens": ["Doch", "son\u00b7der", "so", "gar", "weit", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kan man von diesem Stoff so viel gewi\u00df verstehen,", "tokens": ["Kan", "man", "von", "die\u00b7sem", "Stoff", "so", "viel", "ge\u00b7wi\u00df", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PDAT", "NN", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er sey an jedem Ort. Durch die stets rege Gluht", "tokens": ["Er", "sey", "an", "je\u00b7dem", "Ort", ".", "Durch", "die", "stets", "re\u00b7ge", "Gluht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$.", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Entwickelt er die Pflantzen, senckt ihr Bluht,", "tokens": ["Ent\u00b7wi\u00b7ckelt", "er", "die", "Pflant\u00b7zen", ",", "senckt", "ihr", "Bluht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den fruchtbarn Nahrungs-Safft, in ihren Schoo\u00df;", "tokens": ["Den", "frucht\u00b7barn", "Nah\u00b7rungs\u00b7\u00b7S\u00b7afft", ",", "in", "ih\u00b7ren", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Es wird der Keim durch ihn blo\u00df fruchtbar, reg und gro\u00df.", "tokens": ["Es", "wird", "der", "Keim", "durch", "ihn", "blo\u00df", "frucht\u00b7bar", ",", "reg", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er wirckt in unserm Leib\u2019 den wurtzelichten Safft,", "tokens": ["Er", "wirckt", "in", "un\u00b7serm", "Leib'", "den", "wurt\u00b7ze\u00b7lich\u00b7ten", "Safft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der unsre Geister treibt, wodurch das Bluht", "tokens": ["Der", "uns\u00b7re", "Geis\u00b7ter", "treibt", ",", "wo\u00b7durch", "das", "Bluht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So fl\u00fc\u00dfig wird, und nimmer ruht:", "tokens": ["So", "fl\u00fc\u00b7\u00dfig", "wird", ",", "und", "nim\u00b7mer", "ruht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die g\u00e4ntzliche Natur versp\u00fchret seine Krafft.", "tokens": ["Die", "g\u00e4ntz\u00b7li\u00b7che", "Na\u00b7tur", "ver\u00b7sp\u00fch\u00b7ret", "sei\u00b7ne", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}