{"dta.poem.12008": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst, es ist die Welt geartet zum Entarten,", "tokens": ["Du", "sagst", ",", "es", "ist", "die", "Welt", "ge\u00b7ar\u00b7tet", "zum", "Ent\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und weiter stets von Gott abf\u00fchren ihre Fahrten.", "tokens": ["Und", "wei\u00b7ter", "stets", "von", "Gott", "ab\u00b7f\u00fch\u00b7ren", "ih\u00b7re", "Fahr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich aber sage dir: Sie ist alswie sie war,", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", "dir", ":", "Sie", "ist", "als\u00b7wie", "sie", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieselbige, wie Gott derselb' ist immerdar.", "tokens": ["Die\u00b7sel\u00b7bi\u00b7ge", ",", "wie", "Gott", "der\u00b7selb'", "ist", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "NN", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von wannen kommt sie denn? Von Gott. Wo geht sie hin?", "tokens": ["Von", "wan\u00b7nen", "kommt", "sie", "denn", "?", "Von", "Gott", ".", "Wo", "geht", "sie", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADV", "$.", "APPR", "NN", "$.", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Gott zur\u00fcck. So schwebt in Gott sie mittenin.", "tokens": ["Zu", "Gott", "zu\u00b7r\u00fcck", ".", "So", "schwebt", "in", "Gott", "sie", "mit\u00b7te\u00b7nin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und ferner, n\u00e4her, ist sie ihm auf keinem Schritte,", "tokens": ["Und", "fer\u00b7ner", ",", "n\u00e4\u00b7her", ",", "ist", "sie", "ihm", "auf", "kei\u00b7nem", "Schrit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "$,", "VAFIN", "PPER", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wie am Anfang und am End' ist in der Mitte.", "tokens": ["Der", "wie", "am", "An\u00b7fang", "und", "am", "End'", "ist", "in", "der", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "APPRART", "NN", "KON", "APPRART", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du sagst: des G\u00f6ttlichen, das sie zuerst empfangen,", "tokens": ["Du", "sagst", ":", "des", "G\u00f6tt\u00b7li\u00b7chen", ",", "das", "sie", "zu\u00b7erst", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist im Verlauf der Zeit ihr mehr und mehr entgangen.", "tokens": ["Ist", "im", "Ver\u00b7lauf", "der", "Zeit", "ihr", "mehr", "und", "mehr", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "PPER", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Verlodert ist der Geist, gleich D\u00fcften die zerstieben,", "tokens": ["Ver\u00b7lo\u00b7dert", "ist", "der", "Geist", ",", "gleich", "D\u00fcf\u00b7ten", "die", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "ADV", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer todter ist der Stoff zur\u00fcckgeblieben.", "tokens": ["Und", "im\u00b7mer", "tod\u00b7ter", "ist", "der", "Stoff", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich aber sage dir: Kein Seelend\u00fcftchen gieng", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", "dir", ":", "Kein", "See\u00b7len\u00b7d\u00fcft\u00b7chen", "gieng"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr aus, daf\u00fcr sie nicht ein anderes empfieng.", "tokens": ["Ihr", "aus", ",", "da\u00b7f\u00fcr", "sie", "nicht", "ein", "an\u00b7de\u00b7res", "emp\u00b7fi\u00b7eng", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PAV", "PPER", "PTKNEG", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Der Othem Gottes wirkt nicht nur der Blum' Entfaltung,", "tokens": ["Der", "O\u00b7them", "Got\u00b7tes", "wirkt", "nicht", "nur", "der", "Blum'", "Ent\u00b7fal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihre Erhaltung auch und ew'ge Umgestaltung.", "tokens": ["Ih\u00b7re", "Er\u00b7hal\u00b7tung", "auch", "und", "ew'\u00b7ge", "Um\u00b7ge\u00b7stal\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.9": {"line.1": {"text": "Sch\u00f6n wie des Morgens gl\u00e4nzt des Abends Rosenbucht,", "tokens": ["Sch\u00f6n", "wie", "des", "Mor\u00b7gens", "gl\u00e4nzt", "des", "A\u00b7bends", "Ro\u00b7sen\u00b7bucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sch\u00f6n ist wie Fr\u00fchlingskranz des Herbstes reife Wucht.", "tokens": ["Sch\u00f6n", "ist", "wie", "Fr\u00fch\u00b7lings\u00b7kranz", "des", "Herbs\u00b7tes", "rei\u00b7fe", "Wucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KOKOM", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Mag Morgenfrische dort im Mittagsbrand ermatten,", "tokens": ["Mag", "Mor\u00b7gen\u00b7fri\u00b7sche", "dort", "im", "Mit\u00b7tags\u00b7brand", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herbstd\u00e4mmerung sich hier in Winternacht verschatten;", "tokens": ["Herbst\u00b7d\u00e4m\u00b7me\u00b7rung", "sich", "hier", "in", "Win\u00b7ter\u00b7nacht", "ver\u00b7schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Von neuem immer frisch, von neuem immer klar,", "tokens": ["Von", "neu\u00b7em", "im\u00b7mer", "frisch", ",", "von", "neu\u00b7em", "im\u00b7mer", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "ADJD", "$,", "APPR", "ADJA", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Gottes gro\u00dfer Tag, das ew'ge Weltenjahr.", "tokens": ["Ist", "Got\u00b7tes", "gro\u00b7\u00dfer", "Tag", ",", "das", "ew'\u00b7ge", "Wel\u00b7ten\u00b7jahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Obs wintern, sommern mag, ob tagen oder nachten,", "tokens": ["Obs", "win\u00b7tern", ",", "som\u00b7mern", "mag", ",", "ob", "ta\u00b7gen", "o\u00b7der", "nach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "VMFIN", "$,", "KOUS", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df uns im Flu\u00df der Zeit die Ewigkeit betrachten!", "tokens": ["La\u00df", "uns", "im", "Flu\u00df", "der", "Zeit", "die", "E\u00b7wig\u00b7keit", "be\u00b7trach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}