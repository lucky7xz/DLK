{"dta.poem.3951": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Xi.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Als ein Verm\u00e4hlungs-Tag den Nahmen N.", "tokens": ["Als", "ein", "Ver\u00b7m\u00e4h\u00b7lungs\u00b7Tag", "den", "Nah\u00b7men", "N."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fchrte,", "tokens": ["f\u00fchr\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und treu-ergebne Pflicht die Lust zu schertzen", "tokens": ["Und", "treu\u00b7er\u00b7geb\u00b7ne", "Pflicht", "die", "Lust", "zu", "schert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sp\u00fchrte;", "tokens": ["sp\u00fchr\u00b7te", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Als ein sehr werther Freund in Ehestand ger\u00fcckt,", "tokens": ["Als", "ein", "sehr", "wert\u00b7her", "Freund", "in", "E\u00b7hes\u00b7tand", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat man ihm diesen Brief mit Sehnsucht zuge-", "tokens": ["Hat", "man", "ihm", "die\u00b7sen", "Brief", "mit", "Sehn\u00b7sucht", "zu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "PPER", "PDAT", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "schickt.", "tokens": ["schickt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}