{"textgrid.poem.64051": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Ahndung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laue L\u00fcfte s\u00e4useln,", "tokens": ["Lau\u00b7e", "L\u00fcf\u00b7te", "s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und die Wellen kr\u00e4useln", "tokens": ["Und", "die", "Wel\u00b7len", "kr\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fl\u00fcsternd sich im Meer;", "tokens": ["Fl\u00fcs\u00b7ternd", "sich", "im", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mondenstrahlen beben", "tokens": ["Mon\u00b7dens\u00b7trah\u00b7len", "be\u00b7ben"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf der Fluth und schweben", "tokens": ["Auf", "der", "Fluth", "und", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Gl\u00e4nzend hin und her.", "tokens": ["Gl\u00e4n\u00b7zend", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Holde Melodieen", "tokens": ["Hol\u00b7de", "Me\u00b7lo\u00b7di\u00b7e\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Aus der Ferne ziehen", "tokens": ["Aus", "der", "Fer\u00b7ne", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Klingend durch die Nacht;", "tokens": ["Klin\u00b7gend", "durch", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und die Espen zittern,", "tokens": ["Und", "die", "Es\u00b7pen", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wie in Ungewittern", "tokens": ["Wie", "in", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wenn der Sturm erwacht.", "tokens": ["Wenn", "der", "Sturm", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ist es Geistern\u00e4he,", "tokens": ["Ist", "es", "Geis\u00b7ter\u00b7n\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die mit Wohl und Wehe", "tokens": ["Die", "mit", "Wohl", "und", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schauernd f\u00fcllt mein Herz?", "tokens": ["Schau\u00b7ernd", "f\u00fcllt", "mein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Steigen Engellieder", "tokens": ["Stei\u00b7gen", "En\u00b7gel\u00b7lie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aus den L\u00fcften nieder,", "tokens": ["Aus", "den", "L\u00fcf\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Lindernd meinen Schmerz? \u2013", "tokens": ["Lin\u00b7dernd", "mei\u00b7nen", "Schmerz", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "S\u00fcsse Fantasieen,", "tokens": ["S\u00fcs\u00b7se", "Fan\u00b7ta\u00b7si\u00b7e\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Eilet nicht zu fliehen,", "tokens": ["Ei\u00b7let", "nicht", "zu", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Labt den matten Sinn.", "tokens": ["Labt", "den", "mat\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ach in h\u00f6h're R\u00e4ume", "tokens": ["Ach", "in", "h\u00f6h'\u00b7re", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ziehn der Ahndung Tr\u00e4ume", "tokens": ["Ziehn", "der", "Ahn\u00b7dung", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mitleidsvoll ihn hin.", "tokens": ["Mit\u00b7le\u00b7ids\u00b7voll", "ihn", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}