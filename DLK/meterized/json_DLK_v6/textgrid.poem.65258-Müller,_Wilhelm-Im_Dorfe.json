{"textgrid.poem.65258": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Im Dorfe", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bellen die Hunde, es rasseln die Ketten.", "tokens": ["Es", "bel\u00b7len", "die", "Hun\u00b7de", ",", "es", "ras\u00b7seln", "die", "Ket\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Menschen schnarchen in ihren Betten,", "tokens": ["Die", "Men\u00b7schen", "schnar\u00b7chen", "in", "ih\u00b7ren", "Bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tr\u00e4umen sich Manches, was sie nicht haben,", "tokens": ["Tr\u00e4u\u00b7men", "sich", "Man\u00b7ches", ",", "was", "sie", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PIS", "$,", "PRELS", "PPER", "PTKNEG", "VAINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Thun sich im Guten und Argen erlaben:", "tokens": ["Thun", "sich", "im", "Gu\u00b7ten", "und", "Ar\u00b7gen", "er\u00b7la\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "KON", "NN", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Und morgen fr\u00fch ist Alles zerflossen. \u2013", "tokens": ["Und", "mor\u00b7gen", "fr\u00fch", "ist", "Al\u00b7les", "zer\u00b7flos\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PIS", "VVPP", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Je nun, sie haben ihr Theil genossen,", "tokens": ["Je", "nun", ",", "sie", "ha\u00b7ben", "ihr", "Theil", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und hoffen, was sie noch \u00fcbrig lie\u00dfen,", "tokens": ["Und", "hof\u00b7fen", ",", "was", "sie", "noch", "\u00fcb\u00b7rig", "lie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Doch wieder zu finden auf ihren Kissen.", "tokens": ["Doch", "wie\u00b7der", "zu", "fin\u00b7den", "auf", "ih\u00b7ren", "Kis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Bellt mich nur fort, ihr wachen Hunde,", "tokens": ["Bellt", "mich", "nur", "fort", ",", "ihr", "wa\u00b7chen", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft mich nicht ruhn in der Schlummerstunde!", "tokens": ["La\u00dft", "mich", "nicht", "ruhn", "in", "der", "Schlum\u00b7mer\u00b7stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ich bin zu Ende mit allen Tr\u00e4umen \u2013", "tokens": ["Ich", "bin", "zu", "En\u00b7de", "mit", "al\u00b7len", "Tr\u00e4u\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was will ich unter den Schl\u00e4fern s\u00e4umen?", "tokens": ["Was", "will", "ich", "un\u00b7ter", "den", "Schl\u00e4\u00b7fern", "s\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}