{"dta.poem.13031": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man h\u00f6rte dem Gespr\u00e4ch bisher aufmercksam zu,", "tokens": ["Man", "h\u00f6r\u00b7te", "dem", "Ge\u00b7spr\u00e4ch", "bis\u00b7her", "auf\u00b7merck\u00b7sam", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Fast jedermann gab acht, und blieb in stiller Ruh;", "tokens": ["Fast", "je\u00b7der\u00b7mann", "gab", "acht", ",", "und", "blieb", "in", "stil\u00b7ler", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "CARD", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur diesen Augenblick entstund an einer Seite", "tokens": ["Nur", "die\u00b7sen", "Au\u00b7gen\u00b7blick", "ent\u00b7stund", "an", "ei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "200Ein Murmeln und Ger\u00e4usch, als wann man sich erfreute.", "tokens": ["Mur\u00b7meln", "und", "Ge\u00b7r\u00e4usch", ",", "als", "wann", "man", "sich", "er\u00b7freu\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KOUS", "PWAV", "PIS", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Man wu\u00dft nicht was es sey, bi\u00df endlich eine Th\u00fcr", "tokens": ["Man", "wu\u00dft", "nicht", "was", "es", "sey", ",", "bi\u00df", "end\u00b7lich", "ei\u00b7ne", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKNEG", "PWS", "PPER", "VAFIN", "$,", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Alldort sich \u00f6ffnete; mithin erfuhren wir", "tokens": ["A\u00b7lldort", "sich", "\u00f6ff\u00b7ne\u00b7te", ";", "mi\u00b7thin", "er\u00b7fuh\u00b7ren", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PRF", "VVFIN", "$.", "ADV", "VVFIN", "PPER"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Warum man sich erregt\u2019. Es kamen zwey Personen,", "tokens": ["Wa\u00b7rum", "man", "sich", "er\u00b7regt'", ".", "Es", "ka\u00b7men", "zwey", "Per\u00b7so\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "VVPP", "$.", "PPER", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vielleicht, wie gleich geschah, dem Kreise beyzuwohnen.", "tokens": ["Viel\u00b7leicht", ",", "wie", "gleich", "ge\u00b7schah", ",", "dem", "Krei\u00b7se", "bey\u00b7zu\u00b7woh\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "VVFIN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "205Dadurch fiel einigen die Meinung in den Sinn:", "tokens": ["fiel", "ei\u00b7ni\u00b7gen", "die", "Mei\u00b7nung", "in", "den", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Es komme selber auch vielleicht die K\u00f6niginn;", "tokens": ["Es", "kom\u00b7me", "sel\u00b7ber", "auch", "viel\u00b7leicht", "die", "K\u00f6\u00b7ni\u00b7ginn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man habe schon geh\u00f6rt derselben Stimme klingen;", "tokens": ["Man", "ha\u00b7be", "schon", "ge\u00b7h\u00f6rt", "der\u00b7sel\u00b7ben", "Stim\u00b7me", "klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "VVFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Beyden werden uns gewi\u00df die Nachricht bringen.", "tokens": ["Die", "Bey\u00b7den", "wer\u00b7den", "uns", "ge\u00b7wi\u00df", "die", "Nach\u00b7richt", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}