{"dta.poem.21584": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Wacht auf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Was dr\u00e4ngt ihr Felsen in die Wolken ein,", "tokens": ["Was", "dr\u00e4ngt", "ihr", "Fel\u00b7sen", "in", "die", "Wol\u00b7ken", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon rast das Meer und r\u00fcttelt Stein von Stein.", "tokens": ["Schon", "rast", "das", "Meer", "und", "r\u00fct\u00b7telt", "Stein", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was prahlt ihr W\u00e4lder stolz mit eurem Gr\u00fcn,", "tokens": ["Was", "prahlt", "ihr", "W\u00e4l\u00b7der", "stolz", "mit", "eu\u00b7rem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon seh\u2019 im West den Wetterstrahl ich gl\u00fch\u2019n.", "tokens": ["Schon", "seh'", "im", "West", "den", "Wet\u00b7ter\u00b7strahl", "ich", "gl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was ruft ihr Glocken friedlich zum Gebet,", "tokens": ["Was", "ruft", "ihr", "Glo\u00b7cken", "fried\u00b7lich", "zum", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn schon die Erde hohl und donnernd geht.", "tokens": ["Wenn", "schon", "die", "Er\u00b7de", "hohl", "und", "don\u00b7nernd", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was jauchzt ihr Menschen wie am Feiertag,", "tokens": ["Was", "jauchzt", "ihr", "Men\u00b7schen", "wie", "am", "Fei\u00b7er\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon grinst der Tod euch l\u00fcstern ins Gemach.", "tokens": ["Schon", "grinst", "der", "Tod", "euch", "l\u00fcs\u00b7tern", "ins", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O k\u00f6nntet ihr mit meinen Augen sehn,", "tokens": ["O", "k\u00f6nn\u00b7tet", "ihr", "mit", "mei\u00b7nen", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie br\u00fcnstig w\u00fcrdet ihr zum Himmel flehn.", "tokens": ["Wie", "br\u00fcns\u00b7tig", "w\u00fcr\u00b7det", "ihr", "zum", "Him\u00b7mel", "flehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Allweg kriecht Elend wie ein ekel Gift,", "tokens": ["All\u00b7weg", "kriecht", "E\u00b7lend", "wie", "ein", "e\u00b7kel", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Niemand wei\u00df, wen\u2019s heut zu Hause trifft.", "tokens": ["Und", "Nie\u00b7mand", "wei\u00df", ",", "wen's", "heut", "zu", "Hau\u00b7se", "trifft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Allweg hebt Streit sich ehern auf vom Ro\u00df,", "tokens": ["All\u00b7weg", "hebt", "Streit", "sich", "e\u00b7hern", "auf", "vom", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PRF", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und klirrend f\u00e4hrt ins Mark sein scharf Gescho\u00df.", "tokens": ["Und", "klir\u00b7rend", "f\u00e4hrt", "ins", "Mark", "sein", "scharf", "Ge\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Allweg weicht Einer schen dem Andern aus,", "tokens": ["All\u00b7weg", "weicht", "Ei\u00b7ner", "schen", "dem", "An\u00b7dern", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schlie\u00dft, wie vor dem Todfeind, Hof und Haus.", "tokens": ["Und", "schlie\u00dft", ",", "wie", "vor", "dem", "Tod\u00b7feind", ",", "Hof", "und", "Haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O g\u00e4b\u2019 der Herr mir seines Sturmes Mund,", "tokens": ["O", "g\u00e4b'", "der", "Herr", "mir", "sei\u00b7nes", "Stur\u00b7mes", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihr mich h\u00f6rtet all zur selben Stund.", "tokens": ["Da\u00df", "ihr", "mich", "h\u00f6r\u00b7tet", "all", "zur", "sel\u00b7ben", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "PIAT", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Da\u00df ihr mich h\u00f6rtet, H\u00fctte wie Palast. \u2014", "tokens": ["Da\u00df", "ihr", "mich", "h\u00f6r\u00b7tet", ",", "H\u00fct\u00b7te", "wie", "Pa\u00b7last", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "NN", "KOKOM", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wacht auf, wacht auf aus eurer Liebe Rast.", "tokens": ["Wacht", "auf", ",", "wacht", "auf", "aus", "eu\u00b7rer", "Lie\u00b7be", "Rast", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wacht auf vom feigen Pf\u00fchl hochm\u00fcth\u2019ger Lust,", "tokens": ["Wacht", "auf", "vom", "fei\u00b7gen", "Pf\u00fchl", "hoch\u00b7m\u00fcth'\u00b7ger", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Schlange Neid rei\u00dft von der warmen Brust.", "tokens": ["Die", "Schlan\u00b7ge", "Neid", "rei\u00dft", "von", "der", "war\u00b7men", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Wacht auf vom blut\u2019gen Rausch des Heldenthums,", "tokens": ["Wacht", "auf", "vom", "blut'\u00b7gen", "Rausch", "des", "Hel\u00b7den\u00b7thums", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Barmherzigkeit sei Mutter eures Ruhms.", "tokens": ["Barm\u00b7her\u00b7zig\u00b7keit", "sei", "Mut\u00b7ter", "eu\u00b7res", "Ruhms", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Wacht auf, eh\u2019 euch der Tag des Zorns ereilt,", "tokens": ["Wacht", "auf", ",", "eh'", "euch", "der", "Tag", "des", "Zorns", "er\u00b7eilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Todesangst vereint, was heut sich theilt.", "tokens": ["Und", "To\u00b7de\u00b7sangst", "ver\u00b7eint", ",", "was", "heut", "sich", "theilt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Seid l\u00e4nger nicht, ihr Frauen, matt und lau,", "tokens": ["Seid", "l\u00e4n\u00b7ger", "nicht", ",", "ihr", "Frau\u00b7en", ",", "matt", "und", "lau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Euch schm\u00fcckt ja Milde, wie die Knospe Thau.", "tokens": ["Euch", "schm\u00fcckt", "ja", "Mil\u00b7de", ",", "wie", "die", "Knos\u00b7pe", "Thau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Ihr lieben Frauen habt des Herzens Acht,", "tokens": ["Ihr", "lie\u00b7ben", "Frau\u00b7en", "habt", "des", "Her\u00b7zens", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Legt Gott zu F\u00fc\u00dfen die armsel\u2019ge Pracht.", "tokens": ["Legt", "Gott", "zu", "F\u00fc\u00b7\u00dfen", "die", "arm\u00b7sel'\u00b7ge", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+++-+", "measure": "zehnsilber"}}, "stanza.16": {"line.1": {"text": "Fort schleudr\u2019 ich alle Hoffnung, all\u2019 Vertrau\u2019n,", "tokens": ["Fort", "schleudr'", "ich", "al\u00b7le", "Hoff\u00b7nung", ",", "all'", "Ver\u00b7trau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ihr nicht helft den neuen Tempel bau\u2019n.", "tokens": ["Wenn", "ihr", "nicht", "helft", "den", "neu\u00b7en", "Tem\u00b7pel", "bau'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "O g\u00e4b\u2019 der Herr mir seines Fr\u00fchlings Mund,", "tokens": ["O", "g\u00e4b'", "der", "Herr", "mir", "sei\u00b7nes", "Fr\u00fch\u00b7lings", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von seiner Liebe br\u00e4cht\u2019 ich frohe Kund\u2019.", "tokens": ["Von", "sei\u00b7ner", "Lie\u00b7be", "br\u00e4cht'", "ich", "fro\u00b7he", "Kund'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Schaut einmal, einmal nur zu ihm empor,", "tokens": ["Schaut", "ein\u00b7mal", ",", "ein\u00b7mal", "nur", "zu", "ihm", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich bl\u00fcht euch auf des ganzen Lenzes Flor.", "tokens": ["Gleich", "bl\u00fcht", "euch", "auf", "des", "gan\u00b7zen", "Len\u00b7zes", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.19": {"line.1": {"text": "Werft ab des Alltags Sinn, des Alltags Kleid,", "tokens": ["Werft", "ab", "des", "All\u00b7tags", "Sinn", ",", "des", "All\u00b7tags", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich rauscht hernieder ewige Feierzeit.", "tokens": ["Gleich", "rauscht", "her\u00b7nie\u00b7der", "e\u00b7wi\u00b7ge", "Fei\u00b7er\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.20": {"line.1": {"text": "O werdet warm, facht wieder an die Gluth,", "tokens": ["O", "wer\u00b7det", "warm", ",", "facht", "wie\u00b7der", "an", "die", "Gluth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die unter eurer Hoffart Asche ruht.", "tokens": ["Die", "un\u00b7ter", "eu\u00b7rer", "Hof\u00b7fart", "A\u00b7sche", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.21": {"line.1": {"text": "O fangt nur einmal wieder an den Lauf,", "tokens": ["O", "fangt", "nur", "ein\u00b7mal", "wie\u00b7der", "an", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gott f\u00fchrt euch weiter, \u2014 auf, wacht auf, wacht auf. \u2014", "tokens": ["Gott", "f\u00fchrt", "euch", "wei\u00b7ter", ",", "auf", ",", "wacht", "auf", ",", "wacht", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "$(", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}