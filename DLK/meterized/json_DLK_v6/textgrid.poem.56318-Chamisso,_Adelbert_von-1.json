{"textgrid.poem.56318": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, Graf, die Jagd und wende dein Ro\u00df;", "tokens": ["La\u00df", ",", "Graf", ",", "die", "Jagd", "und", "wen\u00b7de", "dein", "Ro\u00df", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NE", "$,", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es wird, bevor du erreichest dein Schlo\u00df,", "tokens": ["Es", "wird", ",", "be\u00b7vor", "du", "er\u00b7rei\u00b7chest", "dein", "Schlo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo krei\u00dfend die Gr\u00e4fin begehret dein,", "tokens": ["Wo", "krei\u00b7\u00dfend", "die", "Gr\u00e4\u00b7fin", "be\u00b7ge\u00b7hret", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Erbe vielleicht dir geboren sein.", "tokens": ["Der", "Er\u00b7be", "viel\u00b7leicht", "dir", "ge\u00b7bo\u00b7ren", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sprengt er daher mit freudigem Mut!", "tokens": ["Wie", "sprengt", "er", "da\u00b7her", "mit", "freu\u00b7di\u00b7gem", "Mut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie trieft der Rappe von Schwei\u00df und von Blut!", "tokens": ["Wie", "trieft", "der", "Rap\u00b7pe", "von", "Schwei\u00df", "und", "von", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Burg erreicht er mit letzter Kraft, \u2013", "tokens": ["Die", "Burg", "er\u00b7reicht", "er", "mit", "letz\u00b7ter", "Kraft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verwirrung herrscht in der Dienerschaft.", "tokens": ["Ver\u00b7wir\u00b7rung", "herrscht", "in", "der", "Die\u00b7ner\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es dringt in das Frauengemach der Graf;", "tokens": ["Es", "dringt", "in", "das", "Frau\u00b7en\u00b7ge\u00b7mach", "der", "Graf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die W\u00f6chnerin liegt in ruhigem Schlaf,", "tokens": ["Die", "W\u00f6ch\u00b7ne\u00b7rin", "liegt", "in", "ru\u00b7hi\u00b7gem", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Frauen entfernt, die Fenster verh\u00e4ngt,", "tokens": ["Die", "Frau\u00b7en", "ent\u00b7fernt", ",", "die", "Fens\u00b7ter", "ver\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Wiege dicht an das Bette gedr\u00e4ngt.", "tokens": ["Die", "Wie\u00b7ge", "dicht", "an", "das", "Bet\u00b7te", "ge\u00b7dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Er deckt die Wieg auf, atmend kaum; \u2013", "tokens": ["Er", "deckt", "die", "Wieg", "auf", ",", "at\u00b7mend", "kaum", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADJD", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Knaben fa\u00dft der enge Raum,", "tokens": ["Zwei", "Kna\u00b7ben", "fa\u00dft", "der", "en\u00b7ge", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Haupt liegt einer, der andre am Fu\u00df;", "tokens": ["Zu", "Haupt", "liegt", "ei\u00b7ner", ",", "der", "and\u00b7re", "am", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "$,", "PRELS", "PIS", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie schwelgt nun sein Herz in \u00dcberflu\u00df!", "tokens": ["Wie", "schwelgt", "nun", "sein", "Herz", "in", "\u00dc\u00b7berf\u00b7lu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Er hebt den einen, den andern mit Lust", "tokens": ["Er", "hebt", "den", "ei\u00b7nen", ",", "den", "an\u00b7dern", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ART", "$,", "ART", "ADJA", "APPR", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus enger Wiege an seine Brust,", "tokens": ["Aus", "en\u00b7ger", "Wie\u00b7ge", "an", "sei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Er legt sie beisammen, und wieder hervor", "tokens": ["Er", "legt", "sie", "bei\u00b7sam\u00b7men", ",", "und", "wie\u00b7der", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADV", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie hebend h\u00e4lt er die beiden empor.", "tokens": ["Sie", "he\u00b7bend", "h\u00e4lt", "er", "die", "bei\u00b7den", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PPER", "ART", "PIAT", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbwie bin ich so reich, wie war ich so arm!", "tokens": ["\u00bb", "wie", "bin", "ich", "so", "reich", ",", "wie", "war", "ich", "so", "arm", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VAFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun wieg ich der Spr\u00f6\u00dflinge zwei im Arm,", "tokens": ["Nun", "wieg", "ich", "der", "Spr\u00f6\u00df\u00b7lin\u00b7ge", "zwei", "im", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "CARD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun gr\u00fcnt mein Stamm in \u00dcppigkeit,", "tokens": ["Nun", "gr\u00fcnt", "mein", "Stamm", "in", "\u00dcp\u00b7pig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun soll er mir ragen in Herrlichkeit!\u00ab", "tokens": ["Nun", "soll", "er", "mir", "ra\u00b7gen", "in", "Herr\u00b7lich\u00b7keit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da kommt die Wehemutter herein,", "tokens": ["Da", "kommt", "die", "We\u00b7he\u00b7mut\u00b7ter", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie ahndet schon, was geschehen mag sein,", "tokens": ["Sie", "ahn\u00b7det", "schon", ",", "was", "ge\u00b7sche\u00b7hen", "mag", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "VVINF", "VMFIN", "VAINF", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie h\u00f6rt und sieht ihn erschrocken an:", "tokens": ["Sie", "h\u00f6rt", "und", "sieht", "ihn", "er\u00b7schro\u00b7cken", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbwas hast du Graf, was hast du getan?", "tokens": ["\u00bb", "was", "hast", "du", "Graf", ",", "was", "hast", "du", "ge\u00b7tan", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "NE", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Entbunden ward mit der Herrin zugleich", "tokens": ["Ent\u00b7bun\u00b7den", "ward", "mit", "der", "Her\u00b7rin", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Schaffnerin, \u2013 was wirst du so bleich? \u2013", "tokens": ["Die", "Schaff\u00b7ne\u00b7rin", ",", "\u2013", "was", "wirst", "du", "so", "bleich", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "$(", "PWS", "VAFIN", "PPER", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sie hat, die hier sich gesch\u00e4ftigt verletzt,", "tokens": ["Sie", "hat", ",", "die", "hier", "sich", "ge\u00b7sch\u00e4f\u00b7tigt", "ver\u00b7letzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ADV", "PRF", "VVPP", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Kinder eins in die Welt gesetzt.", "tokens": ["Der", "Kin\u00b7der", "eins", "in", "die", "Welt", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Zu H\u00e4upten lag, der dir geh\u00f6rt,", "tokens": ["Zu", "H\u00e4up\u00b7ten", "lag", ",", "der", "dir", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der andre zu F\u00fc\u00dfen, wie sich's geh\u00f6rt.", "tokens": ["Der", "and\u00b7re", "zu", "F\u00fc\u00b7\u00dfen", ",", "wie", "sich's", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wer ist dein Blut, wer dein Geschlecht?", "tokens": ["Wer", "ist", "dein", "Blut", ",", "wer", "dein", "Ge\u00b7schlecht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "$,", "PWS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Leibeigen wer und niedrer Knecht?\u00ab", "tokens": ["Leib\u00b7ei\u00b7gen", "wer", "und", "nie\u00b7drer", "Knecht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PWS", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da ruft er entsetzt: \u00bbWas hab ich getan?", "tokens": ["Da", "ruft", "er", "ent\u00b7setzt", ":", "\u00bb", "Was", "hab", "ich", "ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$.", "$(", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Sohn, mein Sohn! wer zeigt mir ihn an?\u00ab", "tokens": ["Mein", "Sohn", ",", "mein", "Sohn", "!", "wer", "zeigt", "mir", "ihn", "an", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Erwachend ruft die Gr\u00e4fin: \u00bbMein Kind!", "tokens": ["Er\u00b7wa\u00b7chend", "ruft", "die", "Gr\u00e4\u00b7fin", ":", "\u00bb", "Mein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$.", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "O gebt mein eigenes Kind mir geschwind!\u00ab", "tokens": ["O", "gebt", "mein", "ei\u00b7ge\u00b7nes", "Kind", "mir", "ge\u00b7schwind", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.11": {"line.1": {"text": "Vergeblich Klage: kein Zeuge spricht,", "tokens": ["Ver\u00b7geb\u00b7lich", "Kla\u00b7ge", ":", "kein", "Zeu\u00b7ge", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu kennen sind die Kinder nicht,", "tokens": ["Zu", "ken\u00b7nen", "sind", "die", "Kin\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verloren ist der Irrung Spur,", "tokens": ["Ver\u00b7lo\u00b7ren", "ist", "der", "Ir\u00b7rung", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeichen schweigen, es schweigt die Natur.", "tokens": ["Die", "Zei\u00b7chen", "schwei\u00b7gen", ",", "es", "schweigt", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}