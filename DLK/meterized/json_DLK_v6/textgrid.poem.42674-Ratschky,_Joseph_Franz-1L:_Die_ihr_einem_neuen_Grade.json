{"textgrid.poem.42674": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die ihr einem neuen Grade", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr einem neuen Grade", "tokens": ["Die", "ihr", "ei\u00b7nem", "neu\u00b7en", "Gra\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Erkenntniss nun euch naht,", "tokens": ["Der", "Er\u00b7kennt\u00b7niss", "nun", "euch", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandert fest auf eurem Pfade!", "tokens": ["Wan\u00b7dert", "fest", "auf", "eu\u00b7rem", "Pfa\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wisst! es ist der Weisheit Pfad.", "tokens": ["Wisst", "!", "es", "ist", "der", "Weis\u00b7heit", "Pfad", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur der unverdrossne Mann", "tokens": ["Nur", "der", "un\u00b7ver\u00b7dross\u00b7ne", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dringt zum Quell des Lichts hinan.", "tokens": ["Dringt", "zum", "Quell", "des", "Lichts", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nehmt, o Pilger, zum Geleite", "tokens": ["Nehmt", ",", "o", "Pil\u00b7ger", ",", "zum", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eurer Br\u00fcder Segen mit!", "tokens": ["Eu\u00b7rer", "Br\u00fc\u00b7der", "Se\u00b7gen", "mit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorsicht sey euch st\u00e4ts zur Seite!", "tokens": ["Vor\u00b7sicht", "sey", "euch", "st\u00e4ts", "zur", "Sei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wissgier leite jeden Schritt!", "tokens": ["Wiss\u00b7gier", "lei\u00b7te", "je\u00b7den", "Schritt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pr\u00fcft, und werdet nie dem Wahn", "tokens": ["Pr\u00fcft", ",", "und", "wer\u00b7det", "nie", "dem", "Wahn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4ger Blindheit unterthan!", "tokens": ["Tr\u00e4\u00b7ger", "Blind\u00b7heit", "un\u00b7ter\u00b7than", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rauh ist zwar des Lebens Reise,", "tokens": ["Rauh", "ist", "zwar", "des", "Le\u00b7bens", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber s\u00fcst ist auch der Preis,", "tokens": ["A\u00b7ber", "s\u00fcst", "ist", "auch", "der", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der des Wandrers harrt, der weise", "tokens": ["Der", "des", "Wand\u00b7rers", "harrt", ",", "der", "wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Fahrt zu n\u00fctzen weiss.", "tokens": ["Sei\u00b7ne", "Fahrt", "zu", "n\u00fct\u00b7zen", "weiss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcbergl\u00fccklich ist der Mann,", "tokens": ["\u00dc\u00b7berg\u00b7l\u00fcck\u00b7lich", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der des Lichts sich r\u00fchmen kann.", "tokens": ["Der", "des", "Lichts", "sich", "r\u00fch\u00b7men", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}