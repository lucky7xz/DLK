{"textgrid.poem.48035": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Im Abendschatten", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun liegt der Reif auf allen Matten,", "tokens": ["Nun", "liegt", "der", "Reif", "auf", "al\u00b7len", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der letzte fahle Schein erblich,", "tokens": ["der", "letz\u00b7te", "fah\u00b7le", "Schein", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und traumhaft kommt im Abendschatten", "tokens": ["und", "traum\u00b7haft", "kommt", "im", "A\u00b7bend\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein Todessehnen \u00fcber mich.", "tokens": ["ein", "To\u00b7des\u00b7seh\u00b7nen", "\u00fc\u00b7ber", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich lie\u00df in d\u00e4mmergrauer Ferne", "tokens": ["Ich", "lie\u00df", "in", "d\u00e4m\u00b7mer\u00b7grau\u00b7er", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Hoffnung lange schon zur\u00fcck", "tokens": ["die", "Hoff\u00b7nung", "lan\u00b7ge", "schon", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und forsche nicht im Buch der Sterne", "tokens": ["und", "for\u00b7sche", "nicht", "im", "Buch", "der", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dem R\u00e4tsel nach vom Menschengl\u00fcck.", "tokens": ["dem", "R\u00e4t\u00b7sel", "nach", "vom", "Men\u00b7schen\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir geht kein Sommertag zu Ende,", "tokens": ["Mir", "geht", "kein", "Som\u00b7mer\u00b7tag", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "gewebt aus Duft und Farbenpracht: \u2013", "tokens": ["ge\u00b7webt", "aus", "Duft", "und", "Far\u00b7ben\u00b7pracht", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mein ist die Wintersonnenwende", "tokens": ["mein", "ist", "die", "Win\u00b7ter\u00b7son\u00b7nen\u00b7wen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mit ihrer ewig langen Nacht, \u2013", "tokens": ["mit", "ih\u00b7rer", "e\u00b7wig", "lan\u00b7gen", "Nacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "mit ihrer Nacht voll Geisterch\u00f6ren,", "tokens": ["mit", "ih\u00b7rer", "Nacht", "voll", "Geis\u00b7ter\u00b7ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "voll Eisesschauer, Sturmesklang,", "tokens": ["voll", "Ei\u00b7ses\u00b7schau\u00b7er", ",", "Stur\u00b7mes\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die keine Bl\u00fcte mag zerst\u00f6ren,", "tokens": ["die", "kei\u00b7ne", "Bl\u00fc\u00b7te", "mag", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "weil nie ans Licht die Knospe drang.", "tokens": ["weil", "nie", "ans", "Licht", "die", "Knos\u00b7pe", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}