{"dta.poem.19595": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Gedanken des K\u00f6nigs Ren   \u00e9   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Der fromme Lautenschl\u00e4ger Herr Ren", "tokens": ["Der", "from\u00b7me", "Lau\u00b7ten\u00b7schl\u00e4\u00b7ger", "Herr", "Ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Trug braune Locken \u2014 sie sind wei\u00df wie Schnee.", "tokens": ["Trug", "brau\u00b7ne", "Lo\u00b7cken", "sie", "sind", "wei\u00df", "wie", "Schnee", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "PPER", "VAFIN", "VVFIN", "KOKOM", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "An seiner Stirn verglomm der Kronen Glanz,", "tokens": ["An", "sei\u00b7ner", "Stirn", "ver\u00b7glomm", "der", "Kro\u00b7nen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da haftet nichts als nur ein Lorbeerkranz.", "tokens": ["Da", "haf\u00b7tet", "nichts", "als", "nur", "ein", "Lor\u00b7beer\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schlo\u00df Tarascon \u2014 er bietet's zum Verkauf \u2014", "tokens": ["Schlo\u00df", "Ta\u00b7ras\u00b7con", "er", "bie\u00b7tet's", "zum", "Ver\u00b7kauf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "NE", "APPRART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dran spritzt die blaue Rhone scherzend auf,", "tokens": ["Dran", "spritzt", "die", "blau\u00b7e", "Rho\u00b7ne", "scher\u00b7zend", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von hoher Warte wandert rings der Blick", "tokens": ["Von", "ho\u00b7her", "War\u00b7te", "wan\u00b7dert", "rings", "der", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der K\u00f6nig w\u00e4gt als Denker sein Geschick:", "tokens": ["Der", "K\u00f6\u00b7nig", "w\u00e4gt", "als", "Den\u00b7ker", "sein", "Ge\u00b7schick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u201es'ist eigen da\u00df man immer mich vertreibt!", "tokens": ["\u201e", "s'\u00b7ist", "ei\u00b7gen", "da\u00df", "man", "im\u00b7mer", "mich", "ver\u00b7treibt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "KOUS", "PIS", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "S'ist eigen da\u00df mir nichts in H\u00e4nden bleibt!", "tokens": ["S'\u00b7ist", "ei\u00b7gen", "da\u00df", "mir", "nichts", "in", "H\u00e4n\u00b7den", "bleibt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOUS", "PPER", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Lothringen erbt' ich, wo die Trift sich sonnt,", "tokens": ["Loth\u00b7rin\u00b7gen", "erbt'", "ich", ",", "wo", "die", "Trift", "sich", "sonnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das nahm mir weg Anton von Vaudemont.", "tokens": ["Das", "nahm", "mir", "weg", "An\u00b7ton", "von", "Vau\u00b7de\u00b7mont", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "NE", "APPR", "NE", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}, "stanza.4": {"line.1": {"text": "Dann erbt' ich flugs das F\u00fcrstenthum Anjou", "tokens": ["Dann", "erbt'", "ich", "flugs", "das", "F\u00fcrs\u00b7ten\u00b7thum", "An\u00b7jou"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und noch das nette L\u00e4ndlein Bar dazu \u2014", "tokens": ["Und", "noch", "das", "net\u00b7te", "L\u00e4nd\u00b7lein", "Bar", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "NE", "PAV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Herr K\u00f6nig Ludwig trat in mein Gela\u00df", "tokens": ["Herr", "K\u00f6\u00b7nig", "Lud\u00b7wig", "trat", "in", "mein", "Ge\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Gast und schrieb mir meinen Wanderpa\u00df.", "tokens": ["Als", "Gast", "und", "schrieb", "mir", "mei\u00b7nen", "Wan\u00b7der\u00b7pa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Reich Napel war's, das dann zu Erb mir fiel,", "tokens": ["Reich", "Na\u00b7pel", "wa\u00b7r's", ",", "das", "dann", "zu", "Erb", "mir", "fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "$,", "PRELS", "ADV", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Dort mischte sich der Aragon ins Spiel \u2014", "tokens": ["Dort", "mischte", "sich", "der", "A\u00b7ra\u00b7gon", "ins", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das sch\u00f6ne Napel! Richtig werd' ich schlemm!", "tokens": ["Das", "sch\u00f6\u00b7ne", "Na\u00b7pel", "!", "Rich\u00b7tig", "werd'", "ich", "schlemm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir bleibt das himmlische Jerusalem!", "tokens": ["Mir", "bleibt", "das", "himm\u00b7li\u00b7sche", "Je\u00b7ru\u00b7sa\u00b7lem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Da schimmert unverg\u00e4nglich Dach und Fach \u2014", "tokens": ["Da", "schim\u00b7mert", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Dach", "und", "Fach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich erb' es schon. Das Erben ist mein Sach!", "tokens": ["Ich", "erb'", "es", "schon", ".", "Das", "Er\u00b7ben", "ist", "mein", "Sach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch geht mein Sach, wie hier, so droben dort,", "tokens": ["Doch", "geht", "mein", "Sach", ",", "wie", "hier", ",", "so", "dro\u00b7ben", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADV", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jagt aus dem Himmel mich der Teufel fort.\u201c", "tokens": ["Jagt", "aus", "dem", "Him\u00b7mel", "mich", "der", "Teu\u00b7fel", "fort", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}}}}