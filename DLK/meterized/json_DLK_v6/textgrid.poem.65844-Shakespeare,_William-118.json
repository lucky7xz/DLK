{"textgrid.poem.65844": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "118", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie man die E\u00dflust reizt mit scharfen Br\u00fch'n,", "tokens": ["Wie", "man", "die", "E\u00df\u00b7lust", "reizt", "mit", "schar\u00b7fen", "Br\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie, wenn wir reinigende Tr\u00e4nke nehmen,", "tokens": ["Wie", ",", "wenn", "wir", "rei\u00b7ni\u00b7gen\u00b7de", "Tr\u00e4n\u00b7ke", "neh\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um unbekannten \u00dcbeln zu entfliehn,", "tokens": ["Um", "un\u00b7be\u00b7kann\u00b7ten", "\u00dc\u00b7beln", "zu", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus Krankheitsscheu zur Krankheit uns bequemen:", "tokens": ["Aus", "Krank\u00b7heits\u00b7scheu", "zur", "Krank\u00b7heit", "uns", "be\u00b7que\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So legt' auch ich, von deiner S\u00fc\u00dfe krank,", "tokens": ["So", "legt'", "auch", "ich", ",", "von", "dei\u00b7ner", "S\u00fc\u00b7\u00dfe", "krank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die nimmer s\u00e4ttigt, mich auf bitt're Speisen.", "tokens": ["Die", "nim\u00b7mer", "s\u00e4t\u00b7tigt", ",", "mich", "auf", "bitt'\u00b7re", "Spei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wohllebens voll hab ich, den zwar nichts zwang,", "tokens": ["Wohl\u00b7le\u00b7bens", "voll", "hab", "ich", ",", "den", "zwar", "nichts", "zwang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Mir etwas Dienliches hievon verhei\u00dfen.", "tokens": ["Mir", "et\u00b7was", "Dien\u00b7li\u00b7ches", "hie\u00b7von", "ver\u00b7hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIS", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So fiel der Liebe Schlauheit, vorbedacht", "tokens": ["So", "fiel", "der", "Lie\u00b7be", "Schlau\u00b7heit", ",", "vor\u00b7be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auf \u00dcbel, das nicht war, in wahre S\u00fcnden,", "tokens": ["Auf", "\u00dc\u00b7bel", ",", "das", "nicht", "war", ",", "in", "wah\u00b7re", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKNEG", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und der Gesunde, krank durch \u00dcberfracht", "tokens": ["Und", "der", "Ge\u00b7sun\u00b7de", ",", "krank", "durch", "\u00dc\u00b7ber\u00b7fracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Des Guten, wollt' im \u00dcbel Heilung finden.", "tokens": ["Des", "Gu\u00b7ten", ",", "wollt'", "im", "\u00dc\u00b7bel", "Hei\u00b7lung", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch daran lern' ich und mir leuchtet ein:", "tokens": ["Doch", "da\u00b7ran", "lern'", "ich", "und", "mir", "leuch\u00b7tet", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wer krank um dich ward, Gift mu\u00df Arznei ihm sein.", "tokens": ["Wer", "krank", "um", "dich", "ward", ",", "Gift", "mu\u00df", "Arz\u00b7nei", "ihm", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PPER", "VAFIN", "$,", "NN", "VMFIN", "NN", "PPER", "VAINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}