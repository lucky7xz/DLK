{"textgrid.poem.49497": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Vergn\u00fcgen", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das die weite Welt bewegt,", "tokens": ["Das", "die", "wei\u00b7te", "Welt", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sich auch im W\u00fcrmgen regt;", "tokens": ["Und", "sich", "auch", "im", "W\u00fcrm\u00b7gen", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Das alleine gut und mild", "tokens": ["Das", "al\u00b7lei\u00b7ne", "gut", "und", "mild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre ganze Seele f\u00fcllt:", "tokens": ["Uns\u00b7re", "gan\u00b7ze", "See\u00b7le", "f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Vergn\u00fcgen folget nur", "tokens": ["Das", "Ver\u00b7gn\u00fc\u00b7gen", "fol\u00b7get", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sanften Trieben der Natur.", "tokens": ["Sanf\u00b7ten", "Trie\u00b7ben", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stille Lauben sind sein Haus,", "tokens": ["Stil\u00b7le", "Lau\u00b7ben", "sind", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine Pracht ein frischer Straus.", "tokens": ["Sei\u00b7ne", "Pracht", "ein", "fri\u00b7scher", "Straus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Einfalt und Bequemlichkeit", "tokens": ["Ein\u00b7falt", "und", "Be\u00b7quem\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sein gew\u00f6hnliches Geleit!", "tokens": ["Sein", "ge\u00b7w\u00f6hn\u00b7li\u00b7ches", "Ge\u00b7leit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Es erh\u00e4lt durch M\u00e4sigung", "tokens": ["Es", "er\u00b7h\u00e4lt", "durch", "M\u00e4\u00b7si\u00b7gung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Sich stets reizend, sich stets jung.", "tokens": ["Sich", "stets", "rei\u00b7zend", ",", "sich", "stets", "jung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$,", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Neben ihm liegt Cypripor", "tokens": ["Ne\u00b7ben", "ihm", "liegt", "Cyp\u00b7ri\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Gern' in Veilgen auf dem Ohr.", "tokens": ["Gern'", "in", "Veil\u00b7gen", "auf", "dem", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Keiner der es schildern will,", "tokens": ["Kei\u00b7ner", "der", "es", "schil\u00b7dern", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Trift es; dann es h\u00e4lt nicht still.", "tokens": ["Trift", "es", ";", "dann", "es", "h\u00e4lt", "nicht", "still", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Es verfolgen hei\u00dft es fliehn;", "tokens": ["Es", "ver\u00b7fol\u00b7gen", "hei\u00dft", "es", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Es empfinden, nach sich ziehn.", "tokens": ["Es", "emp\u00b7fin\u00b7den", ",", "nach", "sich", "ziehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Wenn sich oft an einem Fest", "tokens": ["Wenn", "sich", "oft", "an", "ei\u00b7nem", "Fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Weisheit von ihm fangen l\u00e4st:", "tokens": ["Weis\u00b7heit", "von", "ihm", "fan\u00b7gen", "l\u00e4st", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Dann begehrt aus seinem Schoos", "tokens": ["Dann", "be\u00b7gehrt", "aus", "sei\u00b7nem", "Schoos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Die Gefangne selbst nicht lo\u00df.", "tokens": ["Die", "Ge\u00b7fang\u00b7ne", "selbst", "nicht", "lo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Sein beliebtster Auffenthalt,", "tokens": ["Sein", "be\u00b7liebt\u00b7ster", "Auf\u00b7fent\u00b7halt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Ist der Musen Thal und Wald:", "tokens": ["Ist", "der", "Mu\u00b7sen", "Thal", "und", "Wald", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Wo er stets nach Rosen l\u00e4uft,", "tokens": ["Wo", "er", "stets", "nach", "Ro\u00b7sen", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Doch nicht stets die sch\u00f6nsten greift,", "tokens": ["Doch", "nicht", "stets", "die", "sch\u00f6ns\u00b7ten", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Weil der Knospen Neuigkeit", "tokens": ["Weil", "der", "Knos\u00b7pen", "Neu\u00b7ig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Mehr, als Sch\u00f6nheit, es erfreut.", "tokens": ["Mehr", ",", "als", "Sch\u00f6n\u00b7heit", ",", "es", "er\u00b7freut", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Manchmahl thronts, voll keuscher Lust,", "tokens": ["Manch\u00b7mahl", "thronts", ",", "voll", "keu\u00b7scher", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Auf Olympens reiner Brust;", "tokens": ["Auf", "O\u00b7lym\u00b7pens", "rei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Oder auf dem Mundrubin", "tokens": ["O\u00b7der", "auf", "dem", "Mun\u00b7dru\u00b7bin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Einer treuen Ehgattin.", "tokens": ["Ei\u00b7ner", "treu\u00b7en", "Eh\u00b7gat\u00b7tin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.33": {"text": "Freunde, wi\u00df't ihr, wo ichs fand?", "tokens": ["Freun\u00b7de", ",", "wi\u00df't", "ihr", ",", "wo", "ichs", "fand", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Wo ich es mit Blumen band? \u2013 \u2013", "tokens": ["Wo", "ich", "es", "mit", "Blu\u00b7men", "band", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Zwischen Tugend und Verstand.", "tokens": ["Zwi\u00b7schen", "Tu\u00b7gend", "und", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}