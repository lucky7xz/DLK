{"textgrid.poem.47235": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir stilles Volk in des Gebirges Kluft,", "tokens": ["Wir", "stil\u00b7les", "Volk", "in", "des", "Ge\u00b7bir\u00b7ges", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tief schlummernd in der Mutter Scho\u00df, Metalle;", "tokens": ["Tief", "schlum\u00b7mernd", "in", "der", "Mut\u00b7ter", "Scho\u00df", ",", "Me\u00b7tal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was habt ihr uns mit eurer gierigen Kralle", "tokens": ["Was", "habt", "ihr", "uns", "mit", "eu\u00b7rer", "gie\u00b7ri\u00b7gen", "Kral\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Heraufgezogen aus der dunklen Gruft?", "tokens": ["Her\u00b7auf\u00b7ge\u00b7zo\u00b7gen", "aus", "der", "dunk\u00b7len", "Gruft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df unsre Starrheit an des Himmels Luft,", "tokens": ["Da\u00df", "uns\u00b7re", "Star\u00b7rheit", "an", "des", "Him\u00b7mels", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der uns verha\u00dften, euch zum Spiel zerfalle!", "tokens": ["Der", "uns", "ver\u00b7ha\u00df\u00b7ten", ",", "euch", "zum", "Spiel", "zer\u00b7fal\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr zwinget unsre Stummheit, da\u00df sie schalle;", "tokens": ["Ihr", "zwin\u00b7get", "uns\u00b7re", "Stumm\u00b7heit", ",", "da\u00df", "sie", "schal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Schall ist Klage, die nach Heimat ruft.", "tokens": ["Der", "Schall", "ist", "Kla\u00b7ge", ",", "die", "nach", "Hei\u00b7mat", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und unsre Heimat ist die ewige Nacht;", "tokens": ["Und", "uns\u00b7re", "Hei\u00b7mat", "ist", "die", "e\u00b7wi\u00b7ge", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ihr aber habt, zu unserm Weh beflissen,", "tokens": ["Ihr", "a\u00b7ber", "habt", ",", "zu", "un\u00b7serm", "Weh", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu eurem Weh uns an das Licht gebracht.", "tokens": ["Zu", "eu\u00b7rem", "Weh", "uns", "an", "das", "Licht", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn wie ihr uns der Finsternis entrissen,", "tokens": ["Denn", "wie", "ihr", "uns", "der", "Fins\u00b7ter\u00b7nis", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So rei\u00dfen wir nun selbst mit dunkler Macht", "tokens": ["So", "rei\u00b7\u00dfen", "wir", "nun", "selbst", "mit", "dunk\u00b7ler", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Euch mit uns nieder zu den Finsternissen.", "tokens": ["Euch", "mit", "uns", "nie\u00b7der", "zu", "den", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}