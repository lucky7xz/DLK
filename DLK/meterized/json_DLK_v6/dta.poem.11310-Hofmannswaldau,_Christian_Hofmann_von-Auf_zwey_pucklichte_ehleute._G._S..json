{"dta.poem.11310": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf zwey pucklichte ehleute.  \n  G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer straft herr Pocquelin, und sein ihm gleiches kind,", "tokens": ["Hjer", "straft", "herr", "Poc\u00b7que\u00b7lin", ",", "und", "sein", "ihm", "glei\u00b7ches", "kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "KON", "PPOSAT", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieselbe alle l\u00fcgen,", "tokens": ["Die\u00b7sel\u00b7be", "al\u00b7le", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die dieser falschen meinung sind:", "tokens": ["Die", "die\u00b7ser", "fal\u00b7schen", "mei\u00b7nung", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df zwey geb\u00fcrge sich nie recht zusammen f\u00fcgen.", "tokens": ["Da\u00df", "zwey", "ge\u00b7b\u00fcr\u00b7ge", "sich", "nie", "recht", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "VVFIN", "PRF", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}