{"textgrid.poem.48070": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedanke du voll stiller Majest\u00e4t,", "tokens": ["Ge\u00b7dan\u00b7ke", "du", "voll", "stil\u00b7ler", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der mir durchs Hirn an sonnigen Tagen geht,", "tokens": ["der", "mir", "durchs", "Hirn", "an", "son\u00b7ni\u00b7gen", "Ta\u00b7gen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "wenn rings die Welt nach Frucht und Reife ringt,", "tokens": ["wenn", "rings", "die", "Welt", "nach", "Frucht", "und", "Rei\u00b7fe", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "du Lied der Sehnsucht, das in lauer Nacht,", "tokens": ["du", "Lied", "der", "Sehn\u00b7sucht", ",", "das", "in", "lau\u00b7er", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "wenn nur der Mond auf blauen Bergen wacht,", "tokens": ["wenn", "nur", "der", "Mond", "auf", "blau\u00b7en", "Ber\u00b7gen", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "das rauschende Blut in meinen Adern singt \u2013", "tokens": ["das", "rau\u00b7schen\u00b7de", "Blut", "in", "mei\u00b7nen", "A\u00b7dern", "singt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Du Lebensflut, die aus den Tiefen quillt", "tokens": ["Du", "Le\u00b7bens\u00b7flut", ",", "die", "aus", "den", "Tie\u00b7fen", "quillt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "begrabnen Seins und rastlos w\u00e4chst und schwillt", "tokens": ["be\u00b7grab\u00b7nen", "Seins", "und", "rast\u00b7los", "w\u00e4chst", "und", "schwillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und von Geschlecht sich zu Geschlecht ergie\u00dft,", "tokens": ["und", "von", "Ge\u00b7schlecht", "sich", "zu", "Ge\u00b7schlecht", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "verborgener Stern im tiefsten Weltenraum,", "tokens": ["ver\u00b7bor\u00b7ge\u00b7ner", "Stern", "im", "tiefs\u00b7ten", "Wel\u00b7ten\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "der schlummernd seine Strahlen keusch verschlie\u00dft, \u2013", "tokens": ["der", "schlum\u00b7mernd", "sei\u00b7ne", "Strah\u00b7len", "keusch", "ver\u00b7schlie\u00dft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "ADJD", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "du meiner Liebe rosiger Knospentraum:", "tokens": ["du", "mei\u00b7ner", "Lie\u00b7be", "ro\u00b7si\u00b7ger", "Knos\u00b7pen\u00b7traum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "die dieses Fr\u00fchlings holde Wunder schafft,", "tokens": ["die", "die\u00b7ses", "Fr\u00fch\u00b7lings", "hol\u00b7de", "Wun\u00b7der", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die, Purpurblut, in schwellender Traube sch\u00e4umt,", "tokens": ["die", ",", "Pur\u00b7pur\u00b7blut", ",", "in", "schwel\u00b7len\u00b7der", "Trau\u00b7be", "sch\u00e4umt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die im begrenzten Raum Unendlichkeiten tr\u00e4umt,", "tokens": ["die", "im", "be\u00b7grenz\u00b7ten", "Raum", "Un\u00b7end\u00b7lich\u00b7kei\u00b7ten", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "ich gl\u00fche nach dir, wie Fr\u00fchrot nach dem Tag!", "tokens": ["ich", "gl\u00fc\u00b7he", "nach", "dir", ",", "wie", "Fr\u00fch\u00b7rot", "nach", "dem", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Aufjauchzend steh ich vor der Zukunft Tor", "tokens": ["Auf\u00b7jauch\u00b7zend", "steh", "ich", "vor", "der", "Zu\u00b7kunft", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und klopfe an mit starkem Herzensschlag:", "tokens": ["und", "klop\u00b7fe", "an", "mit", "star\u00b7kem", "Her\u00b7zens\u00b7schlag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die schweren Marmorfl\u00fcgel drehn sich schon", "tokens": ["die", "schwe\u00b7ren", "Mar\u00b7mor\u00b7fl\u00fc\u00b7gel", "drehn", "sich", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "CARD", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und klaffen weit \u2013 \u2013", "tokens": ["und", "klaf\u00b7fen", "weit", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Auf beiden H\u00e4nden heb ich ", "tokens": ["Auf", "bei\u00b7den", "H\u00e4n\u00b7den", "heb", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "hebe dich zu des Geisterk\u00f6nigs Tron,", "tokens": ["he\u00b7be", "dich", "zu", "des", "Geis\u00b7ter\u00b7k\u00f6\u00b7nigs", "Tron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "da\u00df er mit Feuer deine Stirne weiht,", "tokens": ["da\u00df", "er", "mit", "Feu\u00b7er", "dei\u00b7ne", "Stir\u00b7ne", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "du meine Sehnsucht, meine ", "tokens": ["du", "mei\u00b7ne", "Sehn\u00b7sucht", ",", "mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}