{"textgrid.poem.63516": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Vorfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcrme brausten \u00fcber Nacht,", "tokens": ["St\u00fcr\u00b7me", "braus\u00b7ten", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die kahlen Wipfel troffen.", "tokens": ["Und", "die", "kah\u00b7len", "Wip\u00b7fel", "trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fche war mein Herz erwacht,", "tokens": ["Fr\u00fc\u00b7he", "war", "mein", "Herz", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcchtern zwischen Furcht und Hoffen.", "tokens": ["Sch\u00fcch\u00b7tern", "zwi\u00b7schen", "Furcht", "und", "Hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Horch, ein trautgeschw\u00e4tz'ger Ton", "tokens": ["Horch", ",", "ein", "traut\u00b7ge\u00b7schw\u00e4tz'\u00b7ger", "Ton"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dringt zu mir vom Wald hernieder.", "tokens": ["Dringt", "zu", "mir", "vom", "Wald", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nisten in den Zweigen schon", "tokens": ["Nis\u00b7ten", "in", "den", "Zwei\u00b7gen", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die geliebten Amseln wieder?", "tokens": ["Die", "ge\u00b7lieb\u00b7ten", "Am\u00b7seln", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dort am Weg der wei\u00dfe Streif \u2013", "tokens": ["Dort", "am", "Weg", "der", "wei\u00b7\u00dfe", "Streif", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zweifelnd frag' ich mein Gem\u00fcte:", "tokens": ["Zwei\u00b7felnd", "frag'", "ich", "mein", "Ge\u00b7m\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist's ein sp\u00e4ter Winterreif,", "tokens": ["Ist's", "ein", "sp\u00e4\u00b7ter", "Win\u00b7ter\u00b7reif", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder erste Schlehenbl\u00fcte?", "tokens": ["O\u00b7der", "ers\u00b7te", "Schle\u00b7hen\u00b7bl\u00fc\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}