{"textgrid.poem.67900": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "26. Das Lied der Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoffnung, Hoffnung, immer gr\u00fcn!", "tokens": ["Hoff\u00b7nung", ",", "Hoff\u00b7nung", ",", "im\u00b7mer", "gr\u00fcn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn dem Armen alles fehlet,", "tokens": ["Wenn", "dem", "Ar\u00b7men", "al\u00b7les", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles weicht, ihn alles qu\u00e4let,", "tokens": ["Al\u00b7les", "weicht", ",", "ihn", "al\u00b7les", "qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du, o Hoffnung, labest ihn.", "tokens": ["Du", ",", "o", "Hoff\u00b7nung", ",", "la\u00b7best", "ihn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles mag das Gl\u00fcck uns rauben,", "tokens": ["Al\u00b7les", "mag", "das", "Gl\u00fcck", "uns", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freunde, Freuden, W\u00fcrde, Gut;", "tokens": ["Freun\u00b7de", ",", "Freu\u00b7den", ",", "W\u00fcr\u00b7de", ",", "Gut", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VAFIN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur umsonst ist Gl\u00fcckes Schnauben", "tokens": ["Nur", "um\u00b7sonst", "ist", "Gl\u00fc\u00b7ckes", "Schnau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn uns Hoffnung g\u00fctlich thut.", "tokens": ["Wenn", "uns", "Hoff\u00b7nung", "g\u00fct\u00b7lich", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoffnung, Hoffnung, immer gr\u00fcn!", "tokens": ["Hoff\u00b7nung", ",", "Hoff\u00b7nung", ",", "im\u00b7mer", "gr\u00fcn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn dem Armen alles fehlet,", "tokens": ["Wenn", "dem", "Ar\u00b7men", "al\u00b7les", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles weicht, ihn alles qu\u00e4let,", "tokens": ["Al\u00b7les", "weicht", ",", "ihn", "al\u00b7les", "qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Du, o Hoffnung, tr\u00f6stest ihn.", "tokens": ["Du", ",", "o", "Hoff\u00b7nung", ",", "tr\u00f6s\u00b7test", "ihn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die Meereswogen br\u00fcllen,", "tokens": ["Wenn", "die", "Mee\u00b7res\u00b7wo\u00b7gen", "br\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singet der Sirenen Schaar;", "tokens": ["Sin\u00b7get", "der", "Si\u00b7re\u00b7nen", "Schaar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoffnung kann die Fluthen stillen,", "tokens": ["Hoff\u00b7nung", "kann", "die", "Flut\u00b7hen", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchrt den Schiffer durch Gefahr.", "tokens": ["F\u00fchrt", "den", "Schif\u00b7fer", "durch", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoffnung, Hoffnung u.s.w.", "tokens": ["Hoff\u00b7nung", ",", "Hoff\u00b7nung", "u.", "s.", "w."], "token_info": ["word", "punct", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "$,", "NN", "APPR", "VVIMP", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Du, o Hoffnung, leitest ihn.", "tokens": ["Du", ",", "o", "Hoff\u00b7nung", ",", "lei\u00b7test", "ihn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dir, o s\u00fcsse Hoffnung, s\u00e4et", "tokens": ["Dir", ",", "o", "s\u00fcs\u00b7se", "Hoff\u00b7nung", ",", "s\u00e4et"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Froh der Landmann seine Saat:", "tokens": ["Froh", "der", "Land\u00b7mann", "sei\u00b7ne", "Saat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trauet dir und fr\u00f6lich m\u00e4het", "tokens": ["Trau\u00b7et", "dir", "und", "fr\u00f6\u00b7lich", "m\u00e4\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was er dir vertrauet hat.", "tokens": ["Was", "er", "dir", "ver\u00b7trau\u00b7et", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoffnung, Hoffnung u.f.", "tokens": ["Hoff\u00b7nung", ",", "Hoff\u00b7nung", "u.", "f."], "token_info": ["word", "punct", "word", "abbreviation", "abbreviation"], "pos": ["NN", "$,", "NN", "APPR", "NE"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Jener, der das Reich verlohren,", "tokens": ["Je\u00b7ner", ",", "der", "das", "Reich", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser in den Fesseln hier,", "tokens": ["Die\u00b7ser", "in", "den", "Fes\u00b7seln", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, zum Sklaven nur gebohren,", "tokens": ["Der", ",", "zum", "Skla\u00b7ven", "nur", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle, Alle singen dir:", "tokens": ["Al\u00b7le", ",", "Al\u00b7le", "sin\u00b7gen", "dir", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoffnung, Hoffnung u.f.", "tokens": ["Hoff\u00b7nung", ",", "Hoff\u00b7nung", "u.", "f."], "token_info": ["word", "punct", "word", "abbreviation", "abbreviation"], "pos": ["NN", "$,", "NN", "APPR", "NE"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Ist des Lebens Baum verdorret,", "tokens": ["Ist", "des", "Le\u00b7bens", "Baum", "ver\u00b7dor\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will die letzte Bl\u00fcthe fliehn!", "tokens": ["Will", "die", "letz\u00b7te", "Bl\u00fc\u00b7the", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trittst du, Tr\u00f6sterin, zum Kranken,", "tokens": ["Trittst", "du", ",", "Tr\u00f6s\u00b7te\u00b7rin", ",", "zum", "Kran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigst ihm noch die Wurzel gr\u00fcn.", "tokens": ["Zeigst", "ihm", "noch", "die", "Wur\u00b7zel", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoffnung [, Hoffnung] u.f.", "tokens": ["Hoff\u00b7nung", ",", "Hoff\u00b7nung", "u.", "f."], "token_info": ["word", "punct", "punct", "word", "punct", "abbreviation", "abbreviation"], "pos": ["NN", "$(", "$,", "NN", "$(", "APPR", "NE"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "In Verzweiflung, im Gefechte,", "tokens": ["In", "Ver\u00b7zwei\u00b7flung", ",", "im", "Ge\u00b7fech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn schon alles weicht und f\u00e4llt;", "tokens": ["Wenn", "schon", "al\u00b7les", "weicht", "und", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehst du an des Edlen Rechte,", "tokens": ["Stehst", "du", "an", "des", "Ed\u00b7len", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Winkest ihm in andre Welt.", "tokens": ["Win\u00b7kest", "ihm", "in", "and\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoffnung, Hoffnung u.f.", "tokens": ["Hoff\u00b7nung", ",", "Hoff\u00b7nung", "u.", "f."], "token_info": ["word", "punct", "word", "abbreviation", "abbreviation"], "pos": ["NN", "$,", "NN", "APPR", "NE"], "meter": "+-+-", "measure": "trochaic.di"}}}}}