{"textgrid.poem.57692": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Schwefelgelbe Blitze ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwefelgelbe Blitze fahren", "tokens": ["Schwe\u00b7fel\u00b7gel\u00b7be", "Blit\u00b7ze", "fah\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die blaue Sommernacht,", "tokens": ["Durch", "die", "blau\u00b7e", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Knirschend sich die Buchen biegen", "tokens": ["Knir\u00b7schend", "sich", "die", "Bu\u00b7chen", "bie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PRF", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es st\u00f6hnt und pfeift und kracht.", "tokens": ["Und", "es", "st\u00f6hnt", "und", "pfeift", "und", "kracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und es kracht und knirscht und donnert", "tokens": ["Und", "es", "kracht", "und", "knirscht", "und", "don\u00b7nert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die st\u00e4rkste Buche liegt", "tokens": ["Und", "die", "st\u00e4rks\u00b7te", "Bu\u00b7che", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Wurzeln ausgerissen", "tokens": ["Mit", "den", "Wur\u00b7zeln", "aus\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor mir wettersturmbesiegt.", "tokens": ["Vor", "mir", "wet\u00b7ter\u00b7sturm\u00b7be\u00b7siegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ein Wetter war die Stunde,", "tokens": ["Wie", "ein", "Wet\u00b7ter", "war", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mich brausend \u00fcberwand,", "tokens": ["Die", "mich", "brau\u00b7send", "\u00fc\u00b7ber\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ich ohne Kraft und Willen", "tokens": ["Als", "ich", "oh\u00b7ne", "Kraft", "und", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich zu deinen F\u00fc\u00dfen fand.", "tokens": ["Mich", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}