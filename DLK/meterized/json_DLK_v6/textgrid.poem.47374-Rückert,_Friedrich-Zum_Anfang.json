{"textgrid.poem.47374": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Anfang", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mache deinem Meister Ehre, o Geselle, baue recht!", "tokens": ["Ma\u00b7che", "dei\u00b7nem", "Meis\u00b7ter", "Eh\u00b7re", ",", "o", "Ge\u00b7sel\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,", "FM", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie das Ma\u00df er hat genommen, nimm die Kelle, baue recht!", "tokens": ["Wie", "das", "Ma\u00df", "er", "hat", "ge\u00b7nom\u00b7men", ",", "nimm", "die", "Kel\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VAFIN", "VVPP", "$,", "VVIMP", "ART", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nicht um deine Mitgesellen sorge, wie sie m\u00f6gen baun;", "tokens": ["Nicht", "um", "dei\u00b7ne", "Mit\u00b7ge\u00b7sel\u00b7len", "sor\u00b7ge", ",", "wie", "sie", "m\u00f6\u00b7gen", "baun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Daf\u00fcr la\u00df den Meister sorgen, deine Stelle baue recht!", "tokens": ["Da\u00b7f\u00fcr", "la\u00df", "den", "Meis\u00b7ter", "sor\u00b7gen", ",", "dei\u00b7ne", "Stel\u00b7le", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Frage nicht, was m\u00fchsam heute deine Hand gef\u00fcgt, wie bald", "tokens": ["Fra\u00b7ge", "nicht", ",", "was", "m\u00fch\u00b7sam", "heu\u00b7te", "dei\u00b7ne", "Hand", "ge\u00b7f\u00fcgt", ",", "wie", "bald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "PRELS", "ADJD", "ADV", "PPOSAT", "NN", "VVPP", "$,", "PWAV", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wohl im Sturm der Zeiten wieder es zerschelle, baue recht!", "tokens": ["Wohl", "im", "Sturm", "der", "Zei\u00b7ten", "wie\u00b7der", "es", "zer\u00b7schel\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "La\u00df nicht deinen Unmut fragen, welch Bewohners Ungeschmack", "tokens": ["La\u00df", "nicht", "dei\u00b7nen", "Un\u00b7mut", "fra\u00b7gen", ",", "welch", "Be\u00b7woh\u00b7ners", "Un\u00b7ge\u00b7schmack"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,", "PWAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "K\u00fcnftig die von dir gebaute Wand entstelle, baue recht!", "tokens": ["K\u00fcnf\u00b7tig", "die", "von", "dir", "ge\u00b7bau\u00b7te", "Wand", "ent\u00b7stel\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "APPR", "PPER", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "G\u00e4rtner, dem der Grund zum M\u00f6rtel und zur Kell' ein Spaten dient,", "tokens": ["G\u00e4rt\u00b7ner", ",", "dem", "der", "Grund", "zum", "M\u00f6r\u00b7tel", "und", "zur", "Kell'", "ein", "Spa\u00b7ten", "dient", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "R\u00fchr' dich, und den Bau der Erde treu bestelle, baue recht!", "tokens": ["R\u00fchr'", "dich", ",", "und", "den", "Bau", "der", "Er\u00b7de", "treu", "be\u00b7stel\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KON", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Bau' die Formen der Gew\u00e4chse, gr\u00fcnde Pflanzen und vertilg'", "tokens": ["Bau'", "die", "For\u00b7men", "der", "Ge\u00b7w\u00e4ch\u00b7se", ",", "gr\u00fcn\u00b7de", "Pflan\u00b7zen", "und", "ver\u00b7til\u00b7g'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "ADJA", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Unkraut, da\u00df in Weg dem Kraut es sich nicht stelle, baue recht!", "tokens": ["Un\u00b7kraut", ",", "da\u00df", "in", "Weg", "dem", "Kraut", "es", "sich", "nicht", "stel\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPR", "NN", "ART", "NN", "PPER", "PRF", "PTKNEG", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Ordne deine bl\u00fch'nden Staaten, freu' dich der Bev\u00f6lkerung.", "tokens": ["Ord\u00b7ne", "dei\u00b7ne", "bl\u00fch'n\u00b7den", "Staa\u00b7ten", ",", "freu'", "dich", "der", "Be\u00b7v\u00f6l\u00b7ke\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Beet' und Pfad' und auch die Leitung jedem Quelle baue recht!", "tokens": ["Beet'", "und", "Pfad'", "und", "auch", "die", "Lei\u00b7tung", "je\u00b7dem", "Quel\u00b7le", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "KON", "ADV", "ART", "NN", "PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Fischer, dem das Meer zum Acker und zum Pflug ein Nachen dient,", "tokens": ["Fi\u00b7scher", ",", "dem", "das", "Meer", "zum", "A\u00b7cker", "und", "zum", "Pflug", "ein", "Na\u00b7chen", "dient", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Furche tief das Beet der Fluten, deine Welle baue recht!", "tokens": ["Fur\u00b7che", "tief", "das", "Beet", "der", "Flu\u00b7ten", ",", "dei\u00b7ne", "Wel\u00b7le", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Fleug, Weltteile zu verkn\u00fcpfen, Schiff, und la\u00df den Handel bl\u00fchn!", "tokens": ["Fleug", ",", "Welt\u00b7tei\u00b7le", "zu", "ver\u00b7kn\u00fcp\u00b7fen", ",", "Schiff", ",", "und", "la\u00df", "den", "Han\u00b7del", "bl\u00fchn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PTKZU", "VVINF", "$,", "NN", "$,", "KON", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Handel, deine Mess' und Bude, Wag' und Elle, baue recht!", "tokens": ["Han\u00b7del", ",", "dei\u00b7ne", "Mess'", "und", "Bu\u00b7de", ",", "Wag'", "und", "El\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "La\u00df vom Recht und von der Liebe, K\u00f6nig, dir den Thronsaal baun!", "tokens": ["La\u00df", "vom", "Recht", "und", "von", "der", "Lie\u00b7be", ",", "K\u00f6\u00b7nig", ",", "dir", "den", "Thron\u00b7sa\u00b7al", "baun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$,", "NN", "$,", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.20": {"text": "Bau' den Giebel frei und luftig, und die Schwelle baue recht!", "tokens": ["Bau'", "den", "Gie\u00b7bel", "frei", "und", "luf\u00b7tig", ",", "und", "die", "Schwel\u00b7le", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "KON", "ADJD", "$,", "KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Wenn die Eintracht H\u00e4user bauet, die die Zwietracht niederrei\u00dft:", "tokens": ["Wenn", "die", "Ein\u00b7tracht", "H\u00e4u\u00b7ser", "bau\u00b7et", ",", "die", "die", "Zwiet\u00b7racht", "nie\u00b7der\u00b7rei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Eintracht, komm, nimm unsrer Zwietracht Tr\u00fcmmerf\u00e4lle, baue recht!", "tokens": ["Ein\u00b7tracht", ",", "komm", ",", "nimm", "uns\u00b7rer", "Zwiet\u00b7racht", "Tr\u00fcm\u00b7mer\u00b7f\u00e4l\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVIMP", "PPOSAT", "NN", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Kleinlich ist der Staaten Fachwerk vor dem ew'gen Bau der Welt:", "tokens": ["Klein\u00b7lich", "ist", "der", "Staa\u00b7ten", "Fach\u00b7werk", "vor", "dem", "ew'\u00b7gen", "Bau", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Komm, Weltweisheit, Weltengeistes Baugeselle, baue recht!", "tokens": ["Komm", ",", "Welt\u00b7weis\u00b7heit", ",", "Wel\u00b7ten\u00b7geis\u00b7tes", "Bau\u00b7ge\u00b7sel\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.25": {"text": "Die Vergangenheit der Sch\u00f6pfung bau' uns aus den Tr\u00fcmmern auf,", "tokens": ["Die", "Ver\u00b7gan\u00b7gen\u00b7heit", "der", "Sch\u00f6p\u00b7fung", "bau'", "uns", "aus", "den", "Tr\u00fcm\u00b7mern", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Und die Zukunft der Geschichte baue helle, baue recht!", "tokens": ["Und", "die", "Zu\u00b7kunft", "der", "Ge\u00b7schich\u00b7te", "bau\u00b7e", "hel\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "ADJA", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.27": {"text": "L\u00f6se du die Sprachverwirrung, die den Bau ins Stocken bringt;", "tokens": ["L\u00f6\u00b7se", "du", "die", "Sprach\u00b7ver\u00b7wir\u00b7rung", ",", "die", "den", "Bau", "ins", "Sto\u00b7cken", "bringt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Da\u00df Idee den Plan des Meisters her uns stelle, baue recht!", "tokens": ["Da\u00df", "I\u00b7dee", "den", "Plan", "des", "Meis\u00b7ters", "her", "uns", "stel\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ART", "NN", "APZR", "PPER", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.29": {"text": "Sichre, stille, ungest\u00f6rte Architektin, o Natur,", "tokens": ["Sich\u00b7re", ",", "stil\u00b7le", ",", "un\u00b7ge\u00b7st\u00f6r\u00b7te", "Ar\u00b7chi\u00b7tek\u00b7tin", ",", "o", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Baue fort nach unbewu\u00dftem Kunstmodelle, baue recht!", "tokens": ["Bau\u00b7e", "fort", "nach", "un\u00b7be\u00b7wu\u00df\u00b7tem", "Kunst\u00b7mo\u00b7del\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.31": {"text": "Bau' die stolzgew\u00f6lbte Kuppel deines Saals, o Himmel, wo", "tokens": ["Bau'", "die", "stolz\u00b7ge\u00b7w\u00f6lb\u00b7te", "Kup\u00b7pel", "dei\u00b7nes", "Saals", ",", "o", "Him\u00b7mel", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "PWAV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "Mit Musik sich ewig drehen Sph\u00e4renb\u00e4lle, baue recht!", "tokens": ["Mit", "Mu\u00b7sik", "sich", "e\u00b7wig", "dre\u00b7hen", "Sph\u00e4\u00b7ren\u00b7b\u00e4l\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ADJD", "ADJA", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.33": {"text": "Sonnenbahnen und Milchstra\u00dfen, der Planeten Wohnungen,", "tokens": ["Son\u00b7nen\u00b7bah\u00b7nen", "und", "Milch\u00b7stra\u00b7\u00dfen", ",", "der", "Pla\u00b7ne\u00b7ten", "Woh\u00b7nun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+--++--+---+-", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Die vier H\u00e4user f\u00fcr des Mondes Wechselschnelle, baue recht!", "tokens": ["Die", "vier", "H\u00e4u\u00b7ser", "f\u00fcr", "des", "Mon\u00b7des", "Wech\u00b7sel\u00b7schnel\u00b7le", ",", "bau\u00b7e", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.35": {"text": "Baue die Korallenriffe und die stille Muschelbank,", "tokens": ["Bau\u00b7e", "die", "Ko\u00b7ral\u00b7len\u00b7rif\u00b7fe", "und", "die", "stil\u00b7le", "Mu\u00b7schel\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.36": {"text": "Heil'ges Meer, und der kristallnen Grotten Helle baue recht!", "tokens": ["Heil'\u00b7ges", "Meer", ",", "und", "der", "kris\u00b7tall\u00b7nen", "Grot\u00b7