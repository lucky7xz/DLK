{"textgrid.poem.53551": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das K\u00f6nigswort", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies erg\u00f6tzte hoch und niedrig:", "tokens": ["Dies", "er\u00b7g\u00f6tz\u00b7te", "hoch", "und", "nied\u00b7rig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als der edle K\u00f6nig Friedrich,", "tokens": ["Als", "der", "ed\u00b7le", "K\u00f6\u00b7nig", "Fried\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "August weiland von ganz Sachsen,", "tokens": ["Au\u00b7gust", "wei\u00b7land", "von", "ganz", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "tat zum Hals herau\u00dfer wachsen", "tokens": ["tat", "zum", "Hals", "her\u00b7au\u00b7\u00dfer", "wach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "seinem Volk, das ihn geliebt,", "tokens": ["sei\u00b7nem", "Volk", ",", "das", "ihn", "ge\u00b7liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "so es billigen Rotwein gibt \u2013", "tokens": ["so", "es", "bil\u00b7li\u00b7gen", "Rot\u00b7wein", "gibt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "NN", "VVFIN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "als der K\u00f6nig, sag ich, merkte,", "tokens": ["als", "der", "K\u00f6\u00b7nig", ",", "sag", "ich", ",", "merk\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "wie der innre Feind sich st\u00e4rkte,", "tokens": ["wie", "der", "inn\u00b7re", "Feind", "sich", "st\u00e4rk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "blickt er \u00fcber die Heiducken,", "tokens": ["blickt", "er", "\u00fc\u00b7ber", "die", "Hei\u00b7du\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "und man h\u00f6rt ihn leise schlucken..", "tokens": ["und", "man", "h\u00f6rt", "ihn", "lei\u00b7se", "schlu\u00b7cken", ".."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und er murmelt durch die Z\u00e4hne:", "tokens": ["Und", "er", "mur\u00b7melt", "durch", "die", "Z\u00e4h\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "\u00bbmacht euch euern Dreck alleene!\u00ab", "tokens": ["\u00bb", "macht", "euch", "eu\u00b7ern", "Dreck", "al\u00b7lee\u00b7ne", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welch ein K\u00f6nigswort! Wahrhaftig,", "tokens": ["Welch", "ein", "K\u00f6\u00b7nigs\u00b7wort", "!", "Wahr\u00b7haf\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "so wie er \u2013 so voll und saftig", "tokens": ["so", "wie", "er", "\u2013", "so", "voll", "und", "saf\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "$(", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ist sonst keiner weggegangen.", "tokens": ["ist", "sonst", "kei\u00b7ner", "weg\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn doch heute in der langen", "tokens": ["Wenn", "doch", "heu\u00b7te", "in", "der", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "langen Reihe unsrer Kleber,", "tokens": ["lan\u00b7gen", "Rei\u00b7he", "uns\u00b7rer", "Kle\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wichtigmacher, \u00c4mterstreber,", "tokens": ["Wich\u00b7tig\u00b7ma\u00b7cher", ",", "\u00c4m\u00b7ter\u00b7stre\u00b7ber", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "einer in der langen Kette", "tokens": ["ei\u00b7ner", "in", "der", "lan\u00b7gen", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "nur so viel Courage h\u00e4tte,", "tokens": ["nur", "so", "viel", "Cou\u00b7ra\u00b7ge", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "trotz der Ehre und Moneten", "tokens": ["trotz", "der", "Eh\u00b7re", "und", "Mo\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.10": {"text": "schnell geb\u00fchrend abzutreten!", "tokens": ["schnell", "ge\u00b7b\u00fch\u00b7rend", "ab\u00b7zu\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "O, wie ich sein Wort ersehne:", "tokens": ["O", ",", "wie", "ich", "sein", "Wort", "er\u00b7seh\u00b7ne", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "\u00bbmacht euch euern Dreck alleene!\u00ab", "tokens": ["\u00bb", "macht", "euch", "eu\u00b7ern", "Dreck", "al\u00b7lee\u00b7ne", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Edler K\u00f6nig! Du warst weise!", "tokens": ["Ed\u00b7ler", "K\u00f6\u00b7nig", "!", "Du", "warst", "wei\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du verschwandest still und leise", "tokens": ["Du", "ver\u00b7schwan\u00b7dest", "still", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "in das nahrhafte Zivil.", "tokens": ["in", "das", "nahr\u00b7haf\u00b7te", "Zi\u00b7vil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das hat Charme, und das hat Stil.", "tokens": ["Das", "hat", "Char\u00b7me", ",", "und", "das", "hat", "Stil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "KON", "PDS", "VAFIN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Aber, aber unsereiner!", "tokens": ["A\u00b7ber", ",", "a\u00b7ber", "un\u00b7ser\u00b7ei\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieh, uns pensioniert ja keiner!", "tokens": ["Sieh", ",", "uns", "pen\u00b7si\u00b7o\u00b7niert", "ja", "kei\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "PIS", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und wir treten mit Gef\u00fchle", "tokens": ["Und", "wir", "tre\u00b7ten", "mit", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tag f\u00fcr Tag die Tretem\u00fchle.", "tokens": ["Tag", "f\u00fcr", "Tag", "die", "Tre\u00b7te\u00b7m\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ach, wie gern, in filzenen Schuhen", "tokens": ["Ach", ",", "wie", "gern", ",", "in", "fil\u00b7ze\u00b7nen", "Schu\u00b7hen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "wollten wir gem\u00e4chlich ruhen,", "tokens": ["woll\u00b7ten", "wir", "ge\u00b7m\u00e4ch\u00b7lich", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "sprechend: \u00bbIn exilio bene!", "tokens": ["spre\u00b7chend", ":", "\u00bb", "In", "e\u00b7xi\u00b7lio", "be\u00b7ne", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "$(", "APPR", "NE", "NE", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.12": {"text": "Macht euch euern Dreck alleene!\u00ab", "tokens": ["Macht", "euch", "eu\u00b7ern", "Dreck", "al\u00b7lee\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ADJA", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}