{"textgrid.poem.61008": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als ich aufstand fr\u00fch am Tag", "tokens": ["Und", "als", "ich", "auf\u00b7stand", "fr\u00fch", "am", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meinte, da\u00df es noch Winter sei,", "tokens": ["Und", "mein\u00b7te", ",", "da\u00df", "es", "noch", "Win\u00b7ter", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da jauchzte schon mit lustigem Schlag", "tokens": ["Da", "jauchz\u00b7te", "schon", "mit", "lus\u00b7ti\u00b7gem", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Lerch' an meinem Fenster frei:", "tokens": ["Die", "Ler\u00b7ch'", "an", "mei\u00b7nem", "Fens\u00b7ter", "frei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Tirili, tirili! Vom bl\u00f6den Traum,", "tokens": ["Ti\u00b7ri\u00b7li", ",", "ti\u00b7ri\u00b7li", "!", "Vom", "bl\u00f6\u00b7den", "Traum", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "APPRART", "ADJA", "NN", "$,"], "meter": "-------+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Langschl\u00e4fer, bist du endlich erwacht?", "tokens": ["Lang\u00b7schl\u00e4\u00b7fer", ",", "bist", "du", "end\u00b7lich", "er\u00b7wacht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Du schliefst und merktest das S\u00fc\u00dfe kaum,", "tokens": ["Du", "schliefst", "und", "merk\u00b7test", "das", "S\u00fc\u00b7\u00dfe", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Denn sacht, denn sacht", "tokens": ["Denn", "sacht", ",", "denn", "sacht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ist kommen der Fr\u00fchling \u00fcber Nacht.", "tokens": ["Ist", "kom\u00b7men", "der", "Fr\u00fch\u00b7ling", "\u00fc\u00b7ber", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und als ich schaute zum Himmelsraum,", "tokens": ["Und", "als", "ich", "schau\u00b7te", "zum", "Him\u00b7mels\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da war er so blau, da war er so weit;", "tokens": ["Da", "war", "er", "so", "blau", ",", "da", "war", "er", "so", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und als ich blickt' auf Strauch und Baum,", "tokens": ["Und", "als", "ich", "blickt'", "auf", "Strauch", "und", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da trugen sie all' ein gr\u00fcnes Kleid.", "tokens": ["Da", "tru\u00b7gen", "sie", "all'", "ein", "gr\u00fc\u00b7nes", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und als ich sah in die eigene Brust,", "tokens": ["Und", "als", "ich", "sah", "in", "die", "ei\u00b7ge\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da sa\u00df die Liebe darin und sang,", "tokens": ["Da", "sa\u00df", "die", "Lie\u00b7be", "da\u00b7rin", "und", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PAV", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Was selber so s\u00fc\u00df ich nimmer gewu\u00dft;", "tokens": ["Was", "sel\u00b7ber", "so", "s\u00fc\u00df", "ich", "nim\u00b7mer", "ge\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Das klang, das klang", "tokens": ["Das", "klang", ",", "das", "klang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und soll nun klingen mein Leben lang.", "tokens": ["Und", "soll", "nun", "klin\u00b7gen", "mein", "Le\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}