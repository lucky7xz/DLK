{"textgrid.poem.33648": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kalt und schneidend", "tokens": ["Kalt", "und", "schnei\u00b7dend"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Weht der Wind,", "tokens": ["Weht", "der", "Wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und mein Herz ist bang und leidend", "tokens": ["Und", "mein", "Herz", "ist", "bang", "und", "lei\u00b7dend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinetwegen, sch\u00f6nes Kind!", "tokens": ["Dei\u00b7net\u00b7we\u00b7gen", ",", "sch\u00f6\u00b7nes", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deinetwegen,", "tokens": ["Dei\u00b7net\u00b7we\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "S\u00fc\u00dfe Macht,", "tokens": ["S\u00fc\u00b7\u00dfe", "Macht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ist mein Tagwerk ohne Segen,", "tokens": ["Ist", "mein", "Tag\u00b7werk", "oh\u00b7ne", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ist schlaflos meine Nacht.", "tokens": ["Und", "ist", "schlaf\u00b7los", "mei\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "St\u00fcrme tosen", "tokens": ["St\u00fcr\u00b7me", "to\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Winterlich,", "tokens": ["Win\u00b7ter\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Aber bl\u00fchten auch schon Rosen,", "tokens": ["A\u00b7ber", "bl\u00fch\u00b7ten", "auch", "schon", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was sind Rosen ohne dich?", "tokens": ["Was", "sind", "Ro\u00b7sen", "oh\u00b7ne", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}