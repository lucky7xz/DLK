{"dta.poem.15744": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nichts Ehrw\u00fcrd'ges kennt, mit Ehrfurcht keinen nennt,", "tokens": ["Wer", "nichts", "Ehr\u00b7w\u00fcrd'\u00b7ges", "kennt", ",", "mit", "Ehr\u00b7furcht", "kei\u00b7nen", "nennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "NN", "VVFIN", "$,", "APPR", "NN", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat keine Ehr' und bleibt von Ehren stets getrennt.", "tokens": ["Hat", "kei\u00b7ne", "Ehr'", "und", "bleibt", "von", "Eh\u00b7ren", "stets", "ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "VVFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr achtet kein Gesetz und ehret keine Sitte,", "tokens": ["Ihr", "ach\u00b7tet", "kein", "Ge\u00b7setz", "und", "eh\u00b7ret", "kei\u00b7ne", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Junges Barbarenvolk in der Gesittung Mitte.", "tokens": ["Jun\u00b7ges", "Bar\u00b7ba\u00b7ren\u00b7volk", "in", "der", "Ge\u00b7sit\u00b7tung", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+------+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "All' eure Schreiberei, wie geistreich ihr sie schm\u00fcckt,", "tokens": ["All'", "eu\u00b7re", "Schrei\u00b7be\u00b7rei", ",", "wie", "geist\u00b7reich", "ihr", "sie", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist der Barbarei Gepr\u00e4g ihr aufgedr\u00fcckt.", "tokens": ["Doch", "ist", "der", "Bar\u00b7ba\u00b7rei", "Ge\u00b7pr\u00e4g", "ihr", "auf\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weh, wenns gelingt da\u00df ihr die Welt barbarisirt;", "tokens": ["Weh", ",", "wenns", "ge\u00b7lingt", "da\u00df", "ihr", "die", "Welt", "bar\u00b7ba\u00b7ri\u00b7sirt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "VVFIN", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spott euch, wenn ihr umsonst euch habt prostituirt.", "tokens": ["Spott", "euch", ",", "wenn", "ihr", "um\u00b7sonst", "euch", "habt", "pros\u00b7ti\u00b7tu\u00b7irt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-++-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}