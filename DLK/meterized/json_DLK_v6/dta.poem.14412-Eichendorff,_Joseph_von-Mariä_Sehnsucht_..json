{"dta.poem.14412": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mari\u00e4 Sehnsucht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es ging Maria in den Morgen hinein,", "tokens": ["Es", "ging", "Ma\u00b7ria", "in", "den", "Mor\u00b7gen", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "That die Erd' einen lichten Liebesschein,", "tokens": ["That", "die", "Erd'", "ei\u00b7nen", "lich\u00b7ten", "Lie\u00b7bes\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und \u00fcber die fr\u00f6hlichen, gr\u00fcnen H\u00f6h'n,", "tokens": ["Und", "\u00fc\u00b7ber", "die", "fr\u00f6h\u00b7li\u00b7chen", ",", "gr\u00fc\u00b7nen", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sah Sie den bl\u00e4ulichen Himmel still steh'n.", "tokens": ["Sah", "Sie", "den", "bl\u00e4u\u00b7li\u00b7chen", "Him\u00b7mel", "still", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "VVFIN", "NE"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u201each, h\u00e4tt' ich ein Brautkleid von Himmelsschein,", "tokens": ["\u201e", "ach", ",", "h\u00e4tt'", "ich", "ein", "Braut\u00b7kleid", "von", "Him\u00b7mels\u00b7schein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Zwei goldene Fl\u00fcglein \u2014 wie fl\u00f6g' ich hinein!\u201c", "tokens": ["Zwei", "gol\u00b7de\u00b7ne", "Fl\u00fcg\u00b7lein", "wie", "fl\u00f6g'", "ich", "hin\u00b7ein", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "ADJA", "NN", "$(", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}