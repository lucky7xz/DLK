{"textgrid.poem.37230": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[es kam ein Lump mir in die Quer]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kam ein Lump mir in die Quer", "tokens": ["Es", "kam", "ein", "Lump", "mir", "in", "die", "Quer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hielt den alten Felbel her.", "tokens": ["Und", "hielt", "den", "al\u00b7ten", "Fel\u00b7bel", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Obschon er noch gesund und stark,", "tokens": ["Ob\u00b7schon", "er", "noch", "ge\u00b7sund", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warf ich ihm dennoch eine Mark", "tokens": ["Warf", "ich", "ihm", "den\u00b7noch", "ei\u00b7ne", "Mark"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ART", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Recht freundlich in den Hut hinein.", "tokens": ["Recht", "freund\u00b7lich", "in", "den", "Hut", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Kerl schien Philosoph zu sein.", "tokens": ["Der", "Kerl", "schien", "Phi\u00b7lo\u00b7soph", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sprach mit ernstem Bocksgesicht:", "tokens": ["Er", "sprach", "mit", "erns\u00b7tem", "Bocks\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Herr, Sie sehn, ich danke nicht.", "tokens": ["Mein", "Herr", ",", "Sie", "sehn", ",", "ich", "dan\u00b7ke", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVINF", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Danken bin ich nicht gewohnt.", "tokens": ["Das", "Dan\u00b7ken", "bin", "ich", "nicht", "ge\u00b7wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich nehme an, Sie sind gescheit", "tokens": ["Ich", "neh\u00b7me", "an", ",", "Sie", "sind", "ge\u00b7scheit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und f\u00fchlen sich genug belohnt", "tokens": ["Und", "f\u00fch\u00b7len", "sich", "ge\u00b7nug", "be\u00b7lohnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch Ihre Eitelkeit.", "tokens": ["Durch", "Ih\u00b7re", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}