{"textgrid.poem.66746": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch was ist Gl\u00fcck? Es ist nicht dein Behagen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch was ist Gl\u00fcck? Es ist nicht dein Behagen,", "tokens": ["Doch", "was", "ist", "Gl\u00fcck", "?", "Es", "ist", "nicht", "dein", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist der Quell, den du aus Stein geschlagen,", "tokens": ["Es", "ist", "der", "Quell", ",", "den", "du", "aus", "Stein", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Blitz, der feurig in die D\u00fcrre f\u00e4hrt,", "tokens": ["Der", "Blitz", ",", "der", "feu\u00b7rig", "in", "die", "D\u00fcr\u00b7re", "f\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Licht, das liebend deinen Kampf verkl\u00e4rt.", "tokens": ["Das", "Licht", ",", "das", "lie\u00b7bend", "dei\u00b7nen", "Kampf", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O feige Qual, wenn, meines Werts vergessen,", "tokens": ["O", "fei\u00b7ge", "Qual", ",", "wenn", ",", "mei\u00b7nes", "Werts", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "KOUS", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich nicht mehr w\u00e4ge, was mir zugemessen,", "tokens": ["Ich", "nicht", "mehr", "w\u00e4\u00b7ge", ",", "was", "mir", "zu\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich, verlangend nach gemeinem Lohn,", "tokens": ["Wenn", "ich", ",", "ver\u00b7lan\u00b7gend", "nach", "ge\u00b7mei\u00b7nem", "Lohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Marktgut tausche meinen K\u00f6nigsthron!", "tokens": ["Mit", "Markt\u00b7gut", "tau\u00b7sche", "mei\u00b7nen", "K\u00f6\u00b7nigs\u00b7thron", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom Seelengrunde f\u00fchl ich's grollend steigen,", "tokens": ["Vom", "See\u00b7len\u00b7grun\u00b7de", "f\u00fchl", "ich's", "grol\u00b7lend", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es l\u00e4\u00dft nicht Ruh mir, wenn die Stunden schweigen,", "tokens": ["Es", "l\u00e4\u00dft", "nicht", "Ruh", "mir", ",", "wenn", "die", "Stun\u00b7den", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "PPER", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nachts aus dem Schlummer schreckt es mich empor:", "tokens": ["Nachts", "aus", "dem", "Schlum\u00b7mer", "schreckt", "es", "mich", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "\u00bbdu bist ein Bettler, der ein Reich verlor!\u00ab", "tokens": ["\u00bb", "du", "bist", "ein", "Bett\u00b7ler", ",", "der", "ein", "Reich", "ver\u00b7lor", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}