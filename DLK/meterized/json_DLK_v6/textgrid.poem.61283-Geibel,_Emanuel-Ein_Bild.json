{"textgrid.poem.61283": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ein Bild", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leichtsinnig, redlich, Mann und Kind zugleich,", "tokens": ["Leicht\u00b7sin\u00b7nig", ",", "red\u00b7lich", ",", "Mann", "und", "Kind", "zu\u00b7gleich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll \u00dcbermut und Demut, starr und weich,", "tokens": ["Voll", "\u00dc\u00b7ber\u00b7mut", "und", "De\u00b7mut", ",", "starr", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Sinnen wild und stets damit im Streit,", "tokens": ["Von", "Sin\u00b7nen", "wild", "und", "stets", "da\u00b7mit", "im", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADV", "PAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verfolgt von Lieb' und doch in Liebesleid,", "tokens": ["Ver\u00b7folgt", "von", "Lieb'", "und", "doch", "in", "Lie\u00b7be\u00b7sleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Wandervogel voll Begehr nach Ruh',", "tokens": ["Ein", "Wan\u00b7der\u00b7vo\u00b7gel", "voll", "Be\u00b7gehr", "nach", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Weltkind, das sich sehnt dem Himmel zu -", "tokens": ["Ein", "Welt\u00b7kind", ",", "das", "sich", "sehnt", "dem", "Him\u00b7mel", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "ART", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O Bild des Widerspruchs, wann kommt der Tag,", "tokens": ["O", "Bild", "des", "Wi\u00b7der\u00b7spruchs", ",", "wann", "kommt", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der allen deinen Zwiespalt s\u00fchnen mag!", "tokens": ["Der", "al\u00b7len", "dei\u00b7nen", "Zwies\u00b7palt", "s\u00fch\u00b7nen", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}