{"textgrid.poem.24833": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Herbstgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wenn im frost'gen Luftzug t\u00f6dlich", "tokens": ["Wie", "wenn", "im", "frost'\u00b7gen", "Luft\u00b7zug", "t\u00f6d\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPRART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Sommers letzte Bl\u00fcte krankt,", "tokens": ["Des", "Som\u00b7mers", "letz\u00b7te", "Bl\u00fc\u00b7te", "krankt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hie und da nur, gelb und r\u00f6tlich,", "tokens": ["Und", "hie", "und", "da", "nur", ",", "gelb", "und", "r\u00f6t\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein einzles Blatt im Windhauch schwankt:", "tokens": ["Ein", "einz\u00b7les", "Blatt", "im", "Wind\u00b7hauch", "schwankt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So schauert \u00fcber meinem Leben", "tokens": ["So", "schau\u00b7ert", "\u00fc\u00b7ber", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein n\u00e4chtlich tr\u00fcber, kalter Tag;", "tokens": ["Ein", "n\u00e4cht\u00b7lich", "tr\u00fc\u00b7ber", ",", "kal\u00b7ter", "Tag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum noch vor dem Tode beben,", "tokens": ["Wa\u00b7rum", "noch", "vor", "dem", "To\u00b7de", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Herz, mit deinem ew'gen Schlag?", "tokens": ["O", "Herz", ",", "mit", "dei\u00b7nem", "ew'\u00b7gen", "Schlag", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh rings entbl\u00e4ttert das Gest\u00e4ude!", "tokens": ["Sieh", "rings", "ent\u00b7bl\u00e4t\u00b7tert", "das", "Ge\u00b7st\u00e4u\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was spielst du, wie der Wind am Strauch,", "tokens": ["Was", "spielst", "du", ",", "wie", "der", "Wind", "am", "Strauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch mit der letzten, welken Freude?", "tokens": ["Noch", "mit", "der", "letz\u00b7ten", ",", "wel\u00b7ken", "Freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gieb dich zur Ruh! Bald stirbt sie auch.", "tokens": ["Gieb", "dich", "zur", "Ruh", "!", "Bald", "stirbt", "sie", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}