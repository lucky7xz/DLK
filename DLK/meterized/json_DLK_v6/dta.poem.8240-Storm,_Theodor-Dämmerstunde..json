{"dta.poem.8240": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "D\u00e4mmerstunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Die Abendsonne fiel durch die Gardinen,", "tokens": ["Die", "A\u00b7bend\u00b7son\u00b7ne", "fiel", "durch", "die", "Gar\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die flei\u00dfigen H\u00e4nde f\u00fcgten sich der Ruh,", "tokens": ["Die", "flei\u00b7\u00dfi\u00b7gen", "H\u00e4n\u00b7de", "f\u00fcg\u00b7ten", "sich", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von rothem Licht war deine Stirn beschienen.", "tokens": ["Von", "ro\u00b7them", "Licht", "war", "dei\u00b7ne", "Stirn", "be\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir schwiegen beid'; ich wu\u00dfte mir kein Wort,", "tokens": ["Wir", "schwie\u00b7gen", "beid'", ";", "ich", "wu\u00df\u00b7te", "mir", "kein", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das in der Stunde Zauber mochte taugen;", "tokens": ["Das", "in", "der", "Stun\u00b7de", "Zau\u00b7ber", "moch\u00b7te", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur nebenan die Alten schwatzten fort \u2014", "tokens": ["Nur", "ne\u00b7be\u00b7nan", "die", "Al\u00b7ten", "schwatz\u00b7ten", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Du sahst mich an mit deinen M\u00e4rchenaugen.", "tokens": ["Du", "sahst", "mich", "an", "mit", "dei\u00b7nen", "M\u00e4r\u00b7chen\u00b7au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}