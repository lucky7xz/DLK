{"textgrid.poem.60239": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n ist im f\u00fcrstlichen Garten die Frucht", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist im f\u00fcrstlichen Garten die Frucht", "tokens": ["Sch\u00f6n", "ist", "im", "f\u00fcrst\u00b7li\u00b7chen", "Gar\u00b7ten", "die", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Des Pfirsichbaumes, den die goldne Morgensonne", "tokens": ["Des", "Pfir\u00b7sich\u00b7bau\u00b7mes", ",", "den", "die", "gold\u00b7ne", "Mor\u00b7gen\u00b7son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit honigfl\u00f6\u00dfendem Blicke besucht \u2013", "tokens": ["Mit", "ho\u00b7nig\u00b7fl\u00f6\u00b7\u00dfen\u00b7dem", "Bli\u00b7cke", "be\u00b7sucht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch dreymal sch\u00f6ner ist des F\u00fcrsten Augenwonne,", "tokens": ["Doch", "drey\u00b7mal", "sch\u00f6\u00b7ner", "ist", "des", "F\u00fcrs\u00b7ten", "Au\u00b7gen\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die, f\u00fcr ihn geschaffen, der kommende Herbst", "tokens": ["Die", ",", "f\u00fcr", "ihn", "ge\u00b7schaf\u00b7fen", ",", "der", "kom\u00b7men\u00b7de", "Herbst"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "PPER", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Gleich einer sp\u00e4ten Rose brachte.", "tokens": ["Gleich", "ei\u00b7ner", "sp\u00e4\u00b7ten", "Ro\u00b7se", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O Nachwelt, die du einst ihr Bild in Marmar erbst,", "tokens": ["O", "Nach\u00b7welt", ",", "die", "du", "einst", "ihr", "Bild", "in", "Mar\u00b7mar", "erbst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ADV", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die\u00df Grazienangesicht machte", "tokens": ["Die\u00df", "Gra\u00b7zi\u00b7en\u00b7an\u00b7ge\u00b7sicht", "mach\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PDS", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Sie nicht allein anbetenswerth;", "tokens": ["Sie", "nicht", "al\u00b7lein", "an\u00b7be\u00b7tens\u00b7werth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Nein G\u00f6tterg\u00fcte, die zuweilen", "tokens": ["Nein", "G\u00f6t\u00b7ter\u00b7g\u00fc\u00b7te", ",", "die", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKANT", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von dem Olympus in Sterbliche f\u00e4hrt,", "tokens": ["Von", "dem", "O\u00b7lym\u00b7pus", "in", "Sterb\u00b7li\u00b7che", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Eh sie ans Tagelicht eilen,", "tokens": ["Eh", "sie", "ans", "Ta\u00b7ge\u00b7licht", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Und alle Tugenden, mit welchen die G\u00f6tter vordem", "tokens": ["Und", "al\u00b7le", "Tu\u00b7gen\u00b7den", ",", "mit", "wel\u00b7chen", "die", "G\u00f6t\u00b7ter", "vor\u00b7dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "APPR", "PWAT", "ART", "NN", "ADJA"], "meter": "-+-+---+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Des Telemachs Mutter geschm\u00fccket,", "tokens": ["Des", "Te\u00b7le\u00b7machs", "Mut\u00b7ter", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Die machten sie so himmlisch angenehm,", "tokens": ["Die", "mach\u00b7ten", "sie", "so", "himm\u00b7lisch", "an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und alle Seelen entz\u00fccket.", "tokens": ["Und", "al\u00b7le", "See\u00b7len", "ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.17": {"text": "Wohin ihr liebliches Auge sich wandt',", "tokens": ["Wo\u00b7hin", "ihr", "lieb\u00b7li\u00b7ches", "Au\u00b7ge", "sich", "wandt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Voll s\u00fc\u00dfer geistiger Strahlen,", "tokens": ["Voll", "s\u00fc\u00b7\u00dfer", "geis\u00b7ti\u00b7ger", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Ward jeder Busen entbrannt:", "tokens": ["Ward", "je\u00b7der", "Bu\u00b7sen", "ent\u00b7brannt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Kein Rubens w\u00fc\u00dfte die\u00df Auge zu mahlen,", "tokens": ["Kein", "Ru\u00b7bens", "w\u00fc\u00df\u00b7te", "die\u00df", "Au\u00b7ge", "zu", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PDS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "So reizend, so voll sanfter Majest\u00e4t. \u2013", "tokens": ["So", "rei\u00b7zend", ",", "so", "voll", "sanf\u00b7ter", "Ma\u00b7jes\u00b7t\u00e4t", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ihr l\u00e4ngst nach uns gebohrnen T\u00f6chter seht", "tokens": ["Ihr", "l\u00e4ngst", "nach", "uns", "ge\u00b7bohr\u00b7nen", "T\u00f6ch\u00b7ter", "seht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PPER", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Das eigentlichste Bildni\u00df der holden", "tokens": ["Das", "ei\u00b7gent\u00b7lichs\u00b7te", "Bild\u00b7ni\u00df", "der", "hol\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "In Enkelinnen noch durch jedes Seculum,", "tokens": ["In", "En\u00b7ke\u00b7lin\u00b7nen", "noch", "durch", "je\u00b7des", "Se\u00b7cu\u00b7lum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und h\u00f6rts, sie machte Leopolden", "tokens": ["Und", "h\u00f6rts", ",", "sie", "mach\u00b7te", "Leo\u00b7pol\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Sein Dessau zum Elysium.", "tokens": ["Sein", "Des\u00b7sau", "zum", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.27": {"text": "Er ward von ihr geliebt, als ein arkadischer Hirte,", "tokens": ["Er", "ward", "von", "ihr", "ge\u00b7liebt", ",", "als", "ein", "ar\u00b7ka\u00b7di\u00b7scher", "Hir\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Er flocht ihr Blumen in ihr lockigt Haar,", "tokens": ["Er", "flocht", "ihr", "Blu\u00b7men", "in", "ihr", "lo\u00b7ckigt", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und sie wand einen Kranz von Myrte,", "tokens": ["Und", "sie", "wand", "ei\u00b7nen", "Kranz", "von", "Myr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Die jugendgr\u00fcn und knospigt war,", "tokens": ["Die", "ju\u00b7gend\u00b7gr\u00fcn", "und", "knos\u00b7pigt", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Um sein friedselig Haupt, und sagte:", "tokens": ["Um", "sein", "fried\u00b7se\u00b7lig", "Haupt", ",", "und", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJD", "NN", "$,", "KON", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Wohl mir, da\u00df du, mein lieber F\u00fcrst,", "tokens": ["Wohl", "mir", ",", "da\u00df", "du", ",", "mein", "lie\u00b7ber", "F\u00fcrst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Nie nach dem Kranze ringen wirst,", "tokens": ["Nie", "nach", "dem", "Kran\u00b7ze", "rin\u00b7gen", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Um welchen Hektor einst sein muntres Leben wagte,", "tokens": ["Um", "wel\u00b7chen", "Hek\u00b7tor", "einst", "sein", "mun\u00b7tres", "Le\u00b7ben", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PWAT", "NE", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So bitter sich auch \u00fcber ihn", "tokens": ["So", "bit\u00b7ter", "sich", "auch", "\u00fc\u00b7ber", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PRF", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Andromache, sein armes Weib beklagte. \u2013", "tokens": ["And\u00b7ro\u00b7ma\u00b7che", ",", "sein", "ar\u00b7mes", "Weib", "be\u00b7klag\u00b7te", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "Nein, du wirst nie mit der Bellona ziehn,", "tokens": ["Nein", ",", "du", "wirst", "nie", "mit", "der", "Bel\u00b7lo\u00b7na", "ziehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "APPR", "ART", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Mars wird dich nie von meinem Arme trennen.", "tokens": ["Mars", "wird", "dich", "nie", "von", "mei\u00b7nem", "Ar\u00b7me", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Dein Unterthan wird stets im Freudenton", "tokens": ["Dein", "Un\u00b7ter\u00b7than", "wird", "stets", "im", "Freu\u00b7den\u00b7ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Dich segnen, und dich Vater nennen;", "tokens": ["Dich", "seg\u00b7nen", ",", "und", "dich", "Va\u00b7ter", "nen\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KON", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Und Friedrich, unser Sohn", "tokens": ["Und", "Fried\u00b7rich", ",", "un\u00b7ser", "Sohn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "W\u00e4chst unter deinen Augen auf, und lernet", "tokens": ["W\u00e4chst", "un\u00b7ter", "dei\u00b7nen", "Au\u00b7gen", "auf", ",", "und", "ler\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Die gro\u00dfe Kunst, durch welche der Regent", "tokens": ["Die", "gro\u00b7\u00dfe", "Kunst", ",", "durch", "wel\u00b7che", "der", "Re\u00b7gent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Von seinem Volk den Ueberdru\u00df entfernet,", "tokens": ["Von", "sei\u00b7nem", "Volk", "den", "Ue\u00b7berd\u00b7ru\u00df", "ent\u00b7fer\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Indem es gegen ihn von Kinderliebe brennt,", "tokens": ["In\u00b7dem", "es", "ge\u00b7gen", "ihn", "von", "Kin\u00b7der\u00b7lie\u00b7be", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und sich von ihm geliebt erkennt.", "tokens": ["Und", "sich", "von", "ihm", "ge\u00b7liebt", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}