{"textgrid.poem.67299": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kinder, bei tanzenden Horen gezeugt von dem Witz mit der Laune,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kinder, bei tanzenden Horen gezeugt von dem Witz mit der Laune,", "tokens": ["Kin\u00b7der", ",", "bei", "tan\u00b7zen\u00b7den", "Ho\u00b7ren", "ge\u00b7zeugt", "von", "dem", "Witz", "mit", "der", "Lau\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Da\u00df ihr mit leichtem Gescho\u00df scherzend gen\u00f6sset der Jagd,", "tokens": ["Da\u00df", "ihr", "mit", "leich\u00b7tem", "Ge\u00b7scho\u00df", "scher\u00b7zend", "ge\u00b7n\u00f6s\u00b7set", "der", "Jagd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch an der Spielenden gn\u00fcgte nicht euch, und die h\u00e4mischen Zwillings-", "tokens": ["Doch", "an", "der", "Spie\u00b7len\u00b7den", "gn\u00fcg\u00b7te", "nicht", "euch", ",", "und", "die", "h\u00e4\u00b7mi\u00b7schen", "Zwil\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKNEG", "PPER", "$,", "KON", "ART", "ADJA", "TRUNC"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Buben, ", "tokens": ["Bu\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ha, wie brannten die Wunden! und schw\u00e4render weil, wo der schwarze", "tokens": ["Ha", ",", "wie", "brann\u00b7ten", "die", "Wun\u00b7den", "!", "und", "schw\u00e4\u00b7ren\u00b7der", "weil", ",", "wo", "der", "schwar\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ART", "NN", "$.", "KON", "ADV", "KOUS", "$,", "PWAV", "ART", "ADJA"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Mackel ihn reizete, flugs haftet' im Maale der Pfeil.", "tokens": ["Ma\u00b7ckel", "ihn", "rei\u00b7ze\u00b7te", ",", "flugs", "haf\u00b7tet'", "im", "Maa\u00b7le", "der", "Pfeil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Kinder, mir wallet k\u00fchler das Blut, und l\u00e4ngst hat mein Consul", "tokens": ["Kin\u00b7der", ",", "mir", "wal\u00b7let", "k\u00fch\u00b7ler", "das", "Blut", ",", "und", "l\u00e4ngst", "hat", "mein", "Con\u00b7sul"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "KON", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Plancus", "tokens": ["Plan\u00b7cus"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Her in die Flammen, ihr frevelnden Wichte! Zur S\u00fchn-Hekatombe", "tokens": ["Her", "in", "die", "Flam\u00b7men", ",", "ihr", "fre\u00b7veln\u00b7den", "Wich\u00b7te", "!", "Zur", "S\u00fchn\u00b7He\u00b7ka\u00b7tom\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$.", "APPRART", "NN"], "meter": "-+-+--+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Sollt ihr, in Asch' und in Rauch, b\u00fc\u00dfend verwehen, Gez\u00fccht!", "tokens": ["Sollt", "ihr", ",", "in", "Asch'", "und", "in", "Rauch", ",", "b\u00fc\u00b7\u00dfend", "ver\u00b7we\u00b7hen", ",", "Ge\u00b7z\u00fccht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "NE", "KON", "APPR", "NN", "$,", "ADJD", "VVINF", "$,", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aber, ihr Gaffer dort, weg vom Altare! da\u00df nicht entlarv' ein", "tokens": ["A\u00b7ber", ",", "ihr", "Gaf\u00b7fer", "dort", ",", "weg", "vom", "Al\u00b7ta\u00b7re", "!", "da\u00df", "nicht", "ent\u00b7la\u00b7rv'", "ein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PPOSAT", "NN", "ADV", "$,", "ADV", "APPRART", "NN", "$.", "KOUS", "PTKNEG", "VVFIN", "ART"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.6": {"text": "Schauder ihn, der ein Schelm nahet, ein St\u00fcmper, ein Geck!", "tokens": ["Schau\u00b7der", "ihn", ",", "der", "ein", "Schelm", "na\u00b7het", ",", "ein", "St\u00fcm\u00b7per", ",", "ein", "Geck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}