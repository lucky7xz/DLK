{"dta.poem.15713": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Pflanzen aus der Erd', ohn' ihr was abzubrechen,", "tokens": ["Wie", "Pflan\u00b7zen", "aus", "der", "Erd'", ",", "ohn'", "ihr", "was", "ab\u00b7zu\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,", "KOUI", "PPER", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gehn Gedanken aus vom Geist, ohn' ihn zu schw\u00e4chen.", "tokens": ["So", "gehn", "Ge\u00b7dan\u00b7ken", "aus", "vom", "Geist", ",", "ohn'", "ihn", "zu", "schw\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "APPRART", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wie der Erde Schoo\u00df stets neue Triebe treibt,", "tokens": ["Und", "wie", "der", "Er\u00b7de", "Schoo\u00df", "stets", "neu\u00b7e", "Trie\u00b7be", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So auch der Geist in dir, der nie unth\u00e4tig bleibt.", "tokens": ["So", "auch", "der", "Geist", "in", "dir", ",", "der", "nie", "un\u00b7th\u00e4\u00b7tig", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPER", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn du der Stunde dienst, beherrschest du die Zeit;", "tokens": ["Wenn", "du", "der", "Stun\u00b7de", "dienst", ",", "be\u00b7herr\u00b7schest", "du", "die", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirk' auf den Augenblick! er wirkt in Ewigkeit.", "tokens": ["Wirk'", "auf", "den", "Au\u00b7gen\u00b7blick", "!", "er", "wirkt", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo ist der Sonnstral hin, der \u00fcbers Feld gestreifet?", "tokens": ["Wo", "ist", "der", "Sonn\u00b7stral", "hin", ",", "der", "\u00fc\u00b7bers", "Feld", "ge\u00b7strei\u00b7fet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat am Erntekranz der Welt ein Blatt gereifet.", "tokens": ["Er", "hat", "am", "Ern\u00b7te\u00b7kranz", "der", "Welt", "ein", "Blatt", "ge\u00b7rei\u00b7fet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und alle Rosen bl\u00fchn noch jetzt im irdischen Staube", "tokens": ["Und", "al\u00b7le", "Ro\u00b7sen", "bl\u00fchn", "noch", "jetzt", "im", "ir\u00b7di\u00b7schen", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als Abglanz einer, die gebl\u00fcht an Edens Laube.", "tokens": ["Als", "Ab\u00b7glanz", "ei\u00b7ner", ",", "die", "ge\u00b7bl\u00fcht", "an", "E\u00b7dens", "Lau\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "$,", "PRELS", "VVPP", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}