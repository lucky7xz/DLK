{"dta.poem.13944": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der brave Schiffer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das war von \u00e4chtem Kerne;", "tokens": ["Das", "war", "von", "\u00e4ch\u00b7tem", "Ker\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gedankenvoll die Wacht", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7voll", "die", "Wacht"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaut durch die w\u00fcste Ferne", "tokens": ["Schaut", "durch", "die", "w\u00fcs\u00b7te", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum k\u00f6niglichen Sterne,", "tokens": ["Zum", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Der leuchtet aus der Nacht.", "tokens": ["Der", "leuch\u00b7tet", "aus", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}