{"textgrid.poem.54268": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hattest Lieb und Lust vor kurtzen abgeschwohren,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hattest Lieb und Lust vor kurtzen abgeschwohren,", "tokens": ["Du", "hat\u00b7test", "Lieb", "und", "Lust", "vor", "kurt\u00b7zen", "ab\u00b7ge\u00b7schwoh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Freyheit ward von dir zum Leit-Stern auserkohren,", "tokens": ["Die", "Frey\u00b7heit", "ward", "von", "dir", "zum", "Leit\u00b7S\u00b7tern", "au\u00b7ser\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch dieser Eyd der wird, so fest er war gemacht,", "tokens": ["Doch", "die\u00b7ser", "Eyd", "der", "wird", ",", "so", "fest", "er", "war", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "VAFIN", "$,", "ADV", "ADJD", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach leider! allzufr\u00fch von dir nunmehr verlacht.", "tokens": ["Ach", "lei\u00b7der", "!", "all\u00b7zu\u00b7fr\u00fch", "von", "dir", "nun\u00b7mehr", "ver\u00b7lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ADJD", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "H\u00e4ttst du dem Seladon nicht ins Gesicht gesehen,", "tokens": ["H\u00e4ttst", "du", "dem", "Se\u00b7la\u00b7don", "nicht", "ins", "Ge\u00b7sicht", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So w\u00e4r es nicht um dich und deinen Schwur geschehen.", "tokens": ["So", "w\u00e4r", "es", "nicht", "um", "dich", "und", "dei\u00b7nen", "Schwur", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach dein Gel\u00fcbde scheint auf einmahl gantz zernicht,", "tokens": ["Ach", "dein", "Ge\u00b7l\u00fcb\u00b7de", "scheint", "auf", "ein\u00b7mahl", "gantz", "zer\u00b7nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VVFIN", "APPR", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil dessen Anmuth dir den Schwur, wie Gla\u00df zerbricht.", "tokens": ["Weil", "des\u00b7sen", "An\u00b7muth", "dir", "den", "Schwur", ",", "wie", "Gla\u00df", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "PPER", "ART", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So lodern wiederum von neuen deine Flammen.", "tokens": ["So", "lo\u00b7dern", "wie\u00b7de\u00b7rum", "von", "neu\u00b7en", "dei\u00b7ne", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O freylich schickt sich Ei\u00df und Sonne nicht zusammen.", "tokens": ["O", "frey\u00b7lich", "schickt", "sich", "Ei\u00df", "und", "Son\u00b7ne", "nicht", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PRF", "NN", "KON", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer ruhig bleiben will, der schliest die Augen zu,", "tokens": ["Wer", "ru\u00b7hig", "blei\u00b7ben", "will", ",", "der", "schliest", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wann ihn was frisches reitzt, und macht es nicht wie du.", "tokens": ["Wann", "ihn", "was", "fri\u00b7sches", "reitzt", ",", "und", "macht", "es", "nicht", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADJA", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}