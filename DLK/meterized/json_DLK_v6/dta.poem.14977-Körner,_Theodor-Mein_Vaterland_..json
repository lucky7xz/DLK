{"dta.poem.14977": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Mein Vaterland .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Wo edler Geister Funken spr\u00fchten,", "tokens": ["Wo", "ed\u00b7ler", "Geis\u00b7ter", "Fun\u00b7ken", "spr\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Kr\u00e4nze f\u00fcr das Sch\u00f6ne bl\u00fchten,", "tokens": ["Wo", "Kr\u00e4n\u00b7ze", "f\u00fcr", "das", "Sch\u00f6\u00b7ne", "bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo starke Herzen freudig gl\u00fchten,", "tokens": ["Wo", "star\u00b7ke", "Her\u00b7zen", "freu\u00b7dig", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr alles Heilige entbrannt,", "tokens": ["F\u00fcr", "al\u00b7les", "Hei\u00b7li\u00b7ge", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da war mein Vaterland!", "tokens": ["Da", "war", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie hei\u00dft des S\u00e4ngers Vaterland? \u2014", "tokens": ["Wie", "hei\u00dft", "des", "S\u00e4n\u00b7gers", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt \u00fcber seiner S\u00f6hne Leichen,", "tokens": ["Jetzt", "\u00fc\u00b7ber", "sei\u00b7ner", "S\u00f6h\u00b7ne", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt weint es unter fremden Streichen,", "tokens": ["Jetzt", "weint", "es", "un\u00b7ter", "frem\u00b7den", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst hie\u00df es nur das Land der Eichen,", "tokens": ["Sonst", "hie\u00df", "es", "nur", "das", "Land", "der", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das freie Land, das deutsche Land!", "tokens": ["Das", "frei\u00b7e", "Land", ",", "das", "deut\u00b7sche", "Land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So hie\u00df mein Vaterland!", "tokens": ["So", "hie\u00df", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was weint des S\u00e4ngers Vaterland? \u2014", "tokens": ["Was", "weint", "des", "S\u00e4n\u00b7gers", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df vor des W\u00fcthrichs Ungewittern,", "tokens": ["Da\u00df", "vor", "des", "W\u00fcth\u00b7richs", "Un\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die F\u00fcrsten seiner V\u00f6lker zittern,", "tokens": ["Die", "F\u00fcrs\u00b7ten", "sei\u00b7ner", "V\u00f6l\u00b7ker", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihre heil'gen Worte splittern,", "tokens": ["Da\u00df", "ih\u00b7re", "heil'\u00b7gen", "Wor\u00b7te", "split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df sein Ruf kein H\u00f6ren fand,", "tokens": ["Und", "da\u00df", "sein", "Ruf", "kein", "H\u00f6\u00b7ren", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum weint mein Vaterland!", "tokens": ["Drum", "weint", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wem ruft des S\u00e4ngers Vaterland? \u2014", "tokens": ["Wem", "ruft", "des", "S\u00e4n\u00b7gers", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ruft nach den verstummten G\u00f6ttern,", "tokens": ["Es", "ruft", "nach", "den", "ver\u00b7stumm\u00b7ten", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit der Verzweiflung Donnerwettern,", "tokens": ["Mit", "der", "Ver\u00b7zwei\u00b7flung", "Don\u00b7ner\u00b7wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach seiner Freiheit, seinen Rettern,", "tokens": ["Nach", "sei\u00b7ner", "Frei\u00b7heit", ",", "sei\u00b7nen", "Ret\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nach der Vergeltung R\u00e4cherhand.", "tokens": ["Nach", "der", "Ver\u00b7gel\u00b7tung", "R\u00e4\u00b7cher\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Der ruft mein Vaterland!", "tokens": ["Der", "ruft", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was will des S\u00e4ngers Vaterland? \u2014", "tokens": ["Was", "will", "des", "S\u00e4n\u00b7gers", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Knechte will es niederschlagen,", "tokens": ["Die", "Knech\u00b7te", "will", "es", "nie\u00b7der\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Bluthund aus den Gr\u00e4nzen jagen,", "tokens": ["Den", "Blut\u00b7hund", "aus", "den", "Gr\u00e4n\u00b7zen", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und frei die freien S\u00f6hne tragen,", "tokens": ["Und", "frei", "die", "frei\u00b7en", "S\u00f6h\u00b7ne", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Oder, frei sie betten unterm Sand.", "tokens": ["O\u00b7der", ",", "frei", "sie", "bet\u00b7ten", "un\u00b7term", "Sand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Das will mein Vaterland!", "tokens": ["Das", "will", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und hofft des S\u00e4ngers Vaterland? \u2014", "tokens": ["Und", "hofft", "des", "S\u00e4n\u00b7gers", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hofft auf die gerechte Sache,", "tokens": ["Es", "hofft", "auf", "die", "ge\u00b7rech\u00b7te", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hofft, da\u00df sein treues Volk erwache,", "tokens": ["Hofft", ",", "da\u00df", "sein", "treu\u00b7es", "Volk", "er\u00b7wa\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hofft auf des gro\u00dfen Gottes Rache,", "tokens": ["Hofft", "auf", "des", "gro\u00b7\u00dfen", "Got\u00b7tes", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und hat den R\u00e4cher nicht verkannt.", "tokens": ["Und", "hat", "den", "R\u00e4\u00b7cher", "nicht", "ver\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drauf hofft mein Vaterland!", "tokens": ["Drauf", "hofft", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}