{"textgrid.poem.41077": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck des Verlassenseins", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl ist es sch\u00f6n, zu stehen", "tokens": ["Wohl", "ist", "es", "sch\u00f6n", ",", "zu", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In trauter Freunde Reihn,", "tokens": ["In", "trau\u00b7ter", "Freun\u00b7de", "Reihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sch\u00f6ner ist's, zu gehen", "tokens": ["Doch", "sch\u00f6\u00b7ner", "ist's", ",", "zu", "ge\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In weiter Welt allein.", "tokens": ["In", "wei\u00b7ter", "Welt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mensch! bist du ganz verlassen,", "tokens": ["Mensch", "!", "bist", "du", "ganz", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Klag' keinen Augenblick!", "tokens": ["Klag'", "kei\u00b7nen", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kannst du erst dich fassen,", "tokens": ["Da", "kannst", "du", "erst", "dich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kannst gehn in Gott zur\u00fcck.", "tokens": ["Kannst", "gehn", "in", "Gott", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es t\u00e4uscht die Welt, die tr\u00fcbe,", "tokens": ["Es", "t\u00e4uscht", "die", "Welt", ",", "die", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir nimmer Aug' und Ohr;", "tokens": ["Dir", "nim\u00b7mer", "Aug'", "und", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die innre Welt der Liebe", "tokens": ["Die", "inn\u00b7re", "Welt", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er\u00f6ffnet dir ihr Tor.", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "dir", "ihr", "Tor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In ihr lebst du versunken", "tokens": ["In", "ihr", "lebst", "du", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "In Gottes Angesicht,", "tokens": ["In", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die andern, erdetrunken,", "tokens": ["Die", "an\u00b7dern", ",", "er\u00b7de\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gewahren deiner nicht.", "tokens": ["Ge\u00b7wah\u00b7ren", "dei\u00b7ner", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ja! m\u00f6chten sie dich lassen", "tokens": ["Ja", "!", "m\u00f6ch\u00b7ten", "sie", "dich", "las\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deinem Innern stumm,", "tokens": ["In", "dei\u00b7nem", "In\u00b7nern", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verlassen, ganz verlassen,", "tokens": ["Ver\u00b7las\u00b7sen", ",", "ganz", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis deine Zeit ist um.", "tokens": ["Bis", "dei\u00b7ne", "Zeit", "ist", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "KOUI", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In Tiefen unber\u00fchret", "tokens": ["In", "Tie\u00b7fen", "un\u00b7be\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4chst einsam das Metall;", "tokens": ["W\u00e4chst", "ein\u00b7sam", "das", "Me\u00b7tall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo's nachtet und gefrieret,", "tokens": ["Wo's", "nach\u00b7tet", "und", "ge\u00b7frie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich bildet der Kristall.", "tokens": ["Sich", "bil\u00b7det", "der", "Kris\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}