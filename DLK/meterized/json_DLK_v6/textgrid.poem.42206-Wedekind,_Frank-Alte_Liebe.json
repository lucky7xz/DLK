{"textgrid.poem.42206": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Alte Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab dich lieb, kannst du es denn ermessen,", "tokens": ["Ich", "hab", "dich", "lieb", ",", "kannst", "du", "es", "denn", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstehn das Wort, so traut und s\u00fc\u00df?", "tokens": ["Ver\u00b7stehn", "das", "Wort", ",", "so", "traut", "und", "s\u00fc\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schlie\u00dfet in sich eine Welt von Wonne,", "tokens": ["Es", "schlie\u00b7\u00dfet", "in", "sich", "ei\u00b7ne", "Welt", "von", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es birgt in sich ein ganzes Paradies.", "tokens": ["Es", "birgt", "in", "sich", "ein", "gan\u00b7zes", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich hab dich lieb, so t\u00f6nt es mir entgegen,", "tokens": ["Ich", "hab", "dich", "lieb", ",", "so", "t\u00f6nt", "es", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn morgens ich zu neuem Sein erwacht;", "tokens": ["Wenn", "mor\u00b7gens", "ich", "zu", "neu\u00b7em", "Sein", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn am Abend tausend Sterne funkeln,", "tokens": ["Und", "wenn", "am", "A\u00b7bend", "tau\u00b7send", "Ster\u00b7ne", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich hab dich lieb, so klingt die Nacht.", "tokens": ["Ich", "hab", "dich", "lieb", ",", "so", "klingt", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist mir fern, ich will darob nicht klagen,", "tokens": ["Du", "bist", "mir", "fern", ",", "ich", "will", "da\u00b7rob", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich hegen in des Herzens heil'gem Schrein.", "tokens": ["Dich", "he\u00b7gen", "in", "des", "Her\u00b7zens", "heil'\u00b7gem", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kling fort, mein Lied! Jauchz auf, begl\u00fcckte Seele!", "tokens": ["Kling", "fort", ",", "mein", "Lied", "!", "Jauchz", "auf", ",", "be\u00b7gl\u00fcck\u00b7te", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "NN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich hab dich lieb, und nie wirds anders sein.", "tokens": ["Ich", "hab", "dich", "lieb", ",", "und", "nie", "wirds", "an\u00b7ders", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "KON", "ADV", "VAFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}