{"textgrid.poem.33913": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Rosen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als du fr\u00fchmorgens gingst", "tokens": ["Als", "du", "fr\u00fch\u00b7mor\u00b7gens", "gingst"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und an der Sonne hingst,", "tokens": ["Und", "an", "der", "Son\u00b7ne", "hingst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Pfl\u00fccktest du dir,", "tokens": ["Pfl\u00fcck\u00b7test", "du", "dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die, von ihr angegl\u00fcht,", "tokens": ["Die", ",", "von", "ihr", "an\u00b7ge\u00b7gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Still vor ihr aufgebl\u00fcht,", "tokens": ["Still", "vor", "ihr", "auf\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und nun den Duft verspr\u00fcht,", "tokens": ["Und", "nun", "den", "Duft", "ver\u00b7spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Rosen zur Zier.", "tokens": ["Ro\u00b7sen", "zur", "Zier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "H\u00e4ltst sie noch Abends fest?", "tokens": ["H\u00e4ltst", "sie", "noch", "A\u00b7bends", "fest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schmeichelte dir der West", "tokens": ["Schmei\u00b7chel\u00b7te", "dir", "der", "West"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "L\u00e4ngst sie nicht ab?", "tokens": ["L\u00e4ngst", "sie", "nicht", "ab", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Siehst ja, ihr Leben schwand!", "tokens": ["Siehst", "ja", ",", "ihr", "Le\u00b7ben", "schwand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Wo ist der Farbenbrand?", "tokens": ["Wo", "ist", "der", "Far\u00b7ben\u00b7brand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch nur in deiner Hand", "tokens": ["Doch", "nur", "in", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sind sie im Grab.", "tokens": ["Sind", "sie", "im", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Gieb sie den Winden preis,", "tokens": ["Gieb", "sie", "den", "Win\u00b7den", "preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df sie mit ihnen leis", "tokens": ["Da\u00df", "sie", "mit", "ih\u00b7nen", "leis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "D\u00fcngen den Strauch.", "tokens": ["D\u00fcn\u00b7gen", "den", "Strauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "F\u00fchlt's nicht sogleich der Zweig,", "tokens": ["F\u00fchlt's", "nicht", "sog\u00b7leich", "der", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fchlt's doch die Wurzel gleich,", "tokens": ["F\u00fchlt's", "doch", "die", "Wur\u00b7zel", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und ist nur diese reich,", "tokens": ["Und", "ist", "nur", "die\u00b7se", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PDAT", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wird der es auch!", "tokens": ["Wird", "der", "es", "auch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}