{"textgrid.poem.23928": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Lied in der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stra\u00dfen hin und Stra\u00dfen her", "tokens": ["Stra\u00b7\u00dfen", "hin", "und", "Stra\u00b7\u00dfen", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandr ich in der Nacht;", "tokens": ["Wandr", "ich", "in", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bin aus Tr\u00e4umen dumpf und schwer", "tokens": ["Bin", "aus", "Tr\u00e4u\u00b7men", "dumpf", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schluchzend aufgewacht.", "tokens": ["Schluch\u00b7zend", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Thr\u00e4nen,", "tokens": ["Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Sehnen,", "tokens": ["Seh\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Lust und Schmerz, \u2013", "tokens": ["Lust", "und", "Schmerz", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$,", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ach, wohin treibt mich mein Herz?", "tokens": ["Ach", ",", "wo\u00b7hin", "treibt", "mich", "mein", "Herz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ach, wohin treibt mich mein Herz?", "tokens": ["Ach", ",", "wo\u00b7hin", "treibt", "mich", "mein", "Herz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Steht ein Haus in Gr\u00fcn gebaut", "tokens": ["Steht", "ein", "Haus", "in", "Gr\u00fcn", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00dfen vor der Stadt,", "tokens": ["Drau\u00b7\u00dfen", "vor", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo der Flu\u00df mit leisem Laut", "tokens": ["Wo", "der", "Flu\u00df", "mit", "lei\u00b7sem", "Laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein Gestr\u00f6me hat.", "tokens": ["Sein", "Ge\u00b7str\u00f6\u00b7me", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Bl\u00fcten", "tokens": ["Bl\u00fc\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "H\u00fcten", "tokens": ["H\u00fc\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Dicht es ein:", "tokens": ["Dicht", "es", "ein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Dort m\u00f6cht ich zu Gaste sein,", "tokens": ["Dort", "m\u00f6cht", "ich", "zu", "Gas\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Dort m\u00f6cht ich zu Gaste sein.", "tokens": ["Dort", "m\u00f6cht", "ich", "zu", "Gas\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}