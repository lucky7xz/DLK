{"textgrid.poem.67956": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "24. Das Lied vom Schmetterlinge", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebes, leichtes, luftges Ding,", "tokens": ["Lie\u00b7bes", ",", "leich\u00b7tes", ",", "luft\u00b7ges", "Ding", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmetterling,", "tokens": ["Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Das da \u00fcber Blumen schwebet,", "tokens": ["Das", "da", "\u00fc\u00b7ber", "Blu\u00b7men", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nur von Thau und Bl\u00fcthen lebet,", "tokens": ["Nur", "von", "Thau", "und", "Bl\u00fc\u00b7then", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bl\u00fcthe selbst, ein fliegend Blatt,", "tokens": ["Bl\u00fc\u00b7the", "selbst", ",", "ein", "flie\u00b7gend", "Blatt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das mit welchem Rosenfinger!", "tokens": ["Das", "mit", "wel\u00b7chem", "Ro\u00b7sen\u00b7fin\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wer bepurpurt hat?", "tokens": ["Wer", "be\u00b7pur\u00b7purt", "hat", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wars ein Sylphe, der dein Kleid", "tokens": ["Wars", "ein", "Syl\u00b7phe", ",", "der", "dein", "Kleid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "So bestreut,", "tokens": ["So", "be\u00b7streut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dich aus Morgenduft gewebet,", "tokens": ["Dich", "aus", "Mor\u00b7gen\u00b7duft", "ge\u00b7we\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur auf Tage dich belebet;", "tokens": ["Nur", "auf", "Ta\u00b7ge", "dich", "be\u00b7le\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seelchen und dein kleines Herz", "tokens": ["Seel\u00b7chen", "und", "dein", "klei\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Pocht da unter meinem Finger,", "tokens": ["Pocht", "da", "un\u00b7ter", "mei\u00b7nem", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchlet Todesschmerz.", "tokens": ["F\u00fch\u00b7let", "To\u00b7des\u00b7schmerz", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Fleuch dahin, o Seelchen, sey", "tokens": ["Fleuch", "da\u00b7hin", ",", "o", "Seel\u00b7chen", ",", "sey"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PAV", "$,", "FM", "NN", "$,", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Froh und frei,", "tokens": ["Froh", "und", "frei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mir ein Bild, was ich seyn werde,", "tokens": ["Mir", "ein", "Bild", ",", "was", "ich", "seyn", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PWS", "PPER", "VAINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Raupe dieser Erde,", "tokens": ["Wenn", "die", "Rau\u00b7pe", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch wie du ein Zephyr ist,", "tokens": ["Auch", "wie", "du", "ein", "Ze\u00b7phyr", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Und in Duft und Thau und Honig", "tokens": ["Und", "in", "Duft", "und", "Thau", "und", "Ho\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Jede Bl\u00fcthe k\u00fc\u00dft.", "tokens": ["Je\u00b7de", "Bl\u00fc\u00b7the", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}