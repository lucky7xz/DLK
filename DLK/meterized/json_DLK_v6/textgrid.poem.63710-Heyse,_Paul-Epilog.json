{"textgrid.poem.63710": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Epilog", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur mit flinkem Stift umschrieben,", "tokens": ["Nur", "mit", "flin\u00b7kem", "Stift", "um\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Angetuscht mit leichten T\u00f6nen,", "tokens": ["An\u00b7ge\u00b7tuscht", "mit", "leich\u00b7ten", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum ein Umri\u00df ist geblieben", "tokens": ["Kaum", "ein", "Um\u00b7ri\u00df", "ist", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "All des farbenkr\u00e4ftig Sch\u00f6nen.", "tokens": ["All", "des", "far\u00b7ben\u00b7kr\u00e4f\u00b7tig", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und vorbei noch schattenhafter", "tokens": ["Und", "vor\u00b7bei", "noch", "schat\u00b7ten\u00b7haf\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJA"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Wird euch die Staffage gleiten,", "tokens": ["Wird", "euch", "die", "Staf\u00b7fa\u00b7ge", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ein im Schlendern aufgeraffter", "tokens": ["Ein", "im", "Schlen\u00b7dern", "auf\u00b7ge\u00b7raff\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haufe schlichter Menschlichkeiten.", "tokens": ["Hau\u00b7fe", "schlich\u00b7ter", "Menschlich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Doch des Malers Bild \u2013 gleich jenen", "tokens": ["Doch", "des", "Ma\u00b7lers", "Bild", "\u2013", "gleich", "je\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "$(", "ADV", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwindet's bald ins Ungewisse.", "tokens": ["Schwin\u00b7det's", "bald", "ins", "Un\u00b7ge\u00b7wis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollten sich unsterblich w\u00e4hnen", "tokens": ["Soll\u00b7ten", "sich", "uns\u00b7terb\u00b7lich", "w\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines Schattens Schattenrisse?", "tokens": ["Ei\u00b7nes", "Schat\u00b7tens", "Schat\u00b7ten\u00b7ris\u00b7se", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}