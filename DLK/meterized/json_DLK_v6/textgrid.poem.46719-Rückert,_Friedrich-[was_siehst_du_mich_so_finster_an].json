{"textgrid.poem.46719": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[was siehst du mich so finster an]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was siehst du mich so finster an,", "tokens": ["Was", "siehst", "du", "mich", "so", "fins\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als h\u00e4tt' ich's wahrlich dir gethan?", "tokens": ["Als", "h\u00e4tt'", "ich's", "wahr\u00b7lich", "dir", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es that's der Tod mit seinen Fr\u00f6sten.", "tokens": ["Es", "that's", "der", "Tod", "mit", "sei\u00b7nen", "Fr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4tt' es des Winters Frost gethan,", "tokens": ["H\u00e4tt'", "es", "des", "Win\u00b7ters", "Frost", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "So b\u00f6t' ich meinen Trost dir an,", "tokens": ["So", "b\u00f6t'", "ich", "mei\u00b7nen", "Trost", "dir", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So aber kann dich Gott nur tr\u00f6sten.", "tokens": ["So", "a\u00b7ber", "kann", "dich", "Gott", "nur", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch sieh nur meine Blumen an!", "tokens": ["Doch", "sieh", "nur", "mei\u00b7ne", "Blu\u00b7men", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie haben dir kein Leid gethan,", "tokens": ["Sie", "ha\u00b7ben", "dir", "kein", "Leid", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und m\u00f6chten, k\u00f6nnten sie's, dich tr\u00f6sten.", "tokens": ["Und", "m\u00f6ch\u00b7ten", ",", "k\u00f6nn\u00b7ten", "sie's", ",", "dich", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "VMFIN", "PIS", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nimmst du keinen Trost sonst an,", "tokens": ["Und", "nimmst", "du", "kei\u00b7nen", "Trost", "sonst", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denk: Auch um euch ist's bald gethan,", "tokens": ["Denk", ":", "Auch", "um", "euch", "ist's", "bald", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ADV", "APPR", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr alle sterbet auch in Fr\u00f6sten!", "tokens": ["Ihr", "al\u00b7le", "ster\u00b7bet", "auch", "in", "Fr\u00f6s\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}