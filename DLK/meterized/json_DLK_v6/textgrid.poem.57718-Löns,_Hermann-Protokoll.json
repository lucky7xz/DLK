{"textgrid.poem.57718": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Protokoll", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hei\u00dfe Friedrich Wilhelm Schl\u00e4ger,", "tokens": ["Ich", "hei\u00b7\u00dfe", "Fried\u00b7rich", "Wil\u00b7helm", "Schl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht vorbestraft, bin vierzig alt,", "tokens": ["Nicht", "vor\u00b7bes\u00b7traft", ",", "bin", "vier\u00b7zig", "alt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "VAFIN", "CARD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verehelicht, ich stand als J\u00e4ger", "tokens": ["Ver\u00b7e\u00b7he\u00b7licht", ",", "ich", "stand", "als", "J\u00e4\u00b7ger"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In B\u00fcckeburg und bin bestallt", "tokens": ["In", "B\u00fc\u00b7cke\u00b7burg", "und", "bin", "be\u00b7stallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seit sieben Jahren \u00fcber Echter,", "tokens": ["Seit", "sie\u00b7ben", "Jah\u00b7ren", "\u00fc\u00b7ber", "Ech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aubruch, Kirchlohe, Hellenstein", "tokens": ["Aub\u00b7ruch", ",", "Kirch\u00b7lo\u00b7he", ",", "Hel\u00b7len\u00b7stein"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NN", "$,", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Als Jagdaufseher von dem P\u00e4chter,", "tokens": ["Als", "Jagd\u00b7auf\u00b7se\u00b7her", "von", "dem", "P\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Dem adeligen Jagdverein.", "tokens": ["Dem", "a\u00b7de\u00b7li\u00b7gen", "Jagd\u00b7ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich merkte es seit ein'gen Wochen,", "tokens": ["Ich", "merk\u00b7te", "es", "seit", "ein'\u00b7gen", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es war nicht sauber im Revier,", "tokens": ["Es", "war", "nicht", "sau\u00b7ber", "im", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.3": {"text": "Hier war ein Rehbock aufgebrochen,", "tokens": ["Hier", "war", "ein", "Reh\u00b7bock", "auf\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort fehlte eine Ricke mir;", "tokens": ["Dort", "fehl\u00b7te", "ei\u00b7ne", "Ri\u00b7cke", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Pfingstsonntag wurden zwei Gescheide", "tokens": ["Pfingst\u00b7sonn\u00b7tag", "wur\u00b7den", "zwei", "Ge\u00b7schei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf einmal mir ins Haus gebracht,", "tokens": ["Auf", "ein\u00b7mal", "mir", "ins", "Haus", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von alten Ricken, alle beide", "tokens": ["Von", "al\u00b7ten", "Ri\u00b7cken", ",", "al\u00b7le", "bei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PIAT", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Enthielten eine reife Tracht.", "tokens": ["Ent\u00b7hiel\u00b7ten", "ei\u00b7ne", "rei\u00b7fe", "Tracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Am dritten Juli bei halb viere", "tokens": ["Am", "drit\u00b7ten", "Ju\u00b7li", "bei", "halb", "vie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stand ich am K\u00f6sterbruche an,", "tokens": ["Stand", "ich", "am", "K\u00f6s\u00b7ter\u00b7bru\u00b7che", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem besten Wechsel im Reviere,", "tokens": ["Dem", "bes\u00b7ten", "Wech\u00b7sel", "im", "Re\u00b7vie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da kam von Meyers Busch ein Mann;", "tokens": ["Da", "kam", "von", "Me\u00b7yers", "Busch", "ein", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die M\u00fctze trug er im Gesichte", "tokens": ["Die", "M\u00fct\u00b7ze", "trug", "er", "im", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und unterm Arme das Gewehr,", "tokens": ["Und", "un\u00b7term", "Ar\u00b7me", "das", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er pirschte von der Zwillingsfichte", "tokens": ["Er", "pirschte", "von", "der", "Zwil\u00b7lings\u00b7fich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sich langsam vor der Wiese her.", "tokens": ["Sich", "lang\u00b7sam", "vor", "der", "Wie\u00b7se", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dort \u00e4ste bei der Vorflutbr\u00fccke", "tokens": ["Dort", "\u00e4s\u00b7te", "bei", "der", "Vor\u00b7flut\u00b7br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertraut seit einer Stunde sich", "tokens": ["Ver\u00b7traut", "seit", "ei\u00b7ner", "Stun\u00b7de", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihrem Kitze eine Ricke,", "tokens": ["Mit", "ih\u00b7rem", "Kit\u00b7ze", "ei\u00b7ne", "Ri\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf diese zu der Wilddieb schlich;", "tokens": ["Auf", "die\u00b7se", "zu", "der", "Wild\u00b7dieb", "schlich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah, wie er sich fertig machte", "tokens": ["Ich", "sah", ",", "wie", "er", "sich", "fer\u00b7tig", "mach\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und langsam vortrat aus dem Wald,", "tokens": ["Und", "lang\u00b7sam", "vor\u00b7trat", "aus", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und ehe da\u00df es dr\u00fcben krachte,", "tokens": ["Und", "e\u00b7he", "da\u00df", "es", "dr\u00fc\u00b7ben", "krach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ging ich in Anschlag und schrie: \u00bbHalt!\u00ab", "tokens": ["Ging", "ich", "in", "An\u00b7schlag", "und", "schrie", ":", "\u00bb", "Halt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "$.", "$(", "VVIMP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So wie ich rief, da ri\u00df er Funken,", "tokens": ["So", "wie", "ich", "rief", ",", "da", "ri\u00df", "er", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hagel schlug um mich herum,", "tokens": ["Der", "Ha\u00b7gel", "schlug", "um", "mich", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da nahm aufs Korn ich den Halunken", "tokens": ["Da", "nahm", "aufs", "Korn", "ich", "den", "Ha\u00b7lun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und machte auch den Finger krumm;", "tokens": ["Und", "mach\u00b7te", "auch", "den", "Fin\u00b7ger", "krumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah im Feuer ihn roullieren,", "tokens": ["Ich", "sah", "im", "Feu\u00b7er", "ihn", "roul\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Knall er nicht vernommen hat,", "tokens": ["Den", "Knall", "er", "nicht", "ver\u00b7nom\u00b7men", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er tat nicht Hand noch Fu\u00df mehr r\u00fchren,", "tokens": ["Er", "tat", "nicht", "Hand", "noch", "Fu\u00df", "mehr", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es sa\u00df die Kugel Mitte Blatt.", "tokens": ["Es", "sa\u00df", "die", "Ku\u00b7gel", "Mit\u00b7te", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}