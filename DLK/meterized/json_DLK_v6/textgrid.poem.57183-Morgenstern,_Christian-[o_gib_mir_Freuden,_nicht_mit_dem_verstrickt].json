{"textgrid.poem.57183": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[o gib mir Freuden, nicht mit dem verstrickt]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O gib mir Freuden, nicht mit dem verstrickt,", "tokens": ["O", "gib", "mir", "Freu\u00b7den", ",", "nicht", "mit", "dem", "ver\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "NN", "$,", "PTKNEG", "APPR", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "was ich als niedres Ich in mir empfinde,", "tokens": ["was", "ich", "als", "nie\u00b7dres", "Ich", "in", "mir", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "PIS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "gib solche Freuden mir zum Angebinde", "tokens": ["gib", "sol\u00b7che", "Freu\u00b7den", "mir", "zum", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wie Geist sie Geist, der Seele Seele schickt.", "tokens": ["wie", "Geist", "sie", "Geist", ",", "der", "See\u00b7le", "See\u00b7le", "schickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O nicht mehr dieser schalen Freuden Pein,", "tokens": ["O", "nicht", "mehr", "die\u00b7ser", "scha\u00b7len", "Freu\u00b7den", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "PDAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die doch erkauft nur sind von fremden \u2013 Leiden!", "tokens": ["die", "doch", "er\u00b7kauft", "nur", "sind", "von", "frem\u00b7den", "\u2013", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "VAFIN", "APPR", "ADJA", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schenk Herzen mir, die sich f\u00fcr DICH entscheiden,", "tokens": ["Schenk", "Her\u00b7zen", "mir", ",", "die", "sich", "f\u00fcr", "DiCH", "ent\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "$,", "PRELS", "PRF", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so wird auch meines wahrhaft fr\u00f6hlich sein.", "tokens": ["so", "wird", "auch", "mei\u00b7nes", "wahr\u00b7haft", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}