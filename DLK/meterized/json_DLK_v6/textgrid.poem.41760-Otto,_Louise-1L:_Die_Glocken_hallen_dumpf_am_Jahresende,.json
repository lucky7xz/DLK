{"textgrid.poem.41760": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Glocken hallen dumpf am Jahresende,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Glocken hallen dumpf am Jahresende,", "tokens": ["Die", "Glo\u00b7cken", "hal\u00b7len", "dumpf", "am", "Jah\u00b7re\u00b7sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In diesen schweren unheilvollen Zeiten", "tokens": ["In", "die\u00b7sen", "schwe\u00b7ren", "un\u00b7heil\u00b7vol\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ins Grab die deutsche Freiheit zu geleiten \u2013", "tokens": ["Ins", "Grab", "die", "deut\u00b7sche", "Frei\u00b7heit", "zu", "ge\u00b7lei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach! ohne Hoffnung da\u00df ihr Los sich wende!", "tokens": ["Ach", "!", "oh\u00b7ne", "Hoff\u00b7nung", "da\u00df", "ihr", "Los", "sich", "wen\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "NN", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gef\u00e4ngni\u00df, Flucht und Tod \u2013 das ist die Spende", "tokens": ["Ge\u00b7f\u00e4ng\u00b7ni\u00df", ",", "Flucht", "und", "Tod", "\u2013", "das", "ist", "die", "Spen\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "$(", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr Alle, die dem Vaterland sich weihten,", "tokens": ["F\u00fcr", "Al\u00b7le", ",", "die", "dem", "Va\u00b7ter\u00b7land", "sich", "weih\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Volke Recht und Einheit zu erstreiten,", "tokens": ["Dem", "Vol\u00b7ke", "Recht", "und", "Ein\u00b7heit", "zu", "er\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df es zu einem Reiche sich verb\u00e4nde!", "tokens": ["Da\u00df", "es", "zu", "ei\u00b7nem", "Rei\u00b7che", "sich", "ver\u00b7b\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NE", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und doch, und doch! \u2013 Die Freiheit kann nicht sterben", "tokens": ["Und", "doch", ",", "und", "doch", "!", "\u2013", "Die", "Frei\u00b7heit", "kann", "nicht", "ster\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KON", "ADV", "$.", "$(", "ART", "NN", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Volk, das sich so opferfroh gezeigt,", "tokens": ["Ein", "Volk", ",", "das", "sich", "so", "op\u00b7fer\u00b7froh", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kann nicht f\u00fcr immer, kann nicht ganz verderben!", "tokens": ["Kann", "nicht", "f\u00fcr", "im\u00b7mer", ",", "kann", "nicht", "ganz", "ver\u00b7der\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "ADV", "$,", "VMFIN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn auch jetzt der Hoffnung Saat verbl\u00fcht \u2013", "tokens": ["Und", "wenn", "auch", "jetzt", "der", "Hoff\u00b7nung", "Saat", "ver\u00b7bl\u00fcht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir s\u00e4ten doch \u2013 das Volk wird einst noch erben", "tokens": ["Wir", "s\u00e4\u00b7ten", "doch", "\u2013", "das", "Volk", "wird", "einst", "noch", "er\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ART", "NN", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um was wir k\u00e4mpfen und noch nicht erreicht.", "tokens": ["Um", "was", "wir", "k\u00e4mp\u00b7fen", "und", "noch", "nicht", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "KON", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}