{"textgrid.poem.48697": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18. Er hat Alles wol gemacht", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja mehr als wol gemacht! Nicht Tauben nur und Blinden", "tokens": ["Ja", "mehr", "als", "wol", "ge\u00b7macht", "!", "Nicht", "Tau\u00b7ben", "nur", "und", "Blin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PIAT", "KOKOM", "ADV", "VVPP", "$.", "PTKNEG", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und was ein kranker Leib f\u00fcr Mangel haben kan,", "tokens": ["und", "was", "ein", "kran\u00b7ker", "Leib", "f\u00fcr", "Man\u00b7gel", "ha\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "APPR", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hilft dieser Wunder-Arzt. Es trifft was H\u00f6hers an,", "tokens": ["hilft", "die\u00b7ser", "Wun\u00b7der\u00b7A\u00b7rzt", ".", "Es", "trifft", "was", "H\u00f6\u00b7hers", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$.", "PPER", "VVFIN", "PIS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als ein naturlichs Weh, die Glieder zu entbinden,", "tokens": ["als", "ein", "na\u00b7tur\u00b7lichs", "Weh", ",", "die", "Glie\u00b7der", "zu", "ent\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "ja mehr auch als den Tod. Der Staar der blinden S\u00fcnden,", "tokens": ["ja", "mehr", "auch", "als", "den", "Tod", ".", "Der", "Staar", "der", "blin\u00b7den", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KOUS", "ART", "NN", "$.", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das Band der tauben Lust, der Hoffart stummer Wahn", "tokens": ["das", "Band", "der", "tau\u00b7ben", "Lust", ",", "der", "Hof\u00b7fart", "stum\u00b7mer", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wird sonst durch Keinen nicht, als diesen, abgetan.", "tokens": ["wird", "sonst", "durch", "Kei\u00b7nen", "nicht", ",", "als", "die\u00b7sen", ",", "ab\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "PTKNEG", "$,", "KOUS", "PDAT", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Leibarzt wird sich so zu heilen unterwinden", "tokens": ["Kein", "Leib\u00b7arzt", "wird", "sich", "so", "zu", "hei\u00b7len", "un\u00b7ter\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PRF", "ADV", "PTKZU", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "die Seele, die ist krank. Dem Geiste wird vergeben,", "tokens": ["die", "See\u00b7le", ",", "die", "ist", "krank", ".", "Dem", "Geis\u00b7te", "wird", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "er trinkt den Kelch f\u00fcr uns, stirbt selbst f\u00fcr unser Leben,", "tokens": ["er", "trinkt", "den", "Kelch", "f\u00fcr", "uns", ",", "stirbt", "selbst", "f\u00fcr", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zerbricht der H\u00f6llen Burg und was den Tod verwacht.", "tokens": ["zer\u00b7bricht", "der", "H\u00f6l\u00b7len", "Burg", "und", "was", "den", "Tod", "ver\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "PWS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "schleu\u00dft unser' Gr\u00e4ber auf, wird selbst die Himmelsleiter,", "tokens": ["schleu\u00dft", "un\u00b7ser'", "Gr\u00e4\u00b7ber", "auf", ",", "wird", "selbst", "die", "Him\u00b7mels\u00b7lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ja, selbst der Himmel gar. Ruft l\u00e4uter, rufet weiter:", "tokens": ["ja", ",", "selbst", "der", "Him\u00b7mel", "gar", ".", "Ruft", "l\u00e4u\u00b7ter", ",", "ru\u00b7fet", "wei\u00b7ter", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "ADV", "$.", "VVFIN", "ADJD", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er, er hat Alles wol und mehr als wol gemacht!", "tokens": ["Er", ",", "er", "hat", "Al\u00b7les", "wol", "und", "mehr", "als", "wol", "ge\u00b7macht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VAFIN", "PIS", "ADV", "KON", "PIAT", "KOKOM", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}