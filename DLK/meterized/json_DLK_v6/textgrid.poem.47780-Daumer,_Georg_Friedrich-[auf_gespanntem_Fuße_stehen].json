{"textgrid.poem.47780": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[auf gespanntem Fu\u00dfe stehen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf gespanntem Fu\u00dfe stehen", "tokens": ["Auf", "ge\u00b7spann\u00b7tem", "Fu\u00b7\u00dfe", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit einander Welt und ich;", "tokens": ["Mit", "ein\u00b7an\u00b7der", "Welt", "und", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "NN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebst du deinen guten Namen,", "tokens": ["Liebst", "du", "dei\u00b7nen", "gu\u00b7ten", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber Freund, so meide mich!", "tokens": ["Lie\u00b7ber", "Freund", ",", "so", "mei\u00b7de", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wofern du aufgek\u00fcndigt", "tokens": ["Doch", "wo\u00b7fern", "du", "auf\u00b7ge\u00b7k\u00fcn\u00b7digt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener alten Vettel hast,", "tokens": ["Je\u00b7ner", "al\u00b7ten", "Vet\u00b7tel", "hast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O so komm in diese Rosen,", "tokens": ["O", "so", "komm", "in", "die\u00b7se", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu Hafisen lagre dich!", "tokens": ["Zu", "Ha\u00b7fi\u00b7sen", "la\u00b7gre", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wunderkl\u00e4nge wirst du h\u00f6ren,", "tokens": ["Wun\u00b7der\u00b7kl\u00e4n\u00b7ge", "wirst", "du", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlen nie gekannte Lust,", "tokens": ["F\u00fch\u00b7len", "nie", "ge\u00b7kann\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn von Liedern tr\u00e4uft Hafisens", "tokens": ["Denn", "von", "Lie\u00b7dern", "tr\u00e4uft", "Ha\u00b7fi\u00b7sens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trunkne Lippe wonniglich.", "tokens": ["Trunk\u00b7ne", "Lip\u00b7pe", "won\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sohre selbst verm\u00e4hlet ihnen", "tokens": ["Soh\u00b7re", "selbst", "ver\u00b7m\u00e4h\u00b7let", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den erhabnen Lautenschlag;", "tokens": ["Den", "er\u00b7hab\u00b7nen", "Lau\u00b7ten\u00b7schlag", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um ihn her im Sph\u00e4rentanze", "tokens": ["Um", "ihn", "her", "im", "Sph\u00e4\u00b7ren\u00b7tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drehen alle Himmel sich.", "tokens": ["Dre\u00b7hen", "al\u00b7le", "Him\u00b7mel", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}