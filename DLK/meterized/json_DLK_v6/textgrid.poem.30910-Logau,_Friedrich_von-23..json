{"textgrid.poem.30910": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nullus ist ein Zwerg von Leibe, noch dazu ein Narr von Sinnen;", "tokens": ["Nul\u00b7lus", "ist", "ein", "Zwerg", "von", "Lei\u00b7be", ",", "noch", "da\u00b7zu", "ein", "Narr", "von", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "ADV", "PAV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Also wird man Nichts den Nullum, Nullum Nichts benamen k\u00fcnnen.", "tokens": ["Al\u00b7so", "wird", "man", "Nichts", "den", "Nul\u00b7lum", ",", "Nul\u00b7lum", "Nichts", "be\u00b7na\u00b7men", "k\u00fcn\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PIS", "ART", "NE", "$,", "ADV", "PIS", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}