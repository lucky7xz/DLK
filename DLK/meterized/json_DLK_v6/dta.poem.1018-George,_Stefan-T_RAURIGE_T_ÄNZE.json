{"dta.poem.1018": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr tratet zu dem herde               ", "tokens": ["Ihr", "tra\u00b7tet", "zu", "dem", "her\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo alle glut verstarb               ", "tokens": ["Wo", "al\u00b7le", "glut", "ver\u00b7starb"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Licht war nur an der erde               ", "tokens": ["Licht", "war", "nur", "an", "der", "er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Vom monde leichenfarb             ", "tokens": ["Vom", "mon\u00b7de", "lei\u00b7chen\u00b7farb"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ihr tauchtet in die aschen               ", "tokens": ["Ihr", "tauch\u00b7tet", "in", "die", "asc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die bleichen finger ein               ", "tokens": ["Die", "blei\u00b7chen", "fin\u00b7ger", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit suchen tasten haschen &#8212;               ", "tokens": ["Mit", "su\u00b7chen", "tas\u00b7ten", "ha\u00b7schen", "&#8212;"], "token_info": ["word", "word", "word", "word", "XML_entity"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird es noch einmal schein!             ", "tokens": ["Wird", "es", "noch", "ein\u00b7mal", "schein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Seht was mit trostgeberde               ", "tokens": ["Seht", "was", "mit", "trost\u00b7ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der mond euch r&#228;t:               ", "tokens": ["Der", "mond", "euch", "r", "&#228;", "t", ":"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ART", "NN", "PPER", "NE", "$(", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Tretet weg vom herde               ", "tokens": ["Tre\u00b7tet", "weg", "vom", "her\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Es ist worden sp&#228;t.             ", "tokens": ["Es", "ist", "wor\u00b7den", "sp", "&#228;", "t."], "token_info": ["word", "word", "word", "word", "XML_entity", "abbreviation"], "pos": ["PPER", "VAFIN", "VAPP", "XY", "XY", "XY"], "meter": "--+-+", "measure": "anapaest.init"}}}}}