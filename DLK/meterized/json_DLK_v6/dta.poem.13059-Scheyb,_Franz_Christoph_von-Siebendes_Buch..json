{"dta.poem.13059": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eersinnet was ihr wollt, erfindet eine Pracht!", "tokens": ["\u201e", "er\u00b7sin\u00b7net", "was", "ihr", "wollt", ",", "er\u00b7fin\u00b7det", "ei\u00b7ne", "Pracht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PWS", "PPER", "VMFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201egewi\u00df ists, da\u00df ihr euch vergebne M\u00fche macht:", "tokens": ["\u201e", "ge\u00b7wi\u00df", "ists", ",", "da\u00df", "ihr", "euch", "ver\u00b7geb\u00b7ne", "M\u00fc\u00b7he", "macht", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "$,", "KOUS", "PPER", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ejhr, die ", "tokens": ["\u201e", "jhr", ",", "die"], "token_info": ["punct", "word", "punct", "word"], "pos": ["$(", "PPER", "$,", "PRELS"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "\u201ejhr, derer h\u00f6chster Glanz von ihrem Licht herr\u00fchret.", "tokens": ["\u201e", "jhr", ",", "de\u00b7rer", "h\u00f6chs\u00b7ter", "Glanz", "von", "ih\u00b7rem", "Licht", "her\u00b7r\u00fch\u00b7ret", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PDS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}