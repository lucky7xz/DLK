{"textgrid.poem.62603": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Wie der Adler vor dem Donner nicht erzittert und erbla\u00dft", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Adler vor dem Donner nicht erzittert und erbla\u00dft;", "tokens": ["Wie", "der", "Ad\u00b7ler", "vor", "dem", "Don\u00b7ner", "nicht", "er\u00b7zit\u00b7tert", "und", "er\u00b7bla\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "PTKNEG", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Also ist der Tod den Frommen ein h\u00f6chst angenehmer Gast.", "tokens": ["Al\u00b7so", "ist", "der", "Tod", "den", "From\u00b7men", "ein", "h\u00f6chst", "an\u00b7ge\u00b7neh\u00b7mer", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Er erzittert nicht vor ihm; er erschrickt nicht vor der H\u00f6lle:", "tokens": ["Er", "er\u00b7zit\u00b7tert", "nicht", "vor", "ihm", ";", "er", "er\u00b7schrickt", "nicht", "vor", "der", "H\u00f6l\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Er betritt mit Freudigkeit die gew\u00fcnschte Grabes-Schwelle.", "tokens": ["Er", "be\u00b7tritt", "mit", "Freu\u00b7dig\u00b7keit", "die", "ge\u00b7w\u00fcnschte", "Gra\u00b7bes\u00b7Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}