{"dta.poem.13302": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewir werden Freud und Zanck im Strohm der Wellen sp\u00fchren,", "tokens": ["\u201e", "wir", "wer\u00b7den", "Freud", "und", "Zanck", "im", "Strohm", "der", "Wel\u00b7len", "sp\u00fch\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NN", "KON", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edas Schiff an das Gestad in Sicherheit zu f\u00fchren.", "tokens": ["\u201e", "das", "Schiff", "an", "das", "Ge\u00b7stad", "in", "Si\u00b7cher\u00b7heit", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eder Anmuths-volle Trieb, der allerj\u00fcngste Wind", "tokens": ["\u201e", "der", "An\u00b7muths\u00b7vol\u00b7le", "Trieb", ",", "der", "al\u00b7ler\u00b7j\u00fcngs\u00b7te", "Wind"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewird Sanftmuth, Freundlichkeit, gelassen und gelind", "tokens": ["\u201e", "wird", "Sanft\u00b7muth", ",", "Freund\u00b7lich\u00b7keit", ",", "ge\u00b7las\u00b7sen", "und", "ge\u00b7lind"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VAFIN", "NN", "$,", "NN", "$,", "VVPP", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "235\u201dVon allen Seiten her in unsre Seegel blasen;", "tokens": ["\"", "Von", "al\u00b7len", "Sei\u00b7ten", "her", "in", "uns\u00b7re", "See\u00b7gel", "bla\u00b7sen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edas Volck f\u00fcr Fr\u00f6hlichkeit bey diesem Anblick rasen.", "tokens": ["\u201e", "das", "Volck", "f\u00fcr", "Fr\u00f6h\u00b7lich\u00b7keit", "bey", "die\u00b7sem", "An\u00b7blick", "ra\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edie\u00df wird f\u00fcr Lust entz\u00fcckt an allen Orten stehn,", "tokens": ["\u201e", "die\u00df", "wird", "f\u00fcr", "Lust", "ent\u00b7z\u00fcckt", "an", "al\u00b7len", "Or\u00b7ten", "stehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "APPR", "NN", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eauf welchen man den Flu\u00df und dieses Schiff kann sehn.", "tokens": ["\u201e", "auf", "wel\u00b7chen", "man", "den", "Flu\u00df", "und", "die\u00b7ses", "Schiff", "kann", "sehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWAT", "PIS", "ART", "NN", "KON", "PDAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201emir scheints, ich h\u00f6re schon viel tausend Zungen schreyen:", "tokens": ["\u201e", "mir", "scheints", ",", "ich", "h\u00f6\u00b7re", "schon", "viel", "tau\u00b7send", "Zun\u00b7gen", "schre\u00b7yen", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.10": {"text": "240\u201dDie Last des Schiffs wird uns der Last des Kriegs befreyen.", "tokens": ["\"", "Die", "Last", "des", "Schiffs", "wird", "uns", "der", "Last", "des", "Kriegs", "be\u00b7fre\u00b7yen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}