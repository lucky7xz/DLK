{"textgrid.poem.61586": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df holde Jugend nur zur Liebe tauge,", "tokens": ["Da\u00df", "hol\u00b7de", "Ju\u00b7gend", "nur", "zur", "Lie\u00b7be", "tau\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df es wohl, und da\u00df mein Lenz entschwand;", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "und", "da\u00df", "mein", "Lenz", "ent\u00b7schwand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch sehn' ich mich nach einem treuen Auge,", "tokens": ["Doch", "sehn'", "ich", "mich", "nach", "ei\u00b7nem", "treu\u00b7en", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch sehn' ich mich nach einer wei\u00dfen Hand.", "tokens": ["Doch", "sehn'", "ich", "mich", "nach", "ei\u00b7ner", "wei\u00b7\u00dfen", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nach einem Auge, das mit hellerm Scheine", "tokens": ["Nach", "ei\u00b7nem", "Au\u00b7ge", ",", "das", "mit", "hel\u00b7lerm", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufleuchte, wenn mein Tiefstes ich enth\u00fcllt,", "tokens": ["Auf\u00b7leuch\u00b7te", ",", "wenn", "mein", "Tiefs\u00b7tes", "ich", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und das in jenen b\u00e4ngsten Stunden weine,", "tokens": ["Und", "das", "in", "je\u00b7nen", "b\u00e4ngs\u00b7ten", "Stun\u00b7den", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo meines sich nicht mehr mit Tr\u00e4nen f\u00fcllt;", "tokens": ["Wo", "mei\u00b7nes", "sich", "nicht", "mehr", "mit", "Tr\u00e4\u00b7nen", "f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "PRF", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nach einer Hand, die hier und dort am Wege", "tokens": ["Nach", "ei\u00b7ner", "Hand", ",", "die", "hier", "und", "dort", "am", "We\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir einen Zweig noch pfl\u00fccke, herbstesfarb,", "tokens": ["Mir", "ei\u00b7nen", "Zweig", "noch", "pfl\u00fc\u00b7cke", ",", "herbs\u00b7tes\u00b7farb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die mir zum Rasten weich die Kissen lege", "tokens": ["Die", "mir", "zum", "Ras\u00b7ten", "weich", "die", "Kis\u00b7sen", "le\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mir die Wimpern schlie\u00dfe, wenn ich starb.", "tokens": ["Und", "mir", "die", "Wim\u00b7pern", "schlie\u00b7\u00dfe", ",", "wenn", "ich", "starb", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}