{"textgrid.poem.47261": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Windstille", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Windstill ist es auf der Flut,", "tokens": ["Wind\u00b7still", "ist", "es", "auf", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Windstill ist's im Hage,", "tokens": ["Wind\u00b7still", "ist's", "im", "Ha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Windstill ist es mir zu Mut'", "tokens": ["Wind\u00b7still", "ist", "es", "mir", "zu", "Mut'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "PPER", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dem Fr\u00fchlingstage.", "tokens": ["Wie", "dem", "Fr\u00fch\u00b7lings\u00b7ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Eine leise, leise Luft", "tokens": ["Ei\u00b7ne", "lei\u00b7se", ",", "lei\u00b7se", "Luft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4chelt und erfrischet", "tokens": ["F\u00e4\u00b7chelt", "und", "er\u00b7fri\u00b7schet"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jede Blume, deren Duft", "tokens": ["Je\u00b7de", "Blu\u00b7me", ",", "de\u00b7ren", "Duft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich in andre mischet.", "tokens": ["Sich", "in", "and\u00b7re", "mi\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIS", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und in meinem Busen auch,", "tokens": ["Und", "in", "mei\u00b7nem", "Bu\u00b7sen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Sturm sich leget,", "tokens": ["Wo", "der", "Sturm", "sich", "le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchl' ich einen Himmelshauch,", "tokens": ["F\u00fchl'", "ich", "ei\u00b7nen", "Him\u00b7mels\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mich sanft beweget.", "tokens": ["Der", "mich", "sanft", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Meinen Odem m\u00f6cht' ich rein", "tokens": ["Mei\u00b7nen", "O\u00b7dem", "m\u00f6cht'", "ich", "rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem der Blumen mischen,", "tokens": ["Dem", "der", "Blu\u00b7men", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und wie sie so trunken sein", "tokens": ["Und", "wie", "sie", "so", "trun\u00b7ken", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "VAINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von des Taues Frischen.", "tokens": ["Von", "des", "Tau\u00b7es", "Fri\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}