{"textgrid.poem.49438": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als schlichter Waise, reich genug", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als schlichter Waise, reich genug", "tokens": ["Als", "schlich\u00b7ter", "Wai\u00b7se", ",", "reich", "ge\u00b7nug"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "$,", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An meiner Augen stillem Scheine,", "tokens": ["An", "mei\u00b7ner", "Au\u00b7gen", "stil\u00b7lem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kam ich zur Stadt, fremd und alleine,", "tokens": ["Kam", "ich", "zur", "Stadt", ",", "fremd", "und", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die M\u00e4nner fanden mich nicht klug.", "tokens": ["Die", "M\u00e4n\u00b7ner", "fan\u00b7den", "mich", "nicht", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit zwanzig Jahren wurde ich", "tokens": ["Mit", "zwan\u00b7zig", "Jah\u00b7ren", "wur\u00b7de", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Feuer der verliebten Sinne", "tokens": ["Im", "Feu\u00b7er", "der", "ver\u00b7lieb\u00b7ten", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Weiber s\u00fcsser Sch\u00f6nheit inne:", "tokens": ["Der", "Wei\u00b7ber", "s\u00fcs\u00b7ser", "Sch\u00f6n\u00b7heit", "in\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch freilich sch\u00f6n fand keine mich.", "tokens": ["Doch", "frei\u00b7lich", "sch\u00f6n", "fand", "kei\u00b7ne", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PIAT", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn auch in keines K\u00f6nigs Sold,", "tokens": ["Wenn", "auch", "in", "kei\u00b7nes", "K\u00f6\u00b7nigs", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich Heimatloser Ruhm erworben,", "tokens": ["Ich", "Hei\u00b7mat\u00b7lo\u00b7ser", "Ruhm", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00e4r' gern ich doch im Krieg gestorben,", "tokens": ["W\u00e4r'", "gern", "ich", "doch", "im", "Krieg", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch hat der Tod mich nicht gewollt.", "tokens": ["Doch", "hat", "der", "Tod", "mich", "nicht", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "VMPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kam ich zu fr\u00fch, kam ich zu sp\u00e4t", "tokens": ["Kam", "ich", "zu", "fr\u00fch", ",", "kam", "ich", "zu", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PTKA", "ADJD", "$,", "VVFIN", "PPER", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diese Welt voll herber Trauer?", "tokens": ["In", "die\u00b7se", "Welt", "voll", "her\u00b7ber", "Trau\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was soll mir, ach, des Lebens Dauer?", "tokens": ["Was", "soll", "mir", ",", "ach", ",", "des", "Le\u00b7bens", "Dau\u00b7er", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "ITJ", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denkt an mich Armen im Gebet!", "tokens": ["Denkt", "an", "mich", "Ar\u00b7men", "im", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "APPRART", "NN", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}