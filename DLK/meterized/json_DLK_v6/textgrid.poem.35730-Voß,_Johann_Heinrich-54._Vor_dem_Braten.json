{"textgrid.poem.35730": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "54. Vor dem Braten", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehr willkommen, lieber Hase,", "tokens": ["Sehr", "will\u00b7kom\u00b7men", ",", "lie\u00b7ber", "Ha\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehr willkommen bist du heut;", "tokens": ["Sehr", "will\u00b7kom\u00b7men", "bist", "du", "heut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer duckst du mehr im Grase;", "tokens": ["Nim\u00b7mer", "duckst", "du", "mehr", "im", "Gra\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle wir mit vollem Glase", "tokens": ["Al\u00b7le", "wir", "mit", "vol\u00b7lem", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4uten dir das Grabgel\u00e4ut!", "tokens": ["L\u00e4u\u00b7ten", "dir", "das", "Grab\u00b7ge\u00b7l\u00e4ut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein schon harrt die gro\u00dfe Gabel,", "tokens": ["Dein", "schon", "harrt", "die", "gro\u00b7\u00dfe", "Ga\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das gro\u00dfe Messer dein!", "tokens": ["Und", "das", "gro\u00b7\u00dfe", "Mes\u00b7ser", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald zerhackt dich's, wie ein Sabel,", "tokens": ["Bald", "zer\u00b7hackt", "dich's", ",", "wie", "ein", "Sa\u00b7bel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00f6sewicht! und unsern Schnabel", "tokens": ["B\u00f6\u00b7se\u00b7wicht", "!", "und", "un\u00b7sern", "Schna\u00b7bel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll dein saftig Fleisch erfreun!", "tokens": ["Soll", "dein", "saf\u00b7tig", "Fleisch", "er\u00b7freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ha! dich fing der gute Bauer,", "tokens": ["Ha", "!", "dich", "fing", "der", "gu\u00b7te", "Bau\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem du oft den Kohl geraubt.", "tokens": ["Dem", "du", "oft", "den", "Kohl", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abends stand er auf der Lauer:", "tokens": ["A\u00b7bends", "stand", "er", "auf", "der", "Lau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm nur! sprach er; meinst du, Schlauer,", "tokens": ["Komm", "nur", "!", "sprach", "er", ";", "meinst", "du", ",", "Schlau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was dem Reh, sei dir erlaubt?", "tokens": ["Was", "dem", "Reh", ",", "sei", "dir", "er\u00b7laubt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hirsch' und Rehe k\u00f6nnen grasen,", "tokens": ["Hirsch'", "und", "Re\u00b7he", "k\u00f6n\u00b7nen", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo nur was zu grasen ist;", "tokens": ["Wo", "nur", "was", "zu", "gra\u00b7sen", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie auch mein Korn durchrasen!", "tokens": ["Wenn", "sie", "auch", "mein", "Korn", "durch\u00b7ra\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Anders, wenn ein Schelm von Hasen", "tokens": ["An\u00b7ders", ",", "wenn", "ein", "Schelm", "von", "Ha\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir den Winterkohl zerfri\u00dft!", "tokens": ["Mir", "den", "Win\u00b7ter\u00b7kohl", "zer\u00b7fri\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Endlich h\u00e4lt dich schlauen Rammler", "tokens": ["End\u00b7lich", "h\u00e4lt", "dich", "schlau\u00b7en", "Ramm\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fest am Hinterlauf die Schnur!", "tokens": ["Fest", "am", "Hin\u00b7ter\u00b7lauf", "die", "Schnur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ah, du wackrer Kr\u00e4utersammler,", "tokens": ["Ah", ",", "du", "wack\u00b7rer", "Kr\u00e4u\u00b7ter\u00b7samm\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Streckst die L\u00f6ffel? Sei kein Dammler!", "tokens": ["Streckst", "die", "L\u00f6f\u00b7fel", "?", "Sei", "kein", "Damm\u00b7ler", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du mu\u00dft her! Ja quiecke nur!", "tokens": ["Du", "mu\u00dft", "her", "!", "Ja", "qui\u00b7e\u00b7cke", "nur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKVZ", "$.", "PTKANT", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drohn auch Br\u00fcch' und Nackenschl\u00e4ge,", "tokens": ["Drohn", "auch", "Br\u00fcch'", "und", "Na\u00b7cken\u00b7schl\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn dich hier der F\u00f6rster sp\u00fcrt;", "tokens": ["Wenn", "dich", "hier", "der", "F\u00f6rs\u00b7ter", "sp\u00fcrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was er droht, hat gute Wege!", "tokens": ["Was", "er", "droht", ",", "hat", "gu\u00b7te", "We\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00e4hl' er selbst mir im Gehege,", "tokens": ["St\u00e4hl'", "er", "selbst", "mir", "im", "Ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Traun er w\u00fcrde selbst geschn\u00fcrt!", "tokens": ["Traun", "er", "w\u00fcr\u00b7de", "selbst", "ge\u00b7schn\u00fcrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}