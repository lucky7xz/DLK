{"textgrid.poem.58042": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "80. Nimm alles von Gott an!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast allein mit Gott zu tun, was immer mag geschehen,", "tokens": ["Du", "hast", "al\u00b7lein", "mit", "Gott", "zu", "tun", ",", "was", "im\u00b7mer", "mag", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Du mu\u00dft auf Kreaturen nicht und auf das \u00c4u\u00dfre sehen;", "tokens": ["Du", "mu\u00dft", "auf", "Kre\u00b7a\u00b7tu\u00b7ren", "nicht", "und", "auf", "das", "\u00c4u\u00df\u00b7re", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Was Gott f\u00fcr Mittel je gebraucht, das gehet dich nicht an,", "tokens": ["Was", "Gott", "f\u00fcr", "Mit\u00b7tel", "je", "ge\u00b7braucht", ",", "das", "ge\u00b7het", "dich", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "NN", "ADV", "VVPP", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Was dir begegnet in der Welt, denk stets: Gott hat's getan!", "tokens": ["Was", "dir", "be\u00b7geg\u00b7net", "in", "der", "Welt", ",", "denk", "stets", ":", "Gott", "hat's", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "$.", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}