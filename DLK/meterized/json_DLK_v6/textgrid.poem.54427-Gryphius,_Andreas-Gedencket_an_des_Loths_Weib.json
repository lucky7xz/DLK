{"textgrid.poem.54427": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Gedencket an des Loths Weib", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh/ der gerechte Gott mit Plitz vnd Schweffel-Regen/", "tokens": ["Eh", "/", "der", "ge\u00b7rech\u00b7te", "Gott", "mit", "Plitz", "vnd", "Schwef\u00b7fel\u00b7Re\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ADJA", "NN", "APPR", "NE", "KON", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mit Fewr/ Pech/ Sturm/ vnnd Grim hat Sodom vmbgekehrt;", "tokens": ["Mit", "Fewr", "/", "Pech", "/", "Sturm", "/", "vnnd", "Grim", "hat", "So\u00b7dom", "vmb\u00b7ge\u00b7kehrt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NE", "$(", "NN", "$(", "KON", "NE", "VAFIN", "NE", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Eh Erd vnd Himmel kracht/ vor seines Eyfers Schwerdt;", "tokens": ["Eh", "Erd", "vnd", "Him\u00b7mel", "kracht", "/", "vor", "sei\u00b7nes", "Ey\u00b7fers", "Schwerdt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eyde\u00f1 er Zeboim lie\u00df in die asche leg\u1ebd", "tokens": ["Ey\u00b7de\u00f1", "er", "Ze\u00b7bo\u00b7im", "lie\u00df", "in", "die", "asc\u00b7he", "leg\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Vnd die erhitzte Lufft erklang von Donner-schl\u00e4gen;", "tokens": ["Vnd", "die", "er\u00b7hitz\u00b7te", "Lufft", "er\u00b7klang", "von", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Eh Er auff Adamah sein R\u00fcst-Hau\u00df au\u00dfgeleert/", "tokens": ["Eh", "Er", "auff", "A\u00b7da\u00b7mah", "sein", "R\u00fcst\u00b7Hau\u00df", "au\u00df\u00b7ge\u00b7leert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Ehdenn Gomorra gantz von flammen war verzehrt/", "tokens": ["Eh\u00b7denn", "Go\u00b7mor\u00b7ra", "gantz", "von", "flam\u00b7men", "war", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "APPR", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Must Loth mit Weib vnd Kind von dannen sich bewegen/", "tokens": ["Must", "Loth", "mit", "Weib", "vnd", "Kind", "von", "dan\u00b7nen", "sich", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "KON", "NN", "APPR", "ADV", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vnd hinter sich nicht sehn; Al\u00df aber sein Gemahl", "tokens": ["Vnd", "hin\u00b7ter", "sich", "nicht", "sehn", ";", "Al\u00df", "a\u00b7ber", "sein", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "PTKNEG", "VVINF", "$.", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit vmbgewandtem Aug anschawt/ wie vberall", "tokens": ["Mit", "vmb\u00b7ge\u00b7wand\u00b7tem", "Aug", "an\u00b7schawt", "/", "wie", "vbe\u00b7rall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "KOKOM", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Mit Glutt vnd lichter Loh die H\u00e4user stehn gekr\u00f6net/", "tokens": ["Mit", "Glutt", "vnd", "lich\u00b7ter", "Loh", "die", "H\u00e4u\u00b7ser", "stehn", "ge\u00b7kr\u00f6\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "ART", "NN", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "F\u00fchlt sie/ da\u00df Thr\u00e4nen-saltz au\u00df Ihren Augen rinnt/", "tokens": ["F\u00fchlt", "sie", "/", "da\u00df", "Thr\u00e4\u00b7nen\u00b7saltz", "au\u00df", "Ih\u00b7ren", "Au\u00b7gen", "rinnt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erstarrt auch bald in Saltz/ eh sie sich recht vorsint/", "tokens": ["Er\u00b7starrt", "auch", "bald", "in", "Saltz", "/", "eh", "sie", "sich", "recht", "vor\u00b7sint", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NE", "$(", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So wird mit weiser Straff Ihr Torheit au\u00dfges\u00f6hnet.", "tokens": ["So", "wird", "mit", "wei\u00b7ser", "Straff", "Ihr", "Tor\u00b7heit", "au\u00df\u00b7ge\u00b7s\u00f6h\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}