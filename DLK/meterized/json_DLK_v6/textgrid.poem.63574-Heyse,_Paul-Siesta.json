{"textgrid.poem.63574": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Siesta", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb, o lieb war die Nacht", "tokens": ["Lieb", ",", "o", "lieb", "war", "die", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "ADJD", "VAFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mitten am hellen Tag,", "tokens": ["Mit\u00b7ten", "am", "hel\u00b7len", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Als wir die L\u00e4den geschlossen,", "tokens": ["Als", "wir", "die", "L\u00e4\u00b7den", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als durch die sch\u00fctzenden Sprossen", "tokens": ["Als", "durch", "die", "sch\u00fct\u00b7zen\u00b7den", "Spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Goldige D\u00e4mmerung brach.", "tokens": ["Gol\u00b7di\u00b7ge", "D\u00e4m\u00b7me\u00b7rung", "brach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "K\u00fchl, o k\u00fchl war der Saal,", "tokens": ["K\u00fchl", ",", "o", "k\u00fchl", "war", "der", "Saal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Drinnen die Welt uns verging,", "tokens": ["Drin\u00b7nen", "die", "Welt", "uns", "ver\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da wir in seligem Schmachten", "tokens": ["Da", "wir", "in", "se\u00b7li\u00b7gem", "Schmach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wandelten, fl\u00fcsterten, lachten,", "tokens": ["Wan\u00b7del\u00b7ten", ",", "fl\u00fcs\u00b7ter\u00b7ten", ",", "lach\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Bis uns der Schlummer umfing.", "tokens": ["Bis", "uns", "der", "Schlum\u00b7mer", "um\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00df, o s\u00fc\u00df war der Traum,", "tokens": ["S\u00fc\u00df", ",", "o", "s\u00fc\u00df", "war", "der", "Traum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Herz am Herzen getr\u00e4umt!", "tokens": ["Herz", "am", "Her\u00b7zen", "ge\u00b7tr\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u00dcber uns schwebend im Kreise", "tokens": ["\u00dc\u00b7ber", "uns", "schwe\u00b7bend", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJD", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Flattert' ein Schmetterling leise,", "tokens": ["Flat\u00b7tert'", "ein", "Schmet\u00b7ter\u00b7ling", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Dunkel die Schwingen ums\u00e4umt.", "tokens": ["Dun\u00b7kel", "die", "Schwin\u00b7gen", "um\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}