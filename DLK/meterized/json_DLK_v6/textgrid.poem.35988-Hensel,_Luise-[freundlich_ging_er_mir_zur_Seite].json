{"textgrid.poem.35988": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[freundlich ging er mir zur Seite]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundlich ging er mir zur Seite", "tokens": ["Freund\u00b7lich", "ging", "er", "mir", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Auen, licht und gr\u00fcn,", "tokens": ["\u00dc\u00b7ber", "Au\u00b7en", ",", "licht", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine wei\u00dfen H\u00fcndlein beide", "tokens": ["Sei\u00b7ne", "wei\u00b7\u00dfen", "H\u00fcnd\u00b7lein", "bei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcpften lustig um uns hin.", "tokens": ["H\u00fcpf\u00b7ten", "lus\u00b7tig", "um", "uns", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hell vom Ufer winkte dr\u00fcben", "tokens": ["Hell", "vom", "U\u00b7fer", "wink\u00b7te", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gar ein zarter, blauer Stern.", "tokens": ["Gar", "ein", "zar\u00b7ter", ",", "blau\u00b7er", "Stern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich stand und sagte leise:", "tokens": ["Und", "ich", "stand", "und", "sag\u00b7te", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbach, das Bl\u00fcmlein h\u00e4tt ich gern!\u00ab", "tokens": ["\u00bb", "ach", ",", "das", "Bl\u00fcm\u00b7lein", "h\u00e4tt", "ich", "gern", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und kaum sagt ich's, war behende", "tokens": ["Und", "kaum", "sagt", "ich's", ",", "war", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,", "VAFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch der J\u00e4ger dr\u00fcben schon,", "tokens": ["Auch", "der", "J\u00e4\u00b7ger", "dr\u00fc\u00b7ben", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich wu\u00dfte nicht zu w\u00e4hlen,", "tokens": ["Und", "ich", "wu\u00df\u00b7te", "nicht", "zu", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollt' ich bitten oder drohn.", "tokens": ["Sollt'", "ich", "bit\u00b7ten", "o\u00b7der", "drohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fr\u00f6hlich bracht er's mir getragen,", "tokens": ["Fr\u00f6h\u00b7lich", "bracht", "er's", "mir", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War so sch\u00f6n im Abendlicht.", "tokens": ["War", "so", "sch\u00f6n", "im", "A\u00b7bend\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundlich fragt ich, wie es hie\u00dfe,", "tokens": ["Freund\u00b7lich", "fragt", "ich", ",", "wie", "es", "hie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er sprach: Vergi\u00dfmeinnicht!", "tokens": ["Und", "er", "sprach", ":", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}