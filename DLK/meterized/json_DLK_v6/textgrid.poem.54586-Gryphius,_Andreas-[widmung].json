{"textgrid.poem.54586": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "[widmung]", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wofern durchlauchter Graff die \u00fcberheufte M\u00fch", "tokens": ["Wo\u00b7fern", "durch\u00b7lauch\u00b7ter", "Graff", "die", "\u00fc\u00b7berh\u00b7euf\u00b7te", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor unser aller Heill und Schlesiens Gedeyen", "tokens": ["Vor", "un\u00b7ser", "al\u00b7ler", "Heill", "und", "Schle\u00b7si\u00b7ens", "Ge\u00b7de\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Euch will der steten Sorg auff kurtze Zeitt befreyen", "tokens": ["Euch", "will", "der", "ste\u00b7ten", "Sorg", "auff", "kurt\u00b7ze", "Zeitt", "be\u00b7fre\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Der Sorgen das di\u00df Land nicht in der Angst verbl\u00fch", "tokens": ["Der", "Sor\u00b7gen", "das", "di\u00df", "Land", "nicht", "in", "der", "Angst", "ver\u00b7bl\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PDS", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So schawt doch gn\u00e4digst an was mit gesencktem Knie", "tokens": ["So", "schawt", "doch", "gn\u00e4\u00b7digst", "an", "was", "mit", "ge\u00b7senck\u00b7tem", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Euch meine ", "tokens": ["Euch", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["PPER", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und Laster sich erk\u00fchnt gro\u00dfm\u00fctig anzuspeyen", "tokens": ["Und", "Las\u00b7ter", "sich", "er\u00b7k\u00fchnt", "gro\u00df\u00b7m\u00fc\u00b7tig", "an\u00b7zus\u00b7pe\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PRF", "VVFIN", "ADJD", "VVIZU"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "G\u00f6nt da\u00df sie ewrem Schutz bey diser Thurst anflih", "tokens": ["G\u00f6nt", "da\u00df", "sie", "ew\u00b7rem", "Schutz", "bey", "di\u00b7ser", "Thurst", "an\u00b7flih"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wo ist numehr erlaubt die Tugend hoch zu sch\u00e4tzen", "tokens": ["Wo", "ist", "nu\u00b7mehr", "er\u00b7laubt", "die", "Tu\u00b7gend", "hoch", "zu", "sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "VVFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als wo die Tugend sich entschlossen fest zu setzen", "tokens": ["Als", "wo", "die", "Tu\u00b7gend", "sich", "ent\u00b7schlos\u00b7sen", "fest", "zu", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PWAV", "ART", "NN", "PRF", "VVPP", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo kommt man unverletzt den minsten Feilern bey", "tokens": ["Wo", "kommt", "man", "un\u00b7ver\u00b7letzt", "den", "mins\u00b7ten", "Fei\u00b7lern", "bey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PIS", "ADJD", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Damit ihr den erlaubt den Vorsatz au\u00dfzuf\u00fchren", "tokens": ["Da\u00b7mit", "ihr", "den", "er\u00b7laubt", "den", "Vor\u00b7satz", "au\u00df\u00b7zu\u00b7f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So last in ewrer Burg sie wie mich offt versp\u00fcren", "tokens": ["So", "last", "in", "ew\u00b7rer", "Burg", "sie", "wie", "mich", "offt", "ver\u00b7sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "PWAV", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df wo die Tugend wohn auch wahre Freyheit sey.", "tokens": ["Da\u00df", "wo", "die", "Tu\u00b7gend", "wohn", "auch", "wah\u00b7re", "Frey\u00b7heit", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}