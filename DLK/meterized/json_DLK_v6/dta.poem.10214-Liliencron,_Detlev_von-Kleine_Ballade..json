{"dta.poem.10214": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Kleine Ballade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch weht mein Busch, hell klirrt mein Schild             ", "tokens": ["Hoch", "weht", "mein", "Busch", ",", "hell", "klirrt", "mein", "Schild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Im Wolkenbruch der Feindesklingen.", "tokens": ["Im", "Wol\u00b7ken\u00b7bruch", "der", "Fein\u00b7des\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die malen kein Madonnenbild", "tokens": ["Die", "ma\u00b7len", "kein", "Ma\u00b7don\u00b7nen\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und t\u00f6nen nicht wie Harfensingen.", "tokens": ["Und", "t\u00f6\u00b7nen", "nicht", "wie", "Har\u00b7fen\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und in den Staub der letzte Schelm,", "tokens": ["Und", "in", "den", "Staub", "der", "letz\u00b7te", "Schelm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mich vom Sattel wollte stechen!", "tokens": ["Der", "mich", "vom", "Sat\u00b7tel", "woll\u00b7te", "ste\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schlug ihm Feuer in den Helm,", "tokens": ["Ich", "schlug", "ihm", "Feu\u00b7er", "in", "den", "Helm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sah ihn todt zusammenbrechen.", "tokens": ["Und", "sah", "ihn", "todt", "zu\u00b7sam\u00b7men\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr wolltet st\u00f6ren meinen Herd?", "tokens": ["Ihr", "woll\u00b7tet", "st\u00f6\u00b7ren", "mei\u00b7nen", "Herd", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich zeigte euch die Mannessehne.", "tokens": ["Ich", "zeig\u00b7te", "euch", "die", "Man\u00b7nes\u00b7seh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lachend trockne ich mein Schwert", "tokens": ["Und", "la\u00b7chend", "trock\u00b7ne", "ich", "mein", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meines Rosses schwarzer M\u00e4hne.", "tokens": ["An", "mei\u00b7nes", "Ros\u00b7ses", "schwar\u00b7zer", "M\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}