{"textgrid.poem.40273": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch als du dann gegangen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch als du dann gegangen,", "tokens": ["Doch", "als", "du", "dann", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "da hat sich mein Verlangen", "tokens": ["da", "hat", "sich", "mein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ganz aufgethan nach dir ...", "tokens": ["ganz", "auf\u00b7ge\u00b7than", "nach", "dir", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als sollt'ich dich verlieren,", "tokens": ["Als", "sollt'\u00b7ich", "dich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "sch\u00fcttelte ich mit irren", "tokens": ["sch\u00fct\u00b7tel\u00b7te", "ich", "mit", "ir\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Fingern deine verschlossne Th\u00fcr.", "tokens": ["Fin\u00b7gern", "dei\u00b7ne", "ver\u00b7schloss\u00b7ne", "Th\u00fcr."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Und durch die Nacht der Scheiben,", "tokens": ["Und", "durch", "die", "Nacht", "der", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ob du nicht w\u00fcrdest bleiben,", "tokens": ["ob", "du", "nicht", "w\u00fcr\u00b7dest", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "bettelten meine Augen, und \u2013", "tokens": ["bet\u00b7tel\u00b7ten", "mei\u00b7ne", "Au\u00b7gen", ",", "und", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du gingst hinauf die Stufen", "tokens": ["Du", "gingst", "hin\u00b7auf", "die", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und hast mich nicht gerufen,", "tokens": ["und", "hast", "mich", "nicht", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "mich nicht zur\u00fcck an deinen Mund.", "tokens": ["mich", "nicht", "zu\u00b7r\u00fcck", "an", "dei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vernahm nur noch mit stieren", "tokens": ["Ver\u00b7nahm", "nur", "noch", "mit", "stie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sinnen dein Schl\u00fcsselklirren", "tokens": ["Sin\u00b7nen", "dein", "Schl\u00fcs\u00b7sel\u00b7klir\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "im schwarzen Flur, und dann \u2013", "tokens": ["im", "schwar\u00b7zen", "Flur", ",", "und", "dann", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Traum ... bis die Schatten kamen,", "tokens": ["Traum", "...", "bis", "die", "Schat\u00b7ten", "ka\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "wo wir im Park zusammen", "tokens": ["wo", "wir", "im", "Park", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ins bodenlose Wasser sahn.", "tokens": ["ins", "bo\u00b7den\u00b7lo\u00b7se", "Was\u00b7ser", "sahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}