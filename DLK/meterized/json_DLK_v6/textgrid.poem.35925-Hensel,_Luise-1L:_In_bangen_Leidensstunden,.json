{"textgrid.poem.35925": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: In bangen Leidensstunden,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In bangen Leidensstunden,", "tokens": ["In", "ban\u00b7gen", "Lei\u00b7denss\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Vater Dir gesandt,", "tokens": ["Vom", "Va\u00b7ter", "Dir", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hab' ich Dich gefunden,", "tokens": ["Da", "hab'", "ich", "Dich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da hast Du mich erkannt.", "tokens": ["Da", "hast", "Du", "mich", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es schlossen herbe Schmerzen", "tokens": ["Es", "schlos\u00b7sen", "her\u00b7be", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wol enger unsern Bund:", "tokens": ["Wol", "en\u00b7ger", "un\u00b7sern", "Bund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es waren ja die Herzen", "tokens": ["Es", "wa\u00b7ren", "ja", "die", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom selben Dorne wund.", "tokens": ["Vom", "sel\u00b7ben", "Dor\u00b7ne", "wund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und treu nun zu Dir halten", "tokens": ["Und", "treu", "nun", "zu", "Dir", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das werd' ich je und je", "tokens": ["Das", "werd'", "ich", "je", "und", "je"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nimmer Dir erkalten", "tokens": ["Und", "nim\u00b7mer", "Dir", "er\u00b7kal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Freude wie in Weh.", "tokens": ["In", "Freu\u00b7de", "wie", "in", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach! dieser Erde Freuden,", "tokens": ["Ach", "!", "die\u00b7ser", "Er\u00b7de", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie sind mir l\u00e4ngst verbl\u00fcht,", "tokens": ["Sie", "sind", "mir", "l\u00e4ngst", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ich in bittern Leiden", "tokens": ["Als", "ich", "in", "bit\u00b7tern", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von meiner Heimath schied.", "tokens": ["Von", "mei\u00b7ner", "Hei\u00b7math", "schied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Mutter mu\u00dft' ich lassen,", "tokens": ["Die", "Mut\u00b7ter", "mu\u00dft'", "ich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Bruder-, Schwester-Hand", "tokens": ["Und", "Bru\u00b7der", ",", "Schwes\u00b7ter\u00b7Hand"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "TRUNC", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sollt' nimmer ich erfassen", "tokens": ["Sollt'", "nim\u00b7mer", "ich", "er\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im fremden, fernen Land.", "tokens": ["Im", "frem\u00b7den", ",", "fer\u00b7nen", "Land", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch sprach die ew'ge Milde", "tokens": ["Doch", "sprach", "die", "ew'\u00b7ge", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In heil'ger Liebe Gl\u00fch'n,", "tokens": ["In", "heil'\u00b7ger", "Lie\u00b7be", "Gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als sie im Erdgefilde,", "tokens": ["Als", "sie", "im", "Erd\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Menschensohn, erschien:", "tokens": ["Ein", "Men\u00b7schen\u00b7sohn", ",", "er\u00b7schien", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbwas je ein Herz gemieden,", "tokens": ["\u00bb", "was", "je", "ein", "Herz", "ge\u00b7mie\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das sich zu Mir gekehrt,", "tokens": ["Das", "sich", "zu", "Mir", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das werd' ihm schon hienieden", "tokens": ["Das", "werd'", "ihm", "schon", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In reichem Maa\u00df bescheert.\u00ab", "tokens": ["In", "rei\u00b7chem", "Maa\u00df", "be\u00b7scheert", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drum hat des Herren Segen", "tokens": ["Drum", "hat", "des", "Her\u00b7ren", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Herz f\u00fcr mich ger\u00fchrt", "tokens": ["Dein", "Herz", "f\u00fcr", "mich", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat Dich mir entgegen", "tokens": ["Und", "hat", "Dich", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "PTKVZ"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Den Kreuzesweg gef\u00fchrt.", "tokens": ["Den", "Kreu\u00b7zes\u00b7weg", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du hast trotz aller Fehle", "tokens": ["Du", "hast", "trotz", "al\u00b7ler", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es treu mit mir gemeint,", "tokens": ["Es", "treu", "mit", "mir", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast Deine fromme Seele", "tokens": ["Hast", "Dei\u00b7ne", "from\u00b7me", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Liebe mir geeint.", "tokens": ["In", "Lie\u00b7be", "mir", "ge\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So la\u00df uns treu nun wallen", "tokens": ["So", "la\u00df", "uns", "treu", "nun", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Lebenspfad hinab,", "tokens": ["Den", "Le\u00b7ben\u00b7spfad", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis unsrer Hand entfallen", "tokens": ["Bis", "uns\u00b7rer", "Hand", "ent\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der schwere Wanderstab,", "tokens": ["Der", "schwe\u00b7re", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Bis wir vereint dort oben,", "tokens": ["Bis", "wir", "ver\u00b7eint", "dort", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo nie ein Auge weint,", "tokens": ["Wo", "nie", "ein", "Au\u00b7ge", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn mit den Engeln loben,", "tokens": ["Ihn", "mit", "den", "En\u00b7geln", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der uns durch Schmerz vereint.", "tokens": ["Der", "uns", "durch", "Schmerz", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}