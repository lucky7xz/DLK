{"textgrid.poem.32464": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "144. Abschied an den Leser", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du von allem dem, was diese Bl\u00e4tter f\u00fcllt,", "tokens": ["Wenn", "du", "von", "al\u00b7lem", "dem", ",", "was", "die\u00b7se", "Bl\u00e4t\u00b7ter", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "ART", "$,", "PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Leser, nichts des Dankes wert gefunden:", "tokens": ["Mein", "Le\u00b7ser", ",", "nichts", "des", "Dan\u00b7kes", "wert", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIS", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So sei mir wenigstens f\u00fcr das verbunden,", "tokens": ["So", "sei", "mir", "we\u00b7nigs\u00b7tens", "f\u00fcr", "das", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ich zur\u00fcck behielt.", "tokens": ["Was", "ich", "zu\u00b7r\u00fcck", "be\u00b7hielt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}