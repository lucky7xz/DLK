{"dta.poem.5476": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wirckung G\u00f6ttlicher Allgegenwart.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist in allen seinen Wercken,", "tokens": ["Es", "ist", "in", "al\u00b7len", "sei\u00b7nen", "Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die so bewunderns-w\u00fcrdig sch\u00f6n,", "tokens": ["Die", "so", "be\u00b7wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sch\u00f6pfer selbst zwar nicht zu sehn;", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "selbst", "zwar", "nicht", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch seine Gegenwart zu mercken.", "tokens": ["Doch", "sei\u00b7ne", "Ge\u00b7gen\u00b7wart", "zu", "mer\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und darum wirckt der Wunder Menge", "tokens": ["Und", "da\u00b7rum", "wirckt", "der", "Wun\u00b7der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihr so herrliches Gepr\u00e4nge,", "tokens": ["Und", "ihr", "so", "herr\u00b7li\u00b7ches", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wovon wir uns umgeben schauen,", "tokens": ["Wo\u00b7von", "wir", "uns", "um\u00b7ge\u00b7ben", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In mir ein froh und heiligs Grauen.", "tokens": ["In", "mir", "ein", "froh", "und", "hei\u00b7ligs", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wen sollt\u2019 auch nicht ein Grauen r\u00fchren,", "tokens": ["Wen", "sollt'", "auch", "nicht", "ein", "Grau\u00b7en", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man die Nachbarschaft ermi\u00dft,", "tokens": ["Wenn", "man", "die", "Nach\u00b7bar\u00b7schaft", "er\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die GOttheit selbst so nah zu sp\u00fchren,", "tokens": ["Die", "Got\u00b7theit", "selbst", "so", "nah", "zu", "sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die auch den Engeln schrecklich ist.", "tokens": ["Die", "auch", "den", "En\u00b7geln", "schreck\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nein! Es zeigt der Wunder-Schein", "tokens": ["Doch", "nein", "!", "Es", "zeigt", "der", "Wun\u00b7der\u00b7Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Creatur, die ihn verh\u00fcllet,", "tokens": ["Der", "Crea\u00b7tur", ",", "die", "ihn", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df er zu schrecken nicht gewillet;", "tokens": ["Da\u00df", "er", "zu", "schre\u00b7cken", "nicht", "ge\u00b7wil\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er will von uns geliebet seyn.", "tokens": ["Er", "will", "von", "uns", "ge\u00b7lie\u00b7bet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er wollt ein ungezehltes Heer", "tokens": ["Er", "wollt", "ein", "un\u00b7ge\u00b7zehl\u00b7tes", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von Wundern, uns zur Lust, bestimmen;", "tokens": ["Von", "Wun\u00b7dern", ",", "uns", "zur", "Lust", ",", "be\u00b7stim\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Er schuf ein rechtes Anmuths-Meer,", "tokens": ["Er", "schuf", "ein", "rech\u00b7tes", "An\u00b7muths\u00b7Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Worinn wir Menschen gleichsam schwimmen.", "tokens": ["Wo\u00b7rinn", "wir", "Men\u00b7schen", "gleich\u00b7sam", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Er will, man soll vergn\u00fcget leben,", "tokens": ["Er", "will", ",", "man", "soll", "ver\u00b7gn\u00fc\u00b7get", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PIS", "VMFIN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sonst h\u00e4tt\u2019 er sie uns nicht gegeben;", "tokens": ["Sonst", "h\u00e4tt'", "er", "sie", "uns", "nicht", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Indem er uns f\u00fcr ihre Pracht", "tokens": ["In\u00b7dem", "er", "uns", "f\u00fcr", "ih\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "So Wunder-w\u00fcrdig sinnlich macht.", "tokens": ["So", "Wun\u00b7der\u00b7w\u00fcr\u00b7dig", "sinn\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir haben einen freyen Willen,", "tokens": ["Wir", "ha\u00b7ben", "ei\u00b7nen", "frey\u00b7en", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warum will denn die Menschheit nicht,", "tokens": ["Wa\u00b7rum", "will", "denn", "die", "Menschheit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach ihrer so bequemen Pflicht,", "tokens": ["Nach", "ih\u00b7rer", "so", "be\u00b7que\u00b7men", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was GOtt so gn\u00e4dig will, erf\u00fcllen?", "tokens": ["Was", "Gott", "so", "gn\u00e4\u00b7dig", "will", ",", "er\u00b7f\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "NN", "ADV", "ADJD", "VMFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Will sie denn lieber nichts betrachten,", "tokens": ["Will", "sie", "denn", "lie\u00b7ber", "nichts", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Sch\u00f6pfer im Gesch\u00f6pf verachten,", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "im", "Ge\u00b7sch\u00f6pf", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und lieber murrisch seyn in allen,", "tokens": ["Und", "lie\u00b7ber", "mur\u00b7risch", "seyn", "in", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als fr\u00f6lich seyn, und GOtt gefallen?", "tokens": ["Als", "fr\u00f6\u00b7lich", "seyn", ",", "und", "Gott", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAINF", "$,", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}