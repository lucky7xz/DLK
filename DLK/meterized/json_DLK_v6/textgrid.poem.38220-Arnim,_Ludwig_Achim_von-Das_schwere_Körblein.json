{"textgrid.poem.38220": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das schwere K\u00f6rblein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag mir o M\u00e4gdelein, was tr\u00e4gst im K\u00f6rbelein", "tokens": ["Sag", "mir", "o", "M\u00e4g\u00b7del\u00b7ein", ",", "was", "tr\u00e4gst", "im", "K\u00f6r\u00b7be\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "FM", "NN", "$,", "PWS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schwer und dich bem\u00fchest?", "tokens": ["So", "schwer", "und", "dich", "be\u00b7m\u00fc\u00b7hest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist ein Kn\u00e4belein, der hat das Herze mein So oftmals sehr betr\u00fcbet,", "tokens": ["Es", "ist", "ein", "Kn\u00e4\u00b7be\u00b7lein", ",", "der", "hat", "das", "Her\u00b7ze", "mein", "So", "oft\u00b7mals", "sehr", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "VAFIN", "PDS", "VVFIN", "PPOSAT", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Drum mu\u00df er jezt thun henken,", "tokens": ["Drum", "mu\u00df", "er", "jezt", "thun", "hen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Im Korbe und sich kr\u00e4nken,", "tokens": ["Im", "Kor\u00b7be", "und", "sich", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis da\u00df er f\u00e4llt hindurch.", "tokens": ["Bis", "da\u00df", "er", "f\u00e4llt", "hin\u00b7durch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sprach: O M\u00e4gdelein, thu doch gen\u00e4dig seyn,", "tokens": ["Ich", "sprach", ":", "O", "M\u00e4g\u00b7del\u00b7ein", ",", "thu", "doch", "ge\u00b7n\u00e4\u00b7dig", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "NN", "$,", "VVFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht so grausam tobe,", "tokens": ["Und", "nicht", "so", "grau\u00b7sam", "to\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df heraus das Kn\u00e4belein, und rett das Leben sein,", "tokens": ["La\u00df", "he\u00b7raus", "das", "Kn\u00e4\u00b7be\u00b7lein", ",", "und", "rett", "das", "Le\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,", "KON", "ADJD", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Es bringt dir sonst kein Lobe,", "tokens": ["Es", "bringt", "dir", "sonst", "kein", "Lo\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn du ihn l\u00e4\u00dft verderben,", "tokens": ["Wenn", "du", "ihn", "l\u00e4\u00dft", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und gar in Unmuth sterben,", "tokens": ["Und", "gar", "in", "Un\u00b7muth", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Folg mir, ich rath es dir.", "tokens": ["Folg", "mir", ",", "ich", "rath", "es", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Billig w\u00e4r es da\u00df du, anziehest seine Schuh,", "tokens": ["Bil\u00b7lig", "w\u00e4r", "es", "da\u00df", "du", ",", "an\u00b7zie\u00b7hest", "sei\u00b7ne", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KOUS", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Und tretest an seine statt,", "tokens": ["Und", "tre\u00b7test", "an", "sei\u00b7ne", "statt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So wollt ich tragen dich, im Korbe s\u00e4uberlich,", "tokens": ["So", "wollt", "ich", "tra\u00b7gen", "dich", ",", "im", "Kor\u00b7be", "s\u00e4u\u00b7ber\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPER", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df dir widerf\u00fchr kein Schad!", "tokens": ["Da\u00df", "dir", "wi\u00b7der\u00b7f\u00fchr", "kein", "Schad", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hiemit nahm ich das K\u00f6rbelein,", "tokens": ["Hie\u00b7mit", "nahm", "ich", "das", "K\u00f6r\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und rettete das Kn\u00e4belein,", "tokens": ["Und", "ret\u00b7te\u00b7te", "das", "Kn\u00e4\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sezt drein die Jungfrau fein.", "tokens": ["Sezt", "drein", "die", "Jung\u00b7frau", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df mir doch jezt der Weil, und mit mir nicht so eil,", "tokens": ["La\u00df", "mir", "doch", "jezt", "der", "Weil", ",", "und", "mit", "mir", "nicht", "so", "eil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ART", "NN", "$,", "KON", "APPR", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach sie mit Ungemache,", "tokens": ["Sprach", "sie", "mit", "Un\u00b7ge\u00b7ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nein nein, sprach ich zu ihr, ich will nicht folgen dir,", "tokens": ["Nein", "nein", ",", "sprach", "ich", "zu", "ihr", ",", "ich", "will", "nicht", "fol\u00b7gen", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,", "PPER", "VMFIN", "PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil gut ist jezt die Sache,", "tokens": ["Weil", "gut", "ist", "jezt", "die", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und mu\u00dft also thun henken,", "tokens": ["Und", "mu\u00dft", "al\u00b7so", "thun", "hen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Im Korbe und dich kr\u00e4nken,", "tokens": ["Im", "Kor\u00b7be", "und", "dich", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bis du erl\u00f6set wirst.", "tokens": ["Bis", "du", "er\u00b7l\u00f6\u00b7set", "wirst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}