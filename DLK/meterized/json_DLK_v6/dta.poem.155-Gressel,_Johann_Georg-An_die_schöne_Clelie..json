{"dta.poem.155": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die sch\u00f6ne  Clelie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertz! err\u00f6hte nicht das Siegel auffzubrechen/", "tokens": ["Mein", "Hertz", "!", "er\u00b7r\u00f6h\u00b7te", "nicht", "das", "Sie\u00b7gel", "auff\u00b7zu\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PTKNEG", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So meine k\u00fchne Hand dir jetzund zugeschickt/", "tokens": ["So", "mei\u00b7ne", "k\u00fch\u00b7ne", "Hand", "dir", "je\u00b7tzund", "zu\u00b7ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es soll dasselbe stets von deiner G\u00fcte sprechen/", "tokens": ["Es", "soll", "das\u00b7sel\u00b7be", "stets", "von", "dei\u00b7ner", "G\u00fc\u00b7te", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn du dis schlechte Blatt in Gnaden angeblickt.", "tokens": ["Wenn", "du", "dis", "schlech\u00b7te", "Blatt", "in", "Gna\u00b7den", "an\u00b7ge\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar neulich sagte ich/ da\u00df ich nicht lieben k\u00f6nne/", "tokens": ["Zwar", "neu\u00b7lich", "sag\u00b7te", "ich", "/", "da\u00df", "ich", "nicht", "lie\u00b7ben", "k\u00f6n\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie mich um selbiges dein holder Mund gefragt;", "tokens": ["Wie", "mich", "um", "sel\u00b7bi\u00b7ges", "dein", "hol\u00b7der", "Mund", "ge\u00b7fragt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wei\u00df ich selber nicht ob ich anjetzo brenne/", "tokens": ["Doch", "wei\u00df", "ich", "sel\u00b7ber", "nicht", "ob", "ich", "an\u00b7je\u00b7tzo", "bren\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "KOUS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da eine innre Krafft die reine Seele plagt.", "tokens": ["Da", "ei\u00b7ne", "inn\u00b7re", "Krafft", "die", "rei\u00b7ne", "See\u00b7le", "plagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bin voll Pein und Quaal/ und wei\u00df nicht was mir fehlet/", "tokens": ["Ich", "bin", "voll", "Pein", "und", "Qua\u00b7al", "/", "und", "wei\u00df", "nicht", "was", "mir", "feh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "KON", "NN", "$(", "KON", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Mein Hertz ist noch nicht todt/ doch lebet es auch nicht:", "tokens": ["Mein", "Hertz", "ist", "noch", "nicht", "todt", "/", "doch", "le\u00b7bet", "es", "auch", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$(", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da ein verborgner Schmertz die matte Seele qu\u00e4hlet/", "tokens": ["Da", "ein", "ver\u00b7borg\u00b7ner", "Schmertz", "die", "mat\u00b7te", "See\u00b7le", "qu\u00e4h\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bin ich ein Krancker/ dems an H\u00fclff und Trost gebricht.", "tokens": ["Bin", "ich", "ein", "Kran\u00b7cker", "/", "dems", "an", "H\u00fclff", "und", "Trost", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$(", "PRELS", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In allen B\u00fcchern kan ich kein Vergn\u00fcgen finden/", "tokens": ["In", "al\u00b7len", "B\u00fc\u00b7chern", "kan", "ich", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Sayten sind mir nichts/ als nur ein todter Klang:", "tokens": ["Die", "Say\u00b7ten", "sind", "mir", "nichts", "/", "als", "nur", "ein", "tod\u00b7ter", "Klang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIS", "$(", "KOKOM", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hierbey will mir die Lust zur Tichter-Kunst verschwinden/", "tokens": ["Hier\u00b7bey", "will", "mir", "die", "Lust", "zur", "Tich\u00b7ter\u00b7Kunst", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und das studiren ist nur eine Folter-Banck.", "tokens": ["Und", "das", "stu\u00b7di\u00b7ren", "ist", "nur", "ei\u00b7ne", "Fol\u00b7ter\u00b7Banck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mein Leben sage mir/ woher entstehn die Schmertzen/", "tokens": ["Mein", "Le\u00b7ben", "sa\u00b7ge", "mir", "/", "wo\u00b7her", "ent\u00b7stehn", "die", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$(", "PWAV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und welches meynst du sey die Ursach meiner Pein?", "tokens": ["Und", "wel\u00b7ches", "meynst", "du", "sey", "die", "Ur\u00b7sach", "mei\u00b7ner", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein sch\u00f6nes Auge bringt sie hin zu meinen Hertzen/", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", "bringt", "sie", "hin", "zu", "mei\u00b7nen", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da ich durch dessen Feur jetzt mu\u00df entz\u00fcndet seyn.", "tokens": ["Da", "ich", "durch", "des\u00b7sen", "Feur", "jetzt", "mu\u00df", "ent\u00b7z\u00fcn\u00b7det", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRELAT", "NN", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein Engel z\u00fcrne nicht auf mein so k\u00fchnes Schreiben/", "tokens": ["Mein", "En\u00b7gel", "z\u00fcr\u00b7ne", "nicht", "auf", "mein", "so", "k\u00fch\u00b7nes", "Schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Indem Schmertz und Verdru\u00df die Feder selbst regiert;", "tokens": ["In\u00b7dem", "Schmertz", "und", "Ver\u00b7dru\u00df", "die", "Fe\u00b7der", "selbst", "re\u00b7giert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Denn da die Liebe mir wil meine Sinnen treiben;", "tokens": ["Denn", "da", "die", "Lie\u00b7be", "mir", "wil", "mei\u00b7ne", "Sin\u00b7nen", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was Wunder/ wenn man hier viel tausend Fehler sp\u00fchrt.", "tokens": ["Was", "Wun\u00b7der", "/", "wenn", "man", "hier", "viel", "tau\u00b7send", "Feh\u00b7ler", "sp\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "KOUS", "PIS", "ADV", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es ist die Liebe ja ein Meister unsrer Seelen/", "tokens": ["Es", "ist", "die", "Lie\u00b7be", "ja", "ein", "Meis\u00b7ter", "uns\u00b7rer", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein heimlich Strick/ dem ich selbst nicht entfliehen kan/", "tokens": ["Ein", "heim\u00b7lich", "Strick", "/", "dem", "ich", "selbst", "nicht", "ent\u00b7flie\u00b7hen", "kan", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$(", "PRELS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein Schmertz/ den man nicht leicht kan vor den Artzt verhelen/", "tokens": ["Ein", "Schmertz", "/", "den", "man", "nicht", "leicht", "kan", "vor", "den", "Artzt", "ver\u00b7he\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIS", "PTKNEG", "ADJD", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie betet jeder gern/ als eine G\u00f6ttin an.", "tokens": ["Sie", "be\u00b7tet", "je\u00b7der", "gern", "/", "als", "ei\u00b7ne", "G\u00f6t\u00b7tin", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$(", "KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So wil ich auch vergn\u00fcgt die s\u00fcssen Bande tragen;", "tokens": ["So", "wil", "ich", "auch", "ver\u00b7gn\u00fcgt", "die", "s\u00fcs\u00b7sen", "Ban\u00b7de", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da sie von deiner Hand; so ist mir alles recht/", "tokens": ["Da", "sie", "von", "dei\u00b7ner", "Hand", ";", "so", "ist", "mir", "al\u00b7les", "recht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So da\u00df ich wil fortan mit allen Freuden sagen/", "tokens": ["So", "da\u00df", "ich", "wil", "for\u00b7tan", "mit", "al\u00b7len", "Freu\u00b7den", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VMFIN", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Liebe machet mich zu deinen treuen Knecht.", "tokens": ["Die", "Lie\u00b7be", "ma\u00b7chet", "mich", "zu", "dei\u00b7nen", "treu\u00b7en", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}