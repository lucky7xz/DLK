{"textgrid.poem.24035": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Dame Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nackt mit offenen Armen stand", "tokens": ["Nackt", "mit", "of\u00b7fe\u00b7nen", "Ar\u00b7men", "stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Einen Augenblick das Gl\u00fcck", "tokens": ["Ei\u00b7nen", "Au\u00b7gen\u00b7blick", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dicht vor mir.", "tokens": ["Dicht", "vor", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ei, was eine sch\u00f6ne Brust,", "tokens": ["Ei", ",", "was", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weiche Brust, volle Brust", "tokens": ["Wei\u00b7che", "Brust", ",", "vol\u00b7le", "Brust"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Hat die Dame Gl\u00fcck, es sind", "tokens": ["Hat", "die", "Da\u00b7me", "Gl\u00fcck", ",", "es", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN", "$,", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rosenknospen zweie drauf:", "tokens": ["Ro\u00b7sen\u00b7knos\u00b7pen", "zwei\u00b7e", "drauf", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wundersch\u00f6n!", "tokens": ["Wun\u00b7der\u00b7sch\u00f6n", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Und wie k\u00fc\u00dft die Dame Gl\u00fcck!", "tokens": ["Und", "wie", "k\u00fc\u00dft", "die", "Da\u00b7me", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dfer Ku\u00df, dr\u00e4ngender Ku\u00df,", "tokens": ["Hei\u00b7\u00dfer", "Ku\u00df", ",", "dr\u00e4n\u00b7gen\u00b7der", "Ku\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und man macht die Augen zu,", "tokens": ["Und", "man", "macht", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00dft das Gl\u00fcck.", "tokens": ["K\u00fc\u00dft", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Auf die Schultern legte sie mir", "tokens": ["Auf", "die", "Schul\u00b7tern", "leg\u00b7te", "sie", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihrer Arme s\u00fc\u00dfe Last,", "tokens": ["Ih\u00b7rer", "Ar\u00b7me", "s\u00fc\u00b7\u00dfe", "Last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weiche Last, warme Last,", "tokens": ["Wei\u00b7che", "Last", ",", "war\u00b7me", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "W\u00e4hrend sie mich k\u00fc\u00dfte, lind.", "tokens": ["W\u00e4h\u00b7rend", "sie", "mich", "k\u00fc\u00df\u00b7te", ",", "lind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, was bist du wunderhold,", "tokens": ["Ach", ",", "was", "bist", "du", "wun\u00b7der\u00b7hold", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dame Gl\u00fcck.", "tokens": ["Da\u00b7me", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Und ich that die Augen auf,", "tokens": ["Und", "ich", "that", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollte tief ihr einmal sehn", "tokens": ["Woll\u00b7te", "tief", "ihr", "ein\u00b7mal", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In das sonnige Augenpaar, \u2013", "tokens": ["In", "das", "son\u00b7ni\u00b7ge", "Au\u00b7gen\u00b7paar", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ach, ach, ach \u2013:", "tokens": ["Ach", ",", "ach", ",", "ach", "\u2013", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "XY", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Ausgeloschen war das Licht,", "tokens": ["Aus\u00b7ge\u00b7lo\u00b7schen", "war", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leere H\u00f6hlen grinsten mich an,", "tokens": ["Lee\u00b7re", "H\u00f6h\u00b7len", "grins\u00b7ten", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Eine d\u00fcrre Vettel stand", "tokens": ["Ei\u00b7ne", "d\u00fcr\u00b7re", "Vet\u00b7tel", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dicht vor mir.", "tokens": ["Dicht", "vor", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Rippenhart die runzlige Brust,", "tokens": ["Rip\u00b7pen\u00b7hart", "die", "runz\u00b7li\u00b7ge", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Lippenlos ein geifernder Mund,", "tokens": ["Lip\u00b7pen\u00b7los", "ein", "gei\u00b7fern\u00b7der", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Spitz der Arm und kn\u00f6cherig.", "tokens": ["Spitz", "der", "Arm", "und", "kn\u00f6\u00b7che\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Pfui, Madame! Ist das ein Scherz,", "tokens": ["Pfui", ",", "Ma\u00b7da\u00b7me", "!", "Ist", "das", "ein", "Scherz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "$.", "VAFIN", "PDS", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ist er nicht nach meinem Geschmack.", "tokens": ["Ist", "er", "nicht", "nach", "mei\u00b7nem", "Ge\u00b7schmack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Als Verwandlungsk\u00fcnstlerin", "tokens": ["Als", "Ver\u00b7wand\u00b7lungs\u00b7k\u00fcnst\u00b7le\u00b7rin"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Haben Sie vielleicht Erfolg", "tokens": ["Ha\u00b7ben", "Sie", "viel\u00b7leicht", "Er\u00b7folg"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Th\u00e9\u00e2tre-Vari\u00e9t\u00e9,", "tokens": ["Im", "Th\u00e9\u00e2tre\u00b7Va\u00b7ri\u00e9t\u00e9", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-++-", "measure": "unknown.measure.di"}, "line.6": {"text": "Nicht bei mir.", "tokens": ["Nicht", "bei", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Und die Dame drehte sich", "tokens": ["Und", "die", "Da\u00b7me", "dreh\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Langsam um und ging hinaus,", "tokens": ["Lang\u00b7sam", "um", "und", "ging", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die andre Th\u00fcre kam", "tokens": ["Durch", "die", "and\u00b7re", "Th\u00fc\u00b7re", "kam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Frau herein. \u2013 Ich Thor!", "tokens": ["Mei\u00b7ne", "Frau", "her\u00b7ein", ".", "\u2013", "Ich", "Thor", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "$(", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir geschieht ganz recht: Warum", "tokens": ["Mir", "ge\u00b7schieht", "ganz", "recht", ":", "Wa\u00b7rum"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gab ich mich mit Weibern ab.", "tokens": ["Gab", "ich", "mich", "mit", "Wei\u00b7bern", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00fcnftig will ich treuer sein!", "tokens": ["K\u00fcnf\u00b7tig", "will", "ich", "treu\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}