{"textgrid.poem.66720": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Totenfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesponnen mit feuchten,", "tokens": ["Ge\u00b7spon\u00b7nen", "mit", "feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Segnenden Fingern", "tokens": ["Seg\u00b7nen\u00b7den", "Fin\u00b7gern"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hat n\u00e4chtlich der Fr\u00fchling", "tokens": ["Hat", "n\u00e4cht\u00b7lich", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schimmernder Schleier", "tokens": ["Schim\u00b7mern\u00b7der", "Schlei\u00b7er"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Lichtgr\u00fcnes Gespinst.", "tokens": ["Licht\u00b7gr\u00fc\u00b7nes", "Ge\u00b7spinst", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Nun zittern die Zweige", "tokens": ["Nun", "zit\u00b7tern", "die", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Von zartem Gewebe,", "tokens": ["Von", "zar\u00b7tem", "Ge\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und \u00fcber die schwarzen,", "tokens": ["Und", "\u00fc\u00b7ber", "die", "schwar\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Saftschweren \u00c4ste", "tokens": ["Saft\u00b7schwe\u00b7ren", "\u00c4s\u00b7te"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Flie\u00dft hauchfeiner Flor ...", "tokens": ["Flie\u00dft", "hauch\u00b7fei\u00b7ner", "Flor", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Der Mord und Gemetzel", "tokens": ["Der", "Mord", "und", "Ge\u00b7met\u00b7zel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "L\u00e4\u00dft triefen auf Erden,", "tokens": ["L\u00e4\u00dft", "trie\u00b7fen", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Der Krankheit und Kummer", "tokens": ["Der", "Krank\u00b7heit", "und", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Den Menschen verh\u00e4ngt \u2013", "tokens": ["Den", "Men\u00b7schen", "ver\u00b7h\u00e4ngt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Der Kronen zersplittert", "tokens": ["Der", "Kro\u00b7nen", "zer\u00b7split\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Und Keime verschleudert,", "tokens": ["Und", "Kei\u00b7me", "ver\u00b7schleu\u00b7dert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Der ewige Weber", "tokens": ["Der", "e\u00b7wi\u00b7ge", "We\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Webt br\u00e4utliche Zier.", "tokens": ["Webt", "br\u00e4ut\u00b7li\u00b7che", "Zier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "Tod ist gekommen,", "tokens": ["Tod", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Teures genommen,", "tokens": ["Teu\u00b7res", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Liebende Herzen", "tokens": ["Lie\u00b7ben\u00b7de", "Her\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Geschieden in Qual:", "tokens": ["Ge\u00b7schie\u00b7den", "in", "Qual", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "Nimmer sich freuen", "tokens": ["Nim\u00b7mer", "sich", "freu\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PRF", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Am sprie\u00dfenden Neuen", "tokens": ["Am", "sprie\u00b7\u00dfen\u00b7den", "Neu\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "K\u00f6nnen die Toten,", "tokens": ["K\u00f6n\u00b7nen", "die", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Nimmer sich w\u00e4rmen am sonnigen Strahl.", "tokens": ["Nim\u00b7mer", "sich", "w\u00e4r\u00b7men", "am", "son\u00b7ni\u00b7gen", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.27": {"text": "Allesdurchdringer,", "tokens": ["Al\u00b7les\u00b7durch\u00b7drin\u00b7ger", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Sprengst du den Zwinger,", "tokens": ["Sprengst", "du", "den", "Zwin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Tauchst die verloschenen Augen in Licht?", "tokens": ["Tauchst", "die", "ver\u00b7lo\u00b7sche\u00b7nen", "Au\u00b7gen", "in", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.30": {"text": "Wandelst Begrabene,", "tokens": ["Wan\u00b7delst", "Be\u00b7gra\u00b7be\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.31": {"text": "Schwebend Erhabene \u2013", "tokens": ["Schwe\u00b7bend", "Er\u00b7ha\u00b7be\u00b7ne", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$("], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.32": {"text": "Wir nur trauern in bitterm Verzicht ...?", "tokens": ["Wir", "nur", "trau\u00b7ern", "in", "bit\u00b7term", "Ver\u00b7zicht", "...", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VVINF", "APPR", "ADJA", "NN", "$(", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}