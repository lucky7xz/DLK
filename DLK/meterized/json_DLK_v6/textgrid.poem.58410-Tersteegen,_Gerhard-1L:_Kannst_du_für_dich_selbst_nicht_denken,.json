{"textgrid.poem.58410": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kannst du f\u00fcr dich selbst nicht denken,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kannst du f\u00fcr dich selbst nicht denken,", "tokens": ["Kannst", "du", "f\u00fcr", "dich", "selbst", "nicht", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lerne kindlich dich ersenken", "tokens": ["Ler\u00b7ne", "kind\u00b7lich", "dich", "er\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Willens Gottes Meer;", "tokens": ["In", "des", "Wil\u00b7lens", "Got\u00b7tes", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kannst du nicht dein Schiff bewahren,", "tokens": ["Kannst", "du", "nicht", "dein", "Schiff", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df es nur im Glauben fahren", "tokens": ["La\u00df", "es", "nur", "im", "Glau\u00b7ben", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nichts mehr als Gott begehr!", "tokens": ["Und", "nichts", "mehr", "als", "Gott", "be\u00b7gehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}