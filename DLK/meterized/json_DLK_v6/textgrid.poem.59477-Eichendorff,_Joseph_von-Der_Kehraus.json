{"textgrid.poem.59477": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kehraus", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fiedeln die Geigen,", "tokens": ["Es", "fie\u00b7deln", "die", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da tritt in den Reigen", "tokens": ["Da", "tritt", "in", "den", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ein seltsamer Gast,", "tokens": ["Ein", "selt\u00b7sa\u00b7mer", "Gast", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Kennt keiner den D\u00fcrren,", "tokens": ["Kennt", "kei\u00b7ner", "den", "D\u00fcr\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Galant aus dem Schwirren", "tokens": ["Ga\u00b7lant", "aus", "dem", "Schwir\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die Braut er sich fa\u00dft.", "tokens": ["Die", "Braut", "er", "sich", "fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Hebt an, sich zu schwenken", "tokens": ["Hebt", "an", ",", "sich", "zu", "schwen\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "PRF", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In allen Gelenken.", "tokens": ["In", "al\u00b7len", "Ge\u00b7len\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das Fr\u00e4ulein im Kranz:", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "im", "Kranz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "\u00bbeuch knacken die Beine \u2013\u00ab", "tokens": ["\u00bb", "euch", "kna\u00b7cken", "die", "Bei\u00b7ne", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$(", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "\u00bbbald rasseln auch deine,", "tokens": ["\u00bb", "bald", "ras\u00b7seln", "auch", "dei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "PPOSAT", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Frisch auf spielt zum Tanz!\u00ab", "tokens": ["Frisch", "auf", "spielt", "zum", "Tanz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "----+", "measure": "unknown.measure.single"}}, "stanza.3": {"line.1": {"text": "Die Spr\u00f6de hinterm F\u00e4cher,", "tokens": ["Die", "Spr\u00f6\u00b7de", "hin\u00b7term", "F\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Zecher vom Becher,", "tokens": ["Der", "Ze\u00b7cher", "vom", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Dichter so lind,", "tokens": ["Der", "Dich\u00b7ter", "so", "lind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mu\u00df auch mit zum Tanze,", "tokens": ["Mu\u00df", "auch", "mit", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df die Lorbeern vom Kranze", "tokens": ["Da\u00df", "die", "Lor\u00b7beern", "vom", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Fliegen im Wind.", "tokens": ["Flie\u00b7gen", "im", "Wind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "So schnurret der Reigen", "tokens": ["So", "schnur\u00b7ret", "der", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zum Saal raus ins Schweigen", "tokens": ["Zum", "Saal", "raus", "ins", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der pr\u00e4chtigen Nacht,", "tokens": ["Der", "pr\u00e4ch\u00b7ti\u00b7gen", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Kl\u00e4nge verwehen,", "tokens": ["Die", "Kl\u00e4n\u00b7ge", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die H\u00e4hne schon kr\u00e4hen,", "tokens": ["Die", "H\u00e4h\u00b7ne", "schon", "kr\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Da verstieben sie sacht. \u2013", "tokens": ["Da", "ver\u00b7stie\u00b7ben", "sie", "sacht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "So ging's schon vorzeiten", "tokens": ["So", "ging's", "schon", "vor\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und geht es noch heute,", "tokens": ["Und", "geht", "es", "noch", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und h\u00f6rest du hell", "tokens": ["Und", "h\u00f6\u00b7rest", "du", "hell"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Aufspielen zum Reigen,", "tokens": ["Auf\u00b7spie\u00b7len", "zum", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wer wei\u00df, wem sie geigen \u2013", "tokens": ["Wer", "wei\u00df", ",", "wem", "sie", "gei\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "H\u00fct dich, Gesell!", "tokens": ["H\u00fct", "dich", ",", "Ge\u00b7sell", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}