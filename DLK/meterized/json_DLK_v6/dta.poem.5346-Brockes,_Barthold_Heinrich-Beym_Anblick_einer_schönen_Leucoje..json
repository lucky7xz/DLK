{"dta.poem.5346": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Beym Anblick einer sch\u00f6nen Leucoje.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sey willkommen, liebste Blume,", "tokens": ["Sey", "will\u00b7kom\u00b7men", ",", "liebs\u00b7te", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du, deinem Herrn zum Ruhme,", "tokens": ["Die", "du", ",", "dei\u00b7nem", "Herrn", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblich riechst, und zierlich bl\u00fchest;", "tokens": ["Lieb\u00b7lich", "riechst", ",", "und", "zier\u00b7lich", "bl\u00fc\u00b7hest", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die du, durch der Farben Pracht,", "tokens": ["Die", "du", ",", "durch", "der", "Far\u00b7ben", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den, der alle Dinge macht,", "tokens": ["Den", ",", "der", "al\u00b7le", "Din\u00b7ge", "macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fast zu zeigen dich bem\u00fchest;", "tokens": ["Fast", "zu", "zei\u00b7gen", "dich", "be\u00b7m\u00fc\u00b7hest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja, die du in bunten Sch\u00e4tzen,", "tokens": ["Ja", ",", "die", "du", "in", "bun\u00b7ten", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn wir uns daran ergetzen,", "tokens": ["Wenn", "wir", "uns", "da\u00b7ran", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Unsre Seelen zu ihm ziehest.", "tokens": ["Uns\u00b7re", "See\u00b7len", "zu", "ihm", "zie\u00b7hest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Denn ist in so vielen Wercken,", "tokens": ["Denn", "ist", "in", "so", "vie\u00b7len", "Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Die so herrlich und so sch\u00f6n,", "tokens": ["Die", "so", "herr\u00b7lich", "und", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Keine Weisheit zu bemercken?", "tokens": ["Kei\u00b7ne", "Weis\u00b7heit", "zu", "be\u00b7mer\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Keine GOttes-Kraft zu sehn?", "tokens": ["Kei\u00b7ne", "Got\u00b7tes\u00b7Kraft", "zu", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was um sch\u00f6ne Blumen schwebet", "tokens": ["Was", "um", "sch\u00f6\u00b7ne", "Blu\u00b7men", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sich stets daraus erhebet", "tokens": ["Und", "sich", "stets", "da\u00b7raus", "er\u00b7he\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ADV", "PAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r und blieb\u2019 uns unbekannt;", "tokens": ["W\u00e4r", "und", "blieb'", "uns", "un\u00b7be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sich nicht der holde Duft,", "tokens": ["Wenn", "sich", "nicht", "der", "hol\u00b7de", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch das Dehnen unsrer Lungen,", "tokens": ["Durch", "das", "Deh\u00b7nen", "uns\u00b7rer", "Lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von dem schnellen Druck der Luft", "tokens": ["Von", "dem", "schnel\u00b7len", "Druck", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In das Trichter-Paar gedrungen,", "tokens": ["In", "das", "Trich\u00b7ter\u00b7Paar", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das wir in der Nasen sehn", "tokens": ["Das", "wir", "in", "der", "Na\u00b7sen", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wunderbar formiret stehn;", "tokens": ["Wun\u00b7der\u00b7bar", "for\u00b7mi\u00b7ret", "stehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo sich in den beyden G\u00e4ngen", "tokens": ["Wo", "sich", "in", "den", "bey\u00b7den", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Die sich zu dem Zweck verengen,", "tokens": ["Die", "sich", "zu", "dem", "Zweck", "ver\u00b7en\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Die vorhin zertheilte Kraft", "tokens": ["Die", "vor\u00b7hin", "zer\u00b7theil\u00b7te", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Neu-vereinigt sucht zu drengen,", "tokens": ["Neu\u00b7ver\u00b7ei\u00b7nigt", "sucht", "zu", "dren\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wodurch denn der trockne Saft,", "tokens": ["Wo\u00b7durch", "denn", "der", "trock\u00b7ne", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.15": {"text": "Wenn er, dergestalt gepre\u00dft,", "tokens": ["Wenn", "er", ",", "der\u00b7ge\u00b7stalt", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "St\u00e4rcker sich empfinden l\u00e4\u00dft", "tokens": ["St\u00e4r\u00b7cker", "sich", "emp\u00b7fin\u00b7den", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PRF", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von dem Nervgen, das es r\u00fchret,", "tokens": ["Von", "dem", "Nerv\u00b7gen", ",", "das", "es", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das ins Gehirn ihn f\u00fchret.", "tokens": ["Und", "das", "ins", "Ge\u00b7hirn", "ihn", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da ich dieses \u00fcberlege", "tokens": ["Da", "ich", "die\u00b7ses", "\u00fc\u00b7berl\u00b7e\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Blumen Kraft erwege,", "tokens": ["Und", "der", "Blu\u00b7men", "Kraft", "er\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "F\u00e4llt mir ziemlich glaublich bey,", "tokens": ["F\u00e4llt", "mir", "ziem\u00b7lich", "glaub\u00b7lich", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie vielleicht noch vielerley,", "tokens": ["Wie", "viel\u00b7leicht", "noch", "vie\u00b7ler\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PIAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So zur Lust, als Artzeney,", "tokens": ["So", "zur", "Lust", ",", "als", "Art\u00b7ze\u00b7ney", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In denselbigen verstecket,", "tokens": ["In", "den\u00b7sel\u00b7bi\u00b7gen", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und uns noch verborgen sey.", "tokens": ["Und", "uns", "noch", "ver\u00b7bor\u00b7gen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4tt\u2019 uns nicht die Bien\u2019 entdecket,", "tokens": ["H\u00e4tt'", "uns", "nicht", "die", "Bi\u00b7en'", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Da\u00df des Honigs S\u00fc\u00dfigkeit", "tokens": ["Da\u00df", "des", "Ho\u00b7nigs", "S\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "In der Blumen Art vorhanden;", "tokens": ["In", "der", "Blu\u00b7men", "Art", "vor\u00b7han\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Welcher h\u00e4tt sich unterstanden,", "tokens": ["Wel\u00b7cher", "h\u00e4tt", "sich", "un\u00b7ter\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "VAFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "In der Bl\u00e4tter bunten Gr\u00fcnden", "tokens": ["In", "der", "Bl\u00e4t\u00b7ter", "bun\u00b7ten", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Jemahls die Beschaffenheit", "tokens": ["Je\u00b7mahls", "die", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Solches s\u00fcssen Safts zu finden?", "tokens": ["Sol\u00b7ches", "s\u00fcs\u00b7sen", "Safts", "zu", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Alles menschliche Bem\u00fchn,", "tokens": ["Al\u00b7les", "menschli\u00b7che", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Alle Kunst zu sublimiren,", "tokens": ["Al\u00b7le", "Kunst", "zu", "sub\u00b7li\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Aufzul\u00f6sen, distilliren,", "tokens": ["Auf\u00b7zu\u00b7l\u00f6\u00b7sen", ",", "dis\u00b7til\u00b7li\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PDS", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.18": {"text": "Um den Saft heraus zu ziehn", "tokens": ["Um", "den", "Saft", "he\u00b7raus", "zu", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APZR", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "H\u00e4tt\u2019, um ihn davon zu trennen,", "tokens": ["H\u00e4tt'", ",", "um", "ihn", "da\u00b7von", "zu", "tren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "PPER", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Keinen Nutzen haben k\u00f6nnen.", "tokens": ["Kei\u00b7nen", "Nut\u00b7zen", "ha\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "K\u00f6nnt\u2019 es denn nicht m\u00f6glich seyn,", "tokens": ["K\u00f6nnt'", "es", "denn", "nicht", "m\u00f6g\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Da\u00df nicht diese Kr\u00e4ft\u2019 allein,", "tokens": ["Da\u00df", "nicht", "die\u00b7se", "Kr\u00e4ft'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PDAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Sondern annoch andre S\u00e4fte,", "tokens": ["Son\u00b7dern", "an\u00b7noch", "and\u00b7re", "S\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Andre Geister, andre Kr\u00e4fte", "tokens": ["And\u00b7re", "Geis\u00b7ter", ",", "and\u00b7re", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "In den Blumen sich befinden;", "tokens": ["In", "den", "Blu\u00b7men", "sich", "be\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Wenn wir solches nur verst\u00fcnden?", "tokens": ["Wenn", "wir", "sol\u00b7ches", "nur", "ver\u00b7st\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn da\u00df Menschen es nicht fassen,", "tokens": ["Denn", "da\u00df", "Men\u00b7schen", "es", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Daraus wird die M\u00f6glichkeit", "tokens": ["Da\u00b7raus", "wird", "die", "M\u00f6g\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Mehrerley Beschaffenheit", "tokens": ["Meh\u00b7rer\u00b7ley", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich nicht wiedersprechen lassen.", "tokens": ["Sich", "nicht", "wie\u00b7der\u00b7spre\u00b7chen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonderlich, da solche Lehren", "tokens": ["Son\u00b7der\u00b7lich", ",", "da", "sol\u00b7che", "Leh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der sch\u00f6nen Blumen Heer", "tokens": ["Von", "der", "sch\u00f6\u00b7nen", "Blu\u00b7men", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsers grossen Sch\u00f6pfers Ehr\u2019", "tokens": ["Un\u00b7sers", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Ehr'"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht vermindern, eh vermehren.", "tokens": ["Nicht", "ver\u00b7min\u00b7dern", ",", "eh", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "KOUS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}