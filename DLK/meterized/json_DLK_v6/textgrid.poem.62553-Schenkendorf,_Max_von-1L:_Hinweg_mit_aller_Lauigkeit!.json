{"textgrid.poem.62553": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hinweg mit aller Lauigkeit!", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinweg mit aller Lauigkeit!", "tokens": ["Hin\u00b7weg", "mit", "al\u00b7ler", "Lau\u00b7ig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit aller halben Liebe!", "tokens": ["Mit", "al\u00b7ler", "hal\u00b7ben", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr, ich bin dein, und will es sein", "tokens": ["Herr", ",", "ich", "bin", "dein", ",", "und", "will", "es", "sein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPOSAT", "$,", "KON", "VMFIN", "PPER", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit vollem Flammentriebe.", "tokens": ["Mit", "vol\u00b7lem", "Flam\u00b7men\u00b7trie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein neuer Simon bin ich hier,", "tokens": ["Ein", "neu\u00b7er", "Si\u00b7mon", "bin", "ich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den schweren Gang zu wagen,", "tokens": ["Den", "schwe\u00b7ren", "Gang", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach w\u00fcrd'ge mich, ich bitte dich,", "tokens": ["Ach", "w\u00fcrd'\u00b7ge", "mich", ",", "ich", "bit\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Kreuz dir nachzutragen.", "tokens": ["Dein", "Kreuz", "dir", "nach\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sei's Feindesha\u00df, sei's Judasku\u00df,", "tokens": ["Sei's", "Fein\u00b7des\u00b7ha\u00df", ",", "sei's", "Ju\u00b7das\u00b7ku\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei's Leib's- und Seelenleiden,", "tokens": ["Sei's", "Leib'\u00b7s", "und", "See\u00b7len\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei's bittre Noth, sei's Martertod,", "tokens": ["Sei's", "bitt\u00b7re", "Noth", ",", "sei's", "Mar\u00b7ter\u00b7tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich trag's f\u00fcr dich mit Freuden.", "tokens": ["Ich", "trag's", "f\u00fcr", "dich", "mit", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}