{"dta.poem.9566": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Unterscheid der schwartzen und blauen augen.  \n Auff zwo schwestern.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ljsett und Silvie/ ihr perlen unsrer zeit/", "tokens": ["Ljsett", "und", "Sil\u00b7vie", "/", "ihr", "per\u00b7len", "uns\u00b7rer", "zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jhr k\u00f6nnt den stoltzen prei\u00df vor tausend andern haben:", "tokens": ["Ihr", "k\u00f6nnt", "den", "stolt\u00b7zen", "prei\u00df", "vor", "tau\u00b7send", "an\u00b7dern", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "CARD", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn eure freundligkeit und ungemeine gaben", "tokens": ["Denn", "eu\u00b7re", "freund\u00b7lig\u00b7keit", "und", "un\u00b7ge\u00b7mei\u00b7ne", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind ein ber\u00fchmter zeug der theursten kostbarkeit.", "tokens": ["Sind", "ein", "be\u00b7r\u00fchm\u00b7ter", "zeug", "der", "theurs\u00b7ten", "kost\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus eurem wesen strahlt selbst die vollkommenheit/", "tokens": ["Aus", "eu\u00b7rem", "we\u00b7sen", "strahlt", "selbst", "die", "voll\u00b7kom\u00b7men\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An eurer anmuth kan sich alle welt erlaben.", "tokens": ["An", "eu\u00b7rer", "an\u00b7muth", "kan", "sich", "al\u00b7le", "welt", "er\u00b7la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich glaub/ es ist ein geist in beyder hertz vergraben/", "tokens": ["Ich", "glaub", "/", "es", "ist", "ein", "geist", "in", "bey\u00b7der", "hertz", "ver\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil ihr an muntrigkeit euch so gar \u00e4hnlich seyd.", "tokens": ["Weil", "ihr", "an", "munt\u00b7rig\u00b7keit", "euch", "so", "gar", "\u00e4hn\u00b7lich", "seyd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In einem trefft ihr doch nicht gar zu wohl zusammen;", "tokens": ["In", "ei\u00b7nem", "trefft", "ihr", "doch", "nicht", "gar", "zu", "wohl", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Lisett zeigt offenbahr die blauen himmels-flammen", "tokens": ["Li\u00b7sett", "zeigt", "of\u00b7fen\u00b7bahr", "die", "blau\u00b7en", "him\u00b7mels\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und den verliebten brand/ der durch die augen blitzt;", "tokens": ["Und", "den", "ver\u00b7lieb\u00b7ten", "brand", "/", "der", "durch", "die", "au\u00b7gen", "blitzt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hingegen Stlvie brennt heimlich und verstohlen:", "tokens": ["Hin\u00b7ge\u00b7gen", "Stl\u00b7vie", "brennt", "heim\u00b7lich", "und", "ver\u00b7stoh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ADJD", "KON", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Denn ihr geschw\u00e4rtztes aug\u2019 l\u00e4\u00dft wie verloschne kohlen/", "tokens": ["Denn", "ihr", "ge\u00b7schw\u00e4rtz\u00b7tes", "aug'", "l\u00e4\u00dft", "wie", "ver\u00b7lo\u00b7schne", "koh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "KOKOM", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Darinnen gleichwohl feur und glut verborgen sitzt.", "tokens": ["Da\u00b7rin\u00b7nen", "gleich\u00b7wohl", "feur", "und", "glut", "ver\u00b7bor\u00b7gen", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}