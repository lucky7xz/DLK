{"textgrid.poem.25566": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kaum vermocht' ich vor ihm mein schwimmendes Auge zu bergen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum vermocht' ich vor ihm mein schwimmendes Auge zu bergen,", "tokens": ["Kaum", "ver\u00b7mocht'", "ich", "vor", "ihm", "mein", "schwim\u00b7men\u00b7des", "Au\u00b7ge", "zu", "ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als ich, Jahre getrennt, endlich ihn wieder umfing!", "tokens": ["Als", "ich", ",", "Jah\u00b7re", "ge\u00b7trennt", ",", "end\u00b7lich", "ihn", "wie\u00b7der", "um\u00b7fing", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "VVPP", "$,", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Feuer im Auge, wohin? \u2013 Zu todter Asche verglommen!", "tokens": ["Feu\u00b7er", "im", "Au\u00b7ge", ",", "wo\u00b7hin", "?", "\u2013", "Zu", "tod\u00b7ter", "A\u00b7sche", "ver\u00b7glom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PWAV", "$.", "$(", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und du Stimme voll Klang? Tief in den Busen versenkt!", "tokens": ["Und", "du", "Stim\u00b7me", "voll", "Klang", "?", "Tief", "in", "den", "Bu\u00b7sen", "ver\u00b7senkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "ADJD", "NN", "$.", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Thr\u00e4nen erpre\u00dfte mir da der Sohn, den Kummer und Liebe", "tokens": ["Thr\u00e4\u00b7nen", "er\u00b7pre\u00df\u00b7te", "mir", "da", "der", "Sohn", ",", "den", "Kum\u00b7mer", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Mit einander gezeugt, z\u00e4rtlich die Muse gestillt.", "tokens": ["Mit", "ein\u00b7an\u00b7der", "ge\u00b7zeugt", ",", "z\u00e4rt\u00b7lich", "die", "Mu\u00b7se", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "$,", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Als auch diese zuletzt, gleich einer alternden Amme,", "tokens": ["Als", "auch", "die\u00b7se", "zu\u00b7letzt", ",", "gleich", "ei\u00b7ner", "al\u00b7tern\u00b7den", "Am\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "ADV", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Immer launigter ward, winkte der freundliche Tod.", "tokens": ["Im\u00b7mer", "lau\u00b7nig\u00b7ter", "ward", ",", "wink\u00b7te", "der", "freund\u00b7li\u00b7che", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und ich traure nicht mehr, obgleich ich ihn scheiden gesehen,", "tokens": ["Und", "ich", "trau\u00b7re", "nicht", "mehr", ",", "ob\u00b7gleich", "ich", "ihn", "schei\u00b7den", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Kehrt er gleich nimmer zur\u00fcck, dieser mein \u00e4ltester Freund.", "tokens": ["Kehrt", "er", "gleich", "nim\u00b7mer", "zu\u00b7r\u00fcck", ",", "die\u00b7ser", "mein", "\u00e4l\u00b7tes\u00b7ter", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Endlich h\u00e4tte vielleicht mein lange vergebliches Streben", "tokens": ["End\u00b7lich", "h\u00e4t\u00b7te", "viel\u00b7leicht", "mein", "lan\u00b7ge", "ver\u00b7geb\u00b7li\u00b7ches", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Ihn mit dem Boden vereint, dem er so lieblich entspro\u00df.", "tokens": ["Ihn", "mit", "dem", "Bo\u00b7den", "ver\u00b7eint", ",", "dem", "er", "so", "lieb\u00b7lich", "ent\u00b7spro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Bl\u00fcthen trieb er auf ihm, doch seine goldenen Fr\u00fcchte,", "tokens": ["Bl\u00fc\u00b7then", "trieb", "er", "auf", "ihm", ",", "doch", "sei\u00b7ne", "gol\u00b7de\u00b7nen", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "$,", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Wie sie der Himmel Petrarchs selten zu reifen vermag,", "tokens": ["Wie", "sie", "der", "Him\u00b7mel", "Pe\u00b7trarchs", "sel\u00b7ten", "zu", "rei\u00b7fen", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NE", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Trug er, \u2013 ungl\u00fcckliche Wahl! \u2013 am fremden Ufer der Leine,", "tokens": ["Trug", "er", ",", "\u2013", "un\u00b7gl\u00fcck\u00b7li\u00b7che", "Wahl", "!", "\u2013", "am", "frem\u00b7den", "U\u00b7fer", "der", "Lei\u00b7ne", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "ADJA", "NN", "$.", "$(", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Aber ein zeitiger Herbst welkte die Bl\u00e4tter zu fr\u00fch.", "tokens": ["A\u00b7ber", "ein", "zei\u00b7ti\u00b7ger", "Herbst", "welk\u00b7te", "die", "Bl\u00e4t\u00b7ter", "zu", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Doch ich traure nicht mehr, denn selbst ans Ufer der Spree,", "tokens": ["Doch", "ich", "trau\u00b7re", "nicht", "mehr", ",", "denn", "selbst", "ans", "U\u00b7fer", "der", "Spree", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$,", "KON", "ADV", "APPRART", "NN", "ART", "NE", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Oder und Saale verpflanzt, h\u00e4tt' er nicht l\u00e4nger gegr\u00fcnt.", "tokens": ["O\u00b7der", "und", "Saa\u00b7le", "ver\u00b7pflanzt", ",", "h\u00e4tt'", "er", "nicht", "l\u00e4n\u00b7ger", "ge\u00b7gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVPP", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Vormals konnt' ich ja blo\u00df mit meinen Thr\u00e4nen ihn netzen,", "tokens": ["Vor\u00b7mals", "konnt'", "ich", "ja", "blo\u00df", "mit", "mei\u00b7nen", "Thr\u00e4\u00b7nen", "ihn", "net\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Jetzt kam aber zu sp\u00e4t freundliche Sorge f\u00fcr ihn.", "tokens": ["Jetzt", "kam", "a\u00b7ber", "zu", "sp\u00e4t", "freund\u00b7li\u00b7che", "Sor\u00b7ge", "f\u00fcr", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKA", "ADJD", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Nein! ich traure nicht mehr. Er windet aus bleyernem Schlafe", "tokens": ["Nein", "!", "ich", "trau\u00b7re", "nicht", "mehr", ".", "Er", "win\u00b7det", "aus", "bley\u00b7er\u00b7nem", "Schla\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Nicht am Morgen sich noch m\u00fchevoll dehnend empor,", "tokens": ["Nicht", "am", "Mor\u00b7gen", "sich", "noch", "m\u00fc\u00b7he\u00b7voll", "deh\u00b7nend", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PRF", "ADV", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Ungewissen Erfolg im Auge des Arztes zu lesen,", "tokens": ["Un\u00b7ge\u00b7wis\u00b7sen", "Er\u00b7folg", "im", "Au\u00b7ge", "des", "Arz\u00b7tes", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Das an der Grenze der Kunst tr\u00fcbe zur Erde sich senkt.", "tokens": ["Das", "an", "der", "Gren\u00b7ze", "der", "Kunst", "tr\u00fc\u00b7be", "zur", "Er\u00b7de", "sich", "senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Ihm verwandelt nicht mehr Bocazes betrogener Eh'mann,", "tokens": ["Ihm", "ver\u00b7wan\u00b7delt", "nicht", "mehr", "Bo\u00b7ca\u00b7zes", "be\u00b7tro\u00b7ge\u00b7ner", "Eh'\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Den ein Fremder belacht, pl\u00f6tzlich in Galle den Wein.", "tokens": ["Den", "ein", "Frem\u00b7der", "be\u00b7lacht", ",", "pl\u00f6tz\u00b7lich", "in", "Gal\u00b7le", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,", "ADJD", "APPR", "NN", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Und nun ruhet der Streit des Geistes, der immer nach Thaten,", "tokens": ["Und", "nun", "ru\u00b7het", "der", "Streit", "des", "Geis\u00b7tes", ",", "der", "im\u00b7mer", "nach", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Und des K\u00f6rpers, der stets sich nach der Ruhe gesehnt.", "tokens": ["Und", "des", "K\u00f6r\u00b7pers", ",", "der", "stets", "sich", "nach", "der", "Ru\u00b7he", "ge\u00b7sehnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "So, so sank er dahin im sch\u00f6nsten m\u00e4nnlichen Alter,", "tokens": ["So", ",", "so", "sank", "er", "da\u00b7hin", "im", "sch\u00f6ns\u00b7ten", "m\u00e4nn\u00b7li\u00b7chen", "Al\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PAV", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den ich schon herzlich geliebt, als er dem Rehe noch glich,", "tokens": ["Den", "ich", "schon", "herz\u00b7lich", "ge\u00b7liebt", ",", "als", "er", "dem", "Re\u00b7he", "noch", "glich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADJD", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Als sein kr\u00e4ftiger Arm den Federball \u00fcber die Spitze", "tokens": ["Als", "sein", "kr\u00e4f\u00b7ti\u00b7ger", "Arm", "den", "Fe\u00b7der\u00b7ball", "\u00fc\u00b7ber", "die", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Jenes Denkmals trieb, das sich einst ", "tokens": ["Je\u00b7nes", "Denk\u00b7mals", "trieb", ",", "das", "sich", "einst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "$,", "PRELS", "PRF", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Warum kehrtest du nicht zur\u00fcck zur wartenden Heimath?", "tokens": ["Wa\u00b7rum", "kehr\u00b7test", "du", "nicht", "zu\u00b7r\u00fcck", "zur", "war\u00b7ten\u00b7den", "Hei\u00b7math", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hofftest du leichtere Bahn, irgendwo gr\u00f6\u00dferen Preis?", "tokens": ["Hoff\u00b7test", "du", "leich\u00b7te\u00b7re", "Bahn", ",", "ir\u00b7gend\u00b7wo", "gr\u00f6\u00b7\u00dfe\u00b7ren", "Preis", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Du! Am Ufer der ", "tokens": ["Du", "!", "Am", "U\u00b7fer", "der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$.", "APPRART", "NN", "ART"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Dem Verdienste vielleicht engere Schranken gesetzt?", "tokens": ["Dem", "Ver\u00b7diens\u00b7te", "viel\u00b7leicht", "en\u00b7ge\u00b7re", "Schran\u00b7ken", "ge\u00b7setzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ach! dort lie\u00dfest du dich mit Schn\u00fcren binden, von Amorn", "tokens": ["Ach", "!", "dort", "lie\u00b7\u00dfest", "du", "dich", "mit", "Schn\u00fc\u00b7ren", "bin\u00b7den", ",", "von", "A\u00b7morn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,", "APPR", "NE"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Zwar aus Myrthen gekn\u00fcpft, aber so haltbar wie Hanf.", "tokens": ["Zwar", "aus", "Myr\u00b7then", "ge\u00b7kn\u00fcpft", ",", "a\u00b7ber", "so", "halt\u00b7bar", "wie", "Hanf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,", "ADV", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Dennoch verziehen wir leicht dem ausgewanderten Freunde,", "tokens": ["Den\u00b7noch", "ver\u00b7zie\u00b7hen", "wir", "leicht", "dem", "aus\u00b7ge\u00b7wan\u00b7d