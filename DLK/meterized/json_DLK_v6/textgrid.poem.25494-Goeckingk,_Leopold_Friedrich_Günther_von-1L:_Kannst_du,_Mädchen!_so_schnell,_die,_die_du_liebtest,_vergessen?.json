{"textgrid.poem.25494": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kannst du, M\u00e4dchen! so schnell, die, die du liebtest, vergessen?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kannst du, M\u00e4dchen! so schnell, die, die du liebtest, vergessen?", "tokens": ["Kannst", "du", ",", "M\u00e4d\u00b7chen", "!", "so", "schnell", ",", "die", ",", "die", "du", "lieb\u00b7test", ",", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$.", "ADV", "ADJD", "$,", "PRELS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Unser Leben und Tod k\u00fcmmert dich weiter nicht mehr!", "tokens": ["Un\u00b7ser", "Le\u00b7ben", "und", "Tod", "k\u00fcm\u00b7mert", "dich", "wei\u00b7ter", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "So? das h\u00e4tten wir nicht aus deinem Auge gelesen!", "tokens": ["So", "?", "das", "h\u00e4t\u00b7ten", "wir", "nicht", "aus", "dei\u00b7nem", "Au\u00b7ge", "ge\u00b7le\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PDS", "VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aber wir sehen, es tr\u00fcgt wie ein Kalenderprophet.", "tokens": ["A\u00b7ber", "wir", "se\u00b7hen", ",", "es", "tr\u00fcgt", "wie", "ein", "Ka\u00b7len\u00b7der\u00b7pro\u00b7phet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Warum reichtest du sonst mit jedem Morgen uns R\u00fcbsen?", "tokens": ["Wa\u00b7rum", "reich\u00b7test", "du", "sonst", "mit", "je\u00b7dem", "Mor\u00b7gen", "uns", "R\u00fcb\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "PPER", "NN", "$."], "meter": "-----+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sp\u00fcltest mit eigener Hand unsere Gl\u00e4ser so rein?", "tokens": ["Sp\u00fcl\u00b7test", "mit", "ei\u00b7ge\u00b7ner", "Hand", "un\u00b7se\u00b7re", "Gl\u00e4\u00b7ser", "so", "rein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Warum achtetest du des nassen Grases im Garten", "tokens": ["Wa\u00b7rum", "ach\u00b7te\u00b7test", "du", "des", "nas\u00b7sen", "Gra\u00b7ses", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und der Marokinschuh', uns zu gefallen, nicht dann,", "tokens": ["Und", "der", "Ma\u00b7ro\u00b7kin\u00b7schuh'", ",", "uns", "zu", "ge\u00b7fal\u00b7len", ",", "nicht", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Fiel es am Morgen dir ein, mit H\u00fchnerschwalbe den Strickkorb", "tokens": ["Fiel", "es", "am", "Mor\u00b7gen", "dir", "ein", ",", "mit", "H\u00fch\u00b7ner\u00b7schwal\u00b7be", "den", "Strick\u00b7korb"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "PTKVZ", "$,", "APPR", "NN", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Anzuf\u00fcllen f\u00fcr uns, wenn auch die Mutter dich schalt,", "tokens": ["An\u00b7zu\u00b7f\u00fcl\u00b7len", "f\u00fcr", "uns", ",", "wenn", "auch", "die", "Mut\u00b7ter", "dich", "schalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Und das Taschengeld kaum zu ein Paar Schuhen von Quingang", "tokens": ["Und", "das", "Ta\u00b7schen\u00b7geld", "kaum", "zu", "ein", "Paar", "Schu\u00b7hen", "von", "Quin\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "ART", "NN", "NN", "APPR", "NN"], "meter": "--+--+---+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Hoffnung \u00fcbrig lie\u00df? Redet das Sprichwort nicht wahr:", "tokens": ["Hoff\u00b7nung", "\u00fcb\u00b7rig", "lie\u00df", "?", "Re\u00b7det", "das", "Sprich\u00b7wort", "nicht", "wahr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$.", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Wohl aus dem Auge, wohl aus dem Sinne! denn hast du nur Einmal", "tokens": ["Wohl", "aus", "dem", "Au\u00b7ge", ",", "wohl", "aus", "dem", "Sin\u00b7ne", "!", "denn", "hast", "du", "nur", "Ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADV"], "meter": "+--+-+--+--+-+--", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Unsrer in Briefen erw\u00e4hnt? Einmal uns Arme gegr\u00fc\u00dft?", "tokens": ["Uns\u00b7rer", "in", "Brie\u00b7fen", "er\u00b7w\u00e4hnt", "?", "Ein\u00b7mal", "uns", "Ar\u00b7me", "ge\u00b7gr\u00fc\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$.", "ADV", "PPER", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Aber wie z\u00e4rtlich erkundigst du dich nach ", "tokens": ["A\u00b7ber", "wie", "z\u00e4rt\u00b7lich", "er\u00b7kun\u00b7digst", "du", "dich", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "VVFIN", "PPER", "PRF", "APPR"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Weil er die H\u00e4nde dir k\u00fc\u00dft, weil er dich immer umtanzt;", "tokens": ["Weil", "er", "die", "H\u00e4n\u00b7de", "dir", "k\u00fc\u00dft", ",", "weil", "er", "dich", "im\u00b7mer", "um\u00b7tanzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+--+--++--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Weil er, das K\u00f6pfchen gesenkt, den R\u00fccken zur Sichel gekr\u00fcmmet,", "tokens": ["Weil", "er", ",", "das", "K\u00f6pf\u00b7chen", "ge\u00b7senkt", ",", "den", "R\u00fc\u00b7cken", "zur", "Si\u00b7chel", "ge\u00b7kr\u00fcm\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "VVPP", "$,", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Furchtsam n\u00e4her sich schleicht, wenn er Verweise bekommt;", "tokens": ["Furcht\u00b7sam", "n\u00e4\u00b7her", "sich", "schleicht", ",", "wenn", "er", "Ver\u00b7wei\u00b7se", "be\u00b7kommt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PRF", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Duldend, wenn du dich stellst, als k\u00f6nntest du gar ihn bestrafen,", "tokens": ["Dul\u00b7dend", ",", "wenn", "du", "dich", "stellst", ",", "als", "k\u00f6nn\u00b7test", "du", "gar", "ihn", "be\u00b7stra\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.20": {"text": "Sich zu F\u00fc\u00dfen dir legt, und, wie ein Todter, nicht r\u00fchrt;", "tokens": ["Sich", "zu", "F\u00fc\u00b7\u00dfen", "dir", "legt", ",", "und", ",", "wie", "ein", "Tod\u00b7ter", ",", "nicht", "r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "PPER", "VVFIN", "$,", "KON", "$,", "PWAV", "ART", "NN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Weil er necken sich l\u00e4\u00dft, mit immer h\u00f6heren Spr\u00fcngen", "tokens": ["Weil", "er", "ne\u00b7cken", "sich", "l\u00e4\u00dft", ",", "mit", "im\u00b7mer", "h\u00f6\u00b7he\u00b7ren", "Spr\u00fcn\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "VVFIN", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Zucker zu haschen, den du hurtig dem Springer entr\u00fcckst.", "tokens": ["Zu\u00b7cker", "zu", "ha\u00b7schen", ",", "den", "du", "hur\u00b7tig", "dem", "Sprin\u00b7ger", "ent\u00b7r\u00fcckst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Darum bringen ihm nun die Posten Gr\u00fc\u00dfe auf Gr\u00fc\u00dfe,", "tokens": ["Da\u00b7rum", "brin\u00b7gen", "ihm", "nun", "die", "Pos\u00b7ten", "Gr\u00fc\u00b7\u00dfe", "auf", "Gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Darum empfiehlest du ihn wie ein verzogenes Kind.", "tokens": ["Da\u00b7rum", "emp\u00b7fieh\u00b7lest", "du", "ihn", "wie", "ein", "ver\u00b7zo\u00b7ge\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Aber so seyd ihr M\u00e4dchen! Ihr achtet den S\u00e4nger nicht l\u00e4nger", "tokens": ["A\u00b7ber", "so", "seyd", "ihr", "M\u00e4d\u00b7chen", "!", "Ihr", "ach\u00b7tet", "den", "S\u00e4n\u00b7ger", "nicht", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADJD"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Als er euch singt, und nur Schmeichler vergesset ihr nicht.", "tokens": ["Als", "er", "euch", "singt", ",", "und", "nur", "Schmeich\u00b7ler", "ver\u00b7ges\u00b7set", "ihr", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "KON", "ADV", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Warte! Wir r\u00e4chen uns einst, und singen willkommenen Freiern", "tokens": ["War\u00b7te", "!", "Wir", "r\u00e4\u00b7chen", "uns", "einst", ",", "und", "sin\u00b7gen", "will\u00b7kom\u00b7me\u00b7nen", "Frei\u00b7ern"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "ADJA", "ADJA", "NN"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.28": {"text": "Eine Warnung ins Ohr, welche sie alle verscheucht.", "tokens": ["Ei\u00b7ne", "War\u00b7nung", "ins", "Ohr", ",", "wel\u00b7che", "sie", "al\u00b7le", "ver\u00b7scheucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PRELS", "PPER", "PIS", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}}}}