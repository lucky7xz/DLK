{"textgrid.poem.53391": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzt streichet, d\u00fcnckt mich, eben", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt streichet, d\u00fcnckt mich, eben", "tokens": ["Jetzt", "strei\u00b7chet", ",", "d\u00fcnckt", "mich", ",", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vorbey ein halbes Jahr,", "tokens": ["Vor\u00b7bey", "ein", "hal\u00b7bes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ich mit meinem Leben", "tokens": ["Da", "ich", "mit", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Widderawen war.", "tokens": ["Zu", "Wid\u00b7de\u00b7ra\u00b7wen", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was Freud' ich da empfunden,", "tokens": ["Was", "Freud'", "ich", "da", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie dessen Orthes Lust", "tokens": ["Wie", "des\u00b7sen", "Or\u00b7thes", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PDS", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich aller M\u00fch entbunden", "tokens": ["Mich", "al\u00b7ler", "M\u00fch", "ent\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ist mir gnug bewust.", "tokens": ["Das", "ist", "mir", "gnug", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hie waren P\u00fcsch' und Awen,", "tokens": ["Hie", "wa\u00b7ren", "P\u00fcsch'", "und", "A\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hie ein BaumGezelt,", "tokens": ["Und", "hie", "ein", "Baum", "Ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da Berg' und Thal zu schawen,", "tokens": ["Da", "Ber\u00b7g'", "und", "Thal", "zu", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da wieder freyes Feld.", "tokens": ["Da", "wie\u00b7der", "frey\u00b7es", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es schwommen in dem Teiche", "tokens": ["Es", "schwom\u00b7men", "in", "dem", "Tei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dort G\u00e4ns' und Enten hie,", "tokens": ["Dort", "G\u00e4ns'", "und", "En\u00b7ten", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd hinter dem Gestr\u00e4uche", "tokens": ["Vnd", "hin\u00b7ter", "dem", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gieng klein und grosses Vieh.", "tokens": ["Gieng", "klein", "und", "gros\u00b7ses", "Vieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Lass' ich den Garten bleiben", "tokens": ["Lass'", "ich", "den", "Gar\u00b7ten", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seiner Pracht und Zier?", "tokens": ["Mit", "sei\u00b7ner", "Pracht", "und", "Zier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kan ihn nicht beschreiben,", "tokens": ["Ich", "kan", "ihn", "nicht", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Kr\u00e4ffte fehlen mir.", "tokens": ["Die", "Kr\u00e4ff\u00b7te", "feh\u00b7len", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er f\u00fchrt von allen Seiten", "tokens": ["Er", "f\u00fchrt", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Gn\u00fcge die man wei\u00df,", "tokens": ["Die", "Gn\u00fc\u00b7ge", "die", "man", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Natura scheint zu streiten", "tokens": ["Na\u00b7tu\u00b7ra", "scheint", "zu", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hie mit Verstand und Flei\u00df.", "tokens": ["Hie", "mit", "Ver\u00b7stand", "und", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie frey sind seine G\u00e4nge,", "tokens": ["Wie", "frey", "sind", "sei\u00b7ne", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Zweiglein r\u00fchrt dich an,", "tokens": ["Kein", "Zwei\u00b7glein", "r\u00fchrt", "dich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie weit ist ihre L\u00e4nge?", "tokens": ["Wie", "weit", "ist", "ih\u00b7re", "L\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die man nicht absehn kan.", "tokens": ["Die", "man", "nicht", "ab\u00b7sehn", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von den bekolbten W\u00e4nden", "tokens": ["Von", "den", "be\u00b7kolb\u00b7ten", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ward Zweiffel eingebracht", "tokens": ["Ward", "Zweif\u00b7fel", "ein\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ob die\u00df von Menschen-H\u00e4nden", "tokens": ["Ob", "die\u00df", "von", "Men\u00b7schen\u00b7H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch k\u00f6nte seyn gemacht.", "tokens": ["Auch", "k\u00f6n\u00b7te", "seyn", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Als ich kam zu den Blumen", "tokens": ["Als", "ich", "kam", "zu", "den", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch ein gantz-gr\u00fcnes Thor,", "tokens": ["Durch", "ein", "gant\u00b7zgr\u00fc\u00b7nes", "Thor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Kam mir die Lust Idumen", "tokens": ["Kam", "mir", "die", "Lust", "I\u00b7du\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Hertzen heimlich vor.", "tokens": ["Im", "Hert\u00b7zen", "heim\u00b7lich", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die B\u00e4ume sah' ich ragen", "tokens": ["Die", "B\u00e4u\u00b7me", "sah'", "ich", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sprach: was sch\u00f6ne Frucht", "tokens": ["Und", "sprach", ":", "was", "sch\u00f6\u00b7ne", "Frucht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PWS", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "M\u00f6gt ihr dem Herren tragen", "tokens": ["M\u00f6gt", "ihr", "dem", "Her\u00b7ren", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Danck f\u00fcr seine Zucht!", "tokens": ["Zum", "Danck", "f\u00fcr", "sei\u00b7ne", "Zucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Herr Eppinger, es r\u00fchre", "tokens": ["Herr", "Ep\u00b7pin\u00b7ger", ",", "es", "r\u00fch\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Leid dein edles Hau\u00df,", "tokens": ["Kein", "Leid", "dein", "ed\u00b7les", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was du bedenckst, das f\u00fchre", "tokens": ["Was", "du", "be\u00b7denckst", ",", "das", "f\u00fch\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Wunsch auch stattlich aus.", "tokens": ["Dein", "Wunsch", "auch", "statt\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr diese Gn\u00fcg und Frewde,", "tokens": ["F\u00fcr", "die\u00b7se", "Gn\u00fcg", "und", "Frew\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie macht mich \u00fcberreich", "tokens": ["Sie", "macht", "mich", "\u00fc\u00b7berr\u00b7eich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd ist woran ich weide", "tokens": ["Vnd", "ist", "wo\u00b7ran", "ich", "wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gesicht und Hertz zugleich.", "tokens": ["Ge\u00b7sicht", "und", "Hertz", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wolan ich wil ihr lohnen", "tokens": ["Wo\u00b7lan", "ich", "wil", "ihr", "loh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die\u00df was sie mir erzeigt,", "tokens": ["Die\u00df", "was", "sie", "mir", "er\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch nicht mit tausent Cronen,", "tokens": ["Doch", "nicht", "mit", "tau\u00b7sent", "Cro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nein, was sich hie er\u00e4ugt", "tokens": ["Nein", ",", "was", "sich", "hie", "er\u00b7\u00e4ugt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "PRF", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wird mit der Zeit verschwinden,", "tokens": ["Wird", "mit", "der", "Zeit", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "(denn was mu\u00df nicht vergehn?)", "tokens": ["(", "denn", "was", "mu\u00df", "nicht", "ver\u00b7gehn", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PWS", "VMFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch wird man es noch finden", "tokens": ["Doch", "wird", "man", "es", "noch", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In diesem Liede stehn.", "tokens": ["In", "die\u00b7sem", "Lie\u00b7de", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was habt denn ihr empfunden", "tokens": ["Was", "habt", "denn", "ihr", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "KON", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Daselbst f\u00fcr Lust und Ruh,", "tokens": ["Da\u00b7selbst", "f\u00fcr", "Lust", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr Berents? tieffe Wunden", "tokens": ["Herr", "Be\u00b7rents", "?", "tief\u00b7fe", "Wun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$.", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Vnd tausend Leid dazu.", "tokens": ["Vnd", "tau\u00b7send", "Leid", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr wart dahin entwichen", "tokens": ["Ihr", "wart", "da\u00b7hin", "ent\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hie zu entgehn der Noht", "tokens": ["Hie", "zu", "ent\u00b7gehn", "der", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "ART", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Die mercklich kam geschlichen", "tokens": ["Die", "merck\u00b7lich", "kam", "ge\u00b7schli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd schlug uns h\u00e4uffig tod.", "tokens": ["Vnd", "schlug", "uns", "h\u00e4uf\u00b7fig", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tr