hei\u00df,", "tokens": ["Sie", "h\u00f6rt", "auch", "sein", "Ge\u00b7hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd hat das Mahl empfangen", "tokens": ["Vnd", "hat", "das", "Mahl", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die wahre Seelen-St\u00e4rck'.", "tokens": ["Die", "wah\u00b7re", "See\u00b7len\u00b7St\u00e4rck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jedoch trug sie Verlangen", "tokens": ["Je\u00b7doch", "trug", "sie", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Hieher nach K\u00f6nigsberg.", "tokens": ["Hie\u00b7her", "nach", "K\u00f6\u00b7nigs\u00b7berg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Sie baht es nicht zu schweigen", "tokens": ["Sie", "baht", "es", "nicht", "zu", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was jhr bey Ihr gethan.", "tokens": ["Was", "jhr", "bey", "Ihr", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd de\u00df nahm sie zu Zeugen", "tokens": ["Vnd", "de\u00df", "nahm", "sie", "zu", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "PPER", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dort nicht nur einen an.", "tokens": ["Dort", "nicht", "nur", "ei\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hat jhre lieben Kinder", "tokens": ["Hat", "jhre", "lie\u00b7ben", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Vnd was nach Pflege schawt", "tokens": ["Vnd", "was", "nach", "Pfle\u00b7ge", "schawt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr Hau\u00df und euch nicht minder", "tokens": ["Ihr", "Hau\u00df", "und", "euch", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gott' hertzlich anvertrawt.", "tokens": ["Gott'", "hertz\u00b7lich", "an\u00b7ver\u00b7trawt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Vnd einer selign Stunden", "tokens": ["Vnd", "ei\u00b7ner", "se\u00b7lign", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gewartet mit Begier,", "tokens": ["Ge\u00b7war\u00b7tet", "mit", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sich auch bald gefunden", "tokens": ["Die", "sich", "auch", "bald", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In jhrem Hau\u00df alhier,", "tokens": ["In", "jhrem", "Hau\u00df", "al\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd jhren Geist versetzet", "tokens": ["Vnd", "jhren", "Geist", "ver\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "In Christus ewigs Reich,", "tokens": ["In", "Chris\u00b7tus", "e\u00b7wigs", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo sie sich nun ergetzet", "tokens": ["Wo", "sie", "sich", "nun", "er\u00b7get\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Engeln Gottes gleich.", "tokens": ["Den", "En\u00b7geln", "Got\u00b7tes", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Flieht jhr euch mehr zu kr\u00e4ncken,", "tokens": ["Flieht", "jhr", "euch", "mehr", "zu", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Selig' heisst euch nicht", "tokens": ["Die", "Se\u00b7lig'", "heisst", "euch", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar in den Tod euch sencken,", "tokens": ["Gar", "in", "den", "Tod", "euch", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man r\u00fchmt gnug eure Pflicht,", "tokens": ["Man", "r\u00fchmt", "gnug", "eu\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sich gar zu sehr betr\u00fcben", "tokens": ["Sich", "gar", "zu", "sehr", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "PTKA", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fchrt offt auch Heuchel-Schein,", "tokens": ["F\u00fchrt", "offt", "auch", "Heu\u00b7chel\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Lasst, wollt jhr noch was \u00fcben,", "tokens": ["Lasst", ",", "wollt", "jhr", "noch", "was", "\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Lob euch heilig seyn.", "tokens": ["Ihr", "Lob", "euch", "hei\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}