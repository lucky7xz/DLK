{"dta.poem.18053": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "X.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Bauersmann wollt auf den Jahrmarckt reiten,", "tokens": ["Ein", "Bau\u00b7ers\u00b7mann", "wollt", "auf", "den", "Jahr\u00b7marckt", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sein Esel und sein Knabe sollten ihn begleiten;", "tokens": ["Sein", "E\u00b7sel", "und", "sein", "Kna\u00b7be", "soll\u00b7ten", "ihn", "be\u00b7glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Esel trug ihn fort, der Sohn gieng hinten nach.", "tokens": ["Der", "E\u00b7sel", "trug", "ihn", "fort", ",", "der", "Sohn", "gieng", "hin\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein M\u00e4dgen sah den Ritt, es wundert sich, und sprach:", "tokens": ["Ein", "M\u00e4d\u00b7gen", "sah", "den", "Ritt", ",", "es", "wun\u00b7dert", "sich", ",", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "F\u00fcrwahr es dauret mich des Knaben;", "tokens": ["F\u00fcr\u00b7wahr", "es", "dau\u00b7ret", "mich", "des", "Kna\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er ist zu zart dem Esel nachzutraben,", "tokens": ["Er", "ist", "zu", "zart", "dem", "E\u00b7sel", "nach\u00b7zu\u00b7tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da dieser starckgebeinte Mann", "tokens": ["Da", "die\u00b7ser", "starck\u00b7ge\u00b7bein\u00b7te", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Inde\u00df zu Pferde sitzt, das ist nicht wohl gethan.", "tokens": ["In\u00b7de\u00df", "zu", "Pfer\u00b7de", "sitzt", ",", "das", "ist", "nicht", "wohl", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,", "PDS", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da die\u00df der Alte h\u00f6rt, macht er den Sattel leer,", "tokens": ["Da", "die\u00df", "der", "Al\u00b7te", "h\u00f6rt", ",", "macht", "er", "den", "Sat\u00b7tel", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er setzt den Knaben auf, und geht zu Fu\u00df einher.", "tokens": ["Er", "setzt", "den", "Kna\u00b7ben", "auf", ",", "und", "geht", "zu", "Fu\u00df", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Darauf begegnen ihm zween Greisen,", "tokens": ["Da\u00b7rauf", "be\u00b7geg\u00b7nen", "ihm", "zween", "Grei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die mit den Fingern auf ihn weisen;", "tokens": ["Die", "mit", "den", "Fin\u00b7gern", "auf", "ihn", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und einer spricht: Es ist wohl Schein,", "tokens": ["Und", "ei\u00b7ner", "spricht", ":", "Es", "ist", "wohl", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Alte mu\u00df nicht recht bey Sinnen seyn,", "tokens": ["Der", "Al\u00b7te", "mu\u00df", "nicht", "recht", "bey", "Sin\u00b7nen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJD", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Er geht als ein Lakey dem Esel an der Seiten,", "tokens": ["Er", "geht", "als", "ein", "La\u00b7key", "dem", "E\u00b7sel", "an", "der", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Er k\u00f6nnte doch zugleich mit seinem Buben reiten.", "tokens": ["Er", "k\u00f6nn\u00b7te", "doch", "zu\u00b7gleich", "mit", "sei\u00b7nem", "Bu\u00b7ben", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Alte folgt dem Rath, er sizt zu seinem Sohn", "tokens": ["Der", "Al\u00b7te", "folgt", "dem", "Rath", ",", "er", "sizt", "zu", "sei\u00b7nem", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dem Esel auf das Kreutz, und reitet braf davon.", "tokens": ["Dem", "E\u00b7sel", "auf", "das", "Kreutz", ",", "und", "rei\u00b7tet", "braf", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nicht lange reiten alle beyde,", "tokens": ["Nicht", "lan\u00b7ge", "rei\u00b7ten", "al\u00b7le", "bey\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PIAT", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "So wards dem alten Mann zu Leide. ", "tokens": ["So", "wards", "dem", "al\u00b7ten", "Mann", "zu", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Denn als mehr Leute ihm bekamen, ", "tokens": ["Denn", "als", "mehr", "Leu\u00b7te", "ihm", "be\u00b7ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "So sagten sie; In Midas Nahmen,", "tokens": ["So", "sag\u00b7ten", "sie", ";", "In", "Mi\u00b7das", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Was vor ein alter Thor f\u00e4hrt dort mit seinem Knaben", "tokens": ["Was", "vor", "ein", "al\u00b7ter", "Thor", "f\u00e4hrt", "dort", "mit", "sei\u00b7nem", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auf einem Eselgen? Er will es wohl todt haben.", "tokens": ["Auf", "ei\u00b7nem", "E\u00b7sel\u00b7gen", "?", "Er", "will", "es", "wohl", "todt", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch k\u00f6nnt er dieses leichtlich wenden,", "tokens": ["Doch", "k\u00f6nnt", "er", "die\u00b7ses", "leicht\u00b7lich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und mit dem muntern Sohn den Weg zu Fu\u00df vollenden.", "tokens": ["Und", "mit", "dem", "mun\u00b7tern", "Sohn", "den", "Weg", "zu", "Fu\u00df", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da dieses Wort geschah, sprach jener zu dem Knaben,", "tokens": ["Da", "die\u00b7ses", "Wort", "ge\u00b7schah", ",", "sprach", "je\u00b7ner", "zu", "dem", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "$,", "VVFIN", "PDAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Hinunter Sohn, wir sollen beyde traben;", "tokens": ["Hin\u00b7un\u00b7ter", "Sohn", ",", "wir", "sol\u00b7len", "bey\u00b7de", "tra\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Der Esel mu\u00df auch Ruhe haben.", "tokens": ["Der", "E\u00b7sel", "mu\u00df", "auch", "Ru\u00b7he", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Nach diesem riefen Mann und Frau", "tokens": ["Nach", "die\u00b7sem", "rie\u00b7fen", "Mann", "und", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Aus einem Munde: Ey doch, schau!", "tokens": ["Aus", "ei\u00b7nem", "Mun\u00b7de", ":", "Ey", "doch", ",", "schau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NN", "ADV", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Wie th\u00f6rigt diese beyde sind,", "tokens": ["Wie", "th\u00f6\u00b7rigt", "die\u00b7se", "bey\u00b7de", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDAT", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der alte Mann so wie sein Kind,", "tokens": ["Der", "al\u00b7te", "Mann", "so", "wie", "sein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Da\u00df nicht ihr Sinn zu reiten steht, ", "tokens": ["Da\u00df", "nicht", "ihr", "Sinn", "zu", "rei\u00b7ten", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "PTKZU", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Da doch der Esel ledig geht!", "tokens": ["Da", "doch", "der", "E\u00b7sel", "le\u00b7dig", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Ein Wunder, da\u00df nicht sie den Esel tragen!", "tokens": ["Ein", "Wun\u00b7der", ",", "da\u00df", "nicht", "sie", "den", "E\u00b7sel", "tra\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PTKNEG", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Drauf sprach der Vater: Sohn wohlher!", "tokens": ["Drauf", "sprach", "der", "Va\u00b7ter", ":", "Sohn", "wohl\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$.", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Der Esel ist uns nicht zu schwer,", "tokens": ["Der", "E\u00b7sel", "ist", "uns", "nicht", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Da\u00df wir ihn nicht wohl m\u00f6gten tragen.", "tokens": ["Da\u00df", "wir", "ihn", "nicht", "wohl", "m\u00f6g\u00b7ten", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "La\u00df sehn, was dann die Leute sagen.", "tokens": ["La\u00df", "sehn", ",", "was", "dann", "die", "Leu\u00b7te", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Sie warffen bald den Esel nieder,", "tokens": ["Sie", "warf\u00b7fen", "bald", "den", "E\u00b7sel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und banden ihm den Kopf und alle Glieder.", "tokens": ["Und", "ban\u00b7den", "ihm", "den", "Kopf", "und", "al\u00b7le", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Sie h\u00e4ngten ihn an eine Stangen;", "tokens": ["Sie", "h\u00e4ng\u00b7ten", "ihn", "an", "ei\u00b7ne", "Stan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Doch w\u00e4r er freudiger zu Fu\u00df gegangen.", "tokens": ["Doch", "w\u00e4r", "er", "freu\u00b7di\u00b7ger", "zu", "Fu\u00df", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Bald ward ein gro\u00df Geschrey, man rief von Ort zu Ort:", "tokens": ["Bald", "ward", "ein", "gro\u00df", "Ge\u00b7schrey", ",", "man", "rief", "von", "Ort", "zu", "Ort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "NN", "$,", "PIS", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Zween tragen ihren Esel fort,", "tokens": ["Zween", "tra\u00b7gen", "ih\u00b7ren", "E\u00b7sel", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Der billiger sie beyde sollte tragen.", "tokens": ["Der", "bil\u00b7li\u00b7ger", "sie", "bey\u00b7de", "soll\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Man mag es wohl zur neuen M\u00e4hre sagen. ", "tokens": ["Man", "mag", "es", "wohl", "zur", "neu\u00b7en", "M\u00e4h\u00b7re", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Man sieht wohl, da\u00df sie Narren sind,", "tokens": ["Man", "sieht", "wohl", ",", "da\u00df", "sie", "Nar\u00b7ren", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "An Witze sind sie beyde blind. ", "tokens": ["An", "Wit\u00b7ze", "sind", "sie", "bey\u00b7de", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Der Alte h\u00f6rte wohl, da\u00df jedermann", "tokens": ["Der", "Al\u00b7te", "h\u00f6r\u00b7te", "wohl", ",", "da\u00df", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Nur \u00fcbels von ihm redt. Er fieng izt seuftzend an:", "tokens": ["Nur", "\u00fc\u00b7bels", "von", "ihm", "redt", ".", "Er", "fi\u00b7eng", "izt", "seuft\u00b7zend", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.53": {"text": "Mein Sohn, du h\u00f6rst, der Esel trage mich,", "tokens": ["Mein", "Sohn", ",", "du", "h\u00f6rst", ",", "der", "E\u00b7sel", "tra\u00b7ge", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "So ists nicht recht; er trage dich,", "tokens": ["So", "ists", "nicht", "recht", ";", "er", "tra\u00b7ge", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADJD", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "So zehlet man mich zu den Thoren;", "tokens": ["So", "zeh\u00b7let", "man", "mich", "zu", "den", "Tho\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.56": {"text": "Tr\u00e4gt er uns beyde dann, so ist mein Witz verlohren", "tokens": ["Tr\u00e4gt", "er", "uns", "bey\u00b7de", "dann", ",", "so", "ist", "mein", "Witz", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PIS", "ADV", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Und geht er ledig fort, so wei\u00df ich nicht zu leben;", "tokens": ["Und", "geht", "er", "le\u00b7dig", "fort", ",", "so", "wei\u00df", "ich", "nicht", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Wenn wir ihn dann auf unsre Schultern heben,", "tokens": ["Wenn", "wir", "ihn", "dann", "auf", "uns\u00b7re", "Schul\u00b7tern", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.59": {"text": "So ist kein Mensch unsinniger als wir.", "tokens": ["So", "ist", "kein", "Mensch", "un\u00b7sin\u00b7ni\u00b7ger", "als", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.60": {"text": "Wie ist ihm denn zu thun? Der beste Rath ist hier:", "tokens": ["Wie", "ist", "ihm", "denn", "zu", "thun", "?", "Der", "bes\u00b7te", "Rath", "ist", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$.", "ART", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "In allen Handlungen thu recht und wohl.", "tokens": ["In", "al\u00b7len", "Hand\u00b7lun\u00b7gen", "thu", "recht", "und", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.62": {"text": "Und sieh nicht auf die Welt, sie ist der Schalckheit voll;", "tokens": ["Und", "sieh", "nicht", "auf", "die", "Welt", ",", "sie", "ist", "der", "Schalck\u00b7heit", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PTKNEG", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Und kan nicht ohne Tadeln seyn.", "tokens": ["Und", "kan", "nicht", "oh\u00b7ne", "Ta\u00b7deln", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "De\u00dfwegen h\u00fclle dich in deine Tugend ein.", "tokens": ["De\u00df\u00b7we\u00b7gen", "h\u00fcl\u00b7le", "dich", "in", "dei\u00b7ne", "Tu\u00b7gend", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Thu was du thust, nach des Gewissens Licht", "tokens": ["Thu", "was", "du", "thust", ",", "nach", "des", "Ge\u00b7wis\u00b7sens", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PWS", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.66": {"text": "Und f\u00fcrchte Gottes Zorn, und nicht der Welt Gericht.", "tokens": ["Und", "f\u00fcrch\u00b7te", "Got\u00b7tes", "Zorn", ",", "und", "nicht", "der", "Welt", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "$,", "KON", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}