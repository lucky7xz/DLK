{"textgrid.poem.40192": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Selbstent\u00e4uszerung: Selbsterf\u00fcllung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah durch deine Seele in die Welt", "tokens": ["Ich", "sah", "durch", "dei\u00b7ne", "See\u00b7le", "in", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und in die eigne Seele: stumm versanken", "tokens": ["und", "in", "die", "eig\u00b7ne", "See\u00b7le", ":", "stumm", "ver\u00b7san\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$.", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "im Strom des Schauens zwischen uns die Schranken,", "tokens": ["im", "Strom", "des", "Schau\u00b7ens", "zwi\u00b7schen", "uns", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "es ruhten Welt und Du in Mir gesellt.", "tokens": ["es", "ruh\u00b7ten", "Welt", "und", "Du", "in", "Mir", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "PPER", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Auge sah ich wesenlos erhellt:", "tokens": ["Dein", "Au\u00b7ge", "sah", "ich", "we\u00b7sen\u00b7los", "er\u00b7hellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erleuchtung fluteten, Erleuchtung tranken", "tokens": ["Er\u00b7leuch\u00b7tung", "flu\u00b7te\u00b7ten", ",", "Er\u00b7leuch\u00b7tung", "tran\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zusammenstr\u00f6mend unsre Zwiegedanken,", "tokens": ["zu\u00b7sam\u00b7men\u00b7str\u00f6\u00b7mend", "uns\u00b7re", "Zwie\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in Deiner Seele ruhte Meine Welt.", "tokens": ["in", "Dei\u00b7ner", "See\u00b7le", "ruh\u00b7te", "Mei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und heilig d\u00e4uchten mir \u2013 ob dumpf und kalt", "tokens": ["Und", "hei\u00b7lig", "d\u00e4uch\u00b7ten", "mir", "\u2013", "ob", "dumpf", "und", "kalt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$(", "KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "die Welt es leugne \u2013 unsrer L\u00fcste Hehle,", "tokens": ["die", "Welt", "es", "leug\u00b7ne", "\u2013", "uns\u00b7rer", "L\u00fcs\u00b7te", "Heh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "verkl\u00e4rt zu Lauterkeiten unsre Fehle", "tokens": ["ver\u00b7kl\u00e4rt", "zu", "Lau\u00b7ter\u00b7kei\u00b7ten", "uns\u00b7re", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "durch dieses Blickes tiefe Lichtgewalt;", "tokens": ["durch", "die\u00b7ses", "Bli\u00b7ckes", "tie\u00b7fe", "Licht\u00b7ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "denn Inbrunst ist die Freiheit der Gestalt", "tokens": ["denn", "In\u00b7brunst", "ist", "die", "Frei\u00b7heit", "der", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "vom Zwang der Welt, vom Bann der eignen Seele.", "tokens": ["vom", "Zwang", "der", "Welt", ",", "vom", "Bann", "der", "eig\u00b7nen", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}