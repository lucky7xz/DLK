{"dta.poem.16321": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O klage nicht, mein Herz, da\u00df dir zu sp\u00e4t nun kommen", "tokens": ["O", "kla\u00b7ge", "nicht", ",", "mein", "Herz", ",", "da\u00df", "dir", "zu", "sp\u00e4t", "nun", "kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKA", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Liebe Zeichen, da die Jugend dir verglommen.", "tokens": ["Der", "Lie\u00b7be", "Zei\u00b7chen", ",", "da", "die", "Ju\u00b7gend", "dir", "ver\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja, w\u00e4r' es Gold und Gut, und W\u00fcrd' und Wohlbehagen,", "tokens": ["Ja", ",", "w\u00e4r'", "es", "Gold", "und", "Gut", ",", "und", "W\u00fcrd'", "und", "Wohl\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PPER", "NN", "KON", "ADJD", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So m\u00f6chtest du, da\u00df nun zu sp\u00e4t es komme, klagen.", "tokens": ["So", "m\u00f6ch\u00b7test", "du", ",", "da\u00df", "nun", "zu", "sp\u00e4t", "es", "kom\u00b7me", ",", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "ADV", "PTKA", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bald lassen m\u00fc\u00dftest du zur\u00fcck dis Hausger\u00e4th;", "tokens": ["Bald", "las\u00b7sen", "m\u00fc\u00df\u00b7test", "du", "zu\u00b7r\u00fcck", "dis", "Haus\u00b7ge\u00b7r\u00e4\u00b7th", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "PTKVZ", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch was hin\u00fcber du mitnimmst, kommt nicht zu sp\u00e4t.", "tokens": ["Doch", "was", "hin\u00b7\u00fc\u00b7ber", "du", "mit\u00b7nimmst", ",", "kommt", "nicht", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}