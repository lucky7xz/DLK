{"textgrid.poem.51250": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Meister bin ich worden,", "tokens": ["Ein", "Meis\u00b7ter", "bin", "ich", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu weben Gram und Leid;", "tokens": ["Zu", "we\u00b7ben", "Gram", "und", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich webe Tag und N\u00e4chte", "tokens": ["Ich", "we\u00b7be", "Tag", "und", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am schweren Trauerkleid.", "tokens": ["Am", "schwe\u00b7ren", "Trau\u00b7er\u00b7kleid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schlepp es auf der Stra\u00dfe", "tokens": ["Ich", "schlepp", "es", "auf", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "M\u00fchselig und bestaubt;", "tokens": ["M\u00fch\u00b7se\u00b7lig", "und", "be\u00b7staubt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich trag von spitzen Dornen", "tokens": ["Ich", "trag", "von", "spit\u00b7zen", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Kr\u00e4nzlein auf dem Haupt", "tokens": ["Ein", "Kr\u00e4nz\u00b7lein", "auf", "dem", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Sonne steht am Himmel,", "tokens": ["Die", "Son\u00b7ne", "steht", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie sieht es und sie lacht:", "tokens": ["Sie", "sieht", "es", "und", "sie", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was geht da f\u00fcr ein Zwerglein", "tokens": ["Was", "geht", "da", "f\u00fcr", "ein", "Zwerg\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einer K\u00f6nigstracht?", "tokens": ["In", "ei\u00b7ner", "K\u00f6\u00b7nigs\u00b7tracht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich lege Kron und Mantel", "tokens": ["Ich", "le\u00b7ge", "Kron", "und", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Besch\u00e4mt am Wege hin", "tokens": ["Be\u00b7sch\u00e4mt", "am", "We\u00b7ge", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mu\u00df nun ohne Trauer", "tokens": ["Und", "mu\u00df", "nun", "oh\u00b7ne", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ohne Freuden ziehn!", "tokens": ["Und", "oh\u00b7ne", "Freu\u00b7den", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}