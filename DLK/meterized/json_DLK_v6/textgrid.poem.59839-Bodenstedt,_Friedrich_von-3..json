{"textgrid.poem.59839": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6re, was der Volksmund spricht:", "tokens": ["H\u00f6\u00b7re", ",", "was", "der", "Volks\u00b7mund", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer die Wahrheit liebt, der mu\u00df", "tokens": ["Wer", "die", "Wahr\u00b7heit", "liebt", ",", "der", "mu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "ART", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon sein Pferd am Z\u00fcgel haben \u2013", "tokens": ["Schon", "sein", "Pferd", "am", "Z\u00fc\u00b7gel", "ha\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPRART", "NN", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer die Wahrheit denkt, der mu\u00df", "tokens": ["Wer", "die", "Wahr\u00b7heit", "denkt", ",", "der", "mu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "ART", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schon den Fu\u00df im B\u00fcgel haben \u2013", "tokens": ["Schon", "den", "Fu\u00df", "im", "B\u00fc\u00b7gel", "ha\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer die Wahrheit spricht, der mu\u00df", "tokens": ["Wer", "die", "Wahr\u00b7heit", "spricht", ",", "der", "mu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "ART", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Statt der Arme Fl\u00fcgel haben!", "tokens": ["Statt", "der", "Ar\u00b7me", "Fl\u00fc\u00b7gel", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und doch singt Mirza-Schaffy:", "tokens": ["Und", "doch", "singt", "Mir\u00b7za\u00b7Schaffy", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NE", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.9": {"text": "Wer da l\u00fcgt, mu\u00df Pr\u00fcgel haben!", "tokens": ["Wer", "da", "l\u00fcgt", ",", "mu\u00df", "Pr\u00fc\u00b7gel", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VMFIN", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}