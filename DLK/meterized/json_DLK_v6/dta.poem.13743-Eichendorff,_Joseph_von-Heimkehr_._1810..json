{"dta.poem.13743": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Heimw\u00e4rts kam ich sp\u00e4t gezogen,", "tokens": ["Heim\u00b7w\u00e4rts", "kam", "ich", "sp\u00e4t", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem v\u00e4terlichen Haus,", "tokens": ["Nach", "dem", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Gedanken weit geflogen", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", "weit", "ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Berg und Thal voraus.", "tokens": ["Ue\u00b7ber", "Berg", "und", "Thal", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur noch hier aus diesem Walde!", "tokens": ["Nur", "noch", "hier", "aus", "die\u00b7sem", "Wal\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprach ich, streichelt' sanft mein Ro\u00df,", "tokens": ["Sprach", "ich", ",", "strei\u00b7chelt'", "sanft", "mein", "Ro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gold'nen Haber kriegst du balde,", "tokens": ["Gold'\u00b7nen", "Ha\u00b7ber", "kriegst", "du", "bal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruh'n wir aus auf lichtem Schlo\u00df.", "tokens": ["Ruh'n", "wir", "aus", "auf", "lich\u00b7tem", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}