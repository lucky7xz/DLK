{"textgrid.poem.36746": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Darum irrtest du nicht, in der sinnlichen H\u00fclle dich t\u00e4uschend,", "tokens": ["Da\u00b7rum", "irr\u00b7test", "du", "nicht", ",", "in", "der", "sinn\u00b7li\u00b7chen", "H\u00fcl\u00b7le", "dich", "t\u00e4u\u00b7schend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sinnen erscheint nur der Sinn, aber dem Geiste der Geist.", "tokens": ["Sin\u00b7nen", "er\u00b7scheint", "nur", "der", "Sinn", ",", "a\u00b7ber", "dem", "Geis\u00b7te", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "So erkanntest du sie, wie sie ist, die G\u00f6ttliche, Ernste,", "tokens": ["So", "er\u00b7kann\u00b7test", "du", "sie", ",", "wie", "sie", "ist", ",", "die", "G\u00f6tt\u00b7li\u00b7che", ",", "Erns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "So begeisterte sie dich zum geweihtesten Werk.", "tokens": ["So", "be\u00b7geis\u00b7ter\u00b7te", "sie", "dich", "zum", "ge\u00b7weih\u00b7tes\u00b7ten", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}