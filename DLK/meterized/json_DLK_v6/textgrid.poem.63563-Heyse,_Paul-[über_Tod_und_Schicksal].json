{"textgrid.poem.63563": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[\u00fcber Tod und Schicksal]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber Tod und Schicksal", "tokens": ["\u00dc\u00b7ber", "Tod", "und", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tr\u00f6stet die Sch\u00f6nheit allein,", "tokens": ["Tr\u00f6s\u00b7tet", "die", "Sch\u00f6n\u00b7heit", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Lichtet die n\u00e4chtlichen Kl\u00fcfte,", "tokens": ["Lich\u00b7tet", "die", "n\u00e4cht\u00b7li\u00b7chen", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sonnegemiedene Gr\u00fcfte", "tokens": ["Son\u00b7ne\u00b7ge\u00b7mie\u00b7de\u00b7ne", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Stell umgoldend wie Mondenschein.", "tokens": ["Stell", "um\u00b7gol\u00b7dend", "wie", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KOKOM", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wenn dir Tod und Schicksal", "tokens": ["Wenn", "dir", "Tod", "und", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gl\u00fcck und Jugend geraubt,", "tokens": ["Gl\u00fcck", "und", "Ju\u00b7gend", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nur an der Sch\u00f6nheit Busen,", "tokens": ["Nur", "an", "der", "Sch\u00f6n\u00b7heit", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur vom Hauche der Musen", "tokens": ["Nur", "vom", "Hau\u00b7che", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Heilt das Herz dir und hofft und glaubt.", "tokens": ["Heilt", "das", "Herz", "dir", "und", "hofft", "und", "glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}