{"textgrid.poem.53833": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Lorbeeren der herrschenden Klasse", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sitzt f\u00fcr uns alle.", "tokens": ["Du", "sitzt", "f\u00fcr", "uns", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIS", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unersch\u00fctterlich.", "tokens": ["Un\u00b7er\u00b7sch\u00fct\u00b7ter\u00b7lich", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wir gedenken deiner. Wir gr\u00fc\u00dfen dich.", "tokens": ["Wir", "ge\u00b7den\u00b7ken", "dei\u00b7ner", ".", "Wir", "gr\u00fc\u00b7\u00dfen", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Als es aus war, hast du deinen Kopf hingehalten.", "tokens": ["Als", "es", "aus", "war", ",", "hast", "du", "dei\u00b7nen", "Kopf", "hin\u00b7ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "VAFIN", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Gegen die Presse, die B\u00fcrger, die Polizei \u2013 gegen alle Gewalten.", "tokens": ["Ge\u00b7gen", "die", "Pres\u00b7se", ",", "die", "B\u00fcr\u00b7ger", ",", "die", "Po\u00b7li\u00b7zei", "\u2013", "ge\u00b7gen", "al\u00b7le", "Ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$(", "APPR", "PIAT", "NN", "$."], "meter": "+--+--+--+-+--+--+-", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Als es aus war, hast du vor Gericht gestanden.", "tokens": ["Als", "es", "aus", "war", ",", "hast", "du", "vor", "Ge\u00b7richt", "ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "VAFIN", "$,", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Als ein Mann!", "tokens": ["Als", "ein", "Mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Alle Paragraphen wurden zuschanden.", "tokens": ["Al\u00b7le", "Pa\u00b7ra\u00b7gra\u00b7phen", "wur\u00b7den", "zu\u00b7schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Richter funkelte \u2013 wei\u00df vor ohnm\u00e4chtiger Wut.", "tokens": ["Der", "Rich\u00b7ter", "fun\u00b7kel\u00b7te", "\u2013", "wei\u00df", "vor", "ohn\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Wut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--++-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Du sahst ihn nur an wie der Hauptmann den dummen Rekrut.", "tokens": ["Du", "sahst", "ihn", "nur", "an", "wie", "der", "Haupt\u00b7mann", "den", "dum\u00b7men", "Re\u00b7krut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}}, "stanza.3": {"line.1": {"text": "Der Richter kreischte und schimpfte unfl\u00e4tig \u2013 gemein.", "tokens": ["Der", "Rich\u00b7ter", "kreischte", "und", "schimpf\u00b7te", "un\u00b7fl\u00e4\u00b7tig", "\u2013", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADJD", "$(", "ADJD", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Da standest du auf! Und spieest der Justiz mitten in ihr Gesicht hinein!", "tokens": ["Da", "stan\u00b7dest", "du", "auf", "!", "Und", "spie\u00b7est", "der", "Jus\u00b7tiz", "mit\u00b7ten", "in", "ihr", "Ge\u00b7sicht", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-++-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "\u00bbwer seid ihr?\u00ab Und: \u00bbIch erkenne dies Gericht nicht an!\u00ab", "tokens": ["\u00bb", "wer", "seid", "ihr", "?", "\u00ab", "Und", ":", "\u00bb", "Ich", "er\u00b7ken\u00b7ne", "dies", "Ge\u00b7richt", "nicht", "an", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$.", "$(", "KON", "$.", "$(", "PPER", "VVFIN", "PDS", "NN", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Und: \u00bbWas k\u00f6nnt ihr mir schon \u2013?\u00ab", "tokens": ["Und", ":", "\u00bb", "Was", "k\u00f6nnt", "ihr", "mir", "schon", "\u2013", "?", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "$.", "$(", "PWS", "VVFIN", "PPER", "PPER", "ADV", "$(", "$.", "$("], "meter": "-+---+", "measure": "dactylic.init"}, "line.5": {"text": "Die zappelnden Talare \u00fcbert\u00f6nte dein Ruf:", "tokens": ["Die", "zap\u00b7peln\u00b7den", "Ta\u00b7la\u00b7re", "\u00fc\u00b7ber\u00b7t\u00f6n\u00b7te", "dein", "Ruf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "\u00bbes lebe die Weltrevolution \u2013!\u00ab", "tokens": ["\u00bb", "es", "le\u00b7be", "die", "Welt\u00b7re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$(", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Jetzt sitzt du im Zuchthaus.", "tokens": ["Jetzt", "sitzt", "du", "im", "Zucht\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In der Hand von Gef\u00e4ngnisw\u00e4rtern und Direktoren.", "tokens": ["In", "der", "Hand", "von", "Ge\u00b7f\u00e4ng\u00b7nis\u00b7w\u00e4r\u00b7tern", "und", "Di\u00b7rek\u00b7to\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Du wirst schikaniert, geschlagen, gequ\u00e4lt . . .", "tokens": ["Du", "wirst", "schi\u00b7ka\u00b7niert", ",", "ge\u00b7schla\u00b7gen", ",", "ge\u00b7qu\u00e4lt", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "$,", "VVPP", "$,", "VVPP", "$.", "$.", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du hast den Mut nicht verloren.", "tokens": ["Du", "hast", "den", "Mut", "nicht", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Tausende sitzen wie du. Tapfer, ohne zu klagen, stumm.", "tokens": ["Tau\u00b7sen\u00b7de", "sit\u00b7zen", "wie", "du", ".", "Tap\u00b7fer", ",", "oh\u00b7ne", "zu", "kla\u00b7gen", ",", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "KOKOM", "PPER", "$.", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$,", "ADJD", "$."], "meter": "+--+-+-+-+--+-+", "measure": "iambic.septa.invert"}, "line.6": {"text": "Opfer der Richter. Wer k\u00fcmmert sich drum \u2013?", "tokens": ["Op\u00b7fer", "der", "Rich\u00b7ter", ".", "Wer", "k\u00fcm\u00b7mert", "sich", "drum", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "PWS", "VVFIN", "PRF", "PAV", "$(", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer \u2013?", "tokens": ["Wer", "\u2013", "?"], "token_info": ["word", "punct", "punct"], "pos": ["PWS", "$(", "$."], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Wenn wo Proletarier zusammenstehn,", "tokens": ["Wenn", "wo", "Pro\u00b7le\u00b7ta\u00b7rier", "zu\u00b7sam\u00b7men\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wenn sie deinen Namen h\u00f6ren, dein Bildnis sehn \u2013", "tokens": ["wenn", "sie", "dei\u00b7nen", "Na\u00b7men", "h\u00f6\u00b7ren", ",", "dein", "Bild\u00b7nis", "sehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "dann wird es ganz still. Die K\u00f6pfe neigen sich.", "tokens": ["dann", "wird", "es", "ganz", "still", ".", "Die", "K\u00f6p\u00b7fe", "nei\u00b7gen", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Du sitzt f\u00fcr sie alle.", "tokens": ["Du", "sitzt", "f\u00fcr", "sie", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIS", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie geloben Rache. Schweigen . . .", "tokens": ["Sie", "ge\u00b7lo\u00b7ben", "Ra\u00b7che", ".", "Schwei\u00b7gen", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und gr\u00fc\u00dfen dich.", "tokens": ["Und", "gr\u00fc\u00b7\u00dfen", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}