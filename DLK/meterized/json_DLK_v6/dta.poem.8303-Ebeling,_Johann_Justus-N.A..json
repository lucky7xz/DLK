{"dta.poem.8303": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mann der du am Ver-", "tokens": ["Mann", "der", "du", "am", "Ver"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NE", "APPRART", "TRUNC"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Als wie ein Mandelbaum im Kirchengarten", "tokens": ["Als", "wie", "ein", "Man\u00b7del\u00b7baum", "im", "Kir\u00b7chen\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verg\u00f6nne da\u00df ich darf der Welt den Trieb", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "da\u00df", "ich", "darf", "der", "Welt", "den", "Trieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PPER", "VMFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den in mir Hochachtung und Lieb und Dank", "tokens": ["Den", "in", "mir", "Hoch\u00b7ach\u00b7tung", "und", "Lieb", "und", "Dank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "NN", "KON", "NN", "KON", "NN"], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Seitdem ich Dich gekannt, und Hildesheim", "tokens": ["Seit\u00b7dem", "ich", "Dich", "ge\u00b7kannt", ",", "und", "Hil\u00b7des\u00b7heim"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "PPER", "PRF", "VVPP", "$,", "KON", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Seitdem hat Dich mein Herz mit innren", "tokens": ["Seit\u00b7dem", "hat", "Dich", "mein", "Herz", "mit", "inn\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Billigkeit befiehlt, Vernunft und Schrift", "tokens": ["Die", "Bil\u00b7lig\u00b7keit", "be\u00b7fiehlt", ",", "Ver\u00b7nunft", "und", "Schrift"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df in der Seele mu\u00df der Liebe Feuer lo-", "tokens": ["Da\u00df", "in", "der", "See\u00b7le", "mu\u00df", "der", "Lie\u00b7be", "Feu\u00b7er", "lo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn man dergleichen sieht, die an der Wis-", "tokens": ["Wenn", "man", "derg\u00b7lei\u00b7chen", "sieht", ",", "die", "an", "der", "Wis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PIS", "VVFIN", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Zeit und Fleis erwirbt, die an der Ur-", "tokens": ["Die", "Zeit", "und", "Fleis", "er\u00b7wirbt", ",", "die", "an", "der", "Ur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die durch Erfahrung w\u00e4chst, in einem Leh-", "tokens": ["Die", "durch", "Er\u00b7fah\u00b7rung", "w\u00e4chst", ",", "in", "ei\u00b7nem", "Leh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Zu einem Musterbild sind aufgestellet wor-", "tokens": ["Zu", "ei\u00b7nem", "Mus\u00b7ter\u00b7bild", "sind", "auf\u00b7ge\u00b7stel\u00b7let", "wor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das ist Dein Conterfait, ich finde das an", "tokens": ["Das", "ist", "Dein", "Con\u00b7ter\u00b7fait", ",", "ich", "fin\u00b7de", "das", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PDS", "PTKVZ"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Ein Wissen ohne Stolz, ist Deine wahre", "tokens": ["Ein", "Wis\u00b7sen", "oh\u00b7ne", "Stolz", ",", "ist", "Dei\u00b7ne", "wah\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "VAFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Klugheit ist dein Schmuck, ein gr\u00fcndli-", "tokens": ["Die", "Klug\u00b7heit", "ist", "dein", "Schmuck", ",", "ein", "gr\u00fcnd\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der beste Ehrenkranz an Deinen grauen", "tokens": ["Der", "bes\u00b7te", "Eh\u00b7ren\u00b7kranz", "an", "Dei\u00b7nen", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ich nehme Theil daran; der Alten klugen", "tokens": ["Ich", "neh\u00b7me", "Theil", "da\u00b7ran", ";", "der", "Al\u00b7ten", "klu\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PAV", "$.", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ist ein erw\u00fcnschtes Gl\u00fck das ein noch i\u00fcn-", "tokens": ["Ist", "ein", "er\u00b7w\u00fcnschtes", "Gl\u00fck", "das", "ein", "noch", "i\u00b7\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "ART", "ADV", "TRUNC"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Das Vorbild dienet ihm zum reizenden", "tokens": ["Das", "Vor\u00b7bild", "die\u00b7net", "ihm", "zum", "rei\u00b7zen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Vornemlich wenn er dient als Lehrer in dem", "tokens": ["Vor\u00b7nem\u00b7lich", "wenn", "er", "dient", "als", "Leh\u00b7rer", "in", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "KOUS", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.22": {"text": "Und ihre Warheit zeigt; wodurch wir zu", "tokens": ["Und", "ih\u00b7re", "War\u00b7heit", "zeigt", ";", "wo\u00b7durch", "wir", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "PWAV", "PPER", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der selgen Ewigkeit, als einen Weg ge-", "tokens": ["Der", "sel\u00b7gen", "E\u00b7wig\u00b7keit", ",", "als", "ei\u00b7nen", "Weg", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die kan in dieser Stadt, durch deinen Ab-", "tokens": ["Die", "kan", "in", "die\u00b7ser", "Stadt", ",", "durch", "dei\u00b7nen", "Ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "PDAT", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Du bist der Diamant, der an dem Kranze", "tokens": ["Du", "bist", "der", "Di\u00b7a\u00b7mant", ",", "der", "an", "dem", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Den unser Orden pflicht; wir sind mit Dir", "tokens": ["Den", "un\u00b7ser", "Or\u00b7den", "pflicht", ";", "wir", "sind", "mit", "Dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und Dein Glanz ziert auch uns; Du giebst", "tokens": ["Und", "Dein", "Glanz", "ziert", "auch", "uns", ";", "Du", "giebst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "PPER", "$.", "PPER", "VVFIN"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.28": {"text": "Als unser Aeltester, den Schmuk uns zu be-", "tokens": ["Als", "un\u00b7ser", "A\u00b7el\u00b7tes\u00b7ter", ",", "den", "Schmuk", "uns", "zu", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ART", "NN", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Und ich erkenne auch; Es kan Dein heller", "tokens": ["Und", "ich", "er\u00b7ken\u00b7ne", "auch", ";", "Es", "kan", "Dein", "hel\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$.", "PPER", "VMFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Auch mir zum gr\u00f6\u00dfren Licht, zum hellern", "tokens": ["Auch", "mir", "zum", "gr\u00f6\u00df\u00b7ren", "Licht", ",", "zum", "hel\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ich kan gleich einen Stern, mich auch mit", "tokens": ["Ich", "kan", "gleich", "ei\u00b7nen", "Stern", ",", "mich", "auch", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "$,", "PPER", "ADV", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "Die von Dir auf mich falln, zu ", "tokens": ["Die", "von", "Dir", "auf", "mich", "falln", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "PPER", "APPR", "PPER", "VVINF", "$,", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Der Nuzze treibt mich an, da\u00df ich mit Dank-", "tokens": ["Der", "Nuz\u00b7ze", "treibt", "mich", "an", ",", "da\u00df", "ich", "mit", "Dank"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Dir dieses kleine Buch zum Denkmal ein-\ngeweiht,", "tokens": ["Dir", "die\u00b7ses", "klei\u00b7ne", "Buch", "zum", "Denk\u00b7mal", "ein", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "ADJA", "NN", "APPRART", "NN", "TRUNC", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Damit die Welt erseh, wie man der Ehr-", "tokens": ["Da\u00b7mit", "die", "Welt", "er\u00b7seh", ",", "wie", "man", "der", "Ehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,", "PWAV", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Dem Alter schuldig sey geb\u00fchrend zu ent-", "tokens": ["Dem", "Al\u00b7ter", "schul\u00b7dig", "sey", "ge\u00b7b\u00fch\u00b7rend", "zu", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VAFIN", "VVPP", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Ich f\u00fcge meinen Wunsch aus reinen Trieb", "tokens": ["Ich", "f\u00fc\u00b7ge", "mei\u00b7nen", "Wunsch", "aus", "rei\u00b7nen", "Trieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Ich fleh den Sch\u00f6pfer an, da\u00df stets Dein", "tokens": ["Ich", "fleh", "den", "Sch\u00f6p\u00b7fer", "an", ",", "da\u00df", "stets", "Dein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "ADV", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "Der gr\u00fcnen Jugend gleich; er gebe zu dem", "tokens": ["Der", "gr\u00fc\u00b7nen", "Ju\u00b7gend", "gleich", ";", "er", "ge\u00b7be", "zu", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "PPER", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Stets neue Adlers Kraft, mit Fl\u00fcgeln auf-", "tokens": ["Stets", "neu\u00b7e", "Ad\u00b7lers", "Kraft", ",", "mit", "Fl\u00fc\u00b7geln", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$,", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Auf Zions heilge H\u00f6h. Es bl\u00fch dein Wohl-", "tokens": ["Auf", "Zi\u00b7ons", "heil\u00b7ge", "H\u00f6h", ".", "Es", "bl\u00fch", "dein", "Wohl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Damit wir durch Dein Wohl, das Wohl der", "tokens": ["Da\u00b7mit", "wir", "durch", "Dein", "Wohl", ",", "das", "Wohl", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Der H\u00f6chste lasse Dich gleich denen Mandel-", "tokens": ["Der", "H\u00f6chs\u00b7te", "las\u00b7se", "Dich", "gleich", "de\u00b7nen", "Man\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PDS", "TRUNC"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Gleich Aarons gr\u00fcnen Stab, im Heiligthu-", "tokens": ["Gleich", "Aa\u00b7rons", "gr\u00fc\u00b7nen", "Stab", ",", "im", "Hei\u00b7ligt\u00b7hu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NE", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Dies w\u00fcnscht aus regen Trieb, der Dich von", "tokens": ["Dies", "w\u00fcnscht", "aus", "re\u00b7gen", "Trieb", ",", "der", "Dich", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.46": {"text": "Weil Du als Aeltester zwiefacher Ehren", "tokens": ["Weil", "Du", "als", "A\u00b7el\u00b7tes\u00b7ter", "zwie\u00b7fac\u00b7her", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "NN", "APPR", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.47": {"text": "Nim dies zum Zeichen an, ", "tokens": ["Nim", "dies", "zum", "Zei\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "Da\u00df ich dich werde ehren, bis Herz und", "tokens": ["Da\u00df", "ich", "dich", "wer\u00b7de", "eh\u00b7ren", ",", "bis", "Herz", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$,", "KOUS", "NN", "KON"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}