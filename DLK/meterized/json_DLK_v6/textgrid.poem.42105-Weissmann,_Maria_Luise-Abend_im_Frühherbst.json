{"textgrid.poem.42105": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Abend im Fr\u00fchherbst", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit ausgegossen liegt das breite Land.", "tokens": ["Weit", "aus\u00b7ge\u00b7gos\u00b7sen", "liegt", "das", "brei\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVIZU", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Himmel taucht den Scheitel noch ins Licht,", "tokens": ["Der", "Him\u00b7mel", "taucht", "den", "Schei\u00b7tel", "noch", "ins", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch seitlich hebt gelassen eine Hand", "tokens": ["Doch", "seit\u00b7lich", "hebt", "ge\u00b7las\u00b7sen", "ei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die dunkle Maske Nacht ihm ins Gesicht.", "tokens": ["Die", "dunk\u00b7le", "Mas\u00b7ke", "Nacht", "ihm", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Viel fette L\u00e4mmer weiden auf der Flur,", "tokens": ["Viel", "fet\u00b7te", "L\u00e4m\u00b7mer", "wei\u00b7den", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In G\u00e4rten steht das Kraut in seiner F\u00fclle,", "tokens": ["In", "G\u00e4r\u00b7ten", "steht", "das", "Kraut", "in", "sei\u00b7ner", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herbstw\u00e4lder ziehn als eine goldne Spur,", "tokens": ["Herbst\u00b7w\u00e4l\u00b7der", "ziehn", "als", "ei\u00b7ne", "gold\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Am Baum die Frucht gl\u00e4nzt prall in ihrer H\u00fclle.", "tokens": ["Am", "Baum", "die", "Frucht", "gl\u00e4nzt", "prall", "in", "ih\u00b7rer", "H\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ist der letzte dieser kurzen Tage:", "tokens": ["Es", "ist", "der", "letz\u00b7te", "die\u00b7ser", "kur\u00b7zen", "Ta\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "All Ding steht reif und rund und unbewegt", "tokens": ["All", "Ding", "steht", "reif", "und", "rund", "und", "un\u00b7be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schwebend in sich gebannt wie eine Waage,", "tokens": ["Schwe\u00b7bend", "in", "sich", "ge\u00b7bannt", "wie", "ei\u00b7ne", "Waa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Tod und Leben gleichgewichtig tr\u00e4gt.", "tokens": ["Die", "Tod", "und", "Le\u00b7ben", "gleich\u00b7ge\u00b7wich\u00b7tig", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}