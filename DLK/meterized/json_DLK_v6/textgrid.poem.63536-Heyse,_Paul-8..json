{"textgrid.poem.63536": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sterne blinken und glei\u00dfen,", "tokens": ["Die", "Ster\u00b7ne", "blin\u00b7ken", "und", "glei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Nacht ist stille, der Mond steht tief.", "tokens": ["Die", "Nacht", "ist", "stil\u00b7le", ",", "der", "Mond", "steht", "tief", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wer war's, der meinen Namen rief", "tokens": ["Wer", "wa\u00b7r's", ",", "der", "mei\u00b7nen", "Na\u00b7men", "rief"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bei den Hagerosen, den wei\u00dfen?", "tokens": ["Bei", "den", "Ha\u00b7ge\u00b7ro\u00b7sen", ",", "den", "wei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ob sich im Garten der Hans verlief?", "tokens": ["Ob", "sich", "im", "Gar\u00b7ten", "der", "Hans", "ver\u00b7lief", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPRART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Was bleib' ich nun aber hangen", "tokens": ["Was", "bleib'", "ich", "nun", "a\u00b7ber", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Sternend\u00e4mmer am Rosenstrauch?", "tokens": ["Im", "Ster\u00b7nen\u00b7d\u00e4m\u00b7mer", "am", "Ro\u00b7sen\u00b7strauch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie sanft es fl\u00fcstert mit s\u00fc\u00dfem Hauch", "tokens": ["Wie", "sanft", "es", "fl\u00fcs\u00b7tert", "mit", "s\u00fc\u00b7\u00dfem", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und k\u00fc\u00dft mir Mund und Wangen!", "tokens": ["Und", "k\u00fc\u00dft", "mir", "Mund", "und", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das ist doch nimmer der Dornen Brauch.", "tokens": ["Das", "ist", "doch", "nim\u00b7mer", "der", "Dor\u00b7nen", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein ganz Gesicht in Gluten \u2013", "tokens": ["Mein", "ganz", "Ge\u00b7sicht", "in", "Glu\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ruft von ferne mein M\u00fctterlein.", "tokens": ["Da", "ruft", "von", "fer\u00b7ne", "mein", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Geschwind nur wieder ins Haus hinein,", "tokens": ["Ge\u00b7schwind", "nur", "wie\u00b7der", "ins", "Haus", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wenn meine Lippen bluten,", "tokens": ["Und", "wenn", "mei\u00b7ne", "Lip\u00b7pen", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Dornen m\u00fcssen's gewesen sein!", "tokens": ["Die", "Dor\u00b7nen", "m\u00fcs\u00b7sen's", "ge\u00b7we\u00b7sen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VAPP", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}