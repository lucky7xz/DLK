{"dta.poem.16025": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckt der Weise, der ein kluges Weib gefunden,", "tokens": ["Be\u00b7gl\u00fcckt", "der", "Wei\u00b7se", ",", "der", "ein", "klu\u00b7ges", "Weib", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die den gen\u00fcgenden Beruf darin empfunden,", "tokens": ["Die", "den", "ge\u00b7n\u00fc\u00b7gen\u00b7den", "Be\u00b7ruf", "da\u00b7rin", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit Sinnigkeit das Haupt des Sinnenden zu kr\u00e4nzen,", "tokens": ["Mit", "Sin\u00b7nig\u00b7keit", "das", "Haupt", "des", "Sin\u00b7nen\u00b7den", "zu", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den himmlisch strebenden auch irdisch zu erg\u00e4nzen,", "tokens": ["Den", "himm\u00b7lisch", "stre\u00b7ben\u00b7den", "auch", "ir\u00b7disch", "zu", "er\u00b7g\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Sorge vorzustehn des Hauses und der Zeit,", "tokens": ["Der", "Sor\u00b7ge", "vor\u00b7zu\u00b7stehn", "des", "Hau\u00b7ses", "und", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df seine Sorge sei nur Welt und Ewigkeit.", "tokens": ["Da\u00df", "sei\u00b7ne", "Sor\u00b7ge", "sei", "nur", "Welt", "und", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}