{"textgrid.poem.62838": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Bauer, der die Cypria", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bauer, der die Cypria", "tokens": ["Ein", "Bau\u00b7er", ",", "der", "die", "Cyp\u00b7ria"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit lieber als Caplans Madonne", "tokens": ["Weit", "lie\u00b7ber", "als", "Cap\u00b7lans", "Ma\u00b7don\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "KOUS", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Im Gartensaal des Burgherrn sah,", "tokens": ["Im", "Gar\u00b7ten\u00b7saal", "des", "Burg\u00b7herrn", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchbl\u00e4tterte mit Herzenswonne", "tokens": ["Durch\u00b7bl\u00e4t\u00b7ter\u00b7te", "mit", "Her\u00b7zens\u00b7won\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den neuen Almanach. Er las:", "tokens": ["Den", "neu\u00b7en", "Al\u00b7ma\u00b7nach", ".", "Er", "las", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Titel wars. Wie? rief er, was?", "tokens": ["Ein", "Ti\u00b7tel", "wars", ".", "Wie", "?", "rief", "er", ",", "was", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "PWAV", "$.", "VVFIN", "PPER", "$,", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Venus in der Sonne? Wetter!", "tokens": ["Die", "Ve\u00b7nus", "in", "der", "Son\u00b7ne", "?", "Wet\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Venus? Ach! \u2013 Hier brach sein Ton. \u2013", "tokens": ["Die", "Ve\u00b7nus", "?", "Ach", "!", "\u2013", "Hier", "brach", "sein", "Ton", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "ITJ", "$.", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und ich erfuhr kein Wort davon?", "tokens": ["Und", "ich", "er\u00b7fuhr", "kein", "Wort", "da\u00b7von", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Sonnenwirth ist doch mein Vetter.", "tokens": ["Der", "Son\u00b7nen\u00b7wirth", "ist", "doch", "mein", "Vet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}