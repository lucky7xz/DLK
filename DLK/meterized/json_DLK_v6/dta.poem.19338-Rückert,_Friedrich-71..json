{"dta.poem.19338": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sein eigen nennt der Mensch ein Gut uneigentlich;", "tokens": ["Sein", "ei\u00b7gen", "nennt", "der", "Mensch", "ein", "Gut", "un\u00b7ei\u00b7gent\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Da\u00df Gutes ist von Gott, gesteht er schweigentlich.", "tokens": ["Da\u00df", "Gu\u00b7tes", "ist", "von", "Gott", ",", "ge\u00b7steht", "er", "schwei\u00b7gent\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "APPR", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du sollst, was deiner Art, was deinem Sinn kann eignen,", "tokens": ["Du", "sollst", ",", "was", "dei\u00b7ner", "Art", ",", "was", "dei\u00b7nem", "Sinn", "kann", "eig\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VMFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo dir's auf deiner Fahrt begegnet, dir aneignen.", "tokens": ["Wo", "dir's", "auf", "dei\u00b7ner", "Fahrt", "be\u00b7geg\u00b7net", ",", "dir", "an\u00b7eig\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Eigner dieser Welt das ist ein Geist, der eignet", "tokens": ["Der", "Eig\u00b7ner", "die\u00b7ser", "Welt", "das", "ist", "ein", "Geist", ",", "der", "eig\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich dauernd alles zu, was fl\u00fcchtig sich ereignet.", "tokens": ["Sich", "dau\u00b7ernd", "al\u00b7les", "zu", ",", "was", "fl\u00fcch\u00b7tig", "sich", "er\u00b7eig\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PIS", "PTKVZ", "$,", "PRELS", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das Eigenthum ist nur ein \u00e4u\u00dferlich Geleit,", "tokens": ["Das", "Ei\u00b7gen\u00b7thum", "ist", "nur", "ein", "\u00e4u\u00b7\u00dfer\u00b7lich", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein rechtes Eigenthum ist Eigenth\u00fcmlichkeit.", "tokens": ["Dein", "rech\u00b7tes", "Ei\u00b7gen\u00b7thum", "ist", "Ei\u00b7gent\u00b7h\u00fcm\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Auf diese Eigenblum' halt ohne Eigenruhm,", "tokens": ["Auf", "die\u00b7se", "Ei\u00b7gen\u00b7blum'", "halt", "oh\u00b7ne", "Ei\u00b7gen\u00b7ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df dir rauben nie dein eigenst Eigenthum.", "tokens": ["Und", "la\u00df", "dir", "rau\u00b7ben", "nie", "dein", "ei\u00b7genst", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "ADV", "PPOSAT", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gleichfern von Eigensucht alswie von Eigenflucht,", "tokens": ["Gleich\u00b7fern", "von", "Ei\u00b7gen\u00b7sucht", "als\u00b7wie", "von", "Ei\u00b7gen\u00b7flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Eigenzucht gedeiht des Herzens Eigenfrucht.", "tokens": ["In", "Ei\u00b7gen\u00b7zucht", "ge\u00b7deiht", "des", "Her\u00b7zens", "Ei\u00b7gen\u00b7frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dis sei mein eigner Sinn, zu seyn ohn' Eigensinn;", "tokens": ["Dis", "sei", "mein", "eig\u00b7ner", "Sinn", ",", "zu", "seyn", "ohn'", "Ei\u00b7gen\u00b7sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PTKZU", "VAINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein eigen bin ich nur, wenn ich dein eigen bin.", "tokens": ["Mein", "ei\u00b7gen", "bin", "ich", "nur", ",", "wenn", "ich", "dein", "ei\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich bin in Lust und Schmerz liebeigen und leibeigen", "tokens": ["Ich", "bin", "in", "Lust", "und", "Schmerz", "lie\u00b7bei\u00b7gen", "und", "leib\u00b7ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "VVFIN", "KON", "VVINF"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir, welchem stets mein Herz blieb eigen und bleib' eigen!", "tokens": ["Dir", ",", "wel\u00b7chem", "stets", "mein", "Herz", "blieb", "ei\u00b7gen", "und", "bleib'", "ei\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}