{"textgrid.poem.63043": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der wilde Kaukasus gebahr", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der wilde Kaukasus gebahr", "tokens": ["Der", "wil\u00b7de", "Kau\u00b7ka\u00b7sus", "ge\u00b7bahr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Zeit des Alterthums in seinen Schl\u00fcnden", "tokens": ["Zur", "Zeit", "des", "Al\u00b7ter\u00b7thums", "in", "sei\u00b7nen", "Schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Brut der Greisen, eine R\u00e4uberschaar", "tokens": ["Die", "Brut", "der", "Grei\u00b7sen", ",", "ei\u00b7ne", "R\u00e4u\u00b7ber\u00b7schaar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Fl\u00fcgeln, die wir nicht bey dem Linn\u00e4us finden.", "tokens": ["Mit", "Fl\u00fc\u00b7geln", ",", "die", "wir", "nicht", "bey", "dem", "Lin\u00b7n\u00e4us", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "PTKNEG", "APPR", "ART", "NE", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sie stahlen nach dem strengsten Recht,", "tokens": ["Sie", "stah\u00b7len", "nach", "dem", "strengs\u00b7ten", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Indem sie, wo sie nur auf einen Vogel stie\u00dfen,", "tokens": ["In\u00b7dem", "sie", ",", "wo", "sie", "nur", "auf", "ei\u00b7nen", "Vo\u00b7gel", "stie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "ADV", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dem Adler wie dem Spatz, dem Strau\u00dfe wie dem Specht,", "tokens": ["Dem", "Ad\u00b7ler", "wie", "dem", "Spatz", ",", "dem", "Strau\u00b7\u00dfe", "wie", "dem", "Specht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Federn aus den Schwingen ri\u00dfen.", "tokens": ["Die", "Fe\u00b7dern", "aus", "den", "Schwin\u00b7gen", "ri\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein biedrer Faun, der einst den Unfug sah,", "tokens": ["Ein", "bie\u00b7drer", "Faun", ",", "der", "einst", "den", "Un\u00b7fug", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Trug Mitleid mit den armen Thieren.", "tokens": ["Trug", "Mit\u00b7leid", "mit", "den", "ar\u00b7men", "Thie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Banditen, rief er aus, was macht ihr da?", "tokens": ["Ban\u00b7di\u00b7ten", ",", "rief", "er", "aus", ",", "was", "macht", "ihr", "da", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ey nun, versetzten sie, wir reformieren.", "tokens": ["Ey", "nun", ",", "ver\u00b7setz\u00b7ten", "sie", ",", "wir", "re\u00b7for\u00b7mie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "PPER", "$,", "PPER", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}