{"dta.poem.18640": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n einen guten  F reund in  \n Leipzig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebter Hertzens Freund/ vergib doch meinen Schreiben/", "tokens": ["Ge\u00b7lieb\u00b7ter", "Hert\u00b7zens", "Freund", "/", "ver\u00b7gib", "doch", "mei\u00b7nen", "Schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$(", "VVIMP", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wofern der enge Raum voll tausend klagen ist.", "tokens": ["Wo\u00b7fern", "der", "en\u00b7ge", "Raum", "voll", "tau\u00b7send", "kla\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "CARD", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie kan ich ohne dir vergn\u00fcgt und gl\u00fccklich bleiben/", "tokens": ["Wie", "kan", "ich", "oh\u00b7ne", "dir", "ver\u00b7gn\u00fcgt", "und", "gl\u00fcck\u00b7lich", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PPER", "VVPP", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil du vielleicht entfernt auch von Gem\u00fcthe bist?", "tokens": ["Weil", "du", "viel\u00b7leicht", "ent\u00b7fernt", "auch", "von", "Ge\u00b7m\u00fc\u00b7the", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der letzte Abschied mahlt mir dein getreues Hertze/", "tokens": ["Der", "letz\u00b7te", "Ab\u00b7schied", "mahlt", "mir", "dein", "ge\u00b7treu\u00b7es", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das durch die Wehmuth brach/ mit s\u00fcssen Z\u00fcgen vor/", "tokens": ["Das", "durch", "die", "Weh\u00b7muth", "brach", "/", "mit", "s\u00fcs\u00b7sen", "Z\u00fc\u00b7gen", "vor", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$(", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In meinen Augen brennt noch deine Freundschaffts-Kertze/", "tokens": ["In", "mei\u00b7nen", "Au\u00b7gen", "brennt", "noch", "dei\u00b7ne", "Freund\u00b7schaffts\u00b7Kert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dein verpflichtes Wort klingt noch vor meinen Ohr:", "tokens": ["Und", "dein", "ver\u00b7pflich\u00b7tes", "Wort", "klingt", "noch", "vor", "mei\u00b7nen", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es hei\u00df: Kein fremder Ort soll dich mir fremde machen/", "tokens": ["Es", "hei\u00df", ":", "Kein", "frem\u00b7der", "Ort", "soll", "dich", "mir", "frem\u00b7de", "ma\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "PIAT", "ADJA", "NN", "VMFIN", "PPER", "PPER", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In unsrer Brust zerf\u00e4lt der Fel\u00df der Treue nicht/", "tokens": ["In", "uns\u00b7rer", "Brust", "zer\u00b7f\u00e4lt", "der", "Fel\u00df", "der", "Treu\u00b7e", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich will so Gl\u00fcck als Zeit/ und alles di\u00df verlachen/", "tokens": ["Ich", "will", "so", "Gl\u00fcck", "als", "Zeit", "/", "und", "al\u00b7les", "di\u00df", "ver\u00b7la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "KOUS", "NN", "$(", "KON", "PIS", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So unser festes Band der reinen Freundschafft bricht.", "tokens": ["So", "un\u00b7ser", "fes\u00b7tes", "Band", "der", "rei\u00b7nen", "Freund\u00b7schafft", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Hertzen sollen sich auf diese Weise sprechen/", "tokens": ["Die", "Hert\u00b7zen", "sol\u00b7len", "sich", "auf", "die\u00b7se", "Wei\u00b7se", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So die Entfernung uns durch \u00f6fftres Schreiben lehrt.", "tokens": ["So", "die", "Ent\u00b7fer\u00b7nung", "uns", "durch", "\u00f6fft\u00b7res", "Schrei\u00b7ben", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Allein wie lange Zeit l\u00e4st du mich nichts erbrechen/", "tokens": ["Al\u00b7lein", "wie", "lan\u00b7ge", "Zeit", "l\u00e4st", "du", "mich", "nichts", "er\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJA", "NN", "VVFIN", "PPER", "PRF", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df meinen Kummer stillt und mich mit Freuden nehrt?", "tokens": ["Da\u00df", "mei\u00b7nen", "Kum\u00b7mer", "stillt", "und", "mich", "mit", "Freu\u00b7den", "nehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "KON", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Will etwan der Verdacht nun lichte Flammen fangen/", "tokens": ["Will", "et\u00b7wan", "der", "Ver\u00b7dacht", "nun", "lich\u00b7te", "Flam\u00b7men", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Der in den nechsten Brief nur falsche Funcken wie\u00df/", "tokens": ["Der", "in", "den", "nechs\u00b7ten", "Brief", "nur", "fal\u00b7sche", "Fun\u00b7cken", "wie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Als h\u00e4tte dich dein Freund so sch\u00e4ndlich hintergangen/", "tokens": ["Als", "h\u00e4t\u00b7te", "dich", "dein", "Freund", "so", "sch\u00e4nd\u00b7lich", "hin\u00b7ter\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df deine Briefe nicht der Meyneid folgen lie\u00df?", "tokens": ["Da\u00df", "dei\u00b7ne", "Brie\u00b7fe", "nicht", "der", "Mey\u00b7neid", "fol\u00b7gen", "lie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie sind ja wie ich wei\u00df/ vorl\u00e4ngst in deinen H\u00e4nden/", "tokens": ["Sie", "sind", "ja", "wie", "ich", "wei\u00df", "/", "vor\u00b7l\u00e4ngst", "in", "dei\u00b7nen", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "PPER", "VVFIN", "$(", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und die Entschuldigung von mir liegt auch dabey.", "tokens": ["Und", "die", "Ent\u00b7schul\u00b7di\u00b7gung", "von", "mir", "liegt", "auch", "da\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Nicht meine/ da\u00df mich kan ein toller Stein verblenden/", "tokens": ["Nicht", "mei\u00b7ne", "/", "da\u00df", "mich", "kan", "ein", "tol\u00b7ler", "Stein", "ver\u00b7blen\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "$(", "KOUS", "PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich schwere mein Gem\u00fcth' ist von der Schmincke frey.", "tokens": ["Ich", "schwe\u00b7re", "mein", "Ge\u00b7m\u00fcth'", "ist", "von", "der", "Schmin\u00b7cke", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich lieb ein edel Hertz bey einen Frauenzimmer/", "tokens": ["Ich", "lieb", "ein", "e\u00b7del", "Hertz", "bey", "ei\u00b7nen", "Frau\u00b7en\u00b7zim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJD", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Brust hat sich gew\u00f6hnt den Lastern Feind zu seyn.", "tokens": ["Die", "Brust", "hat", "sich", "ge\u00b7w\u00f6hnt", "den", "Las\u00b7tern", "Feind", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich liebe reines Gold und keinen falschen Schimmer/", "tokens": ["Ich", "lie\u00b7be", "rei\u00b7nes", "Gold", "und", "kei\u00b7nen", "fal\u00b7schen", "Schim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Auch der Verdacht davon fl\u00f6\u00dft einen Eckel ein.", "tokens": ["Auch", "der", "Ver\u00b7dacht", "da\u00b7von", "fl\u00f6\u00dft", "ei\u00b7nen", "E\u00b7ckel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.29": {"text": "Hier ist ein Diamant/ der einen Schreck bekommen/", "tokens": ["Hier", "ist", "ein", "Di\u00b7a\u00b7mant", "/", "der", "ei\u00b7nen", "Schreck", "be\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "ART", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein Stein/ der durch den Thau der Wollust ausgeh\u00f6lt/", "tokens": ["Ein", "Stein", "/", "der", "durch", "den", "Thau", "der", "Wol\u00b7lust", "aus\u00b7ge\u00b7h\u00f6lt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein Berg-Werck/ wo man hat die Sch\u00e4tze draus genommen/", "tokens": ["Ein", "Ber\u00b7g\u00b7Werck", "/", "wo", "man", "hat", "die", "Sch\u00e4t\u00b7ze", "draus", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PIS", "VAFIN", "ART", "NN", "PAV", "VVPP", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Ein Vielfra\u00df welcher sich nur Menschen-Fleisch erwehlt.", "tokens": ["Ein", "Viel\u00b7fra\u00df", "wel\u00b7cher", "sich", "nur", "Men\u00b7schen\u00b7Fleisch", "er\u00b7wehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRELS", "PRF", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein solcher Blumen-Strau\u00df der welck und abgerochen/", "tokens": ["Ein", "sol\u00b7cher", "Blu\u00b7men\u00b7Strau\u00df", "der", "welck", "und", "ab\u00b7ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ein Garten/ der nunmehr vor Rosen-Disteln hegt.", "tokens": ["Ein", "Gar\u00b7ten", "/", "der", "nun\u00b7mehr", "vor", "Ro\u00b7sen\u00b7Dis\u00b7teln", "hegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein Apffel den ein Wurm/ (du kennst ihn wohl) zerstochen.", "tokens": ["Ein", "Apf\u00b7fel", "den", "ein", "Wurm", "/", "(", "du", "kennst", "ihn", "wohl", ")", "zer\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "$(", "$(", "PPER", "VVFIN", "PPER", "ADV", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ein Acker wer ihn kennt/ der welcke Schooten tr\u00e4gt.", "tokens": ["Ein", "A\u00b7cker", "wer", "ihn", "kennt", "/", "der", "wel\u00b7cke", "Schoo\u00b7ten", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWS", "PPER", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein Heerd/ worauf nunmehr fast alle Vogel springen/", "tokens": ["Ein", "Heerd", "/", "wo\u00b7rauf", "nun\u00b7mehr", "fast", "al\u00b7le", "Vo\u00b7gel", "sprin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "ADV", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein Teich/ der jeden Fisch gleich zwinget abzustehn.", "tokens": ["Ein", "Teich", "/", "der", "je\u00b7den", "Fisch", "gleich", "zwin\u00b7get", "ab\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIAT", "NN", "ADV", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein Wall-Fisch/ der mit Lust den Jonas kan verschlingen/", "tokens": ["Ein", "Wall\u00b7Fisch", "/", "der", "mit", "Lust", "den", "Jo\u00b7nas", "kan", "ver\u00b7schlin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Und welcken R\u00fcben gleich l\u00e4st wieder von sich gehn.", "tokens": ["Und", "wel\u00b7cken", "R\u00fc\u00b7ben", "gleich", "l\u00e4st", "wie\u00b7der", "von", "sich", "gehn", "."