, "ADV", "APPR", "$(", "$(", "$(", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.59": {"text": "Und wie die M\u00e4dgen sich den K\u00fctzel selbst vertreiben/", "tokens": ["Und", "wie", "die", "M\u00e4d\u00b7gen", "sich", "den", "K\u00fct\u00b7zel", "selbst", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Was da und dort passirt/ und neulich ist geschehn/", "tokens": ["Was", "da", "und", "dort", "pas\u00b7sirt", "/", "und", "neu\u00b7lich", "ist", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "ADV", "VVPP", "$(", "KON", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Allein die kurtze Zeit gedenckt es zu verschieben/", "tokens": ["Al\u00b7lein", "die", "kurt\u00b7ze", "Zeit", "ge\u00b7denckt", "es", "zu", "ver\u00b7schie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Bi\u00df mich die Antwort auch von dir gel\u00fccklich macht.", "tokens": ["Bi\u00df", "mich", "die", "Ant\u00b7wort", "auch", "von", "dir", "ge\u00b7l\u00fcck\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Indessen schreibe mir die Zuf\u00e4ll' in den lieben/", "tokens": 