{"textgrid.poem.49304": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Loblied zu Ehren dem Cupido", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist, der deine gro\u00dfe Macht,", "tokens": ["Wer", "ist", ",", "der", "dei\u00b7ne", "gro\u00b7\u00dfe", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Cupido, kan erz\u00e4hlen?", "tokens": ["Cu\u00b7pi\u00b7do", ",", "kan", "er\u00b7z\u00e4h\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du hast den besten Rat erdacht,", "tokens": ["Du", "hast", "den", "bes\u00b7ten", "Rat", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir k\u00f6nnen's nicht verhehlen;", "tokens": ["Wir", "k\u00f6n\u00b7nen's", "nicht", "ver\u00b7heh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum r\u00fchmen wir dich ohne Spott,", "tokens": ["Drum", "r\u00fch\u00b7men", "wir", "dich", "oh\u00b7ne", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du bist der allerst\u00e4rkste Gott,", "tokens": ["Du", "bist", "der", "al\u00b7ler\u00b7st\u00e4rks\u00b7te", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dir thun wir uns befehlen.", "tokens": ["Dir", "thun", "wir", "uns", "be\u00b7feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df alles nun aufs neue lebt,", "tokens": ["Da\u00df", "al\u00b7les", "nun", "aufs", "neu\u00b7e", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df sich die V\u00f6glein paaren,", "tokens": ["Da\u00df", "sich", "die", "V\u00f6\u00b7glein", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df Bacchus an den Ulmen klebt,", "tokens": ["Da\u00df", "Bac\u00b7chus", "an", "den", "Ul\u00b7men", "klebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Kr\u00e4uter sich verjahren,", "tokens": ["Die", "Kr\u00e4u\u00b7ter", "sich", "ver\u00b7jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Das, r\u00fchmen wir ohn' allen Spott,", "tokens": ["Das", ",", "r\u00fch\u00b7men", "wir", "ohn'", "al\u00b7len", "Spott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Verschaffest du, der Liebe Gott,", "tokens": ["Ver\u00b7schaf\u00b7fest", "du", ",", "der", "Lie\u00b7be", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Du kanst es auch bewahren.", "tokens": ["Du", "kanst", "es", "auch", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Da\u00df jetzt der Stier die K\u00fche sucht,", "tokens": ["Da\u00df", "jetzt", "der", "Stier", "die", "K\u00fc\u00b7he", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Bock die wei\u00dfen Ziegen,", "tokens": ["Der", "Bock", "die", "wei\u00b7\u00dfen", "Zie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Da\u00df Philomel dem Tereus flucht,", "tokens": ["Da\u00df", "Phi\u00b7lo\u00b7mel", "dem", "Te\u00b7reus", "flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Dohlen h\u00e4ufig fliegen;", "tokens": ["Die", "Doh\u00b7len", "h\u00e4u\u00b7fig", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Das, singen wir ohn' allen Spott,", "tokens": ["Das", ",", "sin\u00b7gen", "wir", "ohn'", "al\u00b7len", "Spott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Verschaffest du, der Liebe Gott,", "tokens": ["Ver\u00b7schaf\u00b7fest", "du", ",", "der", "Lie\u00b7be", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Du kanst sie all betriegen.", "tokens": ["Du", "kanst", "sie", "all", "be\u00b7trie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da\u00df jetzt die V\u00f6glein mannigfalt", "tokens": ["Da\u00df", "jetzt", "die", "V\u00f6\u00b7glein", "man\u00b7nig\u00b7falt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "B\u00e4um', Erd' und Nester f\u00fcllen,", "tokens": ["B\u00e4um'", ",", "Erd'", "und", "Nes\u00b7ter", "f\u00fcl\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Da\u00df sich jetzt freuen Jung und Alt,", "tokens": ["Da\u00df", "sich", "jetzt", "freu\u00b7en", "Jung", "und", "Alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ADJA", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die Schaf auch L\u00e4mmer stillen,", "tokens": ["Die", "Schaf", "auch", "L\u00e4m\u00b7mer", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Das, singen wir ohn' allen Spott,", "tokens": ["Das", ",", "sin\u00b7gen", "wir", "ohn'", "al\u00b7len", "Spott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Verschaffest du, der Liebe Gott,", "tokens": ["Ver\u00b7schaf\u00b7fest", "du", ",", "der", "Lie\u00b7be", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Nach allem deinem Willen.", "tokens": ["Nach", "al\u00b7lem", "dei\u00b7nem", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Da\u00df sich das ungest\u00fcme Meer", "tokens": ["Da\u00df", "sich", "das", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Zur Ruhe thut begeben,", "tokens": ["Zur", "Ru\u00b7he", "thut", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Da\u00df in demselben hin und her", "tokens": ["Da\u00df", "in", "dem\u00b7sel\u00b7ben", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die gro\u00dfen Fische schweben,", "tokens": ["Die", "gro\u00b7\u00