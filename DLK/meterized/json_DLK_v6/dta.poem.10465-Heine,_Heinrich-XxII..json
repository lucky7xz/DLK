{"dta.poem.10465": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edie Jungfrau schl\u00e4ft in der Kammer,", "tokens": ["\u201e", "die", "Jung\u00b7frau", "schl\u00e4ft", "in", "der", "Kam\u00b7mer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Mond schaut zitternd hinein;", "tokens": ["Der", "Mond", "schaut", "zit\u00b7ternd", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da drau\u00dfen singt es und klingt es,", "tokens": ["Da", "drau\u00b7\u00dfen", "singt", "es", "und", "klingt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie Walzermelodein.", "tokens": ["Wie", "Wal\u00b7zer\u00b7me\u00b7lo\u00b7dein", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich will mal schaun aus dem Fenster,", "tokens": ["Ich", "will", "mal", "schaun", "aus", "dem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wer drunten st\u00f6rt meine Ruh'.", "tokens": ["Wer", "drun\u00b7ten", "st\u00f6rt", "mei\u00b7ne", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da steht ein Todtengerippe,", "tokens": ["Da", "steht", "ein", "Tod\u00b7ten\u00b7ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und fidelt und singt dazu:", "tokens": ["Und", "fi\u00b7delt", "und", "singt", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Hast einst mir den Tanz versprochen,", "tokens": ["Hast", "einst", "mir", "den", "Tanz", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und hast gebrochen dein Wort,", "tokens": ["Und", "hast", "ge\u00b7bro\u00b7chen", "dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und heut ist Ball auf dem Kirchhof,", "tokens": ["Und", "heut", "ist", "Ball", "auf", "dem", "Kirch\u00b7hof", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Komm mit, wir tanzen dort.", "tokens": ["Komm", "mit", ",", "wir", "tan\u00b7zen", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Jungfrau ergreift es gewaltig,", "tokens": ["Die", "Jung\u00b7frau", "er\u00b7greift", "es", "ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es lockt sie hervor aus dem Haus;", "tokens": ["Es", "lockt", "sie", "her\u00b7vor", "aus", "dem", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Sie folgt dem Gerippe, das singend", "tokens": ["Sie", "folgt", "dem", "Ge\u00b7rip\u00b7pe", ",", "das", "sin\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und fidelnd schreitet voraus.", "tokens": ["Und", "fi\u00b7delnd", "schrei\u00b7tet", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Es fidelt und t\u00e4nzelt und h\u00fcpfet,", "tokens": ["Es", "fi\u00b7delt", "und", "t\u00e4n\u00b7zelt", "und", "h\u00fcp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und klappert mit seinem Gebein,", "tokens": ["Und", "klap\u00b7pert", "mit", "sei\u00b7nem", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und nickt und nickt mit dem Sch\u00e4del", "tokens": ["Und", "nickt", "und", "nickt", "mit", "dem", "Sch\u00e4\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unheimlich im Mondenschein.\u201c", "tokens": ["Un\u00b7heim\u00b7lich", "im", "Mon\u00b7den\u00b7schein", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}