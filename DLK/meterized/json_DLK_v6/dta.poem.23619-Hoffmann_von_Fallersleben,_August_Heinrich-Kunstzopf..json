{"dta.poem.23619": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kunstzopf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus deinem eignen Haar gewunden", "tokens": ["Aus", "dei\u00b7nem", "eig\u00b7nen", "Haar", "ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward dir ein ungeheurer Zopf.", "tokens": ["Ward", "dir", "ein", "un\u00b7ge\u00b7heu\u00b7rer", "Zopf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon hundert Jahre sind verschwunden,", "tokens": ["Schon", "hun\u00b7dert", "Jah\u00b7re", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir aber blieb der Zopf am Kopf.", "tokens": ["Dir", "a\u00b7ber", "blieb", "der", "Zopf", "am", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Viel gro\u00dfe Meister sahn ihn hangen,", "tokens": ["Viel", "gro\u00b7\u00dfe", "Meis\u00b7ter", "sahn", "ihn", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeder nahm dir ab ein St\u00fcck,", "tokens": ["Und", "je\u00b7der", "nahm", "dir", "ab", "ein", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie alle sind dann heimgegangen,", "tokens": ["Sie", "al\u00b7le", "sind", "dann", "heim\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir aber blieb der Zopf zur\u00fcck.", "tokens": ["Dir", "a\u00b7ber", "blieb", "der", "Zopf", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geheimni\u00dfvoll und zaubrisch schwebet", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7voll", "und", "zaub\u00b7risch", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zopf ob allen Staffelein,", "tokens": ["Der", "Zopf", "ob", "al\u00b7len", "Staf\u00b7fel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und keiner der da lebt und webet", "tokens": ["Und", "kei\u00b7ner", "der", "da", "lebt", "und", "we\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will dich, o freie Kunst, befrei'n.", "tokens": ["Will", "dich", ",", "o", "frei\u00b7e", "Kunst", ",", "be\u00b7frei'", "n."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["VMFIN", "PPER", "$,", "FM", "ADJA", "NN", "$,", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was dir noch blieb, wird werthgehalten", "tokens": ["Was", "dir", "noch", "blieb", ",", "wird", "werth\u00b7ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In allen Kunstakademien;", "tokens": ["In", "al\u00b7len", "Kuns\u00b7ta\u00b7ka\u00b7de\u00b7mi\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Alten bleiben gern beim Alten,", "tokens": ["Die", "Al\u00b7ten", "blei\u00b7ben", "gern", "beim", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und keiner darf ein Haar draus ziehn.", "tokens": ["Und", "kei\u00b7ner", "darf", "ein", "Haar", "draus", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum mag's dich auch nicht weiter qu\u00e4len,", "tokens": ["Drum", "mag's", "dich", "auch", "nicht", "wei\u00b7ter", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir alle tragen unser Leid;", "tokens": ["Wir", "al\u00b7le", "tra\u00b7gen", "un\u00b7ser", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An Z\u00f6pfen wird's der Welt nicht fehlen", "tokens": ["An", "Z\u00f6p\u00b7fen", "wird's", "der", "Welt", "nicht", "feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von nun an bis in Ewigkeit.", "tokens": ["Von", "nun", "an", "bis", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "KON", "APPR", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Und wird dein alter Kopfschmuck schwinden,", "tokens": ["Und", "wird", "dein", "al\u00b7ter", "Kopf\u00b7schmuck", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann sind die Meister gleich bereit,", "tokens": ["Dann", "sind", "die", "Meis\u00b7ter", "gleich", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir einen neuen Zopf zu winden,", "tokens": ["Dir", "ei\u00b7nen", "neu\u00b7en", "Zopf", "zu", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie er sich passt f\u00fcr unsre Zeit.", "tokens": ["Wie", "er", "sich", "passt", "f\u00fcr", "uns\u00b7re", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}