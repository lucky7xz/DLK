{"textgrid.poem.28112": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie weise sonsten gleich ward Salomo geachtet,", "tokens": ["Wie", "wei\u00b7se", "sons\u00b7ten", "gleich", "ward", "Sa\u00b7lo\u00b7mo", "ge\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat er doch in dem nicht alles recht betrachtet:", "tokens": ["So", "hat", "er", "doch", "in", "dem", "nicht", "al\u00b7les", "recht", "be\u00b7trach\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PRELS", "PTKNEG", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df derer Dinge Zahl, die niemals werden satt,", "tokens": ["Da\u00df", "de\u00b7rer", "Din\u00b7ge", "Zahl", ",", "die", "nie\u00b7mals", "wer\u00b7den", "satt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "NN", "$,", "PRELS", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Steuer er nicht auch noch beygesetzet hat.", "tokens": ["Die", "Steu\u00b7er", "er", "nicht", "auch", "noch", "bey\u00b7ge\u00b7set\u00b7zet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}