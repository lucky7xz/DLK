{"textgrid.poem.58155": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "193. Der verbotene Baum", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der S\u00fcnden kurze Lust, die Eitelkeit der Welt", "tokens": ["Der", "S\u00fcn\u00b7den", "kur\u00b7ze", "Lust", ",", "die", "Ei\u00b7tel\u00b7keit", "der", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist ein verbotner Baum; ach, la\u00df dich nicht verf\u00fchren!", "tokens": ["Ist", "ein", "ver\u00b7bot\u00b7ner", "Baum", ";", "ach", ",", "la\u00df", "dich", "nicht", "ver\u00b7f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$.", "XY", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer seine Lust in Gott stets eingekehret h\u00e4lt,", "tokens": ["Wer", "sei\u00b7ne", "Lust", "in", "Gott", "stets", "ein\u00b7ge\u00b7keh\u00b7ret", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird \u00fcber Welt und Feind mit Christus triumphieren.", "tokens": ["Wird", "\u00fc\u00b7ber", "Welt", "und", "Feind", "mit", "Chris\u00b7tus", "tri\u00b7um\u00b7phie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "APPR", "NE", "VVINF", "