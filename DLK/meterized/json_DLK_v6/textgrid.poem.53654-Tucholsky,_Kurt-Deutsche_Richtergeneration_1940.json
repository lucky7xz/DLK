{"textgrid.poem.53654": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Richtergeneration 1940", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Hakenkreuz erzogen,", "tokens": ["Zum", "Ha\u00b7ken\u00b7kreuz", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das damals Mode war,", "tokens": ["das", "da\u00b7mals", "Mo\u00b7de", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "vom Rektor angelogen \u2013", "tokens": ["vom", "Rek\u00b7tor", "an\u00b7ge\u00b7lo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wurdst du Referendar.", "tokens": ["So", "wurdst", "du", "Re\u00b7fe\u00b7ren\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Du warst im tiefen Flandern", "tokens": ["Du", "warst", "im", "tie\u00b7fen", "Flan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Etappenkommandant.", "tokens": ["E\u00b7tap\u00b7pen\u00b7kom\u00b7man\u00b7dant", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du spucktest auf die andern", "tokens": ["Du", "spuck\u00b7test", "auf", "die", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "auch hier, im Vaterland.", "tokens": ["auch", "hier", ",", "im", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr spieltet Wilhelms St\u00fctzen;", "tokens": ["Ihr", "spiel\u00b7tet", "Wil\u00b7helms", "St\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das Korps ersetzt das Heer.", "tokens": ["das", "Korps", "er\u00b7setzt", "das", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "G\u00e4bs keine ohne M\u00fctzen:", "tokens": ["G\u00e4bs", "kei\u00b7ne", "oh\u00b7ne", "M\u00fct\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ihr w\u00e4ret gar nichts mehr.", "tokens": ["ihr", "w\u00e4\u00b7ret", "gar", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nach steifen Amtsvisiten,", "tokens": ["Nach", "stei\u00b7fen", "Amts\u00b7vi\u00b7si\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der Landgerichtsstation", "tokens": ["der", "Land\u00b7ge\u00b7richts\u00b7sta\u00b7ti\u00b7on"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "kam dann nach alten Riten", "tokens": ["kam", "dann", "nach", "al\u00b7ten", "Ri\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die Doktorpromotion.", "tokens": ["die", "Dok\u00b7tor\u00b7pro\u00b7mo\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Es kam das Staatsexamen.", "tokens": ["Es", "kam", "das", "Staats\u00b7e\u00b7xa\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr seid emporger\u00fcckt.", "tokens": ["Ihr", "seid", "em\u00b7por\u00b7ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr setzt nun vor den Namen", "tokens": ["Ihr", "setzt", "nun", "vor", "den", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "den Titel, der euch schm\u00fcckt.", "tokens": ["den", "Ti\u00b7tel", ",", "der", "euch", "schm\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun, deutsche Jugend, richte!", "tokens": ["Nun", ",", "deut\u00b7sche", "Ju\u00b7gend", ",", "rich\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier Waage! Da das Schwert!", "tokens": ["Hier", "Waa\u00b7ge", "!", "Da", "das", "Schwert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "R\u00e4ch dich f\u00fcr die Geschichte!", "tokens": ["R\u00e4ch", "dich", "f\u00fcr", "die", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Zeig dich des Kaisers wert!", "tokens": ["Zeig", "dich", "des", "Kai\u00b7sers", "wert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "W\u00fcrg mit dem Paragraphen!", "tokens": ["W\u00fcrg", "mit", "dem", "Pa\u00b7ra\u00b7gra\u00b7phen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Benutz den Kommentar!", "tokens": ["Be\u00b7nutz", "den", "Kom\u00b7men\u00b7tar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du mu\u00dft den Landsmann strafen,", "tokens": ["Du", "mu\u00dft", "den", "Lands\u00b7mann", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der kein Teutone war.", "tokens": ["der", "kein", "Teu\u00b7to\u00b7ne", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Setz auf das Samtbarettchen!", "tokens": ["Setz", "auf", "das", "Samt\u00b7ba\u00b7rett\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Volk es glaubt an dich.", "tokens": ["Das", "Volk", "es", "glaubt", "an", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Justitia, das Kokettchen,", "tokens": ["Jus\u00b7ti\u00b7tia", ",", "das", "Ko\u00b7kett\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "schl\u00e4ft gern beim Ludewich.", "tokens": ["schl\u00e4ft", "gern", "beim", "Lu\u00b7de\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Du gibst dich unparteilich", "tokens": ["Du", "gibst", "dich", "un\u00b7par\u00b7tei\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "am Strafgesetzbuchband . . .", "tokens": ["am", "Straf\u00b7ge\u00b7setz\u00b7buch\u00b7band", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist es nicht. Nur freilich:", "tokens": ["Du", "bist", "es", "nicht", ".", "Nur", "frei\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Juristen sind gewandt.", "tokens": ["Ju\u00b7ris\u00b7ten", "sind", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du wirst des Rechtes K\u00fcnder.", "tokens": ["Du", "wirst", "des", "Rech\u00b7tes", "K\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich kriegt man nicht \u2013 f\u00fcr Geld.", "tokens": ["Dich", "kriegt", "man", "nicht", "\u2013", "f\u00fcr", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKNEG", "$(", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott gnade dem armen S\u00fcnder,", "tokens": ["Gott", "gna\u00b7de", "dem", "ar\u00b7men", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "der dir in die Finger f\u00e4llt!", "tokens": ["der", "dir", "in", "die", "Fin\u00b7ger", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich gr\u00fc\u00dfe dich, wunderbare", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "wun\u00b7der\u00b7ba\u00b7re"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zukunft der Richterbank!", "tokens": ["Zu\u00b7kunft", "der", "Rich\u00b7ter\u00b7bank", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du nennst das einzig Wahre:", "tokens": ["Du", "nennst", "das", "ein\u00b7zig", "Wah\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rechtspruch nach Stand und Rang!", "tokens": ["Rechts\u00b7pruch", "nach", "Stand", "und", "Rang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ihr w\u00e4hlt euch eure Zeugen!", "tokens": ["Ihr", "w\u00e4hlt", "euch", "eu\u00b7re", "Zeu\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr sichert den Bestand!", "tokens": ["Ihr", "si\u00b7chert", "den", "Be\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo sich euch Rechte beugen,", "tokens": ["Wo", "sich", "euch", "Rech\u00b7te", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ist euer Vaterland!", "tokens": ["ist", "eu\u00b7er", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}