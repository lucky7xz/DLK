{"textgrid.poem.56420": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Griechen Mitte wankt schon in der Schlacht,", "tokens": ["Der", "Grie\u00b7chen", "Mit\u00b7te", "wankt", "schon", "in", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die schwache Tiefe weicht vor den Barbaren,", "tokens": ["Die", "schwa\u00b7che", "Tie\u00b7fe", "weicht", "vor", "den", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die, einem Sturmbock gleich, mit allen Scharen", "tokens": ["Die", ",", "ei\u00b7nem", "Sturm\u00b7bock", "gleich", ",", "mit", "al\u00b7len", "Scha\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "ART", "NN", "ADV", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Keile st\u00fcrmen, riesger \u00dcbermacht.", "tokens": ["Im", "Kei\u00b7le", "st\u00fcr\u00b7men", ",", "ries\u00b7ger", "\u00dc\u00b7ber\u00b7macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor manches Griechen Augen wird es Nacht.", "tokens": ["Vor", "man\u00b7ches", "Grie\u00b7chen", "Au\u00b7gen", "wird", "es", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ins Knie sinkt der, helmlos, den Streichen offen", "tokens": ["Ins", "Knie", "sinkt", "der", ",", "helm\u00b7los", ",", "den", "Strei\u00b7chen", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "$,", "ADJD", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das bare Haupt. Der st\u00fcrzt, ins Herz getroffen,", "tokens": ["Das", "ba\u00b7re", "Haupt", ".", "Der", "st\u00fcrzt", ",", "ins", "Herz", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da eine Lanze durch den Panzer kracht.", "tokens": ["Da", "ei\u00b7ne", "Lan\u00b7ze", "durch", "den", "Pan\u00b7zer", "kracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie schleudern Br\u00e4nde von der Tiere T\u00fcrmen.", "tokens": ["Sie", "schleu\u00b7dern", "Br\u00e4n\u00b7de", "von", "der", "Tie\u00b7re", "T\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Neger schlagen drein mit erznen Keulen.", "tokens": ["Die", "Ne\u00b7ger", "schla\u00b7gen", "drein", "mit", "erz\u00b7nen", "Keu\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die wilden Skythen mit den Rossen st\u00fcrmen.", "tokens": ["Die", "wil\u00b7den", "Sky\u00b7then", "mit", "den", "Ros\u00b7sen", "st\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie Fluten brechen durch der Deiche Haft,", "tokens": ["Wie", "Flu\u00b7ten", "bre\u00b7chen", "durch", "der", "Dei\u00b7che", "Haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So bricht das Schlachtvolk durch mit Schrein und Heulen,", "tokens": ["So", "bricht", "das", "Schlacht\u00b7volk", "durch", "mit", "Schrein", "und", "Heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zerrei\u00dft der Griechenkette stolze Kraft.", "tokens": ["Zer\u00b7rei\u00dft", "der", "Grie\u00b7chen\u00b7ket\u00b7te", "stol\u00b7ze", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}