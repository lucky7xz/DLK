{"dta.poem.7057": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn GOTT den Raum der Welt, der, wie wir ihn be-", "tokens": ["Wenn", "GoTT", "den", "Raum", "der", "Welt", ",", "der", ",", "wie", "wir", "ihn", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ART", "NN", "ART", "NN", "$,", "PRELS", "$,", "PWAV", "PPER", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "finden,", "tokens": ["fin\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Materialisch ist, so gro\u00df, so tief, so weit,", "tokens": ["Ma\u00b7te\u00b7ri\u00b7a\u00b7lisch", "ist", ",", "so", "gro\u00df", ",", "so", "tief", ",", "so", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr C\u00f6rper nur erbaut, da\u00df wir ihn kaum ergr\u00fcnden;", "tokens": ["F\u00fcr", "C\u00f6r\u00b7per", "nur", "er\u00b7baut", ",", "da\u00df", "wir", "ihn", "kaum", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVPP", "$,", "KOUS", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von welcher Unerm\u00e4\u00dflichkeit", "tokens": ["Von", "wel\u00b7cher", "Un\u00b7er\u00b7m\u00e4\u00df\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df nicht ein Wohn-Platz seyn,", "tokens": ["Mu\u00df", "nicht", "ein", "Wohn\u00b7Platz", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Worinn die GOTTHEJT Selbst in einer hellen F\u00fclle", "tokens": ["Wo\u00b7rinn", "die", "GoTTHEJT", "Selbst", "in", "ei\u00b7ner", "hel\u00b7len", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Von Licht und Majest\u00e4t, von Herrlichkeit und Schein,", "tokens": ["Von", "Licht", "und", "Ma\u00b7jes\u00b7t\u00e4t", ",", "von", "Herr\u00b7lich\u00b7keit", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von Glantz und Wonne wohnt? woselbst in seel\u2019ger Stille", "tokens": ["Von", "Glantz", "und", "Won\u00b7ne", "wohnt", "?", "wo\u00b7selbst", "in", "seel'\u00b7ger", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$.", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Viel tausend, tausend Millionen", "tokens": ["Viel", "tau\u00b7send", ",", "tau\u00b7send", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "CARD", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Begl\u00fcckte Seelige, bey so viel Engeln wohnen,", "tokens": ["Be\u00b7gl\u00fcck\u00b7te", "See\u00b7li\u00b7ge", ",", "bey", "so", "viel", "En\u00b7geln", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die an den Glantz der Vollenkommenheiten,", "tokens": ["Die", "an", "den", "Glantz", "der", "Vol\u00b7len\u00b7kom\u00b7men\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So aus der GOTTHEJT fl\u00fcsst, die ewig sich vermehren,", "tokens": ["So", "aus", "der", "GoTTHEJT", "fl\u00fcsst", ",", "die", "e\u00b7wig", "sich", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.14": {"text": "In stets sich mehrenden unnennbar\u2019n S\u00fc\u00dfigkeiten,", "tokens": ["In", "stets", "sich", "meh\u00b7ren\u00b7den", "un\u00b7nenn\u00b7ba\u00b7r'n", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PRF", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "In unaussprechlichem Vergn\u00fcgen n\u00e4hren.", "tokens": ["In", "un\u00b7aus\u00b7sprech\u00b7li\u00b7chem", "Ver\u00b7gn\u00fc\u00b7gen", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Dies ist gewi\u00df, da\u00df unsre Krafft zu dencken", "tokens": ["Dies", "ist", "ge\u00b7wi\u00df", ",", "da\u00df", "uns\u00b7re", "Krafft", "zu", "den\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sich nimmer hoch genug auf diesen Punct zu lencken", "tokens": ["Sich", "nim\u00b7mer", "hoch", "ge\u00b7nug", "auf", "die\u00b7sen", "Punct", "zu", "len\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "ADV", "APPR", "PDAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vermag, noch f\u00e4hig ist, sich etwas vorzustellen,", "tokens": ["Ver\u00b7mag", ",", "noch", "f\u00e4\u00b7hig", "ist", ",", "sich", "et\u00b7was", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "VAFIN", "$,", "PRF", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was bey dem Mittel-Punet der Allmacht, bey den Quellen", "tokens": ["Was", "bey", "dem", "Mit\u00b7tel\u00b7Pu\u00b7net", "der", "All\u00b7macht", ",", "bey", "den", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+---+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Der Weisheit, bey dem Meer der G\u00fcte, nicht geringe", "tokens": ["Der", "Weis\u00b7heit", ",", "bey", "dem", "Meer", "der", "G\u00fc\u00b7te", ",", "nicht", "ge\u00b7rin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,", "PTKNEG", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und niedertr\u00e4chtig ist. Unm\u00f6glich ist es nicht,", "tokens": ["Und", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "ist", ".", "Un\u00b7m\u00f6g\u00b7lich", "ist", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$.", "ADJD", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df bey der Aendrung aller Dinge,", "tokens": ["Da\u00df", "bey", "der", "A\u00b7en\u00b7drung", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "So gar auch unsre Welt, nebst so viel tausend Erden,", "tokens": ["So", "gar", "auch", "uns\u00b7re", "Welt", ",", "nebst", "so", "viel", "tau\u00b7send", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "$,", "ADV", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ein Anhang und ein Theil vom Paradiese werden,", "tokens": ["Ein", "An\u00b7hang", "und", "ein", "Theil", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und sich verhimmeln kann. Denn hier ist gar kein Ziel,", "tokens": ["Und", "sich", "ver\u00b7him\u00b7meln", "kann", ".", "Denn", "hier", "ist", "gar", "kein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "VMFIN", "$.", "KON", "ADV", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und selbst die heil\u2019ge Bibel spricht", "tokens": ["Und", "selbst", "die", "heil'\u00b7ge", "Bi\u00b7bel", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}}}}