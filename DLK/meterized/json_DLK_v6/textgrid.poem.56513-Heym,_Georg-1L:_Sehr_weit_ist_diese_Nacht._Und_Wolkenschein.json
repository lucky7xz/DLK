{"textgrid.poem.56513": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sehr weit ist diese Nacht. Und Wolkenschein", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehr weit ist diese Nacht. Und Wolkenschein", "tokens": ["Sehr", "weit", "ist", "die\u00b7se", "Nacht", ".", "Und", "Wol\u00b7ken\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "$.", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zerrei\u00dfet vor des Mondes Untergang.", "tokens": ["Zer\u00b7rei\u00b7\u00dfet", "vor", "des", "Mon\u00b7des", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und tausend Fenster stehn die Nacht entlang", "tokens": ["Und", "tau\u00b7send", "Fens\u00b7ter", "stehn", "die", "Nacht", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVFIN", "ART", "NN", "APPO"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und blinzeln mit den Lidern, rot und klein.", "tokens": ["Und", "blin\u00b7zeln", "mit", "den", "Li\u00b7dern", ",", "rot", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie Aderwerk gehn Stra\u00dfen durch die Stadt,", "tokens": ["Wie", "A\u00b7der\u00b7werk", "gehn", "Stra\u00b7\u00dfen", "durch", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unz\u00e4hlig Menschen schwemmen aus und ein.", "tokens": ["Un\u00b7z\u00e4h\u00b7lig", "Men\u00b7schen", "schwem\u00b7men", "aus", "und", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und ewig stumpfer Ton von stumpfem Sein", "tokens": ["Und", "e\u00b7wig", "stump\u00b7fer", "Ton", "von", "stump\u00b7fem", "Sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Eint\u00f6nig kommt heraus in Stille matt.", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", "kommt", "he\u00b7raus", "in", "Stil\u00b7le", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Geb\u00e4ren, Tod, gewirktes Einerlei,", "tokens": ["Ge\u00b7b\u00e4\u00b7ren", ",", "Tod", ",", "ge\u00b7wirk\u00b7tes", "Ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lallen der Wehen, langer Sterbeschrei,", "tokens": ["Lal\u00b7len", "der", "We\u00b7hen", ",", "lan\u00b7ger", "Ster\u00b7be\u00b7schrei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Im blinden Wechsel geht es dumpf vorbei.", "tokens": ["Im", "blin\u00b7den", "Wech\u00b7sel", "geht", "es", "dumpf", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und Schein und Feuer, Fackeln rot und Brand,", "tokens": ["Und", "Schein", "und", "Feu\u00b7er", ",", "Fa\u00b7ckeln", "rot", "und", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die drohn im Weiten mit gez\u00fcckter Hand", "tokens": ["Die", "drohn", "im", "Wei\u00b7ten", "mit", "ge\u00b7z\u00fcck\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und scheinen hoch von dunkler Wolkenwand.", "tokens": ["Und", "schei\u00b7nen", "hoch", "von", "dunk\u00b7ler", "Wol\u00b7ken\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}