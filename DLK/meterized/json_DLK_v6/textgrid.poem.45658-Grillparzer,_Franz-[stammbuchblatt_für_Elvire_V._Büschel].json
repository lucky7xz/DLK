{"textgrid.poem.45658": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[stammbuchblatt f\u00fcr Elvire V. B\u00fcschel]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Dichter liegt seit lang begraben,", "tokens": ["Der", "Dich\u00b7ter", "liegt", "seit", "lang", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mensch lebt freilich denn noch jetzt;", "tokens": ["Der", "Mensch", "lebt", "frei\u00b7lich", "denn", "noch", "jetzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erinnerung an deine sch\u00f6nen Gaben", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "an", "dei\u00b7ne", "sch\u00f6\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat mich in fr\u00fchre Zeit zur\u00fcckversetzt.", "tokens": ["Hat", "mich", "in", "fr\u00fch\u00b7re", "Zeit", "zu\u00b7r\u00fcck\u00b7ver\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}