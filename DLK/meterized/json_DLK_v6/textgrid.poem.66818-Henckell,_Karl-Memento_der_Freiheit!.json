{"textgrid.poem.66818": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Memento der Freiheit!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, Freunde! Nicht ewig das Elend bejammern", "tokens": ["Auf", ",", "Freun\u00b7de", "!", "Nicht", "e\u00b7wig", "das", "E\u00b7lend", "be\u00b7jam\u00b7mern"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "NN", "$.", "PTKNEG", "ADJD", "ART", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In sentimentalen Gedichten!", "tokens": ["In", "sen\u00b7ti\u00b7men\u00b7ta\u00b7len", "Ge\u00b7dich\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "La\u00dft uns mit strophischen Eisenklammern", "tokens": ["La\u00dft", "uns", "mit", "stro\u00b7phi\u00b7schen", "Ei\u00b7sen\u00b7klam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Den Bau des Rechtes errichten!", "tokens": ["Den", "Bau", "des", "Rech\u00b7tes", "er\u00b7rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Tr\u00e4nen, die ihr in Versen vergie\u00dft,", "tokens": ["Die", "Tr\u00e4\u00b7nen", ",", "die", "ihr", "in", "Ver\u00b7sen", "ver\u00b7gie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie sie der Protz mit Behagen genie\u00dft!", "tokens": ["Wie", "sie", "der", "Protz", "mit", "Be\u00b7ha\u00b7gen", "ge\u00b7nie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die goldgescheckte Hy\u00e4ne", "tokens": ["Die", "gold\u00b7ge\u00b7scheck\u00b7te", "Hy\u00b7\u00e4\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Weint gar noch selbst eine Tr\u00e4ne.", "tokens": ["Weint", "gar", "noch", "selbst", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Wohl soll mit ersch\u00fctternden Wahrheitst\u00f6nen", "tokens": ["Wohl", "soll", "mit", "er\u00b7sch\u00fct\u00b7tern\u00b7den", "Wahr\u00b7heits\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Not aufschrein in der Dichtung,", "tokens": ["Die", "Not", "auf\u00b7schrein", "in", "der", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wohl soll im Liede das Leid aufst\u00f6hnen,", "tokens": ["Wohl", "soll", "im", "Lie\u00b7de", "das", "Leid", "auf\u00b7st\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sich aufb\u00e4umen die Lebensvernichtung.", "tokens": ["Sich", "auf\u00b7b\u00e4u\u00b7men", "die", "Le\u00b7bens\u00b7ver\u00b7nich\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Doch nicht Lamento der ganze Mann!", "tokens": ["Doch", "nicht", "La\u00b7men\u00b7to", "der", "gan\u00b7ze", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Stimmt das Memento der Freiheit an!", "tokens": ["Stimmt", "das", "Me\u00b7men\u00b7to", "der", "Frei\u00b7heit", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Schlagt mit den \u00c4xten der Reime", "tokens": ["Schlagt", "mit", "den", "\u00c4x\u00b7ten", "der", "Rei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "In die wurmzerfressenen B\u00e4ume!", "tokens": ["In", "die", "wurm\u00b7zer\u00b7fres\u00b7se\u00b7nen", "B\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf, Freunde! Die mitleidselige Laute", "tokens": ["Auf", ",", "Freun\u00b7de", "!", "Die", "mit\u00b7leid\u00b7se\u00b7li\u00b7ge", "Lau\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ein St\u00fcndchen geh\u00e4ngt auf den R\u00fccken!", "tokens": ["Ein", "St\u00fcnd\u00b7chen", "ge\u00b7h\u00e4ngt", "auf", "den", "R\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das Weh, das mitleidlos gestaute,", "tokens": ["Das", "Weh", ",", "das", "mit\u00b7leid\u00b7los", "ge\u00b7stau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df mattes Gewimmer ersticken.", "tokens": ["Mu\u00df", "mat\u00b7tes", "Ge\u00b7wim\u00b7mer", "er\u00b7sti\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die Trommel gewirbelt! Fanfaren ins Feld!", "tokens": ["Die", "Trom\u00b7mel", "ge\u00b7wir\u00b7belt", "!", "Fan\u00b7fa\u00b7ren", "ins", "Feld", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "NN", "APPRART", "NN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Geschmettert den Sturm in die schl\u00e4frige Welt!", "tokens": ["Ge\u00b7schmet\u00b7tert", "den", "Sturm", "in", "die", "schl\u00e4f\u00b7ri\u00b7ge", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Gewitter in eure Lieder!", "tokens": ["Ge\u00b7wit\u00b7ter", "in", "eu\u00b7re", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sonst donnert die Zukunft euch nieder.", "tokens": ["Sonst", "don\u00b7nert", "die", "Zu\u00b7kunft", "euch", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}