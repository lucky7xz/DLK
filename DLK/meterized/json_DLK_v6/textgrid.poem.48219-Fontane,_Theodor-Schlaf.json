{"textgrid.poem.48219": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Schlaf", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun trifft es mich, wie's jeden traf,", "tokens": ["Nun", "trifft", "es", "mich", ",", "wie's", "je\u00b7den", "traf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich liege wach, es meidet mich der Schlaf,", "tokens": ["Ich", "lie\u00b7ge", "wach", ",", "es", "mei\u00b7det", "mich", "der", "Schlaf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur im Vorbeigehn fl\u00fcstert er mir zu:", "tokens": ["Nur", "im", "Vor\u00b7bei\u00b7gehn", "fl\u00fcs\u00b7tert", "er", "mir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u00bbsei nicht in Sorg', ich sammle deine Ruh',", "tokens": ["\u00bb", "sei", "nicht", "in", "Sor\u00b7g'", ",", "ich", "samm\u00b7le", "dei\u00b7ne", "Ruh'", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "APPR", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+++-+-+-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "Und tret' ich ehstens wieder in dein Haus,", "tokens": ["Und", "tret'", "ich", "ehs\u00b7tens", "wie\u00b7der", "in", "dein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So zahl' ich alles dir auf einmal aus.\u00ab", "tokens": ["So", "zahl'", "ich", "al\u00b7les", "dir", "auf", "ein\u00b7mal", "aus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PPER", "APPR", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}