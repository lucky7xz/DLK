 "pos": ["ITJ", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "Du, du kanst mir Gnad' ertheilen,", "tokens": ["Du", ",", "du", "kanst", "mir", "Gnad'", "er\u00b7thei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Hilf mir doch aus dieser Qual;", "tokens": ["Hilf", "mir", "doch", "aus", "die\u00b7ser", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Denn du kennest ja die Schwachen,", "tokens": ["Denn", "du", "ken\u00b7nest", "ja", "die", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.66": {"text": "Die du wiedrum stark wilt machen.", "tokens": ["Die", "du", "wie\u00b7drum", "stark", "wilt", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.67": {"text": "Z\u00e4hle los mich Hochbetr\u00fcbten,", "tokens": ["Z\u00e4h\u00b7le", "los", "mich", "Hoch\u00b7be\u00b7tr\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Der ich nicht bezahlen kan;", "tokens": ["Der", "ich", "nicht", "be\u00b7zah\u00b7len", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.69": {"text": "Liebe mich in dem Geliebten,", "tokens": ["Lie\u00b7be", "mich", "in", "dem", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.70": {"text": "Dein Sohn Jesus nimt mich an,", "tokens": ["Dein", "Sohn", "Je\u00b7sus", "nimt", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.71": {"text": "Jesus l\u00e4\u00dft mich nicht verderben,", "tokens": ["Je\u00b7sus", "l\u00e4\u00dft", "mich", "nicht", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Jesus l\u00e4\u00dft mich nicht im Sterben.", "tokens": ["Je\u00b7sus", "l\u00e4\u00dft", "mich", "nicht", "im", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.73": {"text": "Herr, du wollest Gnade geben,", "tokens": ["Herr", ",", "du", "wol\u00b7lest", "Gna\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Da\u00df die\u00df Jahr mir heilig sei", "tokens": ["Da\u00df", "die\u00df", "Jahr", "mir", "hei\u00b7lig", "sei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "NN", "PPER", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.75": {"text": "Und ich christlich k\u00f6nne leben", "tokens": ["Und", "ich", "christ\u00b7lich", "k\u00f6n\u00b7ne", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Sonder Trug und Heuchelei,", "tokens": ["Son\u00b7der", "Trug", "und", "Heu\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.77": {"text": "Da\u00df ich noch allhie auf Erden", "tokens": ["Da\u00df", "ich", "noch", "all\u00b7hie", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.78": {"text": "Fromm und selig m\u00f6ge werden.", "tokens": ["Fromm", "und", "se\u00b7lig", "m\u00f6\u00b7ge", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.79": {"text": "La\u00df mich armen S\u00fcnder ziehen", "tokens": ["La\u00df", "mich", "ar\u00b7men", "S\u00fcn\u00b7der", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.80": {"text": "Deinen Weg der Fr\u00f6mmigkeit;", "tokens": ["Dei\u00b7nen", "Weg", "der", "Fr\u00f6m\u00b7mig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.81": {"text": "La\u00df mich Stolz und Hoffart fliehen,", "tokens": ["La\u00df", "mich", "Stolz", "und", "Hof\u00b7fart", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.82": {"text": "La\u00df mich beten jederzeit,", "tokens": ["La\u00df", "mich", "be\u00b7ten", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.83": {"text": "La\u00df mich Schand' und Unzucht meiden,", "tokens": ["La\u00df", "mich", "Schand'", "und", "Un\u00b7zucht", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.84": {"text": "La\u00df mich willig Ungl\u00fcck leiden.", "tokens": ["La\u00df", "mich", "wil\u00b7lig", "Un\u00b7gl\u00fcck", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.85": {"text": "Jesus richte mein Beginnen,", "tokens": ["Je\u00b7sus", "rich\u00b7te", "mein", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.86": {"text": "Jesus bleibe stets bei mir,", "tokens": ["Je\u00b7sus", "blei\u00b7be", "stets", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.87": {"text": "Jesus z\u00e4ume mir die Sinnen,", "tokens": ["Je\u00b7sus", "z\u00e4u\u00b7me", "mir", "die", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.88": {"text": "Jesus sei nur mein' Begier,", "tokens": ["Je\u00b7sus", "sei", "nur", "mein'", "Be\u00b7gier", ","], "token_info": ["word", 