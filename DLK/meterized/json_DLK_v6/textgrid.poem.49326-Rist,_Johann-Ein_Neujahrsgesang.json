{"textgrid.poem.49326": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Ein Neujahrsgesang", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hilf, Herr Jesu, la\u00df gelingen,", "tokens": ["Hilf", ",", "Herr", "Je\u00b7su", ",", "la\u00df", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NE", "$,", "VVIMP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hilf, das neue Jahr geht an,", "tokens": ["Hilf", ",", "das", "neu\u00b7e", "Jahr", "geht", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df es neue Kr\u00e4fte bringen,", "tokens": ["La\u00df", "es", "neu\u00b7e", "Kr\u00e4f\u00b7te", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df aufs neu ich wandlen kan,", "tokens": ["Da\u00df", "aufs", "neu", "ich", "wand\u00b7len", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJD", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Neues Gl\u00fcck und neues Leben", "tokens": ["Neu\u00b7es", "Gl\u00fcck", "und", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wollest du mit Gnaden geben.", "tokens": ["Wol\u00b7lest", "du", "mit", "Gna\u00b7den", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles, was ich auszurichten", "tokens": ["Al\u00b7les", ",", "was", "ich", "aus\u00b7zu\u00b7rich\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "PWS", "PPER", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und zu reden bin bedacht,", "tokens": ["Und", "zu", "re\u00b7den", "bin", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00fcsse mich, mein Gott, verpflichten", "tokens": ["M\u00fcs\u00b7se", "mich", ",", "mein", "Gott", ",", "ver\u00b7pflich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Deines theuren Namens Macht,", "tokens": ["Dei\u00b7nes", "theu\u00b7ren", "Na\u00b7mens", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df auch das, was ich gedenke,", "tokens": ["Da\u00df", "auch", "das", ",", "was", "ich", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dich zu preisen stets sich lenke.", "tokens": ["Dich", "zu", "prei\u00b7sen", "stets", "sich", "len\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVFIN", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Meiner H\u00e4nde Werk' und Thaten,", "tokens": ["Mei\u00b7ner", "H\u00e4n\u00b7de", "Werk'", "und", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Meiner Zungen Red und Wort", "tokens": ["Mei\u00b7ner", "Zun\u00b7gen", "Red", "und", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "M\u00fcsse nur durch dich geraten", "tokens": ["M\u00fcs\u00b7se", "nur", "durch", "dich", "ge\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und ganz gl\u00fccklich gehen fort;", "tokens": ["Und", "ganz", "gl\u00fcck\u00b7lich", "ge\u00b7hen", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Neue Kraft la\u00df mich erf\u00fcllen,", "tokens": ["Neu\u00b7e", "Kraft", "la\u00df", "mich", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Zu verrichten deinen Willen.", "tokens": ["Zu", "ver\u00b7rich\u00b7ten", "dei\u00b7nen", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Was ich dichte, was ich mache,", "tokens": ["Was", "ich", "dich\u00b7te", ",", "was", "ich", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Das gescheh' in dir allein,", "tokens": ["Das", "ge\u00b7scheh'", "in", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Wenn ich schlafe, wenn ich wache,", "tokens": ["Wenn", "ich", "schla\u00b7fe", ",", "wenn", "ich", "wa\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wollest du, Herr, bei mir sein;", "tokens": ["Wol\u00b7lest", "du", ",", "Herr", ",", "bei", "mir", "sein", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$,", "APPR", "PPER", "VAINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "Geh' ich aus, halt an zur Seiten,", "tokens": ["Geh'", "ich", "aus", ",", "halt", "an", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Komm' ich heim, so hilf mich gleiten.", "tokens": ["Komm'", "ich", "heim", ",", "so", "hilf", "mich", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "La\u00df mich beugen meine Kniee", "tokens": ["La\u00df", "mich", "beu\u00b7gen", "mei\u00b7ne", "Kni\u00b7ee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Nur zu deines Namens Ehr';", "tokens": ["Nur", "zu", "dei\u00b7nes", "Na\u00b7mens", "Ehr'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Hilf, da\u00df ich mich stets bem\u00fche,", "tokens": ["Hilf", ",", "da\u00df", "ich", "mich", "stets", "be\u00b7m\u00fc\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Dich zu preisen mehr und mehr;", "tokens": ["Dich", "zu", "prei\u00b7sen", "mehr", "und", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVFIN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "La\u00df mein Bitten und mein Flehen", "tokens": ["La\u00df", "mein", "Bit\u00b7ten", "und", "mein", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Doch im Himmel vor dir stehen.", "tokens": ["Doch", "im", "Him\u00b7mel", "vor", "dir", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "La\u00df mich, Herr, in deinem Namen", "tokens": ["La\u00df", "mich", ",", "Herr", ",", "in", "dei\u00b7nem", "Na\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Fr\u00f6lich nehmen Speis' und Trank,", "tokens": ["Fr\u00f6\u00b7lich", "neh\u00b7men", "Speis'", "und", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "G\u00fcter, die von dir her kamen,", "tokens": ["G\u00fc\u00b7ter", ",", "die", "von", "dir", "her", "ka\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Fodern ja von mir den Dank;", "tokens": ["Fo\u00b7dern", "ja", "von", "mir", "den", "Dank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Deine Weisheit kan mich st\u00e4rken", "tokens": ["Dei\u00b7ne", "Weis\u00b7heit", "kan", "mich", "st\u00e4r\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Zu der Lieb' und guten Werken.", "tokens": ["Zu", "der", "Lieb'", "und", "gu\u00b7ten", "Wer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Mein Gebet das mu\u00df aufsteigen,", "tokens": ["Mein", "Ge\u00b7bet", "das", "mu\u00df", "auf\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Herr, f\u00fcr deinen Gnadenthron,", "tokens": ["Herr", ",", "f\u00fcr", "dei\u00b7nen", "Gna\u00b7dent\u00b7hron", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Denn wirst du zu mir dich neigen", "tokens": ["Denn", "wirst", "du", "zu", "mir", "dich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPER", "PPER", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Wie zu deinem lieben Sohn;", "tokens": ["Wie", "zu", "dei\u00b7nem", "lie\u00b7ben", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Herr, ich wei\u00df, es wird f\u00fcr allen", "tokens": ["Herr", ",", "ich", "wei\u00df", ",", "es", "wird", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Die\u00df mein Opfer dir gefallen.", "tokens": ["Die\u00df", "mein", "Op\u00b7fer", "dir", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "La\u00df die\u00df sein ein Jahr der Gnaden,", "tokens": ["La\u00df", "die\u00df", "sein", "ein", "Jahr", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "PPOSAT", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "La\u00df mich b\u00fc\u00dfen meine S\u00fcnd';", "tokens": ["La\u00df", "mich", "b\u00fc\u00b7\u00dfen", "mei\u00b7ne", "S\u00fcnd'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Hilf, da\u00df sie mir nimmer schaden,", "tokens": ["Hilf", ",", "da\u00df", "sie", "mir", "nim\u00b7mer", "scha\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Sondern bald Verzeihung find',", "tokens": ["Son\u00b7dern", "bald", "Ver\u00b7zei\u00b7hung", "find'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Herr, in dir! Nur du, mein Leben,", "tokens": ["Herr", ",", "in", "dir", "!", "Nur", "du", ",", "mein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "$.", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Kanst die S\u00fcnd' allein vergeben.", "tokens": ["Kanst", "die", "S\u00fcnd'", "al\u00b7lein", "ver\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Tr\u00f6ste mich mit deiner Liebe,", "tokens": ["Tr\u00f6s\u00b7te", "mich", "mit", "dei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Nim, o Gott, mein Flehen hin,", "tokens": ["Nim", ",", "o", "Gott", ",", "mein", "Fle\u00b7hen", "hin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Weil ich mich so sehr betr\u00fcbe,", "tokens": ["Weil", "ich", "mich", "so", "sehr", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Ja, vol Angst und Zagen bin;", "tokens": ["Ja", ",", "vol", "Angst", "und", "Za\u00b7gen", "bin", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "St\u00e4rke mich in meinen N\u00f6ten,", "tokens": ["St\u00e4r\u00b7ke", "mich", "in", "mei\u00b7nen", "N\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Da\u00df mich S\u00fcnd' und Tod nicht t\u00f6ten.", "tokens": ["Da\u00df", "mich", "S\u00fcnd'", "und", "Tod", "nicht", "t\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Salb', o Vater, meine Wunden,", "tokens": ["Salb'", ",", "o", "Va\u00b7ter", ",", "mei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Wasche mich mit Isop ab,", 