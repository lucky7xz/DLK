{"dta.poem.19311": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich irret in der Welt die Vielgestaltigkeit,", "tokens": ["Dich", "ir\u00b7ret", "in", "der", "Welt", "die", "Viel\u00b7ge\u00b7stal\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einf\u00e4lt'ger, dir misf\u00e4llt die Manigfaltigkeit:", "tokens": ["Ein\u00b7f\u00e4lt'\u00b7ger", ",", "dir", "mis\u00b7f\u00e4llt", "die", "Ma\u00b7nig\u00b7fal\u00b7tig\u00b7keit", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df nicht an jedem Ort gilt, was an einem gilt,", "tokens": ["Da\u00df", "nicht", "an", "je\u00b7dem", "Ort", "gilt", ",", "was", "an", "ei\u00b7nem", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "PIAT", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df die eine Zeit lobt, was die andre schilt;", "tokens": ["Und", "da\u00df", "die", "ei\u00b7ne", "Zeit", "lobt", ",", "was", "die", "and\u00b7re", "schilt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So ist es, wie der Spruch des Meisters ausgesprochen:", "tokens": ["So", "ist", "es", ",", "wie", "der", "Spruch", "des", "Meis\u00b7ters", "aus\u00b7ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wird hier Widerspruch von Widerspruch gebrochen.", "tokens": ["Es", "wird", "hier", "Wi\u00b7der\u00b7spruch", "von", "Wi\u00b7der\u00b7spruch", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dich aber m\u00f6cht' ich nicht zum G\u00e4rtner meinem Garten,", "tokens": ["Dich", "a\u00b7ber", "m\u00f6cht'", "ich", "nicht", "zum", "G\u00e4rt\u00b7ner", "mei\u00b7nem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da du nicht zugestehst den Blumen ihre Arten.", "tokens": ["Da", "du", "nicht", "zu\u00b7ge\u00b7stehst", "den", "Blu\u00b7men", "ih\u00b7re", "Ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch stellte gar dich Gott in seinem Garten an,", "tokens": ["Doch", "stell\u00b7te", "gar", "dich", "Gott", "in", "sei\u00b7nem", "Gar\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie w\u00fcrde nicht zu Spott sein Plan vor deinem Plan!", "tokens": ["Wie", "w\u00fcr\u00b7de", "nicht", "zu", "Spott", "sein", "Plan", "vor", "dei\u00b7nem", "Plan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PTKNEG", "APPR", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie w\u00fcrde nicht genutzt die Scheer', und weggeputzt", "tokens": ["Wie", "w\u00fcr\u00b7de", "nicht", "ge\u00b7nutzt", "die", "Scheer'", ",", "und", "weg\u00b7ge\u00b7putzt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "PTKNEG", "VVPP", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unn\u00fctzer Putz, und fein gleichf\u00f6rmig zugestutzt;", "tokens": ["Un\u00b7n\u00fct\u00b7zer", "Putz", ",", "und", "fein", "gleich\u00b7f\u00f6r\u00b7mig", "zu\u00b7ge\u00b7stutzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "In Unergetzlichkeit w\u00fcrd' alles eingeschn\u00fcrt,", "tokens": ["In", "Un\u00b7er\u00b7getz\u00b7lich\u00b7keit", "w\u00fcrd'", "al\u00b7les", "ein\u00b7ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soweit Gesetzlichkeit du h\u00e4ttest eingef\u00fchrt.", "tokens": ["So\u00b7weit", "Ge\u00b7setz\u00b7lich\u00b7keit", "du", "h\u00e4t\u00b7test", "ein\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}