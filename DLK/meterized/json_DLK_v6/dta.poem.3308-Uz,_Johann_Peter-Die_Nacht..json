{"dta.poem.3308": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Du verst\u00f6rst uns nicht, o Nacht!", "tokens": ["Du", "ver\u00b7st\u00f6rst", "uns", "nicht", ",", "o", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh! wir trinken im Geb\u00fcsche;", "tokens": ["Sieh", "!", "wir", "trin\u00b7ken", "im", "Ge\u00b7b\u00fc\u00b7sche", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein k\u00fchler Wind erwacht,", "tokens": ["Und", "ein", "k\u00fch\u00b7ler", "Wind", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da\u00df er unsern Wein erfrische.", "tokens": ["Da\u00df", "er", "un\u00b7sern", "Wein", "er\u00b7fri\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mutter holder Dunkelheit,", "tokens": ["Mut\u00b7ter", "hol\u00b7der", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nacht! Vertraute s\u00fcsser Sorgen,", "tokens": ["Nacht", "!", "Ver\u00b7trau\u00b7te", "s\u00fcs\u00b7ser", "Sor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die betrogner Wachsamkeit", "tokens": ["Die", "be\u00b7trog\u00b7ner", "Wach\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Viele K\u00fcsse schon verborgen!", "tokens": ["Vie\u00b7le", "K\u00fcs\u00b7se", "schon", "ver\u00b7bor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dir allein sey mitbewust,", "tokens": ["Dir", "al\u00b7lein", "sey", "mit\u00b7be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch Vergn\u00fcgen mich berausche,", "tokens": ["Welch", "Ver\u00b7gn\u00fc\u00b7gen", "mich", "be\u00b7rau\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann ich an geliebter Brust", "tokens": ["Wann", "ich", "an", "ge\u00b7lieb\u00b7ter", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter Thau und Bluhmen lausche!", "tokens": ["Un\u00b7ter", "Thau", "und", "Bluh\u00b7men", "lau\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Murmelt ihr, wann alles ruht,", "tokens": ["Mur\u00b7melt", "ihr", ",", "wann", "al\u00b7les", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Murmelt, sanftbewegte B\u00e4ume,", "tokens": ["Mur\u00b7melt", ",", "sanft\u00b7be\u00b7weg\u00b7te", "B\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey dem Sprudeln heischrer Fluth,", "tokens": ["Bey", "dem", "Spru\u00b7deln", "heisc\u00b7hrer", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich in wollustvolle Tr\u00e4ume!", "tokens": ["Mich", "in", "wol\u00b7lust\u00b7vol\u00b7le", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}