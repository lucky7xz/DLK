{"textgrid.poem.38693": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch Fichten am H\u00fcgel, durch Erlen am Bach,", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Fichten am H\u00fcgel, durch Erlen am Bach,", "tokens": ["Durch", "Fich\u00b7ten", "am", "H\u00fc\u00b7gel", ",", "durch", "Er\u00b7len", "am", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Folgt immer dein Bildni\u00df, du Traute! mir nach.", "tokens": ["Folgt", "im\u00b7mer", "dein", "Bild\u00b7ni\u00df", ",", "du", "Trau\u00b7te", "!", "mir", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "PPER", "NN", "$.", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es l\u00e4chelt bald Wehmuth, es l\u00e4chelt bald Ruh',", "tokens": ["Es", "l\u00e4\u00b7chelt", "bald", "Weh\u00b7muth", ",", "es", "l\u00e4\u00b7chelt", "bald", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im freundlichen Schimmer des Mondes, mir zu.", "tokens": ["Im", "freund\u00b7li\u00b7chen", "Schim\u00b7mer", "des", "Mon\u00b7des", ",", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Den Rosengestr\u00e4uchen des Gartens entwallt", "tokens": ["Den", "Ro\u00b7sen\u00b7ge\u00b7str\u00e4u\u00b7chen", "des", "Gar\u00b7tens", "ent\u00b7wallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im Glanze der Fr\u00fche die holde Gestalt;", "tokens": ["Im", "Glan\u00b7ze", "der", "Fr\u00fc\u00b7he", "die", "hol\u00b7de", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie schwebt aus der Berge bepurpurtem Flor", "tokens": ["Sie", "schwebt", "aus", "der", "Ber\u00b7ge", "be\u00b7pur\u00b7pur\u00b7tem", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gleich einem elysischen Schatten hervor.", "tokens": ["Gleich", "ei\u00b7nem", "e\u00b7ly\u00b7si\u00b7schen", "Schat\u00b7ten", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Oft hab' ich, im Traum, als die sch\u00f6nste der Feen,", "tokens": ["Oft", "hab'", "ich", ",", "im", "Traum", ",", "als", "die", "sch\u00f6ns\u00b7te", "der", "Feen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPRART", "NN", "$,", "KOUS", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf goldenem Throne dich stralen gesehn;", "tokens": ["Auf", "gol\u00b7de\u00b7nem", "Thro\u00b7ne", "dich", "stra\u00b7len", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Oft hab' ich, zum hohen Olympus entz\u00fcckt,", "tokens": ["Oft", "hab'", "ich", ",", "zum", "ho\u00b7hen", "O\u00b7lym\u00b7pus", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPRART", "ADJA", "NE", "VVPP", "$,"], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Als Hebe dich unter den G\u00f6ttern erblickt.", "tokens": ["Als", "He\u00b7be", "dich", "un\u00b7ter", "den", "G\u00f6t\u00b7tern", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Mir hallt aus den Tiefen, mir hallt von den H\u00f6hn,", "tokens": ["Mir", "hallt", "aus", "den", "Tie\u00b7fen", ",", "mir", "hallt", "von", "den", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dein himmlischer Name wie Sph\u00e4renget\u00f6n.", "tokens": ["Dein", "himm\u00b7li\u00b7scher", "Na\u00b7me", "wie", "Sph\u00e4\u00b7ren\u00b7ge\u00b7t\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KOKOM", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich w\u00e4hne den Hauch der die Bl\u00fcthen umwebt", "tokens": ["Ich", "w\u00e4h\u00b7ne", "den", "Hauch", "der", "die", "Bl\u00fc\u00b7then", "um\u00b7webt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von deiner melodischen Stimme durchbebt.", "tokens": ["Von", "dei\u00b7ner", "me\u00b7lo\u00b7di\u00b7schen", "Stim\u00b7me", "durch\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "In heiliger Mitternachtstunde durchkreist", "tokens": ["In", "hei\u00b7li\u00b7ger", "Mit\u00b7ter\u00b7nachts\u00b7tun\u00b7de", "durch\u00b7kreist"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Aethers Gefilde mein ahndender Geist.", "tokens": ["Des", "A\u00b7e\u00b7thers", "Ge\u00b7fil\u00b7de", "mein", "ahn\u00b7den\u00b7der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Geliebte! dort winkt uns ein Land, wo der Freund", "tokens": ["Ge\u00b7lieb\u00b7te", "!", "dort", "winkt", "uns", "ein", "Land", ",", "wo", "der", "Freund"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Auf ewig der Freundin sich wieder vereint.", "tokens": ["Auf", "e\u00b7wig", "der", "Freun\u00b7din", "sich", "wie\u00b7der", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Die Freude sie schwindet, es dauert kein Leid;", "tokens": ["Die", "Freu\u00b7de", "sie", "schwin\u00b7det", ",", "es", "dau\u00b7ert", "kein", "Leid", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Jahre verrauschen im Strome der Zeit;", "tokens": ["Die", "Jah\u00b7re", "ver\u00b7rau\u00b7schen", "im", "Stro\u00b7me", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Sonne wird sterben, die Erde vergehn:", "tokens": ["Die", "Son\u00b7ne", "wird", "ster\u00b7ben", ",", "die", "Er\u00b7de", "ver\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch Liebe mu\u00df ewig und ewig bestehn.", "tokens": ["Doch", "Lie\u00b7be", "mu\u00df", "e\u00b7wig", "und", "e\u00b7wig", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}