{"textgrid.poem.59575": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Rugard im Herbstmond 1811", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin, du freundlicher Strahl,", "tokens": ["Wo\u00b7hin", ",", "du", "freund\u00b7li\u00b7cher", "Strahl", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wohin locket dein Fr\u00fchlicht?", "tokens": ["Wo\u00b7hin", "lo\u00b7cket", "dein", "Fr\u00fch\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wohin, d\u00e4mmernder Morgen,", "tokens": ["Wo\u00b7hin", ",", "d\u00e4m\u00b7mern\u00b7der", "Mor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Spielet dein wechselnder Schein?", "tokens": ["Spie\u00b7let", "dein", "wech\u00b7seln\u00b7der", "Schein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Berge steigen unter der Bergh\u00f6h',", "tokens": ["Ber\u00b7ge", "stei\u00b7gen", "un\u00b7ter", "der", "Berg\u00b7h\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Waldige H\u00fcgel steigen", "tokens": ["Wal\u00b7di\u00b7ge", "H\u00fc\u00b7gel", "stei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Duftig an dem Gestade des Meers auf,", "tokens": ["Duf\u00b7tig", "an", "dem", "Ge\u00b7sta\u00b7de", "des", "Meers", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Wo ich als Knabe gespielt.", "tokens": ["Wo", "ich", "als", "Kna\u00b7be", "ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Und es schwellet mir Sehnsucht", "tokens": ["Und", "es", "schwel\u00b7let", "mir", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Leuchtende Augen,", "tokens": ["Leuch\u00b7ten\u00b7de", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und es fl\u00fcstert s\u00fc\u00dfe Erinnrung", "tokens": ["Und", "es", "fl\u00fcs\u00b7tert", "s\u00fc\u00b7\u00dfe", "E\u00b7rinn\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.12": {"text": "K\u00fcnftige Freuden", "tokens": ["K\u00fcnf\u00b7ti\u00b7ge", "Freu\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Mir ins lauschende Ohr:", "tokens": ["Mir", "ins", "lau\u00b7schen\u00b7de", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Tor, wohin mit der Unruh'?", "tokens": ["Tor", ",", "wo\u00b7hin", "mit", "der", "Un\u00b7ruh'", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.15": {"text": "Kennst du der Ferne", "tokens": ["Kennst", "du", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Gauklisch \u00e4ffendes Ziel nicht?", "tokens": ["Gau\u00b7klisch", "\u00e4f\u00b7fen\u00b7des", "Ziel", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.17": {"text": "Wei\u00dft du nicht, was um Pal\u00e4ste", "tokens": ["Wei\u00dft", "du", "nicht", ",", "was", "um", "Pa\u00b7l\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Goldenen Trug spinnt?", "tokens": ["Gol\u00b7de\u00b7nen", "Trug", "spinnt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.19": {"text": "Nicht, was an Thronen", "tokens": ["Nicht", ",", "was", "an", "Thro\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Sch\u00fcttelt mit blut'gem Verrat?", "tokens": ["Sch\u00fct\u00b7telt", "mit", "blut'\u00b7gem", "Ver\u00b7rat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.21": {"text": "Hier eine H\u00fctte, wo die liebliche Talkluft", "tokens": ["Hier", "ei\u00b7ne", "H\u00fct\u00b7te", ",", "wo", "die", "lieb\u00b7li\u00b7che", "Tal\u00b7kluft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Gegen den s\u00fcdlichen See", "tokens": ["Ge\u00b7gen", "den", "s\u00fcd\u00b7li\u00b7chen", "See"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "Abschlie\u00dft, wo an dem Waldberg", "tokens": ["Ab\u00b7schlie\u00dft", ",", "wo", "an", "dem", "Wald\u00b7berg"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Nachtigallieder der Fr\u00fchling weckt,", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7lie\u00b7der", "der", "Fr\u00fch\u00b7ling", "weckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Und ein Feldchen, an dessen", "tokens": ["Und", "ein", "Feld\u00b7chen", ",", "an", "des\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PRELAT"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Fernster Grenze dein Weib dir", "tokens": ["Ferns\u00b7ter", "Gren\u00b7ze", "dein", "Weib", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.27": {"text": "Von der Schwelle rufet: Spann aus nun,", "tokens": ["Von", "der", "Schwel\u00b7le", "ru\u00b7fet", ":", "Spann", "aus", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Denn das Mahl ist bereit.", "tokens": ["Denn", "das", "Mahl", "ist", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Aber siehe! Die Nebel", "tokens": ["A\u00b7ber", "sie\u00b7he", "!", "Die", "Ne\u00b7bel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sinken hin vor der h\u00f6heren Sonne.", "tokens": ["Sin\u00b7ken", "hin", "vor", "der", "h\u00f6\u00b7he\u00b7ren", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schaue, wie fliegen", "tokens": ["Schau\u00b7e", ",", "wie", "flie\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wandernde Masten", "tokens": ["Wan\u00b7dern\u00b7de", "Mas\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Hin durch die Flut!", "tokens": ["Hin", "durch", "die", "Flut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Taumelnde Bergh\u00f6hn", "tokens": ["Tau\u00b7meln\u00b7de", "Berg\u00b7h\u00f6hn"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wandeln mit ihnen,", "tokens": ["Wan\u00b7deln", "mit", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Schimmernde T\u00fcrme", "tokens": ["Schim\u00b7mern\u00b7de", "T\u00fcr\u00b7me"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Stattlicher St\u00e4dte", "tokens": ["Statt\u00b7li\u00b7cher", "St\u00e4d\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Flie\u00dfen und tanzen", "tokens": ["Flie\u00b7\u00dfen", "und", "tan\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Jenseits im Blauen,", "tokens": ["Jen\u00b7seits", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Und die Bewegung", "tokens": ["Und", "die", "Be\u00b7we\u00b7gung"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "M\u00e4chtigen Lebens", "tokens": ["M\u00e4ch\u00b7ti\u00b7gen", "Le\u00b7bens"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Brauset auch mir in die", "tokens": ["Brau\u00b7set", "auch", "mir", "in", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "Fl\u00fcgel der Seele,", "tokens": ["Fl\u00fc\u00b7gel", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "L\u00fcftet des Busens", "tokens": ["L\u00fcf\u00b7tet", "des", "Bu\u00b7sens"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Schwellende Segel.", "tokens": ["Schwel\u00b7len\u00b7de", "Se\u00b7gel", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Fahr wohl, Ruhe!", "tokens": ["Fahr", "wohl", ",", "Ru\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Wiege der Kindheit,", "tokens": ["Wie\u00b7ge", "der", "Kind\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Liebliches Eiland, fahr wohl!", "tokens": ["Lieb\u00b7li\u00b7ches", "Ei\u00b7land", ",", "fahr", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.21": {"text": "Und wiege in Freuden", "tokens": ["Und", "wie\u00b7ge", "in", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Hinfort ein gl\u00fccklich Geschlecht!", "tokens": ["Hin\u00b7fort", "ein", "gl\u00fcck\u00b7lich", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Ich mag nicht bleiben,", "tokens": ["Ich", "mag", "nicht", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Denn in die Weite", "tokens": ["Denn", "in", "die", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Lockt die Gefahr mich,", "tokens": ["Lockt", "die", "Ge\u00b7fahr", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "S\u00fc\u00dfe Sirene;", "tokens": ["S\u00fc\u00b7\u00dfe", "Si\u00b7re\u00b7ne", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.27": {"text": "Dr\u00e4uend auch stellt sich", "tokens": ["Dr\u00e4u\u00b7end", "auch", "stellt", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Blinkender R\u00fcstung", "tokens": ["Blin\u00b7ken\u00b7der", "R\u00fcs\u00b7tung"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Riesengestalt mir:", "tokens": ["Rie\u00b7sen\u00b7ge\u00b7stalt", "mir", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Arbeit bei Menschen", "tokens": ["Ar\u00b7beit", "bei", "Men\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Hei\u00dft sie, den G\u00f6ttern", "tokens": ["Hei\u00dft", "sie", ",", "den", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Klingt sie Minerva.", "tokens": ["Klingt", "sie", "Mi\u00b7ner\u00b7va", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.33": {"text": "Drum mu\u00df ich hinnen;", "tokens": ["Drum", "mu\u00df", "ich", "hin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Wo sich die L\u00e4nder", "tokens": ["Wo", "sich", "die", "L\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "H\u00e4ngen an L\u00e4nder,", "tokens": ["H\u00e4n\u00b7gen", "an", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Wo sich die K\u00e4mpfe", "tokens": ["Wo", "sich", "die", "K\u00e4mp\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.37": {"text": "Dr\u00e4ngender mischen,", "tokens": ["Dr\u00e4n\u00b7gen\u00b7der", "mi\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.38": {"text": "Da steht mein Leben:", "tokens": ["Da", "steht", "mein", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Stille, fahr wohl!", "tokens": ["Stil\u00b7le", ",", "fahr", "wohl", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}