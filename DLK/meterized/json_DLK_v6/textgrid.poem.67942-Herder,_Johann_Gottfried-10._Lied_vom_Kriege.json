{"textgrid.poem.67942": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "10. Lied vom Kriege", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon erscholl die Post des Krieges,", "tokens": ["Schon", "er\u00b7scholl", "die", "Post", "des", "Krie\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon erging der Ruf der Feindschaft,", "tokens": ["Schon", "er\u00b7ging", "der", "Ruf", "der", "Feind\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwer von uns geht nun zum Kriege?", "tokens": ["\u00bb", "wer", "von", "uns", "geht", "nun", "zum", "Krie\u00b7ge", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00fcngster Bruder, gr\u00f6ster Bruder!", "tokens": ["J\u00fcngs\u00b7ter", "Bru\u00b7der", ",", "gr\u00f6s\u00b7ter", "Bru\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die h\u00f6chsten H\u00fcte, die sch\u00f6nsten Pferde,", "tokens": ["Die", "h\u00f6chs\u00b7ten", "H\u00fc\u00b7te", ",", "die", "sch\u00f6ns\u00b7ten", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die stolzesten Pferde, die deutschesten S\u00e4ttel! \u2013\u00ab", "tokens": ["Die", "stol\u00b7zes\u00b7ten", "Pfer\u00b7de", ",", "die", "deut\u00b7sches\u00b7ten", "S\u00e4t\u00b7tel", "!", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+--+-+++-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Eilig r\u00fcstet' ich den Bruder,", "tokens": ["Ei\u00b7lig", "r\u00fcs\u00b7tet'", "ich", "den", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fcstet' ihn und unterwies ihn:", "tokens": ["R\u00fcs\u00b7tet'", "ihn", "und", "un\u00b7ter\u00b7wies", "ihn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieber Bruder, guter Bruder,", "tokens": ["Lie\u00b7ber", "Bru\u00b7der", ",", "gu\u00b7ter", "Bru\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reit' nicht vorw\u00e4rts, bleib' nicht r\u00fcckw\u00e4rts,", "tokens": ["Reit'", "nicht", "vor\u00b7w\u00e4rts", ",", "bleib'", "nicht", "r\u00fcck\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$,", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn der Feind erschl\u00e4gt die ersten,", "tokens": ["Denn", "der", "Feind", "er\u00b7schl\u00e4gt", "die", "ers\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Feind erschl\u00e4gt die letzten.", "tokens": ["Und", "der", "Feind", "er\u00b7schl\u00e4gt", "die", "letz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dreh' dich mitten in den Krieg hin,", "tokens": ["Dreh'", "dich", "mit\u00b7ten", "in", "den", "Krieg", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Halt dich nah am Fahnentr\u00e4ger,", "tokens": ["Halt", "dich", "nah", "am", "Fah\u00b7nen\u00b7tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Denn die Mitte kommt nach Hause.", "tokens": ["Denn", "die", "Mit\u00b7te", "kommt", "nach", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bruder kam zur\u00fcck nach Hause", "tokens": ["Bru\u00b7der", "kam", "zu\u00b7r\u00fcck", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ging vor seines Vaters Th\u00fcr:", "tokens": ["Ging", "vor", "sei\u00b7nes", "Va\u00b7ters", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbvater, komm, erkenn den Sohn!\u00ab", "tokens": ["\u00bb", "va\u00b7ter", ",", "komm", ",", "er\u00b7kenn", "den", "Sohn", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater kam und kannt ihn nicht.", "tokens": ["Va\u00b7ter", "kam", "und", "kannt", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ging vor seiner Mutter Th\u00fcr:", "tokens": ["Ging", "vor", "sei\u00b7ner", "Mut\u00b7ter", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbmutter, komm, erkenn den Sohn!\u00ab", "tokens": ["\u00bb", "mut\u00b7ter", ",", "komm", ",", "er\u00b7kenn", "den", "Sohn", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mutter kam, erkannt ihn nicht.", "tokens": ["Mut\u00b7ter", "kam", ",", "er\u00b7kannt", "ihn", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ging vor seines Bruders Th\u00fcr:", "tokens": ["Ging", "vor", "sei\u00b7nes", "Bru\u00b7ders", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbbruder, komm, erkenn den Bruder!\u00ab", "tokens": ["\u00bb", "bru\u00b7der", ",", "komm", ",", "er\u00b7kenn", "den", "Bru\u00b7der", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bruder kam, erkannt ihn nicht.", "tokens": ["Bru\u00b7der", "kam", ",", "er\u00b7kannt", "ihn", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ging vor seiner Schwester Th\u00fcr;", "tokens": ["Ging", "vor", "sei\u00b7ner", "Schwes\u00b7ter", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbschwester, komm, erkenn den Bruder!\u00ab", "tokens": ["\u00bb", "schwes\u00b7ter", ",", "komm", ",", "er\u00b7kenn", "den", "Bru\u00b7der", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwester kam, erkannt den Bruder \u2013", "tokens": ["Schwes\u00b7ter", "kam", ",", "er\u00b7kannt", "den", "Bru\u00b7der", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Woran kannt ich meinen Bruder?", "tokens": ["Wo\u00b7ran", "kannt", "ich", "mei\u00b7nen", "Bru\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kannt ihn an den kurzen Kleidern,", "tokens": ["Kannt", "ihn", "an", "den", "kur\u00b7zen", "Klei\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kannt ihn an dem niedern Mantel,", "tokens": ["Kannt", "ihn", "an", "dem", "nie\u00b7dern", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bblieber Bruder, guter Bruder,", "tokens": ["\u00bb", "lie\u00b7ber", "Bru\u00b7der", ",", "gu\u00b7ter", "Bru\u00b7der", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sag', erz\u00e4hle mir vom Kriege!", "tokens": ["Sag'", ",", "er\u00b7z\u00e4h\u00b7le", "mir", "vom", "Krie\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprich, wie lebt man in dem Kriege?", "tokens": ["Sprich", ",", "wie", "lebt", "man", "in", "dem", "Krie\u00b7ge", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist im Kriege auch das Weib lieb?", "tokens": ["Ist", "im", "Krie\u00b7ge", "auch", "das", "Weib", "lieb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lieb das Weib, die Gattin theuer?\u00ab", "tokens": ["Lieb", "das", "Weib", ",", "die", "Gat\u00b7tin", "theu\u00b7er", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbliebe Schwester, kleine Schwester!", "tokens": ["\u00bb", "lie\u00b7be", "Schwes\u00b7ter", ",", "klei\u00b7ne", "Schwes\u00b7ter", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieh mir aus die staubgen Kleider,", "tokens": ["Zieh", "mir", "aus", "die", "staub\u00b7gen", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wisch mir ab den blutgen Degen,", "tokens": ["Wisch", "mir", "ab", "den", "blut\u00b7gen", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann erz\u00e4hl ich dir vom Kriege.", "tokens": ["Dann", "er\u00b7z\u00e4hl", "ich", "dir", "vom", "Krie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nein im Krieg ist nicht das Weib lieb,", "tokens": ["Nein", "im", "Krieg", "ist", "nicht", "das", "Weib", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPRART", "NN", "VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Nicht das Weib, die Gattin theuer!", "tokens": ["Nicht", "das", "Weib", ",", "die", "Gat\u00b7tin", "theu\u00b7er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieb im Krieg ist blanker Degen,", "tokens": ["Lieb", "im", "Krieg", "ist", "blan\u00b7ker", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieb im Krieg' ein wackres Pferd,", "tokens": ["Lieb", "im", "Krieg'", "ein", "wack\u00b7res", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das den Mann vom Kriege rettet.", "tokens": ["Das", "den", "Mann", "vom", "Krie\u00b7ge", "ret\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieb, zu wechseln Feindes Degen,", "tokens": ["Lieb", ",", "zu", "wech\u00b7seln", "Fein\u00b7des", "De\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKZU", "VVINF", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Gewehr aus Feindes Hand.\u00ab", "tokens": ["Das", "Ge\u00b7wehr", "aus", "Fein\u00b7des", "Hand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}