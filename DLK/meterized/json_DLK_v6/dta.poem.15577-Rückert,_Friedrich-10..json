{"dta.poem.15577": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie kannst du ungethan ein Fehlgethanes machen?", "tokens": ["Wie", "kannst", "du", "un\u00b7ge\u00b7than", "ein", "Fehl\u00b7ge\u00b7tha\u00b7nes", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist die wichtigste und schwierigste der Sachen.", "tokens": ["Das", "ist", "die", "wich\u00b7tigs\u00b7te", "und", "schwie\u00b7rigs\u00b7te", "der", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Wenn du dir sagen darfst, da\u00df, wenn du's wieder nun", "tokens": ["Wenn", "du", "dir", "sa\u00b7gen", "darfst", ",", "da\u00df", ",", "wenn", "du's", "wie\u00b7der", "nun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "KOUS", "$,", "KOUS", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thun k\u00f6nntest, du gewi\u00df es anders w\u00fcrdest thun;", "tokens": ["Thun", "k\u00f6nn\u00b7test", ",", "du", "ge\u00b7wi\u00df", "es", "an\u00b7ders", "w\u00fcr\u00b7dest", "thun", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PPER", "ADV", "PPER", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn so des Willens Kraft du hast daran gemessen,", "tokens": ["Wenn", "so", "des", "Wil\u00b7lens", "Kraft", "du", "hast", "da\u00b7ran", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann sei es abgethan, und, wenn du kannst, vergessen.", "tokens": ["Dann", "sei", "es", "ab\u00b7ge\u00b7than", ",", "und", ",", "wenn", "du", "kannst", ",", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "KON", "$,", "KOUS", "PPER", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}