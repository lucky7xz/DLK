{"textgrid.poem.36660": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drohend umstarren die spiegelnde Fluth apenninische Felsen,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drohend umstarren die spiegelnde Fluth apenninische Felsen,", "tokens": ["Dro\u00b7hend", "um\u00b7star\u00b7ren", "die", "spie\u00b7geln\u00b7de", "Fluth", "a\u00b7pen\u00b7ni\u00b7ni\u00b7sche", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Und acherontisches Grau'n schattet ins Wasser herab.", "tokens": ["Und", "ac\u00b7her\u00b7on\u00b7ti\u00b7sches", "Grau'n", "schat\u00b7tet", "ins", "Was\u00b7ser", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch Avezzano, es lockt mich zum Strand, ich schweb' auf dem Lethe,", "tokens": ["Doch", "A\u00b7vez\u00b7za\u00b7no", ",", "es", "lockt", "mich", "zum", "Strand", ",", "ich", "schweb'", "auf", "dem", "Le\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und das lieblichste Kind ladet zum Mahle mich ein.", "tokens": ["Und", "das", "lieb\u00b7lichs\u00b7te", "Kind", "la\u00b7det", "zum", "Mah\u00b7le", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}