{"textgrid.poem.28792": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutschen sind so alte Leute,", "tokens": ["Deut\u00b7schen", "sind", "so", "al\u00b7te", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernen doch erst reden heute.", "tokens": ["Ler\u00b7nen", "doch", "erst", "re\u00b7den", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVFIN", "ADV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wann sie lernen doch auch wolten,", "tokens": ["Wann", "sie", "ler\u00b7nen", "doch", "auch", "wol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie recht deutsch sie handeln solten!", "tokens": ["Wie", "recht", "deutsch", "sie", "han\u00b7deln", "sol\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}