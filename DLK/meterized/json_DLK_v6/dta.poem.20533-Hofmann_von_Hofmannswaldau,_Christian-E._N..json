{"dta.poem.20533": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich soll mich mit gewalt verlieben/", "tokens": ["Ich", "soll", "mich", "mit", "ge\u00b7walt", "ver\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir ist zur inclination", "tokens": ["Mir", "ist", "zur", "in\u00b7cli\u00b7na\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein artig m\u00e4dgen vorgeschrieben;", "tokens": ["Ein", "ar\u00b7tig", "m\u00e4d\u00b7gen", "vor\u00b7ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wiewohl ich bleibe noch davon/", "tokens": ["Wie\u00b7wohl", "ich", "blei\u00b7be", "noch", "da\u00b7von", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ich das marck der besten jahre", "tokens": ["Weil", "ich", "das", "marck", "der", "bes\u00b7ten", "jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor mich und gute freunde spare.", "tokens": ["Vor", "mich", "und", "gu\u00b7te", "freun\u00b7de", "spa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich habe zwar nichts auszusetzen/", "tokens": ["Ich", "ha\u00b7be", "zwar", "nichts", "aus\u00b7zu\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das m\u00e4dgen ist wohl liebens werth.", "tokens": ["Das", "m\u00e4d\u00b7gen", "ist", "wohl", "lie\u00b7bens", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und diesen will ich gl\u00fccklich sch\u00e4tzen/", "tokens": ["Und", "die\u00b7sen", "will", "ich", "gl\u00fcck\u00b7lich", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den sie zum courtisan begehrt.", "tokens": ["Den", "sie", "zum", "cour\u00b7ti\u00b7san", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur ich will mich des gl\u00fccks begeben/", "tokens": ["Nur", "ich", "will", "mich", "des", "gl\u00fccks", "be\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PRF", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und in beliebter freyheit leben.", "tokens": ["Und", "in", "be\u00b7lieb\u00b7ter", "frey\u00b7heit", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie wei\u00df sich propre auffzuf\u00fchren;", "tokens": ["Sie", "wei\u00df", "sich", "prop\u00b7re", "auff\u00b7zu\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Manch frauen-zimmer l\u00e4st auch drum", "tokens": ["Manch", "frau\u00b7en\u00b7zim\u00b7mer", "l\u00e4st", "auch", "drum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein krummes maul zum possen sp\u00fcren/", "tokens": ["Ein", "krum\u00b7mes", "maul", "zum", "pos\u00b7sen", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch das verdoppelt ihren ruhm.", "tokens": ["Doch", "das", "ver\u00b7dop\u00b7pelt", "ih\u00b7ren", "ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die armen s\u00fcnder m\u00fcssen passen/", "tokens": ["Die", "ar\u00b7men", "s\u00fcn\u00b7der", "m\u00fcs\u00b7sen", "pas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihr das pr\u00e4 in allen lassen.", "tokens": ["Und", "ihr", "das", "pr\u00e4", "in", "al\u00b7len", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDS", "VVFIN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wem sind die wunder-sch\u00f6nen blicke", "tokens": ["Wem", "sind", "die", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihre minen nicht bekandt?", "tokens": ["Und", "ih\u00b7re", "mi\u00b7nen", "nicht", "be\u00b7kandt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es fehlt ihr nichts in keinem st\u00fccke/", "tokens": ["Es", "fehlt", "ihr", "nichts", "in", "kei\u00b7nem", "st\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was sie nur hat/ das ist galant.", "tokens": ["Was", "sie", "nur", "hat", "/", "das", "ist", "ga\u00b7lant", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$(", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der wird erst sch\u00f6ne sachen wissen/", "tokens": ["Der", "wird", "erst", "sch\u00f6\u00b7ne", "sa\u00b7chen", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der sie mit appetit darff k\u00fcssen.", "tokens": ["Der", "sie", "mit", "ap\u00b7pe\u00b7tit", "darff", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir steht die th\u00fcr vor andern offen.", "tokens": ["Mir", "steht", "die", "th\u00fcr", "vor", "an\u00b7dern", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch weil mich vor die courtoisie", "tokens": ["Doch", "weil", "mich", "vor", "die", "cour\u00b7to\u00b7i\u00b7sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Mein gl\u00fccke l\u00e4st was bessers hoffen/", "tokens": ["Mein", "gl\u00fc\u00b7cke", "l\u00e4st", "was", "bes\u00b7sers", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PWS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So geb ich mir auch keine m\u00fch/", "tokens": ["So", "geb", "ich", "mir", "auch", "kei\u00b7ne", "m\u00fch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PIAT", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und habe den termin im lieben", "tokens": ["Und", "ha\u00b7be", "den", "ter\u00b7min", "im", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NE", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auff lange zeit noch abgeschrieben.", "tokens": ["Auff", "lan\u00b7ge", "zeit", "noch", "ab\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich will bey guten freunden bleiben/", "tokens": ["Ich", "will", "bey", "gu\u00b7ten", "freun\u00b7den", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sollen manchen lieben tag", "tokens": ["Die", "sol\u00b7len", "man\u00b7chen", "lie\u00b7ben", "tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die lange zeit gew\u00fcnscht vertreiben.", "tokens": ["Die", "lan\u00b7ge", "zeit", "ge\u00b7w\u00fcnscht", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und weil Coffee und auch toback", "tokens": ["Und", "weil", "Cof\u00b7fee", "und", "auch", "to\u00b7back"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "KON", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die angenehme lust vermehren/", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "lust", "ver\u00b7meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Will ich die liebe noch verschweren.", "tokens": ["Will", "ich", "die", "lie\u00b7be", "noch", "ver\u00b7schwe\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn ja mein hertz an solchen dingen/", "tokens": ["Wenn", "ja", "mein", "hertz", "an", "sol\u00b7chen", "din\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich endlich noch verpl\u00e4mpern soll/", "tokens": ["Sich", "end\u00b7lich", "noch", "ver\u00b7pl\u00e4m\u00b7pern", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df ich mich durchaus nicht zwingen/", "tokens": ["So", "la\u00df", "ich", "mich", "durc\u00b7haus", "nicht", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gezwungne speise schmeckt nicht wohl.", "tokens": ["Ge\u00b7zwung\u00b7ne", "spei\u00b7se", "schmeckt", "nicht", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich mu\u00df die wahl und freyheit haben/", "tokens": ["Ich", "mu\u00df", "die", "wahl", "und", "frey\u00b7heit", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ich mich soll nach wunsche laben.", "tokens": ["Wenn", "ich", "mich", "soll", "nach", "wun\u00b7sche", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und also last mich ", "tokens": ["Und", "al\u00b7so", "last", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mit einer inelination/", "tokens": ["Mit", "ei\u00b7ner", "i\u00b7ne\u00b7li\u00b7na\u00b7ti\u00b7on", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sonst geht mir alle lust verlohren.", "tokens": ["Sonst", "geht", "mir", "al\u00b7le", "lust", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00dfwegen bleib ich auch davon/", "tokens": ["De\u00df\u00b7we\u00b7gen", "bleib", "ich", "auch", "da\u00b7von", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mag mich nicht damit vermengen/", "tokens": ["Und", "mag", "mich", "nicht", "da\u00b7mit", "ver\u00b7men\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch vor der zeit die fl\u00fcgel hengen.", "tokens": ["Noch", "vor", "der", "zeit", "die", "fl\u00fc\u00b7gel", "hen\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}