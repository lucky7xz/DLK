{"textgrid.poem.55754": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Herzen w\u00fchlt und lodert", "tokens": ["Im", "Her\u00b7zen", "w\u00fchlt", "und", "lo\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die w\u00fcsteste, tollste Begier", "tokens": ["Die", "w\u00fcs\u00b7tes\u00b7te", ",", "tolls\u00b7te", "Be\u00b7gier"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und rei\u00dft und st\u00f6\u00dft und peitscht mich,", "tokens": ["Und", "rei\u00dft", "und", "st\u00f6\u00dft", "und", "peitscht", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Madonna Lucia, zu Dir.", "tokens": ["Ma\u00b7don\u00b7na", "Lu\u00b7cia", ",", "zu", "Dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Glieder schauern und beben,", "tokens": ["Die", "Glie\u00b7der", "schau\u00b7ern", "und", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das Auge Flammen spr\u00fcht,", "tokens": ["Das", "Au\u00b7ge", "Flam\u00b7men", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie kochende Lavastr\u00f6me", "tokens": ["Wie", "ko\u00b7chen\u00b7de", "La\u00b7vas\u00b7tr\u00f6\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Das Blut meine Adern durchgl\u00fcht.", "tokens": ["Das", "Blut", "mei\u00b7ne", "A\u00b7dern", "durch\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Ich flehe Dich an, o gebrauche", "tokens": ["Ich", "fle\u00b7he", "Dich", "an", ",", "o", "ge\u00b7brau\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "FM", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Die g\u00f6ttlich d\u00e4monische Macht,", "tokens": ["Die", "g\u00f6tt\u00b7lich", "d\u00e4\u00b7mo\u00b7ni\u00b7sche", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die meine zerfaserten Nerven", "tokens": ["Die", "mei\u00b7ne", "zer\u00b7fa\u00b7ser\u00b7ten", "Ner\u00b7ven"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Zum rasendsten Taumel entfacht.", "tokens": ["Zum", "ra\u00b7sends\u00b7ten", "Tau\u00b7mel", "ent\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Und wenn an Deinem Busen", "tokens": ["Und", "wenn", "an", "Dei\u00b7nem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Zum Wahnwitz schwillt die Lust,", "tokens": ["Zum", "Wahn\u00b7witz", "schwillt", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Dann, \u00fcppigste, geilste der Schlangen,", "tokens": ["Dann", ",", "\u00fcp\u00b7pigs\u00b7te", ",", "geils\u00b7te", "der", "Schlan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "$,", "ADJA", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Erw\u00fcrg' mich an Deiner Brust.", "tokens": ["Er\u00b7w\u00fcr\u00b7g'", "mich", "an", "Dei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}