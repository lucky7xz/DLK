{"dta.poem.21776": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Je sch\u00f6ner/ je h\u00e4rter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Die Anmuht/ Sch\u00f6nheit/ Zierd und\nPrangen/", "tokens": ["Die", "An\u00b7muht", "/", "Sch\u00f6n\u00b7heit", "/", "Zierd", "und", "Pran\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "das Purpur-blut der roten Wangen/", "tokens": ["das", "Pur\u00b7pur\u00b7blut", "der", "ro\u00b7ten", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der Augen-blizz/ der Stirne Glanz/", "tokens": ["der", "Au\u00b7gen\u00b7blizz", "/", "der", "Stir\u00b7ne", "Glanz", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das Spiel der ziehenden Gebehrden/", "tokens": ["das", "Spiel", "der", "zie\u00b7hen\u00b7den", "Ge\u00b7behr\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "der Gang/ die Tracht sind himlisch ganz", "tokens": ["der", "Gang", "/", "die", "Tracht", "sind", "him\u00b7lisch", "ganz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "VAFIN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und k\u00f6nnen nicht versch\u00f6nert werden.", "tokens": ["und", "k\u00f6n\u00b7nen", "nicht", "ver\u00b7sch\u00f6\u00b7nert", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So lieblich sahe nie Dione", "tokens": ["So", "lieb\u00b7lich", "sa\u00b7he", "nie", "Dio\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wenn sie auff dem verg\u00f6ldtem Trohne", "tokens": ["wenn", "sie", "auff", "dem", "ver\u00b7g\u00f6ld\u00b7tem", "Troh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "in Pasos Tempel Ehr\u2019 empfieng!", "tokens": ["in", "Pa\u00b7sos", "Tem\u00b7pel", "Ehr'", "emp\u00b7fi\u00b7eng", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "VVFIN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Betracht\u2019 ich dein bes\u00fc\u00dftes Wesen/", "tokens": ["Be\u00b7tracht'", "ich", "dein", "be\u00b7s\u00fc\u00df\u00b7tes", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "was ich von Helenen gelesen.", "tokens": ["was", "ich", "von", "He\u00b7le\u00b7nen", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NE", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "So kanstu die Vollkommenheiten", "tokens": ["So", "kans\u00b7tu", "die", "Voll\u00b7kom\u00b7men\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der Sch\u00f6nheit/ Sch\u00f6ne/ selbst bestreiten/", "tokens": ["der", "Sch\u00f6n\u00b7heit", "/", "Sch\u00f6\u00b7ne", "/", "selbst", "be\u00b7strei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "du ziehest aller Herzen an.", "tokens": ["du", "zie\u00b7hest", "al\u00b7ler", "Her\u00b7zen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer dich beschauet sonder brennen", "tokens": ["Wer", "dich", "be\u00b7schau\u00b7et", "son\u00b7der", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und Liebes-gluht/ denselben kan", "tokens": ["und", "Lie\u00b7bes\u00b7gluht", "/", "den\u00b7sel\u00b7ben", "kan"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$(", "PDS", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "man einen Stein/ nicht Menschen/ nennen.", "tokens": ["man", "ei\u00b7nen", "Stein", "/", "nicht", "Men\u00b7schen", "/", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "ART", "NN", "$(", "PTKNEG", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie heuffig aber dich mit Gaben", "tokens": ["Wie", "heuf\u00b7fig", "a\u00b7ber", "dich", "mit", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "vor andern die Gebuhrt erhaben:", "tokens": ["vor", "an\u00b7dern", "die", "Ge\u00b7buhrt", "er\u00b7ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit-Leiden/ Trost/ und ein Gem\u00fchte/", "tokens": ["Mit\u00b7Lei\u00b7den", "/", "Trost", "/", "und", "ein", "Ge\u00b7m\u00fch\u00b7te", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df eine wunde Seele heilt", "tokens": ["da\u00df", "ei\u00b7ne", "wun\u00b7de", "See\u00b7le", "heilt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "durch Freundes Zuspruch/ Gunst und G\u00fcte.", "tokens": ["durch", "Freun\u00b7des", "Zu\u00b7spruch", "/", "Gunst", "und", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der scharffe Fels der Diamanten", "tokens": ["Der", "scharf\u00b7fe", "Fels", "der", "Di\u00b7a\u00b7man\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "reicht seines Leibes rauhe Kanten", "tokens": ["reicht", "sei\u00b7nes", "Lei\u00b7bes", "rau\u00b7he", "Kan\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "des K\u00fcssers Lippen willig dar.", "tokens": ["des", "K\u00fcs\u00b7sers", "Lip\u00b7pen", "wil\u00b7lig", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Rose von dem warmen Westen", "tokens": ["Die", "Ro\u00b7se", "von", "dem", "war\u00b7men", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "getrieben/ b\u00fckkt sich mit Gefahr", "tokens": ["ge\u00b7trie\u00b7ben", "/", "b\u00fckkt", "sich", "mit", "Ge\u00b7fahr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "zu ihres Dornes wilden \u00e4sten.", "tokens": ["zu", "ih\u00b7res", "Dor\u00b7nes", "wil\u00b7den", "\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du/ harte/ l\u00e4st dich nicht erweichen/", "tokens": ["Du", "/", "har\u00b7te", "/", "l\u00e4st", "dich", "nicht", "er\u00b7wei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "VVFIN", "$(", "VVFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die minste Gegen-gunst zureichen", "tokens": ["die", "mins\u00b7te", "Ge\u00b7gen\u00b7gunst", "zu\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dehm der in deinen Flammen queelt.", "tokens": ["dehm", "der", "in", "dei\u00b7nen", "Flam\u00b7men", "que\u00b7elt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wer dich erblikkt/ ist ohne Leben/", "tokens": ["Wer", "dich", "er\u00b7blikkt", "/", "ist", "oh\u00b7ne", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ist sonder Geist und wird entseelt/", "tokens": ["ist", "son\u00b7der", "Geist", "und", "wird", "ent\u00b7seelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und du willst ihm kein Mittel geben.", "tokens": ["und", "du", "willst", "ihm", "kein", "Mit\u00b7tel", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "So meinstu/ du seyst dir gebohren/", "tokens": ["So", "meins\u00b7tu", "/", "du", "seyst", "dir", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "warum wir auff der Erde seyn.", "tokens": ["wa\u00b7rum", "wir", "auff", "der", "Er\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Bild wird darum wol gemahlet", "tokens": ["Kein", "Bild", "wird", "da\u00b7rum", "wol", "ge\u00b7mah\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PAV", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df man es birget in den Schrein", "tokens": ["da\u00df", "man", "es", "bir\u00b7get", "in", "den", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Indehm man dich/ wie G\u00f6ttlich preiset/", "tokens": ["In\u00b7dehm", "man", "dich", "/", "wie", "G\u00f6tt\u00b7lich", "prei\u00b7set", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "$(", "PWAV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Pflicht/ Ehr\u2019 und Demuht dir erweiset:", "tokens": ["Pflicht", "/", "Ehr'", "und", "De\u00b7muht", "dir", "er\u00b7wei\u00b7set", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sey/ Sch\u00f6ne/ drum nicht eben stolz.", "tokens": ["Sey", "/", "Sch\u00f6\u00b7ne", "/", "drum", "nicht", "e\u00b7ben", "stolz", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "NN", "$(", "PAV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Knie so f\u00fcr Alt\u00e4ren liegen", "tokens": ["Die", "Knie", "so", "f\u00fcr", "Al\u00b7t\u00e4\u00b7ren", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "pflegt man nicht f\u00fcr ein faules Holz", "tokens": ["pflegt", "man", "nicht", "f\u00fcr", "ein", "fau\u00b7les", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "f\u00fcr G\u00f6tter Freundschafft nur zu biegen.", "tokens": ["f\u00fcr", "G\u00f6t\u00b7ter", "Freund\u00b7schafft", "nur", "zu", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Grausamkeit und s\u00fcsses lachen", "tokens": ["Die", "Grau\u00b7sam\u00b7keit", "und", "s\u00fcs\u00b7ses", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie k\u00f6nnen die Verwandn\u00fcs machen", "tokens": ["wie", "k\u00f6n\u00b7nen", "die", "Ver\u00b7wand\u00b7n\u00fcs", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "in einem sch\u00f6nem Angesicht\u2019?", "tokens": ["in", "ei\u00b7nem", "sch\u00f6\u00b7nem", "An\u00b7ge\u00b7sicht'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entwehn dich/ Kind/ der Ernst-gebehrden/", "tokens": ["Ent\u00b7wehn", "dich", "/", "Kind", "/", "der", "Ernst\u00b7ge\u00b7behr\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "noch tausendfach versch\u00f6nert werden.", "tokens": ["noch", "tau\u00b7send\u00b7fach", "ver\u00b7sch\u00f6\u00b7nert", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}