{"dta.poem.5835": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann ein zerbrechlicher und feuchter C\u00f6rper biegt", "tokens": ["Wann", "ein", "zer\u00b7brech\u00b7li\u00b7cher", "und", "feuch\u00b7ter", "C\u00f6r\u00b7per", "biegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Fahrt desjenigen, der durch ihn abwerts fliegt;", "tokens": ["Die", "Fahrt", "des\u00b7je\u00b7ni\u00b7gen", ",", "der", "durch", "ihn", "ab\u00b7werts", "fliegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist der Gang, der abweicht, was man nennt,", "tokens": ["So", "ist", "der", "Gang", ",", "der", "ab\u00b7weicht", ",", "was", "man", "nennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den R\u00fcck-Schlag, und es ist der Winckel, der sich trennt,", "tokens": ["Den", "R\u00fc\u00b7ck\u00b7Schlag", ",", "und", "es", "ist", "der", "Win\u00b7ckel", ",", "der", "sich", "trennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Gro\u00df oder kleiner offenbar,", "tokens": ["Gro\u00df", "o\u00b7der", "klei\u00b7ner", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nachdem der Weg leicht oder schwerer war.", "tokens": ["Nach\u00b7dem", "der", "Weg", "leicht", "o\u00b7der", "schwe\u00b7rer", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}