{"textgrid.poem.32996": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sprich noch nicht vom Fr\u00fchling, es ist zu fr\u00fch!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprich noch nicht vom Fr\u00fchling, es ist zu fr\u00fch!", "tokens": ["Sprich", "noch", "nicht", "vom", "Fr\u00fch\u00b7ling", ",", "es", "ist", "zu", "fr\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKNEG", "APPRART", "NN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "so lockend die Sonne vom Himmel blitzt,", "tokens": ["so", "lo\u00b7ckend", "die", "Son\u00b7ne", "vom", "Him\u00b7mel", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "so lockend alles gl\u00e4nzt und glitzt ...", "tokens": ["so", "lo\u00b7ckend", "al\u00b7les", "gl\u00e4nzt", "und", "glitzt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sprich noch nicht vom Fr\u00fchling, es ist zu fr\u00fch!", "tokens": ["sprich", "noch", "nicht", "vom", "Fr\u00fch\u00b7ling", ",", "es", "ist", "zu", "fr\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "APPRART", "NN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Es werden Tage wieder kommen", "tokens": ["Es", "wer\u00b7den", "Ta\u00b7ge", "wie\u00b7der", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bevor erbl\u00fcht, wovon du tr\u00e4umst,", "tokens": ["be\u00b7vor", "er\u00b7bl\u00fcht", ",", "wo\u00b7von", "du", "tr\u00e4umst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "da alles wie vorher trostlos weh", "tokens": ["da", "al\u00b7les", "wie", "vor\u00b7her", "trost\u00b7los", "weh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "KOKOM", "ADV", "ADJD", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "in Regen sich begr\u00e4bt und Schnee,", "tokens": ["in", "Re\u00b7gen", "sich", "be\u00b7gr\u00e4bt", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Tage voll Traurigkeit, Tage voll M\u00fch ...", "tokens": ["Ta\u00b7ge", "voll", "Trau\u00b7rig\u00b7keit", ",", "Ta\u00b7ge", "voll", "M\u00fch", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "NN", "ADJD", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "sprich noch nicht vom Fr\u00fchling, es ist zu fr\u00fch!", "tokens": ["sprich", "noch", "nicht", "vom", "Fr\u00fch\u00b7ling", ",", "es", "ist", "zu", "fr\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "APPRART", "NN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und doch und dennoch: mit jubelndem Liede", "tokens": ["Und", "doch", "und", "den\u00b7noch", ":", "mit", "ju\u00b7beln\u00b7dem", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "gr\u00fc\u00dfe dies frohe befreiende Blau", "tokens": ["gr\u00fc\u00b7\u00dfe", "dies", "fro\u00b7he", "be\u00b7frei\u00b7en\u00b7de", "Blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "ADJA", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "\u00fcber all dem farblosen Grau,", "tokens": ["\u00fc\u00b7ber", "all", "dem", "farb\u00b7lo\u00b7sen", "Grau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "freu dich der flimmernden Mittagsstunden,", "tokens": ["freu", "dich", "der", "flim\u00b7mern\u00b7den", "Mit\u00b7tags\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "sonne das Herz dir zu keimender Kraft,", "tokens": ["son\u00b7ne", "das", "Herz", "dir", "zu", "kei\u00b7men\u00b7der", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "da\u00df es dem m\u00fcde machenden Winter", "tokens": ["da\u00df", "es", "dem", "m\u00fc\u00b7de", "ma\u00b7chen\u00b7den", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und seiner Entt\u00e4uschung sich wieder entrafft!", "tokens": ["und", "sei\u00b7ner", "Ent\u00b7t\u00e4u\u00b7schung", "sich", "wie\u00b7der", "en\u00b7trafft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Nur warte, nur wart noch! es wird sich erf\u00fcllen,", "tokens": ["Nur", "war\u00b7te", ",", "nur", "wart", "noch", "!", "es", "wird", "sich", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "PRF", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "es wird sich erf\u00fcllen, was du ersehnst:", "tokens": ["es", "wird", "sich", "er\u00b7f\u00fcl\u00b7len", ",", "was", "du", "er\u00b7sehnst", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Glutig auflodern wird es am Himmel,", "tokens": ["Glu\u00b7tig", "auf\u00b7lo\u00b7dern", "wird", "es", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "\u00fcber die Berge her wird es wehn", "tokens": ["\u00fc\u00b7ber", "die", "Ber\u00b7ge", "her", "wird", "es", "wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APZR", "VAFIN", "PPER", "VVINF"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "und wie donnernde Osterglocken", "tokens": ["und", "wie", "don\u00b7nern\u00b7de", "Os\u00b7ter\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "wird es durch die Lande gehn ...", "tokens": ["wird", "es", "durch", "die", "Lan\u00b7de", "gehn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "nur warte, nur wart noch und hab Geduld!", "tokens": ["nur", "war\u00b7te", ",", "nur", "wart", "noch", "und", "hab", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "KON", "VAFIN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "So sch\u00f6n und so k\u00f6stlich dies blitzende Blau", "tokens": ["So", "sch\u00f6n", "und", "so", "k\u00f6st\u00b7lich", "dies", "blit\u00b7zen\u00b7de", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "PDS", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "mit seinem s\u00fc\u00dfen stillen Locken,", "tokens": ["mit", "sei\u00b7nem", "s\u00fc\u00b7\u00dfen", "stil\u00b7len", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "es kommen Tage noch und Wochen", "tokens": ["es", "kom\u00b7men", "Ta\u00b7ge", "noch", "und", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "farblos grau,", "tokens": ["farb\u00b7los", "grau", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "da alles wie vorher trostlos weh", "tokens": ["da", "al\u00b7les", "wie", "vor\u00b7her", "trost\u00b7los", "weh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "KOKOM", "ADV", "ADJD", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "in Regen sich begr\u00e4bt und Schnee,", "tokens": ["in", "Re\u00b7gen", "sich", "be\u00b7gr\u00e4bt", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Tage voll Traurigkeit, Tage voll M\u00fch ...", "tokens": ["Ta\u00b7ge", "voll", "Trau\u00b7rig\u00b7keit", ",", "Ta\u00b7ge", "voll", "M\u00fch", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "NN", "ADJD", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.15": {"text": "sprich noch nicht vom Fr\u00fchling, es ist zu fr\u00fch!", "tokens": ["sprich", "noch", "nicht", "vom", "Fr\u00fch\u00b7ling", ",", "es", "ist", "zu", "fr\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "APPRART", "NN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}