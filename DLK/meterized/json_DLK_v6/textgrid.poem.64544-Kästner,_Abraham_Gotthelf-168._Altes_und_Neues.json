{"textgrid.poem.64544": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "168. Altes und Neues", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bardenton, Knittelvers, Minneklingklang,", "tokens": ["Bar\u00b7den\u00b7ton", ",", "Knit\u00b7tel\u00b7vers", ",", "Min\u00b7ne\u00b7kling\u00b7klang", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Both'ng'stamm'l, Mordgeschicht, Hexengesang,", "tokens": ["Bo\u00b7th'\u00b7ng'\u00b7stam\u00b7m'l", ",", "Mord\u00b7ge\u00b7schicht", ",", "He\u00b7xen\u00b7ge\u00b7sang", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Hat man in jetzigen Zeiten so gern:", "tokens": ["Hat", "man", "in", "jet\u00b7zi\u00b7gen", "Zei\u00b7ten", "so", "gern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Bibel und Glauben verlangt man modern.", "tokens": ["Bi\u00b7bel", "und", "Glau\u00b7ben", "ver\u00b7langt", "man", "mo\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}