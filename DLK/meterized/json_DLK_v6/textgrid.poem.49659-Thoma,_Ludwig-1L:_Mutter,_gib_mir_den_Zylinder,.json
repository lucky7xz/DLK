{"textgrid.poem.49659": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mutter, gib mir den Zylinder,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter, gib mir den Zylinder,", "tokens": ["Mut\u00b7ter", ",", "gib", "mir", "den", "Zy\u00b7lin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Schmier mir eine Butterbemme,", "tokens": ["Schmier", "mir", "ei\u00b7ne", "But\u00b7ter\u00b7bem\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Linden f\u00e4hrt ein wilder", "tokens": ["Durch", "die", "Lin\u00b7den", "f\u00e4hrt", "ein", "wil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4uptling der Aschantist\u00e4mme.", "tokens": ["H\u00e4upt\u00b7ling", "der", "A\u00b7schan\u00b7ti\u00b7st\u00e4m\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und den schwarzen Negerfritze", "tokens": ["Und", "den", "schwar\u00b7zen", "Ne\u00b7ger\u00b7frit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird der ganze Hof begleiten,", "tokens": ["Wird", "der", "gan\u00b7ze", "Hof", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch zwei K\u00fcrassierschwadronen", "tokens": ["Auch", "zwei", "K\u00fc\u00b7ras\u00b7sier\u00b7schwad\u00b7ro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "CARD", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Werden vorn und hinten reiten.", "tokens": ["Wer\u00b7den", "vorn", "und", "hin\u00b7ten", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so war's. Durchs Brandenburger", "tokens": ["Und", "so", "wa\u00b7r'", "s.", "Durchs", "Bran\u00b7den\u00b7bur\u00b7ger"], "token_info": ["word", "word", "word", "abbreviation", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rasselnd kam die Kavalkade.", "tokens": ["Ras\u00b7selnd", "kam", "die", "Ka\u00b7val\u00b7ka\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Helme funkeln, S\u00e4bel blitzen,", "tokens": ["Hel\u00b7me", "fun\u00b7keln", ",", "S\u00e4\u00b7bel", "blit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es riecht die Bartpomade.", "tokens": ["Und", "es", "riecht", "die", "Bart\u00b7po\u00b7ma\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Links und rechts ein Hurrarufen,", "tokens": ["Links", "und", "rechts", "ein", "Hur\u00b7ra\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumenwerfen, T\u00fccherschwenken,", "tokens": ["Blu\u00b7men\u00b7wer\u00b7fen", ",", "T\u00fc\u00b7cher\u00b7schwen\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der H\u00e4uptling bleckt die Z\u00e4hne", "tokens": ["Und", "der", "H\u00e4upt\u00b7ling", "bleckt", "die", "Z\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Allen, die ihm Beifall schenken.", "tokens": ["Al\u00b7len", ",", "die", "ihm", "Bei\u00b7fall", "schen\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wirklich, der Aschantineger", "tokens": ["Wirk\u00b7lich", ",", "der", "A\u00b7schan\u00b7ti\u00b7ne\u00b7ger"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ART", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "War darob sehr guter Dinge,", "tokens": ["War", "da\u00b7rob", "sehr", "gu\u00b7ter", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ADV", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Alle kommandierten Herren", "tokens": ["Al\u00b7le", "kom\u00b7man\u00b7dier\u00b7ten", "Her\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kriegten goldne Nasenringe.", "tokens": ["Krieg\u00b7ten", "gold\u00b7ne", "Na\u00b7sen\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auch bei Hof war man zufrieden,", "tokens": ["Auch", "bei", "Hof", "war", "man", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VAFIN", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df das Volk sich so benommen.", "tokens": ["Da\u00df", "das", "Volk", "sich", "so", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald l\u00e4\u00dft man f\u00fcr die Berliner", "tokens": ["Bald", "l\u00e4\u00dft", "man", "f\u00fcr", "die", "Ber\u00b7li\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Wieder einen F\u00fcrsten kommen.", "tokens": ["Wie\u00b7der", "ei\u00b7nen", "F\u00fcrs\u00b7ten", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}