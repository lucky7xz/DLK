{"textgrid.poem.36914": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf, die Trompeten geblasen,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, die Trompeten geblasen,", "tokens": ["Auf", ",", "die", "Trom\u00b7pe\u00b7ten", "ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Deutschland, Deutschland ist frei!", "tokens": ["Deutschland", ",", "Deutschland", "ist", "frei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Friedrich Wilhelm hat's gesprochen:", "tokens": ["Fried\u00b7rich", "Wil\u00b7helm", "hat's", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was die V\u00f6lker hofften, es sei!", "tokens": ["Was", "die", "V\u00f6l\u00b7ker", "hoff\u00b7ten", ",", "es", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der K\u00f6nig hat es gesprochen;", "tokens": ["Der", "K\u00f6\u00b7nig", "hat", "es", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nun jubelt's durch's ganze Reich;", "tokens": ["Nun", "ju\u00b7belt's", "durch's", "gan\u00b7ze", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nun schm\u00fccken sich Th\u00fcr und Fenster", "tokens": ["Nun", "schm\u00fc\u00b7cken", "sich", "Th\u00fcr", "und", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Mit Blum' und gr\u00fcnem Zweig.", "tokens": ["Mit", "Blum'", "und", "gr\u00fc\u00b7nem", "Zweig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf, auf, die Glocken gezogen!", "tokens": ["Auf", ",", "auf", ",", "die", "Glo\u00b7cken", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf's Knie zum br\u00fcnst'gen Gebet!", "tokens": ["Auf's", "Knie", "zum", "br\u00fcnst'\u00b7gen", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott hat sein Herz uns gelenket,", "tokens": ["Gott", "hat", "sein", "Herz", "uns", "ge\u00b7len\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Erh\u00f6rt ist, was wir gefleht!", "tokens": ["Er\u00b7h\u00f6rt", "ist", ",", "was", "wir", "ge\u00b7fleht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nun ist ein Singen und Klingen", "tokens": ["Nun", "ist", "ein", "Sin\u00b7gen", "und", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Durch's freie Vaterland;", "tokens": ["Durch's", "frei\u00b7e", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es reichen sich alle Feinde", "tokens": ["Es", "rei\u00b7chen", "sich", "al\u00b7le", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Vers\u00f6hnt die Bruderhand.", "tokens": ["Ver\u00b7s\u00f6hnt", "die", "Bru\u00b7der\u00b7hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Lustfeuer auf den Bergen", "tokens": ["Lust\u00b7feu\u00b7er", "auf", "den", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verscheuchen die schwarze Nacht,", "tokens": ["Ver\u00b7scheu\u00b7chen", "die", "schwar\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis der erste Freiheitsmorgen", "tokens": ["Bis", "der", "ers\u00b7te", "Frei\u00b7heits\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erscheint in goldener Pracht.", "tokens": ["Er\u00b7scheint", "in", "gol\u00b7de\u00b7ner", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Purpur'ne Wolken verk\u00fcnden's:", "tokens": ["Pur\u00b7pur'\u00b7ne", "Wol\u00b7ken", "ver\u00b7k\u00fcn\u00b7den's", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Der Langersehnte ist da!", "tokens": ["Der", "Lan\u00b7ger\u00b7sehn\u00b7te", "ist", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und wie ein jubelnder Donner", "tokens": ["Und", "wie", "ein", "ju\u00b7beln\u00b7der", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Begr\u00fc\u00dft ihn der V\u00f6lker Hurrah.", "tokens": ["Be\u00b7gr\u00fc\u00dft", "ihn", "der", "V\u00f6l\u00b7ker", "Hur\u00b7rah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Die M\u00e4dchen zieren mit Blumen", "tokens": ["Die", "M\u00e4d\u00b7chen", "zie\u00b7ren", "mit", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der M\u00e4nner Pflug und Schwert;", "tokens": ["Der", "M\u00e4n\u00b7ner", "Pflug", "und", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es werden wie h\u00f6here Wesen", "tokens": ["Es", "wer\u00b7den", "wie", "h\u00f6\u00b7he\u00b7re", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Freiheitss\u00e4nger verehrt;", "tokens": ["Die", "Frei\u00b7heits\u00b7s\u00e4n\u00b7ger", "ver\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dann braust es millionenstimmig:", "tokens": ["Dann", "braust", "es", "mil\u00b7li\u00b7o\u00b7nen\u00b7stim\u00b7mig", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Unser Gram, er ist vorbei!", "tokens": ["Un\u00b7ser", "Gram", ",", "er", "ist", "vor\u00b7bei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Erf\u00fcllt ist, was uns versprochen!", "tokens": ["Er\u00b7f\u00fcllt", "ist", ",", "was", "uns", "ver\u00b7spro\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Deutschland, Deutschland ist frei!", "tokens": ["Deutschland", ",", "Deutschland", "ist", "frei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und wie ein Gott der Erde,", "tokens": ["Und", "wie", "ein", "Gott", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steht Friedrich Wilhelm da,", "tokens": ["Steht", "Fried\u00b7rich", "Wil\u00b7helm", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch so viel Gl\u00fcck und Liebe", "tokens": ["Noch", "so", "viel", "Gl\u00fcck", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schuf keines Menschen Ja!", "tokens": ["Schuf", "kei\u00b7nes", "Men\u00b7schen", "Ja", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie feiern ihn mit Thr\u00e4nen", "tokens": ["Sie", "fei\u00b7ern", "ihn", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Wonne und des Danks;", "tokens": ["Der", "Won\u00b7ne", "und", "des", "Danks", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie feiern ihn mit T\u00f6nen", "tokens": ["Sie", "fei\u00b7ern", "ihn", "mit", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des sch\u00f6nsten Lobgesangs. \u2013", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "Lob\u00b7ge\u00b7sangs", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als ich drauf erwachte,", "tokens": ["Und", "als", "ich", "drauf", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fand ich mich wieder kaum;", "tokens": ["Fand", "ich", "mich", "wie\u00b7der", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lerchen dr\u00fcben sangen,", "tokens": ["Die", "Ler\u00b7chen", "dr\u00fc\u00b7ben", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es fl\u00fcsterte im Baum.", "tokens": ["Es", "fl\u00fcs\u00b7ter\u00b7te", "im", "Baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich war da eingeschlummert", "tokens": ["Ich", "war", "da", "ein\u00b7ge\u00b7schlum\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An eines Waldes Saum:", "tokens": ["An", "ei\u00b7nes", "Wal\u00b7des", "Saum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das rauschten wohl die Eichen!", "tokens": ["Das", "rauschten", "wohl", "die", "Ei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das war ein gr\u00fcner Traum.", "tokens": ["Das", "war", "ein", "gr\u00fc\u00b7ner", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}