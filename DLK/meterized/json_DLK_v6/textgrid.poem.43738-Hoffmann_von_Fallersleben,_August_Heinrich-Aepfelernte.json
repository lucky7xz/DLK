{"textgrid.poem.43738": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aepfelernte", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O welche Lust, auf die B\u00e4ume steigen", "tokens": ["O", "wel\u00b7che", "Lust", ",", "auf", "die", "B\u00e4u\u00b7me", "stei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PWAT", "NN", "$,", "APPR", "ART", "NN", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und klettern umher in den schwanken Zweigen!", "tokens": ["Und", "klet\u00b7tern", "um\u00b7her", "in", "den", "schwan\u00b7ken", "Zwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wir m\u00fcssen uns recken, m\u00fcssen uns b\u00fccken,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "uns", "re\u00b7cken", ",", "m\u00fcs\u00b7sen", "uns", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Aepfel und Birnen und Zwetschen zu pfl\u00fccken.", "tokens": ["Die", "A\u00b7e\u00b7pfel", "und", "Bir\u00b7nen", "und", "Zwet\u00b7schen", "zu", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und k\u00f6nnen wir nicht zum Wipfel gelangen,", "tokens": ["Und", "k\u00f6n\u00b7nen", "wir", "nicht", "zum", "Wip\u00b7fel", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo meist die allersch\u00f6nsten hangen,", "tokens": ["Wo", "meist", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann legen wir die Leiter dran", "tokens": ["Dann", "le\u00b7gen", "wir", "die", "Lei\u00b7ter", "dran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ziehen mit Haken die Zweige heran.", "tokens": ["Und", "zie\u00b7hen", "mit", "Ha\u00b7ken", "die", "Zwei\u00b7ge", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Hilft das auch nichts, dann kommt das Beste,", "tokens": ["Hilft", "das", "auch", "nichts", ",", "dann", "kommt", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "PIS", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sch\u00fctteln alle Zweig' und Aeste,", "tokens": ["Wir", "sch\u00fct\u00b7teln", "al\u00b7le", "Zweig'", "und", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wenn es dann regnet Aepfel im Nu,", "tokens": ["Und", "wenn", "es", "dann", "reg\u00b7net", "A\u00b7e\u00b7pfel", "im", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "NN", "APPRART", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dann sehen wir jubelnd von ferne zu.", "tokens": ["Dann", "se\u00b7hen", "wir", "ju\u00b7belnd", "von", "fer\u00b7ne", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}