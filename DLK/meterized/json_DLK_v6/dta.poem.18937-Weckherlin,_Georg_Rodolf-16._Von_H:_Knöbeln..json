{"dta.poem.18937": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "16.  \n  Von H: Kn\u00f6beln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kn\u00f6bel/ gehend nachts zu hau\u00df/", "tokens": ["Der", "Kn\u00f6\u00b7bel", "/", "ge\u00b7hend", "nachts", "zu", "hau\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVPP", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedacht da\u00df jhm ein gaist beg\u00f6gnet/", "tokens": ["Ge\u00b7dacht", "da\u00df", "jhm", "ein", "gaist", "be\u00b7g\u00f6g\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dem/ nach dem er sich gesegnet/", "tokens": ["Zu", "dem", "/", "nach", "dem", "er", "sich", "ge\u00b7seg\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprach Er also/ zwar nicht ohn grau\u00df:", "tokens": ["Sprach", "Er", "al\u00b7so", "/", "zwar", "nicht", "ohn", "grau\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$(", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Bist du von Got/ so wirst du die leut lieben", "tokens": ["Bist", "du", "von", "Got", "/", "so", "wirst", "du", "die", "leut", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$(", "ADV", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bist du dan b\u00f6\u00df/ wirst du mich nicht betrieben;", "tokens": ["Bist", "du", "dan", "b\u00f6\u00df", "/", "wirst", "du", "mich", "nicht", "be\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$(", "VAFIN", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dan (zweifels ohn) du mir verschw\u00e4gert bist/", "tokens": ["Dan", "(", "zwei\u00b7fels", "ohn", ")", "du", "mir", "ver\u00b7schw\u00e4\u00b7gert", "bist", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "APPR", "$(", "PPER", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In dem mein Weib ein wahrer Teufel ist.", "tokens": ["In", "dem", "mein", "Weib", "ein", "wah\u00b7rer", "Teu\u00b7fel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}