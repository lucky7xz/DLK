{"textgrid.poem.59238": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4ngerfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchlrauschend unterm hellen", "tokens": ["K\u00fchl\u00b7rau\u00b7schend", "un\u00b7term", "hel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tiefblauen Himmelsdom", "tokens": ["Tief\u00b7blau\u00b7en", "Him\u00b7mels\u00b7dom"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Treibt seine klaren Wellen", "tokens": ["Treibt", "sei\u00b7ne", "kla\u00b7ren", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ew'gen Jugend Strom.", "tokens": ["Der", "ew'\u00b7gen", "Ju\u00b7gend", "Strom", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Viel r\u00fcstige Gesellen,", "tokens": ["Viel", "r\u00fcs\u00b7ti\u00b7ge", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Argonauten gleich,", "tokens": ["Den", "Ar\u00b7go\u00b7nau\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie fahren auf den Wellen", "tokens": ["Sie", "fah\u00b7ren", "auf", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins duft'ge Fr\u00fchlingsreich.", "tokens": ["Ins", "duft'\u00b7ge", "Fr\u00fch\u00b7lings\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich aber fa\u00df den Becher", "tokens": ["Ich", "a\u00b7ber", "fa\u00df", "den", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df es durchs Schiff erklingt,", "tokens": ["Da\u00df", "es", "durchs", "Schiff", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Mast steh ich als Sprecher,", "tokens": ["Am", "Mast", "steh", "ich", "als", "Spre\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der f\u00fcr euch alle singt.", "tokens": ["Der", "f\u00fcr", "euch", "al\u00b7le", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie stehn wir hier so helle!", "tokens": ["Wie", "stehn", "wir", "hier", "so", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird mancher bald schlafen gehn,", "tokens": ["Wird", "man\u00b7cher", "bald", "schla\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O Leben, wie bist du schnelle,", "tokens": ["O", "Le\u00b7ben", ",", "wie", "bist", "du", "schnel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O Leben, wie bist du sch\u00f6n!", "tokens": ["O", "Le\u00b7ben", ",", "wie", "bist", "du", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Gegr\u00fc\u00dft, du weite Runde,", "tokens": ["Ge\u00b7gr\u00fc\u00dft", ",", "du", "wei\u00b7te", "Run\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Burg auf der Felsenwand,", "tokens": ["Burg", "auf", "der", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du Land voll gro\u00dfer Kunde,", "tokens": ["Du", "Land", "voll", "gro\u00b7\u00dfer", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein gr\u00fcnes Vaterland!", "tokens": ["Mein", "gr\u00fc\u00b7nes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Euch m\u00f6cht ich alles geben,", "tokens": ["Euch", "m\u00f6cht", "ich", "al\u00b7les", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ich bin f\u00fcrstlich reich,", "tokens": ["Und", "ich", "bin", "f\u00fcrst\u00b7lich", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herzblut und mein Leben,", "tokens": ["Mein", "Herz\u00b7blut", "und", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr Br\u00fcder, alles f\u00fcr euch!", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "al\u00b7les", "f\u00fcr", "euch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIS", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "So fahrt im Morgenschimmer!", "tokens": ["So", "fahrt", "im", "Mor\u00b7gen\u00b7schim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei's Donau oder Rhein,", "tokens": ["Sei's", "Do\u00b7nau", "o\u00b7der", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NE", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein rechter Strom bricht immer", "tokens": ["Ein", "rech\u00b7ter", "Strom", "bricht", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins ew'ge Meer hinein.", "tokens": ["Ins", "ew'\u00b7ge", "Meer", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}