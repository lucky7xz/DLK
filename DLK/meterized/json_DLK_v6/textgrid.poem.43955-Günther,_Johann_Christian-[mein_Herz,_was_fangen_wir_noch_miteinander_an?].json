{"textgrid.poem.43955": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[mein Herz, was fangen wir noch miteinander an?]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz, was fangen wir noch miteinander an?", "tokens": ["Mein", "Herz", ",", "was", "fan\u00b7gen", "wir", "noch", "mi\u00b7tein\u00b7an\u00b7der", "an", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es scheint, wir werden bald dem Kummer weichen m\u00fc\u00dfen;", "tokens": ["Es", "scheint", ",", "wir", "wer\u00b7den", "bald", "dem", "Kum\u00b7mer", "wei\u00b7chen", "m\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor alles, was wir sonst dem Nechsten Guts gethan,", "tokens": ["Vor", "al\u00b7les", ",", "was", "wir", "sonst", "dem", "Nechs\u00b7ten", "Guts", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df unsre Redligkeit mit Noth und Elend b\u00fc\u00dfen.", "tokens": ["Mu\u00df", "uns\u00b7re", "Red\u00b7lig\u00b7keit", "mit", "Noth", "und", "E\u00b7lend", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Wei\u00dfheit bringt kein Brodt, die Arbeit keine Lust,", "tokens": ["Die", "Wei\u00df\u00b7heit", "bringt", "kein", "Brodt", ",", "die", "Ar\u00b7beit", "kei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Uns jagt des Himmels Zorn durch Ruthen, Land und Jahre.", "tokens": ["Uns", "jagt", "des", "Him\u00b7mels", "Zorn", "durch", "Ru\u00b7then", ",", "Land", "und", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Fehltritt, den du nur aus \u00dcbereilung thust,", "tokens": ["Ein", "Fehl\u00b7tritt", ",", "den", "du", "nur", "aus", "\u00dc\u00b7be\u00b7rei\u00b7lung", "thust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wird, ob er dich gleich reut, ein neuer Schritt zur Baare.", "tokens": ["Wird", ",", "ob", "er", "dich", "gleich", "reut", ",", "ein", "neu\u00b7er", "Schritt", "zur", "Baa\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Eltern Angst ist dein, der Schwester Gram trift mich;", "tokens": ["Der", "El\u00b7tern", "Angst", "ist", "dein", ",", "der", "Schwes\u00b7ter", "Gram", "trift", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "$,", "ART", "NN", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die L\u00e4strer plagen uns mit unversch\u00e4mten Zungen,", "tokens": ["Die", "L\u00e4st\u00b7rer", "pla\u00b7gen", "uns", "mit", "un\u00b7ver\u00b7sch\u00e4m\u00b7ten", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Armuth macht mich auch den Thoren l\u00e4cherlich,", "tokens": ["Die", "Ar\u00b7muth", "macht", "mich", "auch", "den", "Tho\u00b7ren", "l\u00e4\u00b7cher\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und was nur Schaden bringt, das wird mir aufgedrungen.", "tokens": ["Und", "was", "nur", "Scha\u00b7den", "bringt", ",", "das", "wird", "mir", "auf\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du kanst das von Natur dir anvertraute Pfund", "tokens": ["Du", "kanst", "das", "von", "Na\u00b7tur", "dir", "an\u00b7ver\u00b7trau\u00b7te", "Pfund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "APPR", "NN", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus Mangel hoher Gunst auf keinen Wucher legen;", "tokens": ["Aus", "Man\u00b7gel", "ho\u00b7her", "Gunst", "auf", "kei\u00b7nen", "Wu\u00b7cher", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kein Zufall macht den Werth von meinem Wi\u00dfen kund,", "tokens": ["Kein", "Zu\u00b7fall", "macht", "den", "Werth", "von", "mei\u00b7nem", "Wi\u00b7\u00dfen", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und was dir g\u00fctig scheint, wird elend meinetwegen.", "tokens": ["Und", "was", "dir", "g\u00fc\u00b7tig", "scheint", ",", "wird", "e\u00b7lend", "mei\u00b7net\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVFIN", "$,", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Man w\u00fcrdigt meine Noth der Untersuchung nicht,", "tokens": ["Man", "w\u00fcr\u00b7digt", "mei\u00b7ne", "Noth", "der", "Un\u00b7ter\u00b7su\u00b7chung", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Sp\u00f6tter nennen sie theils Strafe, theils Gedichte;", "tokens": ["Die", "Sp\u00f6t\u00b7ter", "nen\u00b7nen", "sie", "theils", "Stra\u00b7fe", ",", "theils", "Ge\u00b7dich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wer in Gegenwart auch noch so freundlich spricht,", "tokens": ["Und", "wer", "in", "Ge\u00b7gen\u00b7wart", "auch", "noch", "so", "freund\u00b7lich", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": " ... mir hinterw\u00e4rts ein ... Gerichte.", "tokens": ["...", "mir", "hin\u00b7ter\u00b7w\u00e4rts", "ein", "...", "Ge\u00b7rich\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "ADV", "ART", "$(", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Nunmehr ist endlich auch der Jahre Lenz vorbey;", "tokens": ["Nun\u00b7mehr", "ist", "end\u00b7lich", "auch", "der", "Jah\u00b7re", "Lenz", "vor\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wem will ein solches Creuz nicht die Gedult erm\u00fcden?", "tokens": ["Wem", "will", "ein", "sol\u00b7ches", "Creuz", "nicht", "die", "Ge\u00b7dult", "er\u00b7m\u00fc\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "PIAT", "NN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Musen sind mir hold, und Lorchen bleibt noch treu,", "tokens": ["Die", "Mu\u00b7sen", "sind", "mir", "hold", ",", "und", "Lor\u00b7chen", "bleibt", "noch", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "KON", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein Herz, was wiltu mehr? Ich gebe mich zufrieden.", "tokens": ["Mein", "Herz", ",", "was", "wil\u00b7tu", "mehr", "?", "Ich", "ge\u00b7be", "mich", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VMFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}