{"textgrid.poem.42787": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alte Mauer, die ich oft ben\u00e4sse,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alte Mauer, die ich oft ben\u00e4sse,", "tokens": ["Al\u00b7te", "Mau\u00b7er", ",", "die", "ich", "oft", "be\u00b7n\u00e4s\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weil's dort dunkel ist.", "tokens": ["Weil's", "dort", "dun\u00b7kel", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Himmlisches Gefunkel ist", "tokens": ["Himm\u00b7li\u00b7sches", "Ge\u00b7fun\u00b7kel", "ist"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In deiner Bl\u00e4sse.", "tokens": ["In", "dei\u00b7ner", "Bl\u00e4s\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Pilz und Feuchtigkeiten", "tokens": ["Pilz", "und", "Feuch\u00b7tig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und der Wetterschliff der Zeiten", "tokens": ["Und", "der", "Wet\u00b7ter\u00b7schliff", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gaben deiner Haut", "tokens": ["Ga\u00b7ben", "dei\u00b7ner", "Haut"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wogende Gesichter,", "tokens": ["Wo\u00b7gen\u00b7de", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Die nur ein Dichter", "tokens": ["Die", "nur", "ein", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Oder ein K\u00fcnstler", "tokens": ["O\u00b7der", "ein", "K\u00fcnst\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Oder Nureiner schaut.", "tokens": ["O\u00b7der", "Nu\u00b7rei\u00b7ner", "schaut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbk\u00f6nnen wir uns wehren?\u00ab", "tokens": ["\u00bb", "k\u00f6n\u00b7nen", "wir", "uns", "weh\u00b7ren", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fragt's aus dir mild.", "tokens": ["Fragt's", "aus", "dir", "mild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ach, kein Buch, kein Bild", "tokens": ["Ach", ",", "kein", "Buch", ",", "kein", "Bild"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wird mich so belehren.", "tokens": ["Wird", "mich", "so", "be\u00b7leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Was ich an dir schaute,", "tokens": ["Was", "ich", "an", "dir", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Etwas davon blieb", "tokens": ["Et\u00b7was", "da\u00b7von", "blieb"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PAV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Immer. Nie vertraute", "tokens": ["Im\u00b7mer", ".", "Nie", "ver\u00b7trau\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$.", "ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mauer, dich hab' ich lieb.", "tokens": ["Mau\u00b7er", ",", "dich", "hab'", "ich", "lieb", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Weil du gar nicht predigst.", "tokens": ["Weil", "du", "gar", "nicht", "pre\u00b7digst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Weil du nichts erledigst.", "tokens": ["Weil", "du", "nichts", "er\u00b7le\u00b7digst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Weil du gar nicht willst sein.", "tokens": ["Weil", "du", "gar", "nicht", "willst", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weil mir deine Flecken", "tokens": ["Weil", "mir", "dei\u00b7ne", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ahnungen erwecken.", "tokens": ["Ah\u00b7nun\u00b7gen", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Du, eines Schattens Schein.", "tokens": ["Du", ",", "ei\u00b7nes", "Schat\u00b7tens", "Schein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nichts davon wissen", "tokens": ["Nichts", "da\u00b7von", "wis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "PAV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die, die sonst hier pissen,", "tokens": ["Die", ",", "die", "sonst", "hier", "pis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch mir winkt es: Komm!", "tokens": ["Doch", "mir", "winkt", "es", ":", "Komm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Seit ich dich gefunden,", "tokens": ["Seit", "ich", "dich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Macht mich f\u00fcr Sekunden", "tokens": ["Macht", "mich", "f\u00fcr", "Se\u00b7kun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Meine Notdurft an dir fromm.", "tokens": ["Mei\u00b7ne", "Not\u00b7durft", "an", "dir", "fromm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}