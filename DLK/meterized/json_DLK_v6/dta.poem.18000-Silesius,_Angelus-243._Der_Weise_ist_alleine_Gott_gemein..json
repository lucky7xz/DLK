{"dta.poem.18000": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "243.  Der Weise ist alleine Gott gemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df ist de\u00df Weisen mutt/ er machet sich allein/", "tokens": ["Gro\u00df", "ist", "de\u00df", "Wei\u00b7sen", "mutt", "/", "er", "ma\u00b7chet", "sich", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJD", "$(", "PPER", "VVFIN", "PRF", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Herren der Herrligkeit so viel er kan gemein.", "tokens": ["Dem", "Her\u00b7ren", "der", "Herr\u00b7lig\u00b7keit", "so", "viel", "er", "kan", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADV", "PPER", "VMFIN", "ADJD", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}