{"textgrid.poem.54536": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Wunder-grosse Gott der nichts als Wunder macht/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wunder-grosse Gott der nichts als Wunder macht/", "tokens": ["Der", "Wun\u00b7der\u00b7gros\u00b7se", "Gott", "der", "nichts", "als", "Wun\u00b7der", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "PIS", "KOKOM", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pflegt diese/ die ihm treu/ nur wunderlich zu f\u00fchren/", "tokens": ["Pflegt", "die\u00b7se", "/", "die", "ihm", "treu", "/", "nur", "wun\u00b7der\u00b7lich", "zu", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "$(", "PRELS", "PPER", "ADJD", "$(", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie gehn durch Stahl und Schwerdt/ doch mu\u00df kein Schwerdt sie r\u00fchren;", "tokens": ["Sie", "gehn", "durch", "Stahl", "und", "Schwerdt", "/", "doch", "mu\u00df", "kein", "Schwerdt", "sie", "r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$(", "ADV", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie stehn/ ob um und um der Erden Grund erkracht/", "tokens": ["Sie", "stehn", "/", "ob", "um", "und", "um", "der", "Er\u00b7den", "Grund", "er\u00b7kracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "APPR", "KON", "APPR", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es wird kein Haar versehrt ob schon die Flamm erwacht", "tokens": ["Es", "wird", "kein", "Haar", "ver\u00b7sehrt", "ob", "schon", "die", "Flamm", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVFIN", "KOUS", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und an die Sternen schl\u00e4gt. Wo Pest und Todt zu sp\u00fcren", "tokens": ["Und", "an", "die", "Ster\u00b7nen", "schl\u00e4gt", ".", "Wo", "Pest", "und", "Todt", "zu", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$.", "PWAV", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Versp\u00fcrt man seine Krafft/ er wei\u00df sie auszuzieren", "tokens": ["Ver\u00b7sp\u00fcrt", "man", "sei\u00b7ne", "Krafft", "/", "er", "wei\u00df", "sie", "aus\u00b7zu\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit diesem was der Feind zu ihrer Qual erdacht.", "tokens": ["Mit", "die\u00b7sem", "was", "der", "Feind", "zu", "ih\u00b7rer", "Qual", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PWS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er f\u00fchrt in w\u00fcstes Feld durch ungeb\u00e4hnte Wege", "tokens": ["Er", "f\u00fchrt", "in", "w\u00fcs\u00b7tes", "Feld", "durch", "un\u00b7ge\u00b7b\u00e4hn\u00b7te", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und f\u00fchrt auf rechte Weg aus Hecken-vollem Stege;", "tokens": ["Und", "f\u00fchrt", "auf", "rech\u00b7te", "Weg", "aus", "He\u00b7cken\u00b7vol\u00b7lem", "Ste\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und f\u00fchrt durch Hertzens-Angst zu ewig steter Ruh.", "tokens": ["Und", "f\u00fchrt", "durch", "Hert\u00b7zens\u00b7Angst", "zu", "e\u00b7wig", "ste\u00b7ter", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die ungeheure Flut schluckt offt in tieffste T\u00e4uffen", "tokens": ["Die", "un\u00b7ge\u00b7heu\u00b7re", "Flut", "schluckt", "offt", "in", "tieffs\u00b7te", "T\u00e4uf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Er erheben wil/ und darff sie nicht erseuffen:", "tokens": ["Die", "Er", "er\u00b7he\u00b7ben", "wil", "/", "und", "darff", "sie", "nicht", "er\u00b7seuf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$(", "KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gott und was Gottes ist kommt nichts als Wunder zu.", "tokens": ["Gott", "und", "was", "Got\u00b7tes", "ist", "kommt", "nichts", "als", "Wun\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PWS", "NN", "VAFIN", "VVFIN", "PIS", "KOKOM", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}