{"textgrid.poem.53015": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "Des Thirsis Empfindungen bey Damons Hochzeitlust", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die K\u00f6nigin der reinen Triebe,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "der", "rei\u00b7nen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschen Trost und Lust, die Liebe,", "tokens": ["Der", "Men\u00b7schen", "Trost", "und", "Lust", ",", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Begleit, o Damon, deinen Fu\u00df.", "tokens": ["Be\u00b7gleit", ",", "o", "Da\u00b7mon", ",", "dei\u00b7nen", "Fu\u00df", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, Freund, empfange mit Vergn\u00fcgen", "tokens": ["Ja", ",", "Freund", ",", "emp\u00b7fan\u00b7ge", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wirst du bey ihrer Fahne siegen,", "tokens": ["Wirst", "du", "bey", "ih\u00b7rer", "Fah\u00b7ne", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den sanft durchdringend keuschen Ku\u00df.", "tokens": ["Den", "sanft", "durch\u00b7drin\u00b7gend", "keu\u00b7schen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Geist entrei\u00dft sich, dich von weiten", "tokens": ["Mein", "Geist", "ent\u00b7rei\u00dft", "sich", ",", "dich", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,", "PRF", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Sehnsucht immer zu begleiten.", "tokens": ["Voll", "Sehn\u00b7sucht", "im\u00b7mer", "zu", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mich d\u00fcnckt, ich seh dein holdes Kind,", "tokens": ["Mich", "d\u00fcnckt", ",", "ich", "seh", "dein", "hol\u00b7des", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich seh, wie sie mit s\u00fcssen Blicken", "tokens": ["Ich", "seh", ",", "wie", "sie", "mit", "s\u00fcs\u00b7sen", "Bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich seh, wie sie dich mit entz\u00fccken", "tokens": ["Ich", "seh", ",", "wie", "sie", "dich", "mit", "ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihren weissen Armen bindt.", "tokens": ["Mit", "ih\u00b7ren", "weis\u00b7sen", "Ar\u00b7men", "bindt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mich d\u00fcnckt, da\u00df ich euch im Vertrauen", "tokens": ["Mich", "d\u00fcnckt", ",", "da\u00df", "ich", "euch", "im", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kan bey einander sitzen schauen.", "tokens": ["Kan", "bey", "ein\u00b7an\u00b7der", "sit\u00b7zen", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6re, wie du manchen Schertz", "tokens": ["Ich", "h\u00f6\u00b7re", ",", "wie", "du", "man\u00b7chen", "Schertz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sinnreich in die Ohren pfl\u00fcsterst,", "tokens": ["Ihr", "sinn\u00b7reich", "in", "die", "Oh\u00b7ren", "pfl\u00fcs\u00b7terst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihn mit einem Ku\u00df verschwisterst,", "tokens": ["Und", "ihn", "mit", "ei\u00b7nem", "Ku\u00df", "ver\u00b7schwis\u00b7terst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und immer sprichst: Mein Kind, mein Hertz!", "tokens": ["Und", "im\u00b7mer", "sprichst", ":", "Mein", "Kind", ",", "mein", "Hertz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch, wenn ihr nun von andern Sachen", "tokens": ["Doch", ",", "wenn", "ihr", "nun", "von", "an\u00b7dern", "Sa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch etwann werdet Worte machen;", "tokens": ["Auch", "et\u00b7wann", "wer\u00b7det", "Wor\u00b7te", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So bring ihr dann daneben bey,", "tokens": ["So", "bring", "ihr", "dann", "da\u00b7ne\u00b7ben", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Da\u00df einen, wo die Saale fliesset,", "tokens": ["Da\u00df", "ei\u00b7nen", ",", "wo", "die", "Saa\u00b7le", "flies\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Freundschaft Band mit dir umschliesset,", "tokens": ["Der", "Freund\u00b7schaft", "Band", "mit", "dir", "um\u00b7schlies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sag ihr, da\u00df es Thirsis sey.", "tokens": ["Und", "sag", "ihr", ",", "da\u00df", "es", "Thir\u00b7sis", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sag ihr, wie wir uns redlich lieben,", "tokens": ["Sag", "ihr", ",", "wie", "wir", "uns", "red\u00b7lich", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und oft vereint im Singen \u00fcben.", "tokens": ["Und", "oft", "ver\u00b7eint", "im", "Sin\u00b7gen", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sag ihr, ich r\u00fchmte eure Glut,", "tokens": ["Sag", "ihr", ",", "ich", "r\u00fchm\u00b7te", "eu\u00b7re", "Glut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja unbekannt ihr holdes Wesen;", "tokens": ["Ja", "un\u00b7be\u00b7kannt", "ihr", "hol\u00b7des", "We\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil du sie dir zur Braut erlesen,", "tokens": ["Weil", "du", "sie", "dir", "zur", "Braut", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vielleicht wird sie mir darum gut.", "tokens": ["Viel\u00b7leicht", "wird", "sie", "mir", "da\u00b7rum", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und, o wie werd ich voller Feuer", "tokens": ["Und", ",", "o", "wie", "werd", "ich", "vol\u00b7ler", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "FM", "KOKOM", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl bald an ihr, mit meiner Leyer", "tokens": ["Wohl", "bald", "an", "ihr", ",", "mit", "mei\u00b7ner", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-----+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die holden Tugenden erh\u00f6hn;", "tokens": ["Die", "hol\u00b7den", "Tu\u00b7gen\u00b7den", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nein, es werden deine Saiten", "tokens": ["Doch", "nein", ",", "es", "wer\u00b7den", "dei\u00b7ne", "Sai\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr ein weit sch\u00f6ner Lob bereiten,", "tokens": ["Ihr", "ein", "weit", "sch\u00f6\u00b7ner", "Lob", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit ihr zu den Sternen gehn.", "tokens": ["Und", "mit", "ihr", "zu", "den", "Ster\u00b7nen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du wirst auch ihr, bey deinem Singen", "tokens": ["Du", "wirst", "auch", "ihr", ",", "bey", "dei\u00b7nem", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lorbeern um die Haare schlingen,", "tokens": ["Die", "Lor\u00b7beern", "um", "die", "Haa\u00b7re", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die deine Hand am Pindus bricht.", "tokens": ["Die", "dei\u00b7ne", "Hand", "am", "Pin\u00b7dus", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie darff die Laura, voller Freuden,", "tokens": ["Sie", "darff", "die", "Lau\u00b7ra", ",", "vol\u00b7ler", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wie die Doris nicht beneiden;", "tokens": ["So", "wie", "die", "Do\u00b7ris", "nicht", "be\u00b7nei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NE", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie weichet auch Callisten nicht.", "tokens": ["Sie", "wei\u00b7chet", "auch", "Cal\u00b7lis\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie gl\u00fccklich sind der Nymphen Sinnen,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "sind", "der", "Nym\u00b7phen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die einen Dichter lieb gewinnen!", "tokens": ["Die", "ei\u00b7nen", "Dich\u00b7ter", "lieb", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer sollte dich, Euridice,", "tokens": ["Wer", "soll\u00b7te", "dich", ",", "Eu\u00b7ri\u00b7di\u00b7ce", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer, sch\u00f6ne Nemesis, nicht kennen,", "tokens": ["Wer", ",", "sch\u00f6\u00b7ne", "Ne\u00b7me\u00b7sis", ",", "nicht", "ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "$,", "ADJA", "NN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer Lalagen nicht gl\u00fccklich nennen,", "tokens": ["Wer", "La\u00b7la\u00b7gen", "nicht", "gl\u00fcck\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer prei\u00dft dich nicht, o Galathee?", "tokens": ["Wer", "prei\u00dft", "dich", "nicht", ",", "o", "Ga\u00b7la\u00b7thee", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch die wird sie, mit ewgen Zweigen", "tokens": ["Doch", "die", "wird", "sie", ",", "mit", "ew\u00b7gen", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und Ruhm gekr\u00f6nt, weit \u00fcbersteigen.", "tokens": ["Und", "Ruhm", "ge\u00b7kr\u00f6nt", ",", "weit", "\u00fc\u00b7bers\u00b7tei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch, was st\u00f6hr ich eure Ruh!", "tokens": ["Je\u00b7doch", ",", "was", "st\u00f6hr", "ich", "eu\u00b7re", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kurtz, hier sind der Gesellschaft Lieder,", "tokens": ["Kurtz", ",", "hier", "sind", "der", "Ge\u00b7sell\u00b7schaft", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch komm auch bald zur Saale wieder,", "tokens": ["Doch", "komm", "auch", "bald", "zur", "Saa\u00b7le", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und h\u00f6re deinem Damon zu.", "tokens": ["Und", "h\u00f6\u00b7re", "dei\u00b7nem", "Da\u00b7mon", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}