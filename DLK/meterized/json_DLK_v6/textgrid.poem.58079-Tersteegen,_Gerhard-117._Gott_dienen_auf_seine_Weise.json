{"textgrid.poem.58079": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "117. Gott dienen auf seine Weise", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft nicht dienen Gott, nach dem es ", "tokens": ["Du", "mu\u00dft", "nicht", "die\u00b7nen", "Gott", ",", "nach", "dem", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "NN", "$,", "APPR", "PRELS", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dient ihm, der ", "tokens": ["Der", "dient", "ihm", ",", "der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PRELS"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Und kann man ihn nicht ", "tokens": ["Und", "kann", "man", "ihn", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "PPER", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wer sonst nichts will als Gott, der dienet ihm in beiden.", "tokens": ["Wer", "sonst", "nichts", "will", "als", "Gott", ",", "der", "die\u00b7net", "ihm", "in", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "VMFIN", "KOUS", "NN", "$,", "PRELS", "VVFIN", "PPER", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}