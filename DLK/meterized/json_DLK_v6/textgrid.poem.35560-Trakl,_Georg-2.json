{"textgrid.poem.35560": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Arme, der im Geiste einsam starb,", "tokens": ["Der", "Ar\u00b7me", ",", "der", "im", "Geis\u00b7te", "ein\u00b7sam", "starb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Steigt w\u00e4chsern \u00fcber einen alten Pfad.", "tokens": ["Steigt", "w\u00e4ch\u00b7sern", "\u00fc\u00b7ber", "ei\u00b7nen", "al\u00b7ten", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Apfelb\u00e4ume sinken kahl und stad", "tokens": ["Die", "Ap\u00b7fel\u00b7b\u00e4u\u00b7me", "sin\u00b7ken", "kahl", "und", "stad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ins Farbige ihrer Frucht, die schwarz verdarb.", "tokens": ["Ins", "Far\u00b7bi\u00b7ge", "ih\u00b7rer", "Frucht", ",", "die", "schwarz", "ver\u00b7darb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Noch immer w\u00f6lbt das Dach aus d\u00fcrrem Stroh", "tokens": ["Noch", "im\u00b7mer", "w\u00f6lbt", "das", "Dach", "aus", "d\u00fcr\u00b7rem", "Stroh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich \u00fcbern Schlaf der K\u00fche. Die blinde Magd", "tokens": ["Sich", "\u00fc\u00b7bern", "Schlaf", "der", "K\u00fc\u00b7he", ".", "Die", "blin\u00b7de", "Magd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPRART", "NN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erscheint im Hof; ein blaues Wasser klagt;", "tokens": ["Er\u00b7scheint", "im", "Hof", ";", "ein", "blau\u00b7es", "Was\u00b7ser", "klagt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Pferdesch\u00e4del starrt vom morschen Tor.", "tokens": ["Ein", "Pfer\u00b7de\u00b7sch\u00e4\u00b7del", "starrt", "vom", "mor\u00b7schen", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Idiot spricht dunklen Sinns ein Wort", "tokens": ["Der", "I\u00b7diot", "spricht", "dunk\u00b7len", "Sinns", "ein", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Liebe, das im schwarzen Busch verhallt,", "tokens": ["Der", "Lie\u00b7be", ",", "das", "im", "schwar\u00b7zen", "Busch", "ver\u00b7hallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo jene steht in schmaler Traumgestalt.", "tokens": ["Wo", "je\u00b7ne", "steht", "in", "schma\u00b7ler", "Traum\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Abend t\u00f6nt in feuchter Bl\u00e4ue fort.", "tokens": ["Der", "A\u00b7bend", "t\u00f6nt", "in", "feuch\u00b7ter", "Bl\u00e4u\u00b7e", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}