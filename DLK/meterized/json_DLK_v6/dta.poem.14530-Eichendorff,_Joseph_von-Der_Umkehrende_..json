{"dta.poem.14530": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Umkehrende .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist's, der, was wir bauen,", "tokens": ["Du", "bist's", ",", "der", ",", "was", "wir", "bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PRELS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mild \u00fcber uns zerbricht,", "tokens": ["Mild", "\u00fc\u00b7ber", "uns", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df wir den Himmel schauen \u2014", "tokens": ["Da\u00df", "wir", "den", "Him\u00b7mel", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darum so klag' ich nicht.", "tokens": ["Da\u00b7rum", "so", "klag'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}