{"dta.poem.3167": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein wahres Wort: Wer glaubt, der wird betrogen;", "tokens": ["Es", "ist", "ein", "wah\u00b7res", "Wort", ":", "Wer", "glaubt", ",", "der", "wird", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "PWS", "VVFIN", "$,", "PRELS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer aber Keinem glaubt, hat sich noch mehr entzogen.", "tokens": ["Wer", "a\u00b7ber", "Kei\u00b7nem", "glaubt", ",", "hat", "sich", "noch", "mehr", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "VVFIN", "$,", "VAFIN", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn Niemand ihn betr\u00fcgt, wenn Niemand ihn beraubt;", "tokens": ["Wenn", "Nie\u00b7mand", "ihn", "be\u00b7tr\u00fcgt", ",", "wenn", "Nie\u00b7mand", "ihn", "be\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie elend, wer sich stets beraubt betrogen glaubt!", "tokens": ["Wie", "e\u00b7lend", ",", "wer", "sich", "stets", "be\u00b7raubt", "be\u00b7tro\u00b7gen", "glaubt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWS", "PRF", "ADV", "VVPP", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}