{"textgrid.poem.52611": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leb wohl, ich will dich nimmer sehn,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leb wohl, ich will dich nimmer sehn,", "tokens": ["Leb", "wohl", ",", "ich", "will", "dich", "nim\u00b7mer", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will Nichts mehr von dir wissen,", "tokens": ["Will", "Nichts", "mehr", "von", "dir", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob Thr\u00e4nen mir im Auge stehn,", "tokens": ["Ob", "Thr\u00e4\u00b7nen", "mir", "im", "Au\u00b7ge", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hab den Schmerz verbissen.", "tokens": ["Ich", "hab", "den", "Schmerz", "ver\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als wie ein Vogel flattert fort,", "tokens": ["Als", "wie", "ein", "Vo\u00b7gel", "flat\u00b7tert", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie ein Blatt im Lenz verdorrt,", "tokens": ["Als", "wie", "ein", "Blatt", "im", "Lenz", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als wie ein Lenz vergeht,", "tokens": ["Als", "wie", "ein", "Lenz", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sei unser Traum verweht!", "tokens": ["Sei", "un\u00b7ser", "Traum", "ver\u00b7weht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es ist vorbei, es thut kein gut,", "tokens": ["Es", "ist", "vor\u00b7bei", ",", "es", "thut", "kein", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPER", "VVFIN", "PIAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir passen nicht zusammen,", "tokens": ["Wir", "pas\u00b7sen", "nicht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In gleichem Takte springt das Blut", "tokens": ["In", "glei\u00b7chem", "Tak\u00b7te", "springt", "das", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und prasseln unsre Flammen.", "tokens": ["Und", "pras\u00b7seln", "uns\u00b7re", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir liebten uns, es war ein Wahn,", "tokens": ["Wir", "lieb\u00b7ten", "uns", ",", "es", "war", "ein", "Wahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir beteten uns selber an.", "tokens": ["Wir", "be\u00b7te\u00b7ten", "uns", "sel\u00b7ber", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Geheimni\u00df, tief und gro\u00df,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df", ",", "tief", "und", "gro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zieht an und l\u00e4\u00dft nicht los!", "tokens": ["Zieht", "an", "und", "l\u00e4\u00dft", "nicht", "los", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Uns schauen und erlauschen \u2013", "tokens": ["Uns", "schau\u00b7en", "und", "er\u00b7lau\u00b7schen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir k\u00f6nnten schlie\u00dfen neuen Bund", "tokens": ["Wir", "k\u00f6nn\u00b7ten", "schlie\u00b7\u00dfen", "neu\u00b7en", "Bund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Lieb in Freundschaft tauschen,", "tokens": ["Und", "Lieb", "in", "Freund\u00b7schaft", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geschwisterlich zusammengehn,", "tokens": ["Ge\u00b7schwis\u00b7ter\u00b7lich", "zu\u00b7sam\u00b7men\u00b7gehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns friedlich in die Augen sehn,", "tokens": ["Uns", "fried\u00b7lich", "in", "die", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch nein! Leb wohl, leb wohl!", "tokens": ["Doch", "nein", "!", "Leb", "wohl", ",", "leb", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "VVIMP", "ADV", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u2013 So flieht sich gleicher Pol.", "tokens": ["\u2013", "So", "flieht", "sich", "glei\u00b7cher", "Pol", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}