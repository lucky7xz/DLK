{"dta.poem.23339": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wassersucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, ich bin bei allem sch\u00fcchtern,", "tokens": ["Ja", ",", "ich", "bin", "bei", "al\u00b7lem", "sch\u00fcch\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was da irgend Wasser hei\u00dft,", "tokens": ["Was", "da", "ir\u00b7gend", "Was\u00b7ser", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es macht doch gar zu n\u00fcchtern", "tokens": ["Denn", "es", "macht", "doch", "gar", "zu", "n\u00fcch\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Menschen Herz und Geist.", "tokens": ["Je\u00b7des", "Men\u00b7schen", "Herz", "und", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kann Philistern auch nicht schaden", "tokens": ["Kann", "Phi\u00b7lis\u00b7tern", "auch", "nicht", "scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine gute Wasserkur,", "tokens": ["Ei\u00b7ne", "gu\u00b7te", "Was\u00b7ser\u00b7kur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer wird sich drin verbaden", "tokens": ["Im\u00b7mer", "wird", "sich", "drin", "ver\u00b7ba\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die poetische Natur.", "tokens": ["Die", "po\u00b7e\u00b7ti\u00b7sche", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freilich, wer die Dichtkunst setzet", "tokens": ["Frei\u00b7lich", ",", "wer", "die", "Dicht\u00b7kunst", "set\u00b7zet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ins Versmaa\u00df und den Reim,", "tokens": ["Nur", "ins", "Vers\u00b7maa\u00df", "und", "den", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, der kommt wohl unverletzet", "tokens": ["Nun", ",", "der", "kommt", "wohl", "un\u00b7ver\u00b7let\u00b7zet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PRELS", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch aus jedem Wasser heim.", "tokens": ["Auch", "aus", "je\u00b7dem", "Was\u00b7ser", "heim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}