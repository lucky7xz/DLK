{"textgrid.poem.48408": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. Der klagende Br\u00e4utigam. 1", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die du warest mein, mein Leben, meine Zier,", "tokens": ["Du", ",", "die", "du", "wa\u00b7rest", "mein", ",", "mein", "Le\u00b7ben", ",", "mei\u00b7ne", "Zier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VAFIN", "PPOSAT", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wie liegstu hier so bla\u00df, so ganz un\u00e4hnlich ", "tokens": ["wie", "liegs\u00b7tu", "hier", "so", "bla\u00df", ",", "so", "ganz", "un\u00b7\u00e4hn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Fenster sind entzwei, der Mund, die Zung' erstarret,", "tokens": ["Die", "Fens\u00b7ter", "sind", "ent\u00b7zwei", ",", "der", "Mund", ",", "die", "Zung'", "er\u00b7star\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die H\u00e4nde hangen dir, der Leib will sein verscharret.", "tokens": ["die", "H\u00e4n\u00b7de", "han\u00b7gen", "dir", ",", "der", "Leib", "will", "sein", "ver\u00b7schar\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "VMFIN", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo ist, o meine Sonn', ietzt deiner Liechter Schein?", "tokens": ["Wo", "ist", ",", "o", "mei\u00b7ne", "Sonn'", ",", "ietzt", "dei\u00b7ner", "Liech\u00b7ter", "Schein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "FM", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo ist, o meine Braut, die sch\u00f6ne Sch\u00f6nheit dein?", "tokens": ["Wo", "ist", ",", "o", "mei\u00b7ne", "Braut", ",", "die", "sch\u00f6\u00b7ne", "Sch\u00f6n\u00b7heit", "dein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "FM", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Schmerzens-T\u00f6chter mir, die Tr\u00e4nen, tun ausbrechen,", "tokens": ["Die", "Schmer\u00b7zens\u00b7T\u00f6ch\u00b7ter", "mir", ",", "die", "Tr\u00e4\u00b7nen", ",", "tun", "aus\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ART", "NN", "$,", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Herzqu\u00e4len, Augenangst, Hauptschmerzen, Seitenstechen,", "tokens": ["Herz\u00b7qu\u00e4\u00b7len", ",", "Au\u00b7gen\u00b7angst", ",", "Haupt\u00b7schmer\u00b7zen", ",", "Sei\u00b7ten\u00b7ste\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "die st\u00fcrmen alle bald einm\u00fctig zu mir ein,", "tokens": ["die", "st\u00fcr\u00b7men", "al\u00b7le", "bald", "ein\u00b7m\u00fc\u00b7tig", "zu", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PIS", "ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wenn ich dich sehe, ja, wenn ich nur denke dein.", "tokens": ["wenn", "ich", "dich", "se\u00b7he", ",", "ja", ",", "wenn", "ich", "nur", "den\u00b7ke", "dein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "PTKANT", "$,", "KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein denk' ich aber stets, drumb hab ich steten Schmerzen", "tokens": ["Dein", "denk'", "ich", "a\u00b7ber", "stets", ",", "drumb", "hab", "ich", "ste\u00b7ten", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "PPER", "ADV", "ADV", "$,", "PAV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "in Augen, in dem H\u00e4upt', in Seiten und im Herzen;", "tokens": ["in", "Au\u00b7gen", ",", "in", "dem", "H\u00e4upt'", ",", "in", "Sei\u00b7ten", "und", "im", "Her\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "doch kan ichs lassen nicht, ich mu\u00df dich sehen an", "tokens": ["doch", "kan", "ichs", "las\u00b7sen", "nicht", ",", "ich", "mu\u00df", "dich", "se\u00b7hen", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "PTKNEG", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und denken dein, solt' ich gleich noch mehr Schmerzen han:", "tokens": ["und", "den\u00b7ken", "dein", ",", "solt'", "ich", "gleich", "noch", "mehr", "Schmer\u00b7zen", "han", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "$,", "VMFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "di\u00df tu' ich nur darumb, da\u00df durch solch stetes Qu\u00e4len", "tokens": ["di\u00df", "tu'", "ich", "nur", "da\u00b7rumb", ",", "da\u00df", "durch", "solch", "ste\u00b7tes", "Qu\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PAV", "$,", "KOUS", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "die Seele mir vergeh' und folge deiner Seelen.", "tokens": ["die", "See\u00b7le", "mir", "ver\u00b7geh'", "und", "fol\u00b7ge", "dei\u00b7ner", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}