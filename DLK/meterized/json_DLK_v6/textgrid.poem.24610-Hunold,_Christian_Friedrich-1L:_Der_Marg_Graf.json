{"textgrid.poem.24610": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Marg Graf ", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Marg Graf ", "tokens": ["Der", "Marg", "Graf"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "In seiner Wi\u00dfenschafft/ im Spielen niemahls satt;", "tokens": ["In", "sei\u00b7ner", "Wi\u00b7\u00dfen\u00b7schafft", "/", "im", "Spie\u00b7len", "nie\u00b7mahls", "satt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Vier und Sieben mu\u00df er sein Verh\u00e4ngni\u00df hoffen/", "tokens": ["Von", "Vier", "und", "Sie\u00b7ben", "mu\u00df", "er", "sein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "KON", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Leben oder Tod steht durch die W\u00fcrfel offen.", "tokens": ["Und", "Le\u00b7ben", "o\u00b7der", "Tod", "steht", "durch", "die", "W\u00fcr\u00b7fel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn nun ein schlimmer Streich der Unbest\u00e4ndigkeit/", "tokens": ["Wenn", "nun", "ein", "schlim\u00b7mer", "Streich", "der", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihm sein gehabtes Gl\u00fcck mit Ungl\u00fcck \u00fcberstreut:", "tokens": ["Ihm", "sein", "ge\u00b7hab\u00b7tes", "Gl\u00fcck", "mit", "Un\u00b7gl\u00fcck", "\u00fc\u00b7bers\u00b7treut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So sieht man seine Haar als bald zu Berge stehen/", "tokens": ["So", "sieht", "man", "sei\u00b7ne", "Haar", "als", "bald", "zu", "Ber\u00b7ge", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "KOKOM", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sein ergrimmt Gesicht aus Wuht gen Himmel gehen;", "tokens": ["Und", "sein", "er\u00b7grimmt", "Ge\u00b7sicht", "aus", "Wuht", "gen", "Him\u00b7mel", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVPP", "NN", "APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie ein bese\u00dfner Mensch/ den sonst ein Pfaff beschwert/", "tokens": ["Wie", "ein", "be\u00b7se\u00df\u00b7ner", "Mensch", "/", "den", "sonst", "ein", "Pfaff", "be\u00b7schwert", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$(", "ART", "ADV", "ART", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und alle Heiligen der Kirchen drum verehrt.", "tokens": ["Und", "al\u00b7le", "Hei\u00b7li\u00b7gen", "der", "Kir\u00b7chen", "drum", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich f\u00fcrchte/ da\u00df sein Blitz/ und so wir ihn nicht binden/", "tokens": ["Ich", "f\u00fcrch\u00b7te", "/", "da\u00df", "sein", "Blitz", "/", "und", "so", "wir", "ihn", "nicht", "bin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "$(", "KON", "ADV", "PPER", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der neue ", "tokens": ["Der", "neu\u00b7e"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}}}}