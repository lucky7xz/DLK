{"textgrid.poem.47054": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was h\u00fclf' es, ob den Maler in die W\u00e4nde", "tokens": ["Was", "h\u00fcl\u00b7f'", "es", ",", "ob", "den", "Ma\u00b7ler", "in", "die", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Kerkers sorgsam man verschlossen h\u00e4tte,", "tokens": ["Des", "Ker\u00b7kers", "sorg\u00b7sam", "man", "ver\u00b7schlos\u00b7sen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PIS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn man ihm Pinsel mitg\u00e4b' und Palette,", "tokens": ["Wenn", "man", "ihm", "Pin\u00b7sel", "mit\u00b7g\u00e4b'", "und", "Pa\u00b7let\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NN", "PTKVZ", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ja ihm auch mitg\u00e4b' Augen nur und H\u00e4nde.", "tokens": ["Ja", "ihm", "auch", "mit\u00b7g\u00e4b'", "Au\u00b7gen", "nur", "und", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "ADV", "APPR", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob er kein andres Werkger\u00e4t auch f\u00e4nde,", "tokens": ["Ob", "er", "kein", "and\u00b7res", "Werk\u00b7ge\u00b7r\u00e4t", "auch", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So w\u00fcrd' er machen seiner Steinwand Gl\u00e4tte", "tokens": ["So", "w\u00fcrd'", "er", "ma\u00b7chen", "sei\u00b7ner", "Stein\u00b7wand", "Gl\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zur Leinwand und zum Griffel seine Kette", "tokens": ["Zur", "Lein\u00b7wand", "und", "zum", "Grif\u00b7fel", "sei\u00b7ne", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und drauf eingraben seine Gegenst\u00e4nde.", "tokens": ["Und", "drauf", "ein\u00b7gra\u00b7ben", "sei\u00b7ne", "Ge\u00b7gen\u00b7st\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was h\u00fclf' es, da\u00df auch ich den Kerker schl\u00f6sse,", "tokens": ["Was", "h\u00fcl\u00b7f'", "es", ",", "da\u00df", "auch", "ich", "den", "Ker\u00b7ker", "schl\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wenn doch ja meiner Malerkunst Ger\u00e4te", "tokens": ["Wenn", "doch", "ja", "mei\u00b7ner", "Ma\u00b7ler\u00b7kunst", "Ge\u00b7r\u00e4\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir blieb', an Farben Statt, Gedank' und T\u00f6ne?", "tokens": ["Mir", "blieb'", ",", "an", "Far\u00b7ben", "Statt", ",", "Ge\u00b7dank'", "und", "T\u00f6\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und ob kein Strahl des Tags durchs Gitter sch\u00f6sse;", "tokens": ["Und", "ob", "kein", "Strahl", "des", "Tags", "durchs", "Git\u00b7ter", "sch\u00f6s\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So w\u00fc\u00dft' ich, da\u00df im Dunkel vor mich tr\u00e4te", "tokens": ["So", "w\u00fc\u00dft'", "ich", ",", "da\u00df", "im", "Dun\u00b7kel", "vor", "mich", "tr\u00e4\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Bild im Lichtglanz seiner eignen Sch\u00f6ne.", "tokens": ["Ein", "Bild", "im", "Licht\u00b7glanz", "sei\u00b7ner", "eig\u00b7nen", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}