{"dta.poem.14252": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Entfernte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sah ich still mein M\u00e4dchen stehen,", "tokens": ["Sah", "ich", "still", "mein", "M\u00e4d\u00b7chen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Flu\u00df und W\u00e4lder gehen", "tokens": ["Ue\u00b7ber", "Flu\u00df", "und", "W\u00e4l\u00b7der", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der heitern Warte oben", "tokens": ["Von", "der", "hei\u00b7tern", "War\u00b7te", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Augen licht und helle,", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "licht", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann der Liebste kommen werde. \u2014", "tokens": ["Wann", "der", "Liebs\u00b7te", "kom\u00b7men", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "VAFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja! da kam die Sonne schnelle,", "tokens": ["Ja", "!", "da", "kam", "die", "Son\u00b7ne", "schnel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und weit um die ganze Erde", "tokens": ["Und", "weit", "um", "die", "gan\u00b7ze", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "War es morgensch\u00f6n und helle!", "tokens": ["War", "es", "mor\u00b7gen\u00b7sch\u00f6n", "und", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}