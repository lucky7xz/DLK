{"dta.poem.7501": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00dflied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll der Becher,", "tokens": ["Was", "soll", "der", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ihr tobenden Zecher,", "tokens": ["Ihr", "to\u00b7ben\u00b7den", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Was soll die funkelnde Flasche", "tokens": ["Was", "soll", "die", "fun\u00b7keln\u00b7de", "Fla\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In Eurer Hand?", "tokens": ["In", "Eu\u00b7rer", "Hand", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es trauert in Sack und Asche", "tokens": ["Es", "trau\u00b7ert", "in", "Sack", "und", "A\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das Vaterland.", "tokens": ["Das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Was soll, ihr Br\u00e4ute,", "tokens": ["Was", "soll", ",", "ihr", "Br\u00e4u\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das Jubelgel\u00e4ute?", "tokens": ["Das", "Ju\u00b7bel\u00b7ge\u00b7l\u00e4u\u00b7te", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "O hei\u00dft die Rosen erblassen", "tokens": ["O", "hei\u00dft", "die", "Ro\u00b7sen", "er\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Am deutschen Strand?", "tokens": ["Am", "deut\u00b7schen", "Strand", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vom Br\u00e4utigam ist verlassen", "tokens": ["Vom", "Br\u00e4u\u00b7ti\u00b7gam", "ist", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NE", "VAFIN", "VVINF"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Das Vaterland.", "tokens": ["Das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Was soll, ihr F\u00fcrsten,", "tokens": ["Was", "soll", ",", "ihr", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nach Kronen das D\u00fcrsten?", "tokens": ["Nach", "Kro\u00b7nen", "das", "D\u00fcrs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zerrei\u00dft die goldenen Schn\u00fcre,", "tokens": ["Zer\u00b7rei\u00dft", "die", "gol\u00b7de\u00b7nen", "Schn\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Prunkgewand!", "tokens": ["Das", "Prunk\u00b7ge\u00b7wand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es frieret vor Eurer Th\u00fcre", "tokens": ["Es", "frie\u00b7ret", "vor", "Eu\u00b7rer", "Th\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das Vaterland.", "tokens": ["Das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Was macht, ihr Pfaffen,", "tokens": ["Was", "macht", ",", "ihr", "Pfaf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Euch also zu schaffen?", "tokens": ["Euch", "al\u00b7so", "zu", "schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Was soll uns jetzo das Beten?", "tokens": ["Was", "soll", "uns", "jet\u00b7zo", "das", "Be\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O eitler Tand,", "tokens": ["O", "eit\u00b7ler", "Tand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So lang in den Staub getreten", "tokens": ["So", "lang", "in", "den", "Staub", "ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das Vaterland!", "tokens": ["Das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Weh Euch, ihr Reichen,", "tokens": ["Weh", "Euch", ",", "ihr", "Rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die nicht zu erweichen!", "tokens": ["Die", "nicht", "zu", "er\u00b7wei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ihr z\u00e4hlt die Rubel, die runden,", "tokens": ["Ihr", "z\u00e4hlt", "die", "Ru\u00b7bel", ",", "die", "run\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Sonnenbrand", "tokens": ["Im", "Son\u00b7nen\u00b7brand"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der Lazarus seine Wunden,", "tokens": ["Der", "La\u00b7za\u00b7rus", "sei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das Vaterland.", "tokens": ["Das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Weh Euch, ihr Armen!", "tokens": ["Weh", "Euch", ",", "ihr", "Ar\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Was heischt Ihr Erbarmen?", "tokens": ["Was", "heischt", "Ihr", "Er\u00b7bar\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es liegen viel Edelsteine", "tokens": ["Es", "lie\u00b7gen", "viel", "E\u00b7del\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vor Euch im Sand,", "tokens": ["Vor", "Euch", "im", "Sand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Auch meine Thr\u00e4nen, auch meine,", "tokens": ["Auch", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ",", "auch", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ums Vaterland.", "tokens": ["Ums", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Doch Du, o Dichter,", "tokens": ["Doch", "Du", ",", "o", "Dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bist nimmer der Richter!", "tokens": ["Bist", "nim\u00b7mer", "der", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gebeut der fertigen Zungen,", "tokens": ["Ge\u00b7beut", "der", "fer\u00b7ti\u00b7gen", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gebeut ihr Stand!", "tokens": ["Ge\u00b7beut", "ihr", "Stand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dein Schwanenlied ist gesungen", "tokens": ["Dein", "Schwa\u00b7nen\u00b7lied", "ist", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dem Vaterland.", "tokens": ["Dem", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}