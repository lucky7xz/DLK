{"dta.poem.18680": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Lesebuch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wunderlichstes Buch der B\u00fccher", "tokens": ["Wun\u00b7der\u00b7lichs\u00b7tes", "Buch", "der", "B\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist das Buch der Liebe;", "tokens": ["Ist", "das", "Buch", "der", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aufmerksam hab\u2019 ich\u2019s gelesen:", "tokens": ["Auf\u00b7merk\u00b7sam", "hab'", "ich's", "ge\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenig Bl\u00e4tter Freuden,", "tokens": ["We\u00b7nig", "Bl\u00e4t\u00b7ter", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ganze Hefte Leiden,", "tokens": ["Gan\u00b7ze", "Hef\u00b7te", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Einen Abschnitt macht die Trennung.", "tokens": ["Ei\u00b7nen", "Ab\u00b7schnitt", "macht", "die", "Tren\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wiedersehn! ein klein Capitel", "tokens": ["Wie\u00b7der\u00b7sehn", "!", "ein", "klein", "Ca\u00b7pi\u00b7tel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fragmentarisch. B\u00e4nde Kummers", "tokens": ["Frag\u00b7men\u00b7ta\u00b7risch", ".", "B\u00e4n\u00b7de", "Kum\u00b7mers"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mit Erkl\u00e4rungen verl\u00e4ngert,", "tokens": ["Mit", "Er\u00b7kl\u00e4\u00b7run\u00b7gen", "ver\u00b7l\u00e4n\u00b7gert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Endlos ohne Maas.", "tokens": ["End\u00b7los", "oh\u00b7ne", "Maas", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "O! Nisami! \u2014 doch am Ende", "tokens": ["O", "!", "Ni\u00b7sa\u00b7mi", "!", "doch", "am", "En\u00b7de"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "$(", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Hast den rechten Weg gefunden;", "tokens": ["Hast", "den", "rech\u00b7ten", "Weg", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Unaufl\u00f6sliches wer l\u00f6st es?", "tokens": ["Un\u00b7auf\u00b7l\u00f6s\u00b7li\u00b7ches", "wer", "l\u00f6st", "es", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Liebende sich wieder findend.", "tokens": ["Lie\u00b7ben\u00b7de", "sich", "wie\u00b7der", "fin\u00b7dend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}