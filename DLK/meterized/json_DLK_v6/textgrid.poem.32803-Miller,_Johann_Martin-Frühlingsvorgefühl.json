{"textgrid.poem.32803": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsvorgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald ist die Winternacht dahin,", "tokens": ["Bald", "ist", "die", "Win\u00b7ter\u00b7nacht", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kalten Nebelwolken fliehn,", "tokens": ["Die", "kal\u00b7ten", "Ne\u00b7bel\u00b7wol\u00b7ken", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und heller lacht die Sonne.", "tokens": ["Und", "hel\u00b7ler", "lacht", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald weckt der Lenz in Hain und Flur", "tokens": ["Bald", "weckt", "der", "Lenz", "in", "Hain", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die starre, schlummernde Natur", "tokens": ["Die", "star\u00b7re", ",", "schlum\u00b7mern\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu langentbehrter Wonne.", "tokens": ["Zu", "lan\u00b7gent\u00b7behr\u00b7ter", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schon schmilzt der lockre Flockenschnee,", "tokens": ["Schon", "schmilzt", "der", "lock\u00b7re", "Flo\u00b7cken\u00b7schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald werden Blumen, Gras und Klee", "tokens": ["Bald", "wer\u00b7den", "Blu\u00b7men", ",", "Gras", "und", "Klee"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Scho\u00df der Erd' entsprie\u00dfen;", "tokens": ["Dem", "Scho\u00df", "der", "Erd'", "ent\u00b7sprie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald wird der Lerche s\u00fc\u00dfer Schall", "tokens": ["Bald", "wird", "der", "Ler\u00b7che", "s\u00fc\u00b7\u00dfer", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und der Gesang der Nachtigall", "tokens": ["Und", "der", "Ge\u00b7sang", "der", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns Harrende begr\u00fc\u00dfen.", "tokens": ["Uns", "Har\u00b7ren\u00b7de", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann eilen wir hinaus ins Feld,", "tokens": ["Dann", "ei\u00b7len", "wir", "hin\u00b7aus", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und freuen uns der sch\u00f6nen Welt", "tokens": ["Und", "freu\u00b7en", "uns", "der", "sch\u00f6\u00b7nen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Fr\u00fchlingsschmucke wieder,", "tokens": ["Im", "Fr\u00fch\u00b7lings\u00b7schmu\u00b7cke", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und laben uns in warmer Luft,", "tokens": ["Und", "la\u00b7ben", "uns", "in", "war\u00b7mer", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und atmen zarten Bl\u00fctenduft,", "tokens": ["Und", "at\u00b7men", "zar\u00b7ten", "Bl\u00fc\u00b7ten\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und singen Freudenlieder.", "tokens": ["Und", "sin\u00b7gen", "Freu\u00b7den\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann \u00f6ffnet neuer Lebenslust", "tokens": ["Dann", "\u00f6ff\u00b7net", "neu\u00b7er", "Le\u00b7bens\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich traulich auch des Dulders Brust,", "tokens": ["Sich", "trau\u00b7lich", "auch", "des", "Dul\u00b7ders", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Sorg und Gram umfangen:", "tokens": ["Von", "Sorg", "und", "Gram", "um\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn manche Hoffnung, die verblich,", "tokens": ["Denn", "man\u00b7che", "Hoff\u00b7nung", ",", "die", "ver\u00b7blich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erneuert mit den Blumen sich,", "tokens": ["Er\u00b7neu\u00b7ert", "mit", "den", "Blu\u00b7men", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die in den G\u00e4rten prangen.", "tokens": ["Die", "in", "den", "G\u00e4r\u00b7ten", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}