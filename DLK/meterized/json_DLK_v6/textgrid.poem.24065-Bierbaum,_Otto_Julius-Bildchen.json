{"textgrid.poem.24065": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Bildchen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling naht dem Sommer zu,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "naht", "dem", "Som\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein leichter Wind wiegt \u00fcber dem Gras,", "tokens": ["Ein", "leich\u00b7ter", "Wind", "wiegt", "\u00fc\u00b7ber", "dem", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Hell leuchten die Bl\u00fcten im Busche.", "tokens": ["Hell", "leuch\u00b7ten", "die", "Bl\u00fc\u00b7ten", "im", "Bu\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die Blumen im Grase nicken leis,", "tokens": ["Die", "Blu\u00b7men", "im", "Gra\u00b7se", "ni\u00b7cken", "leis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es klingt der kleine, klare Bach", "tokens": ["Es", "klingt", "der", "klei\u00b7ne", ",", "kla\u00b7re", "Bach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus schattigem Dunkel sch\u00fcchtern heraus,", "tokens": ["Aus", "schat\u00b7ti\u00b7gem", "Dun\u00b7kel", "sch\u00fcch\u00b7tern", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als k\u00e4m er vom Reiche der Tr\u00e4ume.", "tokens": ["Als", "k\u00e4m", "er", "vom", "Rei\u00b7che", "der", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NE", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Vom Reiche der Tr\u00e4ume, in dem sie weilt,", "tokens": ["Vom", "Rei\u00b7che", "der", "Tr\u00e4u\u00b7me", ",", "in", "dem", "sie", "weilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das braune M\u00e4del mit flatterndem Haar,", "tokens": ["Das", "brau\u00b7ne", "M\u00e4\u00b7del", "mit", "flat\u00b7tern\u00b7dem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die junge kr\u00e4ftige Bauerndirn.", "tokens": ["Die", "jun\u00b7ge", "kr\u00e4f\u00b7ti\u00b7ge", "Bau\u00b7ern\u00b7dirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Zwischen Fr\u00fchling und Sommer webt ihr Traum,", "tokens": ["Zwi\u00b7schen", "Fr\u00fch\u00b7ling", "und", "Som\u00b7mer", "webt", "ihr", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Zwischen Bl\u00fcte und Frucht, zwischen Hoffen und Gl\u00fcck,", "tokens": ["Zwi\u00b7schen", "Bl\u00fc\u00b7te", "und", "Frucht", ",", "zwi\u00b7schen", "Hof\u00b7fen", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und die Augen gehen ihr \u00fcber.", "tokens": ["Und", "die", "Au\u00b7gen", "ge\u00b7hen", "ihr", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}