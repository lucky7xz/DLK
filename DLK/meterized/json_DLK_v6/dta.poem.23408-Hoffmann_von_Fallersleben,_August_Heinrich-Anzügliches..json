{"dta.poem.23408": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anz\u00fcgliches.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einst machte mir mein Schneider", "tokens": ["Einst", "mach\u00b7te", "mir", "mein", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein neues Hosenpaar:", "tokens": ["Ein", "neu\u00b7es", "Ho\u00b7sen\u00b7paar", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gut, rief ich, ist's, doch leider!", "tokens": ["Gut", ",", "rief", "ich", ",", "ist's", ",", "doch", "lei\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "$,", "VAFIN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist zu eng f\u00fcrwahr.", "tokens": ["Es", "ist", "zu", "eng", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201esie w\u00fcnschen fortzuschreiten", "tokens": ["\u201e", "sie", "w\u00fcn\u00b7schen", "fort\u00b7zu\u00b7schrei\u00b7ten"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVINF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch mit dem Geist der Zeit \u2014", "tokens": ["Doch", "mit", "dem", "Geist", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Das ist zu diesen Zeiten", "tokens": ["Das", "ist", "zu", "die\u00b7sen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Mode weit und breit.\u201c", "tokens": ["Die", "Mo\u00b7de", "weit", "und", "breit", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Schneider unsrer Zeiten,", "tokens": ["Ihr", "Schnei\u00b7der", "uns\u00b7rer", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ihr so pfiffig seid!", "tokens": ["Wie", "ihr", "so", "pfif\u00b7fig", "seid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit wir vorw\u00e4rts schreiten,", "tokens": ["Da\u00b7mit", "wir", "vor\u00b7w\u00e4rts", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Macht ihr zu eng das Kleid.", "tokens": ["Macht", "ihr", "zu", "eng", "das", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}