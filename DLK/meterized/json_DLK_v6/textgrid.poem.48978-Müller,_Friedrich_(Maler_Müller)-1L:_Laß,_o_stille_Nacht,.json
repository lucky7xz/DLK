{"textgrid.poem.48978": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df, o stille Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, o stille Nacht,", "tokens": ["La\u00df", ",", "o", "stil\u00b7le", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Auf den, der harrend wacht,", "tokens": ["Auf", "den", ",", "der", "har\u00b7rend", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zwei Sternlein blicken,", "tokens": ["Zwei", "Stern\u00b7lein", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sein Herz zu begl\u00fccken,", "tokens": ["Sein", "Herz", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das in Sehnsucht bricht!", "tokens": ["Das", "in", "Sehn\u00b7sucht", "bricht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "O du S\u00fc\u00dfe, Reine,", "tokens": ["O", "du", "S\u00fc\u00b7\u00dfe", ",", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Die in Lieb ich meine,", "tokens": ["Die", "in", "Lieb", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "H\u00f6re, was bittend die Lippe spricht:", "tokens": ["H\u00f6\u00b7re", ",", "was", "bit\u00b7tend", "die", "Lip\u00b7pe", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Zaud're nicht! Zaud're nicht!", "tokens": ["Zau\u00b7d'\u00b7re", "nicht", "!", "Zau\u00b7d'\u00b7re", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "NN", "PTKNEG", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "So oft in Schlummer", "tokens": ["So", "oft", "in", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mein Herze schl\u00e4gt,", "tokens": ["Mein", "Her\u00b7ze", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nach der die Sehnsucht", "tokens": ["Nach", "der", "die", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Es mir bewegt;", "tokens": ["Es", "mir", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So oft am Tage", "tokens": ["So", "oft", "am", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Geheime Klage", "tokens": ["Ge\u00b7hei\u00b7me", "Kla\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der Seufzer tr\u00e4gt:", "tokens": ["Der", "Seuf\u00b7zer", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Dir, dir alleine,", "tokens": ["Dir", ",", "dir", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Du S\u00fc\u00dfe, Reine,", "tokens": ["Du", "S\u00fc\u00b7\u00dfe", ",", "Rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Der Seufzer gilt.", "tokens": ["Der", "Seuf\u00b7zer", "gilt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "O h\u00f6re, was bittend die Lippe spricht:", "tokens": ["O", "h\u00f6\u00b7re", ",", "was", "bit\u00b7tend", "die", "Lip\u00b7pe", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Zeige dein liebliches Angesicht!", "tokens": ["Zei\u00b7ge", "dein", "lieb\u00b7li\u00b7ches", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Leise hauchen Abendwinde", "tokens": ["Lei\u00b7se", "hau\u00b7chen", "Ab\u00b7end\u00b7win\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Bl\u00fcten Gold.", "tokens": ["Durch", "der", "Bl\u00fc\u00b7ten", "Gold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, versteh's! Sie sprechen linde:", "tokens": ["Ach", ",", "ver\u00b7steh's", "!", "Sie", "spre\u00b7chen", "lin\u00b7de", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$.", "PPER", "VVFIN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei mir hold!", "tokens": ["Sei", "mir", "hold", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Vom Himmelsblau", "tokens": ["Vom", "Him\u00b7mels\u00b7blau"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Perlet Tau;", "tokens": ["Per\u00b7let", "Tau", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sollen Tr\u00e4nen ihn mehren?", "tokens": ["Sol\u00b7len", "Tr\u00e4\u00b7nen", "ihn", "meh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "O S\u00fc\u00dfe, Reine,", "tokens": ["O", "S\u00fc\u00b7\u00dfe", ",", "Rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Du nur die Eine,", "tokens": ["Du", "nur", "die", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "F\u00fcr die ich lebe,", "tokens": ["F\u00fcr", "die", "ich", "le\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "F\u00fcr die ich strebe,", "tokens": ["F\u00fcr", "die", "ich", "stre\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Willst du mir wehren", "tokens": ["Willst", "du", "mir", "weh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Der \u00c4uglein Schein?", "tokens": ["Der", "\u00c4ug\u00b7lein", "Schein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Soll Nacht in mir, wie um mich sein?", "tokens": ["Soll", "Nacht", "in", "mir", ",", "wie", "um", "mich", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "PPER", "$,", "PWAV", "APPR", "PPER", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "O h\u00f6re, was bittend die Lippe spricht:", "tokens": ["O", "h\u00f6\u00b7re", ",", "was", "bit\u00b7tend", "die", "Lip\u00b7pe", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Neige mir milde dein Angesicht!", "tokens": ["Nei\u00b7ge", "mir", "mil\u00b7de", "dein", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ach, du schweigest! Schlummerst du? \u2013", "tokens": ["Ach", ",", "du", "schwei\u00b7gest", "!", "Schlum\u00b7merst", "du", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$.", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O walle hernieder", "tokens": ["O", "wal\u00b7le", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit leisem Gefieder", "tokens": ["Mit", "lei\u00b7sem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schlummer der Ruh;", "tokens": ["Schlum\u00b7mer", "der", "Ruh", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Siegle das liebe", "tokens": ["Sieg\u00b7le", "das", "lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Augenlid zu!", "tokens": ["Au\u00b7gen\u00b7lid", "zu", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Zaubre, ich flehe,", "tokens": ["Zaub\u00b7re", ",", "ich", "fle\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Da\u00df vor ihr stehe", "tokens": ["Da\u00df", "vor", "ihr", "ste\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Im Traum mein Bild!", "tokens": ["Im", "Traum", "mein", "Bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "O S\u00fc\u00dfe, Reine,", "tokens": ["O", "S\u00fc\u00b7\u00dfe", ",", "Rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "O schlummre mild!", "tokens": ["O", "schlumm\u00b7re", "mild", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}