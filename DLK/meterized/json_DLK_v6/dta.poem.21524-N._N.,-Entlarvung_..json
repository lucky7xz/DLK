{"dta.poem.21524": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Entlarvung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt geschwelgt in S\u00fcnden,", "tokens": ["Ihr", "habt", "ge\u00b7schwelgt", "in", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In S\u00fcnden sonder Zahl!", "tokens": ["In", "S\u00fcn\u00b7den", "son\u00b7der", "Zahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus euren Augen grinst der Tod", "tokens": ["Aus", "eu\u00b7ren", "Au\u00b7gen", "grinst", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und euer Wort ist schaal!", "tokens": ["Und", "eu\u00b7er", "Wort", "ist", "schaal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und euer Schwert zerfrist der Rost \u2014", "tokens": ["Und", "eu\u00b7er", "Schwert", "zer\u00b7frist", "der", "Rost"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dieweil mit Dirnen ihr gekost,", "tokens": ["Die\u00b7weil", "mit", "Dir\u00b7nen", "ihr", "ge\u00b7kost", ","], "token_info": ["word", "word", "word", "word", "word", "