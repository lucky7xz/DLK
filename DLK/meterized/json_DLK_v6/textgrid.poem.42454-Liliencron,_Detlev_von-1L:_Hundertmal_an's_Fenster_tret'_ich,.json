{"textgrid.poem.42454": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hundertmal an's Fenster tret' ich,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hundertmal an's Fenster tret' ich,", "tokens": ["Hun\u00b7dert\u00b7mal", "an's", "Fens\u00b7ter", "tret'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Stra\u00dfen weit zu schauen,", "tokens": ["In", "die", "Stra\u00b7\u00dfen", "weit", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer, immer noch vergebens,", "tokens": ["Im\u00b7mer", ",", "im\u00b7mer", "noch", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, in aller Welt wo bleibt er,", "tokens": ["Ach", ",", "in", "al\u00b7ler", "Welt", "wo", "bleibt", "er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PIAT", "NN", "PWAV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleibt der Bernhardinerhund.", "tokens": ["Bleibt", "der", "Bern\u00b7har\u00b7di\u00b7ner\u00b7hund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Endlich, endlich um die Ecke", "tokens": ["End\u00b7lich", ",", "end\u00b7lich", "um", "die", "E\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Patscht auf w\u00fcrdevollen Tatzen,", "tokens": ["Patscht", "auf", "w\u00fcr\u00b7de\u00b7vol\u00b7len", "Tat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Patscht, die Fahne h\u00e4ngen lassend,", "tokens": ["Patscht", ",", "die", "Fah\u00b7ne", "h\u00e4n\u00b7gen", "las\u00b7send", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Patscht ein gelb und wei\u00df gefleckter", "tokens": ["Patscht", "ein", "gelb", "und", "wei\u00df", "ge\u00b7fleck\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJD", "KON", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ernster Bernhardinerhund.", "tokens": ["Erns\u00b7ter", "Bern\u00b7har\u00b7di\u00b7ner\u00b7hund", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Neben ihm, mit leichten Schritten,", "tokens": ["Ne\u00b7ben", "ihm", ",", "mit", "leich\u00b7ten", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreitet, sommerlich gekleidet,", "tokens": ["Schrei\u00b7tet", ",", "som\u00b7mer\u00b7lich", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine junge, zarte Sch\u00f6ne,", "tokens": ["Ei\u00b7ne", "jun\u00b7ge", ",", "zar\u00b7te", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr H\u00e4ndchen f\u00fchrt am Halsband", "tokens": ["Und", "ihr", "H\u00e4nd\u00b7chen", "f\u00fchrt", "am", "Hals\u00b7band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihren Bernhardinerhund.", "tokens": ["Ih\u00b7ren", "Bern\u00b7har\u00b7di\u00b7ner\u00b7hund", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bald im Zimmer steht das M\u00e4dchen,", "tokens": ["Bald", "im", "Zim\u00b7mer", "steht", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wir halten uns umschlungen.", "tokens": ["Und", "wir", "hal\u00b7ten", "uns", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen uns dr\u00e4ngt seine Schnauze,", "tokens": ["Zwi\u00b7schen", "uns", "dr\u00e4ngt", "sei\u00b7ne", "Schnau\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wedelnd, hechelnd, jener ernste,", "tokens": ["We\u00b7delnd", ",", "he\u00b7chelnd", ",", "je\u00b7ner", "erns\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "PDAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Treue Bernhardinerhund.", "tokens": ["Treu\u00b7e", "Bern\u00b7har\u00b7di\u00b7ner\u00b7hund", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}