{"textgrid.poem.23857": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Schwestern, meine Br\u00fcder, wollt ihr", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Schwestern, meine Br\u00fcder, wollt ihr", "tokens": ["Mei\u00b7ne", "Schwes\u00b7tern", ",", "mei\u00b7ne", "Br\u00fc\u00b7der", ",", "wollt", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit mir gehn in meinen gro\u00dfen Garten?", "tokens": ["Mit", "mir", "gehn", "in", "mei\u00b7nen", "gro\u00b7\u00dfen", "Gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kommt! Ich lad euch ein. Weit steht er offen.", "tokens": ["Kommt", "!", "Ich", "lad", "euch", "ein", ".", "Weit", "steht", "er", "of\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Freude nenn ichs, wenn ich G\u00e4ste habe,", "tokens": ["Freu\u00b7de", "nenn", "ichs", ",", "wenn", "ich", "G\u00e4s\u00b7te", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und mir kann nichts besseres geschehen,", "tokens": ["Und", "mir", "kann", "nichts", "bes\u00b7se\u00b7res", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "ADJA", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Als ein bischen Dank aus euren Herzen.", "tokens": ["Als", "ein", "bi\u00b7schen", "Dank", "aus", "eu\u00b7ren", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Glaubt, ich wei\u00df: Es giebt viel sch\u00f6nere G\u00e4rten,", "tokens": ["Glaubt", ",", "ich", "wei\u00df", ":", "Es", "giebt", "viel", "sch\u00f6\u00b7ne\u00b7re", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Alte, von den Meistern angelegte,", "tokens": ["Al\u00b7te", ",", "von", "den", "Meis\u00b7tern", "an\u00b7ge\u00b7leg\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die in bessren Zeiten freier bauten,", "tokens": ["Die", "in", "bess\u00b7ren", "Zei\u00b7ten", "frei\u00b7er", "bau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00f6nige der Kunst und gro\u00dfe Herren.", "tokens": ["K\u00f6\u00b7ni\u00b7ge", "der", "Kunst", "und", "gro\u00b7\u00dfe", "Her\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Diese G\u00e4rten werden immer sch\u00f6ner,", "tokens": ["Die\u00b7se", "G\u00e4r\u00b7ten", "wer\u00b7den", "im\u00b7mer", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Denn es liegt der Glanz der gro\u00dfen Zeiten", "tokens": ["Denn", "es", "liegt", "der", "Glanz", "der", "gro\u00b7\u00dfen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ueber ihnen, und in ihrem Erdreich", "tokens": ["Ue\u00b7ber", "ih\u00b7nen", ",", "und", "in", "ih\u00b7rem", "Er\u00b7dreich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Ist die Kraft lebendig ersten Samens.", "tokens": ["Ist", "die", "Kraft", "le\u00b7ben\u00b7dig", "ers\u00b7ten", "Sa\u00b7mens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Heiligt\u00fcmer sind es unsrer Freude,", "tokens": ["Hei\u00b7lig\u00b7t\u00fc\u00b7mer", "sind", "es", "uns\u00b7rer", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Wo schon unsre V\u00e4ter heiter gingen,", "tokens": ["Wo", "schon", "uns\u00b7re", "V\u00e4\u00b7ter", "hei\u00b7ter", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Unsre M\u00fctter, eh sie uns geboren,", "tokens": ["Uns\u00b7re", "M\u00fct\u00b7ter", ",", "eh", "sie", "uns", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Sich den Blumen l\u00e4chelnd nieder neigten,", "tokens": ["Sich", "den", "Blu\u00b7men", "l\u00e4\u00b7chelnd", "nie\u00b7der", "neig\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Die noch heute ihren Duft uns schenken.", "tokens": ["Die", "noch", "heu\u00b7te", "ih\u00b7ren", "Duft", "uns", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und ich lad euch, meine lieben Schwestern,", "tokens": ["Und", "ich", "lad", "euch", ",", "mei\u00b7ne", "lie\u00b7ben", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lieben Br\u00fcder dennoch ein, zu kommen", "tokens": ["Lie\u00b7ben", "Br\u00fc\u00b7der", "den\u00b7noch", "ein", ",", "zu", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "ADV", "PTKVZ", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und in meinem Garten froh zu wandeln.", "tokens": ["Und", "in", "mei\u00b7nem", "Gar\u00b7ten", "froh", "zu", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJD", "PTKZU", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine Einsamkeit sehnt sich nach G\u00e4sten,", "tokens": ["Mei\u00b7ne", "Ein\u00b7sam\u00b7keit", "sehnt", "sich", "nach", "G\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Meine Blumen wollen sich verschenken,", "tokens": ["Mei\u00b7ne", "Blu\u00b7men", "wol\u00b7len", "sich", "ver\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Meine vielgewundenen Wege wollen", "tokens": ["Mei\u00b7ne", "viel\u00b7ge\u00b7wun\u00b7de\u00b7nen", "We\u00b7ge", "wol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.7": {"text": "Nicht blo\u00df mich in Busch und Schatten f\u00fchren,", "tokens": ["Nicht", "blo\u00df", "mich", "in", "Busch", "und", "Schat\u00b7ten", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mich, der diesem Garten fremd geworden.", "tokens": ["Mich", ",", "der", "die\u00b7sem", "Gar\u00b7ten", "fremd", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PDAT", "NN", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Denn es ist der Garten meiner Jugend.", "tokens": ["Denn", "es", "ist", "der", "Gar\u00b7ten", "mei\u00b7ner", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ich bin selber nicht mehr hier zuhause;", "tokens": ["Ich", "bin", "sel\u00b7ber", "nicht", "mehr", "hier", "zu\u00b7hau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nur ein Gast noch, und ein seltner, bin ich", "tokens": ["Nur", "ein", "Gast", "noch", ",", "und", "ein", "selt\u00b7ner", ",", "bin", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "KON", "ART", "ADJA", "$,", "VAFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Diesen G\u00e4ngen, diesen Wiesen, Beeten", "tokens": ["Die\u00b7sen", "G\u00e4n\u00b7gen", ",", "die\u00b7sen", "Wie\u00b7sen", ",", "Bee\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und Geb\u00fcschen, und Verwundern fast mich", "tokens": ["Und", "Ge\u00b7b\u00fc\u00b7schen", ",", "und", "Ver\u00b7wun\u00b7dern", "fast", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "KON", "NN", "ADV", "PPER"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Immer, wenn ich durch den Garten schreite.", "tokens": ["Im\u00b7mer", ",", "wenn", "ich", "durch", "den", "Gar\u00b7ten", "schrei\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Manchmal wol auch R\u00fchrung, manchmal Aerger;", "tokens": ["Manch\u00b7mal", "wol", "auch", "R\u00fch\u00b7rung", ",", "manch\u00b7mal", "A\u00b7er\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Diese Blume seh ich l\u00e4chelnd an, und jene", "tokens": ["Die\u00b7se", "Blu\u00b7me", "seh", "ich", "l\u00e4\u00b7chelnd", "an", ",", "und", "je\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "PDS"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "M\u00f6cht ich lieber aus dem Erdreich heben;", "tokens": ["M\u00f6cht", "ich", "lie\u00b7ber", "aus", "dem", "Er\u00b7dreich", "he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hier ein Weg, den ich mit Lust verfolge,", "tokens": ["Hier", "ein", "Weg", ",", "den", "ich", "mit", "Lust", "ver\u00b7fol\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dort ein Pfad, verloren in Gestr\u00fcppen,", "tokens": ["Dort", "ein", "Pfad", ",", "ver\u00b7lo\u00b7ren", "in", "Ge\u00b7str\u00fcp\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Den ich gern versch\u00fcttete. Doch immer", "tokens": ["Den", "ich", "gern", "ver\u00b7sch\u00fct\u00b7te\u00b7te", ".", "Doch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "ADV", "VVFIN", "$.", "KON", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wehr ich ab den W\u00fcnschen: Mag es bleiben,", "tokens": ["Wehr", "ich", "ab", "den", "W\u00fcn\u00b7schen", ":", "Mag", "es", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$.", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie es, unbewust halb, einst geworden.", "tokens": ["Wie", "es", ",", "un\u00b7be\u00b7wust", "halb", ",", "einst", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJD", "ADJD", "$,", "ADV", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wollt ich diesen Garten neu bebauen,", "tokens": ["Wollt", "ich", "die\u00b7sen", "Gar\u00b7ten", "neu", "be\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Keine Zeit f\u00e4nd ich f\u00fcr meinen neuen, \u2013", "tokens": ["Kei\u00b7ne", "Zeit", "f\u00e4nd", "ich", "f\u00fcr", "mei\u00b7nen", "neu\u00b7en", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, vielleicht auch keine Lust. Er bleibe,", "tokens": ["Ach", ",", "viel\u00b7leicht", "auch", "kei\u00b7ne", "Lust", ".", "Er", "blei\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "PIAT", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie er ist. Und schenkt er meinen G\u00e4sten", "tokens": ["Wie", "er", "ist", ".", "Und", "schenkt", "er", "mei\u00b7nen", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "$.", "KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nur ein Hundertteil der Freude, die er", "tokens": ["Nur", "ein", "Hun\u00b7dert\u00b7teil", "der", "Freu\u00b7de", ",", "die", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mir geschenkt, als ich ihn einstens baute,", "tokens": ["Mir", "ge\u00b7schenkt", ",", "als", "ich", "ihn", "eins\u00b7tens", "bau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ist er doch ein rechter Freudengarten.", "tokens": ["Ist", "er", "doch", "ein", "rech\u00b7ter", "Freu\u00b7den\u00b7gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Denn ich habe ihn mit Lust und Schmerzen,", "tokens": ["Denn", "ich", "ha\u00b7be", "ihn", "mit", "Lust", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die der Freuden allertiefste waren,", "tokens": ["Die", "der", "Freu\u00b7den", "al\u00b7ler\u00b7tiefs\u00b7te", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Angebaut auf meinem eignen Lande,", "tokens": ["An\u00b7ge\u00b7baut", "auf", "mei\u00b7nem", "eig\u00b7nen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf dem Mutterboden meines Lebens;", "tokens": ["Auf", "dem", "Mut\u00b7ter\u00b7bo\u00b7den", "mei\u00b7nes", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Habe ihn gespeist mit meinem Blute,", "tokens": ["Ha\u00b7be", "ihn", "ge\u00b7speist", "mit", "mei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Habe ihn gehegt mit meinem Herzen,", "tokens": ["Ha\u00b7be", "ihn", "ge\u00b7hegt", "mit", "mei\u00b7nem", "Her\u00b