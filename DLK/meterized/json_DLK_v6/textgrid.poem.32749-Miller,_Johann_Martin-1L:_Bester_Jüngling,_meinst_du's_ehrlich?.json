{"textgrid.poem.32749": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bester J\u00fcngling, meinst du's ehrlich?", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bester J\u00fcngling, meinst du's ehrlich?", "tokens": ["Bes\u00b7ter", "J\u00fcng\u00b7ling", ",", "meinst", "du's", "ehr\u00b7lich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "PIS", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O so bin ich deine Braut.", "tokens": ["O", "so", "bin", "ich", "dei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, Himmel! wie gef\u00e4hrlich", "tokens": ["A\u00b7ber", ",", "Him\u00b7mel", "!", "wie", "ge\u00b7f\u00e4hr\u00b7lich"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$.", "PWAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird auf J\u00fcnglingswort gebaut!", "tokens": ["Wird", "auf", "J\u00fcng\u00b7lings\u00b7wort", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis ihr unser Ja erlauschet,", "tokens": ["Bis", "ihr", "un\u00b7ser", "Ja", "er\u00b7lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seid ihr alle fromm und gut;", "tokens": ["Seid", "ihr", "al\u00b7le", "fromm", "und", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber dann, ach dann! vertauschet", "tokens": ["A\u00b7ber", "dann", ",", "ach", "dann", "!", "ver\u00b7tau\u00b7schet"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "ITJ", "ADV", "$.", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihr den sanften L\u00e4mmermut.", "tokens": ["Ihr", "den", "sanf\u00b7ten", "L\u00e4m\u00b7mer\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leben, Ehre, Gl\u00fcck und Habe", "tokens": ["Le\u00b7ben", ",", "Eh\u00b7re", ",", "Gl\u00fcck", "und", "Ha\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trau' ich deinen H\u00e4nden an,", "tokens": ["Trau'", "ich", "dei\u00b7nen", "H\u00e4n\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin von nun an bis zum Grabe", "tokens": ["Bin", "von", "nun", "an", "bis", "zum", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADV", "APPR", "APPR", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Dir mit Liebe zugethan,", "tokens": ["Dir", "mit", "Lie\u00b7be", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df in ihren alten Tagen", "tokens": ["La\u00df", "in", "ih\u00b7ren", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine fromme Mutter hier;", "tokens": ["Mei\u00b7ne", "from\u00b7me", "Mut\u00b7ter", "hier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Freud' und Leid mit dir zu tragen,", "tokens": ["Freud'", "und", "Leid", "mit", "dir", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Folg' ich, bester J\u00fcngling, dir.", "tokens": ["Fol\u00b7g'", "ich", ",", "bes\u00b7ter", "J\u00fcng\u00b7ling", ",", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O, du kannst mich nicht ber\u00fccken;", "tokens": ["O", ",", "du", "kannst", "mich", "nicht", "be\u00b7r\u00fc\u00b7cken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder Tugend w\u00e4re Tand,", "tokens": ["O\u00b7der", "Tu\u00b7gend", "w\u00e4\u00b7re", "Tand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dies Herz in deinen Blicken", "tokens": ["Und", "dies", "Herz", "in", "dei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00f6g' im himmlischen Gewand!", "tokens": ["Tr\u00f6g'", "im", "himm\u00b7li\u00b7schen", "Ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, ich glaube diesem Schweigen,", "tokens": ["Ja", ",", "ich", "glau\u00b7be", "die\u00b7sem", "Schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diesen Thr\u00e4nen, diesem Blick!", "tokens": ["Die\u00b7sen", "Thr\u00e4\u00b7nen", ",", "die\u00b7sem", "Blick", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Erd' und Himmel sollen zeugen,", "tokens": ["Erd'", "und", "Him\u00b7mel", "sol\u00b7len", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weich' ich je von dir zur\u00fcck!", "tokens": ["Weich'", "ich", "je", "von", "dir", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}