{"textgrid.poem.42084": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "3. Opuntia Monacantha", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich nahte mich, wie einem frommen Brot", "tokens": ["Ich", "nah\u00b7te", "mich", ",", "wie", "ei\u00b7nem", "from\u00b7men", "Brot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Pilger naht, mit sehnsuchtvollem Munde.", "tokens": ["Ein", "Pil\u00b7ger", "naht", ",", "mit", "sehn\u00b7sucht\u00b7vol\u00b7lem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du stie\u00dfest ihn, Dir aufgetane Wunde,", "tokens": ["Du", "stie\u00b7\u00dfest", "ihn", ",", "Dir", "auf\u00b7ge\u00b7ta\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In eine tiefre nie gestillte Not:", "tokens": ["In", "ei\u00b7ne", "tief\u00b7re", "nie", "ge\u00b7still\u00b7te", "Not", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du h\u00f6hntest ihn mit \u00fcbernommner H\u00fclle", "tokens": ["Du", "h\u00f6hn\u00b7test", "ihn", "mit", "\u00fc\u00b7ber\u00b7nomm\u00b7ner", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Saft und Speisung, bitter bis zum Rand,", "tokens": ["Von", "Saft", "und", "Spei\u00b7sung", ",", "bit\u00b7ter", "bis", "zum", "Rand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O bittre Frucht! Der Mund, der Dich im Brand", "tokens": ["O", "bitt\u00b7re", "Frucht", "!", "Der", "Mund", ",", "der", "Dich", "im", "Brand"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Einmal empfing, sieh, er verlangt die F\u00fclle", "tokens": ["Ein\u00b7mal", "emp\u00b7fing", ",", "sieh", ",", "er", "ver\u00b7langt", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Von Bitterkeit wie S\u00fc\u00dfe; widersteht", "tokens": ["Von", "Bit\u00b7ter\u00b7keit", "wie", "S\u00fc\u00b7\u00dfe", ";", "wi\u00b7der\u00b7steht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KOKOM", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Keiner Erfahrung mehr: Er kommt und m\u00fcndet", "tokens": ["Kei\u00b7ner", "Er\u00b7fah\u00b7rung", "mehr", ":", "Er", "kommt", "und", "m\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "D\u00fcrstend in Dich und nimmt und trinkt und geht", "tokens": ["D\u00fcrs\u00b7tend", "in", "Dich", "und", "nimmt", "und", "trinkt", "und", "geht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Von Dir und ist so ganz mit Schmerz versehrt,", "tokens": ["Von", "Dir", "und", "ist", "so", "ganz", "mit", "Schmerz", "ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er wie ein Bese\u00dfner sich entz\u00fcndet", "tokens": ["Da\u00df", "er", "wie", "ein", "Be\u00b7se\u00df\u00b7ner", "sich", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Neu aus sich selbst und endlos wiederkehrt.", "tokens": ["Neu", "aus", "sich", "selbst", "und", "end\u00b7los", "wie\u00b7der\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "ADV", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}