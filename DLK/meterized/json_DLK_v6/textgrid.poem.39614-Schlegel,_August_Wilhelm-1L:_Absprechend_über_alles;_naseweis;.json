{"textgrid.poem.39614": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Absprechend \u00fcber alles; naseweis;", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Absprechend \u00fcber alles; naseweis;", "tokens": ["Ab\u00b7spre\u00b7chend", "\u00fc\u00b7ber", "al\u00b7les", ";", "na\u00b7se\u00b7weis", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "PIS", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein kleiner litterarischer Scherwenzel;", "tokens": ["Ein", "klei\u00b7ner", "lit\u00b7te\u00b7ra\u00b7ri\u00b7scher", "Scher\u00b7wen\u00b7zel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ein Springinsfeld, der, was er irgend wei\u00df,", "tokens": ["Ein", "Sprin\u00b7gins\u00b7feld", ",", "der", ",", "was", "er", "ir\u00b7gend", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bequemlich tr\u00e4gt in seinem Burschen-R\u00e4nzel;", "tokens": ["Be\u00b7quem\u00b7lich", "tr\u00e4gt", "in", "sei\u00b7nem", "Bur\u00b7schen\u00b7R\u00e4n\u00b7zel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So dr\u00e4ngt er sich in edler Meister Krei\u00df,", "tokens": ["So", "dr\u00e4ngt", "er", "sich", "in", "ed\u00b7ler", "Meis\u00b7ter", "Krei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und zupft aus ihren Lorbeern sich ein Kr\u00e4nzel.", "tokens": ["Und", "zupft", "aus", "ih\u00b7ren", "Lor\u00b7beern", "sich", "ein", "Kr\u00e4n\u00b7zel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie hei\u00dft er doch? Der Nam' entfiel mir. \u2013 \u2013", "tokens": ["Wie", "hei\u00dft", "er", "doch", "?", "Der", "Nam'", "ent\u00b7fiel", "mir", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VVFIN", "PPER", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}