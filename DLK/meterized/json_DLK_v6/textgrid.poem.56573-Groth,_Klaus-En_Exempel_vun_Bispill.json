{"textgrid.poem.56573": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "En Exempel vun Bispill", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Mann de wull liggn,", "tokens": ["De", "Mann", "de", "wull", "liggn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "De Kater wull singn.", "tokens": ["De", "Ka\u00b7ter", "wull", "singn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Do neem he den Kater", "tokens": ["Do", "neem", "he", "den", "Ka\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Un smeet em int Water:", "tokens": ["Un", "smeet", "em", "int", "Wa\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+----", "measure": "dactylic.init"}, "line.5": {"text": "Ik will di doch wisen", "tokens": ["Ik", "will", "di", "doch", "wi\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "NE", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wull Herr in min H\u00fcsen!", "tokens": ["Wull", "Herr", "in", "min", "H\u00fc\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Do legg he sik dal", "tokens": ["Do", "legg", "he", "sik", "dal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+---", "measure": "dactylic.init"}, "line.8": {"text": "Un sleep as en Pahl.", "tokens": ["Un", "sleep", "as", "en", "Pahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Do keemn se ganz lisen", "tokens": ["Do", "ke\u00b7emn", "se", "ganz", "li\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "VVINF"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "In Sch\u00fcn un in H\u00fcsen", "tokens": ["In", "Sch\u00fcn", "un", "in", "H\u00fc\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "FM", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Un pipen so lise", "tokens": ["Un", "pi\u00b7pen", "so", "li\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Un gnappern de M\u00fcse,", "tokens": ["Un", "gnap\u00b7pern", "de", "M\u00fc\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Un gnippen un gnappen", "tokens": ["Un", "gnip\u00b7pen", "un", "gnap\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Un slicken un slappen", "tokens": ["Un", "sli\u00b7cken", "un", "slap\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Op B\u00f6r' un in Schappen", "tokens": ["Op", "B\u00f6r'", "un", "in", "Schap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "FM", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Vun Sch\u00fcttel un Teller,", "tokens": ["Vun", "Sch\u00fct\u00b7tel", "un", "Tel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "FM", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "To B\u0153n un in Keller.", "tokens": ["To", "B\u0153n", "un", "in", "Kel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Se eten sin Speck,", "tokens": ["Se", "e\u00b7ten", "sin", "Speck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Toreten sin S\u00e4ck,", "tokens": ["To\u00b7re\u00b7ten", "sin", "S\u00e4ck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Se eten sin Metten", "tokens": ["Se", "e\u00b7ten", "sin", "Met\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Un keemn in sin Betten:", "tokens": ["Un", "ke\u00b7emn", "in", "sin", "Bet\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.22": {"text": "Dar beten de \u00c6s", "tokens": ["Dar", "be\u00b7ten", "de", "\u00c6s"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NE", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "Den Mann inne N\u0153s!", "tokens": ["Den", "Mann", "in\u00b7ne", "N\u0153s", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}