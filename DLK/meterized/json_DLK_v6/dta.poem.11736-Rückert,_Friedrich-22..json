{"dta.poem.11736": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr die Erd' entehrt, zu geben Gott die Ehre!", "tokens": ["Die", "ihr", "die", "Erd'", "ent\u00b7ehrt", ",", "zu", "ge\u00b7ben", "Gott", "die", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,", "PTKZU", "VVINF", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein schlechtes Zeugnis gebt ihr selber eurer Lehre.", "tokens": ["Ein", "schlech\u00b7tes", "Zeug\u00b7nis", "gebt", "ihr", "sel\u00b7ber", "eu\u00b7rer", "Leh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gott selbst in Ehren will die Welt gehalten wissen,", "tokens": ["Gott", "selbst", "in", "Eh\u00b7ren", "will", "die", "Welt", "ge\u00b7hal\u00b7ten", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "VMFIN", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst h\u00e4tte sie sein Wort um Nichts dem Nichts entrissen.", "tokens": ["Sonst", "h\u00e4t\u00b7te", "sie", "sein", "Wort", "um", "Nichts", "dem", "Nichts", "ent\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PIS", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er hat sie hell gemacht, ihr wollt sie finster machen;", "tokens": ["Er", "hat", "sie", "hell", "ge\u00b7macht", ",", "ihr", "wollt", "sie", "fins\u00b7ter", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat an Menschen Lust, an W\u00fcrmern ihr und Drachen.", "tokens": ["Er", "hat", "an", "Men\u00b7schen", "Lust", ",", "an", "W\u00fcr\u00b7mern", "ihr", "und", "Dra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "$,", "APPR", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Halb Drachen feuerspeind, halb angstgewundne W\u00fcrmer,", "tokens": ["Halb", "Dra\u00b7chen", "feu\u00b7er\u00b7speind", ",", "halb", "angst\u00b7ge\u00b7wund\u00b7ne", "W\u00fcr\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJD", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des ird'schen Heiligthums der Dichtkunst Bilderst\u00fcrmer!", "tokens": ["Des", "ird'\u00b7schen", "Hei\u00b7lig\u00b7thums", "der", "Dicht\u00b7kunst", "Bil\u00b7der\u00b7st\u00fcr\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O Zeit! da\u00df scheulos sich ans Tagslicht wagen Eulen,", "tokens": ["O", "Zeit", "!", "da\u00df", "scheu\u00b7los", "sich", "ans", "Tags\u00b7licht", "wa\u00b7gen", "Eu\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "ADJD", "PRF", "APPRART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und siegreich Nachtigall-Ges\u00e4nge niederheulen!", "tokens": ["Und", "sieg\u00b7reich", "Nach\u00b7ti\u00b7gall\u00b7Ge\u00b7s\u00e4n\u00b7ge", "nie\u00b7der\u00b7heu\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die sehn in Rafaels Verkl\u00e4rung Teufelsfratzen,", "tokens": ["Die", "sehn", "in", "Ra\u00b7faels", "Ver\u00b7kl\u00e4\u00b7rung", "Teu\u00b7fels\u00b7frat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NE", "NN", "NN", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Und, Bilder vom Scheol im Herzen, Liebe schwatzen!", "tokens": ["Und", ",", "Bil\u00b7der", "vom", "Sche\u00b7ol", "im", "Her\u00b7zen", ",", "Lie\u00b7be", "schwat\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "APPRART", "NN", "APPRART", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Macht euch zur Lust nur Qual, und schwelgt im Jammerthal,", "tokens": ["Macht", "euch", "zur", "Lust", "nur", "Qual", ",", "und", "schwelgt", "im", "Jam\u00b7mer\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ADV", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und nie licht' eure Nacht ein Gottes Freudenstral!", "tokens": ["Und", "nie", "licht'", "eu\u00b7re", "Nacht", "ein", "Got\u00b7tes", "Freu\u00b7den\u00b7stral", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Lehre, die nicht rein das Herz wie Sonnenschein", "tokens": ["Die", "Leh\u00b7re", ",", "die", "nicht", "rein", "das", "Herz", "wie", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "ART", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erf\u00fcllt, erfreut, erhebt, kann nicht vom Himmel seyn.", "tokens": ["Er\u00b7f\u00fcllt", ",", "er\u00b7freut", ",", "er\u00b7hebt", ",", "kann", "nicht", "vom", "Him\u00b7mel", "seyn", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VMFIN", "PTKNEG", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}