{"dta.poem.5371": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So z\u00f6gert, liebste Menschen, nicht,", "tokens": ["So", "z\u00f6\u00b7gert", ",", "liebs\u00b7te", "Men\u00b7schen", ",", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um dieses helle Gnaden-Licht", "tokens": ["Um", "die\u00b7ses", "hel\u00b7le", "Gna\u00b7den\u00b7Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Sch\u00f6pfer br\u00fcnstig anzuflehen.", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "br\u00fcns\u00b7tig", "an\u00b7zu\u00b7fle\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil, wo ihr diesen Strahl nicht f\u00fchlet,", "tokens": ["Weil", ",", "wo", "ihr", "die\u00b7sen", "Strahl", "nicht", "f\u00fch\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "PDAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr, ohn euch einst vergn\u00fcgt zu sehen,", "tokens": ["Ihr", ",", "ohn", "euch", "einst", "ver\u00b7gn\u00fcgt", "zu", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "PPER", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Sorgen und Begierden w\u00fchlet,", "tokens": ["In", "Sor\u00b7gen", "und", "Be\u00b7gier\u00b7den", "w\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die weder Grund noch Gr\u00e4ntzen haben.", "tokens": ["Die", "we\u00b7der", "Grund", "noch", "Gr\u00e4nt\u00b7zen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "NN", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erbittet doch, vor allen Gaben,", "tokens": ["Er\u00b7bit\u00b7tet", "doch", ",", "vor", "al\u00b7len", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die, welche w\u00fcrdiger als alle,", "tokens": ["Die", ",", "wel\u00b7che", "w\u00fcr\u00b7di\u00b7ger", "als", "al\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADJD", "KOKOM", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Gabe, da\u00df euch GOTT gefalle!", "tokens": ["Die", "Ga\u00b7be", ",", "da\u00df", "euch", "GoTT", "ge\u00b7fal\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Jhr seyd begl\u00fcckt, wenn euch das Licht", "tokens": ["Ihr", "seyd", "be\u00b7gl\u00fcckt", ",", "wenn", "euch", "das", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Anmuth, aus dem Bau der Welt,", "tokens": ["Der", "An\u00b7muth", ",", "aus", "dem", "Bau", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den GOTT schuf, in die Seele f\u00e4llt;", "tokens": ["Den", "GoTT", "schuf", ",", "in", "die", "See\u00b7le", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Ungl\u00fccklich, wenn es nicht geschicht!", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", ",", "wenn", "es", "nicht", "ge\u00b7schicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}