{"dta.poem.10250": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Four in hand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Vorne vier nickende Pferdek\u00f6pfe,             ", "tokens": ["Vor\u00b7ne", "vier", "ni\u00b7cken\u00b7de", "Pfer\u00b7de\u00b7k\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Neben mir zwei blonde M\u00e4dchenz\u00f6pfe,", "tokens": ["Ne\u00b7ben", "mir", "zwei", "blon\u00b7de", "M\u00e4d\u00b7chen\u00b7z\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hinten der Groom mit wichtigen Mienen,", "tokens": ["Hin\u00b7ten", "der", "Groom", "mit", "wich\u00b7ti\u00b7gen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "An den R\u00e4dern Gebell.", "tokens": ["An", "den", "R\u00e4\u00b7dern", "Ge\u00b7bell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "In den D\u00f6rfern windstillen Lebens Gen\u00fcge,", "tokens": ["In", "den", "D\u00f6r\u00b7fern", "wind\u00b7stil\u00b7len", "Le\u00b7bens", "Ge\u00b7n\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Auf den Feldern flei\u00dfige Eggen und Pfl\u00fcge,", "tokens": ["Auf", "den", "Fel\u00b7dern", "flei\u00b7\u00dfi\u00b7ge", "Eg\u00b7gen", "und", "Pfl\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Alles das von der Sonne beschienen", "tokens": ["Al\u00b7les", "das", "von", "der", "Son\u00b7ne", "be\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "APPR", "ART", "NN", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "So hell, so hell.", "tokens": ["So", "hell", ",", "so", "hell", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}