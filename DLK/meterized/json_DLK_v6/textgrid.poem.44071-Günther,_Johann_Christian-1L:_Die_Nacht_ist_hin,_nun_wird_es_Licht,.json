{"textgrid.poem.44071": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht ist hin, nun wird es Licht,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist hin, nun wird es Licht,", "tokens": ["Die", "Nacht", "ist", "hin", ",", "nun", "wird", "es", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da Jacobs Stern die Wolcken bricht.", "tokens": ["Da", "Ja\u00b7cobs", "Stern", "die", "Wol\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr V\u00f6lcker, hebt die H\u00e4upter auf", "tokens": ["Ihr", "V\u00f6l\u00b7cker", ",", "hebt", "die", "H\u00e4up\u00b7ter", "auf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und merckt der goldnen Zeiten Lauf!", "tokens": ["Und", "merckt", "der", "gold\u00b7nen", "Zei\u00b7ten", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du s\u00fc\u00dfer Zweig aus Je\u00dfe Stamm,", "tokens": ["Du", "s\u00fc\u00b7\u00dfer", "Zweig", "aus", "Je\u00b7\u00dfe", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Heil, mein F\u00fcrst, mein Schaz, mein Lamm,", "tokens": ["Mein", "Heil", ",", "mein", "F\u00fcrst", ",", "mein", "Schaz", ",", "mein", "Lamm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, schau doch hier mit Freuden her,", "tokens": ["Ach", ",", "schau", "doch", "hier", "mit", "Freu\u00b7den", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie mein Herz die Wiege w\u00e4r!", "tokens": ["Wie", "mein", "Herz", "die", "Wie\u00b7ge", "w\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach komm doch, liebster Seelenschaz!", "tokens": ["Ach", "komm", "doch", ",", "liebs\u00b7ter", "See\u00b7len\u00b7schaz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Glaube macht dir reinen Plaz,", "tokens": ["Der", "Glau\u00b7be", "macht", "dir", "rei\u00b7nen", "Plaz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe steckt das Feuer an,", "tokens": ["Die", "Lie\u00b7be", "steckt", "das", "Feu\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das auch den Stall erleuchten kan.", "tokens": ["Das", "auch", "den", "Stall", "er\u00b7leuch\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr T\u00f6chter Salems, k\u00fcst den Sohn!", "tokens": ["Ihr", "T\u00f6ch\u00b7ter", "Sa\u00b7lems", ",", "k\u00fcst", "den", "Sohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des H\u00f6chsten Liebe brennet schon.", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Lie\u00b7be", "bren\u00b7net", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt, k\u00fcst das Kind! Es stillt den Zorn.", "tokens": ["Kommt", ",", "k\u00fcst", "das", "Kind", "!", "Es", "stillt", "den", "Zorn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, nun erhebt der Herr mein Horn!", "tokens": ["Ach", ",", "nun", "er\u00b7hebt", "der", "Herr", "mein", "Horn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}