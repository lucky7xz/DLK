, "line.8": {"text": "Der Saft schon! Knospe zeigt", "tokens": ["Der", "Saft", "schon", "!", "Knos\u00b7pe", "zeigt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Fr\u00fchlingszeiten!", "tokens": ["Fr\u00fch\u00b7lings\u00b7zei\u00b7ten", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Hebt Euer Haupt!", "tokens": ["Hebt", "Eu\u00b7er", "Haupt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Umlaubt, umlaubt", "tokens": ["Um\u00b7laubt", ",", "um\u00b7laubt"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Mit Fr\u00fchling ist, wer an ihn glaubt.", "tokens": ["Mit", "Fr\u00fch\u00b7ling", "ist", ",", "wer", "an", "ihn", "glaubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$,", "PWS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Trunkne Knechte, sieh! sie schlagen", "tokens": ["Trunk\u00b7ne", "Knech\u00b7te", ",", "sieh", "!", "sie", "schla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVIMP", "$.", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Br\u00fcder Mitknecht', h\u00f6hnen, plagen,", "tokens": ["Die", "Br\u00fc\u00b7der", "Mit\u00b7knecht'", ",", "h\u00f6h\u00b7nen", ",", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Statt Labung, sie mit Drang und Spott.", "tokens": ["Statt", "La\u00b7bung", ",", "sie", "mit", "Drang", "und", "Spott", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Meinst Du, da\u00df der K\u00f6nig werde", "tokens": ["Meinst", "Du", ",", "da\u00df", "der", "K\u00f6\u00b7nig", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch ", "tokens": ["Noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Wer ist sich selbst nicht Herr und Gott?", "tokens": ["Wer", "ist", "sich", "selbst", "nicht", "Herr", "und", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "ADV", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bber kommt noch lange nicht!", "tokens": ["\u00bb", "er", "kommt", "noch", "lan\u00b7ge", "nicht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Vielleicht kommt gar er nicht!", "tokens": ["Viel\u00b7leicht", "kommt", "gar", "er", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Er kommt gar nicht!", "tokens": ["Er", "kommt", "gar", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Was Alle thun,", "tokens": ["Was", "Al\u00b7le", "thun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Will ich auch thun", "tokens": ["Will", "ich", "auch", "thun"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Und tr\u00e4umen, prassen, plagen, ruhn!\u00ab", "tokens": ["Und", "tr\u00e4u\u00b7men", ",", "pras\u00b7sen", ",", "pla\u00b7gen", ",", "ruhn", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Herr, wer wird vor Dir bestehen!", "tokens": ["Herr", ",", "wer", "wird", "vor", "Dir", "be\u00b7ste\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer vor Dein Angesicht zu gehen", "tokens": ["Wer", "vor", "Dein", "An\u00b7ge\u00b7sicht", "zu", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erk\u00fchnen, wenn die Erd' entflieht!", "tokens": ["Er\u00b7k\u00fch\u00b7nen", ",", "wenn", "die", "Erd'", "ent\u00b7flieht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, ein Strohhalm in die Flammen", "tokens": ["Ach", ",", "ein", "Stroh\u00b7halm", "in", "die", "Flam\u00b7men"], "token_info": ["w