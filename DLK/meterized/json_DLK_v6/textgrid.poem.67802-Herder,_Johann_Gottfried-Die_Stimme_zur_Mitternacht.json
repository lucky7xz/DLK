{"textgrid.poem.67802": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Stimme zur Mitternacht", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwachet, wachet!\u00ab ruft die Stimme", "tokens": ["\u00bb", "wa\u00b7chet", ",", "wa\u00b7chet", "!", "\u00ab", "ruft", "die", "Stim\u00b7me"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$.", "$(", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der W\u00e4chter auf des Tempels Zinne;", "tokens": ["Der", "W\u00e4ch\u00b7ter", "auf", "des", "Tem\u00b7pels", "Zin\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwach auf, Du Stadt Jerusalem!\u00ab", "tokens": ["\u00bb", "wach", "auf", ",", "Du", "Stadt", "Je\u00b7ru\u00b7sa\u00b7lem", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "PTKVZ", "$,", "PPER", "NN", "NE", "$.", "$("], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Mitternacht hei\u00dft diese Stunde;", "tokens": ["Mit\u00b7ter\u00b7nacht", "hei\u00dft", "die\u00b7se", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie rufen uns mit hellem Munde:", "tokens": ["Sie", "ru\u00b7fen", "uns", "mit", "hel\u00b7lem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbwo seid Ihr klugen Jungfrauen?", "tokens": ["\u00bb", "wo", "seid", "Ihr", "klu\u00b7gen", "Jung\u00b7frau\u00b7en", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Steht auf! der Br\u00e4ut'gam kommt.", "tokens": ["Steht", "auf", "!", "der", "Br\u00e4ut'\u00b7gam", "kommt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Auf! Eure Lampen nehmt,", "tokens": ["Auf", "!", "Eu\u00b7re", "Lam\u00b7pen", "nehmt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Hosianna!", "tokens": ["Ho\u00b7si\u00b7an\u00b7na", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Macht Euch bereit", "tokens": ["Macht", "Euch", "be\u00b7reit"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Zur Freudenzeit!", "tokens": ["Zur", "Freu\u00b7den\u00b7zeit", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Ihr m\u00fcsset ihm entgegengehn!\u00ab", "tokens": ["Ihr", "m\u00fcs\u00b7set", "ihm", "ent\u00b7ge\u00b7gen\u00b7gehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbach, wir schlummern All' und schlafen!", "tokens": ["\u00bb", "ach", ",", "wir", "schlum\u00b7mern", "All'", "und", "schla\u00b7fen", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VVFIN", "PIS", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Hirte schlummert mit den Schafen;", "tokens": ["Der", "Hir\u00b7te", "schlum\u00b7mert", "mit", "den", "Scha\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lamp' ist da! wo ist das Licht?", "tokens": ["Die", "Lamp'", "ist", "da", "!", "wo", "ist", "das", "Licht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "PWAV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie es war in Noah Tagen,", "tokens": ["Wie", "es", "war", "in", "Noah", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "APPR", "NE", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Sie a\u00dfen, tranken, fern von Plagen,", "tokens": ["Sie", "a\u00b7\u00dfen", ",", "tran\u00b7ken", ",", "fern", "von", "Pla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Strafen fern, und dachten's nicht;", "tokens": ["Von", "Stra\u00b7fen", "fern", ",", "und", "dach\u00b7ten's", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "KON", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir frei'n und lassen frei'n;", "tokens": ["Wir", "frei'n", "und", "las\u00b7sen", "frei'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Sorge wiegt uns ein,", "tokens": ["Die", "Sor\u00b7ge", "wiegt", "uns", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wurmessorge!\u00ab", "tokens": ["Wur\u00b7mes\u00b7sor\u00b7ge", "!", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "\u00bberwacht! Erwacht!", "tokens": ["\u00bb", "er\u00b7wacht", "!", "Er\u00b7wacht", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVPP", "$.", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "In Mitternacht", "tokens": ["In", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Ein Blitz soll seine Ankunft sein!\u00ab", "tokens": ["Ein", "Blitz", "soll", "sei\u00b7ne", "An\u00b7kunft", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Falsche Christus und Verr\u00e4ther,", "tokens": ["Fal\u00b7sche", "Chris\u00b7tus", "und", "Ver\u00b7r\u00e4\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vernunft-Verf\u00fchrer, Wunderth\u00e4ter", "tokens": ["Ver\u00b7nunft\u00b7Ver\u00b7f\u00fch\u00b7rer", ",", "Wun\u00b7der\u00b7th\u00e4\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der L\u00fcge sind das Licht der Welt.", "tokens": ["Der", "L\u00fc\u00b7ge", "sind", "das", "Licht", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Meinst Du, da\u00df der Richter werde", "tokens": ["Meinst", "Du", ",", "da\u00df", "der", "Rich\u00b7ter", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch Glauben finden auf der Erde,", "tokens": ["Noch", "Glau\u00b7ben", "fin\u00b7den", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Wollust sie in Fesseln h\u00e4lt?", "tokens": ["Wenn", "Wol\u00b7lust", "sie", "in", "Fes\u00b7seln", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr H\u00fcgel, fallet, fallt!", "tokens": ["Ihr", "H\u00fc\u00b7gel", ",", "fal\u00b7let", ",", "fallt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Menschen Herz ist kalt,", "tokens": ["Der", "Men\u00b7schen", "Herz", "ist", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Kalt die Liebe!", "tokens": ["Kalt", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Voll Heuchelei-", "tokens": ["Voll", "Heu\u00b7che\u00b7lei"], "token_info": ["word", "word"], "pos": ["ADJD", "TRUNC"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Abg\u00f6tterei,", "tokens": ["Ab\u00b7g\u00f6t\u00b7te\u00b7rei", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Sieh, ob nicht Alles, Alles sei!", "tokens": ["Sieh", ",", "ob", "nicht", "Al\u00b7les", ",", "Al\u00b7les", "sei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PTKNEG", "PIS", "$,", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schlangen sind der V\u00f6lker Kronen,", "tokens": ["Schlan\u00b7gen", "sind", "der", "V\u00f6l\u00b7ker", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Nationen Nationen", "tokens": ["Und", "Na\u00b7ti\u00b7o\u00b7nen", "Na\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Gei\u00dfel statt der Bruderhand;", "tokens": ["Zur", "Gei\u00b7\u00dfel", "statt", "der", "Bru\u00b7der\u00b7hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00fctter, T\u00f6chter, S\u00f6hne, V\u00e4ter", "tokens": ["M\u00fct\u00b7ter", ",", "T\u00f6ch\u00b7ter", ",", "S\u00f6h\u00b7ne", ",", "V\u00e4\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Zerrei\u00dfen Blut- und Herzensband!", "tokens": ["Zer\u00b7rei\u00b7\u00dfen", "Blut", "und", "Her\u00b7zens\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo meinet Freund und Freund", "tokens": ["Wo", "mei\u00b7net", "Freund", "und", "Freund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sich bieder? wo vereint", "tokens": ["Sich", "bie\u00b7der", "?", "wo", "ver\u00b7eint"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PRF", "PTKVZ", "$.", "PWAV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Pflicht die Herzen?", "tokens": ["Pflicht", "die", "Her\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Pflicht und Gebet", "tokens": ["Pflicht", "und", "Ge\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "An heil'ger St\u00e4tt',", "tokens": ["An", "heil'\u00b7ger", "St\u00e4tt'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "---+", "measure": "unknown.measure.single"}, "line.12": {"text": "Das ewiglich bei Gott besteht.", "tokens": ["Das", "e\u00b7wig\u00b7lich", "bei", "Gott", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, wie schlummern All' und schlafen!", "tokens": ["Ach", ",", "wie", "schlum\u00b7mern", "All'", "und", "schla\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJA", "NN", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Hirte schlummert mit den Schafen;", "tokens": ["Der", "Hir\u00b7te", "schlum\u00b7mert", "mit", "den", "Scha\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lamp' ist da! wo ist das Licht?", "tokens": ["Die", "Lamp'", "ist", "da", "!", "wo", "ist", "das", "Licht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "PWAV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit den Trunknen schl\u00e4frigtrunken,", "tokens": ["Mit", "den", "Trunk\u00b7nen", "schl\u00e4f\u00b7rig\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In Nacht und Wahn und Graus versunken,", "tokens": ["In", "Nacht", "und", "Wahn", "und", "Graus", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, sehen wir und h\u00f6ren nicht!", "tokens": ["Ach", ",", "se\u00b7hen", "wir", "und", "h\u00f6\u00b7ren", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer tr\u00e4gt nicht Thieres Bild?", "tokens": ["Wer", "tr\u00e4gt", "nicht", "Thie\u00b7res", "Bild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wer, dem das Herz nicht f\u00fcllt", "tokens": ["Wer", ",", "dem", "das", "Herz", "nicht", "f\u00fcllt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Erdensorge?", "tokens": ["Er\u00b7den\u00b7sor\u00b7ge", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Ist Mitternacht!", "tokens": ["Ist", "Mit\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Erwacht, erwacht!", "tokens": ["Er\u00b7wacht", ",", "er\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Blitzschnell erscheint des Menschen Sohn.", "tokens": ["Blitz\u00b7schnell", "er\u00b7scheint", "des", "Men\u00b7schen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Meinst Du, wenn der Hausherr w\u00fc\u00dfte,", "tokens": ["Meinst", "Du", ",", "wenn", "der", "Haus\u00b7herr", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", 