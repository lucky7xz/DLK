{"dta.poem.14792": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, Muhme! was ist Euch geschehen?", "tokens": ["Ach", ",", "Muh\u00b7me", "!", "was", "ist", "Euch", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nase wird Euch so lang,", "tokens": ["Die", "Na\u00b7se", "wird", "Euch", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Augen sich seltsam verdrehen \u2014", "tokens": ["Die", "Au\u00b7gen", "sich", "selt\u00b7sam", "ver\u00b7dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie wird mir vor Euch so bang! \u2014", "tokens": ["Wie", "wird", "mir", "vor", "Euch", "so", "bang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}