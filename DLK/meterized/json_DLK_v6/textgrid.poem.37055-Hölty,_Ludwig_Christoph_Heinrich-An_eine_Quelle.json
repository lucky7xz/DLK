{"textgrid.poem.37055": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An eine Quelle", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil dir, o Bach, der durch die gr\u00fcnen Netze,", "tokens": ["Heil", "dir", ",", "o", "Bach", ",", "der", "durch", "die", "gr\u00fc\u00b7nen", "Net\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NE", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewebt von Haselstauden, flie\u00dft,", "tokens": ["Ge\u00b7webt", "von", "Ha\u00b7sel\u00b7stau\u00b7den", ",", "flie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch die Flur, mit fr\u00f6lichem Geschw\u00e4tze,", "tokens": ["Und", "durch", "die", "Flur", ",", "mit", "fr\u00f6\u00b7li\u00b7chem", "Ge\u00b7schw\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die volle Silberurne gie\u00dft.", "tokens": ["Die", "vol\u00b7le", "Sil\u00b7be\u00b7rur\u00b7ne", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der junge Lenz bemale dein Gestade", "tokens": ["Der", "jun\u00b7ge", "Lenz", "be\u00b7ma\u00b7le", "dein", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Regenb\u00f6gen, lieber Bach:", "tokens": ["Mit", "Re\u00b7gen\u00b7b\u00f6\u00b7gen", ",", "lie\u00b7ber", "Bach", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stets w\u00e4hle dich das sch\u00f6nste Kind zum Bade,", "tokens": ["Stets", "w\u00e4h\u00b7le", "dich", "das", "sch\u00f6ns\u00b7te", "Kind", "zum", "Ba\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dein Geb\u00fcsch umher zum Dach.", "tokens": ["Und", "dein", "Ge\u00b7b\u00fcsch", "um\u00b7her", "zum", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier ist der Platz, wo j\u00fcngst der erste Funken", "tokens": ["Hier", "ist", "der", "Platz", ",", "wo", "j\u00fcngst", "der", "ers\u00b7te", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Lieb' in meinen Busen sank,", "tokens": ["Der", "Lieb'", "in", "mei\u00b7nen", "Bu\u00b7sen", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo j\u00fcngst mein Geist, so wonnevoll, so trunken,", "tokens": ["Wo", "j\u00fcngst", "mein", "Geist", ",", "so", "won\u00b7ne\u00b7voll", ",", "so", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den ersten s\u00fc\u00dfen Taumel trank.", "tokens": ["Den", "ers\u00b7ten", "s\u00fc\u00b7\u00dfen", "Tau\u00b7mel", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Verdecket vom Geb\u00fcsch, sa\u00df ich und blickte", "tokens": ["Ver\u00b7de\u00b7cket", "vom", "Ge\u00b7b\u00fcsch", ",", "sa\u00df", "ich", "und", "blick\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum andern bunten Ufer hin,", "tokens": ["Zum", "an\u00b7dern", "bun\u00b7ten", "U\u00b7fer", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Chloe sa\u00df und Mayenblumen pfl\u00fcckte,", "tokens": ["Wo", "Chloe", "sa\u00df", "und", "Ma\u00b7yen\u00b7blu\u00b7men", "pfl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gleich einer jungen Huldg\u00f6ttin.", "tokens": ["Gleich", "ei\u00b7ner", "jun\u00b7gen", "Huld\u00b7g\u00f6t\u00b7tin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Das Abendroth vergoldete die H\u00fcgel,", "tokens": ["Das", "A\u00b7ben\u00b7droth", "ver\u00b7gol\u00b7de\u00b7te", "die", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die falbe D\u00e4mmerung umzog,", "tokens": ["Die", "fal\u00b7be", "D\u00e4m\u00b7me\u00b7rung", "um\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie da mein Geist, auf der Entz\u00fcckung Fl\u00fcgel,", "tokens": ["Wie", "da", "mein", "Geist", ",", "auf", "der", "Ent\u00b7z\u00fc\u00b7ckung", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fern \u00fcber alle Himmel flog.", "tokens": ["Fern", "\u00fc\u00b7ber", "al\u00b7le", "Him\u00b7mel", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie schlug mein Herz! Wie warf ich durch die Decken", "tokens": ["Wie", "schlug", "mein", "Herz", "!", "Wie", "warf", "ich", "durch", "die", "De\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gr\u00fcnen Zweige Blick auf Blick!", "tokens": ["Der", "gr\u00fc\u00b7nen", "Zwei\u00b7ge", "Blick", "auf", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts konnte mich aus meinem Rausche wecken,", "tokens": ["Nichts", "konn\u00b7te", "mich", "aus", "mei\u00b7nem", "Rau\u00b7sche", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nie wich mein Aug' von ihr zur\u00fcck.", "tokens": ["Nie", "wich", "mein", "Aug'", "von", "ihr", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich tr\u00e4umte mich in goldne Paradiese,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "mich", "in", "gold\u00b7ne", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah Nektar und Elysium", "tokens": ["Sah", "Nek\u00b7tar", "und", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Statt meines Bachs, statt meiner bunten Wiese,", "tokens": ["Statt", "mei\u00b7nes", "Bachs", ",", "statt", "mei\u00b7ner", "bun\u00b7ten", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUI", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um meinen trunknen Blick herum.", "tokens": ["Um", "mei\u00b7nen", "trunk\u00b7nen", "Blick", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So sa\u00df sie lang auf Mayenblumenglocken,", "tokens": ["So", "sa\u00df", "sie", "lang", "auf", "Ma\u00b7yen\u00b7blu\u00b7men\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Grub mir ihr Bild in meine Brust,", "tokens": ["Grub", "mir", "ihr", "Bild", "in", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und band mein Herz an ihre blonden Locken.", "tokens": ["Und", "band", "mein", "Herz", "an", "ih\u00b7re", "blon\u00b7den", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie schwamm ich nicht im Meer von Lust!", "tokens": ["Wie", "schwamm", "ich", "nicht", "im", "Meer", "von", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie lieb' ich dich, o silberwei\u00dfe Quelle,", "tokens": ["Wie", "lieb'", "ich", "dich", ",", "o", "sil\u00b7ber\u00b7wei\u00b7\u00dfe", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Chloe unter Blumen sa\u00df", "tokens": ["Wo", "Chloe", "un\u00b7ter", "Blu\u00b7men", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und mit der Hand, so wei\u00df wie deine Welle,", "tokens": ["Und", "mit", "der", "Hand", ",", "so", "wei\u00df", "wie", "dei\u00b7ne", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sch\u00f6nsten Fr\u00fchlingsbl\u00fcmchen las.", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Fr\u00fch\u00b7lings\u00b7bl\u00fcm\u00b7chen", "las", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}