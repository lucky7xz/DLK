{"textgrid.poem.56695": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Genug!", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Genug! jetzt halte ich den Kreisel stille,", "tokens": ["Ge\u00b7nug", "!", "jetzt", "hal\u00b7te", "ich", "den", "Krei\u00b7sel", "stil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der ohne Rast sich um sich selber dreht", "tokens": ["der", "oh\u00b7ne", "Rast", "sich", "um", "sich", "sel\u00b7ber", "dreht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "PRF", "APPR", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und den ein w\u00fctend blinder Weltenwille", "tokens": ["und", "den", "ein", "w\u00fc\u00b7tend", "blin\u00b7der", "Wel\u00b7ten\u00b7wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mit Peitschen treiben mu\u00df, da\u00df er nur steht", "tokens": ["mit", "Peit\u00b7schen", "trei\u00b7ben", "mu\u00df", ",", "da\u00df", "er", "nur", "steht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "und nicht im n\u00e4chsten Augenblicke matt", "tokens": ["und", "nicht", "im", "n\u00e4chs\u00b7ten", "Au\u00b7gen\u00b7bli\u00b7cke", "matt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPRART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und ewig regungslos zu Boden f\u00e4llt.", "tokens": ["und", "e\u00b7wig", "re\u00b7gungs\u00b7los", "zu", "Bo\u00b7den", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Genug! ich bin der Peitsche \u00fcbersatt,", "tokens": ["Ge\u00b7nug", "!", "ich", "bin", "der", "Peit\u00b7sche", "\u00fc\u00b7ber\u00b7satt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "satt bin ich dieser qualgepeitschten Welt", "tokens": ["satt", "bin", "ich", "die\u00b7ser", "qual\u00b7ge\u00b7peitschten", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "und gebe den Gehorsam endlich auf,", "tokens": ["und", "ge\u00b7be", "den", "Ge\u00b7hor\u00b7sam", "end\u00b7lich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ein Ding zu sein, das alle N\u00f6te hetzen,", "tokens": ["ein", "Ding", "zu", "sein", ",", "das", "al\u00b7le", "N\u00f6\u00b7te", "het\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "bis es nach richtig abgerastem Lauf", "tokens": ["bis", "es", "nach", "rich\u00b7tig", "ab\u00b7ge\u00b7ras\u00b7tem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "umsinkt ein Haufen Staub und Tr\u00fcmmerfetzen.", "tokens": ["um\u00b7sinkt", "ein", "Hau\u00b7fen", "Staub", "und", "Tr\u00fcm\u00b7mer\u00b7fet\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein sei der Augenblick, in dem ich falle,", "tokens": ["Mein", "sei", "der", "Au\u00b7gen\u00b7blick", ",", "in", "dem", "ich", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ich will in meiner N\u00f6te gr\u00f6\u00dfter Not", "tokens": ["ich", "will", "in", "mei\u00b7ner", "N\u00f6\u00b7te", "gr\u00f6\u00df\u00b7ter", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und voller Hohn und bitterschwarzer Galle", "tokens": ["und", "vol\u00b7ler", "Hohn", "und", "bit\u00b7ter\u00b7schwar\u00b7zer", "Gal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "eingehn in einen freiherrlichen Tod!", "tokens": ["ein\u00b7gehn", "in", "ei\u00b7nen", "frei\u00b7herr\u00b7li\u00b7chen", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}}}}