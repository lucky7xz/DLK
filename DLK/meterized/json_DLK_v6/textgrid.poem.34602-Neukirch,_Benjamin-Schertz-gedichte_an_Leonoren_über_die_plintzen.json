{"textgrid.poem.34602": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-gedichte an Leonoren/ \u00fcber die plintzen", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So offt ich euch beschau/ ihr angenehme plintzen/", "tokens": ["So", "offt", "ich", "euch", "be\u00b7schau", "/", "ihr", "an\u00b7ge\u00b7neh\u00b7me", "plint\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "PTKVZ", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So offt wird auch das hertz in meiner brust bewegt/", "tokens": ["So", "offt", "wird", "auch", "das", "hertz", "in", "mei\u00b7ner", "brust", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann unser Friederich ist auff den silber-m\u00fcntzen", "tokens": ["Dann", "un\u00b7ser", "Frie\u00b7de\u00b7rich", "ist", "auff", "den", "sil\u00b7ber\u00b7m\u00fcnt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So deutlich nicht als wir in euren teig gepregt.", "tokens": ["So", "deut\u00b7lich", "nicht", "als", "wir", "in", "eu\u00b7ren", "teig", "ge\u00b7pregt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eur erster ursprung k\u00f6mmt durch weitzen aus der erden/", "tokens": ["Eur", "ers\u00b7ter", "ur\u00b7sprung", "k\u00f6mmt", "durch", "weit\u00b7zen", "aus", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer wei\u00df nicht/ da\u00df wir auch von dieser mutter seyn?", "tokens": ["Wer", "wei\u00df", "nicht", "/", "da\u00df", "wir", "auch", "von", "die\u00b7ser", "mut\u00b7ter", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr m\u00fc\u00dft/ so bald ihr reiff/ in stroh gebunden werden/", "tokens": ["Ihr", "m\u00fc\u00dft", "/", "so", "bald", "ihr", "reiff", "/", "in", "stroh", "ge\u00b7bun\u00b7den", "wer\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ADV", "ADV", "PPER", "VVFIN", "$(", "APPR", "ADJD", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Uns schleu\u00dft man augenblicks in feste windeln ein.", "tokens": ["Uns", "schleu\u00dft", "man", "au\u00b7gen\u00b7blicks", "in", "fes\u00b7te", "win\u00b7deln", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die bauren dreschen euch/ uns aber die tyrannen/", "tokens": ["Die", "bau\u00b7ren", "dre\u00b7schen", "euch", "/", "uns", "a\u00b7ber", "die", "ty\u00b7ran\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die in den schulen uns das hintertheil besehn:", "tokens": ["Die", "in", "den", "schu\u00b7len", "uns", "das", "hin\u00b7ter\u00b7theil", "be\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn was der hencker nicht durch bauren wei\u00df zu bannen/", "tokens": ["Denn", "was", "der", "hen\u00b7cker", "nicht", "durch", "bau\u00b7ren", "wei\u00df", "zu", "ban\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mu\u00df dennoch in der welt durch einen fuchs geschehn.", "tokens": ["Mu\u00df", "den\u00b7noch", "in", "der", "welt", "durch", "ei\u00b7nen", "fuchs", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das ist der erste tantz/ den uns die feinde spielen.", "tokens": ["Das", "ist", "der", "ers\u00b7te", "tantz", "/", "den", "uns", "die", "fein\u00b7de", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wann euch der flegel nun den buckel abgeklopfft/", "tokens": ["Wann", "euch", "der", "fle\u00b7gel", "nun", "den", "bu\u00b7ckel", "ab\u00b7ge\u00b7klopfft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So schicket man alsdann die k\u00f6rner in die m\u00fchlen/", "tokens": ["So", "schi\u00b7cket", "man", "als\u00b7dann", "die", "k\u00f6r\u00b7ner", "in", "die", "m\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "ADJA", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und endlich wird das mehl in einen sack gestopfft.", "tokens": ["Und", "end\u00b7lich", "wird", "das", "mehl", "in", "ei\u00b7nen", "sack", "ge\u00b7stopfft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So, wann die Herren uns die h\u00fclsen abgetreten/", "tokens": ["So", ",", "wann", "die", "Her\u00b7ren", "uns", "die", "h\u00fcl\u00b7sen", "ab\u00b7ge\u00b7tre\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und wir den Calepin bi\u00df auff den band verstehn/", "tokens": ["Und", "wir", "den", "Ca\u00b7le\u00b7pin", "bi\u00df", "auff", "den", "band", "ver\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NE", "KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So sehn wir allererst auff Universit\u00e4ten/", "tokens": ["So", "sehn", "wir", "al\u00b7le\u00b7rerst", "auff", "U\u00b7niv\u00b7er\u00b7si\u00b7t\u00e4\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df wei\u00dfheit und verstand auch durch die m\u00fchle gehn.", "tokens": ["Da\u00df", "wei\u00df\u00b7heit", "und", "ver\u00b7stand", "auch", "durch", "die", "m\u00fch\u00b7le", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dann was wir vor gelernt/ wird alles umbgekehret/", "tokens": ["Dann", "was", "wir", "vor", "ge\u00b7lernt", "/", "wird", "al\u00b7les", "umb\u00b7ge\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "APPR", "VVPP", "$(", "VAFIN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Man dr\u00e4nget die vernunfft in enge kercker ein/", "tokens": ["Man", "dr\u00e4n\u00b7get", "die", "ver\u00b7nunfft", "in", "en\u00b7ge", "ker\u00b7cker", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bi\u00df der gelehrte stein den groben rest verzehret/", "tokens": ["Bi\u00df", "der", "ge\u00b7lehr\u00b7te", "stein", "den", "gro\u00b7ben", "rest", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und unsre reden kern/ die sitten tugend seyn.", "tokens": ["Und", "uns\u00b7re", "re\u00b7den", "kern", "/", "die", "sit\u00b7ten", "tu\u00b7gend", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVINF", "VVINF", "$(", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dann strotzt man wie ein sack vor lauter phantaseyen/", "tokens": ["Dann", "strotzt", "man", "wie", "ein", "sack", "vor", "lau\u00b7ter", "phan\u00b7ta\u00b7se\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ART", "ADJD", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Man zeigt von aussen schon/ was man verborgen tr\u00e4gt/", "tokens": ["Man", "zeigt", "von", "aus\u00b7sen", "schon", "/", "was", "man", "ver\u00b7bor\u00b7gen", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NE", "ADV", "$(", "PWS", "PIS", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und beyde dencken nicht/ indem wir uns erfreuen/", "tokens": ["Und", "bey\u00b7de", "den\u00b7cken", "nicht", "/", "in\u00b7dem", "wir", "uns", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Was zeit und schicksal uns f\u00fcr martern aufferlegt.", "tokens": ["Was", "zeit", "und", "schick\u00b7sal", "uns", "f\u00fcr", "mar\u00b7tern", "auf\u00b7fer\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "ADJD", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Inzwischen fallet ihr den weibern in die h\u00e4nde/", "tokens": ["I\u00b7nzwi\u00b7schen", "fal\u00b7let", "ihr", "den", "wei\u00b7bern", "in", "die", "h\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "APPR", "ART", "ADJA", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.30": {"text": "Die r\u00fchren euch mit milch und weichen eyern ein:", "tokens": ["Die", "r\u00fch\u00b7ren", "euch", "mit", "milch", "und", "wei\u00b7chen", "ey\u00b7ern", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NE", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dann setzen sie den teig auff kohlen und auff br\u00e4nde/", "tokens": ["Dann", "set\u00b7zen", "sie", "den", "teig", "auff", "koh\u00b7len", "und", "auff", "br\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "VVINF", "KON", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und lassen ihn zur lust mit zucker \u00fcberstreun.", "tokens": ["Und", "las\u00b7sen", "ihn", "zur", "lust", "mit", "zu\u00b7cker", "\u00fc\u00b7bers\u00b7treun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wir aber m\u00fcssen uns bey hofe lassen scheren/", "tokens": ["Wir", "a\u00b7ber", "m\u00fcs\u00b7sen", "uns", "bey", "ho\u00b7fe", "las\u00b7sen", "sche\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und werden durch die milch der hoffnung zubereit.", "tokens": ["Und", "wer\u00b7den", "durch", "die", "milch", "der", "hoff\u00b7nung", "zu\u00b7be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die flammen sind der gram/ durch den wir uns verzehren/", "tokens": ["Die", "flam\u00b7men", "sind", "der", "gram", "/", "durch", "den", "wir", "uns", "ver\u00b7zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VAFIN", "ART", "NE", "$(", "APPR", "PRELS", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der zucker aber ist die s\u00fcsse dienstbarkeit.", "tokens": ["Der", "zu\u00b7cker", "a\u00b7ber", "ist", "die", "s\u00fcs\u00b7se", "dienst\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und nunmehr fangen wir dem wetter an zu trauen/", "tokens": ["Und", "nun\u00b7mehr", "fan\u00b7gen", "wir", "dem", "wet\u00b7ter", "an", "zu", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Es scheint/ da\u00df beyde nun ein neuer stern begl\u00fcckt.", "tokens": ["Es", "scheint", "/", "da\u00df", "bey\u00b7de", "nun", "ein", "neu\u00b7er", "stern", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PIS", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Doch eh die menschen euch/ und wir die welt beschauen/", "tokens": ["Doch", "eh", "die", "men\u00b7schen", "euch", "/", "und", "wir", "die", "welt", "be\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "$(", "KON", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So werdet ihr in bauch/ und wir ins grab geschickt.", "tokens": ["So", "wer\u00b7det", "ihr", "in", "bauch", "/", "und", "wir", "ins", "grab", "ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "$(", "KON", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Dann euch verschlinget man schon \u00f6ffters bey dem tiegel/", "tokens": ["Dann", "euch", "ver\u00b7schlin\u00b7get", "man", "schon", "\u00f6ff\u00b7ters", "bey", "dem", "tie\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PIS", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wir sterben, eh uns noch die sonne recht bestrahlt.", "tokens": ["Wir", "ster\u00b7ben", ",", "eh", "uns", "noch", "die", "son\u00b7ne", "recht", "be\u00b7strahlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und also sehen wir uns wie in einem spiegel/", "tokens": ["Und", "al\u00b7so", "se\u00b7hen", "wir", "uns", "wie", "in", "ei\u00b7nem", "spie\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "KOKOM", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ihr findet euch in uns/ wir uns in euch gemahlt.", "tokens": ["Ihr", "fin\u00b7det", "euch", "in", "uns", "/", "wir", "uns", "in", "euch", "ge\u00b7mahlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$(", "PPER", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Doch eines wird und soll mich bi\u00df in tod verdriessen/", "tokens": ["Doch", "ei\u00b7nes", "wird", "und", "soll", "mich", "bi\u00df", "in", "tod", "ver\u00b7dries\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "KON", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da\u00df man euch in den leib/ uns in den sand begr\u00e4bt/", "tokens": ["Da\u00df", "man", "euch", "in", "den", "leib", "/", "uns", "in", "den", "sand", "be\u00b7gr\u00e4bt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ART", "NN", "$(", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Da\u00df euch die jungfern gar in ihre brust verschliessen/", "tokens": ["Da\u00df", "euch", "die", "jung\u00b7fern", "gar", "in", "ih\u00b7re", "brust", "ver\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und unser name kaum auff ihren lippen schwebt.", "tokens": ["Und", "un\u00b7ser", "na\u00b7me", "kaum", "auff", "ih\u00b7ren", "lip\u00b7pen", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "F\u00fcrwahr/ ich wolte mich weit besser in sie schicken/", "tokens": ["F\u00fcr\u00b7wahr", "/", "ich", "wol\u00b7te", "mich", "weit", "bes\u00b7ser", "in", "sie", "schi\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VMFIN", "PRF", "ADJD", "ADJD", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "(ach da\u00df ich \u00e4rmster doch nicht eine plintze bin!)", "tokens": ["(", "ach", "da\u00df", "ich", "\u00e4rms\u00b7ter", "doch", "nicht", "ei\u00b7ne", "plint\u00b7ze", "bin", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "KOUS", "PPER", "ADJD", "ADV", "PTKNEG", "ART", "NN", "VAFIN", "$.", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.51": {"text": "Dann d\u00fcrfft ich erstlich nur bi\u00df in den magen r\u00fccken/", "tokens": ["Dann", "d\u00fcrfft", "ich", "erst\u00b7lich", "nur", "bi\u00df", "in", "den", "ma\u00b7gen", "r\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So k\u00e4m ich mit der zeit auch wohl zum hertzen hin.", "tokens": ["So", "k\u00e4m", "ich", "mit", "der", "zeit", "auch", "wohl", "zum", "hert\u00b7zen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Was h\u00e4ndel wolt ich da nicht Leonoren machen?", "tokens": ["Was", "h\u00e4n\u00b7del", "wolt", "ich", "da", "nicht", "Le\u00b7o\u00b7no\u00b7ren", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VMFIN", "PPER", "ADV", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Jedoch ich irre 