{"textgrid.poem.60705": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man mu\u00df einander helfen, fordert die Natur.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man mu\u00df einander helfen, fordert die Natur.", "tokens": ["Man", "mu\u00df", "ein\u00b7an\u00b7der", "hel\u00b7fen", ",", "for\u00b7dert", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Esel, sonst die beste Kreatur,", "tokens": ["Der", "E\u00b7sel", ",", "sonst", "die", "bes\u00b7te", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vers\u00e4umte einmal dies;", "tokens": ["Ver\u00b7s\u00e4um\u00b7te", "ein\u00b7mal", "dies", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wei\u00df nicht, warum er's unterlie\u00df.", "tokens": ["Wei\u00df", "nicht", ",", "wa\u00b7rum", "er's", "un\u00b7ter\u00b7lie\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er trabte in gemessnem Schritt", "tokens": ["Er", "trab\u00b7te", "in", "ge\u00b7mess\u00b7nem", "Schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gedankenlos durchs Feld,", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7los", "durchs", "Feld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "An seiner Seite lief der Bauer mit,", "tokens": ["An", "sei\u00b7ner", "Sei\u00b7te", "lief", "der", "Bau\u00b7er", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem sich sein treuer Hofhund zugesellt.", "tokens": ["Dem", "sich", "sein", "treu\u00b7er", "Hof\u00b7hund", "zu\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Mann legt unter B\u00e4umen sich zum Schlaf,", "tokens": ["Der", "Mann", "legt", "un\u00b7ter", "B\u00e4u\u00b7men", "sich", "zum", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Esel macht sich drauf ans Grasen", "tokens": ["Der", "E\u00b7sel", "macht", "sich", "drauf", "ans", "Gra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PAV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und freut sich, da\u00df er's g\u00fcnstig traf:", "tokens": ["Und", "freut", "sich", ",", "da\u00df", "er's", "g\u00fcns\u00b7tig", "traf", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Rings gr\u00fcnt ein schmackhaft frischer Rasen.", "tokens": ["Rings", "gr\u00fcnt", "ein", "schmack\u00b7haft", "fri\u00b7scher", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Zwar fehlt die Distel, doch er f\u00fcgt sich drein.", "tokens": ["Zwar", "fehlt", "die", "Dis\u00b7tel", ",", "doch", "er", "f\u00fcgt", "sich", "drein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "F\u00fcr heute nicht zu anspruchsvoll zu sein,", "tokens": ["F\u00fcr", "heu\u00b7te", "nicht", "zu", "an\u00b7spruchs\u00b7voll", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKNEG", "PTKA", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Obschon ein Mahl, das keine Distel w\u00fcrzt,", "tokens": ["Ob\u00b7schon", "ein", "Mahl", ",", "das", "kei\u00b7ne", "Dis\u00b7tel", "w\u00fcrzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Dem Tafelnden die reinste Freude k\u00fcrzt.", "tokens": ["Dem", "Ta\u00b7feln\u00b7den", "die", "reins\u00b7te", "Freu\u00b7de", "k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Nun, Langohr wei\u00df es zu ertragen", "tokens": ["Nun", ",", "Lan\u00b7gohr", "wei\u00df", "es", "zu", "er\u00b7tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und sich f\u00fcr diesmal zu bescheiden.", "tokens": ["Und", "sich", "f\u00fcr", "dies\u00b7mal", "zu", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der arme Hund sah zu mit leerem Magen", "tokens": ["Der", "ar\u00b7me", "Hund", "sah", "zu", "mit", "lee\u00b7rem", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und sprach: \u00bbAch Freund, wie mu\u00df ich Hunger leiden!", "tokens": ["Und", "sprach", ":", "\u00bb", "Ach", "Freund", ",", "wie", "mu\u00df", "ich", "Hun\u00b7ger", "lei\u00b7den", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ITJ", "NN", "$,", "PWAV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du dich b\u00fccken m\u00f6chtest, k\u00f6nnte ich mein Essen", "tokens": ["Wenn", "du", "dich", "b\u00fc\u00b7cken", "m\u00f6ch\u00b7test", ",", "k\u00f6nn\u00b7te", "ich", "mein", "Es\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Aus deinem Korbe mir von deinem R\u00fccken nehmen.\u00ab", "tokens": ["Aus", "dei\u00b7nem", "Kor\u00b7be", "mir", "von", "dei\u00b7nem", "R\u00fc\u00b7cken", "neh\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Langohr hatte keine Lust, beim Fressen", "tokens": ["Doch", "Lan\u00b7gohr", "hat\u00b7te", "kei\u00b7ne", "Lust", ",", "beim", "Fres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PIAT", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu diesem Freundschaftsdienst sich zu bequemen.", "tokens": ["Zu", "die\u00b7sem", "Freund\u00b7schafts\u00b7dienst", "sich", "zu", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er blieb den Bitten stumm und kalt,", "tokens": ["Er", "blieb", "den", "Bit\u00b7ten", "stumm", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schlie\u00dflich sprach er mit Bedacht:", "tokens": ["Und", "schlie\u00df\u00b7lich", "sprach", "er", "mit", "Be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbfreund, warte, bis der Herr erwacht,", "tokens": ["\u00bb", "freund", ",", "war\u00b7te", ",", "bis", "der", "Herr", "er\u00b7wacht", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der gibt dir dann, was dir geh\u00f6rt.\u00ab", "tokens": ["Der", "gibt", "dir", "dann", ",", "was", "dir", "ge\u00b7h\u00f6rt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kaum ist's gesagt, st\u00fcrzt aus dem Wald", "tokens": ["Kaum", "ist's", "ge\u00b7sagt", ",", "st\u00fcrzt", "aus", "dem", "Wald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Wolf, den auch der Hunger plagt.", "tokens": ["Ein", "Wolf", ",", "den", "auch", "der", "Hun\u00b7ger", "plagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbzu Hilfe!\u00ab ruft der Esel ganz verst\u00f6rt.", "tokens": ["\u00bb", "zu", "Hil\u00b7fe", "!", "\u00ab", "ruft", "der", "E\u00b7sel", "ganz", "ver\u00b7st\u00f6rt", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$.", "$(", "VVFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Hund bleibt regungslos und sagt:", "tokens": ["Der", "Hund", "bleibt", "re\u00b7gungs\u00b7los", "und", "sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbfreund, warte, bis der Herr erwacht", "tokens": ["\u00bb", "freund", ",", "war\u00b7te", ",", "bis", "der", "Herr", "er\u00b7wacht"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und sich zu deinem Sch\u00fctzer macht.", "tokens": ["Und", "sich", "zu", "dei\u00b7nem", "Sch\u00fct\u00b7zer", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Flieh nur einstweilen, bis er kommt!", "tokens": ["Flieh", "nur", "einst\u00b7wei\u00b7len", ",", "bis", "er", "kommt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das ist mein Rat, der dir am besten frommt.", "tokens": ["Das", "ist", "mein", "Rat", ",", "der", "dir", "am", "bes\u00b7ten", "frommt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und holt der Wolf dich etwa ein,", "tokens": ["Und", "holt", "der", "Wolf", "dich", "et\u00b7wa", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So schlage unbedenklich drein,", "tokens": ["So", "schla\u00b7ge", "un\u00b7be\u00b7denk\u00b7lich", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Es ist nicht lange her, da\u00df man dich neu beschlagen.\u00ab", "tokens": ["Es", "ist", "nicht", "lan\u00b7ge", "her", ",", "da\u00df", "man", "dich", "neu", "be\u00b7schla\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PTKVZ", "$,", "KOUS", "PIS", "PRF", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er brauchte weiter nichts zu sagen,", "tokens": ["Er", "brauch\u00b7te", "wei\u00b7ter", "nichts", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Schon nahm der Wolf den Esel fest beim Kragen.", "tokens": ["Schon", "nahm", "der", "Wolf", "den", "E\u00b7sel", "fest", "beim", "Kra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich meine, da\u00df es stets das beste sei,", "tokens": ["Ich", "mei\u00b7ne", ",", "da\u00df", "es", "stets", "das", "bes\u00b7te", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der eine steht dem andern treulich bei.", "tokens": ["Der", "ei\u00b7ne", "steht", "dem", "an\u00b7dern", "treu\u00b7lich", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}