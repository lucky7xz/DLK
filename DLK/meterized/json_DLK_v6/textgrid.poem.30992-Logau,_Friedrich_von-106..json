{"textgrid.poem.30992": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "106.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilstu eine Lust dir kauffen? Kauff ein Fa\u00df voll guten Wein,", "tokens": ["Wils\u00b7tu", "ei\u00b7ne", "Lust", "dir", "kauf\u00b7fen", "?", "Kauff", "ein", "Fa\u00df", "voll", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$.", "NN", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Bitt ein Anzahl gute Br\u00fcder: ach, was werden Narren seyn!", "tokens": ["Bitt", "ein", "An\u00b7zahl", "gu\u00b7te", "Br\u00fc\u00b7der", ":", "ach", ",", "was", "wer\u00b7den", "Nar\u00b7ren", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$.", "XY", "$,", "PWS", "VAFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}