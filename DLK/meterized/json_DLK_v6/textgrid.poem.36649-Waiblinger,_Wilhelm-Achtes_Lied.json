{"textgrid.poem.36649": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch umflattern mich die frohen", "tokens": ["Noch", "um\u00b7flat\u00b7tern", "mich", "die", "fro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Saturnalischen Gestalten,", "tokens": ["Sa\u00b7tur\u00b7na\u00b7li\u00b7schen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch von jenem Rosenscheine", "tokens": ["Noch", "von", "je\u00b7nem", "Ro\u00b7sen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchl' ich selig mich umwittert,", "tokens": ["F\u00fchl'", "ich", "se\u00b7lig", "mich", "um\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch von kindisch muntrer Schalkheit", "tokens": ["Noch", "von", "kin\u00b7disch", "mun\u00b7trer", "Schalk\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald geschmeichelt, bald gef\u00e4hrdet,", "tokens": ["Bald", "ge\u00b7schmei\u00b7chelt", ",", "bald", "ge\u00b7f\u00e4hr\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch vom Lebenssturm umrauscht,", "tokens": ["Noch", "vom", "Le\u00b7bens\u00b7sturm", "um\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der zum wilden Tanz begeistert.", "tokens": ["Der", "zum", "wil\u00b7den", "Tanz", "be\u00b7geis\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch die T\u00e4uschung nur der Sinne,", "tokens": ["Doch", "die", "T\u00e4u\u00b7schung", "nur", "der", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Erinn'rung des Genusses", "tokens": ["Die", "Er\u00b7inn'\u00b7rung", "des", "Ge\u00b7nus\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es nur! Von keinem Fenster", "tokens": ["Ist", "es", "nur", "!", "Von", "kei\u00b7nem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$.", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Balkone weht ein Teppich,", "tokens": ["Und", "Bal\u00b7ko\u00b7ne", "weht", "ein", "Tep\u00b7pich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Keine Veilchenstr\u00e4u\u00dfe fliegen", "tokens": ["Kei\u00b7ne", "Veil\u00b7chen\u00b7str\u00e4u\u00b7\u00dfe", "flie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mehr zu sch\u00f6ngeschm\u00fcckten Frauen,", "tokens": ["Mehr", "zu", "sch\u00f6n\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der kurzen Zier beraubt,", "tokens": ["Und", "der", "kur\u00b7zen", "Zier", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Trauert Rom in seiner Stille.", "tokens": ["Trau\u00b7ert", "Rom", "in", "sei\u00b7ner", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tr\u00fcbte sich das Lied des S\u00e4ngers,", "tokens": ["Tr\u00fcb\u00b7te", "sich", "das", "Lied", "des", "S\u00e4n\u00b7gers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei der eigenen Entt\u00e4uschung,", "tokens": ["Bei", "der", "ei\u00b7ge\u00b7nen", "Ent\u00b7t\u00e4u\u00b7schung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei den langen Trauertagen", "tokens": ["Bei", "den", "lan\u00b7gen", "Trau\u00b7er\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit gerechtem Schmerz verweilend?", "tokens": ["Mit", "ge\u00b7rech\u00b7tem", "Schmerz", "ver\u00b7wei\u00b7lend", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Klagt' es um der Liebe Freuden,", "tokens": ["Klagt'", "es", "um", "der", "Lie\u00b7be", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um die Freunde, die Gespielen,", "tokens": ["Um", "die", "Freun\u00b7de", ",", "die", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Um des Ruhmes goldnen Wahn,", "tokens": ["Um", "des", "Ruh\u00b7mes", "gold\u00b7nen", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unersetzliche Verluste?", "tokens": ["Un\u00b7er\u00b7setz\u00b7li\u00b7che", "Ver\u00b7lus\u00b7te", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "K\u00f6nnt' es aller Lust entsagen,", "tokens": ["K\u00f6nnt'", "es", "al\u00b7ler", "Lust", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Haupt, f\u00fcr Myrtenkr\u00e4nze", "tokens": ["Und", "das", "Haupt", ",", "f\u00fcr", "Myr\u00b7ten\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bacchuslaub und sanfte Rosen,", "tokens": ["Bac\u00b7chus\u00b7laub", "und", "sanf\u00b7te", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vielleicht bestimmt f\u00fcr Lorbeer,", "tokens": ["Und", "viel\u00b7leicht", "be\u00b7stimmt", "f\u00fcr", "Lor\u00b7beer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollte Todtenasche decken?", "tokens": ["Soll\u00b7te", "Tod\u00b7ten\u00b7asc\u00b7he", "de\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein, auch dies ist schon vor\u00fcber,", "tokens": ["Nein", ",", "auch", "dies", "ist", "schon", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PDS", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein neues Leben scheint", "tokens": ["Und", "ein", "neu\u00b7es", "Le\u00b7ben", "scheint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich dem S\u00e4nger zu entfalten.", "tokens": ["Sich", "dem", "S\u00e4n\u00b7ger", "zu", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn der Fr\u00fchling naht in seiner", "tokens": ["Denn", "der", "Fr\u00fch\u00b7ling", "naht", "in", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieblichkeit, in s\u00fc\u00dfer W\u00e4rme", "tokens": ["Lieb\u00b7lich\u00b7keit", ",", "in", "s\u00fc\u00b7\u00dfer", "W\u00e4r\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wacht er auf, und frohe V\u00f6gel", "tokens": ["Wacht", "er", "auf", ",", "und", "fro\u00b7he", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "$,", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singen in des Mandels Bl\u00fcte;", "tokens": ["Sin\u00b7gen", "in", "des", "Man\u00b7dels", "Bl\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwindet ja im holden S\u00fcden", "tokens": ["Schwin\u00b7det", "ja", "im", "hol\u00b7den", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nie der Lenz, der sch\u00f6ne J\u00fcngling,", "tokens": ["Nie", "der", "Lenz", ",", "der", "sch\u00f6\u00b7ne", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ganz hinweg \u2013 er schlummert nur", "tokens": ["Ganz", "hin\u00b7weg", "\u2013", "er", "schlum\u00b7mert", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kurze Zeit im Lorbeerschatten.", "tokens": ["Kur\u00b7ze", "Zeit", "im", "Lor\u00b7beer\u00b7schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und es regte nicht dem S\u00e4nger", "tokens": ["Und", "es", "reg\u00b7te", "nicht", "dem", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchlingslust den frischen Busen?", "tokens": ["Fr\u00fch\u00b7lings\u00b7lust", "den", "fri\u00b7schen", "Bu\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Mandelb\u00e4ume bl\u00fchen,", "tokens": ["Wenn", "die", "Man\u00b7del\u00b7b\u00e4u\u00b7me", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keimte nichts in seinem Herzen?", "tokens": ["Ke\u00b7im\u00b7te", "nichts", "in", "sei\u00b7nem", "Her\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn die milden L\u00fcfte jubeln", "tokens": ["Wenn", "die", "mil\u00b7den", "L\u00fcf\u00b7te", "ju\u00b7beln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vom Gesang der V\u00f6gel, griffe", "tokens": ["Vom", "Ge\u00b7sang", "der", "V\u00f6\u00b7gel", ",", "grif\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht zur Leier seine Hand,", "tokens": ["Nicht", "zur", "Lei\u00b7er", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Um ein heitres Lied zu singen?", "tokens": ["Um", "ein", "heit\u00b7res", "Lied", "zu", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Nein! Wer k\u00f6nnte solcher Allmacht,", "tokens": ["Nein", "!", "Wer", "k\u00f6nn\u00b7te", "sol\u00b7cher", "All\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWS", "VMFIN", "PIAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Solcher Lockung widerstehen!", "tokens": ["Sol\u00b7cher", "Lo\u00b7ckung", "wi\u00b7der\u00b7ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Neues f\u00fchlt er in sich werden,", "tokens": ["Neu\u00b7es", "f\u00fchlt", "er", "in", "sich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PRF", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manche Hoffnung sich erf\u00fcllen,", "tokens": ["Man\u00b7che", "Hoff\u00b7nung", "sich", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Zukunft, leicht und selig,", "tokens": ["Ei\u00b7ne", "Zu\u00b7kunft", ",", "leicht", "und", "se\u00b7lig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht er fern her\u00fcberschweben,", "tokens": ["Sieht", "er", "fern", "her\u00b7\u00fc\u00b7bersc\u00b7hwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sei's auch, da\u00df er ", "tokens": ["Sei's", "auch", ",", "da\u00df", "er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "$,", "KOUS", "PPER"], "meter": "+-++", "measure": "unknown.measure.tri"}, "line.8": {"text": "Im Elysium doch erreiche!", "tokens": ["Im", "E\u00b7ly\u00b7si\u00b7um", "doch", "er\u00b7rei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}