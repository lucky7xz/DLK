{"textgrid.poem.51104": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3. Fr\u00fchgesicht", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es donnert \u00fcber der Pfaffenga\u00df", "tokens": ["Es", "don\u00b7nert", "\u00fc\u00b7ber", "der", "Pfaf\u00b7fen\u00b7ga\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des weiland Heil'gen R\u00f6mischen Reiches", "tokens": ["Des", "wei\u00b7land", "Heil'\u00b7gen", "R\u00f6\u00b7mi\u00b7schen", "Rei\u00b7ches"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Gottes Heerschild j\u00e4hen Streiches;", "tokens": ["Wie", "Got\u00b7tes", "Heer\u00b7schild", "j\u00e4\u00b7hen", "Strei\u00b7ches", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Morgen d\u00e4mmert rosig bla\u00df.", "tokens": ["Der", "Mor\u00b7gen", "d\u00e4m\u00b7mert", "ro\u00b7sig", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie der Schlag weithin verhallt,", "tokens": ["Und", "wie", "der", "Schlag", "weit\u00b7hin", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wogt eine graue Nebelmasse,", "tokens": ["Wogt", "ei\u00b7ne", "grau\u00b7e", "Ne\u00b7bel\u00b7mas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als z\u00f6g ein Heervolk seine Stra\u00dfe,", "tokens": ["Als", "z\u00f6g", "ein", "Heer\u00b7volk", "sei\u00b7ne", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das auf den Wassern endlos wallt.", "tokens": ["Das", "auf", "den", "Was\u00b7sern", "end\u00b7los", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Zwielicht raget Dom an Dom,", "tokens": ["Im", "Zwie\u00b7licht", "ra\u00b7get", "Dom", "an", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An allen Fenstern lauscht's verstohlen;", "tokens": ["An", "al\u00b7len", "Fens\u00b7tern", "lauscht's", "ver\u00b7stoh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch auf gedankenleichten Sohlen", "tokens": ["Doch", "auf", "ge\u00b7dan\u00b7ken\u00b7leich\u00b7ten", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor\u00fcber eilt der Schattenstrom.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "eilt", "der", "Schat\u00b7ten\u00b7strom", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das rauscht und tauschet Hand und Ku\u00df,", "tokens": ["Das", "rauscht", "und", "tau\u00b7schet", "Hand", "und", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sturmhauch r\u00fchrt verj\u00e4hrte Fahnen", "tokens": ["Der", "Sturm\u00b7hauch", "r\u00fchrt", "ver\u00b7j\u00e4hr\u00b7te", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie neues Hoffen, altes Mahnen,", "tokens": ["Wie", "neu\u00b7es", "Hof\u00b7fen", ",", "al\u00b7tes", "Mah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erschauernd wie ein Geistergru\u00df.", "tokens": ["Er\u00b7schau\u00b7ernd", "wie", "ein", "Geis\u00b7ter\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was brav und mannhaft ist, vereint", "tokens": ["Was", "brav", "und", "mann\u00b7haft", "ist", ",", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zieht es, den letzten Streit zu schlagen;", "tokens": ["Zieht", "es", ",", "den", "letz\u00b7ten", "Streit", "zu", "schla\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es klirrt zu Fu\u00df, zu Ro\u00df und Wagen,", "tokens": ["Es", "klirrt", "zu", "Fu\u00df", ",", "zu", "Ro\u00df", "und", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Freunde wird der alte Feind,", "tokens": ["Zum", "Freun\u00b7de", "wird", "der", "al\u00b7te", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und neben Siegfried reitet Hagen.", "tokens": ["Und", "ne\u00b7ben", "Sieg\u00b7fried", "rei\u00b7tet", "Ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}