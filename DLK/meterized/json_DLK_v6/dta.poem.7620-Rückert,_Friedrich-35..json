{"dta.poem.7620": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sollst den Stand, auf dem du stehest, nicht verkennen,", "tokens": ["Du", "sollst", "den", "Stand", ",", "auf", "dem", "du", "ste\u00b7hest", ",", "nicht", "ver\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Ird'schen Ewiges nicht eigenm\u00e4chtig trennen.", "tokens": ["Vom", "Ird'\u00b7schen", "E\u00b7wi\u00b7ges", "nicht", "ei\u00b7gen\u00b7m\u00e4ch\u00b7tig", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu beiden bist du da, der Erde Kampf zu streiten,", "tokens": ["Zu", "bei\u00b7den", "bist", "du", "da", ",", "der", "Er\u00b7de", "Kampf", "zu", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADV", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dich zum Frieden vor des Himmels zu bereiten.", "tokens": ["Und", "dich", "zum", "Frie\u00b7den", "vor", "des", "Him\u00b7mels", "zu", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer feige Frieden nur sucht f\u00fcr sein eigen Theil,", "tokens": ["Wer", "fei\u00b7ge", "Frie\u00b7den", "nur", "sucht", "f\u00fcr", "sein", "ei\u00b7gen", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird zum Verr\u00e4ther an der Welt gemeinem Heil.", "tokens": ["Wird", "zum", "Ver\u00b7r\u00e4\u00b7ther", "an", "der", "Welt", "ge\u00b7mei\u00b7nem", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zu f\u00f6rdern Menschengl\u00fcck mit aller Kraft hienieden,", "tokens": ["Zu", "f\u00f6r\u00b7dern", "Men\u00b7schen\u00b7gl\u00fcck", "mit", "al\u00b7ler", "Kraft", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Opfer ist zu gro\u00df, als nur der Seele Frieden.", "tokens": ["Kein", "Op\u00b7fer", "ist", "zu", "gro\u00df", ",", "als", "nur", "der", "See\u00b7le", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKA", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch la\u00df von keiner Macht, von keinem Ruhm dich zwingen,", "tokens": ["Doch", "la\u00df", "von", "kei\u00b7ner", "Macht", ",", "von", "kei\u00b7nem", "Ruhm", "dich", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von keiner Liebe selbst, dis Opfer ihr zu bringen.", "tokens": ["Von", "kei\u00b7ner", "Lie\u00b7be", "selbst", ",", "dis", "Op\u00b7fer", "ihr", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$,", "PDS", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das ist nicht Eigensucht, noch schwerer Pflichten Scheue,", "tokens": ["Das", "ist", "nicht", "Ei\u00b7gen\u00b7sucht", ",", "noch", "schwe\u00b7rer", "Pflich\u00b7ten", "Scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "NN", "$,", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist die deinem Ich, dem ew'gen, schuld'ge Treue.", "tokens": ["Es", "ist", "die", "dei\u00b7nem", "Ich", ",", "dem", "ew'\u00b7gen", ",", "schuld'\u00b7ge", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PPOSAT", "PPER", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}