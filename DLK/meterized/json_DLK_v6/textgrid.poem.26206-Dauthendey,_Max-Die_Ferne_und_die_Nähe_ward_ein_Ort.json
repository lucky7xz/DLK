{"textgrid.poem.26206": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Ferne und die N\u00e4he ward ein Ort", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und dich und mich, uns trug die Flamme fort,", "tokens": ["Und", "dich", "und", "mich", ",", "uns", "trug", "die", "Flam\u00b7me", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Ferne und die N\u00e4he ward ein Ort.", "tokens": ["Die", "Fer\u00b7ne", "und", "die", "N\u00e4\u00b7he", "ward", "ein", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir Menschen wachsen mit den B\u00e4umen auf", "tokens": ["Wir", "Men\u00b7schen", "wach\u00b7sen", "mit", "den", "B\u00e4u\u00b7men", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und werden wie die B\u00e4ume einst zum Scheiterhauf.", "tokens": ["Und", "wer\u00b7den", "wie", "die", "B\u00e4u\u00b7me", "einst", "zum", "Schei\u00b7ter\u00b7hauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es z\u00fcnden sich, wie Scheit an Scheit, so Mann an Weib", "tokens": ["Es", "z\u00fcn\u00b7den", "sich", ",", "wie", "Scheit", "an", "Scheit", ",", "so", "Mann", "an", "Weib"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PWAV", "NN", "APPR", "NN", "$,", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und lodern von der Erde fort als einziger Leib;", "tokens": ["Und", "lo\u00b7dern", "von", "der", "Er\u00b7de", "fort", "als", "ein\u00b7zi\u00b7ger", "Leib", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Sind Freudenfeuer in der kurzen Nacht", "tokens": ["Sind", "Freu\u00b7den\u00b7feu\u00b7er", "in", "der", "kur\u00b7zen", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und haben sich auf Feuerf\u00fc\u00dfen aufgemacht", "tokens": ["Und", "ha\u00b7ben", "sich", "auf", "Feu\u00b7er\u00b7f\u00fc\u00b7\u00dfen", "auf\u00b7ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PRF", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und wissen nichts von ihrer eigenen Pracht.", "tokens": ["Und", "wis\u00b7sen", "nichts", "von", "ih\u00b7rer", "ei\u00b7ge\u00b7nen", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}