{"dta.poem.13543": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So eitel k\u00fcnstlich haben sie verwoben", "tokens": ["So", "ei\u00b7tel", "k\u00fcnst\u00b7lich", "ha\u00b7ben", "sie", "ver\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kunst, die selber sie nicht gl\u00e4ubig achten,", "tokens": ["Die", "Kunst", ",", "die", "sel\u00b7ber", "sie", "nicht", "gl\u00e4u\u00b7big", "ach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und S\u00fcnde so in diese Unschuld brachten.", "tokens": ["Und", "S\u00fcn\u00b7de", "so", "in", "die\u00b7se", "Un\u00b7schuld", "brach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer unterscheidet, was noch stammt von oben?", "tokens": ["Wer", "un\u00b7ter\u00b7schei\u00b7det", ",", "was", "noch", "stammt", "von", "o\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}