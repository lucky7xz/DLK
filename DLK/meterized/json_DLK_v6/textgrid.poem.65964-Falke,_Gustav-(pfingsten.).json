{"textgrid.poem.65964": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "(pfingsten.)", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maisonnentag und fr\u00f6hliche Gesichter.", "tokens": ["Mai\u00b7son\u00b7nen\u00b7tag", "und", "fr\u00f6h\u00b7li\u00b7che", "Ge\u00b7sich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Lachen liegt es in der Luft und Scherzen.", "tokens": ["Wie", "La\u00b7chen", "liegt", "es", "in", "der", "Luft", "und", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Duftwolken ziehen. Tausend bunte Lichter:", "tokens": ["Duft\u00b7wol\u00b7ken", "zie\u00b7hen", ".", "Tau\u00b7send", "bun\u00b7te", "Lich\u00b7ter", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "CARD", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Syringen, Rotdorn, der Kastanie Kerzen. \u2013", "tokens": ["Sy\u00b7rin\u00b7gen", ",", "Rot\u00b7dorn", ",", "der", "Kas\u00b7ta\u00b7nie", "Ker\u00b7zen", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "NN", "$,", "ART", "NN", "NN", "$.", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Bourgoisphilister: Frohgenussvernichter,", "tokens": ["Bour\u00b7go\u00b7is\u00b7phi\u00b7lis\u00b7ter", ":", "Froh\u00b7ge\u00b7nuss\u00b7ver\u00b7nich\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Geldprotz auf R\u00e4dern, reitende Kommerzen,", "tokens": ["Geld\u00b7protz", "auf", "R\u00e4\u00b7dern", ",", "rei\u00b7ten\u00b7de", "Kom\u00b7mer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Zu Fu\u00df im Staub zwei junge deutsche Dichter", "tokens": ["Zu", "Fu\u00df", "im", "Staub", "zwei", "jun\u00b7ge", "deut\u00b7sche", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN", "CARD", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit leerem Beutel und mit vollem Herzen.", "tokens": ["Mit", "lee\u00b7rem", "Beu\u00b7tel", "und", "mit", "vol\u00b7lem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}