{"dta.poem.9695": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Senicio ein alter kerl ist kranck.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr \u00e4rtzte kommt herbey/", "tokens": ["Ihr", "\u00e4rtz\u00b7te", "kommt", "her\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Senicio liegt sehr gef\u00e4hrlich nieder/", "tokens": ["Se\u00b7ni\u00b7cio", "liegt", "sehr", "ge\u00b7f\u00e4hr\u00b7lich", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jhr fragt/ woher die kranckheit kommen sey?", "tokens": ["Ihr", "fragt", "/", "wo\u00b7her", "die", "kran\u00b7ck\u00b7heit", "kom\u00b7men", "sey", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich wei\u00df es nicht/ obs daher r\u00fchren mag/", "tokens": ["Ich", "wei\u00df", "es", "nicht", "/", "obs", "da\u00b7her", "r\u00fch\u00b7ren", "mag", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$(", "KOUS", "PAV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als ihm vergangnen tag", "tokens": ["Als", "ihm", "ver\u00b7gang\u00b7nen", "tag"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die junge magd die stieffel ausgezogen/", "tokens": ["Die", "jun\u00b7ge", "magd", "die", "stief\u00b7fel", "aus\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So schlug ihm was in unterschiedne glieder;", "tokens": ["So", "schlug", "ihm", "was", "in", "un\u00b7ter\u00b7schied\u00b7ne", "glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRELS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die kranckheit ist mir zwar noch unbekant/", "tokens": ["Die", "kran\u00b7ck\u00b7heit", "ist", "mir", "zwar", "noch", "un\u00b7be\u00b7kant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ich w\u00e4hne nur/ es sey der kalte brand.", "tokens": ["Ich", "w\u00e4h\u00b7ne", "nur", "/", "es", "sey", "der", "kal\u00b7te", "brand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}