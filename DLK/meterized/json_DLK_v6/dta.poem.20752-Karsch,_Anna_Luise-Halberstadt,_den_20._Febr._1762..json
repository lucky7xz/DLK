{"dta.poem.20752": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Halberstadt, den 20. Febr. 1762.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Thier hat blo\u00df Instinkt; der Mensch hat                 ", "tokens": ["Das", "Thier", "hat", "blo\u00df", "Ins\u00b7tinkt", ";", "der", "Mensch", "hat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "einen Geist,", "tokens": ["ei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Der mit Gedanken schnell die ganze Welt durchreist.", "tokens": ["Der", "mit", "Ge\u00b7dan\u00b7ken", "schnell", "die", "gan\u00b7ze", "Welt", "durc\u00b7hreist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Hund erinnert sich; ein Biber scheint zu denken;", "tokens": ["Ein", "Hund", "e\u00b7rin\u00b7nert", "sich", ";", "ein", "Bi\u00b7ber", "scheint", "zu", "den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch keiner kann den Blick bis an den Himmel lenken.", "tokens": ["Doch", "kei\u00b7ner", "kann", "den", "Blick", "bis", "an", "den", "Him\u00b7mel", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht zur Unsterblichkeit gemacht", "tokens": ["Nicht", "zur", "U\u00b7nsterb\u00b7lich\u00b7keit", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "VVPP"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Sind Adler gro\u00df und stolz; ihr Tod ist tiefe Nacht.", "tokens": ["Sind", "Ad\u00b7ler", "gro\u00df", "und", "stolz", ";", "ihr", "Tod", "ist", "tie\u00b7fe", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "KON", "ADJD", "$.", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des Menschen Seele nur f\u00fchlt ewig ihre Jugend,", "tokens": ["Des", "Men\u00b7schen", "See\u00b7le", "nur", "f\u00fchlt", "e\u00b7wig", "ih\u00b7re", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erkennet Gott und sich, die Welt, die sch\u00f6ne Tugend;", "tokens": ["Er\u00b7ken\u00b7net", "Gott", "und", "sich", ",", "die", "Welt", ",", "die", "sch\u00f6\u00b7ne", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "PRF", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist voll Empfindungen und f\u00fcr Vernunft und Wiz", "tokens": ["Ist", "voll", "Emp\u00b7fin\u00b7dun\u00b7gen", "und", "f\u00fcr", "Ver\u00b7nunft", "und", "Wiz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "NN", "KON", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Ein von des Sch\u00f6pfers Hand fein ausgebauter Siz.", "tokens": ["Ein", "von", "des", "Sch\u00f6p\u00b7fers", "Hand", "fein", "aus\u00b7ge\u00b7bau\u00b7ter", "Siz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie liebet oder ha\u00dft, verwirft und wei\u00df zu w\u00e4hlen;", "tokens": ["Sie", "lie\u00b7bet", "o\u00b7der", "ha\u00dft", ",", "ver\u00b7wirft", "und", "wei\u00df", "zu", "w\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Zufall macht sie froh; ein Zufall kann sie qu\u00e4len.", "tokens": ["Ein", "Zu\u00b7fall", "macht", "sie", "froh", ";", "ein", "Zu\u00b7fall", "kann", "sie", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie denkt und ihr Begrif macht sich durch unsern", "tokens": ["Sie", "denkt", "und", "ihr", "Be\u00b7grif", "macht", "sich", "durch", "un\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Mund,", "tokens": ["Mund", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Durch unsers Auges Blick und in Geberden kund.", "tokens": ["Durch", "un\u00b7sers", "Au\u00b7ges", "Blick", "und", "in", "Ge\u00b7ber\u00b7den", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Sie freut des Lebens sich und f\u00fcrchtet nicht zu sterben;", "tokens": ["Sie", "freut", "des", "Le\u00b7bens", "sich", "und", "f\u00fcrch\u00b7tet", "nicht", "zu", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "KON", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Heiden Tugend selbst hofft ein zuk\u00fcnftigs Erben.", "tokens": ["Der", "Hei\u00b7den", "Tu\u00b7gend", "selbst", "hofft", "ein", "zu\u00b7k\u00fcnf\u00b7tigs", "Er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und die Religion verbreitet helles Licht", "tokens": ["Und", "die", "Re\u00b7li\u00b7gi\u00b7on", "ver\u00b7brei\u00b7tet", "hel\u00b7les", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "ADJA", "NN"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "In eines Christen Geist, der alle Zweifel bricht,", "tokens": ["In", "ei\u00b7nes", "Chris\u00b7ten", "Geist", ",", "der", "al\u00b7le", "Zwei\u00b7fel", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die \u00fcber ihn sich w\u00f6lken wollen.", "tokens": ["Die", "\u00fc\u00b7ber", "ihn", "sich", "w\u00f6l\u00b7ken", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wir wissen, da\u00df wir einst das Reich besitzen sollen,", "tokens": ["Wir", "wis\u00b7sen", ",", "da\u00df", "wir", "einst", "das", "Reich", "be\u00b7sit\u00b7zen", "sol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das uns bereitet ist, wohin sich alsobald", "tokens": ["Das", "uns", "be\u00b7rei\u00b7tet", "ist", ",", "wo\u00b7hin", "sich", "al\u00b7so\u00b7bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "VVPP", "VAFIN", "$,", "PWAV", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Seele schwingen wird, wenn nun der K\u00f6rper kalt", "tokens": ["Die", "See\u00b7le", "schwin\u00b7gen", "wird", ",", "wenn", "nun", "der", "K\u00f6r\u00b7per", "kalt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VAFIN", "$,", "KOUS", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bla\u00df und benetzet liegt von unsrer Freunde Thr\u00e4nen.", "tokens": ["Bla\u00df", "und", "be\u00b7net\u00b7zet", "liegt", "von", "uns\u00b7rer", "Freun\u00b7de", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie aber geht es zu, da\u00df Menschen sich nicht sehnen", "tokens": ["Wie", "a\u00b7ber", "geht", "es", "zu", ",", "da\u00df", "Men\u00b7schen", "sich", "nicht", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "NN", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nach diesem Ueberschritt? Warum str\u00e4ubt unser Herz", "tokens": ["Nach", "die\u00b7sem", "Ue\u00b7ber\u00b7schritt", "?", "Wa\u00b7rum", "str\u00e4ubt", "un\u00b7ser", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$.", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sich vor dem Tode schon bey kleiner Krankheit", "tokens": ["Sich", "vor", "dem", "To\u00b7de", "schon", "bey", "klei\u00b7ner", "Krank\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Schmerz?", "tokens": ["Schmerz", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.30": {"text": "Der Kanzelredner mag des Himmels Wonne loben;", "tokens": ["Der", "Kan\u00b7zel\u00b7red\u00b7ner", "mag", "des", "Him\u00b7mels", "Won\u00b7ne", "lo\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch wird kein weiser Mann fr\u00fch nach dem Eingang", "tokens": ["Doch", "wird", "kein", "wei\u00b7ser", "Mann", "fr\u00fch", "nach", "dem", "Ein\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "ADJA", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "toben.", "tokens": ["to\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Ein kurzes Leben ist nie unsers Kummers Schuld,", "tokens": ["Ein", "kur\u00b7zes", "Le\u00b7ben", "ist", "nie", "un\u00b7sers", "Kum\u00b7mers", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Bettler selber tr\u00e4gt den Brodsack mit Geduld.", "tokens": ["Der", "Bett\u00b7ler", "sel\u00b7ber", "tr\u00e4gt", "den", "Bro\u00b7dsack", "mit", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein Sklav in Tunis w\u00fchlt in seines R\u00e4ubers Garten,", "tokens": ["Ein", "Sklav", "in", "Tu\u00b7nis", "w\u00fchlt", "in", "sei\u00b7nes", "R\u00e4u\u00b7bers", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "In Fesseln geht er gern Kameel und Esel warten.", "tokens": ["In", "Fes\u00b7seln", "geht", "er", "gern", "Ka\u00b7me\u00b7el", "und", "E\u00b7sel", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Ein an der Ruderbank verdammter S\u00fcnder strebt", "tokens": ["Ein", "an", "der", "Ru\u00b7der\u00b7bank", "ver\u00b7damm\u00b7ter", "S\u00fcn\u00b7der", "strebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Aus Wellen an das Land, weil er so gerne lebt.", "tokens": ["Aus", "Wel\u00b7len", "an", "das", "Land", ",", "weil", "er", "so", "ger\u00b7ne", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Doch alle m\u00fcssen fort. Das z\u00e4rtlichste Umarmen", "tokens": ["Doch", "al\u00b7le", "m\u00fcs\u00b7sen", "fort", ".", "Das", "z\u00e4rt\u00b7lichs\u00b7te", "Um\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Verliebter Seelen reizt den Tod nicht zum Erbarmen.", "tokens": ["Ver\u00b7lieb\u00b7ter", "See\u00b7len", "reizt", "den", "Tod", "nicht", "zum", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich bin von Sterblichkeit zusammen nur gewebt;", "tokens": ["Ich", "bin", "von", "Sterb\u00b7lich\u00b7keit", "zu\u00b7sam\u00b7men", "nur", "ge\u00b7webt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wer gl\u00fccklich ward wie ich, der hat genug gelebt.", "tokens": ["Wer", "gl\u00fcck\u00b7lich", "ward", "wie", "ich", ",", "der", "hat", "ge\u00b7nug", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KOKOM", "PPER", "$,", "PRELS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}