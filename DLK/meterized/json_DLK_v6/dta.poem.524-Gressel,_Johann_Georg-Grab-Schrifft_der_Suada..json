{"dta.poem.524": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Grab-Schrifft der  Suada.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sch\u00f6nheit Eben-Bild/ der Treue bester Kern/", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "E\u00b7ben\u00b7Bild", "/", "der", "Treu\u00b7e", "bes\u00b7ter", "Kern", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Engel aus der Welt nach dem beliebten Wesen/", "tokens": ["Ein", "En\u00b7gel", "aus", "der", "Welt", "nach", "dem", "be\u00b7lieb\u00b7ten", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Kenschheit-Sonnen-Licht/ der Tugend Morgen-Stern/", "tokens": ["Der", "Kenschheit\u00b7Son\u00b7nen\u00b7Licht", "/", "der", "Tu\u00b7gend", "Mor\u00b7gen\u00b7S\u00b7tern", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-++-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Dis Leser/ und noch mehr/ als was du hier gelesen/", "tokens": ["Dis", "Le\u00b7ser", "/", "und", "noch", "mehr", "/", "als", "was", "du", "hier", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$(", "KON", "ADV", "ADV", "$(", "KOUS", "PRELS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Liegt unter diesen Stein in d\u00fcstrer Nacht verh\u00fcllt/", "tokens": ["Liegt", "un\u00b7ter", "die\u00b7sen", "Stein", "in", "d\u00fcst\u00b7rer", "Nacht", "ver\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht wunder dich mein Freund/ da\u00df ich so viel geschrieben/", "tokens": ["Nicht", "wun\u00b7der", "dich", "mein", "Freund", "/", "da\u00df", "ich", "so", "viel", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "PPER", "PPOSAT", "NN", "$(", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Leib der ", "tokens": ["Der", "Leib", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Kern/ Engel/ Sonn\u2019 und Stern sind in dem Geiste blieben.", "tokens": ["Kern", "/", "En\u00b7gel", "/", "Sonn'", "und", "Stern", "sind", "in", "dem", "Geis\u00b7te", "blie\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}