{"dta.poem.7743": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einem H\u00f6fling wird erz\u00e4hlt auf diesem Blatte,", "tokens": ["Von", "ei\u00b7nem", "H\u00f6f\u00b7ling", "wird", "er\u00b7z\u00e4hlt", "auf", "die\u00b7sem", "Blat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df kl\u00fcger als er selbst der Hund war, den er hatte.", "tokens": ["Da\u00df", "kl\u00fc\u00b7ger", "als", "er", "selbst", "der", "Hund", "war", ",", "den", "er", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von diesem immer ward, so oft er mu\u00dfte kommen", "tokens": ["Von", "die\u00b7sem", "im\u00b7mer", "ward", ",", "so", "oft", "er", "mu\u00df\u00b7te", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADV", "VAFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Dienst ins F\u00fcrstenschlo\u00df, das H\u00fcndlein mitgenommen.", "tokens": ["Zum", "Dienst", "ins", "F\u00fcrs\u00b7ten\u00b7schlo\u00df", ",", "das", "H\u00fcnd\u00b7lein", "mit\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Stets lief das H\u00fcndlein nach, bis da\u00df einmal es zauste", "tokens": ["Stets", "lief", "das", "H\u00fcnd\u00b7lein", "nach", ",", "bis", "da\u00df", "ein\u00b7mal", "es", "zaus\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "KOUS", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Schlo\u00df des F\u00fcrsten Hund, davor ihm k\u00fcnftig grauste.", "tokens": ["Im", "Schlo\u00df", "des", "F\u00fcrs\u00b7ten", "Hund", ",", "da\u00b7vor", "ihm", "k\u00fcnf\u00b7tig", "graus\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$,", "PAV", "PPER", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Seit folgt' es seinem Herrn nicht weiter als zur Pforte,", "tokens": ["Seit", "folgt'", "es", "sei\u00b7nem", "Herrn", "nicht", "wei\u00b7ter", "als", "zur", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wartete, bis er herauskam, an dem Orte.", "tokens": ["Und", "war\u00b7te\u00b7te", ",", "bis", "er", "her\u00b7aus\u00b7kam", ",", "an", "dem", "Or\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Da sprach der Hofmann selbst: Mein Hund ist viel gescheiter,", "tokens": ["Da", "sprach", "der", "Hof\u00b7mann", "selbst", ":", "Mein", "Hund", "ist", "viel", "ge\u00b7schei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er zur Pforte geht des Schlosses und nicht weiter,", "tokens": ["Da\u00df", "er", "zur", "Pfor\u00b7te", "geht", "des", "Schlos\u00b7ses", "und", "nicht", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "KON", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Weil drin einmal gezaust ihm wurden Fell und Glieder,", "tokens": ["Weil", "drin", "ein\u00b7mal", "ge\u00b7zaust", "ihm", "wur\u00b7den", "Fell", "und", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "VVFIN", "PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ich, so oft gezaust, hineingeh' immer wieder.", "tokens": ["Da", "ich", ",", "so", "oft", "ge\u00b7zaust", ",", "hin\u00b7ein\u00b7geh'", "im\u00b7mer", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADV", "VVPP", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}