{"textgrid.poem.56311": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonne bringt es an den Tag", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gem\u00e4chlich in der Werkstatt sa\u00df", "tokens": ["Ge\u00b7m\u00e4ch\u00b7lich", "in", "der", "Werk\u00b7statt", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Fr\u00fchtrunk Meister Nikolas,", "tokens": ["Zum", "Fr\u00fch\u00b7trunk", "Meis\u00b7ter", "Ni\u00b7ko\u00b7las", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die junge Hausfrau schenkt' ihm ein,", "tokens": ["Die", "jun\u00b7ge", "Haus\u00b7frau", "schenkt'", "ihm", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es war im heitern Sonnenschein. \u2013", "tokens": ["Es", "war", "im", "hei\u00b7tern", "Son\u00b7nen\u00b7schein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne bringt es an den Tag.", "tokens": ["Die", "Son\u00b7ne", "bringt", "es", "an", "den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sonne blinkt von der Schale Rand,", "tokens": ["Die", "Son\u00b7ne", "blinkt", "von", "der", "Scha\u00b7le", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Malt zitternde Kringeln an die Wand,", "tokens": ["Malt", "zit\u00b7tern\u00b7de", "Krin\u00b7geln", "an", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wie den Schein er ins Auge fa\u00dft,", "tokens": ["Und", "wie", "den", "Schein", "er", "ins", "Au\u00b7ge", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So spricht er f\u00fcr sich, indem er erbla\u00dft:", "tokens": ["So", "spricht", "er", "f\u00fcr", "sich", ",", "in\u00b7dem", "er", "er\u00b7bla\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbdu bringst es doch nicht an den Tag.\u00ab", "tokens": ["\u00bb", "du", "bringst", "es", "doch", "nicht", "an", "den", "Tag", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbwer nicht? was nicht?\u00ab die Frau fragt gleich,", "tokens": ["\u00bb", "wer", "nicht", "?", "was", "nicht", "?", "\u00ab", "die", "Frau", "fragt", "gleich", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PTKNEG", "$.", "PWS", "PTKNEG", "$.", "$(", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwas stierst du so an? was wirst du so bleich?\u00ab", "tokens": ["\u00bb", "was", "stierst", "du", "so", "an", "?", "was", "wirst", "du", "so", "bleich", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PWS", "VAFIN", "PPER", "ADV", "ADJD", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und er darauf: \u00bbSei still, nur still;", "tokens": ["Und", "er", "da\u00b7rauf", ":", "\u00bb", "Sei", "still", ",", "nur", "still", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "$.", "$(", "VAIMP", "ADJD", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich's doch nicht sagen kann, noch will.", "tokens": ["Ich's", "doch", "nicht", "sa\u00b7gen", "kann", ",", "noch", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne bringt's nicht an den Tag.\u00ab", "tokens": ["Die", "Son\u00b7ne", "bringt's", "nicht", "an", "den", "Tag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Frau nur dringender forscht und fragt,", "tokens": ["Die", "Frau", "nur", "drin\u00b7gen\u00b7der", "forscht", "und", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Schmeicheln ihn und Hadern plagt,", "tokens": ["Mit", "Schmei\u00b7cheln", "ihn", "und", "Ha\u00b7dern", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit s\u00fc\u00dfem und mit bitterm Wort,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "und", "mit", "bit\u00b7term", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie fragt und plagt ihn fort und fort:", "tokens": ["Sie", "fragt", "und", "plagt", "ihn", "fort", "und", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbwas bringt die Sonne nicht an den Tag?\u00ab", "tokens": ["\u00bb", "was", "bringt", "die", "Son\u00b7ne", "nicht", "an", "den", "Tag", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbnein, nimmermehr!\u00ab \u2013 \u00bbDu sagst es mir noch.\u00ab", "tokens": ["\u00bb", "nein", ",", "nim\u00b7mer\u00b7mehr", "!", "\u00ab", "\u2013", "\u00bb", "Du", "sagst", "es", "mir", "noch", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "ADV", "$.", "$(", "$(", "$(", "PPER", "VVFIN", "PPER", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbich sag es nicht.\u00ab \u2013 \u00bbDu sagst es mir doch.\u00ab \u2013", "tokens": ["\u00bb", "ich", "sag", "es", "nicht", ".", "\u00ab", "\u2013", "\u00bb", "Du", "sagst", "es", "mir", "doch", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "$(", "$(", "PPER", "VVFIN", "PPER", "PPER", "ADV", "$.", "$(", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da ward zuletzt er m\u00fcd und schwach,", "tokens": ["Da", "ward", "zu\u00b7letzt", "er", "m\u00fcd", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gab der Ungest\u00fcmen nach. \u2013", "tokens": ["Und", "gab", "der", "Un\u00b7ge\u00b7st\u00fc\u00b7men", "nach", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne bringt es an den Tag.", "tokens": ["Die", "Son\u00b7ne", "bringt", "es", "an", "den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbauf der Wanderschaft, 's sind zwanzig Jahr,", "tokens": ["\u00bb", "auf", "der", "Wan\u00b7der\u00b7schaft", ",", "'s", "sind", "zwan\u00b7zig", "Jahr", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da traf es mich einst gar sonderbar,", "tokens": ["Da", "traf", "es", "mich", "einst", "gar", "son\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich hatt nicht Geld, nicht Ranzen, noch Schuh',", "tokens": ["Ich", "hatt", "nicht", "Geld", ",", "nicht", "Ran\u00b7zen", ",", "noch", "Schuh'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "War hungrig und durstig und zornig dazu. \u2013", "tokens": ["War", "hung\u00b7rig", "und", "durs\u00b7tig", "und", "zor\u00b7nig", "da\u00b7zu", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "PAV", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Sonne bringt's nicht an den Tag.", "tokens": ["Die", "Son\u00b7ne", "bringt's", "nicht", "an", "den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da kam mir just ein Jud in die Quer,", "tokens": ["Da", "kam", "mir", "just", "ein", "Jud", "in", "die", "Quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ringsher war's still und menschenleer:", "tokens": ["Rings\u00b7her", "wa\u00b7r's", "still", "und", "men\u00b7schen\u00b7leer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KON", "NN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Du hilfst mir, Hund, aus meiner Not;", "tokens": ["Du", "hilfst", "mir", ",", "Hund", ",", "aus", "mei\u00b7ner", "Not", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Beutel her, sonst schlag ich dich tot!", "tokens": ["Den", "Beu\u00b7tel", "her", ",", "sonst", "schlag", "ich", "dich", "tot", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die Sonne bringt's nicht an den Tag.", "tokens": ["Die", "Son\u00b7ne", "bringt's", "nicht", "an", "den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und er: Vergie\u00dfe nicht mein Blut,", "tokens": ["Und", "er", ":", "Ver\u00b7gie\u00b7\u00dfe", "nicht", "mein", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Acht Pfennige sind mein ganzes Gut!", "tokens": ["Acht", "Pfen\u00b7ni\u00b7ge", "sind", "mein", "gan\u00b7zes", "Gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich glaubt ihm nicht, und fiel ihn an;", "tokens": ["Ich", "glaubt", "ihm", "nicht", ",", "und", "fiel", "ihn", "an", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er war ein alter, schwacher Mann \u2013", "tokens": ["Er", "war", "ein", "al\u00b7ter", ",", "schwa\u00b7cher", "Mann", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne bringt's nicht an den Tag.", "tokens": ["Die", "Son\u00b7ne", "bringt's", "nicht", "an", "den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So r\u00fccklings lag er blutend da,", "tokens": ["So", "r\u00fcck\u00b7lings", "lag", "er", "blu\u00b7tend", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein brechendes Aug in die Sonne sah;", "tokens": ["Sein", "bre\u00b7chen\u00b7des", "Aug", "in", "die", "Son\u00b7ne", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Noch hob er zuckend die Hand empor,", "tokens": ["Noch", "hob", "er", "zu\u00b7ckend", "die", "Hand", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch schrie er r\u00f6chelnd mir ins Ohr:", "tokens": ["Noch", "schrie", "er", "r\u00f6\u00b7chelnd", "mir", "ins", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne bringt es an den Tag.", "tokens": ["Die", "Son\u00b7ne", "bringt", "es", "an", "den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich macht ihn schnell noch vollends stumm,", "tokens": ["Ich", "macht", "ihn", "schnell", "noch", "vol\u00b7lends", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kehrt ihm die Taschen um und um:", "tokens": ["Und", "kehrt", "ihm", "die", "Ta\u00b7schen", "um", "und", "um", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "KON", "KOUI", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Acht Pfenn'ge, das war das ganze Geld.", "tokens": ["