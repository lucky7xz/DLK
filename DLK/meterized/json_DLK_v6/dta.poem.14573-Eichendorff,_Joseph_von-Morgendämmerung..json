{"dta.poem.14573": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgend\u00e4mmerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche auch will nicht die Zeit vers\u00e4umen,", "tokens": ["Die", "Ler\u00b7che", "auch", "will", "nicht", "die", "Zeit", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da solches Schallen bringt die Luft getragen,", "tokens": ["Da", "sol\u00b7ches", "Schal\u00b7len", "bringt", "die", "Luft", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schwingt sich vom Thal, eh's noch beginnt zu tagen,", "tokens": ["Schwingt", "sich", "vom", "Thal", ",", "eh's", "noch", "be\u00b7ginnt", "zu", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,", "KOUS", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Im ersten Strahl die Fl\u00fcgel sich zu s\u00e4umen.", "tokens": ["Im", "ers\u00b7ten", "Strahl", "die", "Fl\u00fc\u00b7gel", "sich", "zu", "s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}