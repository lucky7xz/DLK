{"textgrid.poem.55777": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe genossen \u2013 von Qualen zernagt,", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7nos\u00b7sen", "\u2013", "von", "Qua\u00b7len", "zer\u00b7nagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich b\u00fc\u00dfte mit Qualen \u2013 nun hat es getagt.", "tokens": ["Ich", "b\u00fc\u00df\u00b7te", "mit", "Qua\u00b7len", "\u2013", "nun", "hat", "es", "ge\u00b7tagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Die Qualen zerrinnen, die Kette zerbricht.", "tokens": ["Die", "Qua\u00b7len", "zer\u00b7rin\u00b7nen", ",", "die", "Ket\u00b7te", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wer sind meine Richter \u2013 ich kenne sie nicht.", "tokens": ["Wer", "sind", "mei\u00b7ne", "Rich\u00b7ter", "\u2013", "ich", "ken\u00b7ne", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ich kenne nicht einen, der \u00fcber mir steht,", "tokens": ["Ich", "ken\u00b7ne", "nicht", "ei\u00b7nen", ",", "der", "\u00fc\u00b7ber", "mir", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mich selber nur gr\u00fc\u00dft meiner Seele Gebet.", "tokens": ["Mich", "sel\u00b7ber", "nur", "gr\u00fc\u00dft", "mei\u00b7ner", "See\u00b7le", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den Leib k\u00f6nnt Ihr beugen und brechen im Frohn,", "tokens": ["Den", "Leib", "k\u00f6nnt", "Ihr", "beu\u00b7gen", "und", "bre\u00b7chen", "im", "Frohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Seele, die Seele, sie l\u00e4chelt Euch Hohn.", "tokens": ["Die", "See\u00b7le", ",", "die", "See\u00b7le", ",", "sie", "l\u00e4\u00b7chelt", "Euch", "Hohn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}