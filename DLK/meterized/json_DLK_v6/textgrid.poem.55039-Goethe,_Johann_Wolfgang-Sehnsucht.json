{"textgrid.poem.55039": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was zieht mir das Herz so?", "tokens": ["Was", "zieht", "mir", "das", "Herz", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was zieht mich hinaus?", "tokens": ["Was", "zieht", "mich", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und windet und schraubt mich", "tokens": ["Und", "win\u00b7det", "und", "schraubt", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Aus Zimmer und Haus?", "tokens": ["Aus", "Zim\u00b7mer", "und", "Haus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie dort sich die Wolken", "tokens": ["Wie", "dort", "sich", "die", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PRF", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Um Felsen verziehn!", "tokens": ["Um", "Fel\u00b7sen", "ver\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da m\u00f6cht ich hin\u00fcber,", "tokens": ["Da", "m\u00f6cht", "ich", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Da m\u00f6cht ich wohl hin!", "tokens": ["Da", "m\u00f6cht", "ich", "wohl", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Nun wiegt sich der Raben", "tokens": ["Nun", "wiegt", "sich", "der", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Geselliger Flug;", "tokens": ["Ge\u00b7sel\u00b7li\u00b7ger", "Flug", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich mische mich drunter", "tokens": ["Ich", "mi\u00b7sche", "mich", "drun\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PAV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und folge dem Zug.", "tokens": ["Und", "fol\u00b7ge", "dem", "Zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und Berg und Gem\u00e4uer", "tokens": ["Und", "Berg", "und", "Ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Umfittichen wir;", "tokens": ["Um\u00b7fit\u00b7ti\u00b7chen", "wir", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sie weilet da drunten;", "tokens": ["Sie", "wei\u00b7let", "da", "drun\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ich sp\u00e4he nach ihr.", "tokens": ["Ich", "sp\u00e4\u00b7he", "nach", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da kommt sie und wandelt;", "tokens": ["Da", "kommt", "sie", "und", "wan\u00b7delt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich eile sobald,", "tokens": ["Ich", "ei\u00b7le", "so\u00b7bald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein singender Vogel,", "tokens": ["Ein", "sin\u00b7gen\u00b7der", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zum buschigen Wald.", "tokens": ["Zum", "bu\u00b7schi\u00b7gen", "Wald", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie weilet und horchet", "tokens": ["Sie", "wei\u00b7let", "und", "hor\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und l\u00e4chelt mit sich:", "tokens": ["Und", "l\u00e4\u00b7chelt", "mit", "sich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "\u00bber singet so lieblich", "tokens": ["\u00bb", "er", "sin\u00b7get", "so", "lieb\u00b7lich"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und singt es an mich.\u00ab", "tokens": ["Und", "singt", "es", "an", "mich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Die scheidende Sonne", "tokens": ["Die", "schei\u00b7den\u00b7de", "Son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verguldet die H\u00f6hn;", "tokens": ["Ver\u00b7gul\u00b7det", "die", "H\u00f6hn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die sinnende Sch\u00f6ne,", "tokens": ["Die", "sin\u00b7nen\u00b7de", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie l\u00e4\u00dft es geschehn.", "tokens": ["Sie", "l\u00e4\u00dft", "es", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie wandelt am Bache", "tokens": ["Sie", "wan\u00b7delt", "am", "Ba\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Wiesen entlang,", "tokens": ["Die", "Wie\u00b7sen", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und finster und finstrer", "tokens": ["Und", "fins\u00b7ter", "und", "finst\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Umschlingt sich der Gang;", "tokens": ["Um\u00b7schlingt", "sich", "der", "Gang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Auf einmal erschein ich,", "tokens": ["Auf", "ein\u00b7mal", "er\u00b7schein", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ein blinkender Stern.", "tokens": ["Ein", "blin\u00b7ken\u00b7der", "Stern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u00bbwas gl\u00e4nzet da droben,", "tokens": ["\u00bb", "was", "gl\u00e4n\u00b7zet", "da", "dro\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So nah und so fern?\u00ab", "tokens": ["So", "nah", "und", "so", "fern", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und hast du mit Staunen", "tokens": ["Und", "hast", "du", "mit", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Leuchten erblickt;", "tokens": ["Das", "Leuch\u00b7ten", "er\u00b7blickt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ich lieg dir zu F\u00fc\u00dfen,", "tokens": ["Ich", "lieg", "dir", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Da bin ich begl\u00fcckt!", "tokens": ["Da", "bin", "ich", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+---+", "measure": "dactylic.init"}}}}}