"word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df diese Flut durch die sich legen;", "tokens": ["La\u00df", "die\u00b7se", "Flut", "durch", "die", "sich", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "APPR", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Pfropffrei\u00df trinkt des Stammes Safft", "tokens": ["Ein", "Pfropf\u00b7frei\u00df", "trinkt", "des", "Stam\u00b7mes", "Safft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und tr\u00e4get Frucht von dessen Krafft;", "tokens": ["Und", "tr\u00e4\u00b7get", "Frucht", "von", "des\u00b7sen", "Krafft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PRELAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df mich, o Lebensbaum, deswegen,", "tokens": ["La\u00df", "mich", ",", "o", "Le\u00b7bens\u00b7baum", ",", "des\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "FM", "$,", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der ich dir eingepflanzet bin,", "tokens": ["Der", "ich", "dir", "ein\u00b7ge\u00b7pflan\u00b7zet", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch einig seyn mit deinem Sinn.", "tokens": ["Auch", "ei\u00b7nig", "seyn", "mit", "dei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Dies, o mein Heyl, versprach ich dir", "tokens": ["Dies", ",", "o", "mein", "Heyl", ",", "ver\u00b7sprach", "ich", "dir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "FM", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da, wie du dich verlobt mit mir;", "tokens": ["Da", ",", "wie", "du", "dich", "ver\u00b7lobt", "mit", "mir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PRF", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was aber soll ich S\u00fcnder sagen?", "tokens": ["Was", "a\u00b7ber", "soll", "ich", "S\u00fcn\u00b7der", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich breche t\u00e4glich meine Pflicht,", "tokens": ["Ich", "bre\u00b7che", "t\u00e4g\u00b7lich", "mei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du aber brichst sie, Jesu, nicht,", "tokens": ["Du", "a\u00b7ber", "brichst", "sie", ",", "Je\u00b7su", ",", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "NE", "$,", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn diesen Trost m\u00fcst' ich verzagen.", "tokens": ["Ohn", "die\u00b7sen", "Trost", "m\u00fcst'", "ich", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es stehet unser Bund doch fest,", "tokens": ["Es", "ste\u00b7het", "un\u00b7ser", "Bund", "doch", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So lang ihn deine Treu nicht l\u00e4st.", "tokens": ["So", "lang", "ihn", "dei\u00b7ne", "Treu", "nicht", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}