], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PDS", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df es die Seel' auch rein kan machen,", "tokens": ["Da\u00df", "es", "die", "Seel'", "auch", "rein", "kan", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und den Brand, den sonst nichtes lescht,", "tokens": ["Und", "den", "Brand", ",", "den", "sonst", "nich\u00b7tes", "lescht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Mit einem Gu\u00dfe gantz hinw\u00e4scht.", "tokens": ["Mit", "ei\u00b7nem", "Gu\u00b7\u00dfe", "gantz", "hin\u00b7w\u00e4scht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ist denn der Unflat abgethan,", "tokens": ["Ist", "denn", "der", "Un\u00b7flat", "ab\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sch\u00f6n ziehst du uns, Jesu, an,", "tokens": ["Wie", "sch\u00f6n", "ziehst", "du", "uns", ",", "Je\u00b7su", ",", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "$,", "NE", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Lieb- und Unschulds-volles Leben,", "to