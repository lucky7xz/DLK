sure": "iambic.tetra"}, "line.8": {"text": "Der in mir Abba, Vater, spricht.", "tokens": ["Der", "in", "mir", "Ab\u00b7ba", ",", "Va\u00b7ter", ",", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "PPER", "NE", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dies ist die auserwehlte Zier,", "tokens": ["Dies", "ist", "die", "au\u00b7ser\u00b7wehl\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drinn du zu deiner Braut mich dir", "tokens": ["Drinn", "du", "zu", "dei\u00b7ner", "Braut", "mich", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Ewigkeit her hast erwehlet;", "tokens": ["Von", "E\u00b7wig\u00b7keit", "her", "hast", "er\u00b7weh\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APZR", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Bild wird neu mir eingeetzt,", "tokens": ["Dein", "Bild", "wird", "neu", "mir", "ein\u00b7ge\u00b7etzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich als dein Glied dir eingesetzt", "tokens": ["Ich", "als", "dein", "Glied", "dir", "ein\u00b7ge\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KOUS", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und gantz mit deiner Krafft beseelet,", "