{"textgrid.poem.49139": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Von der Heil. Tauffe", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast uns, Jesu, meine Lust,", "tokens": ["Du", "hast", "uns", ",", "Je\u00b7su", ",", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar von dem faulen S\u00fcnden-Wust", "tokens": ["Zwar", "von", "dem", "fau\u00b7len", "S\u00fcn\u00b7den\u00b7Wust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Durch dein vergo\u00dfnes Blut gewaschen,", "tokens": ["Durch", "dein", "ver\u00b7go\u00df\u00b7nes", "Blut", "ge\u00b7wa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, aber Adams Mi\u00dfethat,", "tokens": ["Ach", ",", "a\u00b7ber", "A\u00b7dams", "Mi\u00b7\u00dfet\u00b7hat", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die uns so sehr verderbet hat,", "tokens": ["Die", "uns", "so", "sehr", "ver\u00b7der\u00b7bet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df stets die Glut bleibt in der Aschen", "tokens": ["Da\u00df", "stets", "die", "Glut", "bleibt", "in", "der", "A\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Keiner mehr, als du allein", "tokens": ["Und", "Kei\u00b7ner", "mehr", ",", "als", "du", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kan Laster-frey geboren seyn.", "tokens": ["Kan", "Las\u00b7ter\u00b7frey", "ge\u00b7bo\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du sahst dies unser Elend an,", "tokens": ["Du", "sahst", "dies", "un\u00b7ser", "E\u00b7lend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie du, belobter Himmels-Mann,", "tokens": ["Wie", "du", ",", "be\u00b7lob\u00b7ter", "Him\u00b7mels\u00b7Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Reich gingst wieder einzunehmen,", "tokens": ["Dein", "Reich", "gingst", "wie\u00b7der", "ein\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und legtest hierum an ein Bad,", "tokens": ["Und", "leg\u00b7test", "hie\u00b7rum", "an", "ein", "Bad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das diese Wunder-Wirkung hat,", "tokens": ["Das", "die\u00b7se", "Wun\u00b7der\u00b7Wir\u00b7kung", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das angezeugte Gifft zu z\u00e4hmen,", "tokens": ["Das", "an\u00b7ge\u00b7zeug\u00b7te", "Gifft", "zu", "z\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und das, wie Ph\u00f6nix sich belebt,", "tokens": ["Und", "das", ",", "wie", "Ph\u00f6\u00b7nix", "sich", "be\u00b7lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PWAV", "NE", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Uns neu aus unserm Tod erhebt.", "tokens": ["Uns", "neu", "aus", "un\u00b7serm", "Tod", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geht, sprachstu, hin in alle Welt,", "tokens": ["Geht", ",", "sprachs\u00b7tu", ",", "hin", "in", "al\u00b7le", "Welt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr, die ihr mir zum Dienst bestellt,", "tokens": ["Ihr", ",", "die", "ihr", "mir", "zum", "Dienst", "be\u00b7stellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht au\u00df und lehret und beweist es,", "tokens": ["Geht", "au\u00df", "und", "leh\u00b7ret", "und", "be\u00b7weist", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dies die neue Bunds-Zeit sey,", "tokens": ["Da\u00df", "dies", "die", "neu\u00b7e", "Bunds\u00b7Zeit", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und taufft im Namen sie dabey", "tokens": ["Und", "taufft", "im", "Na\u00b7men", "sie", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Vaters, Sohns und Heilgen Geistes;", "tokens": ["Des", "Va\u00b7ters", ",", "Sohns", "und", "Heil\u00b7gen", "Geis\u00b7tes", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dies Wort und Wasser soll es seyn,", "tokens": ["Dies", "Wort", "und", "Was\u00b7ser", "soll", "es", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "KON", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Alle macht der Erbschuld rein.", "tokens": ["Das", "Al\u00b7le", "macht", "der", "Erb\u00b7schuld", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie gro\u00df ist, Jesu, deine Gnad',", "tokens": ["Wie", "gro\u00df", "ist", ",", "Je\u00b7su", ",", "dei\u00b7ne", "Gnad'", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin, der dich get\u00f6dtet hat,", "tokens": ["Ich", "bin", ",", "der", "dich", "ge\u00b7t\u00f6d\u00b7tet", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du hast mich nach dem noch lieber.", "tokens": ["Und", "du", "hast", "mich", "nach", "dem", "noch", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "APPR", "ART", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Je mehr Beschwerden ich dir mach,", "tokens": ["Je", "mehr", "Be\u00b7schwer\u00b7den", "ich", "dir", "mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Je freundlicher gehst du mir nach", "tokens": ["Je", "freund\u00b7li\u00b7cher", "gehst", "du", "mir", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "APPR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Und hilffest \u00fcberall mich \u00fcber,", "tokens": ["Und", "hilf\u00b7fest", "\u00fc\u00b7be\u00b7rall", "mich", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00e4ngest diese Huld schon an,", "tokens": ["Und", "f\u00e4n\u00b7gest", "die\u00b7se", "Huld", "schon", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da ich noch nichts verstehen kan.", "tokens": ["Da", "ich", "noch", "nichts", "ver\u00b7ste\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir kommen au\u00df der Mutter Schoo\u00df", "tokens": ["Wir", "kom\u00b7men", "au\u00df", "der", "Mut\u00b7ter", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wie am Leibe nackt und blo\u00df,", "tokens": ["So", "wie", "am", "Lei\u00b7be", "nackt", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht minder d\u00fcrfftig an der Seelen;", "tokens": ["Nicht", "min\u00b7der", "d\u00fcr\u00b7ff\u00b7tig", "an", "der", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Bild ist weg, weg aller Schmuck,", "tokens": ["Dein", "Bild", "ist", "weg", ",", "weg", "al\u00b7ler", "Schmuck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den unser erster Vater trug,", "tokens": ["Den", "un\u00b7ser", "ers\u00b7ter", "Va\u00b7ter", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wer kan alle M\u00e4ngel z\u00e4hlen,", "tokens": ["Und", "wer", "kan", "al\u00b7le", "M\u00e4n\u00b7gel", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die wir ohn Sprach, wie taub und blind", "tokens": ["Die", "wir", "ohn", "Sprach", ",", "wie", "taub", "und", "blind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und stark an lauter Ohnmacht sind.", "tokens": ["Und", "stark", "an", "lau\u00b7ter", "Ohn\u00b7macht", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Also betreten wir die Welt,", "tokens": ["Al\u00b7so", "be\u00b7tre\u00b7ten", "wir", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Fort allem Ungl\u00fcck vorgestellt;", "tokens": ["Fort", "al\u00b7lem", "Un\u00b7gl\u00fcck", "vor\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hie sucht der Satan uns zu f\u00e4llen,", "tokens": ["Hie", "sucht", "der", "Sa\u00b7tan", "uns", "zu", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort krankt und wankt das junge Blut,", "tokens": ["Dort", "krankt", "und", "wankt", "das", "jun\u00b7ge", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Vater selbst ist uns nicht gut,", "tokens": ["Dein", "Va\u00b7ter", "selbst", "ist", "uns", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Selbst unser Fleisch zieht uns zur H\u00f6llen.", "tokens": ["Selbst", "un\u00b7ser", "Fleisch", "zieht", "uns", "zur", "H\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, uns Elenden mehr als sehr,", "tokens": ["Ach", ",", "uns", "E\u00b7len\u00b7den", "mehr", "als", "sehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "NN", "PIAT", "KOKOM", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Wenn dieser Heyles Brunn nicht w\u00e4r.", "tokens": ["Wenn", "die\u00b7ser", "Hey\u00b7les", "Brunn", "nicht", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun ist der unser' Artzeney,", "tokens": ["Nun", "ist", "der", "un\u00b7ser'", "Art\u00b7ze\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sobald das Licht uns bringt herbey,", "tokens": ["So\u00b7bald", "das", "Licht", "uns", "bringt", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die unserm unbelebten Leben", "tokens": ["Die", "un\u00b7serm", "un\u00b7be\u00b7leb\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die angeborne Seuch' abzieht", "tokens": ["Die", "an\u00b7ge\u00b7bor\u00b7ne", "Seuch'", "ab\u00b7zieht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und unserm sterblichen Gebl\u00fct", "tokens": ["Und", "un\u00b7serm", "sterb\u00b7li\u00b7chen", "Ge\u00b7bl\u00fct"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df deines Geistes Wachsthum geben,", "tokens": ["Mu\u00df", "dei\u00b7nes", "Geis\u00b7tes", "Wach\u00b7sthum", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr welcher