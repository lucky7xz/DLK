{"textgrid.poem.35829": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach Dir nur will ich trachten,", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Dir nur will ich trachten,", "tokens": ["Nach", "Dir", "nur", "will", "ich", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Heiland, Jesus Christ!", "tokens": ["Mein", "Hei\u00b7land", ",", "Je\u00b7sus", "Christ", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will k\u00fchn die Welt verachten,", "tokens": ["Will", "k\u00fchn", "die", "Welt", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Deine Feindin ist,", "tokens": ["Die", "Dei\u00b7ne", "Fein\u00b7din", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hinweg die Augen wenden", "tokens": ["Hin\u00b7weg", "die", "Au\u00b7gen", "wen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von ihrem Zauberlicht;", "tokens": ["Von", "ih\u00b7rem", "Zau\u00b7ber\u00b7licht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr Feuer kann nur blenden,", "tokens": ["Ihr", "Feu\u00b7er", "kann", "nur", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erleuchten kann es nicht.", "tokens": ["Er\u00b7leuch\u00b7ten", "kann", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oft will es mich gel\u00fcsten,", "tokens": ["Oft", "will", "es", "mich", "ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Falsche ganz zu fliehn,", "tokens": ["Die", "Fal\u00b7sche", "ganz", "zu", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In W\u00e4lder und in W\u00fcsten", "tokens": ["In", "W\u00e4l\u00b7der", "und", "in", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Dir allein zu ziehn;", "tokens": ["Mit", "Dir", "al\u00b7lein", "zu", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn will ich Dein mich freuen", "tokens": ["Denn", "will", "ich", "Dein", "mich", "freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In rechter Innigkeit,", "tokens": ["In", "rech\u00b7ter", "In\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mu\u00df ich ihr Treiben scheuen,", "tokens": ["Mu\u00df", "ich", "ihr", "Trei\u00b7ben", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das immer mich zerstreut.", "tokens": ["Das", "im\u00b7mer", "mich", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du, Jesus, meine Sonne,", "tokens": ["Du", ",", "Je\u00b7sus", ",", "mei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Leben, meine Welt!", "tokens": ["Mein", "Le\u00b7ben", ",", "mei\u00b7ne", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du meines Herzens Wonne,", "tokens": ["Du", "mei\u00b7nes", "Her\u00b7zens", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O, sei mir treu gesellt!", "tokens": ["O", ",", "sei", "mir", "treu", "ge\u00b7sellt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dir sei es hingegeben,", "tokens": ["Dir", "sei", "es", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der ganzen Erde todt,", "tokens": ["Der", "gan\u00b7zen", "Er\u00b7de", "todt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du, meines Lebens Leben,", "tokens": ["Du", ",", "mei\u00b7nes", "Le\u00b7bens", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du, meiner Seele Brod! \u2013", "tokens": ["Du", ",", "mei\u00b7ner", "See\u00b7le", "Brod", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}