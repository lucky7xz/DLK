{"textgrid.poem.48608": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "19. Auf Verreisen Eines seiner guten Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "um der sanften ", "tokens": ["um", "der", "sanf\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "wo sie und der ", "tokens": ["wo", "sie", "und", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "KON", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "holdreich in einander rinnen.", "tokens": ["hold\u00b7reich", "in", "ein\u00b7an\u00b7der", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Gesellschaft sprach ihm zu:", "tokens": ["Die", "Ge\u00b7sell\u00b7schaft", "sprach", "ihm", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.2": {"line.1": {"text": "\u00bbseit ich,\u00ab sprach er, \u00bbin den Orden", "tokens": ["\u00bb", "seit", "ich", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "in", "den", "Or\u00b7den"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "eurer Kundschaft kommen bin,", "tokens": ["eu\u00b7rer", "Kund\u00b7schaft", "kom\u00b7men", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ist der Sommer f\u00fcnfmal hin,", "tokens": ["ist", "der", "Som\u00b7mer", "f\u00fcnf\u00b7mal", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "f\u00fcnfmal ist es Winter worden:", "tokens": ["f\u00fcnf\u00b7mal", "ist", "es", "Win\u00b7ter", "wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und so lange bin ich hier.", "tokens": ["und", "so", "lan\u00b7ge", "bin", "ich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich will weg aus der Revier.", "tokens": ["Ich", "will", "weg", "aus", "der", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Leid st\u00f6\u00dft mir zwar zu Handen.", "tokens": ["Kein", "Leid", "st\u00f6\u00dft", "mir", "zwar", "zu", "Han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Gesellschaft halt' ich wert.", "tokens": ["Die", "Ge\u00b7sell\u00b7schaft", "halt'", "ich", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So ist ba\u00dflich meine Heerd'", "tokens": ["So", "ist", "ba\u00df\u00b7lich", "mei\u00b7ne", "He\u00b7erd'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "PPOSAT", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "h\u00fcbsch bei feister Kost gestanden.", "tokens": ["h\u00fcbsch", "bei", "feis\u00b7ter", "Kost", "ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein geehrtes Vaterland,", "tokens": ["Mein", "ge\u00b7ehr\u00b7tes", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "das ists, das mir beut die Hand.", "tokens": ["das", "ists", ",", "das", "mir", "beut", "die", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gr\u00fcnet wol, ihr bunten Matten,", "tokens": ["Gr\u00fc\u00b7net", "wol", ",", "ihr", "bun\u00b7ten", "Mat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "seid, ihr L\u00fcfte, seid gek\u00fc\u00dft,", "tokens": ["seid", ",", "ihr", "L\u00fcf\u00b7te", ",", "seid", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sehr ihr B\u00e4che, sehr ihr Schatten,", "tokens": ["sehr", "ihr", "B\u00e4\u00b7che", ",", "sehr", "ihr", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und du dreibestr\u00f6mte Stadt,", "tokens": ["und", "du", "drei\u00b7bes\u00b7tr\u00f6m\u00b7te", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die mich wol bewirtet bat!", "tokens": ["die", "mich", "wol", "be\u00b7wir\u00b7tet", "bat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr auch, gute Nacht, ihr Br\u00fcder!", "tokens": ["Ihr", "auch", ",", "gu\u00b7te", "Nacht", ",", "ihr", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gute Nacht, du liebes Vieh!", "tokens": ["Gu\u00b7te", "Nacht", ",", "du", "lie\u00b7bes", "Vieh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seid gesegnet ie und ie!", "tokens": ["Seid", "ge\u00b7seg\u00b7net", "ie", "und", "ie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott wei\u00df, ob wir uns sehn wieder.\u00ab \u2013", "tokens": ["Gott", "wei\u00df", ",", "ob", "wir", "uns", "sehn", "wie\u00b7der", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so machet' er sich fort.", "tokens": ["Und", "so", "ma\u00b7chet'", "er", "sich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Niemand sprach vor Leid ein Wort.", "tokens": ["Nie\u00b7mand", "sprach", "vor", "Leid", "ein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "schrie ihm nach: \u00bbZeuch, Bruder, hin!", "tokens": ["schrie", "ihm", "nach", ":", "\u00bb", "Zeuch", ",", "Bru\u00b7der", ",", "hin", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "$(", "NN", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich verbleibe, wer ich bin;", "tokens": ["Ich", "ver\u00b7blei\u00b7be", ",", "wer", "ich", "bin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "du vergi\u00df auch nicht der Deinen!\u00ab", "tokens": ["du", "ver\u00b7gi\u00df", "auch", "nicht", "der", "Dei\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle sahn ihm sehnlich nach,", "tokens": ["Al\u00b7le", "sahn", "ihm", "sehn\u00b7lich", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "bis er ihnen ganz entbrach.", "tokens": ["bis", "er", "ih\u00b7nen", "ganz", "ent\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}