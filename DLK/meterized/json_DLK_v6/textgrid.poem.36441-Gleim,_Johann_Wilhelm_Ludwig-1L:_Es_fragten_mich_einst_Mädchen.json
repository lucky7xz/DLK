{"textgrid.poem.36441": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es fragten mich einst M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fragten mich einst M\u00e4dchen", "tokens": ["Es", "frag\u00b7ten", "mich", "einst", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit braunen Augenbraunen:", "tokens": ["Mit", "brau\u00b7nen", "Au\u00b7gen\u00b7brau\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Freund, warum trinkst du Kaffee?", "tokens": ["Freund", ",", "wa\u00b7rum", "trinkst", "du", "Kaf\u00b7fee", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da sprach ich zu dem M\u00e4dchen:", "tokens": ["Da", "sprach", "ich", "zu", "dem", "M\u00e4d\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zur Ehre der Brunetten!", "tokens": ["Zur", "Eh\u00b7re", "der", "Bru\u00b7net\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dis r\u00fchmten sich die Braunen", "tokens": ["Dis", "r\u00fchm\u00b7ten", "sich", "die", "Brau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Heut in der Kaffeestunde,", "tokens": ["Heut", "in", "der", "Kaf\u00b7fee\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als sie den blonden M\u00e4dchen", "tokens": ["Als", "sie", "den", "blon\u00b7den", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Den Vorzug streitig machten.", "tokens": ["Den", "Vor\u00b7zug", "strei\u00b7tig", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sie zankten sich beim Kaffee,", "tokens": ["Sie", "zank\u00b7ten", "sich", "beim", "Kaf\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und riefen mich zum Schlichten.", "tokens": ["Und", "rie\u00b7fen", "mich", "zum", "Schlich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Komm! sprach ein loses M\u00e4dchen,", "tokens": ["Komm", "!", "sprach", "ein", "lo\u00b7ses", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und winkte mit dem F\u00e4cher,", "tokens": ["Und", "wink\u00b7te", "mit", "dem", "F\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Du hast ja einst gesungen,", "tokens": ["Du", "hast", "ja", "einst", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Du k\u00f6ntest H\u00e4ndel schlichten;", "tokens": ["Du", "k\u00f6n\u00b7test", "H\u00e4n\u00b7del", "schlich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Nun schlicht auch unsre H\u00e4ndel.", "tokens": ["Nun", "schlicht", "auch", "uns\u00b7re", "H\u00e4n\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Doch, erst mu\u00df ich dich fragen:", "tokens": ["Doch", ",", "erst", "mu\u00df", "ich", "dich", "fra\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Warum trinkst du den Kaffee?", "tokens": ["Wa\u00b7rum", "trinkst", "du", "den", "Kaf\u00b7fee", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verr\u00e4trische Brunette,", "tokens": ["Ver\u00b7r\u00e4t\u00b7ri\u00b7sche", "Bru\u00b7net\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Es h\u00f6ren mein Bekentni\u00df", "tokens": ["Es", "h\u00f6\u00b7ren", "mein", "Be\u00b7kent\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die artigsten Blondinen,", "tokens": ["Die", "ar\u00b7tigs\u00b7ten", "Blon\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es h\u00f6rts die blonde Doris,", "tokens": ["Es", "h\u00f6rts", "die", "blon\u00b7de", "Do\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "Allein ich darf nicht schweigen.", "tokens": ["Al\u00b7lein", "ich", "darf", "nicht", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sprach, so bald sie fragte:", "tokens": ["Ich", "sprach", ",", "so", "bald", "sie", "frag\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Ehre der Brunetten!", "tokens": ["Zur", "Eh\u00b7re", "der", "Bru\u00b7net\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und that den stillen Seufzer:", "tokens": ["Und", "that", "den", "stil\u00b7len", "Seuf\u00b7zer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Minerva gib mir Weisheit.", "tokens": ["Mi\u00b7ner\u00b7va", "gib", "mir", "Weis\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Noch da ich also seufzte,", "tokens": ["Noch", "da", "ich", "al\u00b7so", "seufz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bewegten sich die Braunen", "tokens": ["Be\u00b7weg\u00b7ten", "sich", "die", "Brau\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "An allen Kaffeetischen,", "tokens": ["An", "al\u00b7len", "Kaf\u00b7fee\u00b7ti\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.8": {"text": "Und klatschten in die H\u00e4nde,", "tokens": ["Und", "klatschten", "in", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Und wiesen auf die Blonden,", "tokens": ["Und", "wie\u00b7sen", "auf", "die", "Blon\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und machten stolzre Minen", "tokens": ["Und", "mach\u00b7ten", "stolz\u00b7re", "Mi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Als Juno, da sie herschte.", "tokens": ["Als", "Ju\u00b7no", ",", "da", "sie", "herschte", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Schnell trat ich zu den Blonden,", "tokens": ["Schnell", "trat", "ich", "zu", "den", "Blon\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und frug die blonde Doris:", "tokens": ["Und", "frug", "die", "blon\u00b7de", "Do\u00b7ris", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.14": {"text": "Was iauchzen denn die Sch\u00f6nen?", "tokens": ["Was", "i\u00b7a\u00b7uch\u00b7zen", "denn", "die", "Sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Da sprach die Blonde sp\u00f6ttisch:", "tokens": ["Da", "sprach", "die", "Blon\u00b7de", "sp\u00f6t\u00b7tisch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "\u00bbdu trinkest ia den Kaffee", "tokens": ["\u00bb", "du", "trin\u00b7kest", "i\u00b7a", "den", "Kaf\u00b7fee"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "NE", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Zur Ehre der Brunetten.\u00ab", "tokens": ["Zur", "Eh\u00b7re", "der", "Bru\u00b7net\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Hier sagt ich langsam wieder,", "tokens": ["Hier", "sagt", "ich", "lang\u00b7sam", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was mir die weise G\u00f6ttin", "tokens": ["Was", "mir", "die", "wei\u00b7se", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Unsichtbar heimlich sagte:", "tokens": ["Un\u00b7sicht\u00b7bar", "heim\u00b7lich", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "\u00bbden braunen Trank der T\u00fcrken", "tokens": ["\u00bb", "den", "brau\u00b7nen", "Trank", "der", "T\u00fcr\u00b7ken"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Trink ich des Nachmittages", "tokens": ["Trink", "ich", "des", "Nach\u00b7mit\u00b7ta\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zur Ehre der Brunetten;", "tokens": ["Zur", "Eh\u00b7re", "der", "Bru\u00b7net\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Den weissen Trank der Seren,", "tokens": ["Den", "weis\u00b7sen", "Trank", "der", "Se\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Thee, trink ich des Morgens", "tokens": ["Den", "Thee", ",", "trink", "ich", "des", "Mor\u00b7gens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zur Ehre der Blondinen.\u00ab", "tokens": ["Zur", "Eh\u00b7re", "der", "Blon\u00b7di\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schnell iauchzten alle Blonden,", "tokens": ["Schnell", "i\u00b7a\u00b7uchz\u00b7ten", "al\u00b7le", "Blon\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und klatschten in die H\u00e4nde,", "tokens": ["Und", "klatschten", "in", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und wiesen auf die Braunen,", "tokens": ["Und", "wie\u00b7sen", "auf", "die", "Brau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und spotteten der Minen", "tokens": ["Und", "spot\u00b7te\u00b7ten", "der", "Mi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Vorzugs und des Stolzes.", "tokens": ["Des", "Vor\u00b7zugs", "und", "des", "Stol\u00b7zes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich aber gab Minerven", "tokens": ["Ich", "a\u00b7ber", "gab", "Mi\u00b7ner\u00b7ven"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Den Dank f\u00fcr ihre Weisheit,", "tokens": ["Den", "Dank", "f\u00fcr", "ih\u00b7re", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schlich mich aus dem Zimmer,", "tokens": ["Und", "schlich", "mich", "aus", "dem", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lie\u00df die Sch\u00f6nen zanken.", "tokens": ["Und", "lie\u00df", "die", "Sch\u00f6\u00b7nen", "zan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}