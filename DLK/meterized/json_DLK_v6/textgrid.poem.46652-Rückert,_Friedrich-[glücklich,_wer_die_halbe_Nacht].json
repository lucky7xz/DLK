{"textgrid.poem.46652": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[gl\u00fccklich, wer die halbe Nacht]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich, wer die halbe Nacht,", "tokens": ["Gl\u00fcck\u00b7lich", ",", "wer", "die", "hal\u00b7be", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhig schl\u00e4ft auf Einem Ohr,", "tokens": ["Ru\u00b7hig", "schl\u00e4ft", "auf", "Ei\u00b7nem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann, vom Schlummer halb erwacht,", "tokens": ["Dann", ",", "vom", "Schlum\u00b7mer", "halb", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zurecht das andre macht", "tokens": ["Sich", "zu\u00b7recht", "das", "and\u00b7re", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so fortschl\u00e4ft wie zuvor.", "tokens": ["Und", "so", "fort\u00b7schl\u00e4ft", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KOKOM", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch begl\u00fcckter, wer da mag", "tokens": ["Doch", "be\u00b7gl\u00fcck\u00b7ter", ",", "wer", "da", "mag"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWS", "ADV", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So in tiefste Ruh getaucht", "tokens": ["So", "in", "tiefs\u00b7te", "Ruh", "ge\u00b7taucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Liegen bleiben, wie er lag,", "tokens": ["Lie\u00b7gen", "blei\u00b7ben", ",", "wie", "er", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er sich bei Nacht und Tag", "tokens": ["Da\u00df", "er", "sich", "bei", "Nacht", "und", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht mehr umzuwenden braucht.", "tokens": ["Nicht", "mehr", "um\u00b7zu\u00b7wen\u00b7den", "braucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}