{"textgrid.poem.63661": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Frage", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr \u00fcber dem Haupt mir schwebt,", "tokens": ["Die", "ihr", "\u00fc\u00b7ber", "dem", "Haupt", "mir", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dunkle M\u00e4chte des Lebens,", "tokens": ["Dunk\u00b7le", "M\u00e4ch\u00b7te", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Holder Gaben die F\u00fclle gebt,", "tokens": ["Hol\u00b7der", "Ga\u00b7ben", "die", "F\u00fcl\u00b7le", "gebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ach, nur da\u00df ihr den Schleier hebt,", "tokens": ["Ach", ",", "nur", "da\u00df", "ihr", "den", "Schlei\u00b7er", "hebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der den sterblichen Blick umwebt,", "tokens": ["Der", "den", "sterb\u00b7li\u00b7chen", "Blick", "um\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Hofft die Seele vergebens?", "tokens": ["Hofft", "die", "See\u00b7le", "ver\u00b7ge\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Allmacht, ewige Meisterin,", "tokens": ["All\u00b7macht", ",", "e\u00b7wi\u00b7ge", "Meis\u00b7te\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ist denn Frevel die Frage,", "tokens": ["Ist", "denn", "Fre\u00b7vel", "die", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ob ich einst das Woher? Wohin?", "tokens": ["Ob", "ich", "einst", "das", "Wo\u00b7her", "?", "Wo\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$.", "PWAV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zu entr\u00e4tseln berufen bin,", "tokens": ["Zu", "ent\u00b7r\u00e4t\u00b7seln", "be\u00b7ru\u00b7fen", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Ob dem ahnungumwobnen Sinn", "tokens": ["Ob", "dem", "ah\u00b7nun\u00b7gum\u00b7wob\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Himmlische Klarheit tage?", "tokens": ["Himm\u00b7li\u00b7sche", "Klar\u00b7heit", "ta\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Oder ruf' ich umsonst dich an?", "tokens": ["O\u00b7der", "ruf'", "ich", "um\u00b7sonst", "dich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00dft du herrschen und schweigen?", "tokens": ["Mu\u00dft", "du", "herr\u00b7schen", "und", "schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Darfst du, wie dem gefangnen Mann,", "tokens": ["Darfst", "du", ",", "wie", "dem", "ge\u00b7fang\u00b7nen", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Was ich nimmer erreichen kann,", "tokens": ["Was", "ich", "nim\u00b7mer", "er\u00b7rei\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch des ehernen Gitters Bann", "tokens": ["Durch", "des", "e\u00b7her\u00b7nen", "Git\u00b7ters", "Bann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Nur von ferne mir zeigen?", "tokens": ["Nur", "von", "fer\u00b7ne", "mir", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}