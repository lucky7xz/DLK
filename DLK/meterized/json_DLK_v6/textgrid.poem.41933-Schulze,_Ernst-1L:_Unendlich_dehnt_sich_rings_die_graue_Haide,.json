{"textgrid.poem.41933": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unendlich dehnt sich rings die graue Haide,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unendlich dehnt sich rings die graue Haide,", "tokens": ["Un\u00b7end\u00b7lich", "dehnt", "sich", "rings", "die", "grau\u00b7e", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dunkel liegt der \u00f6de Fichtenhain;", "tokens": ["Und", "dun\u00b7kel", "liegt", "der", "\u00f6\u00b7de", "Fich\u00b7ten\u00b7hain", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch leise schwimmt im heitern Sonnenschein", "tokens": ["Doch", "lei\u00b7se", "schwimmt", "im", "hei\u00b7tern", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um's finstre Bild ein stiller Strahl der Freude.", "tokens": ["Um's", "finst\u00b7re", "Bild", "ein", "stil\u00b7ler", "Strahl", "der", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl flimmern hell am bunten Feyerkleide", "tokens": ["Wohl", "flim\u00b7mern", "hell", "am", "bun\u00b7ten", "Fe\u00b7yer\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Thr\u00e4nen oft wie k\u00f6stliches Gestein;", "tokens": ["Die", "Thr\u00e4\u00b7nen", "oft", "wie", "k\u00f6st\u00b7li\u00b7ches", "Ge\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch kann auch Tod am Leben sich erfreun?", "tokens": ["Doch", "kann", "auch", "Tod", "am", "Le\u00b7ben", "sich", "er\u00b7freun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NN", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Naht L\u00e4cheln auch dem Ernst und Lust dem Leide?", "tokens": ["Naht", "L\u00e4\u00b7cheln", "auch", "dem", "Ernst", "und", "Lust", "dem", "Lei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O sey getrost! Es giebt ein ew'ges Licht!", "tokens": ["O", "sey", "ge\u00b7trost", "!", "Es", "giebt", "ein", "ew'\u00b7ges", "Licht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Tod noch Schicksal kann die heil'ge Gabe", "tokens": ["Nicht", "Tod", "noch", "Schick\u00b7sal", "kann", "die", "heil'\u00b7ge", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der eignen Kraft der reinen Seele rauben.", "tokens": ["Der", "eig\u00b7nen", "Kraft", "der", "rei\u00b7nen", "See\u00b7le", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl darfst du weinen; zagen darfst du nicht;", "tokens": ["Wohl", "darfst", "du", "wei\u00b7nen", ";", "za\u00b7gen", "darfst", "du", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$.", "VVINF", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn menschlich ist die Thr\u00e4n' am frischen Grabe;", "tokens": ["Denn", "menschlich", "ist", "die", "Thr\u00e4n'", "am", "fri\u00b7schen", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch g\u00f6ttlich ist's, zu l\u00e4cheln und zu glauben.", "tokens": ["Doch", "g\u00f6tt\u00b7lich", "ist's", ",", "zu", "l\u00e4\u00b7cheln", "und", "zu", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PTKZU", "VVFIN", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}