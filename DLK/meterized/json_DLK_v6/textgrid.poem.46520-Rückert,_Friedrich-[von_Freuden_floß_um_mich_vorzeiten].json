{"textgrid.poem.46520": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[von Freuden flo\u00df um mich vorzeiten]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Freuden flo\u00df um mich vorzeiten", "tokens": ["Von", "Freu\u00b7den", "flo\u00df", "um", "mich", "vor\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ueberflu\u00df;", "tokens": ["Ein", "Ue\u00b7berf\u00b7lu\u00df", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und wie ich sch\u00f6pfte, blieb beiseiten", "tokens": ["Und", "wie", "ich", "sch\u00f6pf\u00b7te", ",", "blieb", "bei\u00b7sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ueberschu\u00df.", "tokens": ["Ein", "Ue\u00b7ber\u00b7schu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie dacht' ich, da\u00df versiegen k\u00f6nnte", "tokens": ["Wie", "dacht'", "ich", ",", "da\u00df", "ver\u00b7sie\u00b7gen", "k\u00f6nn\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ueberschwang?", "tokens": ["Der", "Ue\u00b7ber\u00b7schwang", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ergossen war nach allen Weiten", "tokens": ["Er\u00b7gos\u00b7sen", "war", "nach", "al\u00b7len", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Uebergu\u00df.", "tokens": ["Der", "Ue\u00b7ber\u00b7gu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wohin verlaufen hat das Wasser", "tokens": ["Wo\u00b7hin", "ver\u00b7lau\u00b7fen", "hat", "das", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich \u00fcber Nacht?", "tokens": ["Sich", "\u00fc\u00b7ber", "Nacht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es eilt wohl, um mir zu bereiten", "tokens": ["Es", "eilt", "wohl", ",", "um", "mir", "zu", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUI", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Ueberdru\u00df!", "tokens": ["Nicht", "Ue\u00b7berd\u00b7ru\u00df", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Vor\u00fcber eilt des Gl\u00fcckes F\u00fclle", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "eilt", "des", "Gl\u00fc\u00b7ckes", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rauscht von fern", "tokens": ["Und", "rauscht", "von", "fern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mir einen noch, und keinen zweiten", "tokens": ["Mir", "ei\u00b7nen", "noch", ",", "und", "kei\u00b7nen", "zwei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "ADV", "$,", "KON", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor\u00fcbergru\u00df.", "tokens": ["Vor\u00b7\u00fc\u00b7berg\u00b7ru\u00df", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ihr Augen, wollt Ersatz ihr weinen?", "tokens": ["Ihr", "Au\u00b7gen", ",", "wollt", "Er\u00b7satz", "ihr", "wei\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So weinet nur!", "tokens": ["So", "wei\u00b7net", "nur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und mich durchs Leben soll begleiten", "tokens": ["Und", "mich", "durchs", "Le\u00b7ben", "soll", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein tr\u00fcber Flu\u00df.", "tokens": ["Ein", "tr\u00fc\u00b7ber", "Flu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wo ich am Strom der Wehmuth lausche", "tokens": ["Wo", "ich", "am", "Strom", "der", "Weh\u00b7muth", "lau\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Hauch der Nacht,", "tokens": ["Im", "Hauch", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Weht her von jenseit goldner Saiten", "tokens": ["Weht", "her", "von", "jen\u00b7seit", "gold\u00b7ner", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Her\u00fcbergru\u00df.", "tokens": ["Her\u00b7\u00fc\u00b7berg\u00b7ru\u00df", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Am Ufer pflanz' ich dunkle Lieder,", "tokens": ["Am", "U\u00b7fer", "pflanz'", "ich", "dunk\u00b7le", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Duft weht hin,", "tokens": ["Ihr", "Duft", "weht", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bis ich gefl\u00fcgelt selber schreiten", "tokens": ["Bis", "ich", "ge\u00b7fl\u00fc\u00b7gelt", "sel\u00b7ber", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hin\u00fcber mu\u00df.", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "mu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}