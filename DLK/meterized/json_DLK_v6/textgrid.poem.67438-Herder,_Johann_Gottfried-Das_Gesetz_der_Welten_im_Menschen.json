{"textgrid.poem.67438": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Gesetz der Welten im Menschen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nes Sternengefild, Ihr weiten unendlichen Auen,", "tokens": ["Sch\u00f6\u00b7nes", "Ster\u00b7nen\u00b7ge\u00b7fild", ",", "Ihr", "wei\u00b7ten", "un\u00b7end\u00b7li\u00b7chen", "Au\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aus mir selber entz\u00fcckt, hang' ich mit Blicken an Euch,", "tokens": ["Aus", "mir", "sel\u00b7ber", "ent\u00b7z\u00fcckt", ",", "hang'", "ich", "mit", "Bli\u00b7cken", "an", "Euch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$,", "VVFIN", "PPER", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schaue die goldene Heerde der himmlischen Schafe da weiden,", "tokens": ["Schau\u00b7e", "die", "gol\u00b7de\u00b7ne", "Heer\u00b7de", "der", "himm\u00b7li\u00b7schen", "Scha\u00b7fe", "da", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Suche den Hirten in ihr, der mit dem Stabe sie f\u00fchrt.", "tokens": ["Su\u00b7che", "den", "Hir\u00b7ten", "in", "ihr", ",", "der", "mit", "dem", "Sta\u00b7be", "sie", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$,", "PRELS", "APPR", "ART", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "\u00bbsuchst Du den Hirten der Heerde, die droben sich badet im Aether?", "tokens": ["\u00bb", "suchst", "Du", "den", "Hir\u00b7ten", "der", "Heer\u00b7de", ",", "die", "dro\u00b7ben", "sich", "ba\u00b7det", "im", "A\u00b7e\u00b7ther", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+--+--+-++--", "measure": "dactylic.tetra.plus"}, "line.6": {"text": "Suchst das hohe Gesetz, welches die Welten bewegt?", "tokens": ["Suchst", "das", "ho\u00b7he", "Ge\u00b7setz", ",", "wel\u00b7ches", "die", "Wel\u00b7ten", "be\u00b7wegt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sterblicher, blick in Dich selbst! da hast Du die h\u00f6here Regel,", "tokens": ["Sterb\u00b7li\u00b7cher", ",", "blick", "in", "Dich", "selbst", "!", "da", "hast", "Du", "die", "h\u00f6\u00b7he\u00b7re", "Re\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "ADV", "$.", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--++-+--+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Die nicht die Welten allein, die auch sich selber regiert.\u00ab", "tokens": ["Die", "nicht", "die", "Wel\u00b7ten", "al\u00b7lein", ",", "die", "auch", "sich", "sel\u00b7ber", "re\u00b7giert", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "PRF", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}