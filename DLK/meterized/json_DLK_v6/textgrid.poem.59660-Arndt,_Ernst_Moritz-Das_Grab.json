{"textgrid.poem.59660": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Das Grab", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steh hier still, hier w\u00e4chst der Baum", "tokens": ["Steh", "hier", "still", ",", "hier", "w\u00e4chst", "der", "Baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon mit Bl\u00e4ttern gr\u00fcn und voll,", "tokens": ["Schon", "mit", "Bl\u00e4t\u00b7tern", "gr\u00fcn", "und", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der des letzten Schlummers Traum", "tokens": ["Der", "des", "letz\u00b7ten", "Schlum\u00b7mers", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundlich dir umschatten soll.", "tokens": ["Freund\u00b7lich", "dir", "um\u00b7schat\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schau' ihn an, er ist so gr\u00fcn,", "tokens": ["Schau'", "ihn", "an", ",", "er", "ist", "so", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nickt so lustig in die Welt,", "tokens": ["Nickt", "so", "lus\u00b7tig", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rote Rosen ihn umbl\u00fchn,", "tokens": ["Ro\u00b7te", "Ro\u00b7sen", "ihn", "um\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Von der Maienluft geschwellt.", "tokens": ["Von", "der", "Mai\u00b7en\u00b7luft", "ge\u00b7schwellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welch ein Schimmer! Welch ein Duft!", "tokens": ["Welch", "ein", "Schim\u00b7mer", "!", "Welch", "ein", "Duft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Horche, wie der Morgen klingt,", "tokens": ["Hor\u00b7che", ",", "wie", "der", "Mor\u00b7gen", "klingt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Kuckuck unten ruft!", "tokens": ["Wie", "der", "Ku\u00b7ckuck", "un\u00b7ten", "ruft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Lerche oben singt!", "tokens": ["Wie", "die", "Ler\u00b7che", "o\u00b7ben", "singt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dies Leben rosenrot,", "tokens": ["Und", "dies", "Le\u00b7ben", "ro\u00b7sen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese Wonne liederreich", "tokens": ["Die\u00b7se", "Won\u00b7ne", "lie\u00b7der\u00b7reich"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4re graulich, und der Tod", "tokens": ["W\u00e4\u00b7re", "grau\u00b7lich", ",", "und", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4tte hier sein d\u00fcstres Reich?", "tokens": ["H\u00e4t\u00b7te", "hier", "sein", "d\u00fcst\u00b7res", "Reich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein, ihr Rosen, nein, du Baum,", "tokens": ["Nein", ",", "ihr", "Ro\u00b7sen", ",", "nein", ",", "du", "Baum", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PTKANT", "$,", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mich einst ums\u00e4useln wird,", "tokens": ["Der", "mich", "einst", "um\u00b7s\u00e4u\u00b7seln", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, du V\u00f6glein, das den Traum", "tokens": ["Nein", ",", "du", "V\u00f6\u00b7glein", ",", "das", "den", "Traum"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Schlafes einst umschwirrt,", "tokens": ["Die\u00b7ses", "Schla\u00b7fes", "einst", "um\u00b7schwirrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, ihr Maienl\u00fcftchen s\u00fc\u00df,", "tokens": ["Nein", ",", "ihr", "Mai\u00b7en\u00b7l\u00fcft\u00b7chen", "s\u00fc\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ihr mit den Blumen kost,", "tokens": ["Die", "ihr", "mit", "den", "Blu\u00b7men", "kost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Hier bl\u00fcht wieder Paradies,", "tokens": ["Hier", "bl\u00fcht", "wie\u00b7der", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Das nicht Sturm noch Flut umtost.", "tokens": ["Das", "nicht", "Sturm", "noch", "Flut", "um\u00b7tost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wachse denn, du gr\u00fcner Baum,", "tokens": ["Wach\u00b7se", "denn", ",", "du", "gr\u00fc\u00b7ner", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachset, Rosen, zum Geb\u00fcsch,", "tokens": ["Wach\u00b7set", ",", "Ro\u00b7sen", ",", "zum", "Ge\u00b7b\u00fcsch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem vollen Fr\u00fchlingstraum", "tokens": ["Mit", "dem", "vol\u00b7len", "Fr\u00fch\u00b7lings\u00b7traum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Duftet um mein Bette frisch;", "tokens": ["Duf\u00b7tet", "um", "mein", "Bet\u00b7te", "frisch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, h\u00fcte dieses Grab,", "tokens": ["Lie\u00b7be", ",", "h\u00fc\u00b7te", "die\u00b7ses", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoffnung, winde drum dein Gr\u00fcn,", "tokens": ["Hoff\u00b7nung", ",", "win\u00b7de", "drum", "dein", "Gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so la\u00dft mich bald hinab", "tokens": ["Und", "so", "la\u00dft", "mich", "bald", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVIMP", "PPER", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In die sel'ge Stille fliehn.", "tokens": ["In", "die", "sel'\u00b7ge", "Stil\u00b7le", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}