{"textgrid.poem.32672": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ei, Kameraden, sitzt man auch", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, Kameraden, sitzt man auch", "tokens": ["Ei", ",", "Ka\u00b7me\u00b7ra\u00b7den", ",", "sitzt", "man", "auch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Ernteschmaus so stumm?", "tokens": ["Beim", "Ern\u00b7tesc\u00b7hmaus", "so", "stumm", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frisch auf, und singt nach altem Brauch", "tokens": ["Frisch", "auf", ",", "und", "singt", "nach", "al\u00b7tem", "Brauch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein h\u00fcbsches Lied herum!", "tokens": ["Ein", "h\u00fcb\u00b7sches", "Lied", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gesang allein", "tokens": ["Ge\u00b7sang", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Macht s\u00fc\u00df den Wein", "tokens": ["Macht", "s\u00fc\u00df", "den", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und fr\u00f6hlicher den Schmaus;", "tokens": ["Und", "fr\u00f6h\u00b7li\u00b7cher", "den", "Schmaus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So stimmt denn alle mit mir ein,", "tokens": ["So", "stimmt", "denn", "al\u00b7le", "mit", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und trinkt fein wacker aus!", "tokens": ["Und", "trinkt", "fein", "wa\u00b7cker", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Herzgeliebten trink' ich dies,", "tokens": ["Der", "Herz\u00b7ge\u00b7lieb\u00b7ten", "trink'", "ich", "dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie lebe f\u00fcr und f\u00fcr!", "tokens": ["Sie", "le\u00b7be", "f\u00fcr", "und", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wein schmeckt noch einmal so s\u00fc\u00df,", "tokens": ["Der", "Wein", "schmeckt", "noch", "ein\u00b7mal", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sing' ich dabei von ihr.", "tokens": ["Sing'", "ich", "da\u00b7bei", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist so gut", "tokens": ["Sie", "ist", "so", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und wohlgemut,", "tokens": ["Und", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und freundlich, wie ein Lamm;", "tokens": ["Und", "freund\u00b7lich", ",", "wie", "ein", "Lamm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und wenn sie mir am Herzen ruht,", "tokens": ["Und", "wenn", "sie", "mir", "am", "Her\u00b7zen", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nennt sie mich Br\u00e4utigam.", "tokens": ["Nennt", "sie", "mich", "Br\u00e4u\u00b7ti\u00b7gam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohlan, es lebe meine Braut!", "tokens": ["Wo\u00b7hlan", ",", "es", "le\u00b7be", "mei\u00b7ne", "Braut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trinkt's ihr zu Ehren leer! \u2013", "tokens": ["Trinkt's", "ihr", "zu", "Eh\u00b7ren", "leer", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch, schaut! bei meiner Seele, schaut,", "tokens": ["Doch", ",", "schaut", "!", "bei", "mei\u00b7ner", "See\u00b7le", ",", "schaut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da k\u00f6mmt sie selber her.", "tokens": ["Da", "k\u00f6mmt", "sie", "sel\u00b7ber", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Willkommen hier!", "tokens": ["Will\u00b7kom\u00b7men", "hier", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich trink es dir,", "tokens": ["Ich", "trink", "es", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Herzallerliebste, zu!", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", ",", "zu", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Kein Mensch auf Erden k\u00e4me mir", "tokens": ["Kein", "Mensch", "auf", "Er\u00b7den", "k\u00e4\u00b7me", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Willkommener als du!", "tokens": ["Will\u00b7kom\u00b7me\u00b7ner", "als", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Auf, Musikanten, auf! herbei!", "tokens": ["Auf", ",", "Mu\u00b7si\u00b7kan\u00b7ten", ",", "auf", "!", "her\u00b7bei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr wi\u00dft mein Leibst\u00fcck schon:", "tokens": ["Ihr", "wi\u00dft", "mein", "Leib\u00b7st\u00fcck", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbheidideldum, Heidideldei,", "tokens": ["\u00bb", "hei\u00b7di\u00b7del\u00b7dum", ",", "Hei\u00b7di\u00b7del\u00b7dei", ","], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "FM.la", "$,", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ich denk' ans Nachbars Sohn;", "tokens": ["Ich", "denk'", "ans", "Nach\u00b7bars", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und denk' ich ihn,", "tokens": ["Und", "denk'", "ich", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dann wird mein Sinn", "tokens": ["Dann", "wird", "mein", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mit einemmal so froh!", "tokens": ["Mit", "ei\u00b7nem\u00b7mal", "so", "froh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ach, bis zum Sterben lieb' ich ihn!\u00ab", "tokens": ["Ach", ",", "bis", "zum", "Ster\u00b7ben", "lieb'", "ich", "ihn", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "KOUS", "APPRART", "NN", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sag, Liebchen, hei\u00dft's nicht so?", "tokens": ["Sag", ",", "Lieb\u00b7chen", ",", "hei\u00dft's", "nicht", "so", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}