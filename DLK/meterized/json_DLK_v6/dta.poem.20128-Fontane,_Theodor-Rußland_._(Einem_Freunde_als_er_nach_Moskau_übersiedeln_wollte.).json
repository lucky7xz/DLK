{"dta.poem.20128": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Ru\u00dfland .  \n (Einem Freunde als er nach Moskau \u00fcbersiedeln wollte.)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Wer auf die Zukunft schw\u00f6rt und unbek\u00fcmmert", "tokens": ["Wer", "auf", "die", "Zu\u00b7kunft", "schw\u00f6rt", "und", "un\u00b7be\u00b7k\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ew\u2019gen Kraft des Geistes noch vertraut,", "tokens": ["Der", "ew'\u00b7gen", "Kraft", "des", "Geis\u00b7tes", "noch", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die, gleich dem Meere, ", "tokens": ["Die", ",", "gleich", "dem", "Mee\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und eine ", "tokens": ["Und", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wer, ihr vertrauend, unser Kr\u00e4merleben", "tokens": ["Wer", ",", "ihr", "ver\u00b7trau\u00b7end", ",", "un\u00b7ser", "Kr\u00e4\u00b7mer\u00b7le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "$,", "PPER", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ob jener Zeit, die kommen mu\u00df, vergi\u00dft,", "tokens": ["Ob", "je\u00b7ner", "Zeit", ",", "die", "kom\u00b7men", "mu\u00df", ",", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der fliehe ", "tokens": ["Der", "flie\u00b7he"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Und jede Hoffnung eitel Thorheit ist.", "tokens": ["Und", "je\u00b7de", "Hoff\u00b7nung", "ei\u00b7tel", "Thor\u00b7heit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer, trotz der D\u00fcrre, seines Flei\u00dfes Segen \u2014", "tokens": ["Wer", ",", "trotz", "der", "D\u00fcr\u00b7re", ",", "sei\u00b7nes", "Flei\u00b7\u00dfes", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freiheit Saat, voll guten Muths erblickt,", "tokens": ["Der", "Frei\u00b7heit", "Saat", ",", "voll", "gu\u00b7ten", "Muths", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die junge Saat, von keinem Sommerregen,", "tokens": ["Die", "jun\u00b7ge", "Saat", ",", "von", "kei\u00b7nem", "Som\u00b7mer\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch, \u00fcber Nacht, von frischem Thau erquickt;", "tokens": ["Doch", ",", "\u00fc\u00b7ber", "Nacht", ",", "von", "fri\u00b7schem", "Thau", "er\u00b7quickt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der fliehe ", "tokens": ["Der", "flie\u00b7he"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Nur Mehlthau f\u00e4llt, der jeden Keim zerfri\u00dft,", "tokens": ["Nur", "Mehlt\u00b7hau", "f\u00e4llt", ",", "der", "je\u00b7den", "Keim", "zer\u00b7fri\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo\u2019s noch gelingt \u201e", "tokens": ["Wo's", "noch", "ge\u00b7lingt", "\u201e"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und jede Hoffnung eitel Thorheit ist.", "tokens": ["Und", "je\u00b7de", "Hoff\u00b7nung", "ei\u00b7tel", "Thor\u00b7heit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wer, verzweifelnd ob so langem Harren,", "tokens": ["Doch", "wer", ",", "ver\u00b7zwei\u00b7felnd", "ob", "so", "lan\u00b7gem", "Har\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$,", "VVPP", "KOUS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Hoffnung Prachtbau selber niederrei\u00dft,", "tokens": ["Der", "Hoff\u00b7nung", "Pracht\u00b7bau", "sel\u00b7ber", "nie\u00b7der\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unser Thun das Streben eines Narren,", "tokens": ["Und", "un\u00b7ser", "Thun", "das", "Stre\u00b7ben", "ei\u00b7nes", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und unsren Glauben \u201eGeistesschw\u00e4che\u201c hei\u00dft;", "tokens": ["Und", "un\u00b7sren", "Glau\u00b7ben", "\u201e", "Geis\u00b7tes\u00b7schw\u00e4\u00b7che", "\u201c", "hei\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der suche ", "tokens": ["Der", "su\u00b7che"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Ein Maa\u00df, daran er unsre Gr\u00f6\u00dfe mi\u00dft,", "tokens": ["Ein", "Maa\u00df", ",", "da\u00b7ran", "er", "uns\u00b7re", "Gr\u00f6\u00b7\u00dfe", "mi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und lerne ", "tokens": ["Und", "ler\u00b7ne"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Wo jede Hoffnung eitel Thorheit ist.", "tokens": ["Wo", "je\u00b7de", "Hoff\u00b7nung", "ei\u00b7tel", "Thor\u00b7heit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}