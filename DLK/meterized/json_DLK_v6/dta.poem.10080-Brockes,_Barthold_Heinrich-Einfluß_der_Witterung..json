{"dta.poem.10080": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einflu\u00df der Witterung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann der Himmel aufgekl\u00e4ret, und der Sonnen helle", "tokens": ["Wann", "der", "Him\u00b7mel", "auf\u00b7ge\u00b7kl\u00e4\u00b7ret", ",", "und", "der", "Son\u00b7nen", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Glut", "tokens": ["Glut"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Alle Vorw\u00fcrff\u2019 \u00fcberg\u00fcldet; wann das Heer der Winde", "tokens": ["Al\u00b7le", "Vor\u00b7w\u00fcrff'", "\u00fc\u00b7berg\u00b7\u00fcl\u00b7det", ";", "wann", "das", "Heer", "der", "Win\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVPP", "$.", "PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "ruht;", "tokens": ["ruht", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Deucht mich, da\u00df bey dieser Stille, bey dem allgemeinen", "tokens": ["Deucht", "mich", ",", "da\u00df", "bey", "die\u00b7ser", "Stil\u00b7le", ",", "bey", "dem", "all\u00b7ge\u00b7mei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "APPR", "PDAT", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Schein,", "tokens": ["Schein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Da, in s\u00fcssem Gleich-Gewicht, Lufft und Erd\u2019 und Fluth sich", "tokens": ["Da", ",", "in", "s\u00fcs\u00b7sem", "Gleich\u00b7Ge\u00b7wicht", ",", "Lufft", "und", "Erd'", "und", "Fluth", "sich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "PRF"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "schm\u00fccken,", "tokens": ["schm\u00fc\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wir zu z\u00e4rtlichen Gedancken, etwas lieblichs auszudr\u00fccken,", "tokens": ["Wir", "zu", "z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7cken", ",", "et\u00b7was", "lieb\u00b7lichs", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,", "ADV", "ADJA", "VVIZU", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und was sch\u00f6nes zu beschreiben, besser aufger\u00e4umet seyn.", "tokens": ["Und", "was", "sch\u00f6\u00b7nes", "zu", "be\u00b7schrei\u00b7ben", ",", "bes\u00b7ser", "auf\u00b7ge\u00b7r\u00e4u\u00b7met", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "PTKZU", "VVINF", "$,", "ADJD", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Wann hingegen dunckle Wolcken, oder Wind, die L\u00fcfte", "tokens": ["Wann", "hin\u00b7ge\u00b7gen", "dunck\u00b7le", "Wol\u00b7cken", ",", "o\u00b7der", "Wind", ",", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "NN", "$,", "KON", "NN", "$,", "ART", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "f\u00fcllen,", "tokens": ["f\u00fcl\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Und der Sch\u00f6nheit Quell, die Sonne, durch geschw\u00e4rtzten", "tokens": ["Und", "der", "Sch\u00f6n\u00b7heit", "Quell", ",", "die", "Son\u00b7ne", ",", "durch", "ge\u00b7schw\u00e4rtz\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Dufft verh\u00fcllen;", "tokens": ["Dufft", "ver\u00b7h\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Deucht mich, da\u00df zu ernsten Lehren,", "tokens": ["Deucht", "mich", ",", "da\u00df", "zu", "erns\u00b7ten", "Leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Auch die Wirckung der Natur zu versuchen, zu erkl\u00e4ren,", "tokens": ["Auch", "die", "Wir\u00b7ckung", "der", "Na\u00b7tur", "zu", "ver\u00b7su\u00b7chen", ",", "zu", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "---+--+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Unsre Seelen-Kr\u00e4ffte fast f\u00e4hig- und geschickter w\u00e4ren.", "tokens": ["Uns\u00b7re", "See\u00b7len\u00b7Kr\u00e4ff\u00b7te", "fast", "f\u00e4\u00b7hig", "und", "ge\u00b7schick\u00b7ter", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "TRUNC", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}