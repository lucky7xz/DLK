{"dta.poem.18909": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von Hochermelten Seinen F\u00fcrstl. Gn.  \n  &c.   Hertzog Bernharden   &c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Ja/ Spannisch bist du Neyd/ vnd torrecht du", "tokens": ["Ja", "/", "Span\u00b7nisch", "bist", "du", "Neyd", "/", "vnd", "tor\u00b7recht", "du"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$(", "ADJD", "VAFIN", "PPER", "NN", "$(", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mi\u00dfgunst/", "tokens": ["Mi\u00df\u00b7gunst", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Jhr k\u00f6nt so wenig schmach an Hertzog Bernhard", "tokens": ["Ihr", "k\u00f6nt", "so", "we\u00b7nig", "schmach", "an", "Hert\u00b7zog", "Bern\u00b7hard"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "VVFIN", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "finden/", "tokens": ["fin\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Als wenig als der Ri\u00df mit aller macht vnd kunst/", "tokens": ["Als", "we\u00b7nig", "als", "der", "Ri\u00df", "mit", "al\u00b7ler", "macht", "vnd", "kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "ART", "NN", "APPR", "PIAT", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach seinem falschen wohn; den Weltkrai\u00df vber-", "tokens": ["Nach", "sei\u00b7nem", "fal\u00b7schen", "wohn", ";", "den", "Welt\u00b7krai\u00df", "vber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "winden.", "tokens": ["win\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Dan er gedencket/ thut vnd redet nichts vmbsunst/", "tokens": ["Dan", "er", "ge\u00b7den\u00b7cket", "/", "thut", "vnd", "re\u00b7det", "nichts", "vmbsunst", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "PIS", "ADV", "$("], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sein gaist kan des feinds list vnd f\u00fcrsatz bald er-", "tokens": ["Sein", "gaist", "kan", "des", "feinds", "list", "vnd", "f\u00fcr\u00b7satz", "bald", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "ADJA", "NN", "KON", "VVIMP", "ADV", "TRUNC"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "gr\u00fcnden/", "tokens": ["gr\u00fcn\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "So l\u00f6schen seine wort d\u2019 schn\u00f6den auffruhr brunst/", "tokens": ["So", "l\u00f6\u00b7schen", "sei\u00b7ne", "wort", "d'", "schn\u00f6\u00b7den", "auf\u00b7fruhr", "brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NE", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vn\u0303 seiner wafen plitz den Adler selbs verblinden.", "tokens": ["V\u00f1", "sei\u00b7ner", "wa\u00b7fen", "plitz", "den", "Ad\u00b7ler", "selbs", "ver\u00b7blin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "K\u00f6nt er wey\u00df/ kh\u00fcn/ vnnd starck/ des feinds list/", "tokens": ["K\u00f6nt", "er", "wey\u00df", "/", "kh\u00fcn", "/", "vnnd", "starck", "/", "des", "feinds", "list", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$(", "NE", "$(", "KON", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "macht/ betrug/", "tokens": ["macht", "/", "be\u00b7trug", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Entd\u00f6ckend/ jhn zuruck nicht schlagen vnd ver-", "tokens": ["Ent\u00b7d\u00f6\u00b7ckend", "/", "jhn", "zu\u00b7ruck", "nicht", "schla\u00b7gen", "vnd", "ver"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "PPER", "ADV", "PTKNEG", "VVINF", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "treiben/", "tokens": ["trei\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So h\u00e4tten wir l\u00e4ngst nichts/ vnd doch der feind", "tokens": ["So", "h\u00e4t\u00b7ten", "wir", "l\u00e4ngst", "nichts", "/", "vnd", "doch", "der", "feind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "$(", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "kaum gnug.", "tokens": ["kaum", "gnug", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Jedoch ohn deren schand kan man sein lob nicht", "tokens": ["Je\u00b7doch", "ohn", "de\u00b7ren", "schand", "kan", "man", "sein", "lob", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDS", "VVFIN", "VMFIN", "PIS", "PPOSAT", "NN", "PTKNEG"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "schreiben/", "tokens": ["schrei\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die (Stiefs\u00f6hn des Teutschlands) tr\u00e4g/ forcht-", "tokens": ["Die", "(", "Stief\u00b7s\u00f6hn", "des", "Teutschlands", ")", "tr\u00e4g", "/", "forcht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "$(", "NN", "ART", "NN", "$(", "ADJD", "$(", "TRUNC"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "sam vnd vnklug/", "tokens": ["sam", "vnd", "vnk\u00b7lug", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Durch seinen raht vnd hilff gefreyhet/ nicht frey", "tokens": ["Durch", "sei\u00b7nen", "raht", "vnd", "hilff", "ge\u00b7frey\u00b7het", "/", "nicht", "frey"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "$(", "PTKNEG", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "bleiben.", "tokens": ["blei\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}