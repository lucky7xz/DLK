{"textgrid.poem.62429": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Die Preu\u00dfen an der kaiserlichen Grenze", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir gr\u00fc\u00dfen dich mit Waffent\u00e4nzen,", "tokens": ["Wir", "gr\u00fc\u00b7\u00dfen", "dich", "mit", "Waf\u00b7fen\u00b7t\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir neigen uns an deinen Grenzen,", "tokens": ["Wir", "nei\u00b7gen", "uns", "an", "dei\u00b7nen", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du klangreich B\u00f6hmenland!", "tokens": ["Du", "klan\u00b7greich", "B\u00f6h\u00b7men\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O Heer im Schmuck der gr\u00fcnen Reiser,", "tokens": ["O", "Heer", "im", "Schmuck", "der", "gr\u00fc\u00b7nen", "Rei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir rufen Sieg und Heil dem Kaiser,", "tokens": ["Wir", "ru\u00b7fen", "Sieg", "und", "Heil", "dem", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der deinen Sinn erkannt.", "tokens": ["Der", "dei\u00b7nen", "Sinn", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gleichwie im stolzen Brautvereine", "tokens": ["Gleich\u00b7wie", "im", "stol\u00b7zen", "Braut\u00b7ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Main vergeht am starken Rheine,", "tokens": ["Der", "Main", "ver\u00b7geht", "am", "star\u00b7ken", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00fcpft, und braust vor Lust:", "tokens": ["Und", "h\u00fcpft", ",", "und", "braust", "vor", "Lust", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So soll ins Volk ein Volk nun flie\u00dfen,", "tokens": ["So", "soll", "ins", "Volk", "ein", "Volk", "nun", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Heer ans Br\u00fcderheer sich schlie\u00dfen;", "tokens": ["Das", "Heer", "ans", "Br\u00fc\u00b7der\u00b7heer", "sich", "schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Wonne springt die Brust.", "tokens": ["Vor", "Won\u00b7ne", "springt", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Geister Zorn versank in Aschen,", "tokens": ["Der", "Geis\u00b7ter", "Zorn", "ver\u00b7sank", "in", "A\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des R\u00e4chers Hand hat abgewaschen,", "tokens": ["Des", "R\u00e4\u00b7chers", "Hand", "hat", "ab\u00b7ge\u00b7wa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was wider's Recht geschehn.", "tokens": ["Was", "wi\u00b7der's", "Recht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht mehr nun trennt uns S\u00fcd und Norden,", "tokens": ["Nicht", "mehr", "nun", "trennt", "uns", "S\u00fcd", "und", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Lied, Ein Herz, Ein Gott, Ein Orden,", "tokens": ["Ein", "Lied", ",", "Ein", "Herz", ",", "Ein", "Gott", ",", "Ein", "Or\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Deutschland hoch und sch\u00f6n.", "tokens": ["Ein", "Deutschland", "hoch", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Kommt freudig von den Bergen wieder", "tokens": ["Kommt", "freu\u00b7dig", "von", "den", "Ber\u00b7gen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins freie Thal, ihr Fl\u00fcsse, nieder;", "tokens": ["Ins", "frei\u00b7e", "Thal", ",", "ihr", "Fl\u00fcs\u00b7se", ",", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Stra\u00dfen \u00f6ffnet euch!", "tokens": ["Ihr", "Stra\u00b7\u00dfen", "\u00f6ff\u00b7net", "euch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir wollen eure Z\u00f6lle brechen,", "tokens": ["Wir", "wol\u00b7len", "eu\u00b7re", "Z\u00f6l\u00b7le", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir werden alle Schmach nun r\u00e4chen", "tokens": ["Wir", "wer\u00b7den", "al\u00b7le", "Schmach", "nun", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im ganzen heil'gen Reich.", "tokens": ["Im", "gan\u00b7zen", "heil'\u00b7gen", "Reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wo halten wir die Siegesfeier?", "tokens": ["Wo", "hal\u00b7ten", "wir", "die", "Sie\u00b7ge\u00b7sfei\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo wir die Lese halten heuer,", "tokens": ["Wo", "wir", "die", "Le\u00b7se", "hal\u00b7ten", "heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dort bei des Rheines Kraft.", "tokens": ["Dort", "bei", "des", "Rhei\u00b7nes", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir m\u00fcssen ja die Kelter treten:", "tokens": ["Wir", "m\u00fcs\u00b7sen", "ja", "die", "Kel\u00b7ter", "tre\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Blut wird unsre R\u00fcstung r\u00f6then,", "tokens": ["Doch", "Blut", "wird", "uns\u00b7re", "R\u00fcs\u00b7tung", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr jungen Rebensaft.", "tokens": ["F\u00fcr", "jun\u00b7gen", "Re\u00b7ben\u00b7saft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ha, wenn wir dann am Ufer knieen,", "tokens": ["Ha", ",", "wenn", "wir", "dann", "am", "U\u00b7fer", "kni\u00b7e\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und Brust und Stirn der K\u00e4mpfer gl\u00fchen", "tokens": ["Und", "Brust", "und", "Stirn", "der", "K\u00e4mp\u00b7fer", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nach K\u00fchlung in dem Flu\u00df;", "tokens": ["Nach", "K\u00fch\u00b7lung", "in", "dem", "Flu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn wir dann unter Lobges\u00e4ngen", "tokens": ["Wenn", "wir", "dann", "un\u00b7ter", "Lob\u00b7ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Wassers in die L\u00fcfte sprengen", "tokens": ["Des", "Was\u00b7sers", "in", "die", "L\u00fcf\u00b7te", "spren\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zum ersten Freiheitsgru\u00df;", "tokens": ["Zum", "ers\u00b7ten", "Frei\u00b7heits\u00b7gru\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dann dr\u00e4ngt ein Jeder sich zum Bade,", "tokens": ["Dann", "dr\u00e4ngt", "ein", "Je\u00b7der", "sich", "zum", "Ba\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Jeder sich der Schuld entlade:", "tokens": ["Da\u00df", "Je\u00b7der", "sich", "der", "Schuld", "ent\u00b7la\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die V\u00e4ter sind vers\u00f6hnt;", "tokens": ["Die", "V\u00e4\u00b7ter", "sind", "ver\u00b7s\u00f6hnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie wird's euch sein, ihr deutschen Lande,", "tokens": ["Wie", "wird's", "euch", "sein", ",", "ihr", "deut\u00b7schen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VAINF", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn dann das Heer, in Karls Gewande,", "tokens": ["Wenn", "dann", "das", "Heer", ",", "in", "Karls", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Kaiser wieder kr\u00f6nt?", "tokens": ["Den", "Kai\u00b7ser", "wie\u00b7der", "kr\u00f6nt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Panier, Panier, wir sehn dich wallen,", "tokens": ["Pa\u00b7nier", ",", "Pa\u00b7nier", ",", "wir", "sehn", "dich", "wal\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Wunderadler, schrecklich allen", "tokens": ["Du", "Wun\u00b7de\u00b7rad\u00b7ler", ",", "schreck\u00b7lich", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "ADJD", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In deinem heil'gen Glanz!", "tokens": ["In", "dei\u00b7nem", "heil'\u00b7gen", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir sprengen Kette k\u00fchn auf Kette,", "tokens": ["Wir", "spren\u00b7gen", "Ket\u00b7te", "k\u00fchn", "auf", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00e4ngen an des Rhodans Bette", "tokens": ["Und", "h\u00e4n\u00b7gen", "an", "des", "Rho\u00b7dans", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den deutschen Eichenkranz.", "tokens": ["Den", "deut\u00b7schen", "Ei\u00b7chen\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}