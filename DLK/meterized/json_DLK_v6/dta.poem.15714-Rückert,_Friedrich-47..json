{"dta.poem.15714": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie sich ein Greis besinnt auf seine Jugend wieder,", "tokens": ["Wie", "sich", "ein", "Greis", "be\u00b7sinnt", "auf", "sei\u00b7ne", "Ju\u00b7gend", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf seine Jugendspiel', auf seine Jugendlieder;", "tokens": ["Auf", "sei\u00b7ne", "Ju\u00b7gend\u00b7spiel'", ",", "auf", "sei\u00b7ne", "Ju\u00b7gend\u00b7lie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So will sich diese Zeit der Weltgeschicht' entsinnen,", "tokens": ["So", "will", "sich", "die\u00b7se", "Zeit", "der", "Welt\u00b7ge\u00b7schicht'", "ent\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PDAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, als am Ende nun, den Anfang neu gewinnen;", "tokens": ["Und", ",", "als", "am", "En\u00b7de", "nun", ",", "den", "An\u00b7fang", "neu", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "APPRART", "NN", "ADV", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Betrachtend, wie in sich ein abgebl\u00fchter Baum", "tokens": ["Be\u00b7trach\u00b7tend", ",", "wie", "in", "sich", "ein", "ab\u00b7ge\u00b7bl\u00fch\u00b7ter", "Baum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "APPR", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versunken winterlich, nachtr\u00e4umt den Bl\u00fctentraum.", "tokens": ["Ver\u00b7sun\u00b7ken", "win\u00b7ter\u00b7lich", ",", "nach\u00b7tr\u00e4umt", "den", "Bl\u00fc\u00b7ten\u00b7traum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ist er erstorben? nein! und wird auch nicht ersterben,", "tokens": ["Ist", "er", "ers\u00b7tor\u00b7ben", "?", "nein", "!", "und", "wird", "auch", "nicht", "ers\u00b7ter\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "PTKANT", "$.", "KON", "VAFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "---+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wird Kraft durch Winterschlaf zu neuem Lenz erwerben.", "tokens": ["Wird", "Kraft", "durch", "Win\u00b7ter\u00b7schlaf", "zu", "neu\u00b7em", "Lenz", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}