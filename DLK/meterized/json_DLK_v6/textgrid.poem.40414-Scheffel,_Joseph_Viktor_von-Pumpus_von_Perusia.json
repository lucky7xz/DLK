{"textgrid.poem.40414": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Pumpus von Perusia", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feucht hing die Sonne. Des Novembers Schauer ging", "tokens": ["Feucht", "hing", "die", "Son\u00b7ne", ".", "Des", "No\u00b7vem\u00b7bers", "Schau\u00b7er", "ging"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ART", "NN", "NE", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mit leisem Fr\u00f6steln durch das Land Hetruria.", "tokens": ["Mit", "lei\u00b7sem", "Fr\u00f6s\u00b7teln", "durch", "das", "Land", "Het\u00b7ru\u00b7ria", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein mildes Kopfweh, erst der j\u00fcngsten Nacht entstammt,", "tokens": ["Ein", "mil\u00b7des", "Kopf\u00b7weh", ",", "erst", "der", "j\u00fcng\u00b7sten", "Nacht", "ent\u00b7stammt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durchs\u00e4uselte die Luft mit mattem Fl\u00fcgelschlag", "tokens": ["Durch\u00b7s\u00e4u\u00b7sel\u00b7te", "die", "Luft", "mit", "mat\u00b7tem", "Fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ein Gef\u00fchl von Armut lag auf Berg und Tal.", "tokens": ["Und", "ein", "Ge\u00b7f\u00fchl", "von", "Ar\u00b7mut", "lag", "auf", "Berg", "und", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der heilige \u00d6lbaum, dem das letzte gelbe Blatt", "tokens": ["Der", "hei\u00b7li\u00b7ge", "\u00d6l\u00b7baum", ",", "dem", "das", "letz\u00b7te", "gel\u00b7be", "Blatt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Der Wind verweht, reckt' traurig seine \u00c4ste aus,", "tokens": ["Der", "Wind", "ver\u00b7weht", ",", "reckt'", "trau\u00b7rig", "sei\u00b7ne", "\u00c4s\u00b7te", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So kahl und \u00f6d', als fehl' ihm das Notwendigste.", "tokens": ["So", "kahl", "und", "\u00f6d'", ",", "als", "fehl'", "ihm", "das", "Not\u00b7wen\u00b7digs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Verd\u00e4chtig selbst das Stra\u00dfenpflaster. Bl\u00f6dem Aug'", "tokens": ["Ver\u00b7d\u00e4ch\u00b7tig", "selbst", "das", "Stra\u00b7\u00dfen\u00b7pflas\u00b7ter", ".", "Bl\u00f6\u00b7dem", "Aug'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADV", "ART", "NN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Schien des Basaltes urgebirgig fester Stoff", "tokens": ["Schien", "des", "Ba\u00b7sal\u00b7tes", "ur\u00b7ge\u00b7bir\u00b7gig", "fes\u00b7ter", "Stoff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verwandelt heut in sehr por\u00f6ses Tropfgestein,", "tokens": ["Ver\u00b7wan\u00b7delt", "heut", "in", "sehr", "po\u00b7r\u00f6\u00b7ses", "Tropf\u00b7ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und alles \u2013 alles \u2013 alles sah durchl\u00f6chert aus.", "tokens": ["Und", "al\u00b7les", "\u2013", "al\u00b7les", "\u2013", "al\u00b7les", "sah", "durch\u00b7l\u00f6\u00b7chert", "aus", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$(", "PIS", "$(", "PIS", "VVFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So war der Tag, da in der ersten Fr\u00fchestund'", "tokens": ["So", "war", "der", "Tag", ",", "da", "in", "der", "ers\u00b7ten", "Fr\u00fc\u00b7he\u00b7stund'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein m\u00fcder Held aus Populonias Toren zog.", "tokens": ["Ein", "m\u00fc\u00b7der", "Held", "aus", "Po\u00b7pu\u00b7lo\u00b7ni\u00b7as", "To\u00b7ren", "zog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Vergeblich warf von dem kyklopischen Mauerwall", "tokens": ["Ver\u00b7geb\u00b7lich", "warf", "von", "dem", "kyk\u00b7lo\u00b7pi\u00b7schen", "Mau\u00b7er\u00b7wall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Der W\u00e4chter einen trinkgeldhoffnungvollen Blick,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "ei\u00b7nen", "trink\u00b7geld\u00b7hoff\u00b7nung\u00b7vol\u00b7len", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er hielt ihn aus \u2013 und schaute starr \u2013 und gab ihm nichts.", "tokens": ["Er", "hielt", "ihn", "aus", "\u2013", "und", "schau\u00b7te", "starr", "\u2013", "und", "gab", "ihm", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$(", "KON", "VVFIN", "ADJD", "$(", "KON", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dort, wo der Weg sich einbiegt gegen Suessulae", "tokens": ["Dort", ",", "wo", "der", "Weg", "sich", "ein\u00b7biegt", "ge\u00b7gen", "Su\u00b7es\u00b7su\u00b7lae"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PRF", "VVPP", "APPR", "NE"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.2": {"text": "Und eines Priesters kegelturmgeziertes Grab", "tokens": ["Und", "ei\u00b7nes", "Pries\u00b7ters", "ke\u00b7gel\u00b7turm\u00b7ge\u00b7zier\u00b7tes", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Tr\u00fcbtraurig seinen Schatten wirft ins Blachgefild,", "tokens": ["Tr\u00fcb\u00b7trau\u00b7rig", "sei\u00b7nen", "Schat\u00b7ten", "wirft", "ins", "Blach\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dort hielt er still \u2013 und stie\u00df den Speer ins Riedgras ein", "tokens": ["Dort", "hielt", "er", "still", "\u2013", "und", "stie\u00df", "den", "Speer", "ins", "Ried\u00b7gras", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$(", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und suchte lang' in seiner Chlamys Faltenwurf,", "tokens": ["Und", "such\u00b7te", "lang'", "in", "sei\u00b7ner", "Chla\u00b7mys", "Fal\u00b7ten\u00b7wurf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und suchte wieder \u2013 suchte auch zum drittenmal", "tokens": ["Und", "such\u00b7te", "wie\u00b7der", "\u2013", "such\u00b7te", "auch", "zum", "drit\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$(", "VVFIN", "ADV", "APPRART", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und fand nicht, was er suchte ...", "tokens": ["Und", "fand", "nicht", ",", "was", "er", "such\u00b7te", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O wer kennt den Schmerz", "tokens": ["O", "wer", "kennt", "den", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "VVFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Der auf sich b\u00e4umt im biederen Etruskerherz,", "tokens": ["Der", "auf", "sich", "b\u00e4umt", "im", "bie\u00b7de\u00b7ren", "E\u00b7trus\u00b7ker\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn alles \u2013 alles \u2013 alles auf die Neige ging", "tokens": ["Wenn", "al\u00b7les", "\u2013", "al\u00b7les", "\u2013", "al\u00b7les", "auf", "die", "Nei\u00b7ge", "ging"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$(", "PIS", "$(", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und nur der Graus des Leeren in der Tasche wohnt,", "tokens": ["Und", "nur", "der", "Graus", "des", "Lee\u00b7ren", "in", "der", "Ta\u00b7sche", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo der Sesterz sonst fr\u00f6hlich beim Denar erklang!...", "tokens": ["Wo", "der", "Ses\u00b7terz", "sonst", "fr\u00f6h\u00b7lich", "beim", "De\u00b7nar", "er\u00b7klang", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Den Helm abnehmend von dem schwerbedr\u00fcckten Haupt,", "tokens": ["Den", "Helm", "ab\u00b7neh\u00b7mend", "von", "dem", "schwer\u00b7be\u00b7dr\u00fcck\u00b7ten", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fuhr mit der Rechten langsam er zur Stirn empor.", "tokens": ["Fuhr", "mit", "der", "Rech\u00b7ten", "lang\u00b7sam", "er", "zur", "Stirn", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gen Populonia r\u00fcckw\u00e4rts flog sein feuchter Blick", "tokens": ["Gen", "Po\u00b7pu\u00b7lo\u00b7nia", "r\u00fcck\u00b7w\u00e4rts", "flog", "sein", "feuch\u00b7ter", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und blaue Blitze leuchteten im Heldenaug'.", "tokens": ["Und", "blau\u00b7e", "Blit\u00b7ze", "leuch\u00b7te\u00b7ten", "im", "Hel\u00b7de\u00b7naug'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u00bbo Wirtshaus zur Chim\u00e4ra!\u00ab sprach er wehmutvoll,", "tokens": ["\u00bb", "o", "Wirts\u00b7haus", "zur", "Chi\u00b7m\u00e4\u00b7ra", "!", "\u00ab", "sprach", "er", "weh\u00b7mut\u00b7voll", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "APPRART", "NN", "$.", "$(", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u00bbist ", "tokens": ["\u00bb", "ist"], "token_info": ["punct", "word"], "pos": ["$(", "VAFIN"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Der vor drei Tagen kr\u00e4chzend mir zur Linken strich?", "tokens": ["Der", "vor", "drei", "Ta\u00b7gen", "kr\u00e4ch\u00b7zend", "mir", "zur", "Lin\u00b7ken", "strich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "ADJD", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sprach ", "tokens": ["Sprach"], "token_info": ["word"], "pos": ["NN"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "O Wirtshaus zur Chim\u00e4ra! was ist lieblicher", "tokens": ["O", "Wirts\u00b7haus", "zur", "Chi\u00b7m\u00e4\u00b7ra", "!", "was", "ist", "lieb\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "APPRART", "NN", "$.", "PWS", "VAFIN", "ADJD"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Als einzuziehn, ein Gastfreund, in dein Gastgemach?", "tokens": ["Als", "ein\u00b7zu\u00b7ziehn", ",", "ein", "Gast\u00b7freund", ",", "in", "dein", "Gast\u00b7ge\u00b7mach", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVIZU", "$,", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verst\u00e4ndig waltet dort ein vielge\u00fcbter Wirt,", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", "wal\u00b7tet", "dort", "ein", "viel\u00b7ge\u00b7\u00fcb\u00b7ter", "Wirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und edle Helden sitzen um den k\u00fchlen Trank,", "tokens": ["Und", "ed\u00b7le", "Hel\u00b7den", "sit\u00b7zen", "um", "den", "k\u00fch\u00b7len", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den von dem Berg herabgesendet Dimeros.", "tokens": ["Den", "von", "dem", "Berg", "her\u00b7ab\u00b7ge\u00b7sen\u00b7det", "Di\u00b7me\u00b7ros", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weisheit entstr\u00f6mt bedachtsam zechender M\u00e4nner Mund", "tokens": ["Weis\u00b7heit", "ent\u00b7str\u00f6mt", "be\u00b7dacht\u00b7sam", "ze\u00b7chen\u00b7der", "M\u00e4n\u00b7ner", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "ADJA", "NN", "NN"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Zumal an jenem obern, linnenwei\u00dfen Tisch,", "tokens": ["Zu\u00b7mal", "an", "je\u00b7nem", "o\u00b7bern", ",", "lin\u00b7nen\u00b7wei\u00b7\u00dfen", "Tisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Tegulinums Augur, sp\u00e4ter Mitternacht", "tokens": ["Wo", "Te\u00b7gu\u00b7li\u00b7nums", "Au\u00b7gur", ",", "