{"textgrid.poem.52940": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein Sonntag-Morgen im Mai,", "tokens": ["Es", "war", "ein", "Sonn\u00b7tag\u00b7Mor\u00b7gen", "im", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df ich am Pilatus fuhr vorbei.", "tokens": ["Da\u00df", "ich", "am", "Pi\u00b7la\u00b7tus", "fuhr", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Freund sa\u00df neben mir im Kahn,", "tokens": ["Ein", "Freund", "sa\u00df", "ne\u00b7ben", "mir", "im", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Wir sahen uns Wasser und Felsen an.", "tokens": ["Wir", "sa\u00b7hen", "uns", "Was\u00b7ser", "und", "Fel\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Der See lag glatt wie ein Spiegel da,", "tokens": ["Der", "See", "lag", "glatt", "wie", "ein", "Spie\u00b7gel", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Segel, kein Ruder fern und nah.", "tokens": ["Kein", "Se\u00b7gel", ",", "kein", "Ru\u00b7der", "fern", "und", "nah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Um die Alpen flogen ungestalt", "tokens": ["Um", "die", "Al\u00b7pen", "flo\u00b7gen", "un\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nebel und Wolken, zu Klumpen geballt;", "tokens": ["Ne\u00b7bel", "und", "Wol\u00b7ken", ",", "zu", "Klum\u00b7pen", "ge\u00b7ballt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur wenn das Sonnenlicht sie brach,", "tokens": ["Nur", "wenn", "das", "Son\u00b7nen\u00b7licht", "sie", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerrissen die Schatten allgemach.", "tokens": ["Zer\u00b7ris\u00b7sen", "die", "Schat\u00b7ten", "all\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Auf einmal flammten Zinken und H\u00f6hn", "tokens": ["Auf", "ein\u00b7mal", "flamm\u00b7ten", "Zin\u00b7ken", "und", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In heller Verkl\u00e4rung wundersch\u00f6n.", "tokens": ["In", "hel\u00b7ler", "Ver\u00b7kl\u00e4\u00b7rung", "wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich jubelte: Trifft erst die Spitzen ein Strahl,", "tokens": ["Ich", "ju\u00b7bel\u00b7te", ":", "Trifft", "erst", "die", "Spit\u00b7zen", "ein", "Strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So f\u00e4llt auch bald der Nebel im Tal.", "tokens": ["So", "f\u00e4llt", "auch", "bald", "der", "Ne\u00b7bel", "im", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Mein Freund schwieg still und nickte f\u00fcr sich,", "tokens": ["Mein", "Freund", "schwieg", "still", "und", "nick\u00b7te", "f\u00fcr", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "APPR", "PRF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nach kurzer Weile ergriff er mich", "tokens": ["Nach", "kur\u00b7zer", "Wei\u00b7le", "er\u00b7griff", "er", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und wies auf die Felsen und wies ins Tal, \u2013", "tokens": ["Und", "wies", "auf", "die", "Fel\u00b7sen", "und", "wies", "ins", "Tal", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das war eine Nacht, ein Nebel zumal.", "tokens": ["Das", "war", "ei\u00b7ne", "Nacht", ",", "ein", "Ne\u00b7bel", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Im See und am Himmel kein bi\u00dfchen Blau,", "tokens": ["Im", "See", "und", "am", "Him\u00b7mel", "kein", "bi\u00df\u00b7chen", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nichts Gr\u00fcnes am Ufer! \u2013 Nur Grau in Grau!", "tokens": ["Nichts", "Gr\u00fc\u00b7nes", "am", "U\u00b7fer", "!", "\u2013", "Nur", "Grau", "in", "Grau", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "APPRART", "NN", "$.", "$(", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Wir dr\u00fcckten uns stark und stumm die Hand,", "tokens": ["Wir", "dr\u00fcck\u00b7ten", "uns", "stark", "und", "stumm", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir dachten \u2013 an unser Vaterland.", "tokens": ["Wir", "dach\u00b7ten", "\u2013", "an", "un\u00b7ser", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}