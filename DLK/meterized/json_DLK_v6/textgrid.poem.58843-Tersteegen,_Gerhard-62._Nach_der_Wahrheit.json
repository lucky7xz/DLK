{"textgrid.poem.58843": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "62. Nach der Wahrheit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich 'was ber\u00fchren will,", "tokens": ["Wenn", "dich", "'was", "be\u00b7r\u00fch\u00b7ren", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau Gott an einf\u00e4ltig, still,", "tokens": ["Schau", "Gott", "an", "ein\u00b7f\u00e4l\u00b7tig", ",", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du nach der Wahrheit Lehr'", "tokens": ["Da\u00df", "du", "nach", "der", "Wahr\u00b7heit", "Lehr'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich bewegest und nicht mehr!", "tokens": ["Dich", "be\u00b7we\u00b7gest", "und", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}