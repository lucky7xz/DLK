{"dta.poem.3310": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Gl\u00fcckseligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wahrheit ernste Stimm erschallt in mei-", "tokens": ["Der", "Wahr\u00b7heit", "erns\u00b7te", "Stimm", "er\u00b7schallt", "in", "mei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nem Busen:", "tokens": ["nem", "Bu\u00b7sen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "H\u00f6rt eure Lehrerinn! sie selbst hat mich er-", "tokens": ["H\u00f6rt", "eu\u00b7re", "Leh\u00b7re\u00b7rinn", "!", "sie", "selbst", "hat", "mich", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "$.", "PPER", "ADV", "VAFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "nannt", "tokens": ["nannt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und auf den Fl\u00fcgeln s\u00fcsser Musen", "tokens": ["Und", "auf", "den", "Fl\u00fc\u00b7geln", "s\u00fcs\u00b7ser", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An euch, ihr Sterblichen! gesandt.", "tokens": ["An", "euch", ",", "ihr", "Sterb\u00b7li\u00b7chen", "!", "ge\u00b7sandt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$.", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Es flammt ein Welten-Heer in angewiesnen Gr\u00e4n-", "tokens": ["Es", "flammt", "ein", "Wel\u00b7ten\u00b7Heer", "in", "an\u00b7ge\u00b7wi\u00b7es\u00b7nen", "Gr\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "zen:", "tokens": ["zen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Es ist im lichten Raum, wo in bestimmter Bahn", "tokens": ["Es", "ist", "im", "lich\u00b7ten", "Raum", ",", "wo", "in", "be\u00b7stimm\u00b7ter", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ungez\u00e4hlten Sonnen gl\u00e4nzen,", "tokens": ["Die", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Son\u00b7nen", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Ordnung alles unterthan.", "tokens": ["Der", "Ord\u00b7nung", "al\u00b7les", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zur Ordnung ward, was ist, eh etwas war, erlesen:", "tokens": ["Zur", "Ord\u00b7nung", "ward", ",", "was", "ist", ",", "eh", "et\u00b7was", "war", ",", "er\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,", "PWS", "VAFIN", "$,", "KOUS", "PIS", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie fordert sanften West und st\u00fcrmisch Ungest\u00fcm:", "tokens": ["Sie", "for\u00b7dert", "sanf\u00b7ten", "West", "und", "st\u00fcr\u00b7misch", "Un\u00b7ge\u00b7st\u00fcm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr Band verkn\u00fcpfet alle Wesen,", "tokens": ["Ihr", "Band", "ver\u00b7kn\u00fcp\u00b7fet", "al\u00b7le", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Staube bis zu Cherubim.", "tokens": ["Vom", "Stau\u00b7be", "bis", "zu", "Che\u00b7ru\u00b7bim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der ganzen Sch\u00f6pfung Wohl ist unser erst Gesetze:", "tokens": ["Der", "gan\u00b7zen", "Sch\u00f6p\u00b7fung", "Wohl", "ist", "un\u00b7ser", "erst", "Ge\u00b7set\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "PPOSAT", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich werde gl\u00fccklich seyn, wenn ich durch keine That", "tokens": ["Ich", "wer\u00b7de", "gl\u00fcck\u00b7lich", "seyn", ",", "wenn", "ich", "durch", "kei\u00b7ne", "That"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VAINF", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die\u00df allgemeine Wohl verletze,", "tokens": ["Die\u00df", "all\u00b7ge\u00b7mei\u00b7ne", "Wohl", "ver\u00b7let\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr welches ich die Welt betrat:", "tokens": ["F\u00fcr", "wel\u00b7ches", "ich", "die", "Welt", "be\u00b7trat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn wider meine Pflicht mein Herz sich nicht em-", "tokens": ["Wenn", "wi\u00b7der", "mei\u00b7ne", "Pflicht", "mein", "Herz", "sich", "nicht", "em"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "PRF", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "p\u00f6ret,", "tokens": ["p\u00f6\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und niedrer Eigennutz, der die Begierden stimmt", "tokens": ["Und", "nie\u00b7drer", "Ei\u00b7gen\u00b7nutz", ",", "der", "die", "Be\u00b7gier\u00b7den", "stimmt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre Harmonie zerst\u00f6ret,", "tokens": ["Und", "ih\u00b7re", "Har\u00b7mo\u00b7nie", "zer\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht unter meinen Trieben glimmt.", "tokens": ["Nicht", "un\u00b7ter", "mei\u00b7nen", "Trie\u00b7ben", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Quelle falscher Lust, die Aristipp gefunden,", "tokens": ["Die", "Quel\u00b7le", "fal\u00b7scher", "Lust", ",", "die", "A\u00b7ris\u00b7tipp", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Haucht ekle Bitterkeit selbst unter Bluhmen aus.", "tokens": ["Haucht", "ek\u00b7le", "Bit\u00b7ter\u00b7keit", "selbst", "un\u00b7ter", "Bluh\u00b7men", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Weichling dr\u00fccken leere Stunden:", "tokens": ["Den", "Weich\u00b7ling", "dr\u00fc\u00b7cken", "lee\u00b7re", "Stun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ruhe flieht sein marmorn Haus.", "tokens": ["Die", "Ru\u00b7he", "flieht", "sein", "mar\u00b7morn", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn reine Freude quillt allein aus reinem Herzen:", "tokens": ["Denn", "rei\u00b7ne", "Freu\u00b7de", "quillt", "al\u00b7lein", "aus", "rei\u00b7nem", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Zeugni\u00df, da\u00df wir thun, was unsre Pflicht gebeut,", "tokens": ["Sein", "Zeug\u00b7ni\u00df", ",", "da\u00df", "wir", "thun", ",", "was", "uns\u00b7re", "Pflicht", "ge\u00b7beut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entwaffnet Ungeduld und Schmerzen,", "tokens": ["Ent\u00b7waff\u00b7net", "Un\u00b7ge\u00b7duld", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Tagen voller Dunkelheit.", "tokens": ["In", "Ta\u00b7gen", "vol\u00b7ler", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Qu\u00e4lt mich sein Urtheil nicht mit nagendem Verdrusse,", "tokens": ["Qu\u00e4lt", "mich", "sein", "Ur\u00b7theil", "nicht", "mit", "na\u00b7gen\u00b7dem", "Ver\u00b7drus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sey mein Eigenthum der schlauen Bosheit Raub;", "tokens": ["So", "sey", "mein", "Ei\u00b7gen\u00b7thum", "der", "schlau\u00b7en", "Bos\u00b7heit", "Raub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So trete mich mit stolzem Fusse", "tokens": ["So", "tre\u00b7te", "mich", "mit", "stol\u00b7zem", "Fus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ungest\u00fcme Gl\u00fcck in Staub.", "tokens": ["Das", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Gl\u00fcck", "in", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich winsle nicht um Trost, nicht weibisch um Er-", "tokens": ["Ich", "wins\u00b7le", "nicht", "um", "Trost", ",", "nicht", "wei\u00b7bisch", "um", "Er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "PTKNEG", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "barmen:", "tokens": ["bar\u00b7men", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die Ruhe folget mir zum armen Strohdach hin,", "tokens": ["Die", "Ru\u00b7he", "fol\u00b7get", "mir", "zum", "ar\u00b7men", "Stroh\u00b7dach", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo ich in reiner Wollust Armen", "tokens": ["Wo", "ich", "in", "rei\u00b7ner", "Wol\u00b7lust", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch Unschuld reich und gl\u00fccklich bin.", "tokens": ["Durch", "Un\u00b7schuld", "reich", "und", "gl\u00fcck\u00b7lich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Fehlt innre Ruhe nicht; was fehlet meinem Leben,", "tokens": ["Fehlt", "inn\u00b7re", "Ru\u00b7he", "nicht", ";", "was", "feh\u00b7let", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKNEG", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als was entbehrlich ist und unentbehrlich scheint?", "tokens": ["Als", "was", "ent\u00b7behr\u00b7lich", "ist", "und", "un\u00b7ent\u00b7behr\u00b7lich", "scheint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sollt ich bey iedem Unfall beben,", "tokens": ["Sollt", "ich", "bey", "ie\u00b7dem", "Un\u00b7fall", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und weinen, wann die Thorheit weint?", "tokens": ["Und", "wei\u00b7nen", ",", "wann", "die", "Thor\u00b7heit", "weint", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Mit weiser Huld vertheilt das Schicksal Weh und", "tokens": ["Mit", "wei\u00b7ser", "Huld", "ver\u00b7theilt", "das", "Schick\u00b7sal", "Weh", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "ART", "NN", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Freuden,", "tokens": ["Freu\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Das bald auf Rosen uns durchs Leben wandern hei\u00dft,", "tokens": ["Das", "bald", "auf", "Ro\u00b7sen", "uns", "durchs", "Le\u00b7ben", "wan\u00b7dern", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "PPER", "APPRART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald aber durch bedornte Leiden", "tokens": ["Bald", "a\u00b7ber", "durch", "be\u00b7dorn\u00b7te", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Lasters Armen uns entrei\u00dft.", "tokens": ["Des", "Las\u00b7ters", "Ar\u00b7men", "uns", "ent\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ein Blick in vorig Leid wird k\u00fcnftig uns entz\u00fccken,", "tokens": ["Ein", "Blick", "in", "vo\u00b7rig", "Leid", "wird", "k\u00fcnf\u00b7tig", "uns", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "NN", "VAFIN", "ADJD", "PPER"