{"textgrid.poem.26704": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber Str\u00f6me hast du gesetzt und Meere durchschwommen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber Str\u00f6me hast du gesetzt und Meere durchschwommen,", "tokens": ["\u00dc\u00b7ber", "Str\u00f6\u00b7me", "hast", "du", "ge\u00b7setzt", "und", "Mee\u00b7re", "durch\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "KON", "NN", "VVINF", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00dcber der Alpen Gebirg trug dich der schwindligte Steg,", "tokens": ["\u00dc\u00b7ber", "der", "Al\u00b7pen", "Ge\u00b7birg", "trug", "dich", "der", "schwind\u00b7lig\u00b7te", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mich in der N\u00e4he zu schaun und meine Sch\u00f6ne zu preisen,", "tokens": ["Mich", "in", "der", "N\u00e4\u00b7he", "zu", "schaun", "und", "mei\u00b7ne", "Sch\u00f6\u00b7ne", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die der begeisterte Ruf r\u00fchmt durch die staunende Welt;", "tokens": ["Die", "der", "be\u00b7geis\u00b7ter\u00b7te", "Ruf", "r\u00fchmt", "durch", "die", "stau\u00b7nen\u00b7de", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und nun stehst du vor mir, du darfst mich Heilge ber\u00fchren,", "tokens": ["Und", "nun", "stehst", "du", "vor", "mir", ",", "du", "darfst", "mich", "Heil\u00b7ge", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "--+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Aber bist du mir jetzt n\u00e4her, und bin ich es dir?", "tokens": ["A\u00b7ber", "bist", "du", "mir", "jetzt", "n\u00e4\u00b7her", ",", "und", "bin", "ich", "es", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$,", "KON", "VAFIN", "PPER", "PPER", "PPER", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}