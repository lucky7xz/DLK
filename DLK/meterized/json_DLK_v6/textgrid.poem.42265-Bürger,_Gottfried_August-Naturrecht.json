{"textgrid.poem.42265": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Naturrecht", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Blum' und Frucht, so die Natur erschafft,", "tokens": ["Von", "Blum'", "und", "Frucht", ",", "so", "die", "Na\u00b7tur", "er\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Darf ich zur Lust, wie zum Bed\u00fcrfnis, pfl\u00fccken.", "tokens": ["Darf", "ich", "zur", "Lust", ",", "wie", "zum", "Be\u00b7d\u00fcrf\u00b7nis", ",", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich darf getrost nach allem Sch\u00f6nen blicken,", "tokens": ["Ich", "darf", "ge\u00b7trost", "nach", "al\u00b7lem", "Sch\u00f6\u00b7nen", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und atmen darf ich jeder W\u00fcrze Kraft.", "tokens": ["Und", "at\u00b7men", "darf", "ich", "je\u00b7der", "W\u00fcr\u00b7ze", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich darf die Traub', ich darf der Biene Saft,", "tokens": ["Ich", "darf", "die", "Traub'", ",", "ich", "darf", "der", "Bie\u00b7ne", "Saft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PPER", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Schafes Milch in meine Schale dr\u00fccken.", "tokens": ["Des", "Scha\u00b7fes", "Milch", "in", "mei\u00b7ne", "Scha\u00b7le", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir front der Stier; mir beut das Ro\u00df den R\u00fccken;", "tokens": ["Mir", "front", "der", "Stier", ";", "mir", "beut", "das", "Ro\u00df", "den", "R\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Seidenwurm spinnt Atlas mir und Taft.", "tokens": ["Der", "Sei\u00b7den\u00b7wurm", "spinnt", "At\u00b7las", "mir", "und", "Taft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es darf das Lied der holden Nachtigallen", "tokens": ["Es", "darf", "das", "Lied", "der", "hol\u00b7den", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich, hingestreckt auf Flaumen oder Moos,", "tokens": ["Mich", ",", "hin\u00b7ge\u00b7streckt", "auf", "Flau\u00b7men", "o\u00b7der", "Moos", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl in den Schlaf, wohl aus dem Schlafe hallen.", "tokens": ["Wohl", "in", "den", "Schlaf", ",", "wohl", "aus", "dem", "Schla\u00b7fe", "hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "Was wehrt es denn mir Menschensatzung, blo\u00df", "tokens": ["Was", "wehrt", "es", "denn", "mir", "Men\u00b7schen\u00b7sat\u00b7zung", ",", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPER", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus bl\u00f6dem Wahn, in Molly's Wonnescho\u00df,", "tokens": ["Aus", "bl\u00f6\u00b7dem", "Wahn", ",", "in", "Mol\u00b7ly's", "Won\u00b7ne\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Lieb' und Lust bezwungen, hinzufallen?", "tokens": ["Von", "Lieb'", "und", "Lust", "be\u00b7zwun\u00b7gen", ",", "hin\u00b7zu\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}