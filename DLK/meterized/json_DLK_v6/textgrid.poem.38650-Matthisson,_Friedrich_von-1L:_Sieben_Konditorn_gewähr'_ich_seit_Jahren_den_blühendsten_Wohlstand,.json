{"textgrid.poem.38650": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieben Konditorn gew\u00e4hr' ich seit Jahren den bl\u00fchendsten Wohlstand,", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieben Konditorn gew\u00e4hr' ich seit Jahren den bl\u00fchendsten Wohlstand,", "tokens": ["Sie\u00b7ben", "Kon\u00b7di\u00b7torn", "ge\u00b7w\u00e4hr'", "ich", "seit", "Jah\u00b7ren", "den", "bl\u00fc\u00b7hends\u00b7ten", "Wohl\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+--+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Aber beim B\u00fccherverkehr s\u00e4nk' ich in Armuth und Schmach.", "tokens": ["A\u00b7ber", "beim", "B\u00fc\u00b7cher\u00b7ver\u00b7kehr", "s\u00e4nk'", "ich", "in", "Ar\u00b7muth", "und", "Schmach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-++-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Jetzo regiert mich, als Bernischer Landvogt, ein Liebling der Musen;", "tokens": ["Jet\u00b7zo", "re\u00b7giert", "mich", ",", "als", "Ber\u00b7ni\u00b7scher", "Land\u00b7vogt", ",", "ein", "Lieb\u00b7ling", "der", "Mu\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So wird nun auf dem Parna\u00df endlich auch meiner gedacht.", "tokens": ["So", "wird", "nun", "auf", "dem", "Par\u00b7na\u00df", "end\u00b7lich", "auch", "mei\u00b7ner", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NE", "ADV", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}