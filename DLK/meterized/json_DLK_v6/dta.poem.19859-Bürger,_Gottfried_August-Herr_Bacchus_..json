{"dta.poem.19859": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Herr Bacchus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Bacchus ist ein braver Man,               ", "tokens": ["Herr", "Bac\u00b7chus", "ist", "ein", "bra\u00b7ver", "Man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ART", "ADJA", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das kan ich euch versichern.", "tokens": ["Das", "kan", "ich", "euch", "ver\u00b7si\u00b7chern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mehr als Apoll, der Leierman,", "tokens": ["Mehr", "als", "A\u00b7poll", ",", "der", "Lei\u00b7er\u00b7man", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Mit seinen Notenb\u00fcchern.", "tokens": ["Mit", "sei\u00b7nen", "No\u00b7ten\u00b7b\u00fc\u00b7chern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Armen ganzer Reichthum ist", "tokens": ["Des", "Ar\u00b7men", "gan\u00b7zer", "Reicht\u00b7hum", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die goldbemalte Leier,", "tokens": ["Die", "gold\u00b7be\u00b7mal\u00b7te", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von der er pralet, wie ihr wist,", "tokens": ["Von", "der", "er", "pra\u00b7let", ",", "wie", "ihr", "wist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sey entsezlich theuer.", "tokens": ["Sie", "sey", "ent\u00b7sez\u00b7lich", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch borgt ihm auf sein Instrument", "tokens": ["Doch", "borgt", "ihm", "auf", "sein", "Inst\u00b7ru\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Kluger einen Heller;", "tokens": ["Kein", "Klu\u00b7ger", "ei\u00b7nen", "Hel\u00b7ler", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn frohere Musik ert\u00f6nt", "tokens": ["Denn", "fro\u00b7he\u00b7re", "Mu\u00b7sik", "er\u00b7t\u00f6nt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Vater Evans Keller.", "tokens": ["Aus", "Va\u00b7ter", "E\u00b7vans", "Kel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und ob Apoll sich gleich voran", "tokens": ["Und", "ob", "A\u00b7poll", "sich", "gleich", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NE", "PRF", "ADV", "PTKVZ"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit seiner Dichtkunst bl\u00e4het;", "tokens": ["Mit", "sei\u00b7ner", "Dicht\u00b7kunst", "bl\u00e4\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So ist doch Bacchus auch ein Man,", "tokens": ["So", "ist", "doch", "Bac\u00b7chus", "auch", "ein", "Man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NE", "ADV", "ART", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der seinen Vers verstehet.", "tokens": ["Der", "sei\u00b7nen", "Vers", "ver\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie mag am waldigen Parnas", "tokens": ["Wie", "mag", "am", "wal\u00b7di\u00b7gen", "Par\u00b7nas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wol sein Diskant gefallen?", "tokens": ["Wol", "sein", "Dis\u00b7kant", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Hier solte Bacchus Kantorbas", "tokens": ["Hier", "sol\u00b7te", "Bac\u00b7chus", "Kan\u00b7tor\u00b7bas"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcrwahr weit besser schallen.", "tokens": ["F\u00fcr\u00b7wahr", "weit", "bes\u00b7ser", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Auf! last uns ihn f\u00fcr den Apoll", "tokens": ["Auf", "!", "last", "uns", "ihn", "f\u00fcr", "den", "A\u00b7poll"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "$.", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zum Dichtergott erbitten;", "tokens": ["Zum", "Dich\u00b7ter\u00b7gott", "er\u00b7bit\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn er ist gar vortreflich wohl", "tokens": ["Denn", "er", "ist", "gar", "vor\u00b7tre\u00b7flich", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei grossen Herrn gelitten.", "tokens": ["Bei", "gros\u00b7sen", "Herrn", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Apoll mus tief geb\u00fckt und krum", "tokens": ["A\u00b7poll", "mus", "tief", "ge\u00b7b\u00fckt", "und", "krum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADJD", "VVPP", "KON", "XY"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In F\u00fcrstens\u00e4le schleichen;", "tokens": ["In", "F\u00fcrs\u00b7ten\u00b7s\u00e4\u00b7le", "schlei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein mit Bacchus gehn sie um,", "tokens": ["Al\u00b7lein", "mit", "Bac\u00b7chus", "gehn", "sie", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie mit ihres Gleichen.", "tokens": ["Als", "wie", "mit", "ih\u00b7res", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dann wollen wir auf den Parnas,", "tokens": ["Dann", "wol\u00b7len", "wir", "auf", "den", "Par\u00b7nas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Vor allen andern Dingen,", "tokens": ["Vor", "al\u00b7len", "an\u00b7dern", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das grosse Heidelberger Fas", "tokens": ["Das", "gros\u00b7se", "Hei\u00b7del\u00b7ber\u00b7ger", "Fas"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vol Nierensteiner bringen.", "tokens": ["Vol", "Nie\u00b7ren\u00b7stei\u00b7ner", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Stat Lorbeerb\u00e4ume wollen wir", "tokens": ["Stat", "Lor\u00b7beer\u00b7b\u00e4u\u00b7me", "wol\u00b7len", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort Rebenst\u00f6cke pflanzen,", "tokens": ["Dort", "Re\u00b7ben\u00b7st\u00f6\u00b7cke", "pflan\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und rings um volle Tonnen, schier", "tokens": ["Und", "rings", "um", "vol\u00b7le", "Ton\u00b7nen", ",", "schier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie die Bacchanten tanzen.", "tokens": ["Wie", "die", "Bac\u00b7chan\u00b7ten", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Man lebte so nach altem Brauch", "tokens": ["Man", "leb\u00b7te", "so", "nach", "al\u00b7tem", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bisher dort alzun\u00fcchtern.", "tokens": ["Bis\u00b7her", "dort", "al\u00b7zu\u00b7n\u00fcch\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum blieben die neun Jungfern auch", "tokens": ["Drum", "blie\u00b7ben", "die", "neun", "Jung\u00b7fern", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "CARD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von je und je so sch\u00fcchtern.", "tokens": ["Von", "je", "und", "je", "so", "sch\u00fcch\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ha! zapften sie sich ihren Trank", "tokens": ["Ha", "!", "zapf\u00b7ten", "sie", "sich", "ih\u00b7ren", "Trank"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Bacchus Nektartonnen,", "tokens": ["Aus", "Bac\u00b7chus", "Nek\u00b7tar\u00b7ton\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie jagten Bl\u00f6digkeit und Zwang", "tokens": ["Sie", "jag\u00b7ten", "Bl\u00f6\u00b7dig\u00b7keit", "und", "Zwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Kl\u00f6ster zu den Nonnen.", "tokens": ["In", "Kl\u00f6s\u00b7ter", "zu", "den", "Non\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "F\u00fcrwahr! sie liessen nicht mit M\u00fch", "tokens": ["F\u00fcr\u00b7wahr", "!", "sie", "lies\u00b7sen", "nicht", "mit", "M\u00fch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur kleinsten Gunst sich zwingen,", "tokens": ["Zur", "kleins\u00b7ten", "Gunst", "sich", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ungerufen w\u00fcrden sie", "tokens": ["Und", "un\u00b7ge\u00b7ru\u00b7fen", "w\u00fcr\u00b7den", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns in die Arme springen.", "tokens": ["Uns", "in", "die", "Ar\u00b7me", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}