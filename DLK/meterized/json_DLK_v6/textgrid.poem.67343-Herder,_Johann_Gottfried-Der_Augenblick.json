{"textgrid.poem.67343": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Augenblick", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum denn w\u00e4hrt des Lebens Gl\u00fcck", "tokens": ["Wa\u00b7rum", "denn", "w\u00e4hrt", "des", "Le\u00b7bens", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die zarteste der Freuden", "tokens": ["Die", "zar\u00b7tes\u00b7te", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stirbt wie der Schmetterling,", "tokens": ["Stirbt", "wie", "der", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der, hangend an der Blume,", "tokens": ["Der", ",", "han\u00b7gend", "an", "der", "Blu\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verging, verging.", "tokens": ["Ver\u00b7ging", ",", "ver\u00b7ging", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wir ahnen, wir genie\u00dfen kaum", "tokens": ["Wir", "ah\u00b7nen", ",", "wir", "ge\u00b7nie\u00b7\u00dfen", "kaum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens kurzen Traum.", "tokens": ["Des", "Le\u00b7bens", "kur\u00b7zen", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur im unsel'gen Leiden", "tokens": ["Nur", "im", "un\u00b7sel'\u00b7gen", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wird unser Herzeleid", "tokens": ["Wird", "un\u00b7ser", "Her\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In einer bangen Stunde", "tokens": ["In", "ei\u00b7ner", "ban\u00b7gen", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zur Ewigkeit.", "tokens": ["Zur", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}