{"dta.poem.8208": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Silvia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Noch jene Mondesm\u00e4rchenpracht,", "tokens": ["Noch", "je\u00b7ne", "Mon\u00b7des\u00b7m\u00e4r\u00b7chen\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00fcnd' sie noch in Waldesschatten", "tokens": ["Und", "st\u00fcnd'", "sie", "noch", "in", "Wal\u00b7des\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Inmitten jener Sommernacht;", "tokens": ["In\u00b7mit\u00b7ten", "je\u00b7ner", "Som\u00b7mer\u00b7nacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00e4nd' ich selber wie im Traume", "tokens": ["Und", "f\u00e4nd'", "ich", "sel\u00b7ber", "wie", "im", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Weg zur\u00fcck durch Moor und Feld,", "tokens": ["Den", "Weg", "zu\u00b7r\u00fcck", "durch", "Moor", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schritte doch von Waldessaume", "tokens": ["Sie", "schrit\u00b7te", "doch", "von", "Wal\u00b7des\u00b7sau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Niemals hinunter in die Welt.", "tokens": ["Nie\u00b7mals", "hin\u00b7un\u00b7ter", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}