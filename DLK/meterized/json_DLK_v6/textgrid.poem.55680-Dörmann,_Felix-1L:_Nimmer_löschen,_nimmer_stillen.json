{"textgrid.poem.55680": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimmer l\u00f6schen, nimmer stillen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimmer l\u00f6schen, nimmer stillen", "tokens": ["Nim\u00b7mer", "l\u00f6\u00b7schen", ",", "nim\u00b7mer", "stil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann ich diese dunkle Sehnsucht", "tokens": ["Kann", "ich", "die\u00b7se", "dunk\u00b7le", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Tode.", "tokens": ["Nach", "dem", "To\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "All mein atemloses K\u00e4mpfen,", "tokens": ["All", "mein", "a\u00b7tem\u00b7lo\u00b7ses", "K\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie zu zwingen, ist vergebens.", "tokens": ["Sie", "zu", "zwin\u00b7gen", ",", "ist", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jene Zeiten, wo ich glaubte", "tokens": ["Je\u00b7ne", "Zei\u00b7ten", ",", "wo", "ich", "glaub\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine hei\u00dfe, tiefe Liebe", "tokens": ["Ei\u00b7ne", "hei\u00b7\u00dfe", ",", "tie\u00b7fe", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnte tilgen diese Sehnsucht,", "tokens": ["K\u00f6nn\u00b7te", "til\u00b7gen", "die\u00b7se", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind vor\u00fcber \u2013 tot \u2013 begraben;", "tokens": ["Sind", "vor\u00b7\u00fc\u00b7ber", "\u2013", "tot", "\u2013", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "$(", "ADJD", "$(", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die Liebe ist gekommen", "tokens": ["Denn", "die", "Lie\u00b7be", "ist", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die dunkle Sehnsucht blieb,", "tokens": ["Und", "die", "dunk\u00b7le", "Sehn\u00b7sucht", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Liebe ist geschieden,", "tokens": ["Und", "die", "Lie\u00b7be", "ist", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Sehnsucht stieg und stieg.", "tokens": ["Und", "die", "Sehn\u00b7sucht", "stieg", "und", "stieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Nimmer l\u00f6schen, nimmer stillen", "tokens": ["Nim\u00b7mer", "l\u00f6\u00b7schen", ",", "nim\u00b7mer", "stil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann ich diese dunkle Sehnsucht", "tokens": ["Kann", "ich", "die\u00b7se", "dunk\u00b7le", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Tode.", "tokens": ["Nach", "dem", "To\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "All mein atemloses K\u00e4mpfen,", "tokens": ["All", "mein", "a\u00b7tem\u00b7lo\u00b7ses", "K\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie zu zwingen, ist vergebens.", "tokens": ["Sie", "zu", "zwin\u00b7gen", ",", "ist", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}