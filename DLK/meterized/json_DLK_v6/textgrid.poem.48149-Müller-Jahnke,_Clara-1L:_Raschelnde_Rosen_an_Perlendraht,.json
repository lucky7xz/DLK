{"textgrid.poem.48149": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Raschelnde Rosen an Perlendraht,", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Raschelnde Rosen an Perlendraht,", "tokens": ["Ra\u00b7scheln\u00b7de", "Ro\u00b7sen", "an", "Per\u00b7len\u00b7draht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Badep\u00fcppchen im Heiligenstaat,", "tokens": ["Ba\u00b7de\u00b7p\u00fcpp\u00b7chen", "im", "Hei\u00b7li\u00b7gen\u00b7staat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Gruftkapellchen mit Polstersitzen,", "tokens": ["Gruft\u00b7ka\u00b7pell\u00b7chen", "mit", "Pols\u00b7ter\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "leinene Deckchen mit H\u00e4kelspitzen,", "tokens": ["lei\u00b7ne\u00b7ne", "Deck\u00b7chen", "mit", "H\u00e4\u00b7kel\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Kreuzchen und Bildwerk, Flitter und Spiel,", "tokens": ["Kreuzc\u00b7hen", "und", "Bild\u00b7werk", ",", "Flit\u00b7ter", "und", "Spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "gl\u00e4serne Ampeln im Jugendstil,", "tokens": ["gl\u00e4\u00b7ser\u00b7ne", "Am\u00b7peln", "im", "Ju\u00b7gend\u00b7stil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "steinerne Engel im Modekleid,", "tokens": ["stei\u00b7ner\u00b7ne", "En\u00b7gel", "im", "Mo\u00b7de\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Platte an Platte dicht gereiht \u2013", "tokens": ["Plat\u00b7te", "an", "Plat\u00b7te", "dicht", "ge\u00b7reiht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "VVPP", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "und um des marmornen Schweigens Schauer", "tokens": ["und", "um", "des", "mar\u00b7mor\u00b7nen", "Schwei\u00b7gens", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NE"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "die himmelversperrende, neidische Mauer:", "tokens": ["die", "him\u00b7mel\u00b7ver\u00b7sper\u00b7ren\u00b7de", ",", "nei\u00b7di\u00b7sche", "Mau\u00b7er", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "das ist, von Orangen und Rosen umbl\u00fcht", "tokens": ["das", "ist", ",", "von", "O\u00b7ran\u00b7gen", "und", "Ro\u00b7sen", "um\u00b7bl\u00fcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "die Heimstatt der Toten im sonnigen S\u00fcd.", "tokens": ["die", "Heims\u00b7tatt", "der", "To\u00b7ten", "im", "son\u00b7ni\u00b7gen", "S\u00fcd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "In der eisigen Oede bin ich allein.", "tokens": ["In", "der", "ei\u00b7si\u00b7gen", "O\u00b7e\u00b7de", "bin", "ich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hart klingt mein Schritt auf dem harten Stein.", "tokens": ["Hart", "klingt", "mein", "Schritt", "auf", "dem", "har\u00b7ten", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und die Tr\u00e4ne, die mir so rasch den Blick", "tokens": ["Und", "die", "Tr\u00e4\u00b7ne", ",", "die", "mir", "so", "rasch", "den", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "verdunkelt, kriecht scheu ins Herz zur\u00fcck.", "tokens": ["ver\u00b7dun\u00b7kelt", ",", "kriecht", "scheu", "ins", "Herz", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein Windsto\u00df kommt aus dem Pinienhain,", "tokens": ["Ein", "Wind\u00b7sto\u00df", "kommt", "aus", "dem", "Pi\u00b7ni\u00b7en\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und die Kr\u00e4nze klappern wie Totenbein . . .", "tokens": ["und", "die", "Kr\u00e4n\u00b7ze", "klap\u00b7pern", "wie", "To\u00b7ten\u00b7bein", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "NN", "$.", "$.", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "O du Ewige, Weltenbeschattende du,", "tokens": ["O", "du", "E\u00b7wi\u00b7ge", ",", "Wel\u00b7ten\u00b7be\u00b7schat\u00b7ten\u00b7de", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "PWS", "PPER", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Mutter des Lebens, zeugende Ruh,", "tokens": ["Mut\u00b7ter", "des", "Le\u00b7bens", ",", "zeu\u00b7gen\u00b7de", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "wie haben sie dich so klein gemacht,", "tokens": ["wie", "ha\u00b7ben", "sie", "dich", "so", "klein", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "mit ihrer Plunder- und Flickenpracht!", "tokens": ["mit", "ih\u00b7rer", "Plun\u00b7der", "und", "Fli\u00b7cken\u00b7pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sie spielten ein gellendes Jahrmarktsst\u00fcck", "tokens": ["Sie", "spiel\u00b7ten", "ein", "gel\u00b7len\u00b7des", "Jahr\u00b7markts\u00b7st\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "auf deiner heitern himmlischen Harfe", "tokens": ["auf", "dei\u00b7ner", "hei\u00b7tern", "himm\u00b7li\u00b7schen", "Har\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "und h\u00e4ngen die grinsende Faschingslarve", "tokens": ["und", "h\u00e4n\u00b7gen", "die", "grin\u00b7sen\u00b7de", "Fa\u00b7schings\u00b7lar\u00b7ve"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "vor deinen g\u00fctigen Mutterblick.", "tokens": ["vor", "dei\u00b7nen", "g\u00fc\u00b7ti\u00b7gen", "Mut\u00b7ter\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Nein, meine Heilige, hier wohnst du nicht!", "tokens": ["Nein", ",", "mei\u00b7ne", "Hei\u00b7li\u00b7ge", ",", "hier", "wohnst", "du", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Aufatmend gr\u00fc\u00df ich das Fr\u00fchlingslicht!", "tokens": ["Auf\u00b7at\u00b7mend", "gr\u00fc\u00df", "ich", "das", "Fr\u00fch\u00b7lings\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}