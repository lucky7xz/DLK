{"textgrid.poem.60617": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Wolf war nichts als Haut und Knochen,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wolf war nichts als Haut und Knochen,", "tokens": ["Ein", "Wolf", "war", "nichts", "als", "Haut", "und", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die treuen Hunde waren schuld daran.", "tokens": ["Die", "treu\u00b7en", "Hun\u00b7de", "wa\u00b7ren", "schuld", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie er nun einst so matt des Wegs gekrochen,", "tokens": ["Wie", "er", "nun", "einst", "so", "matt", "des", "Wegs", "ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Traf er die sch\u00f6nste, st\u00e4rkste Dogge an,", "tokens": ["Traf", "er", "die", "sch\u00f6ns\u00b7te", ",", "st\u00e4rks\u00b7te", "Dog\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die sich vom Herrenhof verlaufen hatte.", "tokens": ["Die", "sich", "vom", "Her\u00b7ren\u00b7hof", "ver\u00b7lau\u00b7fen", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Hund war solch ein fester, wohlgen\u00e4hrter Klotz,", "tokens": ["Der", "Hund", "war", "solch", "ein", "fes\u00b7ter", ",", "wohl\u00b7ge\u00b7n\u00e4hr\u00b7ter", "Klotz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df neben ihm der Wolf nur eine hagre Latte.", "tokens": ["Da\u00df", "ne\u00b7ben", "ihm", "der", "Wolf", "nur", "ei\u00b7ne", "hag\u00b7re", "Lat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So gern der's auch getan, so schien's ihm leider Gotts", "tokens": ["So", "gern", "der's", "auch", "ge\u00b7tan", ",", "so", "schien's", "ihm", "lei\u00b7der", "Gotts"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "H\u00f6chst ungeraten, diesen Burschen anzuspringen,", "tokens": ["H\u00f6chst", "un\u00b7ge\u00b7ra\u00b7ten", ",", "die\u00b7sen", "Bur\u00b7schen", "an\u00b7zu\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "PDAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn solch ein Gegner war so leicht nicht zu verschlingen.", "tokens": ["Denn", "solch", "ein", "Geg\u00b7ner", "war", "so", "leicht", "nicht", "zu", "ver\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "VAFIN", "ADV", "ADJD", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So also sprach voll Demut unser Wolf ihn an", "tokens": ["So", "al\u00b7so", "sprach", "voll", "De\u00b7mut", "un\u00b7ser", "Wolf", "ihn", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADJD", "NN", "PPOSAT", "NE", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit Komplimenten \u00fcber seine Wohlgestalt.", "tokens": ["Mit", "Kom\u00b7pli\u00b7men\u00b7ten", "\u00fc\u00b7ber", "sei\u00b7ne", "Wohl\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Da sprach der Hund: \u00bbMein sch\u00f6ner Herr, liegt Euch daran,", "tokens": ["Da", "sprach", "der", "Hund", ":", "\u00bb", "Mein", "sch\u00f6\u00b7ner", "Herr", ",", "liegt", "Euch", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So fett zu sein wie ich, nun, so verla\u00dft den Wald,", "tokens": ["So", "fett", "zu", "sein", "wie", "ich", ",", "nun", ",", "so", "ver\u00b7la\u00dft", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF", "KOKOM", "PPER", "$,", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In dem nur arme Schlucker lungern.", "tokens": ["In", "dem", "nur", "ar\u00b7me", "Schlu\u00b7cker", "lun\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ihr lebt ja nur, um zu verhungern,", "tokens": ["Ihr", "lebt", "ja", "nur", ",", "um", "zu", "ver\u00b7hun\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Habt Tag und Nacht nicht Ruh und nichts zu schnabulieren;", "tokens": ["Habt", "Tag", "und", "Nacht", "nicht", "Ruh", "und", "nichts", "zu", "schna\u00b7bu\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKNEG", "NN", "KON", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Folgt mir, Ihr werdet ein vergn\u00fcgtres Leben f\u00fchren.\u00ab", "tokens": ["Folgt", "mir", ",", "Ihr", "wer\u00b7det", "ein", "ver\u00b7gn\u00fcg\u00b7tres", "Le\u00b7ben", "f\u00fch\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da sprach der Wolf: \u00bbWas h\u00e4tte ich daf\u00fcr zu leisten?\u00ab", "tokens": ["Da", "sprach", "der", "Wolf", ":", "\u00bb", "Was", "h\u00e4t\u00b7te", "ich", "da\u00b7f\u00fcr", "zu", "leis\u00b7ten", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$.", "$(", "PWS", "VAFIN", "PPER", "PAV", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Hund: \u00bbFast nichts! Nur Leute zu verjagen,", "tokens": ["Der", "Hund", ":", "\u00bb", "Fast", "nichts", "!", "Nur", "Leu\u00b7te", "zu", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ADV", "PIS", "$.", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die Bettels\u00e4cke oder St\u00f6cke tragen,", "tokens": ["Die", "Bet\u00b7tel\u00b7s\u00e4\u00b7cke", "o\u00b7der", "St\u00f6\u00b7cke", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Dem Hausgesind zu schmeicheln, und am meisten", "tokens": ["Dem", "Haus\u00b7ge\u00b7sind", "zu", "schmei\u00b7cheln", ",", "und", "am", "meis\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Dem Herrn. Als Sold bekommt Ihr sch\u00f6ne Rester,", "tokens": ["Dem", "Herrn", ".", "Als", "Sold", "be\u00b7kommt", "Ihr", "sch\u00f6\u00b7ne", "Res\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KOUS", "NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "H\u00fchner- und Taubenknochen \u2013 ja, mein Bester! \u2013", "tokens": ["H\u00fch\u00b7ner", "und", "Tau\u00b7ben\u00b7kno\u00b7chen", "\u2013", "ja", ",", "mein", "Bes\u00b7ter", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["TRUNC", "KON", "NN", "$(", "PTKANT", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.25": {"text": "Und manches Kosew\u00f6rtchen obendrein.\u00ab", "tokens": ["Und", "man\u00b7ches", "Ko\u00b7se\u00b7w\u00f6rt\u00b7chen", "o\u00b7ben\u00b7drein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Der Wolf glaubt schon im Paradies zu sein.", "tokens": ["Der", "Wolf", "glaubt", "schon", "im", "Pa\u00b7ra\u00b7dies", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "APPRART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Er weint vor Gl\u00fcck und will den Hund begleiten.", "tokens": ["Er", "weint", "vor", "Gl\u00fcck", "und", "will", "den", "Hund", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Da sieht am Hundehals er eine Stell,", "tokens": ["Da", "sieht", "am", "Hun\u00b7de\u00b7hals", "er", "ei\u00b7ne", "Stell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "\u00bbwas ist das?\u00ab fragt er. \u2013 \u00bbNichts!\u00ab \u2013 \u00bbWieso?\u00ab \u2013 \u00bbAch, Kleinigkeiten!\u00ab \u2013", "tokens": ["\u00bb", "was", "ist", "das", "?", "\u00ab", "fragt", "er", ".", "\u2013", "\u00bb", "Nichts", "!", "\u00ab", "\u2013", "\u00bb", "Wie\u00b7so", "?", "\u00ab", "\u2013", "\u00bb", "Ach", ",", "Klei\u00b7nig\u00b7kei\u00b7ten", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PDS", "$.", "$(", "VVFIN", "PPER", "$.", "$(", "$(", "PIS", "$.", "$(", "$(", "$(", "PWAV", "$.", "$(", "$(", "$(", "ITJ", "$,", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "\u00bbnun was denn?\u00ab \u2013 Drauf der Hund:", "tokens": ["\u00bb", "nun", "was", "denn", "?", "\u00ab", "\u2013", "Drauf", "der", "Hund", ":"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PWS", "ADV", "$.", "$(", "$(", "PAV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.31": {"text": "\u00bbdas Halsband meiner Kette rieb mich wund.\u00ab \u2013", "tokens": ["\u00bb", "das", "Hals\u00b7band", "mei\u00b7ner", "Ket\u00b7te", "rieb", "mich", "wund", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "\u00bbwie? Was? In Ketten dienet Ihr?", "tokens": ["\u00bb", "wie", "?", "Was", "?", "In", "Ket\u00b7ten", "die\u00b7net", "Ihr", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "PWS", "$.", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Lauft nicht, wohin Ihr wollt?\u00ab \u2013", "tokens": ["Lauft", "nicht", ",", "wo\u00b7hin", "Ihr", "wollt", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VMFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "\u00bbnicht immer. Doch was tut's?\u00ab \u2013 \u00bbEs tut so viel, da\u00df mir", "tokens": ["\u00bb", "nicht", "im\u00b7mer", ".", "Doch", "was", "tut's", "?", "\u00ab", "\u2013", "\u00bb", "Es", "tut", "so", "viel", ",", "da\u00df", "mir"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PTKNEG", "ADV", "$.", "KON", "PWS", "NE", "$.", "$(", "$(", "$(", "PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Lust vergeht nach Eurem sch\u00f6nen Sold.", "tokens": ["Die", "Lust", "ver\u00b7geht", "nach", "Eu\u00b7rem", "sch\u00f6\u00b7nen", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Ich ging nicht mit um eine ganze Kuh!\u00ab", "tokens": ["Ich", "ging", "nicht", "mit", "um", "ei\u00b7ne", "gan\u00b7ze", "Kuh", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Und Meister Wolf hat sich getrollt", "tokens": ["Und", "Meis\u00b7ter", "Wolf", "hat", "sich", "get\u00b7rollt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "VAFIN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Und l\u00e4uft noch immerzu.", "tokens": ["Und", "l\u00e4uft", "noch", "im\u00b7mer\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}