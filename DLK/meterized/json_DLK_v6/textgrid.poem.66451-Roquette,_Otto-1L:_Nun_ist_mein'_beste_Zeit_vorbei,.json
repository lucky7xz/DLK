{"textgrid.poem.66451": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun ist mein' beste Zeit vorbei,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist mein' beste Zeit vorbei,", "tokens": ["Nun", "ist", "mein'", "bes\u00b7te", "Zeit", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun ist mir Alles einerlei", "tokens": ["Nun", "ist", "mir", "Al\u00b7les", "ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohin ich wandern soll.", "tokens": ["Wo\u00b7hin", "ich", "wan\u00b7dern", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verlassen mu\u00df ich meine Lust,", "tokens": ["Ver\u00b7las\u00b7sen", "mu\u00df", "ich", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein ganzes Herz ist in der Brust", "tokens": ["Mein", "gan\u00b7zes", "Herz", "ist", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Thr\u00e4nen, von Thr\u00e4nen voll!", "tokens": ["Von", "Thr\u00e4\u00b7nen", ",", "von", "Thr\u00e4\u00b7nen", "voll", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Durch die alten Gassen hab ich zuletzt", "tokens": ["Durch", "die", "al\u00b7ten", "Gas\u00b7sen", "hab", "ich", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Heut Nacht meinen Wanderstab gesetzt,", "tokens": ["Heut", "Nacht", "mei\u00b7nen", "Wan\u00b7der\u00b7stab", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit manchem Gesellen gut.", "tokens": ["Mit", "man\u00b7chem", "Ge\u00b7sel\u00b7len", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie dr\u00fcckten mir alle die Bruderhand:", "tokens": ["Sie", "dr\u00fcck\u00b7ten", "mir", "al\u00b7le", "die", "Bru\u00b7der\u00b7hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und denk' an uns im fremden Land,", "tokens": ["Und", "denk'", "an", "uns", "im", "frem\u00b7den", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Halt' uns in treuer Hut!", "tokens": ["Halt'", "uns", "in", "treu\u00b7er", "Hut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Noch Einmal von der Neckarbr\u00fcck'", "tokens": ["Noch", "Ein\u00b7mal", "von", "der", "Ne\u00b7cka\u00b7rbr\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schau ich in's weite Thal zur\u00fcck,", "tokens": ["Schau", "ich", "in's", "wei\u00b7te", "Thal", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wasser rauschten daher,", "tokens": ["Die", "Was\u00b7ser", "rauschten", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie rauschten stets, ich merkt' es kaum,", "tokens": ["Sie", "rauschten", "stets", ",", "ich", "merkt'", "es", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie rauschen und singen mir alten Traum,", "tokens": ["Sie", "rau\u00b7schen", "und", "sin\u00b7gen", "mir", "al\u00b7ten", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und machen das Herz mir schwer.", "tokens": ["Und", "ma\u00b7chen", "das", "Herz", "mir", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich sah nach jedem Giebeldach,", "tokens": ["Ich", "sah", "nach", "je\u00b7dem", "Gie\u00b7bel\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir war's, als riefen sie mir nach:", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "rie\u00b7fen", "sie", "mir", "nach", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fahr wohl, Gesell, fahr wohl!", "tokens": ["Fahr", "wohl", ",", "Ge\u00b7sell", ",", "fahr", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und mit dem Abschied war's vorbei,", "tokens": ["Und", "mit", "dem", "Ab\u00b7schied", "wa\u00b7r's", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Nun ist mir Alles einerlei", "tokens": ["Nun", "ist", "mir", "Al\u00b7les", "ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohin ich wandern soll!", "tokens": ["Wo\u00b7hin", "ich", "wan\u00b7dern", "soll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}