{"dta.poem.19823": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Klingenberg am Maine,               ", "tokens": ["Zu", "Klin\u00b7gen\u00b7berg", "am", "Mai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu W\u00fcrzburg an dem Steine,", "tokens": ["Zu", "W\u00fcrz\u00b7burg", "an", "dem", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu Bacherach am Rhein", "tokens": ["Zu", "Ba\u00b7che\u00b7rach", "am", "Rhein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hab ich in meinen Tagen", "tokens": ["Hab", "ich", "in", "mei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gar oftmals h\u00f6ren sagen,", "tokens": ["Gar", "oft\u00b7mals", "h\u00f6\u00b7ren", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Soll'n sein die besten Wein.", "tokens": ["Soll'n", "sein", "die", "bes\u00b7ten", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jung schenk mir ein", "tokens": ["Jung", "schenk", "mir", "e