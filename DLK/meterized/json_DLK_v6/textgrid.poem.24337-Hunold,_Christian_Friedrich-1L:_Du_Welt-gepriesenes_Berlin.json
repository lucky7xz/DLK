{"textgrid.poem.24337": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Welt-gepriesenes Berlin/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Welt-gepriesenes Berlin/", "tokens": ["Du", "Welt\u00b7ge\u00b7prie\u00b7se\u00b7nes", "Ber\u00b7lin", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erlauchter Sitz der Herrlichkeiten!", "tokens": ["Er\u00b7lauch\u00b7ter", "Sitz", "der", "Herr\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Glantz ist allen vorzuziehn/", "tokens": ["Dein", "Glantz", "ist", "al\u00b7len", "vor\u00b7zu\u00b7ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deinem Gl\u00fccke nichts zur Seiten.", "tokens": ["Und", "dei\u00b7nem", "Gl\u00fc\u00b7cke", "nichts", "zur", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Freude stimmt auch heute bey:", "tokens": ["Die", "Freu\u00b7de", "stimmt", "auch", "heu\u00b7te", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df alles unvergleichlich sey.", "tokens": ["Da\u00df", "al\u00b7les", "un\u00b7ver\u00b7gleich\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Thron ist Salomonis Thron/", "tokens": ["Dein", "Thron", "ist", "Sa\u00b7lo\u00b7mo\u00b7nis", "Thron", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein K\u00f6nig jenem gleich erlesen;", "tokens": ["Dein", "K\u00f6\u00b7nig", "je\u00b7nem", "gleich", "er\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur da\u00df er gl\u00fccklicher im Sohn", "tokens": ["Nur", "da\u00df", "er", "gl\u00fcck\u00b7li\u00b7cher", "im", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ehmahls Salomo gewesen:", "tokens": ["Als", "eh\u00b7mahls", "Sa\u00b7lo\u00b7mo", "ge\u00b7we\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Voraus da dieser Sohn anitzt/", "tokens": ["Vo\u00b7raus", "da", "die\u00b7ser", "Sohn", "a\u00b7nitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Thron mit einem Erben st\u00fctzt.", "tokens": ["Den", "Thron", "mit", "ei\u00b7nem", "Er\u00b7ben", "st\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In Adlers Blut ist Krafft und Geist/", "tokens": ["In", "Ad\u00b7lers", "Blut", "ist", "Krafft", "und", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn solches steiget zu den Sternen/", "tokens": ["Denn", "sol\u00b7ches", "stei\u00b7get", "zu", "den", "Ster\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie es dein Cron-Printz dir verheist/", "tokens": ["Wie", "es", "dein", "Cron\u00b7Printz", "dir", "ver\u00b7heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der seinem Vater folgen lernen:", "tokens": ["Der", "sei\u00b7nem", "Va\u00b7ter", "fol\u00b7gen", "ler\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und der/ nach welchem Er sich nennt/", "tokens": ["Und", "der", "/", "nach", "wel\u00b7chem", "Er", "sich", "nennt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Friedrich Wilhelms Tugend brennt.", "tokens": ["Von", "Fried\u00b7rich", "Wil\u00b7helms", "Tu\u00b7gend", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du bist der gro\u00dfe K\u00f6nigs-Sitz/", "tokens": ["Du", "bist", "der", "gro\u00b7\u00dfe", "K\u00f6\u00b7nigs\u00b7Sitz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wei\u00dfheit wohnt in deinen Gassen.", "tokens": ["Die", "Wei\u00df\u00b7heit", "wohnt", "in", "dei\u00b7nen", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein/ wei\u00df deiner Redner Witz/", "tokens": ["Al\u00b7lein", "/", "wei\u00df", "dei\u00b7ner", "Red\u00b7ner", "Witz", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Gl\u00fcck in Worten abzufassen/", "tokens": ["Dein", "Gl\u00fcck", "in", "Wor\u00b7ten", "ab\u00b7zu\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Gl\u00fcck das sich/ wie sehr es bl\u00fcht/", "tokens": ["Dein", "Gl\u00fcck", "das", "sich", "/", "wie", "sehr", "es", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PRF", "$(", "KOKOM", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf k\u00fcnfftig schon befestigt sieht?", "tokens": ["Auf", "k\u00fcnff\u00b7tig", "schon", "be\u00b7fes\u00b7tigt", "sieht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gewi\u00df der Himmel der dich liebt/", "tokens": ["Ge\u00b7wi\u00df", "der", "Him\u00b7mel", "der", "dich", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dir in deines K\u00f6nigs Leben/", "tokens": ["Und", "dir", "in", "dei\u00b7nes", "K\u00f6\u00b7nigs", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So gro\u00dfe Gnaden-Zeichen giebt/", "tokens": ["So", "gro\u00b7\u00dfe", "Gna\u00b7den\u00b7Zei\u00b7chen", "giebt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will Ihm itzt einen Enckel geben:", "tokens": ["Will", "Ihm", "itzt", "ei\u00b7nen", "En\u00b7ckel", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Damit Sein Reich/ das ungemein/", "tokens": ["Da\u00b7mit", "Sein", "Reich", "/", "das", "un\u00b7ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$(", "PDS", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zugleich unendlich m\u00f6ge seyn.", "tokens": ["Zu\u00b7gleich", "un\u00b7end\u00b7lich", "m\u00f6\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Betrachte deinen Zustand nur/", "tokens": ["Be\u00b7trach\u00b7te", "dei\u00b7nen", "Zu\u00b7stand", "nur", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sieh/ wie manche H\u00e4user sterben;", "tokens": ["Und", "sieh", "/", "wie", "man\u00b7che", "H\u00e4u\u00b7ser", "ster\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$(", "KOKOM", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da dir hingegen deine Chur", "tokens": ["Da", "dir", "hin\u00b7ge\u00b7gen", "dei\u00b7ne", "Chur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat Kronen wissen zu erwerben.", "tokens": ["Hat", "Kro\u00b7nen", "wis\u00b7sen", "zu", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da Friedrich deinen Thron erh\u00f6ht;", "tokens": ["Da", "Fried\u00b7rich", "dei\u00b7nen", "Thron", "er\u00b7h\u00f6ht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Inde\u00df da\u00df mancher untergeht.", "tokens": ["In\u00b7de\u00df", "da\u00df", "man\u00b7cher", "un\u00b7ter\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja da viel unbeerbet sind/", "tokens": ["Ja", "da", "viel", "un\u00b7be\u00b7er\u00b7bet", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4st Gott dir Lebens-Erben schauen.", "tokens": ["L\u00e4st", "Gott", "dir", "Le\u00b7bens\u00b7Er\u00b7ben", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Friedrich siehet Kindes-Kind/", "tokens": ["Dein", "Fried\u00b7rich", "sie\u00b7het", "Kin\u00b7des\u00b7Kind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Er gebauet/ fort zu bauen.", "tokens": ["Was", "Er", "ge\u00b7bau\u00b7et", "/", "fort", "zu", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Worinnen Ihm kein K\u00f6nig gleich;", "tokens": ["Wo\u00b7rin\u00b7nen", "Ihm", "kein", "K\u00f6\u00b7nig", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als der nur aus dem Liljen Reich.", "tokens": ["Als", "der", "nur", "aus", "dem", "Lil\u00b7jen", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "O komm/ und eile zum Altar/", "tokens": ["O", "komm", "/", "und", "ei\u00b7le", "zum", "Al\u00b7tar", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "KON", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem H\u00f6chsten dafar Danck zu sagen!", "tokens": ["Dem", "H\u00f6chs\u00b7ten", "da\u00b7far", "Danck", "zu", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der mach' auch allen Gl\u00fcckwunsch wahr/", "tokens": ["Der", "mach'", "auch", "al\u00b7len", "Gl\u00fcck\u00b7wunsch", "wahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den wir f\u00fcr dich im Hertzen tragen!", "tokens": ["Den", "wir", "f\u00fcr", "dich", "im", "Hert\u00b7zen", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein K\u00f6nig/ Cron-Printz und sein Hau\u00df.", "tokens": ["Dein", "K\u00f6\u00b7nig", "/", "Cron\u00b7Printz", "und", "sein", "Hau\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NE", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Sey gro\u00df/ und sterbe nimmer aus.", "tokens": ["Sey", "gro\u00df", "/", "und", "ster\u00b7be", "nim\u00b7mer", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$(", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}