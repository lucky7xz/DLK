{"textgrid.poem.57073": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1. Fr\u00fcchte der Bildung", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schr\u00e4nke \u00f6ffnen sich allein,", "tokens": ["Schr\u00e4n\u00b7ke", "\u00f6ff\u00b7nen", "sich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schr\u00e4nke klaffen auf und spein", "tokens": ["Schr\u00e4n\u00b7ke", "klaf\u00b7fen", "auf", "und", "spein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00e4cke, Hosen aus und Kleider,", "tokens": ["Fr\u00e4\u00b7cke", ",", "Ho\u00b7sen", "aus", "und", "Klei\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PTKVZ", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nebst den Attributen beider.", "tokens": ["nebst", "den", "At\u00b7tri\u00b7bu\u00b7ten", "bei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sie wandeln in den Raum,", "tokens": ["Und", "sie", "wan\u00b7deln", "in", "den", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wie ein sonderbarer Traum,", "tokens": ["wie", "ein", "son\u00b7der\u00b7ba\u00b7rer", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wehen hin und her und schreiten", "tokens": ["we\u00b7hen", "hin", "und", "her", "und", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ganz wie zu benutzten Zeiten.", "tokens": ["ganz", "wie", "zu", "be\u00b7nutz\u00b7ten", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf den Sofas, auf den Truhn", "tokens": ["Auf", "den", "So\u00b7fas", ",", "auf", "den", "Truhn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "sieht man sitzen sie und ruhn,", "tokens": ["sieht", "man", "sit\u00b7zen", "sie", "und", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "PPER", "KON", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "auf den Sesseln, an den Tischen", "tokens": ["auf", "den", "Ses\u00b7seln", ",", "an", "den", "Ti\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "am Kamin und in den Nischen.", "tokens": ["am", "Ka\u00b7min", "und", "in", "den", "Ni\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seltsam sind sie anzuschaun,", "tokens": ["Selt\u00b7sam", "sind", "sie", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "kopflos, handlos, M\u00e4nner, Fraun;", "tokens": ["kopf\u00b7los", ",", "hand\u00b7los", ",", "M\u00e4n\u00b7ner", ",", "Fraun", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "NN", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "doch mit Recht verwundert jeden,", "tokens": ["doch", "mit", "Recht", "ver\u00b7wun\u00b7dert", "je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df sie nicht ein W\u00f6rtlein reden.", "tokens": ["da\u00df", "sie", "nicht", "ein", "W\u00f6rt\u00b7lein", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dieser Frack und jener Rock,", "tokens": ["Die\u00b7ser", "Frack", "und", "je\u00b7ner", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "beide schweigen wie ein Stock,", "tokens": ["bei\u00b7de", "schwei\u00b7gen", "wie", "ein", "Stock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "lehnen ab, wie einst im M\u00e4rchen,", "tokens": ["leh\u00b7nen", "ab", ",", "wie", "einst", "im", "M\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sich zu rufen Franz und Kl\u00e4rchen.", "tokens": ["sich", "zu", "ru\u00b7fen", "Franz", "und", "Kl\u00e4r\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "NE", "KON", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ohne Mund entsteht kein Ton,", "tokens": ["Oh\u00b7ne", "Mund", "ent\u00b7steht", "kein", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "lernten sie als Kinder schon:", "tokens": ["lern\u00b7ten", "sie", "als", "Kin\u00b7der", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so reden Wams und Weste", "tokens": ["Und", "so", "re\u00b7den", "Wams", "und", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "lediglich in stummer Geste.", "tokens": ["le\u00b7di\u00b7glich", "in", "stum\u00b7mer", "Ges\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Uhr schl\u00e4gt's, die Schr\u00e4nke schrein:", "tokens": ["Ein", "Uhr", "schl\u00e4gt's", ",", "die", "Schr\u00e4n\u00b7ke", "schrein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kommt, und m\u00f6g euch Gott verzeihn!", "tokens": ["Kommt", ",", "und", "m\u00f6g", "euch", "Gott", "ver\u00b7zeihn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Krachend fliegen zu die Fl\u00fcgel,", "tokens": ["Kra\u00b7chend", "flie\u00b7gen", "zu", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und \u2013 nur eins h\u00e4ngt nicht am B\u00fcgel!", "tokens": ["und", "\u2013", "nur", "eins", "h\u00e4ngt", "nicht", "am", "B\u00fc\u00b7gel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "PIS", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}}}}}