{"textgrid.poem.46544": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[und soll ich nicht der Sitte fluchen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und soll ich nicht der Sitte fluchen", "tokens": ["Und", "soll", "ich", "nicht", "der", "Sit\u00b7te", "flu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Fest zu feiern beim Begraben?", "tokens": ["Ein", "Fest", "zu", "fei\u00b7ern", "beim", "Be\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man b\u00e4ckt im Hause Mandelkuchen,", "tokens": ["Man", "b\u00e4ckt", "im", "Hau\u00b7se", "Man\u00b7del\u00b7ku\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil wir der Tochter Leiche haben,", "tokens": ["Weil", "wir", "der", "Toch\u00b7ter", "Lei\u00b7che", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ofenwarm l\u00e4\u00dft ihn versuchen", "tokens": ["Und", "o\u00b7fen\u00b7warm", "l\u00e4\u00dft", "ihn", "ver\u00b7su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Leichenfrau den kranken Knaben;", "tokens": ["Die", "Lei\u00b7chen\u00b7frau", "den", "kran\u00b7ken", "Kna\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er soll wohl auch den Ort besuchen,", "tokens": ["Er", "soll", "wohl", "auch", "den", "Ort", "be\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den sie der armen Schwester gaben!", "tokens": ["Den", "sie", "der", "ar\u00b7men", "Schwes\u00b7ter", "ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und soll ich nicht der Sitte fluchen", "tokens": ["Und", "soll", "ich", "nicht", "der", "Sit\u00b7te", "flu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Fest zu feiern beim Begraben?", "tokens": ["Ein", "Fest", "zu", "fei\u00b7ern", "beim", "Be\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}