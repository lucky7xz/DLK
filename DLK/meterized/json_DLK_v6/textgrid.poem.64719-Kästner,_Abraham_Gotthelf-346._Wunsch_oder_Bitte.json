{"textgrid.poem.64719": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "346. Wunsch oder Bitte", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gern s\u00e4h' ich, da\u00df er bey uns blieb';", "tokens": ["Gern", "s\u00e4h'", "ich", ",", "da\u00df", "er", "bey", "uns", "blieb'", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch w\u00e4r er ja nicht zu bewegen:", "tokens": ["Doch", "w\u00e4r", "er", "ja", "nicht", "zu", "be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So schreib' er ein Recept f\u00fcr seine Herrn Collegen,", "tokens": ["So", "schreib'", "er", "ein", "Re\u00b7cept", "f\u00fcr", "sei\u00b7ne", "Herrn", "Col\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie das sein Ahnherr einst ", "tokens": ["Wie", "das", "sein", "Ahn\u00b7herr", "einst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}