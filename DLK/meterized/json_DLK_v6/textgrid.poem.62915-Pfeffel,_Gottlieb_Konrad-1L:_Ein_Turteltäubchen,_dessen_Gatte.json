{"textgrid.poem.62915": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Turtelt\u00e4ubchen, dessen Gatte", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Turtelt\u00e4ubchen, dessen Gatte", "tokens": ["Ein", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", ",", "des\u00b7sen", "Gat\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch eines J\u00e4gers Tyranney", "tokens": ["Durch", "ei\u00b7nes", "J\u00e4\u00b7gers", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den fr\u00fchen Tod gefunden hatte,", "tokens": ["Den", "fr\u00fc\u00b7hen", "Tod", "ge\u00b7fun\u00b7den", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zog durch sein banges Klaggeschrey", "tokens": ["Zog", "durch", "sein", "ban\u00b7ges", "Klag\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom nahgelegnen Rittersitze", "tokens": ["Vom", "nah\u00b7ge\u00b7leg\u00b7nen", "Rit\u00b7ter\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Papagey des Junkers Fritze", "tokens": ["Den", "Pa\u00b7pa\u00b7gey", "des", "Jun\u00b7kers", "Frit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aus seinem weichen Nest herbey.", "tokens": ["Aus", "sei\u00b7nem", "wei\u00b7chen", "Nest", "her\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Kind, h\u00f6r auf, dich so zu gr\u00e4men,", "tokens": ["Mein", "Kind", ",", "h\u00f6r", "auf", ",", "dich", "so", "zu", "gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sprach er in gnadenreichem Ton,", "tokens": ["Sprach", "er", "in", "gna\u00b7den\u00b7rei\u00b7chem", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich will in meinen Schutz dich nehmen,", "tokens": ["Ich", "will", "in", "mei\u00b7nen", "Schutz", "dich", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und, hier gesagt, beym Herrn Baron", "tokens": ["Und", ",", "hier", "ge\u00b7sagt", ",", "beym", "Herrn", "Ba\u00b7ron"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "VVPP", "$,", "APPRART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Gelt ich weit mehr, als Frau und Sohn:", "tokens": ["Gelt", "ich", "weit", "mehr", ",", "als", "Frau", "und", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Mein Rang ist gleich nach der Maitresse;", "tokens": ["Mein", "Rang", "ist", "gleich", "nach", "der", "Mai\u00b7tres\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Ein Z\u00f6fchen dient mir, wenn ich esse,", "tokens": ["Ein", "Z\u00f6f\u00b7chen", "dient", "mir", ",", "wenn", "ich", "es\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und mein Gebauer ist ein Thron.", "tokens": ["Und", "mein", "Ge\u00b7bau\u00b7er", "ist", "ein", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Komm, Freundin, ihn mit mir zu theilen,", "tokens": ["Komm", ",", "Freun\u00b7din", ",", "ihn", "mit", "mir", "zu", "thei\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Verla\u00df das d\u00fcstre Land der Eulen,", "tokens": ["Ver\u00b7la\u00df", "das", "d\u00fcst\u00b7re", "Land", "der", "Eu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sey gutes Muths und sch\u00e4me dich,", "tokens": ["Sey", "gu\u00b7tes", "Muths", "und", "sch\u00e4\u00b7me", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mein Kind, so deutsch, so b\u00fcrgerlich,", "tokens": ["Mein", "Kind", ",", "so", "deutsch", ",", "so", "b\u00fcr\u00b7ger\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Um nichts \u2013 um einen Mann zu heulen.", "tokens": ["Um", "nichts", "\u2013", "um", "ei\u00b7nen", "Mann", "zu", "heu\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "$(", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Das T\u00e4ubchen sprach: ich danke dir,", "tokens": ["Das", "T\u00e4ub\u00b7chen", "sprach", ":", "ich", "dan\u00b7ke", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der Schmerz ist nun mein Loos auf Erden", "tokens": ["Der", "Schmerz", "ist", "nun", "mein", "Loos", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und du \u2013 du bist, vergieb es mir,", "tokens": ["Und", "du", "\u2013", "du", "bist", ",", "ver\u00b7gieb", "es", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Zu gl\u00fccklich, um mein Freund zu werden.", "tokens": ["Zu", "gl\u00fcck\u00b7lich", ",", "um", "mein", "Freund", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}