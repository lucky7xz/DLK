{"textgrid.poem.58350": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "388. Die seligste Heimat", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Gottheit reines, stilles Wesen", "tokens": ["Der", "Got\u00b7theit", "rei\u00b7nes", ",", "stil\u00b7les", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich zur Wohnung mir erlesen,", "tokens": ["Hab'", "ich", "zur", "Woh\u00b7nung", "mir", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da, da geh\u00f6rt mein Geist zu Haus,", "tokens": ["Da", ",", "da", "ge\u00b7h\u00f6rt", "mein", "Geist", "zu", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da wird mir Kraft und Licht und Leben", "tokens": ["Da", "wird", "mir", "Kraft", "und", "Licht", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und volle S\u00e4ttigung gegeben;", "tokens": ["Und", "vol\u00b7le", "S\u00e4t\u00b7ti\u00b7gung", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, da\u00df ich nimmer kehrte draus!", "tokens": ["Ach", ",", "da\u00df", "ich", "nim\u00b7mer", "kehr\u00b7te", "draus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}