{"dta.poem.7716": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df vier Wissende, ein f\u00fcnfter geht mit drein;", "tokens": ["Ich", "wei\u00df", "vier", "Wis\u00b7sen\u00b7de", ",", "ein", "f\u00fcnf\u00b7ter", "geht", "mit", "drein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "$,", "ART", "ADJA", "VVFIN", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die viere wissen nichts, der f\u00fcnfte wei\u00df allein.", "tokens": ["Die", "vie\u00b7re", "wis\u00b7sen", "nichts", ",", "der", "f\u00fcnf\u00b7te", "wei\u00df", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PIS", "$,", "ART", "ADJA", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der eine wei\u00df zum Ruhm, der andre zum Genu\u00df,", "tokens": ["Der", "ei\u00b7ne", "wei\u00df", "zum", "Ruhm", ",", "der", "and\u00b7re", "zum", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "$,", "PRELS", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dritte zum Erwerb, der vierte zum Verdru\u00df.", "tokens": ["Der", "drit\u00b7te", "zum", "Er\u00b7werb", ",", "der", "vier\u00b7te", "zum", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$,", "ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der f\u00fcnfte wei\u00df nicht, was, woher, wozu ers wei\u00df,", "tokens": ["Der", "f\u00fcnf\u00b7te", "wei\u00df", "nicht", ",", "was", ",", "wo\u00b7her", ",", "wo\u00b7zu", "ers", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "$,", "PRELS", "$,", "PWAV", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stralt W\u00e4rm' aus wie die Sonn', und wird ihm selbst nicht hei\u00df.", "tokens": ["Stralt", "W\u00e4rm'", "aus", "wie", "die", "Sonn'", ",", "und", "wird", "ihm", "selbst", "nicht", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "KOKOM", "ART", "NN", "$,", "KON", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}