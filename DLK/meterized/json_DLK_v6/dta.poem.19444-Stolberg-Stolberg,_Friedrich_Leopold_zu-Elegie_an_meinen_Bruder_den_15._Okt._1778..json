{"dta.poem.19444": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "Elegie an meinen Bruder  \n den 15. Okt. 1778.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Freudiger w\u00fcrde mein Geist, in treuer, s\u00fcsser", "tokens": ["Freu\u00b7di\u00b7ger", "w\u00fcr\u00b7de", "mein", "Geist", ",", "in", "treu\u00b7er", ",", "s\u00fcs\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "$,", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Umarmung,", "tokens": ["Um\u00b7ar\u00b7mung", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Bester, eilen zu dir, wie zur Quelle das Reh,", "tokens": ["Bes\u00b7ter", ",", "ei\u00b7len", "zu", "dir", ",", "wie", "zur", "Quel\u00b7le", "das", "Reh", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "W\u00fcrde, bebend und sprachlos, von meiner Lippe", "tokens": ["W\u00fcr\u00b7de", ",", "be\u00b7bend", "und", "sprach\u00b7los", ",", "von", "mei\u00b7ner", "Lip\u00b7pe"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "VVPP", "KON", "ADJD", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "zur deinen,", "tokens": ["zur", "dei\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "PPOSAT", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Bester, eilen zu dir, auf gefl\u00fcgeltem Ku\u00df.", "tokens": ["Bes\u00b7ter", ",", "ei\u00b7len", "zu", "dir", ",", "auf", "ge\u00b7fl\u00fc\u00b7gel\u00b7tem", "Ku\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.7": {"text": "Z\u00e4rtlicher bebte der Freundschaft Bund auf Jo-", "tokens": ["Z\u00e4rt\u00b7li\u00b7cher", "beb\u00b7te", "der", "Freund\u00b7schaft", "Bund", "auf", "Jo"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "APPR", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "nathans Lippe", "tokens": ["na\u00b7thans", "Lip\u00b7pe"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Nicht, im heimlichen Thal, wo er dem Lie-", "tokens": ["Nicht", ",", "im", "heim\u00b7li\u00b7chen", "Thal", ",", "wo", "er", "dem", "Lie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "benden schwur;", "tokens": ["ben\u00b7den", "schwur", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.11": {"text": "Z\u00e4rtlicher zitterte nicht an Benjamins Auge die", "tokens": ["Z\u00e4rt\u00b7li\u00b7cher", "zit\u00b7ter\u00b7te", "nicht", "an", "Ben\u00b7ja\u00b7mins", "Au\u00b7ge", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "NE", "NN", "ART"], "meter": "+--+-++-+--+-+", "measure": "iambic.septa.invert"}, "line.12": {"text": "Thr\u00e4ne,", "tokens": ["Thr\u00e4\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Als sein Joseph ihm lag an der klopfenden", "tokens": ["Als", "sein", "Jo\u00b7se\u00b7ph", "ihm", "lag", "an", "der", "klop\u00b7fen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NE", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "--+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Brust!", "tokens": ["Brust", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Aber, trennen uns nicht die ausgedehnten Ge-", "tokens": ["A\u00b7ber", ",", "tren\u00b7nen", "uns", "nicht", "die", "aus\u00b7ge\u00b7dehn\u00b7ten", "Ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "filde?", "tokens": ["fil\u00b7de", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Trennen Fluten uns nicht, rauschend im herbst-", "tokens": ["Tren\u00b7nen", "Flu\u00b7ten", "uns", "nicht", ",", "rau\u00b7schend", "im", "herbs\u00b7t"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "PTKNEG", "$,", "ADJD", "APPRART", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "lichen Sturm?", "tokens": ["li\u00b7chen", "Sturm", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.19": {"text": "Sieh, ich eile zu dir auf t\u00f6nenden Fl\u00fcgeln des", "tokens": ["Sieh", ",", "ich", "ei\u00b7le", "zu", "dir", "auf", "t\u00f6\u00b7nen\u00b7den", "Fl\u00fc\u00b7geln", "des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "ART"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Liedes,", "tokens": ["Lie\u00b7des", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "An dem Tage, der dich deinen Liebenden gab;", "tokens": ["An", "dem", "Ta\u00b7ge", ",", "der", "dich", "dei\u00b7nen", "Lie\u00b7ben\u00b7den", "gab", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.22": {"text": "Dich dem z\u00e4rtlichen Vater, der Freude weinen-", "tokens": ["Dich", "dem", "z\u00e4rt\u00b7li\u00b7chen", "Va\u00b7ter", ",", "der", "Freu\u00b7de", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "den Mutter,", "tokens": ["den", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Deinen Schwestern und mir, deiner Luise", "tokens": ["Dei\u00b7nen", "Schwes\u00b7tern", "und", "mir", ",", "dei\u00b7ner", "Lu\u00b7i\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPER", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "dich gab!", "tokens": ["dich", "gab", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "Zwar es wiegte mich da auf ihrem blumigen", "tokens": ["Zwar", "es", "wieg\u00b7te", "mich", "da", "auf", "ih\u00b7rem", "blu\u00b7mi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Schoosse", "tokens": ["Schoos\u00b7se"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Mutter Erde noch nicht, Sonnen stralten mir", "tokens": ["Mut\u00b7ter", "Er\u00b7de", "noch", "nicht", ",", "Son\u00b7nen", "stral\u00b7ten", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "PTKNEG", "$,", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.29": {"text": "nicht,", "tokens": ["nicht", ","], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$,"], "meter": "-", "measure": "single.down"}, "line.30": {"text": "Als in den jauchzenden Hallen des frohen Hauses", "tokens": ["Als", "in", "den", "jauch\u00b7zen\u00b7den", "Hal\u00b7len", "des", "fro\u00b7hen", "Hau\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.31": {"text": "die Stimme", "tokens": ["die", "Stim\u00b7me"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.32": {"text": "Scholl: \u201eein Kn\u00e4blein ist da! freut euch!", "tokens": ["Scholl", ":", "\u201e", "ein", "Kn\u00e4\u00b7blein", "ist", "da", "!", "freut", "euch", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "$(", "ART", "NN", "VAFIN", "ADV", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "ein Kn\u00e4blein ist da!\u201d", "tokens": ["ein", "Kn\u00e4\u00b7blein", "ist", "da", "!", "\""], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.34": {"text": "Als der beste der V\u00e4ter dich, gl\u00fchend im heissen", "tokens": ["Als", "der", "bes\u00b7te", "der", "V\u00e4\u00b7ter", "dich", ",", "gl\u00fc\u00b7hend", "im", "heis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ART", "NN", "PPER", "$,", "ADJD", "APPRART", "ADJA"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.35": {"text": "Gebete,", "tokens": ["Ge\u00b7be\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "Hub zum Himmel empor, mit froh bebendem", "tokens": ["Hub", "zum", "Him\u00b7mel", "em\u00b7por", ",", "mit", "froh", "be\u00b7ben\u00b7dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$,", "APPR", "ADJD", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "Arm,", "tokens": ["Arm", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "Als in l\u00e4chelnder Ohnmacht, schon sinkend, die", "tokens": ["Als", "in", "l\u00e4\u00b7cheln\u00b7der", "Ohn\u00b7macht", ",", "schon", "sin\u00b7kend", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "PRELS"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.39": {"text": "Mutter dich ansah,", "tokens": ["Mut\u00b7ter", "dich", "an\u00b7sah", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.40": {"text": "Und erwachend dich sand an der wallenden", "tokens": ["Und", "er\u00b7wa\u00b7chend", "dich", "sand", "an", "der", "wal\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.41": {"text": "Brust.", "tokens": ["Brust", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.42": {"text": "Als, schon z\u00e4rtlich, die lallende Schwester, mit", "tokens": ["Als", ",", "schon", "z\u00e4rt\u00b7lich", ",", "die", "lal\u00b7len\u00b7de", "Schwes\u00b7ter", ",", "mit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "$,", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "$,", "APPR"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.43": {"text": "h\u00fcpfenden F\u00fcssen", "tokens": ["h\u00fcp\u00b7fen\u00b7den", "F\u00fcs\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Dein sich freute, schon da in die Arme dich schlo\u00df!", "tokens": ["Dein", "sich", "freu\u00b7te", ",", "schon", "da", "in", "die", "Ar\u00b7me", "dich", "schlo\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PRF", "VVFIN", "$,", "ADV", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.45": {"text": "Oft mit kindisch sorgsamer Hand die wankende", "tokens": ["Oft", "mit", "kin\u00b7disch", "sorg\u00b7sa\u00b7mer", "Hand", "die", "wan\u00b7ken\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Wiege", "tokens": ["Wie\u00b7ge"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "Fa\u00dfte, und von dir summende Fliegen vertrieb!", "tokens": ["Fa\u00df\u00b7te", ",", "und", "von", "dir", "sum\u00b7men\u00b7de", "Flie\u00b7gen", "ver\u00b7trieb", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "APPR", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.48": {"text": "Sp\u00e4ter ward ich, und sp\u00e4ter die j\u00fcngern Schwe-", "tokens": ["Sp\u00e4\u00b7ter", "ward", "ich", ",", "und", "sp\u00e4\u00b7ter", "die", "j\u00fcn\u00b7gern", "Schwe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KON", "ADJD", "ART", "ADJA", "TRUNC"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.49": {"text": "stern geboren,", "tokens": ["stern", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.50": {"text": "Und wir wuchsen empor freudig, wie Stauden", "tokens": ["Und", "wir", "wuch\u00b7sen", "em\u00b7por", "freu\u00b7dig", ",", "wie", "Stau\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "ADJD", "$,", "PWAV", "NN"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.51": {"text": "am Bach,", "tokens": ["am", "Bach", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.52": {"text": "Kanten fr\u00fch die s\u00fcssesten Freuden des Lebens, und", "tokens": ["Kan\u00b7ten", "fr\u00fch", "die", "s\u00fcs\u00b7ses\u00b7ten", "Freu\u00b7den", "des", "Le\u00b7bens", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "$,", "KON"], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.53": {"text": "pfl\u00fcckten", "tokens": ["pfl\u00fcck\u00b7ten"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.54": {"text": "Jeden kleinen Genu\u00df, der sich im Schatten", "tokens": ["Je\u00b7den", "klei\u00b7nen", "Ge\u00b7nu\u00df", ",", "der", "sich", "im", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.55": {"text": "verbirgt.", "tokens": ["ver\u00b7birgt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.56": {"text": "Ungesondert lebt\u2019 ich mit dir die Tage der Jugend;", "tokens": ["Un\u00b7ge\u00b7son\u00b7dert", "lebt'", "ich", "mit", "dir", "die", "Ta\u00b7ge", "der", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.57": {"text": "Wenn ein Morgen uns schied, schied uns der", "tokens": ["Wenn", "ein", "Mor\u00b7gen", "uns", "schied", ",", "schied", "uns", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.58": {"text": "A