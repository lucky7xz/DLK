{"textgrid.poem.25251": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willkommen, Ritter Morgen!", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, Ritter Morgen!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Rit\u00b7ter", "Mor\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor deinem g\u00fcldnen Haupt", "tokens": ["Vor", "dei\u00b7nem", "g\u00fcld\u00b7nen", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entfliehn die W\u00f6lfe Sorgen,", "tokens": ["Ent\u00b7fliehn", "die", "W\u00f6l\u00b7fe", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die mir den Schlaf geraubt.", "tokens": ["Die", "mir", "den", "Schlaf", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Fels vor meiner Klause", "tokens": ["Der", "Fels", "vor", "mei\u00b7ner", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Starrt feierlich mich an.", "tokens": ["Starrt", "fei\u00b7er\u00b7lich", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wipfel mit Gebrause", "tokens": ["Die", "Wip\u00b7fel", "mit", "Ge\u00b7brau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wiegt unter mir der Tann.", "tokens": ["Wiegt", "un\u00b7ter", "mir", "der", "Tann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Steingraue Wolkenwogen", "tokens": ["Stein\u00b7grau\u00b7e", "Wol\u00b7ken\u00b7wo\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Verh\u00fcllen noch das Tal.", "tokens": ["Ver\u00b7h\u00fcl\u00b7len", "noch", "das", "Tal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darob der Himmelsbogen", "tokens": ["Da\u00b7rob", "der", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Matt leuchtender Opal.", "tokens": ["Matt", "leuch\u00b7ten\u00b7der", "O\u00b7pal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und aus dem Dunstmeer ragen", "tokens": ["Und", "aus", "dem", "Dunst\u00b7meer", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Riesenberge steil.", "tokens": ["Die", "Rie\u00b7sen\u00b7ber\u00b7ge", "steil", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Stirnenglanz will sagen:", "tokens": ["Ihr", "Stir\u00b7nen\u00b7glanz", "will", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ganz oben thront das Heil!", "tokens": ["Ganz", "o\u00b7ben", "thront", "das", "Heil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun bl\u00fcht von Purpursonne", "tokens": ["Nun", "bl\u00fcht", "von", "Pur\u00b7pur\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Nebelmeer wie Klee;", "tokens": ["Das", "Ne\u00b7bel\u00b7meer", "wie", "Klee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auch mein Gram ward Wonne,", "tokens": ["Und", "auch", "mein", "Gram", "ward", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil ich dar\u00fcber steh.", "tokens": ["Weil", "ich", "da\u00b7r\u00fc\u00b7ber", "steh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Als Lerche schwebt mein Schauen", "tokens": ["Als", "Ler\u00b7che", "schwebt", "mein", "Schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hoch ob dem Erdennest", "tokens": ["Hoch", "ob", "dem", "Er\u00b7den\u00b7nest"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch selig freie Auen ...", "tokens": ["Durch", "se\u00b7lig", "frei\u00b7e", "Au\u00b7en", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "O Himmel, halt mich fest!", "tokens": ["O", "Him\u00b7mel", ",", "halt", "mich", "fest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}