{"textgrid.poem.33496": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Punkt", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die w\u00fcsten Stra\u00dfen flie\u00dfen lichterloh", "tokens": ["Die", "w\u00fcs\u00b7ten", "Stra\u00b7\u00dfen", "flie\u00b7\u00dfen", "lich\u00b7ter\u00b7loh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch den erloschnen Kopf. Und tun mir weh.", "tokens": ["Durch", "den", "er\u00b7lo\u00b7schnen", "Kopf", ".", "Und", "tun", "mir", "weh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchle deutlich, da\u00df ich bald vergeh \u2013", "tokens": ["Ich", "f\u00fch\u00b7le", "deut\u00b7lich", ",", "da\u00df", "ich", "bald", "ver\u00b7geh", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dornrosen meines Fleisches, stecht nicht so.", "tokens": ["Dorn\u00b7ro\u00b7sen", "mei\u00b7nes", "Flei\u00b7sches", ",", "stecht", "nicht", "so", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Nacht verschimmelt. Giftlaternenschein", "tokens": ["Die", "Nacht", "ver\u00b7schim\u00b7melt", ".", "Gift\u00b7la\u00b7ter\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVPP", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat, kriechend, sie mit gr\u00fcnem Dreck beschmiert.", "tokens": ["Hat", ",", "krie\u00b7chend", ",", "sie", "mit", "gr\u00fc\u00b7nem", "Dreck", "be\u00b7schmiert", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVPP", "$,", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Herz ist wie ein Sack. Das Blut erfriert.", "tokens": ["Das", "Herz", "ist", "wie", "ein", "Sack", ".", "Das", "Blut", "er\u00b7friert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$.", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Welt f\u00e4llt um. Die Augen st\u00fcrzen ein.", "tokens": ["Die", "Welt", "f\u00e4llt", "um", ".", "Die", "Au\u00b7gen", "st\u00fcr\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}