{"textgrid.poem.53253": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Post cineres restat gratia nulla malis", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh' al\u00df wir au\u00df der H\u00f6len", "tokens": ["Eh'", "al\u00df", "wir", "au\u00df", "der", "H\u00f6\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des finstern C\u00f6rpers ziehn,", "tokens": ["Des", "fins\u00b7tern", "C\u00f6r\u00b7pers", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd zu den leichten Seelen", "tokens": ["Vnd", "zu", "den", "leich\u00b7ten", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der abgeleibten fliehn,", "tokens": ["Der", "ab\u00b7ge\u00b7leib\u00b7ten", "fliehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Steht vns die Th\u00fcr der Gnaden", "tokens": ["Steht", "vns", "die", "Th\u00fcr", "der", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch immer auffgethan,", "tokens": ["Noch", "im\u00b7mer", "auff\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dahin der, so beladen", "tokens": ["Da\u00b7hin", "der", ",", "so", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PAV", "ART", "$,", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Schulden, fliehen kan.", "tokens": ["Mit", "Schul\u00b7den", ",", "flie\u00b7hen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kommt, klopffet an, jhr S\u00fcnder,", "tokens": ["Kommt", ",", "klopf\u00b7fet", "an", ",", "jhr", "S\u00fcn\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch wahre Rew, vnd sprecht:", "tokens": ["Durch", "wah\u00b7re", "Rew", ",", "vnd", "sprecht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist, O Gott, nicht minder", "tokens": ["Du", "bist", ",", "O", "Gott", ",", "nicht", "min\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "NE", "NN", "$,", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gedultig, al\u00df gerecht,", "tokens": ["Ge\u00b7dul\u00b7tig", ",", "al\u00df", "ge\u00b7recht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn wirst Du ohn' erbarmen", "tokens": ["Denn", "wirst", "Du", "ohn'", "er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit vns zu rechten gehn,", "tokens": ["Mit", "vns", "zu", "rech\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer wird wol von vns Armen", "tokens": ["Wer", "wird", "wol", "von", "vns", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Al\u00dfdann vor Dir bestehn?", "tokens": ["Al\u00df\u00b7dann", "vor", "Dir", "be\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ist nicht Dein Sohn gestorben", "tokens": ["Ist", "nicht", "Dein", "Sohn", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den S\u00fcndern nur zu gut',", "tokens": ["Den", "S\u00fcn\u00b7dern", "nur", "zu", "gut'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd hat vns jhm erworben", "tokens": ["Vnd", "hat", "vns", "jhm", "er\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch sein selbst eigen Blut?", "tokens": ["Durch", "sein", "selbst", "ei\u00b7gen", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur einig dich zu stillen", "tokens": ["Nur", "ei\u00b7nig", "dich", "zu", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geschahe dieser Kauff,", "tokens": ["Ge\u00b7scha\u00b7he", "die\u00b7ser", "Kauff", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Darumb vmb seinet willen", "tokens": ["Da\u00b7rumb", "vmb", "sei\u00b7net", "wil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nimm vns zu Gnaden auff!", "tokens": ["Nimm", "vns", "zu", "Gna\u00b7den", "auff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie wollen wir vns frewen", "tokens": ["Wie", "wol\u00b7len", "wir", "vns", "fre\u00b7wen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Al\u00dfdann der G\u00fctigkeit,", "tokens": ["Al\u00df\u00b7dann", "der", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Du au\u00df Vater-Trewen", "tokens": ["Die", "Du", "au\u00df", "Va\u00b7ter\u00b7Tre\u00b7wen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vns zeigst zu rechter Zeit!", "tokens": ["Vns", "zeigst", "zu", "rech\u00b7ter", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du solt gepriesen werden", "tokens": ["Du", "solt", "ge\u00b7prie\u00b7sen", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dan, wann die Sonn' erwacht,", "tokens": ["Dan", ",", "wann", "die", "Sonn'", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Vnd mit den schnellen Pferden", "tokens": ["Vnd", "mit", "den", "schnel\u00b7len", "Pfer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Sich wieder von vns macht.", "tokens": ["Sich", "wie\u00b7der", "von", "vns", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nur la\u00df vns das ver\u00fcben", "tokens": ["Nur", "la\u00df", "vns", "das", "ver\u00b7\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PDS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Worauff du vns geweist,", "tokens": ["Wo\u00b7rauff", "du", "vns", "ge\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd nicht die Zeit verschieben", "tokens": ["Vnd", "nicht", "die", "Zeit", "ver\u00b7schie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wie ein Strohm hinfleusst;", "tokens": ["Die", "wie", "ein", "Strohm", "hin\u00b7fleusst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du wirst es wol belohnen", "tokens": ["Du", "wirst", "es", "wol", "be\u00b7loh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ob wir's wol nicht verdient,", "tokens": ["Ob", "wir's", "wol", "nicht", "ver\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd ewig vnser schonen,", "tokens": ["Vnd", "e\u00b7wig", "vn\u00b7ser", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Weil Gott vns dir vers\u00fchnt.", "tokens": ["Weil", "Gott", "vns", "dir", "ver\u00b7s\u00fchnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}