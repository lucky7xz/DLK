{"textgrid.poem.24828": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm, Herr, von meiner Brust die Klammer,", "tokens": ["Nimm", ",", "Herr", ",", "von", "mei\u00b7ner", "Brust", "die", "Klam\u00b7mer", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die auf ihr lastet, schwer wie Erz!", "tokens": ["Die", "auf", "ihr", "las\u00b7tet", ",", "schwer", "wie", "Erz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein kein Truggebild verhehle", "tokens": ["Al\u00b7lein", "kein", "Trug\u00b7ge\u00b7bild", "ver\u00b7heh\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den blut'gen Ri\u00df in meiner Seele;", "tokens": ["Den", "blut'\u00b7gen", "Ri\u00df", "in", "mei\u00b7ner", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht Tr\u00f6stung such' ich in meinem Jammer;", "tokens": ["Nicht", "Tr\u00f6s\u00b7tung", "such'", "ich", "in", "mei\u00b7nem", "Jam\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich flehe nur um tiefern Schmerz.", "tokens": ["Ich", "fle\u00b7he", "nur", "um", "tie\u00b7fern", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was soll die T\u00e4uschung mir, die kurze?", "tokens": ["Was", "soll", "die", "T\u00e4u\u00b7schung", "mir", ",", "die", "kur\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPER", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was mir ein \u00f6der, armer Trost?", "tokens": ["Was", "mir", "ein", "\u00f6\u00b7der", ",", "ar\u00b7mer", "Trost", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, rei\u00df mir tiefer auf die Wunden,", "tokens": ["Nein", ",", "rei\u00df", "mir", "tie\u00b7fer", "auf", "die", "Wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit mein Gram, der Haft entbunden", "tokens": ["Da\u00b7mit", "mein", "Gram", ",", "der", "Haft", "ent\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hinflute gleich dem Wassersturze,", "tokens": ["Hin\u00b7flu\u00b7te", "gleich", "dem", "Was\u00b7ser\u00b7stur\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der von dem Felsen niedertost!", "tokens": ["Der", "von", "dem", "Fel\u00b7sen", "nie\u00b7der\u00b7tost", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}