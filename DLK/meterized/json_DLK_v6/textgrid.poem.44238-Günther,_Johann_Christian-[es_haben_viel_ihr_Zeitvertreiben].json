{"textgrid.poem.44238": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[es haben viel ihr Zeitvertreiben]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es haben viel ihr Zeitvertreiben", "tokens": ["Es", "ha\u00b7ben", "viel", "ihr", "Zeit\u00b7ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von B\u00e4umen ungemeiner Frucht;", "tokens": ["Von", "B\u00e4u\u00b7men", "un\u00b7ge\u00b7mei\u00b7ner", "Frucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein ich will bey Blumen bleiben,", "tokens": ["Al\u00b7lein", "ich", "will", "bey", "Blu\u00b7men", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rath ein, was meine Wahl jezt sucht.", "tokens": ["Rath", "ein", ",", "was", "mei\u00b7ne", "Wahl", "jezt", "sucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Obgleich die D\u00f6rner anfangs stechen,", "tokens": ["Ob\u00b7gleich", "die", "D\u00f6r\u00b7ner", "an\u00b7fangs", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So will ich dennoch Rosen brechen.", "tokens": ["So", "will", "ich", "den\u00b7noch", "Ro\u00b7sen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich kenn und weis dergleichen eine,", "tokens": ["Ich", "kenn", "und", "weis", "derg\u00b7lei\u00b7chen", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PTKVZ", "PIS", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts ist ihr an Entz\u00fcckung gleich;", "tokens": ["Nichts", "ist", "ihr", "an", "Ent\u00b7z\u00fc\u00b7ckung", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach w\u00fcrde sie nur endlich meine!", "tokens": ["Ach", "w\u00fcr\u00b7de", "sie", "nur", "end\u00b7lich", "mei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gewis, ich h\u00e4tt ein Himmelreich.", "tokens": ["Ge\u00b7wis", ",", "ich", "h\u00e4tt", "ein", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein jedes Blat auf ihrem Stengel", "tokens": ["Ein", "je\u00b7des", "Blat", "auf", "ih\u00b7rem", "Sten\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Besch\u00e4mt den j\u00fcngsten Liebesengel.", "tokens": ["Be\u00b7sch\u00e4mt", "den", "j\u00fcng\u00b7sten", "Lie\u00b7be\u00b7sen\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Liebe, die du selbst den Rosen", "tokens": ["O", "Lie\u00b7be", ",", "die", "du", "selbst", "den", "Ro\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herz, Lust, Geruch und Blut geschenckt,", "tokens": ["Herz", ",", "Lust", ",", "Ge\u00b7ruch", "und", "Blut", "ge\u00b7schenckt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Regiere sie, mir liebzukosen,", "tokens": ["Re\u00b7gie\u00b7re", "sie", ",", "mir", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdem es das Verh\u00e4ngn\u00fc\u00df lenckt;", "tokens": ["Nach\u00b7dem", "es", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "lenckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es soll ihr auch von mir auf Erden", "tokens": ["Es", "soll", "ihr", "auch", "von", "mir", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Herz, Trieb und Geist gegeben werden.", "tokens": ["Herz", ",", "Trieb", "und", "Geist", "ge\u00b7ge\u00b7ben", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Ku\u00df auf ihren Purpurbl\u00e4ttern", "tokens": ["Ein", "Ku\u00df", "auf", "ih\u00b7ren", "Pur\u00b7pur\u00b7bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rei\u00dft meine Freyheit ewig hin.", "tokens": ["Rei\u00dft", "mei\u00b7ne", "Frey\u00b7heit", "e\u00b7wig", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleibt, Thoren, bey den Fr\u00fchlingswettern,", "tokens": ["Bleibt", ",", "Tho\u00b7ren", ",", "bey", "den", "Fr\u00fch\u00b7lings\u00b7wet\u00b7tern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stehlt Sommerthau und Wintergr\u00fcn,", "tokens": ["Stehlt", "Som\u00b7mert\u00b7hau", "und", "Win\u00b7ter\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hof und seufze mit Verlangen", "tokens": ["Ich", "hof", "und", "seuf\u00b7ze", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Rosen, die im Herbste prangen.", "tokens": ["Nach", "Ro\u00b7sen", ",", "die", "im", "Herbs\u00b7te", "pran\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}