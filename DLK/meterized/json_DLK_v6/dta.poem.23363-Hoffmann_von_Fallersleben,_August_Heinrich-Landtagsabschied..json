{"dta.poem.23363": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Landtagsabschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt gehen wir nach Haus,", "tokens": ["Jetzt", "ge\u00b7hen", "wir", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Landtag ist nun aus.", "tokens": ["Der", "Land\u00b7tag", "ist", "nun", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir waren einig allezeit,", "tokens": ["Wir", "wa\u00b7ren", "ei\u00b7nig", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und thaten unsre Schuldigkeit,", "tokens": ["Und", "tha\u00b7ten", "uns\u00b7re", "Schul\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sogar bei jedem Schmause, ja Schmause.", "tokens": ["So\u00b7gar", "bei", "je\u00b7dem", "Schmau\u00b7se", ",", "ja", "Schmau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir haben Tag und Nacht", "tokens": ["Wir", "ha\u00b7ben", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gesessen und gedacht,", "tokens": ["Ge\u00b7ses\u00b7sen", "und", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sahen fest und unverwandt", "tokens": ["Und", "sa\u00b7hen", "fest", "und", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf unser theures Vaterland,", "tokens": ["Auf", "un\u00b7ser", "theu\u00b7res", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sogar bei jedem Schmause, ja Schmause.", "tokens": ["So\u00b7gar", "bei", "je\u00b7dem", "Schmau\u00b7se", ",", "ja", "Schmau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Zeitung giebt Bericht:", "tokens": ["Die", "Zei\u00b7tung", "giebt", "Be\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir thaten unsre Pflicht;", "tokens": ["Wir", "tha\u00b7ten", "uns\u00b7re", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir hielten nicht umsonst Di\u00e4t,", "tokens": ["Wir", "hiel\u00b7ten", "nicht", "um\u00b7sonst", "Di\u00b7\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "(das wei\u00df auch Seine Majest\u00e4t,)", "tokens": ["(", "das", "wei\u00df", "auch", "Sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sogar bei jedem Schmause, ja Schmause.", "tokens": ["So\u00b7gar", "bei", "je\u00b7dem", "Schmau\u00b7se", ",", "ja", "Schmau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}