{"textgrid.poem.56063": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Damen-Bildnis aus den", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wartend stand sie an den schwergerafften", "tokens": ["War\u00b7tend", "stand", "sie", "an", "den", "schwer\u00b7ge\u00b7raff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "dunklen Atlasdraperien,", "tokens": ["dunk\u00b7len", "At\u00b7las\u00b7dra\u00b7pe\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "die ein Aufwand falscher Leidenschaften", "tokens": ["die", "ein", "Auf\u00b7wand", "fal\u00b7scher", "Lei\u00b7den\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00fcber ihr zu ballen schien;", "tokens": ["\u00fc\u00b7ber", "ihr", "zu", "bal\u00b7len", "schien", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "seit den noch so nahen M\u00e4dchenjahren", "tokens": ["seit", "den", "noch", "so", "na\u00b7hen", "M\u00e4d\u00b7chen\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wie mit einer anderen vertauscht:", "tokens": ["wie", "mit", "ei\u00b7ner", "an\u00b7de\u00b7ren", "ver\u00b7tauscht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "m\u00fcde unter den get\u00fcrmten Haaren,", "tokens": ["m\u00fc\u00b7de", "un\u00b7ter", "den", "ge\u00b7t\u00fcrm\u00b7ten", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "in den R\u00fcschen-Roben unerfahren", "tokens": ["in", "den", "R\u00fc\u00b7schen\u00b7Ro\u00b7ben", "un\u00b7er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "und von allen Falten wie belauscht", "tokens": ["und", "von", "al\u00b7len", "Fal\u00b7ten", "wie", "be\u00b7lauscht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "KOKOM", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "bei dem Heimweh und dem schwachen Planen,", "tokens": ["bei", "dem", "Heim\u00b7weh", "und", "dem", "schwa\u00b7chen", "Pla\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wie das Leben weiter werden soll:", "tokens": ["wie", "das", "Le\u00b7ben", "wei\u00b7ter", "wer\u00b7den", "soll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "anders, wirklicher, wie in Romanen,", "tokens": ["an\u00b7ders", ",", "wirk\u00b7li\u00b7cher", ",", "wie", "in", "Ro\u00b7ma\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "hingerissen und verh\u00e4ngnisvoll, \u2013", "tokens": ["hin\u00b7ge\u00b7ris\u00b7sen", "und", "ver\u00b7h\u00e4ng\u00b7nis\u00b7voll", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "da\u00df man etwas erst in die Schatullen", "tokens": ["da\u00df", "man", "et\u00b7was", "erst", "in", "die", "Scha\u00b7tul\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "legen d\u00fcrfte, um sich im Geruch", "tokens": ["le\u00b7gen", "d\u00fcrf\u00b7te", ",", "um", "sich", "im", "Ge\u00b7ruch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "VMFIN", "$,", "KOUI", "PRF", "APPRART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "von Erinnerungen einzulullen;", "tokens": ["von", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "ein\u00b7zu\u00b7lul\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "da\u00df man endlich in dem Tagebuch", "tokens": ["da\u00df", "man", "end\u00b7lich", "in", "dem", "Ta\u00b7ge\u00b7buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "einen Anfang f\u00e4nde, der nicht schon", "tokens": ["ei\u00b7nen", "An\u00b7fang", "f\u00e4n\u00b7de", ",", "der", "nicht", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PTKNEG", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "unterm Schreiben sinnlos wird und L\u00fcge,", "tokens": ["un\u00b7term", "Schrei\u00b7ben", "sinn\u00b7los", "wird", "und", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VAFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und ein Blatt von einer Rose tr\u00fcge", "tokens": ["und", "ein", "Blatt", "von", "ei\u00b7ner", "Ro\u00b7se", "tr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "in dem schweren leeren Medaillon,", "tokens": ["in", "dem", "schwe\u00b7ren", "lee\u00b7ren", "Me\u00b7dail\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "welches liegt auf jedem Atemzug.", "tokens": ["wel\u00b7ches", "liegt", "auf", "je\u00b7dem", "A\u00b7tem\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df man einmal durch das Fenster winkte;", "tokens": ["Da\u00df", "man", "ein\u00b7mal", "durch", "das", "Fens\u00b7ter", "wink\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "diese schlanke Hand, die neuberingte,", "tokens": ["die\u00b7se", "schlan\u00b7ke", "Hand", ",", "die", "neu\u00b7be\u00b7ring\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "h\u00e4tte dran f\u00fcr Monate genug.", "tokens": ["h\u00e4t\u00b7te", "dran", "f\u00fcr", "Mo\u00b7na\u00b7te", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "APPR", "NN", "ADV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}