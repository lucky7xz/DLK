{"dta.poem.15626": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df uns um Dinge, die wir nicht verstehn, nicht streiten,", "tokens": ["La\u00df", "uns", "um", "Din\u00b7ge", ",", "die", "wir", "nicht", "ver\u00b7stehn", ",", "nicht", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nothwendigkeiten nicht machen aus M\u00f6glichkeiten.", "tokens": ["Noth\u00b7wen\u00b7dig\u00b7kei\u00b7ten", "nicht", "ma\u00b7chen", "aus", "M\u00f6g\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "APPR", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ich denk' es so, du so; und wie es jeder dachte,", "tokens": ["Ich", "denk'", "es", "so", ",", "du", "so", ";", "und", "wie", "es", "je\u00b7der", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADV", "$.", "KON", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ists f\u00fcr ihn; an sich wie's ist, wei\u00df Gott ders machte.", "tokens": ["So", "ists", "f\u00fcr", "ihn", ";", "an", "sich", "wie's", "ist", ",", "wei\u00df", "Gott", "ders", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "