{"textgrid.poem.35193": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[ihr Matten voll Schatten/ begrasete Wasen]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Matten voll Schatten/ begrasete Wasen/", "tokens": ["Ihr", "Mat\u00b7ten", "voll", "Schat\u00b7ten", "/", "be\u00b7gra\u00b7se\u00b7te", "Wa\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ihr n\u00e4rbicht- und f\u00e4rbicht gebl\u00fcmete Rasen/", "tokens": ["Ihr", "n\u00e4r\u00b7bicht", "und", "f\u00e4r\u00b7bicht", "ge\u00b7bl\u00fc\u00b7me\u00b7te", "Ra\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "VVFIN", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ihr buntlichen Sternen /", "tokens": ["Ihr", "bunt\u00b7li\u00b7chen", "Ster\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ihr Felderlaternen.", "tokens": ["Ihr", "Fel\u00b7der\u00b7la\u00b7ter\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "H\u00f6rt wieder die Lieder von Sch\u00e4ferschalmeyen/", "tokens": ["H\u00f6rt", "wie\u00b7der", "die", "Lie\u00b7der", "von", "Sch\u00e4\u00b7fer\u00b7schal\u00b7me\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "APPR", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wir bringen das Springen zu freudigen Reyen/", "tokens": ["Wir", "brin\u00b7gen", "das", "Sprin\u00b7gen", "zu", "freu\u00b7di\u00b7gen", "Re\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Wir lassen euch f\u00fcrter nicht mehr", "tokens": ["Wir", "las\u00b7sen", "euch", "f\u00fcr\u00b7ter", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Vnd geben euch Leben und Ehr.", "tokens": ["Vnd", "ge\u00b7ben", "euch", "Le\u00b7ben", "und", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Ihr vormals-uns\u00e4glich beh\u00e4gliche Triften/", "tokens": ["Ihr", "vor\u00b7mals\u00b7un\u00b7s\u00e4g\u00b7lich", "be\u00b7h\u00e4g\u00b7li\u00b7che", "Trif\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ihr Heiden und Weiden bey lieblichen L\u00fcften/", "tokens": ["Ihr", "Hei\u00b7den", "und", "Wei\u00b7den", "bey", "lieb\u00b7li\u00b7chen", "L\u00fcf\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Ihr H\u00fcrden voll Myrten/", "tokens": ["Ihr", "H\u00fcr\u00b7den", "voll", "Myr\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Vor unser Bewirten/", "tokens": ["Vor", "un\u00b7ser", "Be\u00b7wir\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Sch\u00f6pft aber und aber beliebliches Dr\u00f6nen/", "tokens": ["Sch\u00f6pft", "a\u00b7ber", "und", "a\u00b7ber", "be\u00b7lieb\u00b7li\u00b7ches", "Dr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.14": {"text": "Der schleiffenden Pfeiffen Lust-schl\u00fcrfendes T\u00f6nen/", "tokens": ["Der", "schleif\u00b7fen\u00b7den", "Pfeif\u00b7fen", "Lust\u00b7schl\u00fcr\u00b7fen\u00b7des", "T\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Wir werden euch wieder bewohnen/", "tokens": ["Wir", "wer\u00b7den", "euch", "wie\u00b7der", "be\u00b7woh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Ihr werdet die Lieder belohnen.", "tokens": ["Ihr", "wer\u00b7det", "die", "Lie\u00b7der", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "Ihr tr\u00e4gen Goldb\u00e4chlein/ ihr hellen Glasquellen/", "tokens": ["Ihr", "tr\u00e4\u00b7gen", "Gold\u00b7b\u00e4ch\u00b7lein", "/", "ihr", "hel\u00b7len", "Glas\u00b7quel\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.18": {"text": "Ihr schw\u00e4llende Wellen/ ihr Silberflut-Zellen/", "tokens": ["Ihr", "schw\u00e4l\u00b7len\u00b7de", "Wel\u00b7len", "/", "ihr", "Sil\u00b7ber\u00b7flut\u00b7Zel\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.19": {"text": "Ihr Pegnitz-Najaden", "tokens": ["Ihr", "Peg\u00b7nitz\u00b7Na\u00b7ja\u00b7den"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "In sumpflichten Pfaden!", "tokens": ["In", "sum\u00b7pf\u00b7lich\u00b7ten", "Pfa\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Nehmt dieses/ nehmt hiesig-erneurende Lieder/", "tokens": ["Nehmt", "die\u00b7ses", "/", "nehmt", "hie\u00b7sig\u00b7er\u00b7neu\u00b7ren\u00b7de", "Lie\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "$(", "VVFIN", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.22": {"text": "Wir ringen und klingen und singen hier wieder/", "tokens": ["Wir", "rin\u00b7gen", "und", "klin\u00b7gen", "und", "sin\u00b7gen", "hier", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "KON", "VVFIN", "ADV", "ADV", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.23": {"text": "Erbauen gepflogene Freud/", "tokens": ["Er\u00b7bau\u00b7en", "ge\u00b7pflo\u00b7ge\u00b7ne", "Freud", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "Vnd suchen erfreuliche Weid.", "tokens": ["Vnd", "su\u00b7chen", "er\u00b7freu\u00b7li\u00b7che", "Weid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.25": {"text": "Ihr wollichte Wollenb\u00e4hrete Heerden/", "tokens": ["Ihr", "wol\u00b7lich\u00b7te", "Wol\u00b7len\u00b7b\u00e4h\u00b7re\u00b7te", "Heer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Ihr B\u00f6kke beb\u00e4rtet mit zottichten B\u00e4rten/", "tokens": ["Ihr", "B\u00f6k\u00b7ke", "be\u00b7b\u00e4r\u00b7tet", "mit", "zot\u00b7tich\u00b7ten", "B\u00e4r\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Ihr Kletterges\u00fcchte/", "tokens": ["Ihr", "Klet\u00b7ter\u00b7ge\u00b7s\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Ihr Mertzengez\u00fcchte!", "tokens": ["Ihr", "Mert\u00b7zen\u00b7ge\u00b7z\u00fcch\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Lauft munder jetzunder/ und irret in Brachen/", "tokens": ["Lauft", "mun\u00b7der", "je\u00b7tzun\u00b7der", "/", "und", "ir\u00b7ret", "in", "Bra\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "$(", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.30": {"text": "Wir sollen und wollen euch wieder Lust machen/", "tokens": ["Wir", "sol\u00b7len", "und", "wol\u00b7len", "euch", "wie\u00b7der", "Lust", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$("], "meter": "-+--+--+-++-", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "Mit Reimen die Tafel vers\u00fcssen/", "tokens": ["Mit", "Rei\u00b7men", "die", "Ta\u00b7fel", "ver\u00b7s\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.32": {"text": "Mit Liedern ersprieslich begr\u00fcssen.", "tokens": ["Mit", "Lie\u00b7dern", "er\u00b7spries\u00b7lich", "be\u00b7gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}