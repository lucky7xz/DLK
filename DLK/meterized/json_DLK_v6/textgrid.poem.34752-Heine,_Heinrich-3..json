{"textgrid.poem.34752": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist eine wei\u00dfe M\u00f6we,", "tokens": ["Das", "ist", "ei\u00b7ne", "wei\u00b7\u00dfe", "M\u00f6\u00b7we", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die ich dort flattern seh", "tokens": ["Die", "ich", "dort", "flat\u00b7tern", "seh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl \u00fcber die dunklen Fluten;", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "dunk\u00b7len", "Flu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Mond steht hoch in der H\u00f6h'.", "tokens": ["Der", "Mond", "steht", "hoch", "in", "der", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Der Haifisch und der Roche,", "tokens": ["Der", "Hai\u00b7fisch", "und", "der", "Ro\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schnappen hervor aus der See,", "tokens": ["Die", "schnap\u00b7pen", "her\u00b7vor", "aus", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es hebt sich, es senkt sich die M\u00f6we;", "tokens": ["Es", "hebt", "sich", ",", "es", "senkt", "sich", "die", "M\u00f6\u00b7we", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Mond steht hoch in der H\u00f6h'.", "tokens": ["Der", "Mond", "steht", "hoch", "in", "der", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Oh, liebe, fl\u00fcchtige Seele,", "tokens": ["Oh", ",", "lie\u00b7be", ",", "fl\u00fcch\u00b7ti\u00b7ge", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dir ist so bang und weh!", "tokens": ["Dir", "ist", "so", "bang", "und", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu nah ist dir das Wasser,", "tokens": ["Zu", "nah", "ist", "dir", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Mond steht hoch in der H\u00f6h'.", "tokens": ["Der", "Mond", "steht", "hoch", "in", "der", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}