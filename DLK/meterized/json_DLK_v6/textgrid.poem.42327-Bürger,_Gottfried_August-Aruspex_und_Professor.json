{"textgrid.poem.42327": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Aruspex und Professor", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Aruspex dem Kollegen", "tokens": ["Wie", "ein", "A\u00b7rus\u00b7pex", "dem", "Kol\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ohn' aufzulachen, einst entgegen", "tokens": ["Ohn'", "auf\u00b7zu\u00b7la\u00b7chen", ",", "einst", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Ernst zu treten f\u00e4hig war,", "tokens": ["Mit", "Ernst", "zu", "tre\u00b7ten", "f\u00e4\u00b7hig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schien, Tullius, dir wunderbar.", "tokens": ["Schien", ",", "Tul\u00b7li\u00b7us", ",", "dir", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein gr\u00f6\u00dfres Wunder fast w\u00e4r's unter uns zu nennen,", "tokens": ["Ein", "gr\u00f6\u00df\u00b7res", "Wun\u00b7der", "fast", "w\u00e4r's", "un\u00b7ter", "uns", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie's manche Professoren k\u00f6nnen.", "tokens": ["Wie's", "man\u00b7che", "Pro\u00b7fes\u00b7so\u00b7ren", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}