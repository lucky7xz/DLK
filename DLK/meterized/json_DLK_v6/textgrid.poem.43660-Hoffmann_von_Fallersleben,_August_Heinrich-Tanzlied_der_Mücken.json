{"textgrid.poem.43660": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tanzlied der M\u00fccken", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch, ihr Blumen und Halme,", "tokens": ["Frisch", ",", "ihr", "Blu\u00b7men", "und", "Hal\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Frisch, spielt uns zum Reih'n!", "tokens": ["Frisch", ",", "spielt", "uns", "zum", "Reih'n", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Denn es winket die Sonne", "tokens": ["Denn", "es", "win\u00b7ket", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mit purpurnem Schein.", "tokens": ["Mit", "pur\u00b7pur\u00b7nem", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "La\u00dft uns schweben im Tanze", "tokens": ["La\u00dft", "uns", "schwe\u00b7ben", "im", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Am Bach und im Feld!", "tokens": ["Am", "Bach", "und", "im", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Uns geh\u00f6ret die ganze,", "tokens": ["Uns", "ge\u00b7h\u00f6\u00b7ret", "die", "gan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Die unendliche Welt.", "tokens": ["Die", "un\u00b7end\u00b7li\u00b7che", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Weile l\u00e4nger, o Sonne!", "tokens": ["Wei\u00b7le", "l\u00e4n\u00b7ger", ",", "o", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "FM", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wer wei\u00df, ob einmal", "tokens": ["Wer", "wei\u00df", ",", "ob", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Uns noch scheinet zum Tanze", "tokens": ["Uns", "noch", "schei\u00b7net", "zum", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dein lieblicher Strahl?", "tokens": ["Dein", "lieb\u00b7li\u00b7cher", "Strahl", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ach, wer wei\u00df, ob uns morgen", "tokens": ["Ach", ",", "wer", "wei\u00df", ",", "ob", "uns", "mor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Ein Wiedersehn lacht?", "tokens": ["Ein", "Wie\u00b7der\u00b7sehn", "lacht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Hei\u00dfa, lustig getanzet,", "tokens": ["Hei\u00b7\u00dfa", ",", "lus\u00b7tig", "ge\u00b7tan\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Eh' uns scheidet die Nacht.", "tokens": ["Eh'", "uns", "schei\u00b7det", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Hei\u00dfa, lustig getanzet", "tokens": ["Hei\u00b7\u00dfa", ",", "lus\u00b7tig", "ge\u00b7tan\u00b7zet"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJD", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Im blumigen Duft!", "tokens": ["Im", "blu\u00b7mi\u00b7gen", "Duft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "La\u00dft uns singen und summen", "tokens": ["La\u00dft", "uns", "sin\u00b7gen", "und", "sum\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "KON", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "In s\u00e4uselnder Luft!", "tokens": ["In", "s\u00e4u\u00b7seln\u00b7der", "Luft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "La\u00dft uns schweben und weben", "tokens": ["La\u00dft", "uns", "schwe\u00b7ben", "und", "we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "KON", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Hinab und hinauf!", "tokens": ["Hin\u00b7ab", "und", "hin\u00b7auf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Denn es hat ja die Sonne", "tokens": ["Denn", "es", "hat", "ja", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Bald vollendet den Lauf.", "tokens": ["Bald", "voll\u00b7en\u00b7det", "den", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}