{"textgrid.poem.66239": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Und sonst?", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrwahr die Welt", "tokens": ["F\u00fcr\u00b7wahr", "die", "Welt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ist trefflich bestellt.", "tokens": ["Ist", "treff\u00b7lich", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "T\u00fcrken, Russen und Deutsche", "tokens": ["T\u00fcr\u00b7ken", ",", "Rus\u00b7sen", "und", "Deut\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schwingen des Krieges blutige Peitsche.", "tokens": ["Schwin\u00b7gen", "des", "Krie\u00b7ges", "blu\u00b7ti\u00b7ge", "Peit\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Zu Donnerschlachten auf dem Meer", "tokens": ["Zu", "Don\u00b7ner\u00b7schlach\u00b7ten", "auf", "dem", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "R\u00fcstet Poseidaon sich sehr.", "tokens": ["R\u00fcs\u00b7tet", "Po\u00b7sei\u00b7da\u00b7on", "sich", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Die Polen, gleich verzehrenden Flammen,", "tokens": ["Die", "Po\u00b7len", ",", "gleich", "ver\u00b7zeh\u00b7ren\u00b7den", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rotten sich in wildem Bunde zusammen.", "tokens": ["Rot\u00b7ten", "sich", "in", "wil\u00b7dem", "Bun\u00b7de", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Es schickt sich Alles zum w\u00fcthigen Streit.", "tokens": ["Es", "schickt", "sich", "Al\u00b7les", "zum", "w\u00fct\u00b7hi\u00b7gen", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Musen, die Gerechtigkeit,", "tokens": ["Die", "Mu\u00b7sen", ",", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Handel und der Kunstflei\u00df flieht;", "tokens": ["Der", "Han\u00b7del", "und", "der", "Kunst\u00b7flei\u00df", "flieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Andacht weinend gen Himmel sieht.", "tokens": ["Die", "An\u00b7dacht", "wei\u00b7nend", "gen", "Him\u00b7mel", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Inde\u00df z\u00fcndet der Unglaub' und Wahn", "tokens": ["In\u00b7de\u00df", "z\u00fcn\u00b7det", "der", "Un\u00b7glaub'", "und", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Am H\u00f6llenfeuer die Pechfackel an,", "tokens": ["Am", "H\u00f6l\u00b7len\u00b7feu\u00b7er", "die", "Pech\u00b7fa\u00b7ckel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und sagt und schw\u00f6rt es theuer,", "tokens": ["Und", "sagt", "und", "schw\u00f6rt", "es", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Es sei ", "tokens": ["Es", "sei"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Da rennen die Leut' \u00fcber Stock und Stein", "tokens": ["Da", "ren\u00b7nen", "die", "Leut'", "\u00fc\u00b7ber", "Stock", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "In stinkende Pf\u00fctzen und S\u00fcmpfe hinein.", "tokens": ["In", "stin\u00b7ken\u00b7de", "Pf\u00fct\u00b7zen", "und", "S\u00fcmp\u00b7fe", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "Auch herrschen Weiblein \u00fcberall,", "tokens": ["Auch", "herr\u00b7schen", "Weib\u00b7lein", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In H\u00fctten und im F\u00fcrstensaal,", "tokens": ["In", "H\u00fct\u00b7ten", "und", "im", "F\u00fcrs\u00b7ten\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Thun so wunderlich kosen und minnen,", "tokens": ["Thun", "so", "wun\u00b7der\u00b7lich", "ko\u00b7sen", "und", "min\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "VVINF", "KON", "ADJA", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Bis sie Herkulesse bringen zum Spinnen.", "tokens": ["Bis", "sie", "Her\u00b7ku\u00b7les\u00b7se", "brin\u00b7gen", "zum", "Spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.17": {"text": "F\u00fcrwahr die Welt", "tokens": ["F\u00fcr\u00b7wahr", "die", "Welt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Ist trefflich bestellt! \u2013", "tokens": ["Ist", "treff\u00b7lich", "be\u00b7stellt", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "S\u00e4h Gott nicht droben zum Sternfenster 'raus", "tokens": ["S\u00e4h", "Gott", "nicht", "dro\u00b7ben", "zum", "Stern\u00b7fens\u00b7ter", "'raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PTKNEG", "ADV", "APPRART", "NN", "NE"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Und lenkte den Wirrwarr, wie s\u00e4h es aus!", "tokens": ["Und", "lenk\u00b7te", "den", "Wirr\u00b7warr", ",", "wie", "s\u00e4h", "es", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}}}}