{"textgrid.poem.34471": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[die Dummheit spricht aus deinem zarten Antlitz]", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Dummheit spricht aus deinem zarten Antlitz,", "tokens": ["Die", "Dumm\u00b7heit", "spricht", "aus", "dei\u00b7nem", "zar\u00b7ten", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Dummheit schaut aus deinen tiefen Augen,", "tokens": ["die", "Dumm\u00b7heit", "schaut", "aus", "dei\u00b7nen", "tie\u00b7fen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und \u00f6ffnest du das rote, s\u00fcsse M\u00fcndchen,", "tokens": ["und", "\u00f6ff\u00b7nest", "du", "das", "ro\u00b7te", ",", "s\u00fcs\u00b7se", "M\u00fcnd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so ists, als \u00f6ffne sich der Quell der Dummheit!", "tokens": ["so", "ists", ",", "als", "\u00f6ff\u00b7ne", "sich", "der", "Quell", "der", "Dumm\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum, wie mich auch dein wundersch\u00f6ner Leib", "tokens": ["Drum", ",", "wie", "mich", "auch", "dein", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Leib"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PWAV", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "berauscht und immer wieder noch berauscht,", "tokens": ["be\u00b7rauscht", "und", "im\u00b7mer", "wie\u00b7der", "noch", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "einmal muss ich dir doch den Abschied geben:", "tokens": ["ein\u00b7mal", "muss", "ich", "dir", "doch", "den", "Ab\u00b7schied", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "denn deine Dummheit ist nicht zu ertragen.", "tokens": ["denn", "dei\u00b7ne", "Dumm\u00b7heit", "ist", "nicht", "zu", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u2013 Du glaubst, dem Schmerz der Trennung zu erliegen?", "tokens": ["\u2013", "Du", "glaubst", ",", "dem", "Schmerz", "der", "Tren\u00b7nung", "zu", "er\u00b7lie\u00b7gen", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O tr\u00f6ste dich, mein liebes, gutes M\u00e4dchen:", "tokens": ["O", "tr\u00f6s\u00b7te", "dich", ",", "mein", "lie\u00b7bes", ",", "gu\u00b7tes", "M\u00e4d\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "den Schmerz zu f\u00fchlen, bist du auch zu dumm.", "tokens": ["den", "Schmerz", "zu", "f\u00fch\u00b7len", ",", "bist", "du", "auch", "zu", "dumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}