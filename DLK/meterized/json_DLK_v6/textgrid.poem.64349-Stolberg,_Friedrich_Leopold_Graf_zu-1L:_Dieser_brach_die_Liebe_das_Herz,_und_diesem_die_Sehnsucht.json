{"textgrid.poem.64349": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dieser brach die Liebe das Herz, und diesem die Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieser brach die Liebe das Herz, und diesem die Sehnsucht", "tokens": ["Die\u00b7ser", "brach", "die", "Lie\u00b7be", "das", "Herz", ",", "und", "die\u00b7sem", "die", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KON", "PDAT", "ART", "NN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nach der Toten: im Tod nannt' er mit Namen sie noch.", "tokens": ["Nach", "der", "To\u00b7ten", ":", "im", "Tod", "nannt'", "er", "mit", "Na\u00b7men", "sie", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "PPER", "ADV", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Freunde gesellten den Staub zum Staude seiner Geliebten;", "tokens": ["Freun\u00b7de", "ge\u00b7sell\u00b7ten", "den", "Staub", "zum", "Stau\u00b7de", "sei\u00b7ner", "Ge\u00b7lieb\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Beide lieben sich dort, wo die Liebe nicht weint.", "tokens": ["Bei\u00b7de", "lie\u00b7ben", "sich", "dort", ",", "wo", "die", "Lie\u00b7be", "nicht", "weint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word",