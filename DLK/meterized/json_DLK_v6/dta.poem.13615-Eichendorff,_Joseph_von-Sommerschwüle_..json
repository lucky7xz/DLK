{"dta.poem.13615": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sommerschw\u00fcle .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich klimm' zum Berg und schau' zur niedern Erde,", "tokens": ["Ich", "klimm'", "zum", "Berg", "und", "schau'", "zur", "nie\u00b7dern", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich klimm' hinab und schau' die Berge an,", "tokens": ["Ich", "klimm'", "hin\u00b7ab", "und", "schau'", "die", "Ber\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "S\u00fc\u00df-melancholisch spitzt sich die Geberde", "tokens": ["S\u00fc\u00df\u00b7me\u00b7lan\u00b7cho\u00b7lisch", "spitzt", "sich", "die", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und gift'ge Weltverachtung ficht mich an;", "tokens": ["Und", "gift'\u00b7ge", "Welt\u00b7ver\u00b7ach\u00b7tung", "ficht", "mich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch will aus Schmerz und Ha\u00df nichts Rechtes werden.", "tokens": ["Doch", "will", "aus", "Schmerz", "und", "Ha\u00df", "nichts", "Rech\u00b7tes", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "KON", "NN", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ermanne Dich! \u2014 Ich bin doch wohl ein Mann? \u2014", "tokens": ["Er\u00b7man\u00b7ne", "Dich", "!", "Ich", "bin", "doch", "wohl", "ein", "Mann", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und ach! wie tr\u00e4ge Sylb' aus Sylbe schleichet,", "tokens": ["Und", "ach", "!", "wie", "tr\u00e4\u00b7ge", "Sylb'", "aus", "Syl\u00b7be", "schlei\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "ADJA", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit Noth hab' ich den letzten Reim erreichet.", "tokens": ["Mit", "Noth", "hab'", "ich", "den", "letz\u00b7ten", "Reim", "er\u00b7rei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}