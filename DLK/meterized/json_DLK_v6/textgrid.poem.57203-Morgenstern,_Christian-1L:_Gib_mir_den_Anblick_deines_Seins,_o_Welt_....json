{"textgrid.poem.57203": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gib mir den Anblick deines Seins, o Welt ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gib mir den Anblick deines Seins, o Welt ...", "tokens": ["Gib", "mir", "den", "An\u00b7blick", "dei\u00b7nes", "Seins", ",", "o", "Welt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPOSAT", "NN", "$,", "FM", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Sinnenschein la\u00df langsam mich durchdringen ...", "tokens": ["Den", "Sin\u00b7nen\u00b7schein", "la\u00df", "lang\u00b7sam", "mich", "durch\u00b7drin\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So wie ein Haus sich nach und nach erhellt,", "tokens": ["So", "wie", "ein", "Haus", "sich", "nach", "und", "nach", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "PRF", "APPR", "KON", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bis es des Tages Strahlen ganz durchschwingen \u2013", "tokens": ["bis", "es", "des", "Ta\u00b7ges", "Strah\u00b7len", "ganz", "durch\u00b7schwin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und so wie wenn dies Haus dem Himmelsglanz", "tokens": ["und", "so", "wie", "wenn", "dies", "Haus", "dem", "Him\u00b7mels\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "KOUS", "PDS", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "noch Dach und Wand zum Opfer k\u00f6nnte bringen \u2013", "tokens": ["noch", "Dach", "und", "Wand", "zum", "Op\u00b7fer", "k\u00f6nn\u00b7te", "brin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "da\u00df es zuletzt, von goldner F\u00fclle ganz", "tokens": ["da\u00df", "es", "zu\u00b7letzt", ",", "von", "gold\u00b7ner", "F\u00fcl\u00b7le", "ganz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "durchstr\u00f6mt, als wie ein Geisterbauwerk st\u00e4nde,", "tokens": ["durch\u00b7str\u00f6mt", ",", "als", "wie", "ein", "Geis\u00b7ter\u00b7bau\u00b7werk", "st\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "gleich einer geistdurchleuchteten Monstranz:", "tokens": ["gleich", "ei\u00b7ner", "geist\u00b7durch\u00b7leuch\u00b7te\u00b7ten", "Mons\u00b7tranz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So m\u00f6chte auch die Starrheit meiner W\u00e4nde", "tokens": ["So", "m\u00f6ch\u00b7te", "auch", "die", "Star\u00b7rheit", "mei\u00b7ner", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sich l\u00f6sen, da\u00df dein volles Sein in mein,", "tokens": ["sich", "l\u00f6\u00b7sen", ",", "da\u00df", "dein", "vol\u00b7les", "Sein", "in", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mein volles Sein in dein Sein Einla\u00df f\u00e4nde \u2013", "tokens": ["mein", "vol\u00b7les", "Sein", "in", "dein", "Sein", "Ein\u00b7la\u00df", "f\u00e4n\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und so sich rein vereinte Sein mit Sein.", "tokens": ["und", "so", "sich", "rein", "ver\u00b7ein\u00b7te", "Sein", "mit", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "ADJD", "VVFIN", "PPOSAT", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}