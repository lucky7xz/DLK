{"textgrid.poem.43485": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du gro\u00dfe Kr\u00e4mernation,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gro\u00dfe Kr\u00e4mernation,", "tokens": ["Du", "gro\u00b7\u00dfe", "Kr\u00e4\u00b7mer\u00b7na\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du reichstes Volk der armen Welt,", "tokens": ["Du", "reichs\u00b7tes", "Volk", "der", "ar\u00b7men", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du singst von Freiheit, von Freiheit im Jubelton,", "tokens": ["Du", "singst", "von", "Frei\u00b7heit", ",", "von", "Frei\u00b7heit", "im", "Ju\u00b7bel\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und meinst doch immer nur das Geld.", "tokens": ["Und", "meinst", "doch", "im\u00b7mer", "nur", "das", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Britannia, Britannia, werde brav!", "tokens": ["O", "Bri\u00b7tan\u00b7nia", ",", "Bri\u00b7tan\u00b7nia", ",", "wer\u00b7de", "brav", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bleib doch nicht des Geldes Sklav!", "tokens": ["Bleib", "doch", "nicht", "des", "Gel\u00b7des", "Sklav", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stolzier' und prahle nicht so sehr!", "tokens": ["Stol\u00b7zier'", "und", "prah\u00b7le", "nicht", "so", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Herrschen ist f\u00fcrwahr ein kleins,", "tokens": ["Dein", "Herr\u00b7schen", "ist", "f\u00fcr\u00b7wahr", "ein", "kleins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn dich beherrschet, beherrschet auf dem Meer", "tokens": ["Denn", "dich", "be\u00b7herr\u00b7schet", ",", "be\u00b7herr\u00b7schet", "auf", "dem", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie \u00fcberall das Einmaleins.", "tokens": ["Wie", "\u00fc\u00b7be\u00b7rall", "das", "Ein\u00b7mal\u00b7eins", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Britannia, Britannia, werde brav!", "tokens": ["O", "Bri\u00b7tan\u00b7nia", ",", "Bri\u00b7tan\u00b7nia", ",", "wer\u00b7de", "brav", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bleib doch nicht des Geldes Sklav!", "tokens": ["Bleib", "doch", "nicht", "des", "Gel\u00b7des", "Sklav", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O w\u00e4rst du was du k\u00f6nntest sein,", "tokens": ["O", "w\u00e4rst", "du", "was", "du", "k\u00f6nn\u00b7test", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PWS", "PPER", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freiheit Schild und Schutzpanier,", "tokens": ["Der", "Frei\u00b7heit", "Schild", "und", "Schutz\u00b7pa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, f\u00fcr Europa nur allein!", "tokens": ["Ach", ",", "f\u00fcr", "Eu\u00b7ro\u00b7pa", "nur", "al\u00b7lein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NE", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ganze Weltall, das Weltall s\u00e4nge dir:", "tokens": ["Das", "gan\u00b7ze", "Wel\u00b7tall", ",", "das", "Wel\u00b7tall", "s\u00e4n\u00b7ge", "dir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Herrsch', Britannia, Britannia gro\u00df und hehr!", "tokens": ["Herr\u00b7sch'", ",", "Bri\u00b7tan\u00b7nia", ",", "Bri\u00b7tan\u00b7nia", "gro\u00df", "und", "hehr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "NE", "$,", "NE", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Dank dir, Dank singt Land und Meer.", "tokens": ["Dank", "dir", ",", "Dank", "singt", "Land", "und", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}