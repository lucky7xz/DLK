{"textgrid.poem.24133": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was will ich mehr? Auf meinem Tische stehn", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was will ich mehr? Auf meinem Tische stehn", "tokens": ["Was", "will", "ich", "mehr", "?", "Auf", "mei\u00b7nem", "Ti\u00b7sche", "stehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In sch\u00f6nem Glase dunkelrote Rosen,", "tokens": ["In", "sch\u00f6\u00b7nem", "Gla\u00b7se", "dun\u00b7kel\u00b7ro\u00b7te", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der wei\u00dfe Marmor-Goethe sieht mich an,", "tokens": ["Der", "wei\u00b7\u00dfe", "Mar\u00b7mor\u00b7Goe\u00b7the", "sieht", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und eben h\u00f6rt ich Mozarts Figaro.", "tokens": ["Und", "e\u00b7ben", "h\u00f6rt", "ich", "Mo\u00b7zarts", "Fi\u00b7ga\u00b7ro", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich litt einst Schmerz? Ich war einst m\u00fcd und krank?", "tokens": ["Ich", "litt", "einst", "Schmerz", "?", "Ich", "war", "einst", "m\u00fcd", "und", "krank", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich log mir Gl\u00fcck und dichtete ein Wunder", "tokens": ["Ich", "log", "mir", "Gl\u00fcck", "und", "dich\u00b7te\u00b7te", "ein", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Weib, das nichts als gute Maske war? \u2013:", "tokens": ["Von", "Weib", ",", "das", "nichts", "als", "gu\u00b7te", "Mas\u00b7ke", "war", "?", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIS", "KOKOM", "ADJA", "NN", "VAFIN", "$.", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Rosen gl\u00fchen: Alles war ein Traum,", "tokens": ["Die", "Ro\u00b7sen", "gl\u00fc\u00b7hen", ":", "Al\u00b7les", "war", "ein", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "PIS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der wei\u00dfe Goethe leuchtet Heiterkeit,", "tokens": ["Der", "wei\u00b7\u00dfe", "Goe\u00b7the", "leuch\u00b7tet", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und in mir singt Susanne, Cherubin.", "tokens": ["Und", "in", "mir", "singt", "Su\u00b7san\u00b7ne", ",", "Che\u00b7ru\u00b7bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie aber: Hab ich denn nicht Kummers viel?", "tokens": ["Wie", "a\u00b7ber", ":", "Hab", "ich", "denn", "nicht", "Kum\u00b7mers", "viel", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "NN", "PPER", "ADV", "PTKNEG", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verliebten Zweifel und des Schaffens Angst? \u2013:", "tokens": ["Ver\u00b7lieb\u00b7ten", "Zwei\u00b7fel", "und", "des", "Schaf\u00b7fens", "Angst", "?", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "NN", "NN", "$.", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die roten Rosen gl\u00fchen: Sieh uns an,", "tokens": ["Die", "ro\u00b7ten", "Ro\u00b7sen", "gl\u00fc\u00b7hen", ":", "Sieh", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "NE", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der wei\u00dfe Goethe l\u00e4chelt: Denk an mich,", "tokens": ["Der", "wei\u00b7\u00dfe", "Goe\u00b7the", "l\u00e4\u00b7chelt", ":", "Denk", "an", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Mozart singt mich s\u00fc\u00df und heiter ein.", "tokens": ["Und", "Mo\u00b7zart", "singt", "mich", "s\u00fc\u00df", "und", "hei\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich frevelte, wollt ich nicht gl\u00fccklich sein.", "tokens": ["Ich", "fre\u00b7vel\u00b7te", ",", "wollt", "ich", "nicht", "gl\u00fcck\u00b7lich", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}