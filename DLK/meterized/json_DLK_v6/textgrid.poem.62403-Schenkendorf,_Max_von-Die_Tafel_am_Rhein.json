{"textgrid.poem.62403": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Die Tafel am Rhein", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der S\u00e4nger kommt zur guten Stunde", "tokens": ["Der", "S\u00e4n\u00b7ger", "kommt", "zur", "gu\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihn empf\u00e4ngt ein holder Gru\u00df,", "tokens": ["Und", "ihn", "emp\u00b7f\u00e4ngt", "ein", "hol\u00b7der", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Feldherrn und die Tafelrunde", "tokens": ["Den", "Feld\u00b7herrn", "und", "die", "Ta\u00b7fel\u00b7run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erblickt er an dem gr\u00fcnen Flu\u00df.", "tokens": ["Er\u00b7blickt", "er", "an", "dem", "gr\u00fc\u00b7nen", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Feldherr l\u00e4\u00dft den Becher f\u00fcllen", "tokens": ["Der", "Feld\u00b7herr", "l\u00e4\u00dft", "den", "Be\u00b7cher", "f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit altem Wein von R\u00fcdesheim:", "tokens": ["Mit", "al\u00b7tem", "Wein", "von", "R\u00fc\u00b7des\u00b7heim", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du kannst, o Herr, die Sehnsucht stillen,", "tokens": ["Du", "kannst", ",", "o", "Herr", ",", "die", "Sehn\u00b7sucht", "stil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "FM", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein frischer Trunk weckt frischen Reim.", "tokens": ["Ein", "fri\u00b7scher", "Trunk", "weckt", "fri\u00b7schen", "Reim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den Becher heb' ich in die L\u00fcfte,", "tokens": ["Den", "Be\u00b7cher", "heb'", "ich", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halb trink ich ihn, und gie\u00df ihn aus,", "tokens": ["Halb", "trink", "ich", "ihn", ",", "und", "gie\u00df", "ihn", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spreng' ihn auf die Rasengr\u00fcfte,", "tokens": ["Und", "spreng'", "ihn", "auf", "die", "Ra\u00b7sen\u00b7gr\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf unsrer V\u00e4ter stilles Haus.", "tokens": ["Auf", "uns\u00b7rer", "V\u00e4\u00b7ter", "stil\u00b7les", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun eingeweiht mit Blut und Weine,", "tokens": ["Nun", "ein\u00b7ge\u00b7weiht", "mit", "Blut", "und", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Land, mein Heldenvaterland,", "tokens": ["Mein", "Land", ",", "mein", "Hel\u00b7den\u00b7va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O starker Flu\u00df, ihr dunkeln Haine,", "tokens": ["O", "star\u00b7ker", "Flu\u00df", ",", "ihr", "dun\u00b7keln", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der S\u00e4nger weiht euch Brust und Hand!", "tokens": ["Der", "S\u00e4n\u00b7ger", "weiht", "euch", "Brust", "und", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Freiheit la\u00df ich nun erschallen", "tokens": ["Der", "Frei\u00b7heit", "la\u00df", "ich", "nun", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein zweites Wort, mein k\u00fchnstes Lied,", "tokens": ["Mein", "zwei\u00b7tes", "Wort", ",", "mein", "k\u00fchns\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Heldenbraut, die von den Hallen", "tokens": ["Der", "Hel\u00b7den\u00b7braut", ",", "die", "von", "den", "Hal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Sternendoms hernieder sieht.", "tokens": ["Des", "Ster\u00b7nen\u00b7doms", "her\u00b7nie\u00b7der", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hat uns unser Herz genommen,", "tokens": ["Sie", "hat", "uns", "un\u00b7ser", "Herz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat hoch entz\u00fcndet unsern Muth:", "tokens": ["Hat", "hoch", "ent\u00b7z\u00fcn\u00b7det", "un\u00b7sern", "Muth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O s\u00fc\u00dfe Maid, wann willst du kommen", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Maid", ",", "wann", "willst", "du", "kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit deinen Pfeilen, deinem Hut?", "tokens": ["Mit", "dei\u00b7nen", "Pfei\u00b7len", ",", "dei\u00b7nem", "Hut", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Sch\u00f6nsten jetzt, die still im Herzen", "tokens": ["Der", "Sch\u00f6ns\u00b7ten", "jetzt", ",", "die", "still", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Jeder nennt und Jeder meint,", "tokens": ["Ein", "Je\u00b7der", "nennt", "und", "Je\u00b7der", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Guten, die mit Spiel und Scherzen", "tokens": ["Der", "Gu\u00b7ten", ",", "die", "mit", "Spiel", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den wunderbaren Ernst vereint.", "tokens": ["Den", "wun\u00b7der\u00b7ba\u00b7ren", "Ernst", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sendet uns in ferne Schlachten,", "tokens": ["Sie", "sen\u00b7det", "uns", "in", "fer\u00b7ne", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir ziehn um seligen Gewinnst,", "tokens": ["Wir", "ziehn", "um", "se\u00b7li\u00b7gen", "Ge\u00b7winnst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie wir d\u00fcrsten, wie wir schmachten,", "tokens": ["Und", "wie", "wir", "d\u00fcrs\u00b7ten", ",", "wie", "wir", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VMFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wir sind begl\u00fcckt in ihrem Dienst.", "tokens": ["Wir", "sind", "be\u00b7gl\u00fcckt", "in", "ih\u00b7rem", "Dienst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den Feldherrn sing' ich und die Waffen,", "tokens": ["Den", "Feld\u00b7herrn", "sing'", "ich", "und", "die", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die k\u00fchn das Vaterland befreit,", "tokens": ["Die", "k\u00fchn", "das", "Va\u00b7ter\u00b7land", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie m\u00f6gen ewig Recht verschaffen", "tokens": ["Sie", "m\u00f6\u00b7gen", "e\u00b7wig", "Recht", "ver\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Sieg der theuern Christenheit.", "tokens": ["Und", "Sieg", "der", "theu\u00b7ern", "Chris\u00b7ten\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So hab' ich wol im Knabentraume", "tokens": ["So", "hab'", "ich", "wol", "im", "Kna\u00b7ben\u00b7trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die alte Ritterschaft gesehn,", "tokens": ["Die", "al\u00b7te", "Rit\u00b7ter\u00b7schaft", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich sehe gleich dem Eichenbaume", "tokens": ["Ich", "se\u00b7he", "gleich", "dem", "Ei\u00b7chen\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Waffenschmuck den Feldherrn stehn.", "tokens": ["Im", "Waf\u00b7fen\u00b7schmuck", "den", "Feld\u00b7herrn", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich seh' ihn strafend ab sich wenden", "tokens": ["Ich", "seh'", "ihn", "stra\u00b7fend", "ab", "sich", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Feldherrn, der vor Demuth gl\u00fcht,", "tokens": ["Den", "Feld\u00b7herrn", ",", "der", "vor", "De\u00b7muth", "gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun darf ich nicht mein Lied vollenden,", "tokens": ["Nun", "darf", "ich", "nicht", "mein", "Lied", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Leben ist ein Heldenlied.", "tokens": ["Sein", "Le\u00b7ben", "ist", "ein", "Hel\u00b7den\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Klingt hell dazu ihr Glockenspiele,", "tokens": ["Klingt", "hell", "da\u00b7zu", "ihr", "Glo\u00b7cken\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr alten Th\u00fcrme schaut herein,", "tokens": ["Ihr", "al\u00b7ten", "Th\u00fcr\u00b7me", "schaut", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O komm aus tiefer Nacht und K\u00fchle,", "tokens": ["O", "komm", "aus", "tie\u00b7fer", "Nacht", "und", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du Sonnenkind, komm edler Wein!", "tokens": ["Du", "Son\u00b7nen\u00b7kind", ",", "komm", "ed\u00b7ler", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der S\u00e4nger schweigt, er f\u00e4hrt hinunter,", "tokens": ["Der", "S\u00e4n\u00b7ger", "schweigt", ",", "er", "f\u00e4hrt", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf leichtem Kahn den gr\u00fcnen Flu\u00df,", "tokens": ["Auf", "leich\u00b7tem", "Kahn", "den", "gr\u00fc\u00b7nen", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bunter wird's und immer bunter,", "tokens": ["Und", "bun\u00b7ter", "wird's", "und", "im\u00b7mer", "bun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es kommt geflogen Gru\u00df auf Gru\u00df.", "tokens": ["Es", "kommt", "ge\u00b7flo\u00b7gen", "Gru\u00df", "auf", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn der letzte Ton verklungen,", "tokens": ["Und", "wenn", "der", "letz\u00b7te", "Ton", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ins Meer der letzte Tropfen rann,", "tokens": ["Ins", "Meer", "der", "letz\u00b7te", "Trop\u00b7fen", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So f\u00e4ngt ein Lied in h\u00f6hern Zungen,", "tokens": ["So", "f\u00e4ngt", "ein", "Lied", "in", "h\u00f6\u00b7hern", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im h\u00f6hern Licht ein Leben an.", "tokens": ["Im", "h\u00f6\u00b7hern", "Licht", "ein", "Le\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}