{"textgrid.poem.63777": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Nach Hause!", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den letzten Gru\u00df herab von den Terrassen", "tokens": ["Den", "letz\u00b7ten", "Gru\u00df", "her\u00b7ab", "von", "den", "Ter\u00b7ras\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Pincio dir, du Sonne Roms! In Glut", "tokens": ["Des", "Pin\u00b7cio", "dir", ",", "du", "Son\u00b7ne", "Roms", "!", "In", "Glut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "$,", "PPER", "NN", "NE", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tauchst du die H\u00fcgel rings in deiner Hut,", "tokens": ["Tauchst", "du", "die", "H\u00fc\u00b7gel", "rings", "in", "dei\u00b7ner", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Eh' sie f\u00fcr immer meinem Aug erblassen.", "tokens": ["Eh'", "sie", "f\u00fcr", "im\u00b7mer", "mei\u00b7nem", "Aug", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zum letztenmal umwogt mich in den Gassen", "tokens": ["Zum", "letz\u00b7ten\u00b7mal", "um\u00b7wogt", "mich", "in", "den", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die heimw\u00e4rtsstr\u00f6mend rege Menschenflut.", "tokens": ["Die", "heim\u00b7w\u00e4rts\u00b7str\u00f6\u00b7mend", "re\u00b7ge", "Men\u00b7schen\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nachtstimmen Roms \u2013 wie kenn' ich euch so gut", "tokens": ["Nachts\u00b7tim\u00b7men", "Roms", "\u2013", "wie", "kenn'", "ich", "euch", "so", "gut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$(", "PWAV", "VVFIN", "PPER", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und soll euch morgen fern verbrausen lassen? \u2013", "tokens": ["Und", "soll", "euch", "mor\u00b7gen", "fern", "ver\u00b7brau\u00b7sen", "las\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch da ich lag in kurzem Schlummer kaum,", "tokens": ["Doch", "da", "ich", "lag", "in", "kur\u00b7zem", "Schlum\u00b7mer", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tr\u00e4umt' ich, das W\u00e4ldchen h\u00f6rt' ich wieder rauschen", "tokens": ["Tr\u00e4umt'", "ich", ",", "das", "W\u00e4ld\u00b7chen", "h\u00f6rt'", "ich", "wie\u00b7der", "rau\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An meinem Haus im Hauch des deutschen Windes.", "tokens": ["An", "mei\u00b7nem", "Haus", "im", "Hauch", "des", "deut\u00b7schen", "Win\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und helle Sehnsucht rei\u00dft mich aus dem Traum,", "tokens": ["Und", "hel\u00b7le", "Sehn\u00b7sucht", "rei\u00dft", "mich", "aus", "dem", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Morgenlied des Amselpaars zu lauschen,", "tokens": ["Dem", "Mor\u00b7gen\u00b7lied", "des", "Am\u00b7sel\u00b7paars", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Spielgef\u00e4hrten meines lieben Kindes.", "tokens": ["Der", "Spiel\u00b7ge\u00b7f\u00e4hr\u00b7ten", "mei\u00b7nes", "lie\u00b7ben", "Kin\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}