{"textgrid.poem.65590": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "63. Geldstolz", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist kein Stolz so erdentoll, wie der auf deines Beutels Last.", "tokens": ["Es", "ist", "kein", "Stolz", "so", "er\u00b7den\u00b7toll", ",", "wie", "der", "auf", "dei\u00b7nes", "Beu\u00b7tels", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "$,", "PWAV", "ART", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Speis' alle Bettler heut', und sieh, was f\u00fcr das Geld du morgen hast!", "tokens": ["Speis'", "al\u00b7le", "Bett\u00b7ler", "heut'", ",", "und", "sieh", ",", "was", "f\u00fcr", "das", "Geld", "du", "mor\u00b7gen", "hast", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADV", "$,", "KON", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}