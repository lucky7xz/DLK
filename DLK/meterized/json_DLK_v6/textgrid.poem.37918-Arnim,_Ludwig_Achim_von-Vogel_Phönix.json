{"textgrid.poem.37918": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Vogel Ph\u00f6nix", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ph\u00f6nix, der edle Vogel werth,", "tokens": ["Ph\u00f6\u00b7nix", ",", "der", "ed\u00b7le", "Vo\u00b7gel", "werth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat seines Gleichen nicht auf Erd,", "tokens": ["Hat", "sei\u00b7nes", "Glei\u00b7chen", "nicht", "auf", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Um seinen Hals ist's goldgelb klar,", "tokens": ["Um", "sei\u00b7nen", "Hals", "ist's", "gold\u00b7gelb", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Leib und Fl\u00fcgel Purpur gar;", "tokens": ["Sein", "Leib", "und", "Fl\u00fc\u00b7gel", "Pur\u00b7pur", "gar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hat auf dem Haupte eine Kron,", "tokens": ["Hat", "auf", "dem", "Haup\u00b7te", "ei\u00b7ne", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der h\u00f6chste Baum sein hoher Thron.", "tokens": ["Der", "h\u00f6chs\u00b7te", "Baum", "sein", "ho\u00b7her", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er wohnt und lebet lang allein,", "tokens": ["Er", "wohnt", "und", "le\u00b7bet", "lang", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann stellen sich viel V\u00f6gel ein.", "tokens": ["Dann", "stel\u00b7len", "sich", "viel", "V\u00f6\u00b7gel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die V\u00f6gel sammeln f\u00fcr ihn frey", "tokens": ["Die", "V\u00f6\u00b7gel", "sam\u00b7meln", "f\u00fcr", "ihn", "frey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Weihrauch und die Specerey,", "tokens": ["Den", "Weih\u00b7rauch", "und", "die", "Spe\u00b7ce\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Von edlem Holz wohlriechend Aest,", "tokens": ["Von", "ed\u00b7lem", "Holz", "wohl\u00b7rie\u00b7chend", "A\u00b7est", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie machen aus dem alln ein Nest.", "tokens": ["Sie", "ma\u00b7chen", "aus", "dem", "alln", "ein", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann schwingt er dr\u00fcber sein Gefieder", "tokens": ["Dann", "schwingt", "er", "dr\u00fc\u00b7ber", "sein", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Sonnenglanze auf und nieder.", "tokens": ["Am", "Son\u00b7nen\u00b7glan\u00b7ze", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn er das Rauchwerk so gez\u00fcndt,", "tokens": ["Wenn", "er", "das", "Rauch\u00b7werk", "so", "ge\u00b7z\u00fcndt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Flamme sich zur H\u00f6he windt.", "tokens": ["Die", "Flam\u00b7me", "sich", "zur", "H\u00f6\u00b7he", "windt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dann l\u00e4\u00dft er sich herab zur Gluth,", "tokens": ["Dann", "l\u00e4\u00dft", "er", "sich", "her\u00b7ab", "zur", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verbrennt sich willig wohlgemuth.", "tokens": ["Ver\u00b7brennt", "sich", "wil\u00b7lig", "wohl\u00b7ge\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Alsdann in seiner Asche wird", "tokens": ["Als\u00b7dann", "in", "sei\u00b7ner", "A\u00b7sche", "wird"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein leuchtend W\u00fcrmlein erst formirt,", "tokens": ["Ein", "leuch\u00b7tend", "W\u00fcrm\u00b7lein", "erst", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Darnach ein Vogel rein und pur,", "tokens": ["Dar\u00b7nach", "ein", "Vo\u00b7gel", "rein", "und", "pur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NE", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem vor'gen gleich in der Natur.", "tokens": ["Dem", "vor'\u00b7gen", "gleich", "in", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Christus, des Himmels Ph\u00f6nix rein,", "tokens": ["Chris\u00b7tus", ",", "des", "Him\u00b7mels", "Ph\u00f6\u00b7nix", "rein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat so gewohnt auf Erd' allein,", "tokens": ["Hat", "so", "ge\u00b7wohnt", "auf", "Erd'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Ein Adler stark, der \u00fcberwand", "tokens": ["Ein", "Ad\u00b7ler", "stark", ",", "der", "\u00fc\u00b7ber\u00b7wand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6ll, Teufel, S\u00fcnd und Todesband.", "tokens": ["H\u00f6ll", ",", "Teu\u00b7fel", ",", "S\u00fcnd", "und", "To\u00b7des\u00b7band", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Sein Gottheit ist die g\u00fcldne Farb,", "tokens": ["Sein", "Got\u00b7theit", "ist", "die", "g\u00fcld\u00b7ne", "Farb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sein Verdienst uns Heil erwarb.", "tokens": ["Und", "sein", "Ver\u00b7dienst", "uns", "Heil", "er\u00b7warb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Das Purpur-Kleid er hat auch an,", "tokens": ["Das", "Pur\u00b7pur\u00b7Kleid", "er", "hat", "auch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinem Haupt die Dornenkron.", "tokens": ["Auf", "sei\u00b7nem", "Haupt", "die", "Dor\u00b7nen\u00b7kron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Aus rechter Lieb inbr\u00fcnstiglich", "tokens": ["Aus", "rech\u00b7ter", "Lieb", "in\u00b7br\u00fcns\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er opfert darauf willig sich.", "tokens": ["Er", "op\u00b7fert", "da\u00b7rauf", "wil\u00b7lig", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADJD", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Und man begrub ihn ehrlich frey,", "tokens": ["Und", "man", "be\u00b7grub", "ihn", "ehr\u00b7lich", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit k\u00f6stlich edler Specerey.", "tokens": ["Mit", "k\u00f6st\u00b7lich", "ed\u00b7ler", "Spe\u00b7ce\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Also des Himmels Ph\u00f6nix lag,", "tokens": ["Al\u00b7so", "des", "Him\u00b7mels", "Ph\u00f6\u00b7nix", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Im Grab, bis an den dritten Tag,", "tokens": ["Im", "Grab", ",", "bis", "an", "den", "drit\u00b7ten", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Alsdann er wieder lebend wurd'", "tokens": ["Als\u00b7dann", "er", "wie\u00b7der", "le\u00b7bend", "wurd'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch seine ew'ge Geistsgeburt.", "tokens": ["Durch", "sei\u00b7ne", "ew'\u00b7ge", "Geists\u00b7ge\u00b7burt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}