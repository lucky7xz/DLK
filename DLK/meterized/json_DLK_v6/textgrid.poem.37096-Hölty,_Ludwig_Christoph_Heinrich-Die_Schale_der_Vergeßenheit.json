{"textgrid.poem.37096": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Schale der Verge\u00dfenheit", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Schale des Stroms, welcher Verge\u00dfenheit", "tokens": ["Ei\u00b7ne", "Scha\u00b7le", "des", "Stroms", ",", "wel\u00b7cher", "Ver\u00b7ge\u00b7\u00dfen\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAT", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Durch Elysiums Blumen rollt,", "tokens": ["Durch", "E\u00b7ly\u00b7si\u00b7ums", "Blu\u00b7men", "rollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Eine Schale des Stroms spende mir, Genius!", "tokens": ["Ei\u00b7ne", "Scha\u00b7le", "des", "Stroms", "spen\u00b7de", "mir", ",", "Ge\u00b7nius", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Dort wo Phaons die S\u00e4ngerin,", "tokens": ["Dort", "wo", "Phaons", "die", "S\u00e4n\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort wo Orpheus verga\u00df seiner Eurydice,", "tokens": ["Dort", "wo", "Or\u00b7pheus", "ver\u00b7ga\u00df", "sei\u00b7ner", "Eu\u00b7ry\u00b7di\u00b7ce", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NE", "VVFIN", "PPOSAT", "NE", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Sch\u00f6pf die goldene Urne voll!", "tokens": ["Sch\u00f6pf", "die", "gol\u00b7de\u00b7ne", "Ur\u00b7ne", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Dann versenk ich dein Bild, spr\u00f6de Gebieterin,", "tokens": ["Dann", "ver\u00b7senk", "ich", "dein", "Bild", ",", "spr\u00f6\u00b7de", "Ge\u00b7bie\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "In den silbernen Schlummerquell!", "tokens": ["In", "den", "sil\u00b7ber\u00b7nen", "Schlum\u00b7mer\u00b7quell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Den allsiegenden Blick, der mir im Marke zuckt,", "tokens": ["Den", "all\u00b7sie\u00b7gen\u00b7den", "Blick", ",", "der", "mir", "im", "Mar\u00b7ke", "zuckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und das Beben der wei\u00dfen Brust,", "tokens": ["Und", "das", "Be\u00b7ben", "der", "wei\u00b7\u00dfen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Und die s\u00fc\u00dfe Musik, welche der Lipp' entflo\u00df,", "tokens": ["Und", "die", "s\u00fc\u00b7\u00dfe", "Mu\u00b7sik", ",", "wel\u00b7che", "der", "Lipp'", "ent\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Tauch ich tief in den Schlummerquell!", "tokens": ["Tauch", "ich", "tief", "in", "den", "Schlum\u00b7mer\u00b7quell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}