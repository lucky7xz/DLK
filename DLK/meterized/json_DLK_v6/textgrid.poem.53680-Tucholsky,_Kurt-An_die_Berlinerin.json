{"textgrid.poem.53680": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An die Berlinerin", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.71", "fi:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen, kein Casanova", "tokens": ["M\u00e4d\u00b7chen", ",", "kein", "Ca\u00b7sa\u00b7no\u00b7va"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "h\u00e4tte dir je imponiert.", "tokens": ["h\u00e4t\u00b7te", "dir", "je", "im\u00b7po\u00b7niert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Glaubst du vielleicht, was ein doofer", "tokens": ["Glaubst", "du", "viel\u00b7leicht", ",", "was", "ein", "doo\u00b7fer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schw\u00e4rmer von dir phantasiert?", "tokens": ["Schw\u00e4r\u00b7mer", "von", "dir", "phan\u00b7ta\u00b7siert", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00e4nge mit wogenden N\u00fcstern", "tokens": ["S\u00e4n\u00b7ge", "mit", "wo\u00b7gen\u00b7den", "N\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Romeo, liebesbesiegt,", "tokens": ["Ro\u00b7meo", ",", "lie\u00b7bes\u00b7be\u00b7siegt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "w\u00fcrdest du leise fl\u00fcstern:", "tokens": ["w\u00fcr\u00b7dest", "du", "lei\u00b7se", "fl\u00fcs\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "\u00bbwoll mit die Pauke jepiekt \u2013?\u00ab", "tokens": ["\u00bb", "woll", "mit", "die", "Pau\u00b7ke", "je\u00b7piekt", "\u2013", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NE", "VVPP", "$(", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Willst du romantische Feste,", "tokens": ["Willst", "du", "ro\u00b7man\u00b7ti\u00b7sche", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "gehst du beis Kino hin . . .", "tokens": ["gehst", "du", "beis", "Ki\u00b7no", "hin", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "PTKVZ", "$.", "$.", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Du bist doch Mutterns Beste,", "tokens": ["Du", "bist", "doch", "Mut\u00b7terns", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "du, die Berlinerin \u2013!", "tokens": ["du", ",", "die", "Ber\u00b7li\u00b7ne\u00b7rin", "\u2013", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$(", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Venus der Spree \u2013 wie so flei\u00dfig", "tokens": ["Ve\u00b7nus", "der", "Spree", "\u2013", "wie", "so", "flei\u00b7\u00dfig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NE", "$(", "KOKOM", "ADV", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "liebst du, wie p\u00fcnktlich dabei!", "tokens": ["liebst", "du", ",", "wie", "p\u00fcnkt\u00b7lich", "da\u00b7bei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "PAV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Zieren bis zw\u00f6lf Uhr drei\u00dfig,", "tokens": ["Zie\u00b7ren", "bis", "zw\u00f6lf", "Uhr", "drei\u00b7\u00dfig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "CARD", "$,"], "meter": "+----+-", "measure": "dactylic.init"}, "line.4": {"text": "K\u00fcssen bis nachts um zwei.", "tokens": ["K\u00fcs\u00b7sen", "bis", "nachts", "um", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "APPR", "CARD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Alles erledigst du fachlich,", "tokens": ["Al\u00b7les", "er\u00b7le\u00b7digst", "du", "fach\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "bleibst noch im Liebesschwur", "tokens": ["bleibst", "noch", "im", "Lie\u00b7bes\u00b7schwur"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "ordentlich, sauber und sachlich:", "tokens": ["or\u00b7dent\u00b7lich", ",", "sau\u00b7ber", "und", "sach\u00b7lich", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Lebende Registratur!", "tokens": ["Le\u00b7ben\u00b7de", "Re\u00b7gist\u00b7ra\u00b7tur", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.9": {"text": "Wie dich sein Arm auch pre\u00dfte:", "tokens": ["Wie", "dich", "sein", "Arm", "auch", "pre\u00df\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "gibst dich nur her und nicht hin.", "tokens": ["gibst", "dich", "nur", "her", "und", "nicht", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "KON", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Bist ja doch Mutterns Beste,", "tokens": ["Bist", "ja", "doch", "Mut\u00b7terns", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "du, die Berlinerin \u2013!", "tokens": ["du", ",", "die", "Ber\u00b7li\u00b7ne\u00b7rin", "\u2013", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$(", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wochentags f\u00fchrst du ja gerne", "tokens": ["Wo\u00b7chen\u00b7tags", "f\u00fchrst", "du", "ja", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nadel und Lineal.", "tokens": ["Na\u00b7del", "und", "Li\u00b7ne\u00b7al", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonntags leuchten die Sterne", "tokens": ["Sonn\u00b7tags", "leuch\u00b7ten", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "preu\u00dfisch-sentimental.", "tokens": ["preu\u00b7\u00dfischsen\u00b7ti\u00b7men\u00b7tal", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Denkst du der Maulwurfstola,", "tokens": ["Denkst", "du", "der", "Maul\u00b7wurf\u00b7sto\u00b7la", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "die dir dein Freund spendiert?", "tokens": ["die", "dir", "dein", "Freund", "spen\u00b7diert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Leuchtendes Vorbild der Pola!", "tokens": ["Leuch\u00b7ten\u00b7des", "Vor\u00b7bild", "der", "Po\u00b7la", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wackle wie sie geziert.", "tokens": ["Wack\u00b7le", "wie", "sie", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "\u00c4lter wirst du. Die Reste", "tokens": ["\u00c4l\u00b7ter", "wirst", "du", ".", "Die", "Res\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "gehn mit den Jahren dahin.", "tokens": ["gehn", "mit", "den", "Jah\u00b7ren", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PAV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "La\u00df die mond\u00e4ne Geste!", "tokens": ["La\u00df", "die", "mon\u00b7d\u00e4\u00b7ne", "Ges\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Bist ja doch Mutterns Beste,", "tokens": ["Bist", "ja", "doch", "Mut\u00b7terns", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "du s\u00fc\u00dfe Berlinerin \u2013!", "tokens": ["du", "s\u00fc\u00b7\u00dfe", "Ber\u00b7li\u00b7ne\u00b7rin", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}