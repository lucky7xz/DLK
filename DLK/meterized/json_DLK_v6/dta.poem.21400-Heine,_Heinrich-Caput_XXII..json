{"dta.poem.21400": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sch\u00fcttelten uns die H\u00e4nde, es schwamm", "tokens": ["Wir", "sch\u00fct\u00b7tel\u00b7ten", "uns", "die", "H\u00e4n\u00b7de", ",", "es", "schwamm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Auge des Manns eine Thr\u00e4ne.", "tokens": ["Im", "Au\u00b7ge", "des", "Manns", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie freute er sich, mich wieder zu sehn!", "tokens": ["Wie", "freu\u00b7te", "er", "sich", ",", "mich", "wie\u00b7der", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es war eine r\u00fchrende Scene. \u2013", "tokens": ["Es", "war", "ei\u00b7ne", "r\u00fch\u00b7ren\u00b7de", "Sce\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Nicht alle fand ich. Mancher hat", "tokens": ["Nicht", "al\u00b7le", "fand", "ich", ".", "Man\u00b7cher", "hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "PIS", "VVFIN", "PPER", "$.", "PIS", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Zeitliche gesegnet.", "tokens": ["Das", "Zeit\u00b7li\u00b7che", "ge\u00b7seg\u00b7net", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach! meinem Gumpelino sogar", "tokens": ["Ach", "!", "mei\u00b7nem", "Gum\u00b7pe\u00b7li\u00b7no", "so\u00b7gar"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPOSAT", "NE", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bin ich nicht mehr begegnet.", "tokens": ["Bin", "ich", "nicht", "mehr", "be\u00b7geg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Edle hatte ausgehaucht", "tokens": ["Der", "Ed\u00b7le", "hat\u00b7te", "aus\u00b7ge\u00b7haucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gro\u00dfe Seele so eben,", "tokens": ["Die", "gro\u00b7\u00dfe", "See\u00b7le", "so", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wird als verkl\u00e4rter Seraph jetzt", "tokens": ["Und", "wird", "als", "ver\u00b7kl\u00e4r\u00b7ter", "Se\u00b7raph", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "KOKOM", "ADJA", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Throne Jehovahs schweben.", "tokens": ["Am", "Thro\u00b7ne", "Je\u00b7ho\u00b7vahs", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Vergebens suchte ich \u00fcberall", "tokens": ["Ver\u00b7ge\u00b7bens", "such\u00b7te", "ich", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den krummen Adonis, der Tassen", "tokens": ["Den", "krum\u00b7men", "A\u00b7do\u00b7nis", ",", "der", "Tas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Nachtgeschirr von Porzelan", "tokens": ["Und", "Nacht\u00b7ge\u00b7schirr", "von", "Por\u00b7ze\u00b7lan"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Feil bot in Hamburgs Gassen.", "tokens": ["Feil", "bot", "in", "Ham\u00b7burgs", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}