{"textgrid.poem.58665": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir werden nimmer heilig ganz durch Wirksamsein alleine,", "tokens": ["Wir", "wer\u00b7den", "nim\u00b7mer", "hei\u00b7lig", "ganz", "durch", "Wirk\u00b7sam\u00b7sein", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Bem\u00fche dich durch dies und das, dein Herz wird doch nicht reine;", "tokens": ["Be\u00b7m\u00fc\u00b7he", "dich", "durch", "dies", "und", "das", ",", "dein", "Herz", "wird", "doch", "nicht", "rei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDS", "KON", "PDS", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Der Boden ist ganz angebrannt, das B\u00f6se sitzt im Grund,", "tokens": ["Der", "Bo\u00b7den", "ist", "ganz", "an\u00b7ge\u00b7brannt", ",", "das", "B\u00f6\u00b7se", "sitzt", "im", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Du mu\u00dft, du mu\u00dft ins Feu'r hinein, sonst wird dir's nimmer kund!", "tokens": ["Du", "mu\u00dft", ",", "du", "mu\u00dft", "ins", "Feu'r", "hin\u00b7ein", ",", "sonst", "wird", "dir's", "nim\u00b7mer", "kund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "APPRART", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Du kannst von deiner Eigenheit nicht gr\u00fcndlich reine werden,", "tokens": ["Du", "kannst", "von", "dei\u00b7ner", "Ei\u00b7gen\u00b7heit", "nicht", "gr\u00fcnd\u00b7lich", "rei\u00b7ne", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADJD", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Wo du im Kreuz des Herren Grimm nicht f\u00fchlest hier auf Erden;", "tokens": ["Wo", "du", "im", "Kreuz", "des", "Her\u00b7ren", "Grimm", "nicht", "f\u00fch\u00b7lest", "hier", "auf", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "NE", "PTKNEG", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Herr, du willst gern mich reinigen, mein Herz auch gerne will,", "tokens": ["Herr", ",", "du", "willst", "gern", "mich", "rei\u00b7ni\u00b7gen", ",", "mein", "Herz", "auch", "ger\u00b7ne", "will", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "PPER", "VVINF", "$,", "PPOSAT", "NN", "ADV", "ADV", "VMFIN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.8": {"text": "Gib Gnade denn, da\u00df ich dir kann im Leiden halten still!", "tokens": ["Gib", "Gna\u00b7de", "denn", ",", "da\u00df", "ich", "dir", "kann", "im", "Lei\u00b7den", "hal\u00b7ten", "still", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "$,", "KOUS", "PPER", "PPER", "VMFIN", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}