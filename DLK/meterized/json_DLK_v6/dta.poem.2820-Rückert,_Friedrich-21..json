{"dta.poem.2820": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Irrthum abgethan ficht dich nicht weiter an,", "tokens": ["Ein", "Irr\u00b7thum", "ab\u00b7ge\u00b7than", "ficht", "dich", "nicht", "wei\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du geh'st an ihm vorbei ohn' Ansto\u00df deine Bahn.", "tokens": ["Du", "geh'st", "an", "ihm", "vor\u00b7bei", "ohn'", "An\u00b7sto\u00df", "dei\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Und Wunder nimmt dichs fast, wie man in vorigen Tagen", "tokens": ["Und", "Wun\u00b7der", "nimmt", "dichs", "fast", ",", "wie", "man", "in", "vo\u00b7ri\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PIS", "ADV", "$,", "PWAV", "PIS", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich mit so schwachem Feind ernsthaft herumgeschlagen.", "tokens": ["Sich", "mit", "so", "schwa\u00b7chem", "Feind", "ernst\u00b7haft", "her\u00b7um\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADV", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch h\u00e4tten sie gescherzt, so w\u00e4r' er nicht besiegt;", "tokens": ["Doch", "h\u00e4t\u00b7ten", "sie", "ge\u00b7scherzt", ",", "so", "w\u00e4r'", "er", "nicht", "be\u00b7siegt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gut scherzen hast du nun, da er zu Boden liegt.", "tokens": ["Gut", "scher\u00b7zen", "hast", "du", "nun", ",", "da", "er", "zu", "Bo\u00b7den", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}