{"textgrid.poem.37942": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Jagdgl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ritt ein J\u00e4ger wohlgemuth,", "tokens": ["Es", "ritt", "ein", "J\u00e4\u00b7ger", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl in der Morgenstunde,", "tokens": ["Wohl", "in", "der", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wollt jagen in dem gr\u00fcnen Wald", "tokens": ["Wollt", "ja\u00b7gen", "in", "dem", "gr\u00fc\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinem Ro\u00df und Hunde;", "tokens": ["Mit", "sei\u00b7nem", "Ro\u00df", "und", "Hun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und als er kam auf gr\u00fcner Heid,", "tokens": ["Und", "als", "er", "kam", "auf", "gr\u00fc\u00b7ner", "Heid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da fand sein Herze Lust und Freud,", "tokens": ["Da", "fand", "sein", "Her\u00b7ze", "Lust", "und", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Mayen,", "tokens": ["Im", "Ma\u00b7yen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Am Reihen,", "tokens": ["Am", "Rei\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Sich freuen alle Knaben und M\u00e4gdelein.", "tokens": ["Sich", "freu\u00b7en", "al\u00b7le", "Kna\u00b7ben", "und", "M\u00e4g\u00b7del\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Kukuk schreit, der Auerhan falzt,", "tokens": ["Der", "Ku\u00b7kuk", "schreit", ",", "der", "Au\u00b7er\u00b7han", "falzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dazu die Turteltauben,", "tokens": ["Da\u00b7zu", "die", "Tur\u00b7tel\u00b7tau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da fing des J\u00e4gers R\u00f6\u00dflein an", "tokens": ["Da", "fing", "des", "J\u00e4\u00b7gers", "R\u00f6\u00df\u00b7lein", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu schnarchen und zu schnauben,", "tokens": ["Zu", "schnar\u00b7chen", "und", "zu", "schnau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der J\u00e4ger dacht in seinem Muth,", "tokens": ["Der", "J\u00e4\u00b7ger", "dacht", "in", "sei\u00b7nem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Jagen kann noch werden gut,", "tokens": ["Das", "Ja\u00b7gen", "kann", "noch", "wer\u00b7den", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Mayen,", "tokens": ["Im", "Ma\u00b7yen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Am Reihen,", "tokens": ["Am", "Rei\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Sich freuen alle Knaben und M\u00e4gdelein.", "tokens": ["Sich", "freu\u00b7en", "al\u00b7le", "Kna\u00b7ben", "und", "M\u00e4g\u00b7del\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Der J\u00e4ger sah ein edles Wild,", "tokens": ["Der", "J\u00e4\u00b7ger", "sah", "ein", "ed\u00b7les", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frisch, hurtig und geschwinde,", "tokens": ["Frisch", ",", "hur\u00b7tig", "und", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es war ein sch\u00f6nes Frauenbild,", "tokens": ["Es", "war", "ein", "sch\u00f6\u00b7nes", "Frau\u00b7en\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sich allda lie\u00df finden;", "tokens": ["Das", "sich", "all\u00b7da", "lie\u00df", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PAV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der J\u00e4ger dacht in seinem Sinn:", "tokens": ["Der", "J\u00e4\u00b7ger", "dacht", "in", "sei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu diesem Wilde jag ich hin;", "tokens": ["Zu", "die\u00b7sem", "Wil\u00b7de", "jag", "ich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Mayen,", "tokens": ["Im", "Ma\u00b7yen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Am Reihen,", "tokens": ["Am", "Rei\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Sich freuen alle Knaben und M\u00e4gdelein.", "tokens": ["Sich", "freu\u00b7en", "al\u00b7le", "Kna\u00b7ben", "und", "M\u00e4g\u00b7del\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbich gr\u00fc\u00df euch Jungfrau, h\u00fcbsch und fein,", "tokens": ["\u00bb", "ich", "gr\u00fc\u00df", "euch", "Jung\u00b7frau", ",", "h\u00fcbsch", "und", "fein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Tugend reich und sch\u00f6ne,", "tokens": ["Von", "Tu\u00b7gend", "reich", "und", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ich in diesem Wald erschleich,", "tokens": ["Was", "ich", "in", "die\u00b7sem", "Wald", "er\u00b7schleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mach ich mir zu eigen.\u00ab", "tokens": ["Das", "mach", "ich", "mir", "zu", "ei\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbach, edler J\u00e4ger, wohlgestalt,", "tokens": ["\u00bb", "ach", ",", "ed\u00b7ler", "J\u00e4\u00b7ger", ",", "wohl\u00b7ge\u00b7stalt", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin nunmehr in eurer Gewalt,", "tokens": ["Ich", "bin", "nun\u00b7mehr", "in", "eu\u00b7rer", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Im Mayen,", "tokens": ["Im", "Ma\u00b7yen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Am Reihen,", "tokens": ["Am", "Rei\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Sich freuen alle Knaben und M\u00e4gdelein.\u00ab", "tokens": ["Sich", "freu\u00b7en", "al\u00b7le", "Kna\u00b7ben", "und", "M\u00e4g\u00b7del\u00b7ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "VVFIN", "PIAT", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Er nahm sie bei ihrer schneewei\u00dfen Hand,", "tokens": ["Er", "nahm", "sie", "bei", "ih\u00b7rer", "schnee\u00b7wei\u00b7\u00dfen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nach J\u00e4ger Manier und Weise,", "tokens": ["Nach", "J\u00e4\u00b7ger", "Ma\u00b7nier", "und", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er schwang sie vorne auf sein Ro\u00df,", "tokens": ["Er", "schwang", "sie", "vor\u00b7ne", "auf", "sein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fcck zu! wohl auf die Reise.", "tokens": ["Gl\u00fcck", "zu", "!", "wohl", "auf", "die", "Rei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Drum ist das Gl\u00fcck so kugelrund,", "tokens": ["Drum", "ist", "das", "Gl\u00fcck", "so", "ku\u00b7gel\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "De\u00df freut sich mancher, der mir kund,", "tokens": ["De\u00df", "freut", "sich", "man\u00b7cher", ",", "der", "mir", "kund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PIS", "$,", "PRELS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Mayen,", "tokens": ["Im", "Ma\u00b7yen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Am Reihen,", "tokens": ["Am", "Rei\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Sich freuen alle Knaben und M\u00e4gdelein.", "tokens": ["Sich", "freu\u00b7en", "al\u00b7le", "Kna\u00b7ben", "und", "M\u00e4g\u00b7del\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}