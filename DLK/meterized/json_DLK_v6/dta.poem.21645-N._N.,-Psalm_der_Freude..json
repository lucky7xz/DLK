{"dta.poem.21645": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Psalm der Freude.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn auf der Bergesh\u00f6h\u2019", "tokens": ["Wenn", "auf", "der", "Ber\u00b7ges\u00b7h\u00f6h'"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Unter dir Wolken ruh\u2019n,", "tokens": ["Un\u00b7ter", "dir", "Wol\u00b7ken", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Schleichende Wolken zieh\u2019n \u2014", "tokens": ["Schlei\u00b7chen\u00b7de", "Wol\u00b7ken", "zieh'n"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Rei\u00dfen die St\u00fcrme hinein,", "tokens": ["Rei\u00b7\u00dfen", "die", "St\u00fcr\u00b7me", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Fern durch den Wolkenspalt", "tokens": ["Fern", "durch", "den", "Wol\u00b7ken\u00b7spalt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Gl\u00e4nzt dir ein Sonnenthal", "tokens": ["Gl\u00e4nzt", "dir", "ein", "Son\u00b7nen\u00b7thal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schimmernd wie goldner Traum \u2014", "tokens": ["Schim\u00b7mernd", "wie", "gold\u00b7ner", "Traum"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Wenn dich herrlich umkr\u00f6nt", "tokens": ["Wenn", "dich", "herr\u00b7lich", "um\u00b7kr\u00f6nt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Mit dem Flammenkranz", "tokens": ["Mit", "dem", "Flam\u00b7men\u00b7kranz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Allliebender Sonne", "tokens": ["A\u00b7lllie\u00b7ben\u00b7der", "Son\u00b7ne"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "W\u00e4rmender Strahl,", "tokens": ["W\u00e4r\u00b7men\u00b7der", "Strahl", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Da\u00df du leuchtest", "tokens": ["Da\u00df", "du", "leuch\u00b7test"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Ueber den Wolken \u2014", "tokens": ["Ue\u00b7ber", "den", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Wenn all\u2019 die Lust", "tokens": ["Wenn", "all'", "die", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Und die Seligkeit", "tokens": ["Und", "die", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Athmender Menschenkinder im Thal", "tokens": ["Ath\u00b7men\u00b7der", "Men\u00b7schen\u00b7kin\u00b7der", "im", "Thal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "M\u00e4chtig zum Busen dir dr\u00e4ngt,", "tokens": ["M\u00e4ch\u00b7tig", "zum", "Bu\u00b7sen", "dir", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.18": {"text": "Im Busen dir wiederklingt:", "tokens": ["Im", "Bu\u00b7sen", "dir", "wie\u00b7der\u00b7klingt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "O, wie gro\u00df, du gewaltige", "tokens": ["O", ",", "wie", "gro\u00df", ",", "du", "ge\u00b7wal\u00b7ti\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "$,", "PPER", "ADJA"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Welt des Staubs! \u2014", "tokens": ["Welt", "des", "Staubs", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ja, Herz, du darfst in Demuth still", "tokens": ["Ja", ",", "Herz", ",", "du", "darfst", "in", "De\u00b7muth", "still"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "PAV", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verehren all\u2019 das Himmlische,", "tokens": ["Ver\u00b7eh\u00b7ren", "all'", "das", "Himm\u00b7li\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du darfst es; streben, heilig Herz,", "tokens": ["Du", "darfst", "es", ";", "stre\u00b7ben", ",", "hei\u00b7lig", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "VVFIN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Guten all\u2019 und Sch\u00f6nen dieser Welt", "tokens": ["Des", "Gu\u00b7ten", "all'", "und", "Sch\u00f6\u00b7nen", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "KON", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dich w\u00fcrdig zu erweisen:", "tokens": ["Dich", "w\u00fcr\u00b7dig", "zu", "er\u00b7wei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Darfst sinken betend hin", "tokens": ["Darfst", "sin\u00b7ken", "be\u00b7tend", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Vor der gewaltigen Feste des Alls", "tokens": ["Vor", "der", "ge\u00b7wal\u00b7ti\u00b7gen", "Fes\u00b7te", "des", "Alls"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Und Erd\u2019 und Himmel schauen,", "tokens": ["Und", "Erd'", "und", "Him\u00b7mel", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Danken mit Wonneblick,", "tokens": ["Dan\u00b7ken", "mit", "Won\u00b7ne\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Leuchtend im Wolkenreich", "tokens": ["Leuch\u00b7tend", "im", "Wol\u00b7ken\u00b7reich"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Fl\u00fcstern: o Gl\u00fcck", "tokens": ["Fl\u00fcs\u00b7tern", ":", "o", "Gl\u00fcck"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "FM", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Ein Mensch zu sein. \u2014", "tokens": ["Ein", "Mensch", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}