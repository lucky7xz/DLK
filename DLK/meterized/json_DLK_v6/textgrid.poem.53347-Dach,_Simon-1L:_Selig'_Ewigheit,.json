{"textgrid.poem.53347": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Selig' Ewigheit,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selig' Ewigheit,", "tokens": ["Se\u00b7lig'", "E\u00b7wig\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Lohn der Himmels-Erben,", "tokens": ["Lohn", "der", "Him\u00b7mels\u00b7Er\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Derer Hertzeleid", "tokens": ["De\u00b7rer", "Hert\u00b7ze\u00b7leid"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Die in S\u00fcnden sterben.", "tokens": ["Die", "in", "S\u00fcn\u00b7den", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bild doch dich allein", "tokens": ["Bild", "doch", "dich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Immer mir recht ein.", "tokens": ["Im\u00b7mer", "mir", "recht", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df mir nichts dein Wort", "tokens": ["La\u00df", "mir", "nichts", "dein", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PIS", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Aus dem Hertzen lencken,", "tokens": ["Aus", "dem", "Hert\u00b7zen", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sondern fort und fort", "tokens": ["Son\u00b7dern", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "KON", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mich an dich gedencken:", "tokens": ["Mich", "an", "dich", "ge\u00b7den\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sey mein Tritt, mein Gang", "tokens": ["Sey", "mein", "Tritt", ",", "mein", "Gang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Vnd mein Lebens-Zwang.", "tokens": ["Vnd", "mein", "Le\u00b7bens\u00b7Zwang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hast du dich gesellt", "tokens": ["Hast", "du", "dich", "ge\u00b7sellt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wol zu meinen Sinnen,", "tokens": ["Wol", "zu", "mei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nichts in dieser Welt", "tokens": ["Nichts", "in", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "APPR", "PDAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wird mein Hertz gewinnen:", "tokens": ["Wird", "mein", "Hertz", "ge\u00b7win\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn was gleicht allhier", "tokens": ["Denn", "was", "gleicht", "all\u00b7hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Deiner hohen Zier?", "tokens": ["Dei\u00b7ner", "ho\u00b7hen", "Zier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Deinen rechten Stand", "tokens": ["Dei\u00b7nen", "rech\u00b7ten", "Stand"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "W\u00fcrdig auszusprechen,", "tokens": ["W\u00fcr\u00b7dig", "aus\u00b7zu\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wird uns Witz und Hand", "tokens": ["Wird", "uns", "Witz", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Zung und Mund gebrechen:", "tokens": ["Zung", "und", "Mund", "ge\u00b7bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hier hat Wissenschafft", "tokens": ["Hier", "hat", "Wis\u00b7sen\u00b7schafft"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Weder Art noch Krafft.", "tokens": ["We\u00b7der", "Art", "noch", "Krafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Gott von Angesicht,", "tokens": ["Gott", "von", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wie Er ist, erkennen,", "tokens": ["Wie", "Er", "ist", ",", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durch das grosse Licht", "tokens": ["Durch", "das", "gros\u00b7se", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Seiner Liebe brennen:", "tokens": ["Sei\u00b7ner", "Lie\u00b7be", "bren\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sprechen: Meine Ruh,", "tokens": ["Spre\u00b7chen", ":", "Mei\u00b7ne", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Gott, bist einig Du.", "tokens": ["Gott", ",", "bist", "ei\u00b7nig", "Du", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADJD", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Vber alle ma\u00df", "tokens": ["Vber", "al\u00b7le", "ma\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Gn\u00fcgsam sein geniessen,", "tokens": ["Gn\u00fcg\u00b7sam", "sein", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihn ohn unterla\u00df", "tokens": ["Ihn", "ohn", "un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "In die Arme schliessen:", "tokens": ["In", "die", "Ar\u00b7me", "schlies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd sich spiegeln gantz", "tokens": ["Vnd", "sich", "spie\u00b7geln", "gantz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRF", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nur in seinem Glantz.", "tokens": ["Nur", "in", "sei\u00b7nem", "Glantz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Aller W\u00fcnsche Macht,", "tokens": ["Al\u00b7ler", "W\u00fcn\u00b7sche", "Macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Aller Wei\u00dfheit Gaben,", "tokens": ["Al\u00b7ler", "Wei\u00df\u00b7heit", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aller Hoheit Pracht", "tokens": ["Al\u00b7ler", "Ho\u00b7heit", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Allen Reichtthum haben:", "tokens": ["Al\u00b7len", "Reicht\u00b7thum", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nirgends sehn Verdru\u00df", "tokens": ["Nir\u00b7gends", "sehn", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADV", "CARD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "In dem Vberflu\u00df.", "tokens": ["In", "dem", "Vberf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-++", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Aller V\u00e4ter Schar", "tokens": ["Al\u00b7ler", "V\u00e4\u00b7ter", "Schar"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Vnd die lieben Seinen", "tokens": ["Vnd", "die", "lie\u00b7ben", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sprechen immerdar:", "tokens": ["Spre\u00b7chen", "im\u00b7mer\u00b7dar", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nirgends \u00fcber weinen.", "tokens": ["Nir\u00b7gends", "\u00fc\u00b7ber", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ohn Gefahr und Pein", "tokens": ["Ohn", "Ge\u00b7fahr", "und", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Vnd ohn Kranckheit seyn.", "tokens": ["Vnd", "ohn", "Kran\u00b7ck\u00b7heit", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Seine Stimm empor", "tokens": ["Sei\u00b7ne", "Stimm", "em\u00b7por"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mit den Engeln schwingen", "tokens": ["Mit", "den", "En\u00b7geln", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vnd in vollem Chor", "tokens": ["Vnd", "in", "vol\u00b7lem", "Chor"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Vnserm Sch\u00f6pffer singen:", "tokens": ["Vn\u00b7serm", "Sch\u00f6pf\u00b7fer", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Heilig bist du, Gott,", "tokens": ["Hei\u00b7lig", "bist", "du", ",", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "O Herr Zebaoth.", "tokens": ["O", "Herr", "Ze\u00b7bao\u00b7th", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Vnd di\u00df alles zwar", "tokens": ["Vnd", "di\u00df", "al\u00b7les", "zwar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "PIS", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nicht nur lange Zeiten,", "tokens": ["Nicht", "nur", "lan\u00b7ge", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hundert tausend Jahr,", "tokens": ["Hun\u00b7dert", "tau\u00b7send", "Jahr", ","], "token_info": ["word", "word", "word", "punct"], 