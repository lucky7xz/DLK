{"textgrid.poem.31541": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was soll ich weiter lang das Heiligste verh\u00fcllen?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich weiter lang das Heiligste verh\u00fcllen?", "tokens": ["Was", "soll", "ich", "wei\u00b7ter", "lang", "das", "Hei\u00b7ligs\u00b7te", "ver\u00b7h\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "h\u00e4ngt es doch ganz entbl\u00f6st vor aller Welt allhier.", "tokens": ["h\u00e4ngt", "es", "doch", "ganz", "ent\u00b7bl\u00f6st", "vor", "al\u00b7ler", "Welt", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df allerheiligst' Herz brennt todte von Begier /", "tokens": ["Da\u00df", "al\u00b7ler\u00b7hei\u00b7ligst'", "Herz", "brennt", "tod\u00b7te", "von", "Be\u00b7gier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "recht zuentdecken sich / zu zeigen Gottes Willen.", "tokens": ["recht", "zu\u00b7ent\u00b7de\u00b7cken", "sich", "/", "zu", "zei\u00b7gen", "Got\u00b7tes", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$(", "PTKZU", "VVINF", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Das Gott-vereinigt Blut / l\u00e4st eher sich nit stillen /", "tokens": ["Das", "Got\u00b7tver\u00b7ei\u00b7nigt", "Blut", "/", "l\u00e4st", "e\u00b7her", "sich", "nit", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "VVFIN", "ADV", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "bis es ganz ausgesch\u00fctt und halb verw\u00e4ssert schier.", "tokens": ["bis", "es", "ganz", "aus\u00b7ge\u00b7sch\u00fctt", "und", "halb", "ver\u00b7w\u00e4s\u00b7sert", "schier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVPP", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach diesen weichet erst des Gnaden-Stuhles Zier.", "tokens": ["Ach", "die\u00b7sen", "wei\u00b7chet", "erst", "des", "Gna\u00b7den\u00b7Stuh\u00b7les", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDS", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Seht / hier den Sinn-entwurff / und dort das Werk-erf\u00fcllen!", "tokens": ["Seht", "/", "hier", "den", "Sinn\u00b7ent\u00b7wurff", "/", "und", "dort", "das", "Werk\u00b7er\u00b7f\u00fcl\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADV", "ART", "NN", "$(", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "die G\u00f6ttliche Geheim / mit Dunkelheit verh\u00e4ngt", "tokens": ["die", "G\u00f6tt\u00b7li\u00b7che", "Ge\u00b7heim", "/", "mit", "Dun\u00b7kel\u00b7heit", "ver\u00b7h\u00e4ngt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "in manchem lebens-Bild der V\u00e4tter und Propheten /", "tokens": ["in", "man\u00b7chem", "le\u00b7bens\u00b7Bild", "der", "V\u00e4t\u00b7ter", "und", "Pro\u00b7phe\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "in wahre Klarheits-Sonn' ist aus dem Schatten tretten.", "tokens": ["in", "wah\u00b7re", "Kla\u00b7rheits\u00b7Sonn'", "ist", "aus", "dem", "Schat\u00b7ten", "tret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ihr' Vnverberglichkeit hat mich nun auch zerspr\u00e4ngt /", "tokens": ["Ih\u00b7r'", "Vn\u00b7ver\u00b7berg\u00b7lich\u00b7keit", "hat", "mich", "nun", "auch", "zer\u00b7spr\u00e4ngt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "reist alle Vorh\u00e4ng weg der Falschheit / zeigt im Herzen", "tokens": ["reist", "al\u00b7le", "Vor\u00b7h\u00e4ng", "weg", "der", "Falschheit", "/", "zeigt", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "ART", "NN", "$(", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "da\u00df Heiligst Christi Blut / und wahrer Reue Schmerzen.", "tokens": ["da\u00df", "Hei\u00b7ligst", "Chris\u00b7ti", "Blut", "/", "und", "wah\u00b7rer", "Reu\u00b7e", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "NN", "$(", "KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}