{"textgrid.poem.48693": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14. Das Blut Jesu Christi, des Sohns Gottes u.s.w.", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier steh' ich armer Mensch und sch\u00e4me mich vor mir,", "tokens": ["Hier", "steh'", "ich", "ar\u00b7mer", "Mensch", "und", "sch\u00e4\u00b7me", "mich", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "KON", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit so viel H\u00e4\u00dflichkeit der S\u00fcnden ganz beklecket.", "tokens": ["mit", "so", "viel", "H\u00e4\u00df\u00b7lich\u00b7keit", "der", "S\u00fcn\u00b7den", "ganz", "be\u00b7kle\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Mein erstes sch\u00f6nes Kleid, wie ist es doch beflecket,", "tokens": ["Mein", "ers\u00b7tes", "sch\u00f6\u00b7nes", "Kleid", ",", "wie", "ist", "es", "doch", "be\u00b7fle\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wie hat doch dieser Wust ersticket alle Zier", "tokens": ["wie", "hat", "doch", "die\u00b7ser", "Wust", "er\u00b7sti\u00b7cket", "al\u00b7le", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "PDAT", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die schwache Seele tut kaum noch ihr H\u00e4upt herf\u00fcr,", "tokens": ["Die", "schwa\u00b7che", "See\u00b7le", "tut", "kaum", "noch", "ihr", "H\u00e4upt", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weil sie der tiefe Schlam mit Wuste ganz bedecket", "tokens": ["weil", "sie", "der", "tie\u00b7fe", "Schlam", "mit", "Wus\u00b7te", "ganz", "be\u00b7de\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und der versinkenden kein Arm wird zugerecket,", "tokens": ["und", "der", "ver\u00b7sin\u00b7ken\u00b7den", "kein", "Arm", "wird", "zu\u00b7ge\u00b7re\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ihr Loch, ihr Kot, ihr Tod ist der Leib, dieser hier.", "tokens": ["ihr", "Loch", ",", "ihr", "Kot", ",", "ihr", "Tod", "ist", "der", "Leib", ",", "die\u00b7ser", "hier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Gott Jesu, Christe Mensch, nur deine Hand, die fromme,", "tokens": ["Gott", "Je\u00b7su", ",", "Chris\u00b7te", "Mensch", ",", "nur", "dei\u00b7ne", "Hand", ",", "die", "from\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die kan es, da\u00df ich auf aus dieser H\u00f6llen komme.", "tokens": ["die", "kan", "es", ",", "da\u00df", "ich", "auf", "aus", "die\u00b7ser", "H\u00f6l\u00b7len", "kom\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zeuch, starker, mich heraus und mache mir ein Bad,", "tokens": ["Zeuch", ",", "star\u00b7ker", ",", "mich", "he\u00b7raus", "und", "ma\u00b7che", "mir", "ein", "Bad", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "ein Bad, ein rotes Bad von deinem teuren Blute.", "tokens": ["ein", "Bad", ",", "ein", "ro\u00b7tes", "Bad", "von", "dei\u00b7nem", "teu\u00b7ren", "Blu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "ADJA", "NE", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel darfstu dessen nicht vergie\u00dfen mir zu gute,", "tokens": ["Viel", "darfs\u00b7tu", "des\u00b7sen", "nicht", "ver\u00b7gie\u00b7\u00dfen", "mir", "zu", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDS", "PTKNEG", "VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "an einem Tr\u00f6pflein nur, Erl\u00f6ser, ist es satt.", "tokens": ["an", "ei\u00b7nem", "Tr\u00f6pf\u00b7lein", "nur", ",", "Er\u00b7l\u00f6\u00b7ser", ",", "ist", "es", "satt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "NN", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}