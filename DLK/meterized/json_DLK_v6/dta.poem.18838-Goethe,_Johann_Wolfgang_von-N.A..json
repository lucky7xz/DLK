{"dta.poem.18838": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Alle Menschen gross und klein", "tokens": ["Al\u00b7le", "Men\u00b7schen", "gross", "und", "klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spinnen sich ein Gewebe fein,", "tokens": ["Spin\u00b7nen", "sich", "ein", "Ge\u00b7we\u00b7be", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wo sie mit ihrer Scheeren Spitzen", "tokens": ["Wo", "sie", "mit", "ih\u00b7rer", "Schee\u00b7ren", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gar zierlich in der Mitte sitzen.", "tokens": ["Gar", "zier\u00b7lich", "in", "der", "Mit\u00b7te", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn nun darein ein Besen f\u00e4hrt,", "tokens": ["Wenn", "nun", "da\u00b7rein", "ein", "Be\u00b7sen", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sagen sie es sey unerh\u00f6rt,", "tokens": ["Sa\u00b7gen", "sie", "es", "sey", "un\u00b7er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Man habe den gr\u00f6ssten Pallast zerst\u00f6rt.", "tokens": ["Man", "ha\u00b7be", "den", "gr\u00f6ss\u00b7ten", "Pal\u00b7last", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}