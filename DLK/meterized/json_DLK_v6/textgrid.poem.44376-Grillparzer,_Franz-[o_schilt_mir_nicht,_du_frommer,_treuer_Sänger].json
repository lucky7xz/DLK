{"textgrid.poem.44376": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[o schilt mir nicht, du frommer, treuer S\u00e4nger]", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O schilt mir nicht, du frommer, treuer S\u00e4nger,", "tokens": ["O", "schilt", "mir", "nicht", ",", "du", "from\u00b7mer", ",", "treu\u00b7er", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Weg, der mir zuteil und meinem Liede fiel,", "tokens": ["Den", "Weg", ",", "der", "mir", "zu\u00b7teil", "und", "mei\u00b7nem", "Lie\u00b7de", "fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Scheint er gleich dunkler, wildverschlungner, l\u00e4nger,", "tokens": ["Scheint", "er", "gleich", "dunk\u00b7ler", ",", "wild\u00b7ver\u00b7schlung\u00b7ner", ",", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "$,", "ADJA", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er f\u00fchrt darum nicht minder doch zum Ziel.", "tokens": ["Er", "f\u00fchrt", "da\u00b7rum", "nicht", "min\u00b7der", "doch", "zum", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hast du den Herrn im Flammenbusch erschauet,", "tokens": ["Hast", "du", "den", "Herrn", "im", "Flam\u00b7men\u00b7busch", "er\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Seh ich daf\u00fcr im gr\u00fcnen Busch den Herrn.", "tokens": ["Seh", "ich", "da\u00b7f\u00fcr", "im", "gr\u00fc\u00b7nen", "Busch", "den", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}