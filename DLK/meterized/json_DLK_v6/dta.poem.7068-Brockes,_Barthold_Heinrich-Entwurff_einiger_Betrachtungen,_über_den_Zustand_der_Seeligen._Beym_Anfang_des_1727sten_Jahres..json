{"dta.poem.7068": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Gerechte nur allein von dieser Erden", "tokens": ["Wenn", "die", "Ge\u00b7rech\u00b7te", "nur", "al\u00b7lein", "von", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schau-Pl\u00e4tz\u2019 \u00fcbersehen werden;", "tokens": ["Die", "Schau\u00b7Pl\u00e4tz'", "\u00fc\u00b7ber\u00b7se\u00b7hen", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie stets die G\u00f6ttliche Versehung hier gespielt,", "tokens": ["Wie", "stets", "die", "G\u00f6tt\u00b7li\u00b7che", "Ver\u00b7se\u00b7hung", "hier", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie sie allemal zum besten abgezielt:", "tokens": ["Und", "wie", "sie", "al\u00b7le\u00b7mal", "zum", "bes\u00b7ten", "ab\u00b7ge\u00b7zielt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was mu\u00df es f\u00fcr Belustigung erwecken!", "tokens": ["Was", "mu\u00df", "es", "f\u00fcr", "Be\u00b7lus\u00b7ti\u00b7gung", "er\u00b7we\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zumal wenn sie die Sorgfalt selbst entdecken,", "tokens": ["Zu\u00b7mal", "wenn", "sie", "die", "Sorg\u00b7falt", "selbst", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die GOTT f\u00fcr sie so wunderbar getragen,", "tokens": ["Die", "GoTT", "f\u00fcr", "sie", "so", "wun\u00b7der\u00b7bar", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auch wenn, dem Ansehn nach, Er sie geschlagen;", "tokens": ["Auch", "wenn", ",", "dem", "An\u00b7sehn", "nach", ",", "Er", "sie", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "ART", "NN", "PTKVZ", "$,", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da auch, indem Er sie betr\u00fcbt,", "tokens": ["Da", "auch", ",", "in\u00b7dem", "Er", "sie", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.10": {"text": "Er sie dennoch nicht weniger geliebt.", "tokens": ["Er", "sie", "den\u00b7noch", "nicht", "we\u00b7ni\u00b7ger", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wenn sie zugleich als wie in einem Spiegel sehen,", "tokens": ["Wenn", "sie", "zu\u00b7gleich", "als", "wie", "in", "ei\u00b7nem", "Spie\u00b7gel", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KOUS", "KOKOM", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie alles in der Welt zu ihrem Heil geschehen:", "tokens": ["Wie", "al\u00b7les", "in", "der", "Welt", "zu", "ih\u00b7rem", "Heil", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie manche Noth, wie mancherley Gefahr,", "tokens": ["Wie", "man\u00b7che", "Noth", ",", "wie", "man\u00b7cher\u00b7ley", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die sonsten unvermeidlich war,", "tokens": ["Die", "sons\u00b7ten", "un\u00b7ver\u00b7meid\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Sch\u00f6pfer doch so wunderbar", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "doch", "so", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von ihnen abgewandt.", "tokens": ["Von", "ih\u00b7nen", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Wie GOTTES m\u00e4cht\u2019ge Wunder-Hand,", "tokens": ["Wie", "GoT\u00b7TES", "m\u00e4cht'\u00b7ge", "Wun\u00b7der\u00b7Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Von dem schon gegenw\u00e4rt\u2019gen Grabe", "tokens": ["Von", "dem", "schon", "ge\u00b7gen\u00b7w\u00e4rt'\u00b7gen", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Sie so gar offt befreyet habe.", "tokens": ["Sie", "so", "gar", "offt", "be\u00b7fre\u00b7yet", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Aus welchem Mangel-Pfuhl, drinn sie versincken m\u00fcssen,", "tokens": ["Aus", "wel\u00b7chem", "Man\u00b7gel\u00b7P\u00b7fuhl", ",", "drinn", "sie", "ver\u00b7sin\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "ADV", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jhr GOTT, ihr VATER, sie so offt heraus gerissen.", "tokens": ["Ihr", "GoTT", ",", "ihr", "Va\u00b7TER", ",", "sie", "so", "offt", "he\u00b7raus", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "ADV", "ADV", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie er so offt mit Seegen und Gedeyen,", "tokens": ["Wie", "er", "so", "offt", "mit", "See\u00b7gen", "und", "Ge\u00b7de\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Mit Ehr und Neichthum sie gew\u00fcrdigt zu erfreuen.", "tokens": ["Mit", "Ehr", "und", "Neicht\u00b7hum", "sie", "ge\u00b7w\u00fcr\u00b7digt", "zu", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}