{"textgrid.poem.45583": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Verfasser des Kosmos", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergleich ich dich mit deinen Ruhmesgef\u00e4hrten,", "tokens": ["Ver\u00b7gleich", "ich", "dich", "mit", "dei\u00b7nen", "Ruh\u00b7mes\u00b7ge\u00b7f\u00e4hr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Scheinst du mir der gelehrigste aller Gelehrten.", "tokens": ["Scheinst", "du", "mir", "der", "ge\u00b7leh\u00b7rigs\u00b7te", "al\u00b7ler", "Ge\u00b7lehr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "PIAT", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}}}}}