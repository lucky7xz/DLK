{"textgrid.poem.33767": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kreuzritter", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herauf, herauf, mein treues Ro\u00df!", "tokens": ["Her\u00b7auf", ",", "her\u00b7auf", ",", "mein", "treu\u00b7es", "Ro\u00df", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stund es einst, mein Ahnenschlo\u00df,", "tokens": ["Da", "stund", "es", "einst", ",", "mein", "Ah\u00b7nen\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun ist es Schutt und alles tot,", "tokens": ["Nun", "ist", "es", "Schutt", "und", "al\u00b7les", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was mir einst Lust und Liebe bot.", "tokens": ["Was", "mir", "einst", "Lust", "und", "Lie\u00b7be", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Vaterland, du teures Land!", "tokens": ["O", "Va\u00b7ter\u00b7land", ",", "du", "teu\u00b7res", "Land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich schlief auf hei\u00dfem W\u00fcstensand,", "tokens": ["Ich", "schlief", "auf", "hei\u00b7\u00dfem", "W\u00fcs\u00b7ten\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verwundet und im Felsennest", "tokens": ["Ver\u00b7wun\u00b7det", "und", "im", "Fel\u00b7sen\u00b7nest"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auf dem Leichentuch der Pest.", "tokens": ["Und", "auf", "dem", "Lei\u00b7chen\u00b7tuch", "der", "Pest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Panzerkleid, am Pilgerstab,", "tokens": ["Im", "Pan\u00b7zer\u00b7kleid", ",", "am", "Pil\u00b7ger\u00b7stab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In hei\u00dfer Schlacht, am heil'gen Grab,", "tokens": ["In", "hei\u00b7\u00dfer", "Schlacht", ",", "am", "heil'\u00b7gen", "Grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gefangen und beim Siegesmahl,", "tokens": ["Ge\u00b7fan\u00b7gen", "und", "beim", "Sie\u00b7ges\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedacht' ich dein, mein Heimattal!", "tokens": ["Ge\u00b7dacht'", "ich", "dein", ",", "mein", "Hei\u00b7mat\u00b7tal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die V\u00f6lker schlugen auf das Zelt,", "tokens": ["Die", "V\u00f6l\u00b7ker", "schlu\u00b7gen", "auf", "das", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo einst gewallt der Herr der Welt;", "tokens": ["Wo", "einst", "ge\u00b7wallt", "der", "Herr", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab' den heil'gen Ort gesehn,", "tokens": ["Ich", "hab'", "den", "heil'\u00b7gen", "Ort", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es sank vor mir der Sarazen.", "tokens": ["Es", "sank", "vor", "mir", "der", "Sa\u00b7ra\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun alle Schuld ist abgeb\u00fc\u00dft,", "tokens": ["Nun", "al\u00b7le", "Schuld", "ist", "ab\u00b7ge\u00b7b\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun kam ich heim, sei mir gegr\u00fc\u00dft", "tokens": ["Nun", "kam", "ich", "heim", ",", "sei", "mir", "ge\u00b7gr\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Grab der V\u00e4ter, Eichenbaum;", "tokens": ["Am", "Grab", "der", "V\u00e4\u00b7ter", ",", "Ei\u00b7chen\u00b7baum", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du schautest meinen sch\u00f6nsten Traum!", "tokens": ["Du", "schau\u00b7test", "mei\u00b7nen", "sch\u00f6ns\u00b7ten", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mir blieb mein Hort, mein treues Schwert,", "tokens": ["Mir", "blieb", "mein", "Hort", ",", "mein", "treu\u00b7es", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Siegesruhm, mein Ritterwert.", "tokens": ["Mein", "Sie\u00b7ges\u00b7ruhm", ",", "mein", "Rit\u00b7ter\u00b7wert", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gibst du mir nur ein Grab daf\u00fcr,", "tokens": ["Gibst", "du", "mir", "nur", "ein", "Grab", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Heimatland, wie dank' ich dir!", "tokens": ["Mein", "Hei\u00b7mat\u00b7land", ",", "wie", "dank'", "ich", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}