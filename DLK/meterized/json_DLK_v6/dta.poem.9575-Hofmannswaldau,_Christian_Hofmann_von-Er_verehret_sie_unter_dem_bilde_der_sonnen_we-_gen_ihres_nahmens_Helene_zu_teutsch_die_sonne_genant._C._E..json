{"dta.poem.9575": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er verehret sie unter dem bilde der sonnen we-  \n gen ihres nahmens/ Helene/ zu teutsch  \n die sonne genant.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Helene/ sonnen-kind/ du fackel dieser welt/", "tokens": ["He\u00b7le\u00b7ne", "/", "son\u00b7nen\u00b7kind", "/", "du", "fa\u00b7ckel", "die\u00b7ser", "welt", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "XY", "$(", "PPER", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du pr\u00e4cht\u2019ges meister-st\u00fcck des himmels und der erden/", "tokens": ["Du", "pr\u00e4cht'\u00b7ges", "meis\u00b7ter\u00b7st\u00fcck", "des", "him\u00b7mels", "und", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ohn erstaunen nicht kan angeschauet werden/", "tokens": ["Das", "ohn", "er\u00b7stau\u00b7nen", "nicht", "kan", "an\u00b7ge\u00b7schau\u00b7et", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "PTKNEG", "VMFIN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und recht was g\u00f6etliches in sich verschlossen h\u00e4lt;", "tokens": ["Und", "recht", "was", "g\u00f6et\u00b7li\u00b7ches", "in", "sich", "ver\u00b7schlos\u00b7sen", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PWS", "ADJA", "APPR", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ohn dich liegt die natur verfinstert und verstellt.", "tokens": ["Ohn", "dich", "liegt", "die", "na\u00b7tur", "ver\u00b7fins\u00b7tert", "und", "ver\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ART", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie seufftzt nach deinem blick und reitzenden geberden/", "tokens": ["Sie", "seufftzt", "nach", "dei\u00b7nem", "blick", "und", "reit\u00b7zen\u00b7den", "ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und f\u00fchlt nur all zuviel der schmertzlichsten beschwerden/", "tokens": ["Und", "f\u00fchlt", "nur", "all", "zu\u00b7viel", "der", "schmertz\u00b7lichs\u00b7ten", "be\u00b7schwer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "PIS", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wann dein belebter glantz was sparsam auff sie f\u00e4llt.", "tokens": ["Wann", "dein", "be\u00b7leb\u00b7ter", "glantz", "was", "spar\u00b7sam", "auff", "sie", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PWS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O sonn! ich feyre heut dein hohes nahmens-fest/", "tokens": ["O", "sonn", "!", "ich", "fey\u00b7re", "heut", "dein", "ho\u00b7hes", "nah\u00b7mens\u00b7fest", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das an nichts niedriges durchaus mich dencken l\u00e4st/", "tokens": ["Das", "an", "nichts", "nied\u00b7ri\u00b7ges", "durc\u00b7haus", "mich", "den\u00b7cken", "l\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIS", "PIS", "ADV", "PPER", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn wer zur sonnen will/ mu\u00df sich zum himmel schwingen.", "tokens": ["Denn", "wer", "zur", "son\u00b7nen", "will", "/", "mu\u00df", "sich", "zum", "him\u00b7mel", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "VVINF", "VMFIN", "$(", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was aber opffr\u2019 ich dir? blut/ leben/ seel und geist;", "tokens": ["Was", "a\u00b7ber", "opf\u00b7fr'", "ich", "dir", "?", "blut", "/", "le\u00b7ben", "/", "seel", "und", "geist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "PPER", "$.", "NN", "$(", "VVINF", "$(", "XY", "KON", "VVPP", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Ja! dieses ists/ was mir aus deiner g\u00fcte fleust/", "tokens": ["Ja", "!", "die\u00b7ses", "ists", "/", "was", "mir", "aus", "dei\u00b7ner", "g\u00fc\u00b7te", "fleust", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PDS", "VAFIN", "$(", "PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer kan was bessres dir auch zum geschencke bringen?", "tokens": ["Wer", "kan", "was", "bess\u00b7res", "dir", "auch", "zum", "ge\u00b7schen\u00b7cke", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "ADJA", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}