{"textgrid.poem.49088": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "47. Die Zeugen", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gr\u00fcn bewachs'nes Thal", "tokens": ["Du", "gr\u00fcn", "be\u00b7wachs'\u00b7nes", "Thal"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Voll klarer, frischer Quellen,", "tokens": ["Voll", "kla\u00b7rer", ",", "fri\u00b7scher", "Quel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du Bach, der du so sanft", "tokens": ["Du", "Bach", ",", "der", "du", "so", "sanft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "PRELS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hinrieselst deine Wellen,", "tokens": ["Hin\u00b7rie\u00b7selst", "dei\u00b7ne", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du wilder Wasserfall,", "tokens": ["Du", "wil\u00b7der", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Umflort von Silberweiden,", "tokens": ["Um\u00b7flort", "von", "Sil\u00b7ber\u00b7wei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O du geliebter Hain,", "tokens": ["O", "du", "ge\u00b7lieb\u00b7ter", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bei euch nur wohnen Freuden,", "tokens": ["Bei", "euch", "nur", "woh\u00b7nen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wohnt Friede nur allein!", "tokens": ["Wohnt", "Frie\u00b7de", "nur", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Euch einen Tag zu meiden,", "tokens": ["Euch", "ei\u00b7nen", "Tag", "zu", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "W\u00e4r' meinem Herzen Pein!", "tokens": ["W\u00e4r'", "mei\u00b7nem", "Her\u00b7zen", "Pein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Denn wenn ich von ihr tr\u00e4ume,", "tokens": ["Denn", "wenn", "ich", "von", "ihr", "tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "So tr\u00e4um' ich auch von euch!", "tokens": ["So", "tr\u00e4um'", "ich", "auch", "von", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Wenn ihrer ich gedenke,", "tokens": ["Wenn", "ih\u00b7rer", "ich", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Gedenk' ich eurer gleich.", "tokens": ["Ge\u00b7denk'", "ich", "eu\u00b7rer", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Denn hier in diesem Thal", "tokens": ["Denn", "hier", "in", "die\u00b7sem", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Traf mich zum erstenmal", "tokens": ["Traf", "mich", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Aus ihrem Aug' ein Strahl", "tokens": ["Aus", "ih\u00b7rem", "Aug'", "ein", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Der tief ins Herz mir drang,", "tokens": ["Der", "tief", "ins", "Herz", "mir", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Der sie zu lieben mich", "tokens": ["Der", "sie", "zu", "lie\u00b7ben", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "F\u00fcr Ewigkeiten zwang.", "tokens": ["F\u00fcr", "E\u00b7wig\u00b7kei\u00b7ten", "zwang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Hier l\u00e4chelte sie mir,", "tokens": ["Hier", "l\u00e4\u00b7chel\u00b7te", "sie", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Hier hielt ich ihre Hand,", "tokens": ["Hier", "hielt", "ich", "ih\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Hier ihrer Lippe sich", "tokens": ["Hier", "ih\u00b7rer", "Lip\u00b7pe", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Das s\u00fc\u00dfe Ja entwand.", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "Ja", "ent\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Ihr sahet all' mein Gl\u00fcck", "tokens": ["Ihr", "sa\u00b7het", "all'", "mein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "War't Zeugen meiner Lust;", "tokens": ["Wa\u00b7r't", "Zeu\u00b7gen", "mei\u00b7ner", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Zu euch, zu euch zur\u00fcck", "tokens": ["Zu", "euch", ",", "zu", "euch", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Zieht es mich unbewu\u00dft,", "tokens": ["Zieht", "es", "mich", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.30": {"text": "Hier lausch' ich, ob ihr Tritt", "tokens": ["Hier", "lausch'", "ich", ",", "ob", "ihr", "Tritt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Nicht in den B\u00fcschen rausche \u2013", "tokens": ["Nicht", "in", "den", "B\u00fc\u00b7schen", "rau\u00b7sche", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Und \u2013 ach, ich wei\u00df es doch,", "tokens": ["Und", "\u2013", "ach", ",", "ich", "wei\u00df", "es", "doch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "XY", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Da\u00df ich vergebens lausche!", "tokens": ["Da\u00df", "ich", "ver\u00b7ge\u00b7bens", "lau\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Deckt nicht das k\u00fchle Grab,", "tokens": ["Deckt", "nicht", "das", "k\u00fch\u00b7le", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Was ich so hei\u00df geliebt?", "tokens": ["Was", "ich", "so", "hei\u00df", "ge\u00b7liebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Nacht, senke dich herab,", "tokens": ["Nacht", ",", "sen\u00b7ke", "dich", "her\u00b7ab", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.37": {"text": "Die keinen Morgen giebt!", "tokens": ["Die", "kei\u00b7nen", "Mor\u00b7gen", "giebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}