{"dta.poem.6537": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So fern die Farben nun", "tokens": ["So", "fern", "die", "Far\u00b7ben", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vom Lichte, wie sie w\u00fcrcklich thun", "tokens": ["Vom", "Lich\u00b7te", ",", "wie", "sie", "w\u00fcrck\u00b7lich", "thun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entspriessen und abhangend stammen,", "tokens": ["Ent\u00b7spries\u00b7sen", "und", "ab\u00b7han\u00b7gend", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und da\u00df wir sehn, wie sie so fest zusammen", "tokens": ["Und", "da\u00df", "wir", "sehn", ",", "wie", "sie", "so", "fest", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVINF", "$,", "PWAV", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Berbunden sind, wo anders der Verband", "tokens": ["Ber\u00b7bun\u00b7den", "sind", ",", "wo", "an\u00b7ders", "der", "Ver\u00b7band"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den forschenden Verstand", "tokens": ["Den", "for\u00b7schen\u00b7den", "Ver\u00b7stand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Begleitet, wie er soll; so werden wir ergr\u00fcnden,", "tokens": ["Be\u00b7glei\u00b7tet", ",", "wie", "er", "soll", ";", "so", "wer\u00b7den", "wir", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "VMFIN", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dann das Wesen erst gef\u00e4rbter C\u00f6rper finden,", "tokens": ["Und", "dann", "das", "We\u00b7sen", "erst", "ge\u00b7f\u00e4rb\u00b7ter", "C\u00f6r\u00b7per", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df die Ver\u00e4nderung, woran", "tokens": ["Da\u00df", "die", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", ",", "wo\u00b7ran"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sich unsre Aug\u2019 empfindlich laben", "tokens": ["Sich", "uns\u00b7re", "Aug'", "emp\u00b7find\u00b7lich", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ja selbst das Hertz ergetzen kan,", "tokens": ["Ja", "selbst", "das", "Hertz", "er\u00b7get\u00b7zen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein Gut sey so sie blo\u00df vom Licht erborget haben.", "tokens": ["Ein", "Gut", "sey", "so", "sie", "blo\u00df", "vom", "Licht", "er\u00b7bor\u00b7get", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPER", "ADV", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}