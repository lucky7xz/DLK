{"textgrid.poem.57633": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das alte Lied, das alte Leiden,", "tokens": ["Das", "al\u00b7te", "Lied", ",", "das", "al\u00b7te", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das jeden Menschen einst betr\u00fcbt:", "tokens": ["Das", "je\u00b7den", "Men\u00b7schen", "einst", "be\u00b7tr\u00fcbt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ade, ade, jetzt mu\u00df ich scheiden", "tokens": ["A\u00b7de", ",", "a\u00b7de", ",", "jetzt", "mu\u00df", "ich", "schei\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Von dir, die ich so sehr geliebt.", "tokens": ["Von", "dir", ",", "die", "ich", "so", "sehr", "ge\u00b7liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer kann es sagen, kann es wissen,", "tokens": ["Wer", "kann", "es", "sa\u00b7gen", ",", "kann", "es", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob er die Lieben wiedersieht;", "tokens": ["Ob", "er", "die", "Lie\u00b7ben", "wie\u00b7der\u00b7sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein letzter Gru\u00df, ein letztes K\u00fcssen,", "tokens": ["Ein", "letz\u00b7ter", "Gru\u00df", ",", "ein", "letz\u00b7tes", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das alte Leid, das alte Lied.", "tokens": ["Das", "al\u00b7te", "Leid", ",", "das", "al\u00b7te", "Lied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun reich' mir deine beiden H\u00e4nde,", "tokens": ["Nun", "reich'", "mir", "dei\u00b7ne", "bei\u00b7den", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den letzten Ku\u00df, leb wohl, ade!", "tokens": ["Den", "letz\u00b7ten", "Ku\u00df", ",", "leb", "wohl", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "$,", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So la\u00df mich los und mach' ein Ende \u2013", "tokens": ["So", "la\u00df", "mich", "los", "und", "mach'", "ein", "En\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wei\u00df, ob ich dich wiederseh ...", "tokens": ["Wer", "wei\u00df", ",", "ob", "ich", "dich", "wie\u00b7der\u00b7seh", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}