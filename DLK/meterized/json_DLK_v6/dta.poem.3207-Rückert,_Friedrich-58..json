{"dta.poem.3207": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl ist's ein s\u00fc\u00df Gef\u00fchl, etwas gethan zu haben,", "tokens": ["Wohl", "ist's", "ein", "s\u00fc\u00df", "Ge\u00b7f\u00fchl", ",", "et\u00b7was", "ge\u00b7than", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schon auch etwas nur gelitten, mag dich laben;", "tokens": ["Doch", "schon", "auch", "et\u00b7was", "nur", "ge\u00b7lit\u00b7ten", ",", "mag", "dich", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ADV", "VVPP", "$,", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du auch weiter nichts vollbracht, nur hast erfahren,", "tokens": ["Wenn", "du", "auch", "wei\u00b7ter", "nichts", "voll\u00b7bracht", ",", "nur", "hast", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PIS", "VVPP", "$,", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was deine Fasern auszuhalten f\u00e4hig waren.", "tokens": ["Was", "dei\u00b7ne", "Fa\u00b7sern", "aus\u00b7zu\u00b7hal\u00b7ten", "f\u00e4\u00b7hig", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVIZU", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}