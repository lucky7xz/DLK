{"textgrid.poem.67014": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Unverzeihlich", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder Mensch lebt noch immer in M\u00fcnchen", "tokens": ["\u00bb", "der", "Mensch", "lebt", "noch", "im\u00b7mer", "in", "M\u00fcn\u00b7chen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "NE"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und schreibt, ein vulg\u00e4rer Poet,", "tokens": ["Und", "schreibt", ",", "ein", "vul\u00b7g\u00e4\u00b7rer", "Po\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u2013 Wir wollen ihn st\u00e4upen und lynchen \u2013", "tokens": ["\u2013", "Wir", "wol\u00b7len", "ihn", "st\u00e4u\u00b7pen", "und", "lyn\u00b7chen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Schreibt Verse noch, die man ", "tokens": ["Schreibt", "Ver\u00b7se", "noch", ",", "die", "man"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "ADV", "$,", "PRELS", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}