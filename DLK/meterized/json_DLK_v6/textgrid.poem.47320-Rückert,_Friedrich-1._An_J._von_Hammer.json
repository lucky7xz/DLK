{"textgrid.poem.47320": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1. An J. von Hammer", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst am bl\u00fchenden Rosenhag sprach mit wichtiger Miene", "tokens": ["J\u00fcngst", "am", "bl\u00fc\u00b7hen\u00b7den", "Ro\u00b7sen\u00b7hag", "sprach", "mit", "wich\u00b7ti\u00b7ger", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Gegen S\u00e4ngerin Nachtigall Honigsammlerin Biene:", "tokens": ["Ge\u00b7gen", "S\u00e4n\u00b7ge\u00b7rin", "Nach\u00b7ti\u00b7gall", "Ho\u00b7ni\u00b7gsamm\u00b7le\u00b7rin", "Bie\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "NN", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Immer saugest du Rosenduft, immer Duft nur der Rosen,", "tokens": ["Im\u00b7mer", "sau\u00b7gest", "du", "Ro\u00b7sen\u00b7duft", ",", "im\u00b7mer", "Duft", "nur", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADV", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Kosest immer vom gl\u00fchenden Rosenlippenrubine.", "tokens": ["Ko\u00b7sest", "im\u00b7mer", "vom", "gl\u00fc\u00b7hen\u00b7den", "Ro\u00b7sen\u00b7lip\u00b7pen\u00b7ru\u00b7bi\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Zur Werkst\u00e4tte von meinem Flei\u00df dient dagegen mir jede", "tokens": ["Zur", "Werk\u00b7st\u00e4t\u00b7te", "von", "mei\u00b7nem", "Flei\u00df", "dient", "da\u00b7ge\u00b7gen", "mir", "je\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PAV", "PPER", "PIAT"], "meter": "-++--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Von den Knospen des Fr\u00fchlinges zur Entfaltung gedieh'ne.", "tokens": ["Von", "den", "Knos\u00b7pen", "des", "Fr\u00fch\u00b7lin\u00b7ges", "zur", "Ent\u00b7fal\u00b7tung", "ge\u00b7dieh'\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPRART", "NN", "ADJA", "$."], "meter": "--+--++-+-+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Denn zum k\u00f6stlichen Honigseim umzuwandeln versteh' ich", "tokens": ["Denn", "zum", "k\u00f6st\u00b7li\u00b7chen", "Ho\u00b7ni\u00b7gseim", "um\u00b7zu\u00b7wan\u00b7deln", "ver\u00b7steh'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVIZU", "VVFIN", "PPER"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Alles S\u00fc\u00dfe, ohn' Unterschied allen Kelchen Entlieh'ne.", "tokens": ["Al\u00b7les", "S\u00fc\u00b7\u00dfe", ",", "ohn'", "Un\u00b7ter\u00b7schied", "al\u00b7len", "Kel\u00b7chen", "Ent\u00b7lieh'\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "NN", "PIAT", "NN", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Ob der Bl\u00fcte die Farbe fehlt, leicht verzeih' ich den Fehler,", "tokens": ["Ob", "der", "Bl\u00fc\u00b7te", "die", "Far\u00b7be", "fehlt", ",", "leicht", "ver\u00b7zeih'", "ich", "den", "Feh\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Nur der fehlende Nektar bleibt das von mir Unverzieh'ne.", "tokens": ["Nur", "der", "feh\u00b7len\u00b7de", "Nek\u00b7tar", "bleibt", "das", "von", "mir", "Un\u00b7ver\u00b7zieh'\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ART", "APPR", "PPER", "NN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Leider, da\u00df mir der Flug versagt, um zu sehn, ob zu holen", "tokens": ["Lei\u00b7der", ",", "da\u00df", "mir", "der", "Flug", "ver\u00b7sagt", ",", "um", "zu", "sehn", ",", "ob", "zu", "ho\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,", "KOUI", "PTKZU", "VVINF", "$,", "KOUS", "PTKZU", "VVINF"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Duft nicht sei aus des bl\u00fchenden Morgenrotes Karmine.", "tokens": ["Duft", "nicht", "sei", "aus", "des", "bl\u00fc\u00b7hen\u00b7den", "Mor\u00b7gen\u00b7ro\u00b7tes", "Kar\u00b7mi\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VAFIN", "APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Darum bin ich durch Emsigkeit die im Land Ber\u00fchmte,", "tokens": ["Da\u00b7rum", "bin", "ich", "durch", "Em\u00b7sig\u00b7keit", "die", "im", "Land", "Be\u00b7r\u00fchm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "NN", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Du, Verliebte, durch M\u00fc\u00dfiggang bleibst mit Recht die Verschrie'ne.", "tokens": ["Du", ",", "Ver\u00b7lieb\u00b7te", ",", "durch", "M\u00fc\u00b7\u00dfig\u00b7gang", "bleibst", "mit", "Recht", "die", "Ver\u00b7schrie'\u00b7ne", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "$,", "APPR", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Sieh, derweil du dich abgeh\u00e4rmt hast am Dorne der Rosen,", "tokens": ["Sieh", ",", "der\u00b7weil", "du", "dich", "ab\u00b7ge\u00b7h\u00e4rmt", "hast", "am", "Dor\u00b7ne", "der", "Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PRF", "VVPP", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "---+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Stieg ich duftend aus Veilchenscho\u00df mit vergoldeter Schiene.", "tokens": ["Stieg", "ich", "duf\u00b7tend", "aus", "Veil\u00b7chen\u00b7scho\u00df", "mit", "ver\u00b7gol\u00b7de\u00b7ter", "Schie\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "NE", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.17": {"text": "Und nun sage mit Einem Wort, ob du selber nicht meinest,", "tokens": ["Und", "nun", "sa\u00b7ge", "mit", "Ei\u00b7nem", "Wort", ",", "ob", "du", "sel\u00b7ber", "nicht", "mei\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Da\u00df ich Kleine den Preis vor dir, stolze Gro\u00dfe! verdiene?", "tokens": ["Da\u00df", "ich", "Klei\u00b7ne", "den", "Preis", "vor", "dir", ",", "stol\u00b7ze", "Gro\u00b7\u00dfe", "!", "ver\u00b7die\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "ART", "NN", "APPR", "PPER", "$,", "ADJA", "NN", "$.", "VVFIN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Oder, willst du noch streiten, la\u00df zum Schiedsrichter uns w\u00e4hlen", "tokens": ["O\u00b7der", ",", "willst", "du", "noch", "strei\u00b7ten", ",", "la\u00df", "zum", "Schieds\u00b7rich\u00b7ter", "uns", "w\u00e4h\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "ADV", "VVFIN", "$,", "VVIMP", "APPRART", "NN", "PPER", "VVINF"], "meter": "--+--+-+-++--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Den Dolmetschen der Pforte dort im hocht\u00fcrmenden Wiene,", "tokens": ["Den", "Dol\u00b7met\u00b7schen", "der", "Pfor\u00b7te", "dort", "im", "hoch\u00b7t\u00fcr\u00b7men\u00b7den", "Wie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+---+-", "measure": "unknown.measure.hexa"}, "line.21": {"text": "Der, so hat mir Hafis gesagt, l\u00f6st mit gl\u00fccklicher Schnelle", "tokens": ["Der", ",", "so", "hat", "mir", "Ha\u00b7fis", "ge\u00b7sagt", ",", "l\u00f6st", "mit", "gl\u00fcck\u00b7li\u00b7cher", "Schnel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "ADV", "VAFIN", "PPER", "NE", "VVPP", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Jedes R\u00e4tsel aus Osten, das schwierig anderen schiene.", "tokens": ["Je\u00b7des", "R\u00e4t\u00b7sel", "aus", "Os\u00b7ten", ",", "das", "schwie\u00b7rig", "an\u00b7de\u00b7ren", "schie\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,", "PRELS", "ADJD", "ADJA", "ADJA", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}}}}}