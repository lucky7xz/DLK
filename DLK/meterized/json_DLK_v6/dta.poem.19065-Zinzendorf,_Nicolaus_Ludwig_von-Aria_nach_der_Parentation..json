{"dta.poem.19065": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Aria   nach der   Parentation.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Die Christen gehn von Ort zu Ort,", "tokens": ["Die", "Chris\u00b7ten", "gehn", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gerade durch den Jammer,", "tokens": ["Ge\u00b7ra\u00b7de", "durch", "den", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kommen in den Friedens-Port,", "tokens": ["Und", "kom\u00b7men", "in", "den", "Frie\u00b7dens\u00b7Port", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruhn in ihrer Kammer.", "tokens": ["Und", "ruhn", "in", "ih\u00b7rer", "Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gott h\u00e4lt der Seelen Lauf", "tokens": ["Gott", "h\u00e4lt", "der", "See\u00b7len", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch sein Umarmen auf;", "tokens": ["Durch", "sein", "Um\u00b7ar\u00b7men", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Weitzen-Korn wird in sein Beet,", "tokens": ["Das", "Weit\u00b7zen\u00b7Korn", "wird", "in", "sein", "Beet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf Hofnung reicher Frucht, ges\u00e4t.", "tokens": ["Auf", "Hof\u00b7nung", "rei\u00b7cher", "Frucht", ",", "ge\u00b7s\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie seyd ihr doch so wohl gereist,", "tokens": ["Wie", "seyd", "ihr", "doch", "so", "wohl", "ge\u00b7reist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gelobt seyn eure Schritte,", "tokens": ["Ge\u00b7lobt", "seyn", "eu\u00b7re", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du allbereit befreyter Geist,", "tokens": ["Du", "all\u00b7be\u00b7reit", "be\u00b7frey\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du noch verschlo\u00dfne H\u00fctte!", "tokens": ["Du", "noch", "ver\u00b7schlo\u00df\u00b7ne", "H\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den r\u00fchrt der Br\u00e4utigam", "tokens": ["Den", "r\u00fchrt", "der", "Br\u00e4u\u00b7ti\u00b7gam"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit sanfter Liebes-Flamm;", "tokens": ["Mit", "sanf\u00b7ter", "Lie\u00b7bes\u00b7Flamm", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die deckt in ungest\u00f6hrter Ruh", "tokens": ["Die", "deckt", "in", "un\u00b7ge\u00b7st\u00f6hr\u00b7ter", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Liebe stiller Schatten zu.", "tokens": ["Der", "Lie\u00b7be", "stil\u00b7ler", "Schat\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir freun uns in Gelassenheit", "tokens": ["Wir", "freun", "uns", "in", "Ge\u00b7las\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der grossen Offenbahrung.", "tokens": ["Der", "gros\u00b7sen", "Of\u00b7fen\u00b7bah\u00b7rung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Indessen bleibt dein Pilger-Kleid", "tokens": ["In\u00b7des\u00b7sen", "bleibt", "dein", "Pil\u00b7ger\u00b7Kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In heiliger Verwahrung.", "tokens": ["In", "hei\u00b7li\u00b7ger", "Ver\u00b7wah\u00b7rung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie ist dein Gl\u00fcck so gro\u00df!", "tokens": ["Wie", "ist", "dein", "Gl\u00fcck", "so", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sey froh im Gnaden-Schoo\u00df!", "tokens": ["Sey", "froh", "im", "Gna\u00b7den\u00b7Schoo\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Liebe f\u00fchr uns gleiche Bahn,", "tokens": ["Die", "Lie\u00b7be", "f\u00fchr", "uns", "glei\u00b7che", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So tief hinab, so hoch hinan!", "tokens": ["So", "tief", "hin\u00b7ab", ",", "so", "hoch", "hi\u00b7nan", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}