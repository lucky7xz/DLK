{"textgrid.poem.62176": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[zertr\u00fcmmert das Leben]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zertr\u00fcmmert das Leben,", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mert", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zertr\u00fcmmert das Gl\u00fcck,", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mert", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Freuden, sie schweben,", "tokens": ["Die", "Freu\u00b7den", ",", "sie", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ach, niemals zur\u00fcck.", "tokens": ["Ach", ",", "nie\u00b7mals", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Geopfert das Leben", "tokens": ["Ge\u00b7o\u00b7pfert", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Der h\u00f6chsten Idee \u2013", "tokens": ["Der", "h\u00f6chs\u00b7ten", "I\u00b7dee", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Umsonst es gegeben \u2013", "tokens": ["Um\u00b7sonst", "es", "ge\u00b7ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wem t\u00e4t' es nicht weh?", "tokens": ["Wem", "t\u00e4t'", "es", "nicht", "weh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Geopfert das Leben", "tokens": ["Ge\u00b7o\u00b7pfert", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Dem menschlichen Gl\u00fcck,", "tokens": ["Dem", "menschli\u00b7chen", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sie schlugen das Streben,", "tokens": ["Sie", "schlu\u00b7gen", "das", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zerschlugen's in St\u00fcck'. \u2013", "tokens": ["Zer\u00b7schlu\u00b7gen's", "in", "St\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Geopfert das Leben", "tokens": ["Ge\u00b7o\u00b7pfert", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Der h\u00f6chsten Idee \u2013", "tokens": ["Der", "h\u00f6chs\u00b7ten", "I\u00b7dee", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Umsonst es gegeben:", "tokens": ["Um\u00b7sonst", "es", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wem t\u00e4t' es nicht weh!", "tokens": ["Wem", "t\u00e4t'", "es", "nicht", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}