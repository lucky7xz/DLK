{"textgrid.poem.59422": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Kurze Fahrt", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Posthorn, wie so keck und fr\u00f6hlich", "tokens": ["Post\u00b7horn", ",", "wie", "so", "keck", "und", "fr\u00f6h\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brachst du einst den Morgen an,", "tokens": ["Brachst", "du", "einst", "den", "Mor\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor mir lag's so fr\u00fchlingsselig,", "tokens": ["Vor", "mir", "lag's", "so", "fr\u00fch\u00b7lings\u00b7se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich still auf Lieder sann.", "tokens": ["Da\u00df", "ich", "still", "auf", "Lie\u00b7der", "sann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dunkel rauscht es schon im Walde,", "tokens": ["Dun\u00b7kel", "rauscht", "es", "schon", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie so abendk\u00fchl wird's hier,", "tokens": ["Wie", "so", "a\u00b7bend\u00b7k\u00fchl", "wird's", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwager, sto\u00df ins Horn \u2013 wie balde", "tokens": ["Schwa\u00b7ger", ",", "sto\u00df", "ins", "Horn", "\u2013", "wie", "bal\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "$(", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind auch wir im Nachtquartier!", "tokens": ["Sind", "auch", "wir", "im", "Nacht\u00b7quar\u00b7tier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}