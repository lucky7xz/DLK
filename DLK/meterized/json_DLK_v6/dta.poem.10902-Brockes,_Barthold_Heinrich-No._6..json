{"dta.poem.10902": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht geschwinde! wie so rasch, munter, fertig, schnell und\nleicht,", "tokens": ["Seht", "ge\u00b7schwin\u00b7de", "!", "wie", "so", "rasch", ",", "mun\u00b7ter", ",", "fer\u00b7tig", ",", "schnell", "und", "leicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "$.", "PWAV", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Hier der Hirsch, auf flacher Ebne, nach dem Walde springt und", "tokens": ["Hier", "der", "Hirsch", ",", "auf", "fla\u00b7cher", "Eb\u00b7ne", ",", "nach", "dem", "Wal\u00b7de", "springt", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Er ist in so reger Stellung, da\u00df sein Fliehn ich nicht nur sehe,", "tokens": ["Er", "ist", "in", "so", "re\u00b7ger", "Stel\u00b7lung", ",", "da\u00df", "sein", "Fliehn", "ich", "nicht", "nur", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Sondern fast das Strampfen h\u00f6r\u2019. Seht, wie lieblich, von der", "tokens": ["Son\u00b7dern", "fast", "das", "Stramp\u00b7fen", "h\u00f6r'", ".", "Seht", ",", "wie", "lieb\u00b7lich", ",", "von", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$.", "VVFIN", "$,", "PWAV", "ADJD", "$,", "APPR", "ART"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Dort die langen Schatten fallen, und den k\u00fchlen Abend weisen;", "tokens": ["Dort", "die", "lan\u00b7gen", "Schat\u00b7ten", "fal\u00b7len", ",", "und", "den", "k\u00fch\u00b7len", "A\u00b7bend", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,", "KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Selbst in der Copie der Anmuth, kann man hier den Sch\u00f6p-", "tokens": ["Selbst", "in", "der", "Co\u00b7pie", "der", "An\u00b7muth", ",", "kann", "man", "hier", "den", "Sch\u00f6p"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "VMFIN", "PIS", "ADV", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Denn mich deucht, ich w\u00e4r im Felde, bey gek\u00fchlter Abendzeit,", "tokens": ["Denn", "mich", "deucht", ",", "ich", "w\u00e4r", "im", "Fel\u00b7de", ",", "bey", "ge\u00b7k\u00fchl\u00b7ter", "A\u00b7bend\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und bewunderte der Sonnen untergehnde Herrlichkeit.", "tokens": ["Und", "be\u00b7wun\u00b7der\u00b7te", "der", "Son\u00b7nen", "un\u00b7ter\u00b7gehn\u00b7de", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ist die Kunst nicht hochzusch\u00e4tzen, da durch sie wir, wie so sch\u00f6n", "tokens": ["Ist", "die", "Kunst", "nicht", "hoch\u00b7zu\u00b7sch\u00e4t\u00b7zen", ",", "da", "durch", "sie", "wir", ",", "wie", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$,", "KOUS", "APPR", "PPER", "PPER", "$,", "PWAV", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Die im Fr\u00fchling sch\u00f6ne Welt, auch im Frost, in Zimmern", "tokens": ["Die", "im", "Fr\u00fch\u00b7ling", "sch\u00f6\u00b7ne", "Welt", ",", "auch", "im", "Frost", ",", "in", "Zim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADJA", "NN", "$,", "ADV", "APPRART", "NN", "$,", "APPR", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wann du der Gesch\u00f6pfe Sch\u00f6nheit, durch das Aug, uns ein-", "tokens": ["Wann", "du", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Sch\u00f6n\u00b7heit", ",", "durch", "das", "Aug", ",", "uns", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "$,", "APPR", "ART", "NN", "$,", "PPER", "TRUNC"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "R\u00fchrest du, durch deine Hand, Ridinger, uns unser Herz.", "tokens": ["R\u00fch\u00b7rest", "du", ",", "durch", "dei\u00b7ne", "Hand", ",", "Ri\u00b7din\u00b7ger", ",", "uns", "un\u00b7ser", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.13": {"text": "Eines guten Schreibers Griffel ist dein Griffel. Denn du", "tokens": ["Ei\u00b7nes", "gu\u00b7ten", "Schrei\u00b7bers", "Grif\u00b7fel", "ist", "dein", "Grif\u00b7fel", ".", "Denn", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$.", "KON", "PPER"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.14": {"text": "Unsers gro\u00dfen Sch\u00f6pfers Thaten, wirklich in der That, in Erz.", "tokens": ["Un\u00b7sers", "gro\u00b7\u00dfen", "Sch\u00f6p\u00b7fers", "Tha\u00b7ten", ",", "wirk\u00b7lich", "in", "der", "That", ",", "in", "Erz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}