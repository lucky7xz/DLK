{"dta.poem.2123": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einige Gedanken \u00fcber Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wovon ein\u2019 jede, durch den Wechsel der Farben, die be-", "tokens": ["Wo\u00b7von", "ein'", "je\u00b7de", ",", "durch", "den", "Wech\u00b7sel", "der", "Far\u00b7ben", ",", "die", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "PIAT", "$,", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "flammte R\u00f6the", "tokens": ["flamm\u00b7te", "R\u00f6\u00b7the"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und die Rubinen gleiche Pracht, der vielen Rosen, noch", "tokens": ["Und", "die", "Ru\u00b7bi\u00b7nen", "glei\u00b7che", "Pracht", ",", "der", "vie\u00b7len", "Ro\u00b7sen", ",", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "erh\u00f6hte.", "tokens": ["er\u00b7h\u00f6h\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ich wu\u00dfte nicht, was ich, vor Lust, beginnen oder sa-", "tokens": ["Ich", "wu\u00df\u00b7te", "nicht", ",", "was", "ich", ",", "vor", "Lust", ",", "be\u00b7gin\u00b7nen", "o\u00b7der", "sa"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "$,", "APPR", "NN", "$,", "VVINF", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "gen sollte,", "tokens": ["gen", "soll\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VMFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Nur f\u00fchlt\u2019 ich, da\u00df ein reges Heer vergn\u00fcglicher Jdeen,", "tokens": ["Nur", "f\u00fchlt'", "ich", ",", "da\u00df", "ein", "re\u00b7ges", "Heer", "ver\u00b7gn\u00fcg\u00b7li\u00b7cher", "Jdeen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "mir", "tokens": ["mir"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "In meiner frohen Phantasey, sich selber jagend, gleich-", "tokens": ["In", "mei\u00b7ner", "fro\u00b7hen", "Phan\u00b7ta\u00b7sey", ",", "sich", "sel\u00b7ber", "ja\u00b7gend", ",", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRF", "ADV", "ADJD", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "sam rollte.", "tokens": ["sam", "roll\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ach! rief ich, unausdr\u00fccklich sch\u00f6n ist der vereinten", "tokens": ["Ach", "!", "rief", "ich", ",", "un\u00b7aus\u00b7dr\u00fcck\u00b7lich", "sch\u00f6n", "ist", "der", "ver\u00b7ein\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "ADJD", "ADJD", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Blumen Zier!", "tokens": ["Blu\u00b7men", "Zier", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Es ist auf dieser ganzen Erden kein auserlesners Schau-", "tokens": ["Es", "ist", "auf", "die\u00b7ser", "gan\u00b7zen", "Er\u00b7den", "kein", "au\u00b7ser\u00b7les\u00b7ners", "Schau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "ADJA", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "gericht!", "tokens": ["ge\u00b7richt", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Zuletzt fiel dieser Schlu\u00df mir bey: Wie weis der Sch\u00f6-", "tokens": ["Zu\u00b7letzt", "fiel", "die\u00b7ser", "Schlu\u00df", "mir", "bey", ":", "Wie", "weis", "der", "Sch\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "PPER", "PTKVZ", "$.", "PWAV", "PTKVZ", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "pfer, durchs Gesicht,", "tokens": ["pfer", ",", "durchs", "Ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Durch die Vortrefflichkeit, die Sch\u00f6nheit und Lieblich-", "tokens": ["Durch", "die", "Vor\u00b7treff\u00b7lich\u00b7keit", ",", "die", "Sch\u00f6n\u00b7heit", "und", "Lieb\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "KON", "TRUNC"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "keiten seiner Gaben", "tokens": ["kei\u00b7ten", "sei\u00b7ner", "Ga\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Die Seele, schon auf dieser Welt, fast zu beseligen, zu", "tokens": ["Die", "See\u00b7le", ",", "schon", "auf", "die\u00b7ser", "Welt", ",", "fast", "zu", "be\u00b7se\u00b7li\u00b7gen", ",", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PDAT", "NN", "$,", "ADV", "PTKZU", "VVINF", "$,", "APPR"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.20": {"text": "laben!", "tokens": ["la\u00b7ben", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Was l\u00e4\u00dft uns die Betrachtung nicht,", "tokens": ["Was", "l\u00e4\u00dft", "uns", "die", "Be\u00b7trach\u00b7tung", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Von einem solchen Wesen hoffen, das, zum Genu\u00df von", "tokens": ["Von", "ei\u00b7nem", "sol\u00b7chen", "We\u00b7sen", "hof\u00b7fen", ",", "das", ",", "zum", "Ge\u00b7nu\u00df", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN", "VVINF", "$,", "PDS", "$,", "APPRART", "NN", "APPR"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Himmelssch\u00e4tzen", "tokens": ["Him\u00b7mels\u00b7sch\u00e4t\u00b7zen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "Und einst zu einem ewgen Gl\u00fcck, uns blo\u00df aus Huld und", "tokens": ["Und", "einst", "zu", "ei\u00b7nem", "ew\u00b7gen", "Gl\u00fcck", ",", "uns", "blo\u00df", "aus", "Huld", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "ADV", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Lieb erw\u00e4hlt:", "tokens": ["Lieb", "er\u00b7w\u00e4hlt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "Dem, um uns ewig zu ergetzen,", "tokens": ["Dem", ",", "um", "uns", "e\u00b7wig", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Kein Wollen, kein Verm\u00f6gen fehlt!", "tokens": ["Kein", "Wol\u00b7len", ",", "kein", "Ver\u00b7m\u00f6\u00b7gen", "fehlt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}