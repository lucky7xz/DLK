{"dta.poem.16124": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "178.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist der Weg, mein Sohn, an dem du noch nicht bist,", "tokens": ["Was", "ist", "der", "Weg", ",", "mein", "Sohn", ",", "an", "dem", "du", "noch", "nicht", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der gleich dem vor'gen lang, und doch viel k\u00fcrzer ist?", "tokens": ["Der", "gleich", "dem", "vor'\u00b7gen", "lang", ",", "und", "doch", "viel", "k\u00fcr\u00b7zer", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "ADJD", "$,", "KON", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das ist der Weg den Berg hinab, den ich nun schreite,", "tokens": ["Das", "ist", "der", "Weg", "den", "Berg", "hin\u00b7ab", ",", "den", "ich", "nun", "schrei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel langsamer kam ich herauf die andre Seite.", "tokens": ["Viel", "lang\u00b7sa\u00b7mer", "kam", "ich", "her\u00b7auf", "die", "and\u00b7re", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dort war ich r\u00fcstiger, doch ward der Weg mir l\u00e4nger,", "tokens": ["Dort", "war", "ich", "r\u00fcs\u00b7ti\u00b7ger", ",", "doch", "ward", "der", "Weg", "mir", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier wird er k\u00fcrzer mir, dem doch schon m\u00fcden G\u00e4nger.", "tokens": ["Hier", "wird", "er", "k\u00fcr\u00b7zer", "mir", ",", "dem", "doch", "schon", "m\u00fc\u00b7den", "G\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPER", "$,", "PRELS", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}