{"dta.poem.9597": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An ihre augen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Strahlt/ sch\u00f6nste sonnen/ mich mit holden blicken an/", "tokens": ["Strahlt", "/", "sch\u00f6ns\u00b7te", "son\u00b7nen", "/", "mich", "mit", "hol\u00b7den", "bli\u00b7cken", "an", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr wi\u00dft/ was eure glut in meiner seelen kan;", "tokens": ["Ihr", "wi\u00dft", "/", "was", "eu\u00b7re", "glut", "in", "mei\u00b7ner", "see\u00b7len", "kan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "APPR", "PPOSAT", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch nein! versteckt euch nur/ ihr spiegel harter sinnen/", "tokens": ["Doch", "nein", "!", "ver\u00b7steckt", "euch", "nur", "/", "ihr", "spie\u00b7gel", "har\u00b7ter", "sin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "VVFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was n\u00fctzt mir eure lust/ und angenehmer schertz?", "tokens": ["Was", "n\u00fctzt", "mir", "eu\u00b7re", "lust", "/", "und", "an\u00b7ge\u00b7neh\u00b7mer", "schertz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aurora bleibt doch stoltz/ und ihr ergrimmtes hertz", "tokens": ["Au\u00b7ro\u00b7ra", "bleibt", "doch", "stoltz", "/", "und", "ihr", "er\u00b7grimm\u00b7tes", "hertz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "$(", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan niemand durch gewalt noch bitte mehr gewinnen.", "tokens": ["Kan", "nie\u00b7mand", "durch", "ge\u00b7walt", "noch", "bit\u00b7te", "mehr", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}