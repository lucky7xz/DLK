{"textgrid.poem.51993": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du darffst dir kein Pulpet zu diesem Buche machen,", "tokens": ["Du", "darffst", "dir", "kein", "Pul\u00b7pet", "zu", "die\u00b7sem", "Bu\u00b7che", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch in dem Zimmer es bedachtsam \u00fcbersehn:", "tokens": ["Noch", "in", "dem", "Zim\u00b7mer", "es", "be\u00b7dacht\u00b7sam", "\u00fc\u00b7ber\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn ist es rechte Zeit, die Bl\u00e4tter umbzudrehn,", "tokens": ["Denn", "ist", "es", "rech\u00b7te", "Zeit", ",", "die", "Bl\u00e4t\u00b7ter", "umb\u00b7zu\u00b7drehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn du bey Tische sitzst und trinckst und pflegst zu lachen.", "tokens": ["Wenn", "du", "bey", "Ti\u00b7sche", "sitz\u00b7st", "und", "trinckst", "und", "pflegst", "zu", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PTKZU", "VVINF", "$."], 