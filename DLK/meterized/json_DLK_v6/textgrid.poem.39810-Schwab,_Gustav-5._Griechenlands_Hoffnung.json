{"textgrid.poem.39810": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "5. Griechenlands Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging das Jahr in mattem Schlummer", "tokens": ["Es", "ging", "das", "Jahr", "in", "mat\u00b7tem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verachtet seinem Ende zu,", "tokens": ["Ver\u00b7ach\u00b7tet", "sei\u00b7nem", "En\u00b7de", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Osten w\u00fchlt der alte Kummer,", "tokens": ["Im", "Os\u00b7ten", "w\u00fchlt", "der", "al\u00b7te", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und um uns her ist Grabesruh;", "tokens": ["Und", "um", "uns", "her", "ist", "Gra\u00b7be\u00b7sruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "APZR", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Licht der Wahrheit \u2013 mag's ersterben!", "tokens": ["Das", "Licht", "der", "Wahr\u00b7heit", "\u2013", "mag's", "ers\u00b7ter\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "NE", "VVPP", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das Volk der Freiheit \u2013 mag's verderben!", "tokens": ["Das", "Volk", "der", "Frei\u00b7heit", "\u2013", "mag's", "ver\u00b7der\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geht, hoffet noch auf Wunderwerke,", "tokens": ["Geht", ",", "hof\u00b7fet", "noch", "auf", "Wun\u00b7der\u00b7wer\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und glaubt, da\u00df euer rost'ger Stahl,", "tokens": ["Und", "glaubt", ",", "da\u00df", "eu\u00b7er", "rost'\u00b7ger", "Stahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hineingesandt, die Schwachen st\u00e4rke,", "tokens": ["Hin\u00b7ein\u00b7ge\u00b7sandt", ",", "die", "Schwa\u00b7chen", "st\u00e4r\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu trotzen Feinden ohne Zahl!", "tokens": ["Zu", "trot\u00b7zen", "Fein\u00b7den", "oh\u00b7ne", "Zahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geht, reicht den Weibern, Kindern, Greisen,", "tokens": ["Geht", ",", "reicht", "den", "Wei\u00b7bern", ",", "Kin\u00b7dern", ",", "Grei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcnf Gerstenbrote, sie zu speisen!", "tokens": ["F\u00fcnf", "Gers\u00b7ten\u00b7bro\u00b7te", ",", "sie", "zu", "spei\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So sprach der Zweifel, hohen Hauptes", "tokens": ["So", "sprach", "der", "Zwei\u00b7fel", ",", "ho\u00b7hen", "Haup\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ging er durch unsre Stra\u00dfen hin;", "tokens": ["Ging", "er", "durch", "uns\u00b7re", "Stra\u00b7\u00dfen", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Geiz erfreut's, die Schw\u00e4che glaubt es,", "tokens": ["Den", "Geiz", "er\u00b7freut's", ",", "die", "Schw\u00e4\u00b7che", "glaubt", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der kalten Bosheit d\u00e4ucht's Gewinn:", "tokens": ["Der", "kal\u00b7ten", "Bos\u00b7heit", "d\u00e4ucht's", "Ge\u00b7winn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt ist die letzte Glut verglommen,", "tokens": ["Jetzt", "ist", "die", "letz\u00b7te", "Glut", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ja, bleiern wird die Nacht jetzt kommen!", "tokens": ["Ja", ",", "blei\u00b7ern", "wird", "die", "Nacht", "jetzt", "kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVINF", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und anderw\u00e4rts hebt schon die Schande,", "tokens": ["Und", "an\u00b7der\u00b7w\u00e4rts", "hebt", "schon", "die", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Thorheit schon ihr Banner dreist: \u2013", "tokens": ["Die", "Thor\u00b7heit", "schon", "ihr", "Ban\u00b7ner", "dreist", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da regt sich an Europens Rande", "tokens": ["Da", "regt", "sich", "an", "Eu\u00b7ro\u00b7pens", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der niebezwungne, freie Geist;", "tokens": ["Der", "nie\u00b7be\u00b7zwung\u00b7ne", ",", "frei\u00b7e", "Geist", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Land, um das die Fluten wallen,", "tokens": ["Im", "Land", ",", "um", "das", "die", "Flu\u00b7ten", "wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00e4\u00dft Ein Mann seine Stimme schallen.", "tokens": ["L\u00e4\u00dft", "Ein", "Mann", "sei\u00b7ne", "Stim\u00b7me", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Wer heftet nicht auf Ihn die Blicke,", "tokens": ["Wer", "hef\u00b7tet", "nicht", "auf", "Ihn", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dessen Mund die Rede weht,", "tokens": ["Von", "des\u00b7sen", "Mund", "die", "Re\u00b7de", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df durch die langsamen Geschicke", "tokens": ["Da\u00df", "durch", "die", "lang\u00b7sa\u00b7men", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zeit ein Fieberschauer geht,", "tokens": ["Der", "Zeit", "ein", "Fie\u00b7ber\u00b7schau\u00b7er", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df von seinem Wink ersch\u00fcttert", "tokens": ["Und", "da\u00df", "von", "sei\u00b7nem", "Wink", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der dumpfe, ferne S\u00fcden zittert!", "tokens": ["Der", "dump\u00b7fe", ",", "fer\u00b7ne", "S\u00fc\u00b7den", "zit\u00b7tert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zwar gilt es nicht dem armen Volke,", "tokens": ["Zwar", "gilt", "es", "nicht", "dem", "ar\u00b7men", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das schmachtend nach dem Ritter blickt,", "tokens": ["Das", "schmach\u00b7tend", "nach", "dem", "Rit\u00b7ter", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf das die steh'nde Wetterwolke", "tokens": ["Auf", "das", "die", "steh'n\u00b7de", "Wet\u00b7ter\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vertilgungsstralen niederschickt:", "tokens": ["Ver\u00b7til\u00b7gungs\u00b7stra\u00b7len", "nie\u00b7der\u00b7schickt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch darf das eine Leid schon hoffen,", "tokens": ["Doch", "darf", "das", "ei\u00b7ne", "Leid", "schon", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PDS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn andrem Leid ein Ohr steht offen.", "tokens": ["Wenn", "an\u00b7drem", "Leid", "ein", "Ohr", "steht", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O die ihr Worte habt wie Schwerter,", "tokens": ["O", "die", "ihr", "Wor\u00b7te", "habt", "wie", "Schwer\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beschwingte Schiffe, Waffen, Gold:", "tokens": ["Be\u00b7schwing\u00b7te", "Schif\u00b7fe", ",", "Waf\u00b7fen", ",", "Gold", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort dr\u00e4ngt die Wut, die Not noch h\u00e4rter,", "tokens": ["Dort", "dr\u00e4ngt", "die", "Wut", ",", "die", "Not", "noch", "h\u00e4r\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als wo der M\u00f6nch die Fahn' entrollt;", "tokens": ["Als", "wo", "der", "M\u00f6nch", "die", "Fahn'", "ent\u00b7rollt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort, wo das Sichelschwert seit Jahren", "tokens": ["Dort", ",", "wo", "das", "Si\u00b7chel\u00b7schwert", "seit", "Jah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wild durch die fremde Saat gefahren.", "tokens": ["Wild", "durch", "die", "frem\u00b7de", "Saat", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Die Saat des Korns, die Saat der Helden,", "tokens": ["Die", "Saat", "des", "Korns", ",", "die", "Saat", "der", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der M\u00fctter und der Kinder Saat!", "tokens": ["Der", "M\u00fct\u00b7ter", "und", "der", "Kin\u00b7der", "Saat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Haufen liegen sie und melden,", "tokens": ["In", "Hau\u00b7fen", "lie\u00b7gen", "sie", "und", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was dort der Schnitter niedertrat!", "tokens": ["Was", "dort", "der", "Schnit\u00b7ter", "nie\u00b7der\u00b7trat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort helft, dort stellt euch an die Spitze,", "tokens": ["Dort", "helft", ",", "dort", "stellt", "euch", "an", "die", "Spit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dort schleudert rettend eure Blitze.", "tokens": ["Dort", "schleu\u00b7dert", "ret\u00b7tend", "eu\u00b7re", "Blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihr aber, ihr in allen Landen,", "tokens": ["Ihr", "a\u00b7ber", ",", "ihr", "in", "al\u00b7len", "Lan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die noch erweichet andrer Not,", "tokens": ["Die", "noch", "er\u00b7wei\u00b7chet", "an\u00b7drer", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf, la\u00dft uns r\u00fctteln an den Banden,", "tokens": ["Auf", ",", "la\u00dft", "uns", "r\u00fct\u00b7teln", "an", "den", "Ban\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf, theilet euren Bissen Brot;", "tokens": ["Auf", ",", "thei\u00b7let", "eu\u00b7ren", "Bis\u00b7sen", "Brot", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df hier und dort ein Arm, der bebet,", "tokens": ["Da\u00df", "hier", "und", "dort", "ein", "Arm", ",", "der", "be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erstarkt zum Kampfe neu sich hebet.", "tokens": ["Er\u00b7starkt", "zum", "Kamp\u00b7fe", "neu", "sich", "he\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Zeit blickt uns mit Hoffnungsaugen", "tokens": ["Die", "Zeit", "blickt", "uns", "mit", "Hoff\u00b7nungs\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tiefsinnig funkelnd, fragend, an;", "tokens": ["Tief\u00b7sin\u00b7nig", "fun\u00b7kelnd", ",", "fra\u00b7gend", ",", "an", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVPP", "$,", "PTKVZ", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt will sie Herzen, welche taugen,", "tokens": ["Jetzt", "will", "sie", "Her\u00b7zen", ",", "wel\u00b7che", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt r\u00fcst'ge Wandler ihrer Bahn.", "tokens": ["Jetzt", "r\u00fcst'\u00b7ge", "Wand\u00b7ler", "ih\u00b7rer", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum nicht mehr lau, nicht mehr verzaget;", "token