{"textgrid.poem.65301": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Amor, ein Bettler", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verbannet aus dem Himmel", "tokens": ["Ver\u00b7ban\u00b7net", "aus", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um seine losen Streiche,", "tokens": ["Um", "sei\u00b7ne", "lo\u00b7sen", "Strei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df Amor hier auf Erden", "tokens": ["Mu\u00df", "A\u00b7mor", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NE", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verstohlen betteln gehen.", "tokens": ["Ver\u00b7stoh\u00b7len", "bet\u00b7teln", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er klopft an alle Herzen,", "tokens": ["Er", "klopft", "an", "al\u00b7le", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und bettelt um ein St\u00fcbchen,", "tokens": ["Und", "bet\u00b7telt", "um", "ein", "St\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er schaut in jedes Auge,", "tokens": ["Er", "schaut", "in", "je\u00b7des", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und bettelt um ein Fl\u00e4mmchen,", "tokens": ["Und", "bet\u00b7telt", "um", "ein", "Fl\u00e4mm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Er geht an alle Lippen,", "tokens": ["Er", "geht", "an", "al\u00b7le", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und bettelt um ein K\u00fc\u00dfchen.", "tokens": ["Und", "bet\u00b7telt", "um", "ein", "K\u00fc\u00df\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ach, wenn von allen M\u00e4dchen", "tokens": ["Ach", ",", "wenn", "von", "al\u00b7len", "M\u00e4d\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ihm Eine, die ich meine,", "tokens": ["Ihm", "Ei\u00b7ne", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die milden Gaben g\u00e4be,", "tokens": ["Die", "mil\u00b7den", "Ga\u00b7ben", "g\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "So w\u00fcrd' er seinen Himmel", "tokens": ["So", "w\u00fcrd'", "er", "sei\u00b7nen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Auf Erden wiederfinden.", "tokens": ["Auf", "Er\u00b7den", "wie\u00b7der\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}