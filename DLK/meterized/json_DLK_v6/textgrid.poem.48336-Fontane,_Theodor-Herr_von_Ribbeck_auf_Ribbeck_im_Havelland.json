{"textgrid.poem.48336": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Herr von Ribbeck auf Ribbeck im Havelland", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr von Ribbeck auf Ribbeck im Havelland,", "tokens": ["Herr", "von", "Rib\u00b7beck", "auf", "Rib\u00b7beck", "im", "Ha\u00b7vel\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "APPR", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein Birnbaum in seinem Garten stand,", "tokens": ["Ein", "Birn\u00b7baum", "in", "sei\u00b7nem", "Gar\u00b7ten", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und kam die goldene Herbsteszeit", "tokens": ["Und", "kam", "die", "gol\u00b7de\u00b7ne", "Herbs\u00b7tes\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die Birnen leuchteten weit und breit,", "tokens": ["Und", "die", "Bir\u00b7nen", "leuch\u00b7te\u00b7ten", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da stopfte, wenn's Mittag vom Turme scholl,", "tokens": ["Da", "stopf\u00b7te", ",", "wenn's", "Mit\u00b7tag", "vom", "Tur\u00b7me", "scholl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der von Ribbeck sich beide Taschen voll,", "tokens": ["Der", "von", "Rib\u00b7beck", "sich", "bei\u00b7de", "Ta\u00b7schen", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "PRF", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und kam in Pantinen ein Junge daher,", "tokens": ["Und", "kam", "in", "Pan\u00b7ti\u00b7nen", "ein", "Jun\u00b7ge", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "PAV", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "So rief er: \u00bbJunge, wiste 'ne Beer?\u00ab", "tokens": ["So", "rief", "er", ":", "\u00bb", "Jun\u00b7ge", ",", "wis\u00b7te", "'ne", "Beer", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Und kam ein M\u00e4del, so rief er: \u00bbL\u00fctt Dirn,", "tokens": ["Und", "kam", "ein", "M\u00e4\u00b7del", ",", "so", "rief", "er", ":", "\u00bb", "L\u00fctt", "Dirn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$.", "$(", "NE", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Kumm man r\u00f6wer, ick hebb 'ne Birn.\u00ab", "tokens": ["Kumm", "man", "r\u00f6\u00b7wer", ",", "ick", "hebb", "'ne", "Birn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PIS", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "So ging es viel Jahre, bis lobesam", "tokens": ["So", "ging", "es", "viel", "Jah\u00b7re", ",", "bis", "lo\u00b7be\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "APPR", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der von Ribbeck auf Ribbeck zu sterben kam.", "tokens": ["Der", "von", "Rib\u00b7beck", "auf", "Rib\u00b7beck", "zu", "ster\u00b7ben", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "APPR", "NE", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Er f\u00fchlte sein Ende. 's war Herbsteszeit,", "tokens": ["Er", "f\u00fchl\u00b7te", "sein", "En\u00b7de", ".", "'s", "war", "Herbs\u00b7tes\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wieder lachten die Birnen weit und breit,", "tokens": ["Wie\u00b7der", "lach\u00b7ten", "die", "Bir\u00b7nen", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Da sagte von Ribbeck: \u00bbIch scheide nun ab.", "tokens": ["Da", "sag\u00b7te", "von", "Rib\u00b7beck", ":", "\u00bb", "Ich", "schei\u00b7de", "nun", "ab", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "$.", "$(", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Legt mir eine Birne mit ins Grab.\u00ab", "tokens": ["Legt", "mir", "ei\u00b7ne", "Bir\u00b7ne", "mit", "ins", "Grab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und drei Tage drauf, aus dem Doppeldachhaus,", "tokens": ["Und", "drei", "Ta\u00b7ge", "drauf", ",", "aus", "dem", "Dop\u00b7pel\u00b7dach\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "PTKVZ", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Trugen von Ribbeck sie hinaus,", "tokens": ["Tru\u00b7gen", "von", "Rib\u00b7beck", "sie", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Alle Bauern und B\u00fcdner mit Feiergesicht", "tokens": ["Al\u00b7le", "Bau\u00b7ern", "und", "B\u00fcd\u00b7ner", "mit", "Fei\u00b7er\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "APPR", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Sangen \u00bbJesus meine Zuversicht\u00ab,", "tokens": ["San\u00b7gen", "\u00bb", "Je\u00b7sus", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "\u00ab", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$(", "NE", "PPOSAT", "NN", "$(", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Und die Kinder klagten, das Herze schwer:", "tokens": ["Und", "die", "Kin\u00b7der", "klag\u00b7ten", ",", "das", "Her\u00b7ze", "schwer", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "ADJD", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "\u00bbhe is dod nu. Wer giwt uns nu 'ne Beer?\u00ab", "tokens": ["\u00bb", "he", "is", "dod", "nu", ".", "Wer", "giwt", "uns", "nu", "'ne", "Beer", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "FM", "FM", "ADV", "$.", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So klagten die Kinder. Das war nicht recht,", "tokens": ["So", "klag\u00b7ten", "die", "Kin\u00b7der", ".", "Das", "war", "nicht", "recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PDS", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ach, sie kannten den alten Ribbeck schlecht,", "tokens": ["Ach", ",", "sie", "kann\u00b7ten", "den", "al\u00b7ten", "Rib\u00b7beck", "schlecht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "H\u00e4lt Park und Birnbaum strenge verwahrt.", "tokens": ["H\u00e4lt", "Park", "und", "Birn\u00b7baum", "stren\u00b7ge", "ver\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Aber der ", "tokens": ["A\u00b7ber", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Und voll Mi\u00dftrauen gegen den eigenen Sohn,", "tokens": ["Und", "voll", "Mi\u00df\u00b7trau\u00b7en", "ge\u00b7gen", "den", "ei\u00b7ge\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der wu\u00dfte genau, was damals er tat,", "tokens": ["Der", "wu\u00df\u00b7te", "ge\u00b7nau", ",", "was", "da\u00b7mals", "er", "tat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als um eine Birn' ins Grab er bat,", "tokens": ["Als", "um", "ei\u00b7ne", "Birn'", "ins", "Grab", "er", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Und im dritten Jahr, aus dem stillen Haus", "tokens": ["Und", "im", "drit\u00b7ten", "Jahr", ",", "aus", "dem", "stil\u00b7len", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ein Birnbaumspr\u00f6\u00dfling spro\u00dft heraus.", "tokens": ["Ein", "Birn\u00b7baum\u00b7spr\u00f6\u00df\u00b7ling", "spro\u00dft", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Jahre gehen wohl auf und ab,", "tokens": ["Und", "die", "Jah\u00b7re", "ge\u00b7hen", "wohl", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "L\u00e4ngst w\u00f6lbt sich ein Birnbaum \u00fcber dem Grab,", "tokens": ["L\u00e4ngst", "w\u00f6lbt", "sich", "ein", "Birn\u00b7baum", "\u00fc\u00b7ber", "dem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und in der goldenen Herbsteszeit", "tokens": ["Und", "in", "der", "gol\u00b7de\u00b7nen", "Herbs\u00b7tes\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Leuchtet's wieder weit und breit.", "tokens": ["Leuch\u00b7tet's", "wie\u00b7der", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und kommt ein Jung' \u00fcbern Kirchhof her,", "tokens": ["Und", "kommt", "ein", "Jung'", "\u00fc\u00b7bern", "Kirch\u00b7hof", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So fl\u00fcstert's im Baume: \u00bbWist 'ne Beer?\u00ab", "tokens": ["So", "fl\u00fcs\u00b7tert's", "im", "Bau\u00b7me", ":", "\u00bb", "Wist", "'ne", "Beer", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$.", "$(", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und kommt ein M\u00e4del, so fl\u00fcstert's: \u00bbL\u00fctt Dirn,", "tokens": ["Und", "kommt", "ein", "M\u00e4\u00b7del", ",", "so", "fl\u00fcs\u00b7tert's", ":", "\u00bb", "L\u00fctt", "Dirn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "$.", "$(", "NE", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Kumm man r\u00f6wer, ich gew di 'ne Birn. \u00ab", "tokens": ["Kumm", "man", "r\u00f6\u00b7wer", ",", "ich", "gew", "di", "'ne", "Birn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PIS", "ADJD", "$,", "PPER", "ADJD", "NE", "ART", "NN", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "So spendet Segen noch immer die Hand", "tokens": ["So", "spen\u00b7det", "Se\u00b7gen", "noch", "im\u00b7mer", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des von Ribbeck auf Ribbeck im Havelland.", "tokens": ["Des", "von", "Rib\u00b7beck", "auf", "Rib\u00b7beck", "im", "Ha\u00b7vel\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "APPR", "NE", "APPRART", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}}}}