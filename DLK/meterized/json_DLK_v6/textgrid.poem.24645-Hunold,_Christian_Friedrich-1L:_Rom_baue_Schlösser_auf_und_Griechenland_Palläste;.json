{"textgrid.poem.24645": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rom baue Schl\u00f6sser auf/ und Griechenland Pall\u00e4ste;", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rom baue Schl\u00f6sser auf/ und Griechenland Pall\u00e4ste;", "tokens": ["Rom", "bau\u00b7e", "Schl\u00f6s\u00b7ser", "auf", "/", "und", "Grie\u00b7chen\u00b7land", "Pal\u00b7l\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "APPR", "$(", "KON", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da meinen immer hin der Erden hohe G\u00e4ste", "tokens": ["Da", "mei\u00b7nen", "im\u00b7mer", "hin", "der", "Er\u00b7den", "ho\u00b7he", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch des ", "tokens": ["Durch", "des"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Vor allen diesen Prei\u00dft die Tugend Wanders leben/", "tokens": ["Vor", "al\u00b7len", "die\u00b7sen", "Prei\u00dft", "die", "Tu\u00b7gend", "Wan\u00b7ders", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PDAT", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und rufft: hier siehet man die Wohnung dieser Welt.", "tokens": ["Und", "rufft", ":", "hier", "sie\u00b7het", "man", "die", "Woh\u00b7nung", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Hier kehren F\u00fcrsten ein/ und m\u00fcssen Abschied geben/", "tokens": ["Hier", "keh\u00b7ren", "F\u00fcrs\u00b7ten", "ein", "/", "und", "m\u00fcs\u00b7sen", "Ab\u00b7schied", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "$(", "KON", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hier baut sich Troja auf/ und wird auch hingef\u00e4llt.", "tokens": ["Hier", "baut", "sich", "Tro\u00b7ja", "auf", "/", "und", "wird", "auch", "hin\u00b7ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NE", "APPR", "$(", "KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du Wanders leben bleibst ein Bild der St\u00e4dt' und Leute:", "tokens": ["Du", "Wan\u00b7ders", "le\u00b7ben", "bleibst", "ein", "Bild", "der", "St\u00e4dt'", "und", "Leu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Di\u00df wandert morgen ab/ und jenes reis't noch heute.", "tokens": ["Di\u00df", "wan\u00b7dert", "mor\u00b7gen", "ab", "/", "und", "je\u00b7nes", "reis't", "noch", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "$(", "KON", "PDS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Schrieb jene kluge Faust zu Mosen in der W\u00fcsten:", "tokens": ["Schrieb", "je\u00b7ne", "klu\u00b7ge", "Faust", "zu", "Mo\u00b7sen", "in", "der", "W\u00fcs\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wandern/ aber doch unsicher allzumahl:", "tokens": ["Sie", "wan\u00b7dern", "/", "a\u00b7ber", "doch", "un\u00b7si\u00b7cher", "all\u00b7zu\u00b7mahl", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So schreiben wirs zur Welt/ zum Labyrinth in L\u00fcsten/", "tokens": ["So", "schrei\u00b7ben", "wirs", "zur", "Welt", "/", "zum", "La\u00b7by\u00b7rinth", "in", "L\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "$(", "APPRART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die w\u00fcst' an Gl\u00fcck und Wohl/ und reich an Angst und Qvaal.", "tokens": ["Die", "w\u00fcst'", "an", "Gl\u00fcck", "und", "Wohl", "/", "und", "reich", "an", "Angst", "und", "Qvaal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "ADV", "$(", "KON", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die h\u00f6chste Tugend ist ein Pilgerim auf Erden/", "tokens": ["Die", "h\u00f6chs\u00b7te", "Tu\u00b7gend", "ist", "ein", "Pil\u00b7ge\u00b7rim", "auf", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie findet hier nicht statt/ und wandert wieder fort/", "tokens": ["Sie", "fin\u00b7det", "hier", "nicht", "statt", "/", "und", "wan\u00b7dert", "wie\u00b7der", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$(", "KON", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn ihr bewehrter Sitz mu\u00df nur im Himmel werden.", "tokens": ["Denn", "ihr", "be\u00b7wehr\u00b7ter", "Sitz", "mu\u00df", "nur", "im", "Him\u00b7mel", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VMFIN", "ADV", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drum ungl\u00fcckseeliges/ doch allzu wahres Wort:", "tokens": ["Drum", "un\u00b7gl\u00fcck\u00b7see\u00b7li\u00b7ges", "/", "doch", "all\u00b7zu", "wah\u00b7res", "Wort", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "$(", "ADV", "PTKA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein Graf der Lebens wehrt und r\u00fchmlich war vor andern/", "tokens": ["Ein", "Graf", "der", "Le\u00b7bens", "wehrt", "und", "r\u00fchm\u00b7lich", "war", "vor", "an\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KON", "ADJD", "VAFIN", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mu\u00df aus der Herberge der Welt auch wieder wandern!", "tokens": ["Mu\u00df", "aus", "der", "Her\u00b7ber\u00b7ge", "der", "Welt", "auch", "wie\u00b7der", "wan\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie auf den Erden Ball die Sonnen Strahlen kommen/", "tokens": ["Wie", "auf", "den", "Er\u00b7den", "Ball", "die", "Son\u00b7nen", "Strah\u00b7len", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kamst du theurer Graf/ zum Heil auf diese Welt/", "tokens": ["So", "kamst", "du", "theu\u00b7rer", "Graf", "/", "zum", "Heil", "auf", "die\u00b7se", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NE", "$(", "APPRART", "NN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von herrlicher Geburt und Tugend hoch entglommen/", "tokens": ["Von", "herr\u00b7li\u00b7cher", "Ge\u00b7burt", "und", "Tu\u00b7gend", "hoch", "ent\u00b7glom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum ", "tokens": ["Zum"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Dein Huld- und Wei\u00dfheits-Strahl f\u00fchrt alle die zum Hafen/", "tokens": ["Dein", "Huld", "und", "Wei\u00df\u00b7heits\u00b7Strahl", "f\u00fchrt", "al\u00b7le", "die", "zum", "Ha\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "NN", "VVFIN", "PIS", "ART", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Ehr-Furcht/ Lieb und Treu dir unterthan gemacht.", "tokens": ["Die", "Ehr\u00b7Furcht", "/", "Lieb", "und", "Treu", "dir", "un\u00b7ter\u00b7than", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "KON", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn hier im Wander-Thal uns Sturm und Ungl\u00fcck trafen/", "tokens": ["Wenn", "hier", "im", "Wan\u00b7der\u00b7Thal", "uns", "Sturm", "und", "Un\u00b7gl\u00fcck", "tra\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So hast du uns gar offt in Ruh und Schutz gebracht.", "tokens": ["So", "hast", "du", "uns", "gar", "offt", "in", "Ruh", "und", "Schutz", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bey allen Wegen hat uns so ein Herr vertreten/", "tokens": ["Bey", "al\u00b7len", "We\u00b7gen", "hat", "uns", "so", "ein", "Herr", "ver\u00b7tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In dem ein Hertz/ worum Gott Salomon gebeten.", "tokens": ["In", "dem", "ein", "Hertz", "/", "wo\u00b7rum", "Gott", "Sa\u00b7lo\u00b7mon", "ge\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "$(", "PWAV", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie Bienen trugest du aus Blumen fremder Erden", "tokens": ["Wie", "Bie\u00b7nen", "tru\u00b7gest", "du", "aus", "Blu\u00b7men", "frem\u00b7der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch deine Reisen dir der Klugheit Honig ein.", "tokens": ["Durch", "dei\u00b7ne", "Rei\u00b7sen", "dir", "der", "Klug\u00b7heit", "Ho\u00b7nig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Durch h\u00f6ren/ thun und sehn/ wie andre Herren seyn.", "tokens": ["Durch", "h\u00f6\u00b7ren", "/", "thun", "und", "sehn", "/", "wie", "and\u00b7re", "Her\u00b7ren", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$(", "VVINF", "KON", "VVINF", "$(", "KOKOM", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du kamst vortrefflicher/ so wie des Mondes-Strahlen", "tokens": ["Du", "kamst", "vor\u00b7treff\u00b7li\u00b7cher", "/", "so", "wie", "des", "Mon\u00b7des\u00b7Strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im vollem Glantz zur\u00fcck. Dein Einflu\u00df hat erqvickt.", "tokens": ["Im", "vol\u00b7lem", "Glantz", "zu\u00b7r\u00fcck", ".", "Dein", "Ein\u00b7flu\u00df", "hat", "er\u00b7qvickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Licht wolt' aus der Fern das Land mit Huld bemahlen/", "tokens": ["Dein", "Licht", "wolt'", "aus", "der", "Fern", "das", "Land", "mit", "Huld", "be\u00b7mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein Schimmer hat dir Ruhm/ uns Leben zugeschickt.", "tokens": ["Dein", "Schim\u00b7mer", "hat", "dir", "Ruhm", "/", "uns", "Le\u00b7ben", "zu\u00b7ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN", "$(", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Tugend sahen wir in deiner Wei\u00dfheit prangen:", "tokens": ["Die", "Tu\u00b7gend", "sa\u00b7hen", "wir", "in", "dei\u00b7ner", "Wei\u00df\u00b7heit", "pran\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bi\u00df deiner Sonnen-Gold zum Westen ist gegangen.", "tokens": ["Bi\u00df", "dei\u00b7ner", "Son\u00b7nen\u00b7Gold", "zum", "Wes\u00b7ten", "ist", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr B\u00fcrger dieser Welt/ die hier im Ungl\u00fcck schweben/", "tokens": ["Ihr", "B\u00fcr\u00b7ger", "die\u00b7ser", "Welt", "/", "die", "hier", "im", "Un\u00b7gl\u00fcck", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$(", "ART", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedencket und beweint die allgemeine Pein:", "tokens": ["Be\u00b7den\u00b7cket", "und", "be\u00b7weint", "die", "all\u00b7ge\u00b7mei\u00b7ne", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Wanders-leben geht der Weg nach Grabes-leben/", "tokens": ["Von", "Wan\u00b7der\u00b7sle\u00b7ben", "geht", "der", "Weg", "nach", "Gra\u00b7bes\u00b7le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexa