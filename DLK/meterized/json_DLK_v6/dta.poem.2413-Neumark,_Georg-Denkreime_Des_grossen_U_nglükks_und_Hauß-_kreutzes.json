{"dta.poem.2413": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Denkreime  \n  Des grossen  U ngl\u00fckks und Hau\u00df-  \n kreutzes", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wil dirs gern gestehn/ Amfion/ da\u00df die Schmertzen", "tokens": ["Ich", "wil", "dirs", "gern", "ge\u00b7stehn", "/", "Am\u00b7fi\u00b7on", "/", "da\u00df", "die", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "VVPP", "$(", "NE", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du mit grossem Leid\u2019 in deinem Vaterhertzen", "tokens": ["Die", "du", "mit", "gros\u00b7sem", "Leid'", "in", "dei\u00b7nem", "Va\u00b7ter\u00b7hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als tausend Wunden f\u00fchlst/ fast unertr\u00e4glich sind:", "tokens": ["Als", "tau\u00b7send", "Wun\u00b7den", "f\u00fchlst", "/", "fast", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "sind", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "VVFIN", "$(", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich gl\u00e4ub es Hekuba/ da\u00df dich dein liebes Kind", "tokens": ["Ich", "gl\u00e4ub", "es", "He\u00b7ku\u00b7ba", "/", "da\u00df", "dich", "dein", "lie\u00b7bes", "Kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NE", "$(", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Polidorus schmertzt; auch kan ich leicht gedenken", "tokens": ["Dein", "Po\u00b7li\u00b7do\u00b7rus", "schmertzt", ";", "auch", "kan", "ich", "leicht", "ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df es Antikle dich nicht wenig nur mu\u00df kr\u00e4nken/", "tokens": ["Da\u00df", "es", "An\u00b7tik\u00b7le", "dich", "nicht", "we\u00b7nig", "nur", "mu\u00df", "kr\u00e4n\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn dir zu Ohren k\u00f6mmt/ zwar durch ein falsch Ge-", "tokens": ["Wenn", "dir", "zu", "Oh\u00b7ren", "k\u00f6mmt", "/", "zwar", "durch", "ein", "falsch", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$(", "ADV", "APPR", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "schrey/", "tokens": ["schrey", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Als wenn vor Troja dort dein Sohn geblieben sey", "tokens": ["Als", "wenn", "vor", "Tro\u00b7ja", "dort", "dein", "Sohn", "ge\u00b7blie\u00b7ben", "sey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "NE", "ADV", "PPOSAT", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein liebster Sohn Uly\u00df. Es mag sich so verhalten/", "tokens": ["Dein", "liebs\u00b7ter", "Sohn", "U\u00b7ly\u00df", ".", "Es", "mag", "sich", "so", "ver\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$.", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Ich geb\u2019 es gerne nach da\u00df euch die Hertzen spalten", "tokens": ["Ich", "geb'", "es", "ger\u00b7ne", "nach", "da\u00df", "euch", "die", "Hert\u00b7zen", "spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vor grossem ach und weh. Doch schweigt anitzo still\u2019", "tokens": ["Vor", "gros\u00b7sem", "ach", "und", "weh", ".", "Doch", "schweigt", "a\u00b7nit\u00b7zo", "still'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "XY", "KON", "PTKVZ", "$.", "KON", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und h\u00f6rt ein wenig zu was ich erzehlen wil.", "tokens": ["Und", "h\u00f6rt", "ein", "we\u00b7nig", "zu", "was", "ich", "er\u00b7zeh\u00b7len", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIS", "APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dann sprecht das Urtheil selbst ob ihr nicht m\u00fcsset weichen", "tokens": ["Dann", "sprecht", "das", "Ur\u00b7theil", "selbst", "ob", "ihr", "nicht", "m\u00fcs\u00b7set", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was Wehmuhtanbelangt? ob euch nicht sey zugleichen", "tokens": ["Was", "Weh\u00b7muh\u00b7tan\u00b7be\u00b7langt", "?", "ob", "euch", "nicht", "sey", "zu\u00b7glei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Frau Me", "tokens": ["Frau", "Me"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "So ihm den kalten Tod bey nahe zugetreut:", "tokens": ["So", "ihm", "den", "kal\u00b7ten", "Tod", "bey", "na\u00b7he", "zu\u00b7ge\u00b7treut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der beyden einger Sohn/ der wie die Rose pranget/", "tokens": ["Der", "bey\u00b7den", "ein\u00b7ger", "Sohn", "/", "der", "wie", "die", "Ro\u00b7se", "pran\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$(", "ART", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und der die helffte kaum vom elfften Jahr\u2019 erlanget/", "tokens": ["Und", "der", "die", "helff\u00b7te", "kaum", "vom", "elff\u00b7ten", "Jahr'", "er\u00b7lan\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Geht ungefehr allein au\u00df antrieb seiner Lust", "tokens": ["Geht", "un\u00b7ge\u00b7fehr", "al\u00b7lein", "au\u00df", "an\u00b7trieb", "sei\u00b7ner", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ins Oberhau\u00df hinauf des Ungl\u00fcks unbewust", "tokens": ["Ins", "O\u00b7berh\u00b7au\u00df", "hin\u00b7auf", "des", "Un\u00b7gl\u00fcks", "un\u00b7be\u00b7wust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "(denn was der Himmel wil das sol und mu\u00df geschehen", "tokens": ["(", "denn", "was", "der", "Him\u00b7mel", "wil", "das", "sol", "und", "mu\u00df", "ge\u00b7sche\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PWS", "ART", "NN", "VMFIN", "PDS", "VMFIN", "KON", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Kein Mensch wie klug er auch kan diesem widerstehen.)", "tokens": ["Kein", "Mensch", "wie", "klug", "er", "auch", "kan", "die\u00b7sem", "wi\u00b7der\u00b7ste\u00b7hen", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "KOKOM", "ADJD", "PPER", "ADV", "VMFIN", "PDS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Et spielet mit sich sebst/ geht allzuweit herf\u00fcr", "tokens": ["Et", "spie\u00b7let", "mit", "sich", "sebst", "/", "geht", "all\u00b7zu\u00b7weit", "her\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PRF", "ADV", "$(", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zum Ladenfenster zu (O weh es grauet mir", "tokens": ["Zum", "La\u00b7den\u00b7fens\u00b7ter", "zu", "(", "O", "weh", "es", "grau\u00b7et", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKZU", "$(", "NE", "VVFIN", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Vor solchem Ungel\u00fckk\u2019! es stokken meine Sinnen/", "tokens": ["Vor", "sol\u00b7chem", "Un\u00b7ge\u00b7l\u00fc\u00b7kk'", "!", "es", "stok\u00b7ken", "mei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Die Feder wil nicht fort es wollen die Verse nicht rinnen/", "tokens": ["Die", "Fe\u00b7der", "wil", "nicht", "fort", "es", "wol\u00b7len", "die", "Ver\u00b7se", "nicht", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "PTKVZ", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.28": {"text": "Wie sie pflegen und wie sie sollen.) ", "tokens": ["Wie", "sie", "pfle\u00b7gen", "und", "wie", "sie", "sol\u00b7len", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VVINF", "KON", "PWAV", "PPER", "VMFIN", "$.", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}