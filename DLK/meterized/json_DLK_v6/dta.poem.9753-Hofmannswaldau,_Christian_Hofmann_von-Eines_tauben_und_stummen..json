{"dta.poem.9753": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines tauben und stummen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob ich gleich taub zur welt gebracht war und gebohren/", "tokens": ["Ob", "ich", "gleich", "taub", "zur", "welt", "ge\u00b7bracht", "war", "und", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVPP", "VAFIN", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ich schon nie kein wort aus meinem mund verlohren/", "tokens": ["Ob", "ich", "schon", "nie", "kein", "wort", "aus", "mei\u00b7nem", "mund", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nicht was s\u00fcnde heist kont h\u00f6ren oder sagen/", "tokens": ["Und", "nicht", "was", "s\u00fcn\u00b7de", "heist", "kont", "h\u00f6\u00b7ren", "o\u00b7der", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PWS", "ADJA", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So must ich doch den tod/ der s\u00fcnden straffe/ tragen.", "tokens": ["So", "must", "ich", "doch", "den", "tod", "/", "der", "s\u00fcn\u00b7den", "straf\u00b7fe", "/", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "$(", "ART", "ADJA", "VVFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}