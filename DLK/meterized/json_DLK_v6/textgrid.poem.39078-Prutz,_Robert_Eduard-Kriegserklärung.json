{"textgrid.poem.39078": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Kriegserkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, sie sollen wiederklingen,", "tokens": ["Nein", ",", "sie", "sol\u00b7len", "wie\u00b7der\u00b7klin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie sie ehemals erklungen,", "tokens": ["wie", "sie", "e\u00b7he\u00b7mals", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "nein, wir wollen wieder singen,", "tokens": ["nein", ",", "wir", "wol\u00b7len", "wie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die wir ehemals gesungen:", "tokens": ["die", "wir", "e\u00b7he\u00b7mals", "ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Berghinauf und talhernieder,", "tokens": ["Berg\u00b7hin\u00b7auf", "und", "tal\u00b7her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "unbek\u00fcmmert, unerschrocken,", "tokens": ["un\u00b7be\u00b7k\u00fcm\u00b7mert", ",", "un\u00b7er\u00b7schro\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "unsrer Zukunft Osterglocken,", "tokens": ["uns\u00b7rer", "Zu\u00b7kunft", "Os\u00b7ter\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "unsrer Freiheit Morgenlieder!", "tokens": ["uns\u00b7rer", "Frei\u00b7heit", "Mor\u00b7gen\u00b7lie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was wir einsam, gramumd\u00fcstert,", "tokens": ["Was", "wir", "ein\u00b7sam", ",", "gra\u00b7mum\u00b7d\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nur gedacht mit Herzenspochen,", "tokens": ["nur", "ge\u00b7dacht", "mit", "Her\u00b7zen\u00b7spo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "was wir heimlich nur gefl\u00fcstert", "tokens": ["was", "wir", "heim\u00b7lich", "nur", "ge\u00b7fl\u00fcs\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und vor Gott nur ausgesprochen:", "tokens": ["und", "vor", "Gott", "nur", "aus\u00b7ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft es nun die Welt erfahren!", "tokens": ["La\u00dft", "es", "nun", "die", "Welt", "er\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00dft es nun aus jedem Munde,", "tokens": ["La\u00dft", "es", "nun", "aus", "je\u00b7dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "la\u00dft es nun aus Herzens Grunde", "tokens": ["la\u00dft", "es", "nun", "aus", "Her\u00b7zens", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "unverh\u00fcllt sich offenbaren!", "tokens": ["un\u00b7ver\u00b7h\u00fcllt", "sich", "of\u00b7fen\u00b7ba\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun herbei, die Glocken l\u00e4uten,", "tokens": ["Nun", "her\u00b7bei", ",", "die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und die Fahnen seh' ich wehen:", "tokens": ["und", "die", "Fah\u00b7nen", "seh'", "ich", "we\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun herbei, ihr Weitzerstreuten,", "tokens": ["Nun", "her\u00b7bei", ",", "ihr", "Weit\u00b7zer\u00b7streu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "nun zum Banner la\u00dft uns stehen!", "tokens": ["nun", "zum", "Ban\u00b7ner", "la\u00dft", "uns", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werdet M\u00e4nner nun aus Knechten,", "tokens": ["Wer\u00b7det", "M\u00e4n\u00b7ner", "nun", "aus", "Knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "werdet Krieger nun aus Hirten:", "tokens": ["wer\u00b7det", "Krie\u00b7ger", "nun", "aus", "Hir\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das wir trugen unter Myrten,", "tokens": ["Das", "wir", "tru\u00b7gen", "un\u00b7ter", "Myr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "mit dem Schwert nun la\u00dft uns fechten!", "tokens": ["mit", "dem", "Schwert", "nun", "la\u00dft", "uns", "fech\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun herbei auch ihr zur Stunde,", "tokens": ["Nun", "her\u00b7bei", "auch", "ihr", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die ihr Frieden uns geheuchelt", "tokens": ["die", "ihr", "Frie\u00b7den", "uns", "ge\u00b7heu\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und mit L\u00e4cheln auf dem Munde", "tokens": ["und", "mit", "L\u00e4\u00b7cheln", "auf", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mittern\u00e4chtlich uns gemeuchelt:", "tokens": ["mit\u00b7ter\u00b7n\u00e4cht\u00b7lich", "uns", "ge\u00b7meu\u00b7chelt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun beiseite eure Schlingen,", "tokens": ["Nun", "bei\u00b7sei\u00b7te", "eu\u00b7re", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "eure Netze, eure Fallen!", "tokens": ["eu\u00b7re", "Net\u00b7ze", ",", "eu\u00b7re", "Fal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn nur ein Recht gilt uns allen:", "tokens": ["Denn", "nur", "ein", "Recht", "gilt", "uns", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "und das ist das Recht der Klingen.", "tokens": ["und", "das", "ist", "das", "Recht", "der", "Klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schweigend, in den L\u00fcften droben,", "tokens": ["Schwei\u00b7gend", ",", "in", "den", "L\u00fcf\u00b7ten", "dro\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ernste Geister seh' ich sitzen,", "tokens": ["erns\u00b7te", "Geis\u00b7ter", "seh'", "ich", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "eine Hand seh' ich erhoben,", "tokens": ["ei\u00b7ne", "Hand", "seh'", "ich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und ein Richtbeil seh' ich blitzen.", "tokens": ["und", "ein", "Richt\u00b7beil", "seh'", "ich", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese Stunde ruft uns beide!", "tokens": ["Die\u00b7se", "Stun\u00b7de", "ruft", "uns", "bei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wagt es nun, die Glut zu d\u00e4mpfen,", "tokens": ["Wagt", "es", "nun", ",", "die", "Glut", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "wagt es nun, mit uns zu k\u00e4mpfen \u2013", "tokens": ["wagt", "es", "nun", ",", "mit", "uns", "zu", "k\u00e4mp\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "und der ew'ge Gott entscheide!", "tokens": ["und", "der", "ew'\u00b7ge", "Gott", "ent\u00b7schei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}