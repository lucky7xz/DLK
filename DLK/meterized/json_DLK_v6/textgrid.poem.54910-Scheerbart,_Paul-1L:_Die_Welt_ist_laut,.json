{"textgrid.poem.54910": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Welt ist laut,", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist laut,", "tokens": ["Die", "Welt", "ist", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und ich bin still!", "tokens": ["Und", "ich", "bin", "still", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Erloschen sind die Flammen.", "tokens": ["Er\u00b7lo\u00b7schen", "sind", "die", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kann nicht mehr,", "tokens": ["Ich", "kann", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So wie ich will!", "tokens": ["So", "wie", "ich", "will", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Rausch mu\u00df ich verdammen.", "tokens": ["Den", "Rausch", "mu\u00df", "ich", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Welt ist laut,", "tokens": ["Die", "Welt", "ist", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich m\u00f6cht so viel!", "tokens": ["Ich", "m\u00f6cht", "so", "viel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch bring ich's nicht zusammen.", "tokens": ["Doch", "bring", "ich's", "nicht", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}