{"textgrid.poem.56252": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtw\u00e4chterlied", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, ihr Herrn, und la\u00dft euch sagen,", "tokens": ["H\u00f6rt", ",", "ihr", "Herrn", ",", "und", "la\u00dft", "euch", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Glocke hat geschlagen:", "tokens": ["Was", "die", "Glo\u00b7cke", "hat", "ge\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht nach Haus und wahrt das Licht,", "tokens": ["Geht", "nach", "Haus", "und", "wahrt", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df dem Staat kein Schaden geschicht.", "tokens": ["Da\u00df", "dem", "Staat", "kein", "Scha\u00b7den", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Lobt die Jesuiten!", "tokens": ["Lobt", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt, ihr Herrn, wir brauchen heute", "tokens": ["H\u00f6rt", ",", "ihr", "Herrn", ",", "wir", "brau\u00b7chen", "heu\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gute, nicht gelehrte Leute,", "tokens": ["Gu\u00b7te", ",", "nicht", "ge\u00b7lehr\u00b7te", "Leu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seid ihr einmal doch gelehrt,", "tokens": ["Seid", "ihr", "ein\u00b7mal", "doch", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sorgt, da\u00df keiner es erf\u00e4hrt.", "tokens": ["Sorgt", ",", "da\u00df", "kei\u00b7ner", "es", "er\u00b7f\u00e4hrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobt die Jesuiten!", "tokens": ["Lobt", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt, ihr Herrn, so soll es werden:", "tokens": ["H\u00f6rt", ",", "ihr", "Herrn", ",", "so", "soll", "es", "wer\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott im Himmel, wir auf Erden,", "tokens": ["Gott", "im", "Him\u00b7mel", ",", "wir", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der K\u00f6nig absolut,", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "ab\u00b7so\u00b7lut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er unsern Willen tut.", "tokens": ["Wenn", "er", "un\u00b7sern", "Wil\u00b7len", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobt die Jesuiten!", "tokens": ["Lobt", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Seid, ihr Herrn, es wird euch frommen,", "tokens": ["Seid", ",", "ihr", "Herrn", ",", "es", "wird", "euch", "from\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den gutgesinnten Frommen;", "tokens": ["Von", "den", "gut\u00b7ge\u00b7sinn\u00b7ten", "From\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blase jeder, was er kann,", "tokens": ["Bla\u00b7se", "je\u00b7der", ",", "was", "er", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lichter aus, und Feuer an.", "tokens": ["Lich\u00b7ter", "aus", ",", "und", "Feu\u00b7er", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobt die Jesuiten!", "tokens": ["Lobt", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Feuer, ja, zu Gottes Ehren,", "tokens": ["Feu\u00b7er", ",", "ja", ",", "zu", "Got\u00b7tes", "Eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Ketzer zu bekehren,", "tokens": ["Um", "die", "Ket\u00b7zer", "zu", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Philosophen auch,", "tokens": ["Und", "die", "Phi\u00b7lo\u00b7so\u00b7phen", "auch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Nach dem alten, guten Brauch.", "tokens": ["Nach", "dem", "al\u00b7ten", ",", "gu\u00b7ten", "Brauch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobt die Jesuiten!", "tokens": ["Lobt", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "H\u00f6rt, ihr Herrn, ihr seid geborgen,", "tokens": ["H\u00f6rt", ",", "ihr", "Herrn", ",", "ihr", "seid", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht nach Haus, und ohne Sorgen", "tokens": ["Geht", "nach", "Haus", ",", "und", "oh\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlaft die lange, liebe Nacht,", "tokens": ["Schlaft", "die", "lan\u00b7ge", ",", "lie\u00b7be", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn wir halten gute Wacht.", "tokens": ["Denn", "wir", "hal\u00b7ten", "gu\u00b7te", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobt die Jesuiten!", "tokens": ["Lobt", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}