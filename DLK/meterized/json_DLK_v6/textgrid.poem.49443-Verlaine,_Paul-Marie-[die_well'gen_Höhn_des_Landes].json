{"textgrid.poem.49443": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[die well'gen H\u00f6hn des Landes]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die well'gen H\u00f6hn des Landes", "tokens": ["Die", "well'\u00b7gen", "H\u00f6hn", "des", "Lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gehn endlos bis zur Flut,", "tokens": ["Gehn", "end\u00b7los", "bis", "zur", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die klar-verschleiert ruht", "tokens": ["Die", "kla\u00b7rver\u00b7schlei\u00b7ert", "ruht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im jungen Duft des Strandes.", "tokens": ["Im", "jun\u00b7gen", "Duft", "des", "Stran\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf zartem Gr\u00fcn stehn leicht", "tokens": ["Auf", "zar\u00b7tem", "Gr\u00fcn", "stehn", "leicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die M\u00fchlen und die B\u00e4ume,", "tokens": ["Die", "M\u00fch\u00b7len", "und", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo flink die weiten R\u00e4ume", "tokens": ["Wo", "flink", "die", "wei\u00b7ten", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der F\u00fcllen Lauf durchstreicht.", "tokens": ["Der", "F\u00fcl\u00b7len", "Lauf", "durch\u00b7streicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der helle, ruhevolle", "tokens": ["Der", "hel\u00b7le", ",", "ru\u00b7he\u00b7vol\u00b7le"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sonntag erblickt im Spiel", "tokens": ["Sonn\u00b7tag", "er\u00b7blickt", "im", "Spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der weissen Schafe viel", "tokens": ["Der", "weis\u00b7sen", "Scha\u00b7fe", "viel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sanft in der lichten Wolle.", "tokens": ["Sanft", "in", "der", "lich\u00b7ten", "Wol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die See rollt weissbekr\u00e4nzt", "tokens": ["Die", "See", "rollt", "weiss\u00b7be\u00b7kr\u00e4nzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihr brandend Flutgewimmel", "tokens": ["Ihr", "bran\u00b7dend", "Flut\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Fl\u00f6tenklang zum Himmel,", "tokens": ["Mit", "Fl\u00f6\u00b7ten\u00b7klang", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der hell wie Milch ergl\u00e4nzt.", "tokens": ["Der", "hell", "wie", "Milch", "er\u00b7gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}