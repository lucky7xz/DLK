{"textgrid.poem.57023": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Palmstr\u00f6m ist nerv\u00f6s geworden;", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6m ist nerv\u00f6s geworden;", "tokens": ["Palm\u00b7str\u00f6m", "ist", "ner\u00b7v\u00f6s", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "darum schl\u00e4ft er jetzt nach Norden.", "tokens": ["da\u00b7rum", "schl\u00e4ft", "er", "jetzt", "nach", "Nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn nach Osten, Westen, S\u00fcden", "tokens": ["Denn", "nach", "Os\u00b7ten", ",", "Wes\u00b7ten", ",", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schlafen, hei\u00dft das Herz erm\u00fcden.", "tokens": ["schla\u00b7fen", ",", "hei\u00dft", "das", "Herz", "er\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "(wenn man n\u00e4mlich in Europen", "tokens": ["(", "wenn", "man", "n\u00e4m\u00b7lich", "in", "Eu\u00b7ro\u00b7pen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PIS", "ADV", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "lebt, nicht s\u00fcdlich in den Tropen.)", "tokens": ["lebt", ",", "nicht", "s\u00fcd\u00b7lich", "in", "den", "Tro\u00b7pen", ".", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Solches steht bei zwei Gelehrten,", "tokens": ["Sol\u00b7ches", "steht", "bei", "zwei", "Ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die auch Dickens schon bekehrten \u2013", "tokens": ["die", "auch", "Di\u00b7ckens", "schon", "be\u00b7kehr\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "und erkl\u00e4rt sich aus dem steten", "tokens": ["und", "er\u00b7kl\u00e4rt", "sich", "aus", "dem", "ste\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Magnetismus des Planeten.", "tokens": ["Mag\u00b7ne\u00b7tis\u00b7mus", "des", "Pla\u00b7ne\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Palmstr\u00f6m also heilt sich \u00f6rtlich,", "tokens": ["Palm\u00b7str\u00f6m", "al\u00b7so", "heilt", "sich", "\u00f6rt\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nimmt sein Bett und stellt es n\u00f6rdlich.", "tokens": ["nimmt", "sein", "Bett", "und", "stellt", "es", "n\u00f6rd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und im Traum, in einigen F\u00e4llen,", "tokens": ["Und", "im", "Traum", ",", "in", "ei\u00b7ni\u00b7gen", "F\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "h\u00f6rt er den Polarfuchs bellen.", "tokens": ["h\u00f6rt", "er", "den", "Po\u00b7lar\u00b7fuchs", "bel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}