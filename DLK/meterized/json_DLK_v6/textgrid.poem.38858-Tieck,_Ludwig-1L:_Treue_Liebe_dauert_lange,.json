{"textgrid.poem.38858": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Treue Liebe dauert lange,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Treue Liebe dauert lange,", "tokens": ["Treu\u00b7e", "Lie\u00b7be", "dau\u00b7ert", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueberlebet manche Stund,", "tokens": ["Ue\u00b7ber\u00b7le\u00b7bet", "man\u00b7che", "Stund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein Zweifel macht sie bange,", "tokens": ["Und", "kein", "Zwei\u00b7fel", "macht", "sie", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer bleibt ihr Muth gesund.", "tokens": ["Im\u00b7mer", "bleibt", "ihr", "Muth", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dr\u00e4uen gleich in dichten Schaaren,", "tokens": ["Dr\u00e4u\u00b7en", "gleich", "in", "dich\u00b7ten", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fordern gleich zum Wankelmuth", "tokens": ["For\u00b7dern", "gleich", "zum", "Wan\u00b7kel\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Strum und Tod, setzt den Gefahren", "tokens": ["Strum", "und", "Tod", ",", "setzt", "den", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Lieb' entgegen treues Blut.", "tokens": ["Lieb'", "ent\u00b7ge\u00b7gen", "treu\u00b7es", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie Nebel st\u00fcrzt zur\u00fccke", "tokens": ["Und", "wie", "Ne\u00b7bel", "st\u00fcrzt", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was den Sinn gefangen h\u00e4lt,", "tokens": ["Was", "den", "Sinn", "ge\u00b7fan\u00b7gen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem heitern Fr\u00fchlingsblicke", "tokens": ["Und", "dem", "hei\u00b7tern", "Fr\u00fch\u00b7lings\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oeffnet sich die weite Welt.", "tokens": ["Oeff\u00b7net", "sich", "die", "wei\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Errungen", "tokens": ["Er\u00b7run\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Bezwungen", "tokens": ["Be\u00b7zwun\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Von Lieb' ist das Gl\u00fcck,", "tokens": ["Von", "Lieb'", "ist", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Verschwunden", "tokens": ["Ver\u00b7schwun\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die Stunden", "tokens": ["Die", "Stun\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Sie fliehen zur\u00fcck;", "tokens": ["Sie", "flie\u00b7hen", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und seelig Lust", "tokens": ["Und", "see\u00b7lig", "Lust"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sie stillet", "tokens": ["Sie", "stil\u00b7let"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Erf\u00fcllet", "tokens": ["Er\u00b7f\u00fcl\u00b7let"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Die trunkene wonneklopfende Brust,", "tokens": ["Die", "trun\u00b7ke\u00b7ne", "won\u00b7nek\u00b7lop\u00b7fen\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Sie scheide", "tokens": ["Sie", "schei\u00b7de"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Von Leide", "tokens": ["Von", "Lei\u00b7de"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Auf immer,", "tokens": ["Auf", "im\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Und nimmer", "tokens": ["Und", "nim\u00b7mer"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Entschwinde die liebliche, seelige, himmlische Lust!", "tokens": ["Ent\u00b7schwin\u00b7de", "die", "lieb\u00b7li\u00b7che", ",", "see\u00b7li\u00b7ge", ",", "himm\u00b7li\u00b7sche", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}}}}}