{"textgrid.poem.48159": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: So la\u00df uns trinken den letzten Trank,", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So la\u00df uns trinken den letzten Trank,", "tokens": ["So", "la\u00df", "uns", "trin\u00b7ken", "den", "letz\u00b7ten", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "den Trank, der nicht versch\u00e4umt,", "tokens": ["den", "Trank", ",", "der", "nicht", "ver\u00b7sch\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "in dessen Tiefen die Perle versank,", "tokens": ["in", "des\u00b7sen", "Tie\u00b7fen", "die", "Per\u00b7le", "ver\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "die unsere Jugend ertr\u00e4umt.", "tokens": ["die", "un\u00b7se\u00b7re", "Ju\u00b7gend", "er\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Leere das Glas bis auf den Grund,", "tokens": ["Lee\u00b7re", "das", "Glas", "bis", "auf", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "singe dein Lied bis zum Schlu\u00df \u2013", "tokens": ["sin\u00b7ge", "dein", "Lied", "bis", "zum", "Schlu\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "APPRART", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "von meinem glutweinfeuchten Mund", "tokens": ["von", "mei\u00b7nem", "glut\u00b7wein\u00b7feuch\u00b7ten", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "trinke den letzten Ku\u00df.", "tokens": ["trin\u00b7ke", "den", "letz\u00b7ten", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Siehst du, wie tief schon die Sonne steht", "tokens": ["Siehst", "du", ",", "wie", "tief", "schon", "die", "Son\u00b7ne", "steht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "ADV", "ART", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "und wie so rot ihr Licht? \u2013", "tokens": ["und", "wie", "so", "rot", "ihr", "Licht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ob sie in funkelnden Wassern zergeht,", "tokens": ["Und", "ob", "sie", "in", "fun\u00b7keln\u00b7den", "Was\u00b7sern", "zer\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "uns beiden, uns stirbt sie nicht.", "tokens": ["uns", "bei\u00b7den", ",", "uns", "stirbt", "sie", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Uns leuchtet die Nacht, die niedersinkt,", "tokens": ["Uns", "leuch\u00b7tet", "die", "Nacht", ",", "die", "nie\u00b7der\u00b7sinkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "und ladet zum letzten Genu\u00df \u2013", "tokens": ["und", "la\u00b7det", "zum", "letz\u00b7ten", "Ge\u00b7nu\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und unsere lebendige Seele ertrinkt", "tokens": ["Und", "un\u00b7se\u00b7re", "le\u00b7ben\u00b7di\u00b7ge", "See\u00b7le", "er\u00b7trinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "jauchzend im Sch\u00f6pferku\u00df.", "tokens": ["jauch\u00b7zend", "im", "Sch\u00f6p\u00b7fer\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}