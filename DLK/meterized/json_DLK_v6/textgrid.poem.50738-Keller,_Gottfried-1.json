{"textgrid.poem.50738": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie zieht das finster t\u00fcrmende", "tokens": ["Wie", "zieht", "das", "fins\u00b7ter", "t\u00fcr\u00b7men\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Gew\u00f6lk so kalt und schwer!", "tokens": ["Ge\u00b7w\u00f6lk", "so", "kalt", "und", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie jagt der Wind, der st\u00fcrmende,", "tokens": ["Wie", "jagt", "der", "Wind", ",", "der", "st\u00fcr\u00b7men\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Das Schneegest\u00f6ber her!", "tokens": ["Das", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo sonst die Venus funkelte,", "tokens": ["Wo", "sonst", "die", "Ve\u00b7nus", "fun\u00b7kel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist es nun grau und tot;", "tokens": ["Ist", "es", "nun", "grau", "und", "tot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich denk in das verdunkelte", "tokens": ["Ich", "denk", "in", "das", "ver\u00b7dun\u00b7kel\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Westland das Abendrot!", "tokens": ["West\u00b7land", "das", "A\u00b7ben\u00b7drot", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Verschwunden ist die bl\u00fchende", "tokens": ["Ver\u00b7schwun\u00b7den", "ist", "die", "bl\u00fc\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und gr\u00fcne Weltgestalt;", "tokens": ["Und", "gr\u00fc\u00b7ne", "Welt\u00b7ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es eilt der Fu\u00df, der fliehende,", "tokens": ["Es", "eilt", "der", "Fu\u00df", ",", "der", "flie\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Durchs Schneefeld na\u00df und kalt.", "tokens": ["Durchs", "Schnee\u00b7feld", "na\u00df", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wohl dem, der nun zufrieden ist", "tokens": ["Wohl", "dem", ",", "der", "nun", "zu\u00b7frie\u00b7den", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und innerlich sich kennt,", "tokens": ["Und", "in\u00b7ner\u00b7lich", "sich", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem warm ein Herz beschieden ist,", "tokens": ["Dem", "warm", "ein", "Herz", "be\u00b7schie\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das heimlich loht und brennt,", "tokens": ["Das", "heim\u00b7lich", "loht", "und", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wo, traulich sich dran schmiegend, es", "tokens": ["Wo", ",", "trau\u00b7lich", "sich", "dran", "schmie\u00b7gend", ",", "es"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "$,", "ADJD", "PRF", "PAV", "ADJD", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die stille Seele sch\u00fcrt,", "tokens": ["Die", "stil\u00b7le", "See\u00b7le", "sch\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein sprudelnd \u00fcbersiegendes", "tokens": ["Ein", "spru\u00b7delnd", "\u00fc\u00b7ber\u00b7sie\u00b7gen\u00b7des"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedankens\u00fcpplein r\u00fchrt!", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7s\u00fcp\u00b7plein", "r\u00fchrt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}