{"textgrid.poem.38013": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied f\u00fcr immer", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute marschieren wir,", "tokens": ["Heu\u00b7te", "mar\u00b7schie\u00b7ren", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Morgen marschieren wir,", "tokens": ["Mor\u00b7gen", "mar\u00b7schie\u00b7ren", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zu dem hohen Thor hinaus,", "tokens": ["Zu", "dem", "ho\u00b7hen", "Thor", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ey du wacker schwarzbraun M\u00e4gdlein,", "tokens": ["Ey", "du", "wa\u00b7cker", "schwarz\u00b7braun", "M\u00e4gd\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsre Lieb ist noch nicht aus.", "tokens": ["Uns\u00b7re", "Lieb", "ist", "noch", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Reist du schon fort?", "tokens": ["Reist", "du", "schon", "fort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Reist du denn schon fort?", "tokens": ["Reist", "du", "denn", "schon", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kommst du niemals wieder heim?", "tokens": ["Kommst", "du", "nie\u00b7mals", "wie\u00b7der", "heim", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wenn du kommst in ein fremdes L\u00e4ndchen,", "tokens": ["Und", "wenn", "du", "kommst", "in", "ein", "frem\u00b7des", "L\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Liebster Schatz vergi\u00df mein nicht.", "tokens": ["Liebs\u00b7ter", "Schatz", "ver\u00b7gi\u00df", "mein", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Trink du ein Gl\u00e4schen Wein,", "tokens": ["Trink", "du", "ein", "Gl\u00e4s\u00b7chen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zur Gesundheit mein und dein,", "tokens": ["Zur", "Ge\u00b7sund\u00b7heit", "mein", "und", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "KON", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kauf mir einen Strau\u00df am Huth,", "tokens": ["Kauf", "mir", "ei\u00b7nen", "Strau\u00df", "am", "Huth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm mein T\u00fcchlein in die Tasche,", "tokens": ["Nimm", "mein", "T\u00fcch\u00b7lein", "in", "die", "Ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Thr\u00e4nlein mit abwasch.", "tokens": ["Dei\u00b7ne", "Thr\u00e4n\u00b7lein", "mit", "ab\u00b7wasch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Es kommt die Lerche,", "tokens": ["Es", "kommt", "die", "Ler\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Es kommt der Storch,", "tokens": ["Es", "kommt", "der", "Storch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es kommt die Sonne ans Firmament.", "tokens": ["Es", "kommt", "die", "Son\u00b7ne", "ans", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In das Kloster will ich gehn,", "tokens": ["In", "das", "Klos\u00b7ter", "will", "ich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil ich mein Sch\u00e4tzchen nicht mehr thu sehen,", "tokens": ["Weil", "ich", "mein", "Sch\u00e4tz\u00b7chen", "nicht", "mehr", "thu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Weil nicht wiederkommt mein Schatz!", "tokens": ["Weil", "nicht", "wie\u00b7der\u00b7kommt", "mein", "Schatz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbdorten sind zwey Turtelt\u00e4ubchen,", "tokens": ["\u00bb", "dor\u00b7ten", "sind", "zwey", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzen auf dem d\u00fcrren Ast,", "tokens": ["Sit\u00b7zen", "auf", "dem", "d\u00fcr\u00b7ren", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sich zwey Verliebte scheiden,", "tokens": ["Wo", "sich", "zwey", "Ver\u00b7lieb\u00b7te", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da verwelket Laub und Gras,", "tokens": ["Da", "ver\u00b7wel\u00b7ket", "Laub", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was batt mich ein sch\u00f6ner Garten,", "tokens": ["Was", "batt", "mich", "ein", "sch\u00f6\u00b7ner", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn ich nichts darinnen hab,", "tokens": ["Wenn", "ich", "nichts", "da\u00b7rin\u00b7nen", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was batt mich die sch\u00f6nste Rose,", "tokens": ["Was", "batt", "mich", "die", "sch\u00f6ns\u00b7te", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wenn ich sie nicht brechen soll,", "tokens": ["Wenn", "ich", "sie", "nicht", "bre\u00b7chen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Was batt mich ein jung frisch Leben,", "tokens": ["Was", "batt", "mich", "ein", "jung", "frisch", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wenn ichs nicht der Lieb ergeb?\u00ab", "tokens": ["Wenn", "ichs", "nicht", "der", "Lieb", "er\u00b7geb", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}