{"textgrid.poem.41627": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist ein Herbstazur, in leisem Rot verblutend!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist ein Herbstazur, in leisem Rot verblutend!", "tokens": ["Du", "bist", "ein", "Herbs\u00b7ta\u00b7zur", ",", "in", "lei\u00b7sem", "Rot", "ver\u00b7blu\u00b7tend", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedoch die Traurigkeit steigt in mir wie die See,", "tokens": ["Je\u00b7doch", "die", "Trau\u00b7rig\u00b7keit", "steigt", "in", "mir", "wie", "die", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und auf den Lippen l\u00e4\u00dft allm\u00e4hlich r\u00fcckw\u00e4rts flutend", "tokens": ["Und", "auf", "den", "Lip\u00b7pen", "l\u00e4\u00dft", "all\u00b7m\u00e4h\u00b7lich", "r\u00fcck\u00b7w\u00e4rts", "flu\u00b7tend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie ihres salzgen Schlamms erinnrungsbittres Weh.", "tokens": ["Sie", "ih\u00b7res", "salz\u00b7gen", "Schlamms", "e\u00b7rinn\u00b7rungs\u00b7bitt\u00b7res", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du legst die Hand umsonst auf meines Busens Beben,", "tokens": ["Du", "legst", "die", "Hand", "um\u00b7sonst", "auf", "mei\u00b7nes", "Bu\u00b7sens", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tempel, den du suchst, sank, Liebe, l\u00e4ngst in Staub.", "tokens": ["Der", "Tem\u00b7pel", ",", "den", "du", "suchst", ",", "sank", ",", "Lie\u00b7be", ",", "l\u00e4ngst", "in", "Staub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Frauen Krall und Zahn nahm alles mir im Leben,", "tokens": ["Der", "Frau\u00b7en", "Krall", "und", "Zahn", "nahm", "al\u00b7les", "mir", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "PIS", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht suche mehr mein Herz, es ward der Tiere Raub.", "tokens": ["Nicht", "su\u00b7che", "mehr", "mein", "Herz", ",", "es", "ward", "der", "Tie\u00b7re", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Herz ist ein Palast, vom wilden Hauf gesch\u00e4ndet,", "tokens": ["Mein", "Herz", "ist", "ein", "Pa\u00b7last", ",", "vom", "wil\u00b7den", "Hauf", "ge\u00b7sch\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der drin sich t\u00f6tet, packt und tobt, berauscht und roh ...", "tokens": ["Der", "drin", "sich", "t\u00f6\u00b7tet", ",", "packt", "und", "tobt", ",", "be\u00b7rauscht", "und", "roh", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O welchen s\u00fc\u00dfen Duft dein nackter Busen spendet!", "tokens": ["O", "wel\u00b7chen", "s\u00fc\u00b7\u00dfen", "Duft", "dein", "nack\u00b7ter", "Bu\u00b7sen", "spen\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O Sch\u00f6nheit! Gei\u00dfel, die uns schl\u00e4gt! Du willst es so!", "tokens": ["O", "Sch\u00f6n\u00b7heit", "!", "Gei\u00b7\u00dfel", ",", "die", "uns", "schl\u00e4gt", "!", "Du", "willst", "es", "so", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit deinem Feuerblick, dem festlich-glanzverkl\u00e4rten,", "tokens": ["Mit", "dei\u00b7nem", "Feu\u00b7er\u00b7blick", ",", "dem", "fest\u00b7lich\u00b7glanz\u00b7ver\u00b7kl\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verbrenn die Fetzen, die die Tiere nicht verzehrten.", "tokens": ["Ver\u00b7brenn", "die", "Fet\u00b7zen", ",", "die", "die", "Tie\u00b7re", "nicht", "ver\u00b7zehr\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}