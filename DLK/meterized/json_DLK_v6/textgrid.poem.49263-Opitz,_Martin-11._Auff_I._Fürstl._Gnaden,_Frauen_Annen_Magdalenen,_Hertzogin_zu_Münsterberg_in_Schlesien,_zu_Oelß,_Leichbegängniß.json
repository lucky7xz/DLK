{"textgrid.poem.49263": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "11. Auff I. F\u00fcrstl. Gnaden, Frauen Annen Magdalenen, Hertzogin zu M\u00fcnsterberg in Schlesien, zu Oel\u00df, Leichbeg\u00e4ngni\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wol doch wiederf\u00e4hret", "tokens": ["Wie", "wol", "doch", "wie\u00b7der\u00b7f\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem, dem zu solcher Zeit", "tokens": ["Dem", ",", "dem", "zu", "sol\u00b7cher", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein St\u00fcndlein ist bescheret,", "tokens": ["Sein", "St\u00fcnd\u00b7lein", "ist", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann er der V\u00f6lcker Streit,", "tokens": ["Wann", "er", "der", "V\u00f6l\u00b7cker", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Lauff der Welt betrachtet,", "tokens": ["Den", "Lauff", "der", "Welt", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und h\u00e4rtet seinen Sinn,", "tokens": ["Und", "h\u00e4r\u00b7tet", "sei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df er den Tod nicht achtet,", "tokens": ["Da\u00df", "er", "den", "Tod", "nicht", "ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "L\u00e4ufft ihm entgegen hin.", "tokens": ["L\u00e4ufft", "ihm", "ent\u00b7ge\u00b7gen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Der zu dem St\u00e4dte-Brande", "tokens": ["Der", "zu", "dem", "St\u00e4d\u00b7te\u00b7Bran\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Christenhertze bringt", "tokens": ["Ein", "Chris\u00b7ten\u00b7hert\u00b7ze", "bringt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nach dem Vatterlande,", "tokens": ["Und", "nach", "dem", "Vat\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da kein Feind eink\u00f6mpt, ringt;", "tokens": ["Da", "kein", "Feind", "ein\u00b7k\u00f6mpt", ",", "ringt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der de\u00df Gebetes St\u00fccke", "tokens": ["Der", "de\u00df", "Ge\u00b7be\u00b7tes", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Pflantzt f\u00fcr die Himmelsstatt", "tokens": ["Pflantzt", "f\u00fcr", "die", "Him\u00b7mels\u00b7statt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und weichet nicht zur\u00fccke", "tokens": ["Und", "wei\u00b7chet", "nicht", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bi\u00df er das Jawort hat.", "tokens": ["Bi\u00df", "er", "das", "Ja\u00b7wort", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er ist schon hier im Hertzen", "tokens": ["Er", "ist", "schon", "hier", "im", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Lust und Freuden voll,", "tokens": ["Der", "Lust", "und", "Freu\u00b7den", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darzu kein Leyd noch Schmertzen", "tokens": ["Dar\u00b7zu", "kein", "Leyd", "noch", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PIAT", "NN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich jemals dringen soll;", "tokens": ["Sich", "je\u00b7mals", "drin\u00b7gen", "soll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wann es so weit kommen,", "tokens": ["Und", "wann", "es", "so", "weit", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df nun die Uhr ist au\u00df,", "tokens": ["Da\u00df", "nun", "die", "Uhr", "ist", "au\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So wird er auffgenommen", "tokens": ["So", "wird", "er", "auff\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In seines Gottes Hau\u00df.", "tokens": ["In", "sei\u00b7nes", "Got\u00b7tes", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da weydet sein Gem\u00fcte", "tokens": ["Da", "wey\u00b7det", "sein", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich mit der G\u00f6ttligkeit,", "tokens": ["Sich", "mit", "der", "G\u00f6tt\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An derer Huld und G\u00fcte", "tokens": ["An", "de\u00b7rer", "Huld", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es schon hieng f\u00fcr der Zeit;", "tokens": ["Es", "schon", "hieng", "f\u00fcr", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Da sieht er, wie die Kronen", "tokens": ["Da", "sieht", "er", ",", "wie", "die", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Scepter mi\u00dflich sind,", "tokens": ["Und", "Scep\u00b7ter", "mi\u00df\u00b7lich", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie dieses, wo wir wohnen,", "tokens": ["Wie", "die\u00b7ses", ",", "wo", "wir", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nichts sey als Rauch und Wind", "tokens": ["Nichts", "sey", "als", "Rauch", "und", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "KOKOM", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du auch, du Liecht der Frauen,", "tokens": ["Du", "auch", ",", "du", "Liecht", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Heldinn, Bild der Zucht,", "tokens": ["O", "Hel\u00b7dinn", ",", "Bild", "der", "Zucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann du hast m\u00fcssen schauen", "tokens": ["Wann", "du", "hast", "m\u00fcs\u00b7sen", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Freyheit schn\u00f6de Flucht,", "tokens": ["Der", "Frey\u00b7heit", "schn\u00f6\u00b7de", "Flucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Zeit, in der wir leben,", "tokens": ["Die", "Zeit", ",", "in", "der", "wir", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Dinge blinden Schein,", "tokens": ["Der", "Din\u00b7ge", "blin\u00b7den", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So hast du dich ergeben", "tokens": ["So", "hast", "du", "dich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "De\u00df Lebens satt zu seyn.", "tokens": ["De\u00df", "Le\u00b7bens", "satt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du auch bist hin versetzet", "tokens": ["Du", "auch", "bist", "hin", "ver\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ein solche Schar,", "tokens": ["In", "ein", "sol\u00b7che", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die sich mit dem ergetzet,", "tokens": ["Die", "sich", "mit", "dem", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der bleibt und ist und war,", "tokens": ["Der", "bleibt", "und", "ist", "und", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VAFIN", "KON", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der dir hat angeleget", "tokens": ["Der", "dir", "hat", "an\u00b7ge\u00b7le\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Rock der Ewigkeit", "tokens": ["Den", "Rock", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der keine Hitze tr\u00e4get", "tokens": ["Der", "kei\u00b7ne", "Hit\u00b7ze", "tr\u00e4\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und den kein Frost beschneyt,", "tokens": ["Und", "den", "kein", "Frost", "be\u00b7schneyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du darffst nun nicht mehr fragen", "tokens": ["Du", "darffst", "nun", "nicht", "mehr", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was umb den sch\u00f6nen Rhein", "tokens": ["Was", "umb", "den", "sch\u00f6\u00b7nen", "Rhein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich etwann zugetragen,", "tokens": ["Sich", "et\u00b7wann", "zu\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der jetzt mu\u00df dienstbar seyn,", "tokens": ["Der", "jetzt", "mu\u00df", "dienst\u00b7bar", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ob deinem Vatterlande", "tokens": ["Ob", "dei\u00b7nem", "Vat\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was Neues ist bestimpt,", "tokens": ["Was", "Neu\u00b7es", "ist", "be\u00b7stimpt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ob an der Mosel Strande", "tokens": ["Ob", "an", "der", "Mo\u00b7sel", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein frembdes Feuer glimmt.", "tokens": ["Ein", "fremb\u00b7des", "Feu\u00b7er", "glimmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du darffst nicht weiter sehen,", "tokens": ["Du", "darffst", "nicht", "wei\u00b7ter", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie auff di\u00df arme Land", "tokens": ["Wie", "auff", "di\u00df", "ar\u00b7me", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PDS", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wilde St\u00fcrme wehen", "tokens": ["So", "wil\u00b7de", "St\u00fcr\u00b7me", "we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dr\u00e4uen Mord und Brand,", "tokens": ["Und", "dr\u00e4u\u00b7en", "Mord", "und", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie so viel werthe F\u00fcrsten", "tokens": ["Wie", "so", "viel", "wert\u00b7he", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VVFIN", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Im Streiten untergehn,", "tokens": ["Im", "Strei\u00b7ten", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie wir nach Blute d\u00fcrsten", "tokens": ["Wie", "wir", "nach", "Blu\u00b7te", "d\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach Feind' und Freunde stehn.", "tokens": ["Nach", "Feind'", "und", "Freun\u00b7de", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambi