{"dta.poem.3792": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Vorbericht.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Entweicht, ihr unberuffnen Dich-", "tokens": ["Ent\u00b7weicht", ",", "ihr", "un\u00b7be\u00b7ruff\u00b7nen", "Dich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ter,", "tokens": ["ter", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Singt auf den B\u00e4ncken Bauren", "tokens": ["Singt", "auf", "den", "B\u00e4n\u00b7cken", "Bau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "vor,", "tokens": ["vor", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Sorgt niemand vor ein kennend", "tokens": ["Sorgt", "nie\u00b7mand", "vor", "ein", "ken\u00b7nend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ohr?", "tokens": ["Ohr", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Gantz Teutschland schnarrt von fei-", "tokens": ["Gantz", "Teutschland", "schnarrt", "von", "fei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "APPR", "TRUNC"], "meter": "--+-+", "measure": "anapaest.init"}, "line.8": {"text": "len Leyern,", "tokens": ["len", "Ley\u00b7ern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Gantz Teutschland qvillt von n\u00fcch-", "tokens": ["Gantz", "Teutschland", "qvillt", "von", "n\u00fcch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "APPR", "TRUNC"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "tern Schreyern,", "tokens": ["tern", "Schrey\u00b7ern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Auch Fr\u00f6sche sind nicht so gemein!", "tokens": ["Auch", "Fr\u00f6\u00b7sche", "sind", "nicht", "so", "ge\u00b7mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}}}}