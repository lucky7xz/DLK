{"textgrid.poem.49094": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst zu dem ber\u00fchmten Waffenh\u00e4ndler", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst zu dem ber\u00fchmten Waffenh\u00e4ndler", "tokens": ["Einst", "zu", "dem", "be\u00b7r\u00fchm\u00b7ten", "Waf\u00b7fen\u00b7h\u00e4nd\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kam ein J\u00fcngling. Lehre mich gebrauchen,", "tokens": ["Kam", "ein", "J\u00fcng\u00b7ling", ".", "Leh\u00b7re", "mich", "ge\u00b7brau\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bat er, deine starke, sch\u00f6ne Kriegerwaffen,", "tokens": ["Bat", "er", ",", "dei\u00b7ne", "star\u00b7ke", ",", "sch\u00f6\u00b7ne", "Krie\u00b7ger\u00b7waf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Da\u00df ich tapfer sie, mit Anstand f\u00fchre,", "tokens": ["Da\u00df", "ich", "tap\u00b7fer", "sie", ",", "mit", "An\u00b7stand", "f\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn zum Kampfe die Drommete ruft!", "tokens": ["Wenn", "zum", "Kamp\u00b7fe", "die", "Drom\u00b7me\u00b7te", "ruft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--++-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "D'rauf der Alte, mit dem Kopfe sch\u00fcttelnd:", "tokens": ["D'\u00b7rauf", "der", "Al\u00b7te", ",", "mit", "dem", "Kop\u00b7fe", "sch\u00fct\u00b7telnd", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sieh', die Waffen stehn hier zum Verkaufe;", "tokens": ["Sieh'", ",", "die", "Waf\u00b7fen", "stehn", "hier", "zum", "Ver\u00b7kau\u00b7fe", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schmieden lernt' ich sie, doch nimmer f\u00fchren.", "tokens": ["Schmie\u00b7den", "lernt'", "ich", "sie", ",", "doch", "nim\u00b7mer", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Anders ist's nicht mit den Theorien,", "tokens": ["An\u00b7ders", "ist's", "nicht", "mit", "den", "The\u00b7o\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Von den Meistern fein und klug ersonnen.", "tokens": ["Von", "den", "Meis\u00b7tern", "fein", "und", "klug", "er\u00b7son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Anzuwenden, praktisch zu bew\u00e4hren,", "tokens": ["An\u00b7zu\u00b7wen\u00b7den", ",", "prak\u00b7tisch", "zu", "be\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fehlt die Kraft den Meistern, und je blanker", "tokens": ["Fehlt", "die", "Kraft", "den", "Meis\u00b7tern", ",", "und", "je", "blan\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "KON", "ADV", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ihre Waffen gl\u00e4nzen, desto seltner", "tokens": ["Ih\u00b7re", "Waf\u00b7fen", "gl\u00e4n\u00b7zen", ",", "des\u00b7to", "selt\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "ADV", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Taugen sie, wenn's gilt, im Waffentanze.", "tokens": ["Tau\u00b7gen", "sie", ",", "wenn's", "gilt", ",", "im", "Waf\u00b7fen\u00b7tan\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PIS", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}