{"textgrid.poem.39440": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der V\u00f6lkersitten, mancher fremden St\u00e4tte", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der V\u00f6lkersitten, mancher fremden St\u00e4tte", "tokens": ["Der", "V\u00f6l\u00b7ker\u00b7sit\u00b7ten", ",", "man\u00b7cher", "frem\u00b7den", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihrer Sprache fr\u00fche schon erfahren,", "tokens": ["Und", "ih\u00b7rer", "Spra\u00b7che", "fr\u00fc\u00b7he", "schon", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was alte Zeit, was neue Zeit gebaren", "tokens": ["Was", "al\u00b7te", "Zeit", ",", "was", "neu\u00b7e", "Zeit", "ge\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "NN", "$,", "PWS", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vereinigend in Eines Wi\u00dfens Kette,", "tokens": ["Ver\u00b7ei\u00b7ni\u00b7gend", "in", "Ei\u00b7nes", "Wi\u00b7\u00dfens", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Stehn, im Gehn, im Wachen und im Bette,", "tokens": ["Im", "Stehn", ",", "im", "Gehn", ",", "im", "Wa\u00b7chen", "und", "im", "Bet\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Reisen selbst, wie unter'm Schutz der Laren", "tokens": ["Auf", "Rei\u00b7sen", "selbst", ",", "wie", "un\u00b7ter'm", "Schutz", "der", "La\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "$,", "PWAV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stets dichtend, aller, die es sind und waren,", "tokens": ["Stets", "dich\u00b7tend", ",", "al\u00b7ler", ",", "die", "es", "sind", "und", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PIAT", "$,", "PRELS", "PPER", "VAFIN", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Besieger, Muster, Meister im Sonette.", "tokens": ["Be\u00b7sie\u00b7ger", ",", "Mus\u00b7ter", ",", "Meis\u00b7ter", "im", "So\u00b7net\u00b7te", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Erste, der's gewagt auf deutscher Erde", "tokens": ["Der", "Ers\u00b7te", ",", "der's", "ge\u00b7wagt", "auf", "deut\u00b7scher", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Shakspear's Geist zu ringen und mit Dante,", "tokens": ["Mit", "Shak\u00b7spea\u00b7r's", "Geist", "zu", "rin\u00b7gen", "und", "mit", "Dan\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKZU", "VVINF", "KON", "APPR", "NE", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Zugleich der Sch\u00f6pfer und das Bild der Regel:", "tokens": ["Zu\u00b7gleich", "der", "Sch\u00f6p\u00b7fer", "und", "das", "Bild", "der", "Re\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie ihn der Mund der Zukunft nennen werde,", "tokens": ["Wie", "ihn", "der", "Mund", "der", "Zu\u00b7kunft", "nen\u00b7nen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist unbekannt, doch die\u00df Geschlecht erkannte", "tokens": ["Ist", "un\u00b7be\u00b7kannt", ",", "doch", "die\u00df", "Ge\u00b7schlecht", "er\u00b7kann\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KON", "PDS", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihn bei dem Namen ", "tokens": ["Ihn", "bei", "dem", "Na\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}}}}