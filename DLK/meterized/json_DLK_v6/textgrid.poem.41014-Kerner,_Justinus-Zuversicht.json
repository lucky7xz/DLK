{"textgrid.poem.41014": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Zuversicht", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergesset nicht, warum so fr\u00fch", "tokens": ["Ver\u00b7ges\u00b7set", "nicht", ",", "wa\u00b7rum", "so", "fr\u00fch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie unsern Augen ist entschwunden, \u2013", "tokens": ["Sie", "un\u00b7sern", "Au\u00b7gen", "ist", "ent\u00b7schwun\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VAFIN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf da\u00df zum Schutzgeist werde sie", "tokens": ["Auf", "da\u00df", "zum", "Schutz\u00b7geist", "wer\u00b7de", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPRART", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Lande in des Leides Stunden.", "tokens": ["Dem", "Lan\u00b7de", "in", "des", "Lei\u00b7des", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es fl\u00f6ssen jetzt noch meine Tr\u00e4nen,", "tokens": ["Es", "fl\u00f6s\u00b7sen", "jetzt", "noch", "mei\u00b7ne", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df solch ein Herz so bald gestorben,", "tokens": ["Da\u00df", "solch", "ein", "Herz", "so", "bald", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fchlt' ich nicht fest in meinem Sehnen,", "tokens": ["F\u00fchlt'", "ich", "nicht", "fest", "in", "mei\u00b7nem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir es tot erst ganz erworben.", "tokens": ["Da\u00df", "wir", "es", "tot", "erst", "ganz", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie sie im Leben einst dem Land", "tokens": ["Wie", "sie", "im", "Le\u00b7ben", "einst", "dem", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Hungerjahren war ein Segen,", "tokens": ["In", "Hun\u00b7ger\u00b7jah\u00b7ren", "war", "ein", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt sie, ein Engel jetzt, die Hand", "tokens": ["Kommt", "sie", ",", "ein", "En\u00b7gel", "jetzt", ",", "die", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seines K\u00f6nigs Brust zu legen.", "tokens": ["Auf", "sei\u00b7nes", "K\u00f6\u00b7nigs", "Brust", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie schwebt um ihn zur Zeit, der tr\u00fcben,", "tokens": ["Sie", "schwebt", "um", "ihn", "zur", "Zeit", ",", "der", "tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo seiner Kinder Herzen beben,", "tokens": ["Wo", "sei\u00b7ner", "Kin\u00b7der", "Her\u00b7zen", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo seines Volkes hei\u00dfes Lieben", "tokens": ["Wo", "sei\u00b7nes", "Vol\u00b7kes", "hei\u00b7\u00dfes", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Leben kn\u00fcpfet an sein Leben.", "tokens": ["Sein", "Le\u00b7ben", "kn\u00fcp\u00b7fet", "an", "sein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Seitdem sie unter Engeln weilt,", "tokens": ["Seit\u00b7dem", "sie", "un\u00b7ter", "En\u00b7geln", "weilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat dieses Land kein Leid erfahren.", "tokens": ["Hat", "die\u00b7ses", "Land", "kein", "Leid", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist der Schutzgeist, der ihn heilt", "tokens": ["Sie", "ist", "der", "Schutz\u00b7geist", ",", "der", "ihn", "heilt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihn noch lang uns wird bewahren.", "tokens": ["Und", "ihn", "noch", "lang", "uns", "wird", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}