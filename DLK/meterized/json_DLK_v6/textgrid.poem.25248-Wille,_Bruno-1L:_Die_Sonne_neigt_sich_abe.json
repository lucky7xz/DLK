{"textgrid.poem.25248": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne neigt sich abe", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne neigt sich abe", "tokens": ["Die", "Son\u00b7ne", "neigt", "sich", "a\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum blauen H\u00fcgelgrabe.", "tokens": ["Zum", "blau\u00b7en", "H\u00fc\u00b7gel\u00b7gra\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So leb denn wohl, du rotes Liebesfeuer!", "tokens": ["So", "leb", "denn", "wohl", ",", "du", "ro\u00b7tes", "Lie\u00b7bes\u00b7feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich stehe ganz allein", "tokens": ["Ich", "ste\u00b7he", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf \u00f6dem Berggestein.", "tokens": ["Auf", "\u00f6\u00b7dem", "Berg\u00b7ge\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wohl heime m\u00f6cht ich gahn", "tokens": ["Wohl", "hei\u00b7me", "m\u00f6cht", "ich", "gahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VMFIN", "PPER", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wei\u00df doch nicht, wo Herberg han ...", "tokens": ["Und", "wei\u00df", "doch", "nicht", ",", "wo", "Her\u00b7berg", "han", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "NE", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schon dr\u00e4un die Wolken schwarz wie Ungeheuer.", "tokens": ["Schon", "dr\u00e4un", "die", "Wol\u00b7ken", "schwarz", "wie", "Un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da mahnt die Sonn im Sinken:", "tokens": ["Da", "mahnt", "die", "Sonn", "im", "Sin\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sieh dort die Zinnen winken!", "tokens": ["Sieh", "dort", "die", "Zin\u00b7nen", "win\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den irren Wandrer laden sie, zu hausen.", "tokens": ["Den", "ir\u00b7ren", "Wand\u00b7rer", "la\u00b7den", "sie", ",", "zu", "hau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Burgherrn Trostlicht wacht", "tokens": ["Des", "Burg\u00b7herrn", "Trost\u00b7licht", "wacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Getreu die ganze Nacht.", "tokens": ["Ge\u00b7treu", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Entz\u00fcnde dran dein Herze", "tokens": ["Ent\u00b7z\u00fcn\u00b7de", "dran", "dein", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PAV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als eine fromme Klausenkerze!", "tokens": ["Als", "ei\u00b7ne", "from\u00b7me", "Klau\u00b7sen\u00b7ker\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ums Fenstergitter la\u00df Unholde sausen!", "tokens": ["Ums", "Fens\u00b7ter\u00b7git\u00b7ter", "la\u00df", "Un\u00b7hol\u00b7de", "sau\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVIMP", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}