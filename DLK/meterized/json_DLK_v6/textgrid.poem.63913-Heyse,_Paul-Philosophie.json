{"textgrid.poem.63913": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Philosophie", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachdenken doch immer M\u00fche macht,", "tokens": ["Nach\u00b7den\u00b7ken", "doch", "im\u00b7mer", "M\u00fc\u00b7he", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie gut man euch auch vorgedacht.", "tokens": ["Wie", "gut", "man", "euch", "auch", "vor\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor deine Dialektik stellt", "tokens": ["Vor", "dei\u00b7ne", "Di\u00b7a\u00b7lek\u00b7tik", "stellt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich wie im Stereoskop die Welt", "tokens": ["Sich", "wie", "im", "Ste\u00b7reos\u00b7kop", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "KOKOM", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwiefach geteilten Scheines.", "tokens": ["Zwie\u00b7fach", "ge\u00b7teil\u00b7ten", "Schei\u00b7nes", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Doch hast du wahren Tiefsinns Kraft,", "tokens": ["Doch", "hast", "du", "wah\u00b7ren", "Tief\u00b7sinns", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So schaue, was auseinanderklafft,", "tokens": ["So", "schau\u00b7e", ",", "was", "aus\u00b7ein\u00b7an\u00b7der\u00b7klafft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Lebendig wieder in eines.", "tokens": ["Le\u00b7ben\u00b7dig", "wie\u00b7der", "in", "ei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PIS", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}