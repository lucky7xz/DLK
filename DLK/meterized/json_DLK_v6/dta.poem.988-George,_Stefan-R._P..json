{"dta.poem.988": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "R. P.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Was frommt die weisheit dem bezirk des wahnes nahe                 ", "tokens": ["Was", "frommt", "die", "weis\u00b7heit", "dem", "be\u00b7zirk", "des", "wah\u00b7nes", "na\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die uns mit grellem blenden schreckt und &#252;berw&#228;ltigt                 ", "tokens": ["Die", "uns", "mit", "grel\u00b7lem", "blen\u00b7den", "schreckt", "und", "&#252;", "berw", "&#228;", "ltigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "XML_entity", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "KON", "$(", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des einen unkund wo sie b&#252;rde wird und frevel?                 ", "tokens": ["Des", "ei\u00b7nen", "un\u00b7kund", "wo", "sie", "b", "&#252;", "rde", "wird", "und", "fre\u00b7vel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "KON", "PWAV", "PPER", "XY", "$(", "NN", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie friedenlos du allerbleichster unsrer br&#252;der                 ", "tokens": ["Wie", "frie\u00b7den\u00b7los", "du", "al\u00b7ler\u00b7bleichs\u00b7ter", "uns\u00b7rer", "br", "&#252;", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "PPOSAT", "NN", "$(", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durchirrst du deine traurigen und weiten lande!                 ", "tokens": ["Durch\u00b7irrst", "du", "dei\u00b7ne", "trau\u00b7ri\u00b7gen", "und", "wei\u00b7ten", "lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann wirst du m&#252;de neue felder zu erobern                 ", "tokens": ["Wann", "wirst", "du", "m", "&#252;", "de", "neu\u00b7e", "fel\u00b7der", "zu", "er\u00b7o\u00b7bern"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "FM", "FM", "FM", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und lernest einmal pflanzen pflegen und dich freuen                 ", "tokens": ["Und", "ler\u00b7nest", "ein\u00b7mal", "pflan\u00b7zen", "pfle\u00b7gen", "und", "dich", "freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "VVINF", "VVINF", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An dem was bl&#252;ht und gr&#252;nt und reift in dreien g&#228;rten?               ", "tokens": ["An", "dem", "was", "bl", "&#252;", "ht", "und", "gr", "&#252;", "nt", "und", "reift", "in", "drei\u00b7en", "g", "&#228;", "rten", "?"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["APPR", "ART", "PRELS", "NE", "$(", "VVFIN", "KON", "XY", "$(", "VVFIN", "KON",