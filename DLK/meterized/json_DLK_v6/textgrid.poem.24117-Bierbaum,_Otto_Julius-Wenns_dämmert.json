{"textgrid.poem.24117": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Wenns d\u00e4mmert", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Tag um Tag geht still dahin,", "tokens": ["Und", "Tag", "um", "Tag", "geht", "still", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine ruhigen Augen sehn,", "tokens": ["Und", "mei\u00b7ne", "ru\u00b7hi\u00b7gen", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie alle W\u00fcnsche wunschlos still", "tokens": ["Wie", "al\u00b7le", "W\u00fcn\u00b7sche", "wun\u00b7schlos", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In eine blasse D\u00e4mmerung gehn.", "tokens": ["In", "ei\u00b7ne", "blas\u00b7se", "D\u00e4m\u00b7me\u00b7rung", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Dich lieb ich, du! Oh komm, sei mein!", "tokens": ["Dich", "lieb", "ich", ",", "du", "!", "Oh", "komm", ",", "sei", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "$.", "ITJ", "VVFIN", "$,", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein grauer Nebel kommt und steht.", "tokens": ["Ein", "grau\u00b7er", "Ne\u00b7bel", "kommt", "und", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo bist du?! Alles grau und leer.", "tokens": ["Wo", "bist", "du", "?!", "Al\u00b7les", "grau", "und", "leer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mein Begehren wankt und geht.", "tokens": ["Und", "mein", "Be\u00b7geh\u00b7ren", "wankt", "und", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohin, wohin!? Ich seh kein Licht,", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", "!?", "Ich", "seh", "kein", "Licht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Graue schwindet, was ich will.", "tokens": ["Ins", "Grau\u00b7e", "schwin\u00b7det", ",", "was", "ich", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df gehn dahin und frage nicht,", "tokens": ["La\u00df", "gehn", "da\u00b7hin", "und", "fra\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PAV", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df gehn dahin und blicke still.", "tokens": ["La\u00df", "gehn", "da\u00b7hin", "und", "bli\u00b7cke", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PAV", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wunsch geht und Welt geruhig hin,", "tokens": ["Wunsch", "geht", "und", "Welt", "ge\u00b7ru\u00b7hig", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine ruhigen Augen sehn,", "tokens": ["Und", "mei\u00b7ne", "ru\u00b7hi\u00b7gen", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie alle W\u00fcnsche wunschlos still", "tokens": ["Wie", "al\u00b7le", "W\u00fcn\u00b7sche", "wun\u00b7schlos", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In eine blasse D\u00e4mmerung gehn.", "tokens": ["In", "ei\u00b7ne", "blas\u00b7se", "D\u00e4m\u00b7me\u00b7rung", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}