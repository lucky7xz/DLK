{"textgrid.poem.47969": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr heut", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will dir keine Freude rauben", "tokens": ["Ich", "will", "dir", "kei\u00b7ne", "Freu\u00b7de", "rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und binde dich mit keiner Pflicht;", "tokens": ["und", "bin\u00b7de", "dich", "mit", "kei\u00b7ner", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich baue nicht auf Treu und Glauben,", "tokens": ["ich", "bau\u00b7e", "nicht", "auf", "Treu", "und", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein festes Wort begehr ich nicht!", "tokens": ["ein", "fes\u00b7tes", "Wort", "be\u00b7gehr", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr all die Liebe la\u00df mich danken,", "tokens": ["F\u00fcr", "all", "die", "Lie\u00b7be", "la\u00df", "mich", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die du mir reich und gl\u00fchend gibst, \u2013", "tokens": ["die", "du", "mir", "reich", "und", "gl\u00fc\u00b7hend", "gibst", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und mag dein Herz schon morgen wanken:", "tokens": ["und", "mag", "dein", "Herz", "schon", "mor\u00b7gen", "wan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich wei\u00df, da\u00df du mich heute liebst!", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "du", "mich", "heu\u00b7te", "liebst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch sch\u00e4umt der Wein im Goldpokale,", "tokens": ["Noch", "sch\u00e4umt", "der", "Wein", "im", "Gold\u00b7po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "noch duftet frisch der Bl\u00fctenstrau\u00df,", "tokens": ["noch", "duf\u00b7tet", "frisch", "der", "Bl\u00fc\u00b7ten\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Jugend gie\u00dft die volle Schale", "tokens": ["die", "Ju\u00b7gend", "gie\u00dft", "die", "vol\u00b7le", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "des Gl\u00fccks ob unsern H\u00e4upten aus; \u2013", "tokens": ["des", "Gl\u00fccks", "ob", "un\u00b7sern", "H\u00e4up\u00b7ten", "aus", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KOUS", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mit allen seinen Glutgedanken", "tokens": ["mit", "al\u00b7len", "sei\u00b7nen", "Glut\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "zu eigen nimm mein tiefstes Sein . . .", "tokens": ["zu", "ei\u00b7gen", "nimm", "mein", "tiefs\u00b7tes", "Sein", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PTKA", "ADJD", "VVIMP", "PPOSAT", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und mag der Erdball morgen wanken:", "tokens": ["und", "mag", "der", "Erd\u00b7ball", "mor\u00b7gen", "wan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr heut, Geliebter, bist du mein!", "tokens": ["F\u00fcr", "heut", ",", "Ge\u00b7lieb\u00b7ter", ",", "bist", "du", "mein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}