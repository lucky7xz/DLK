{"textgrid.poem.42220": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Am Scheidewege", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der schwere Fluch, der auf dem Haupt mir lastet,", "tokens": ["Der", "schwe\u00b7re", "Fluch", ",", "der", "auf", "dem", "Haupt", "mir", "las\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dr\u00fcckt mich darnieder in den Stra\u00dfenkot;", "tokens": ["Dr\u00fcckt", "mich", "dar\u00b7nie\u00b7der", "in", "den", "Stra\u00b7\u00dfen\u00b7kot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Gott, o Welt, erbarmt euch meiner Not;", "tokens": ["O", "Gott", ",", "o", "Welt", ",", "er\u00b7barmt", "euch", "mei\u00b7ner", "Not", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr wi\u00dft, weswegen ich ihn angetastet.", "tokens": ["Ihr", "wi\u00dft", ",", "wes\u00b7we\u00b7gen", "ich", "ihn", "an\u00b7ge\u00b7tas\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr wi\u00dft, ihr selber jagtet mich hinein,", "tokens": ["Ihr", "wi\u00dft", ",", "ihr", "sel\u00b7ber", "jag\u00b7tet", "mich", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit tausend Peitschenhieben ins Verderben;", "tokens": ["Mit", "tau\u00b7send", "Peit\u00b7schen\u00b7hie\u00b7ben", "ins", "Ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nehmt mich zur S\u00fchne denn und la\u00dft mich sterben,", "tokens": ["Nehmt", "mich", "zur", "S\u00fch\u00b7ne", "denn", "und", "la\u00dft", "mich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur la\u00dft mich nicht so schimpflich elend sein.", "tokens": ["Nur", "la\u00dft", "mich", "nicht", "so", "schimpf\u00b7lich", "e\u00b7lend", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKNEG", "ADV", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich war nicht schlecht; nun mag ich's freilich werden,", "tokens": ["Ich", "war", "nicht", "schlecht", ";", "nun", "mag", "ich's", "frei\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "ADV", "VMFIN", "PIS", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gab ich mein Bestes doch zum Opfer hin.", "tokens": ["Gab", "ich", "mein", "Bes\u00b7tes", "doch", "zum", "Op\u00b7fer", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nehmt mich hinweg, solang ich Mensch noch bin!", "tokens": ["Nehmt", "mich", "hin\u00b7weg", ",", "so\u00b7lang", "ich", "Mensch", "noch", "bin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APZR", "$,", "KOUS", "PPER", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Tier, ein Teufel werd ich sonst auf Erden.", "tokens": ["Ein", "Tier", ",", "ein", "Teu\u00b7fel", "werd", "ich", "sonst", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}