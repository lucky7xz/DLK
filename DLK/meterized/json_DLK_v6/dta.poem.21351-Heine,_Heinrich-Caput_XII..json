{"dta.poem.21351": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich bin kein Schaaf, ich bin kein Hund,", "tokens": ["\u201e", "ich", "bin", "kein", "Schaaf", ",", "ich", "bin", "kein", "Hund", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Hofrath und kein Schellfisch \u2013", "tokens": ["Kein", "Hof\u00b7rath", "und", "kein", "Schell\u00b7fisch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bin ein Wolf geblieben, mein Herz", "tokens": ["Ich", "bin", "ein", "Wolf", "ge\u00b7blie\u00b7ben", ",", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NE", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und meine Z\u00e4hne sind w\u00f6lfisch.", "tokens": ["Und", "mei\u00b7ne", "Z\u00e4h\u00b7ne", "sind", "w\u00f6l\u00b7fisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eich bin ein Wolf und werde stets", "tokens": ["\u201e", "ich", "bin", "ein", "Wolf", "und", "wer\u00b7de", "stets"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "NE", "KON", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch heulen mit den W\u00f6lfen \u2013", "tokens": ["Auch", "heu\u00b7len", "mit", "den", "W\u00f6l\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ja, z\u00e4hlt auf mich und helft Euch selbst,", "tokens": ["Ja", ",", "z\u00e4hlt", "auf", "mich", "und", "helft", "Euch", "selbst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann wird auch Gott Euch helfen!\u201c", "tokens": ["Dann", "wird", "auch", "Gott", "Euch", "hel\u00b7fen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das war die Rede, die ich hielt,", "tokens": ["Das", "war", "die", "Re\u00b7de", ",", "die", "ich", "hielt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz ohne Vorbereitung;", "tokens": ["Ganz", "oh\u00b7ne", "Vor\u00b7be\u00b7rei\u00b7tung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verst\u00fcmmelt hat Kolb sie abgedruckt", "tokens": ["Ver\u00b7st\u00fcm\u00b7melt", "hat", "Kolb", "sie", "ab\u00b7ge\u00b7druckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "NE", "PPER", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In der Allgemeinen Zeitung.", "tokens": ["In", "der", "All\u00b7ge\u00b7mei\u00b7nen", "Zei\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}