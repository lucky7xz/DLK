{"textgrid.poem.32600": {"metadata": {"author": {"name": "Shelley, Percy Bysshe", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wilder Westwind, du des Herbstes Lied,", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wilder Westwind, du des Herbstes Lied,", "tokens": ["O", "wil\u00b7der", "West\u00b7wind", ",", "du", "des", "Herbs\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor dessen unsichtbarem Hauch das Blatt,", "tokens": ["Vor", "des\u00b7sen", "un\u00b7sicht\u00b7ba\u00b7rem", "Hauch", "das", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Schemen gleich, der vor dem Zaubrer flieht,", "tokens": ["Dem", "Sche\u00b7men", "gleich", ",", "der", "vor", "dem", "Zaub\u00b7rer", "flieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Fahl, pestergriffen, hektisch roth und matt,", "tokens": ["Fahl", ",", "pes\u00b7ter\u00b7grif\u00b7fen", ",", "hek\u00b7tisch", "roth", "und", "matt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein todtes Laub, zur Erde f\u00e4llt! O du,", "tokens": ["Ein", "tod\u00b7tes", "Laub", ",", "zur", "Er\u00b7de", "f\u00e4llt", "!", "O", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "$.", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der zu der winterlichen Ruhestatt", "tokens": ["Der", "zu", "der", "win\u00b7ter\u00b7li\u00b7chen", "Ru\u00b7he\u00b7statt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Saaten f\u00fchrt \u2013 die Scholle deckt sie zu,", "tokens": ["Die", "Saa\u00b7ten", "f\u00fchrt", "\u2013", "die", "Schol\u00b7le", "deckt", "sie", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da liegen sie wie Leichen starr und kalt,", "tokens": ["Da", "lie\u00b7gen", "sie", "wie", "Lei\u00b7chen", "starr", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis deine Fr\u00fchlingsschwester aus der Ruh'", "tokens": ["Bis", "dei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7schwes\u00b7ter", "aus", "der", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die tr\u00e4umenden Gefilde weckt, und bald", "tokens": ["Die", "tr\u00e4u\u00b7men\u00b7den", "Ge\u00b7fil\u00b7de", "weckt", ",", "und", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die auferstandnen Keim' in Bl\u00fcthen sich", "tokens": ["Die", "auf\u00b7er\u00b7stand\u00b7nen", "Keim'", "in", "Bl\u00fc\u00b7then", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verwandeln, denen s\u00fc\u00dfer Duft entwallt: \u2013", "tokens": ["Ver\u00b7wan\u00b7deln", ",", "de\u00b7nen", "s\u00fc\u00b7\u00dfer", "Duft", "ent\u00b7wallt", ":", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Allgegenw\u00e4rt'ger Geist, ich rufe dich,", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4rt'\u00b7ger", "Geist", ",", "ich", "ru\u00b7fe", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Zerst\u00f6rer und Erhalter, h\u00f6re mich!", "tokens": ["Zer\u00b7st\u00f6\u00b7rer", "und", "Er\u00b7hal\u00b7ter", ",", "h\u00f6\u00b7re", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du, dessen Str\u00f6mung bei des Wetters Groll", "tokens": ["Du", ",", "des\u00b7sen", "Str\u00f6\u00b7mung", "bei", "des", "Wet\u00b7ters", "Groll"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Wolken von des Himmels Luftgezweig", "tokens": ["Die", "Wol\u00b7ken", "von", "des", "Him\u00b7mels", "Luft\u00b7ge\u00b7zweig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(engel von Blitz und Regen sind es) toll", "tokens": ["(", "en\u00b7gel", "von", "Blitz", "und", "Re\u00b7gen", "sind", "es", ")", "toll"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "NE", "APPR", "NN", "KON", "NN", "VAFIN", "PPER", "$(", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Wie sinkend Laub zur Erde sch\u00fcttelt: \u2013 gleich", "tokens": ["Wie", "sin\u00b7kend", "Laub", "zur", "Er\u00b7de", "sch\u00fct\u00b7telt", ":", "\u2013", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PWAV", "ADJD", "NN", "APPRART", "NN", "VVFIN", "$.", "$(", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem schwarzen Haare, das man flattern sieht", "tokens": ["Dem", "schwar\u00b7zen", "Haa\u00b7re", ",", "das", "man", "flat\u00b7tern", "sieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um ein M\u00e4nadenhaupt, ist wild und reich,", "tokens": ["Um", "ein", "M\u00e4\u00b7na\u00b7den\u00b7haupt", ",", "ist", "wild", "und", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Vom Saum des Horizonts bis zum Zenith", "tokens": ["Vom", "Saum", "des", "Ho\u00b7ri\u00b7zonts", "bis", "zum", "Ze\u00b7nith"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf deinem Azurfeld die Lokenpracht", "tokens": ["Auf", "dei\u00b7nem", "A\u00b7zur\u00b7feld", "die", "Lo\u00b7ken\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des nahnden Sturms verstreut! Du Klagelied", "tokens": ["Des", "nahn\u00b7den", "Sturms", "ver\u00b7streut", "!", "Du", "Kla\u00b7ge\u00b7lied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Des sterbenden Jahres, welchem diese Nacht", "tokens": ["Des", "ster\u00b7ben\u00b7den", "Jah\u00b7res", ",", "wel\u00b7chem", "die\u00b7se", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PDAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als Kuppel eines weiten Grabes sich", "tokens": ["Als", "Kup\u00b7pel", "ei\u00b7nes", "wei\u00b7ten", "Gra\u00b7bes", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gew\u00f6lbt mit all der aufgeth\u00fcrmten Macht", "tokens": ["Ge\u00b7w\u00f6lbt", "mit", "all", "der", "auf\u00b7ge\u00b7th\u00fcrm\u00b7ten", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Von Dampf und Dunst, die bald sich pr\u00e4chtiglich", "tokens": ["Von", "Dampf", "und", "Dunst", ",", "die", "bald", "sich", "pr\u00e4ch\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "ADV", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als Regen, Blitz entladen: \u2013 h\u00f6re mich!", "tokens": ["Als", "Re\u00b7gen", ",", "Blitz", "ent\u00b7la\u00b7den", ":", "\u2013", "h\u00f6\u00b7re", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "VVPP", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Du, der geweckt aus seinem Sommertraum", "tokens": ["Du", ",", "der", "ge\u00b7weckt", "aus", "sei\u00b7nem", "Som\u00b7mer\u00b7traum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das blaue Mittelmeer, das schlummernd lag,", "tokens": ["Das", "blau\u00b7e", "Mit\u00b7tel\u00b7meer", ",", "das", "schlum\u00b7mernd", "lag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gewiegt an einer Bimsstein-Insel Schaum", "tokens": ["Ge\u00b7wiegt", "an", "ei\u00b7ner", "Bims\u00b7stein\u00b7In\u00b7sel", "Schaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "In Baj\u00e4's Bucht von sanftem Wellenschlag,", "tokens": ["In", "Ba\u00b7j\u00e4's", "Bucht", "von", "sanf\u00b7tem", "Wel\u00b7len\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und tief im Schlaf die Wunderstadt gesehn,", "tokens": ["Und", "tief", "im", "Schlaf", "die", "Wun\u00b7der\u00b7stadt", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ergl\u00e4nzend in der Fluth kristallnem Tag,", "tokens": ["Er\u00b7gl\u00e4n\u00b7zend", "in", "der", "Fluth", "kris\u00b7tall\u00b7nem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Wo blaues Moos und helle Blumen stehn,", "tokens": ["Wo", "blau\u00b7es", "Moos", "und", "hel\u00b7le", "Blu\u00b7men", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So sch\u00f6n, wie nimmer sie ein Dichter schuf!", "tokens": ["So", "sch\u00f6n", ",", "wie", "nim\u00b7mer", "sie", "ein", "Dich\u00b7ter", "schuf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du, dem im Zorne selbst entfesselt gehn", "tokens": ["Du", ",", "dem", "im", "Zor\u00b7ne", "selbst", "ent\u00b7fes\u00b7selt", "gehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Des Weltmeers Wogen, wenn sie trat dein Huf,", "tokens": ["Des", "Welt\u00b7meers", "Wo\u00b7gen", ",", "wenn", "sie", "trat", "dein", "Huf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Inde\u00df der schlammige Wald, der saftlos sich", "tokens": ["In\u00b7de\u00df", "der", "schlam\u00b7mi\u00b7ge", "Wald", ",", "der", "saft\u00b7los", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Blatt am Grunde fristet, deinen Ruf", "tokens": ["Das", "Blatt", "am", "Grun\u00b7de", "fris\u00b7tet", ",", "dei\u00b7nen", "Ruf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Vernahm, da\u00df falb sein gr\u00fcnes Haar erblich", "tokens": ["Ver\u00b7nahm", ",", "da\u00df", "falb", "sein", "gr\u00fc\u00b7nes", "Haar", "er\u00b7blich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und er sich bebend neigte: \u2013 h\u00f6re mich!", "tokens": ["Und", "er", "sich", "be\u00b7bend", "neig\u00b7te", ":", "\u2013", "h\u00f6\u00b7re", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "ADJD", "VVFIN", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.16": {"line.1": {"text": "W\u00e4r' ich ein todtes Blatt, von dir entf\u00fchrt,", "tokens": ["W\u00e4r'", "ich", "ein", "tod\u00b7tes", "Blatt", ",", "von", "dir", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4r' eine Wolke, ziehnd auf deiner Spur,", "tokens": ["W\u00e4r'", "ei\u00b7ne", "Wol\u00b7ke", ",", "ziehnd", "auf", "dei\u00b7ner", "Spur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4r' eine Welle, die den Odem sp\u00fcrt", "tokens": ["W\u00e4r'", "ei\u00b7ne", "Wel\u00b7le", ",", "die", "den", "O\u00b7dem", "sp\u00fcrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Von deiner Kraft, und selbst sie theilte, nur", "tokens": ["Von", "dei\u00b7ner", "Kraft", ",", "und", "selbst", "sie", "theil\u00b7te", ",", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "ADV", "PPER", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So frei nicht, St\u00fcrmender, wie du! Ja, schritt'", "tokens": ["So", "frei", "nicht", ",", "St\u00fcr\u00b7men\u00b7der", ",", "wie", "du", "!", "Ja", ",", "schritt'"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADJD", "PTKNEG", "$,", "NN", "$,", "PWAV", "PPER", "$.", "PTKANT", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, 