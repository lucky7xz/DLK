{"textgrid.poem.43212": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Rosen duften, die Luft weht lind,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rosen duften, die Luft weht lind,", "tokens": ["Die", "Ro\u00b7sen", "duf\u00b7ten", ",", "die", "Luft", "weht", "lind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Flieder am Fenster rauscht,", "tokens": ["Der", "Flie\u00b7der", "am", "Fens\u00b7ter", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Flechten gl\u00e4ttet das junge Kind", "tokens": ["Die", "Flech\u00b7ten", "gl\u00e4t\u00b7tet", "das", "jun\u00b7ge", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und summet, kichert und lauscht.", "tokens": ["Und", "sum\u00b7met", ",", "ki\u00b7chert", "und", "lauscht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sie lauschet hinab zum gr\u00fcnen See", "tokens": ["Sie", "lau\u00b7schet", "hin\u00b7ab", "zum", "gr\u00fc\u00b7nen", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und l\u00e4chelt in's Mondenlicht,", "tokens": ["Und", "l\u00e4\u00b7chelt", "in's", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So keusch wie der weiche Bl\u00fcthenschnee", "tokens": ["So", "keusch", "wie", "der", "wei\u00b7che", "Bl\u00fc\u00b7then\u00b7schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist auch ihr liebes Gesicht.", "tokens": ["Ist", "auch", "ihr", "lie\u00b7bes", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und leise wie in der Sommernacht", "tokens": ["Und", "lei\u00b7se", "wie", "in", "der", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Thau von den Bl\u00e4ttern tropft,", "tokens": ["Der", "Thau", "von", "den", "Bl\u00e4t\u00b7tern", "tropft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie die Lerchen zwitschern schlummersacht,", "tokens": ["Wie", "die", "Ler\u00b7chen", "zwit\u00b7schern", "schlum\u00b7mer\u00b7sacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "So leise das Herz ihr klopft.", "tokens": ["So", "lei\u00b7se", "das", "Herz", "ihr", "klopft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie schlie\u00dft das Fenster und l\u00f6scht das Licht,", "tokens": ["Sie", "schlie\u00dft", "das", "Fens\u00b7ter", "und", "l\u00f6scht", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sinkt vor dem Bett in die Knie,", "tokens": ["Sinkt", "vor", "dem", "Bett", "in", "die", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ihr l\u00e4chelndes rothes M\u00fcndlein spricht:", "tokens": ["Ihr", "l\u00e4\u00b7cheln\u00b7des", "ro\u00b7thes", "M\u00fcnd\u00b7lein", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbgegr\u00fc\u00dfet seist Du, ", "tokens": ["\u00bb", "ge\u00b7gr\u00fc\u00b7\u00dfet", "seist", "Du", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}