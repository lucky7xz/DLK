{"textgrid.poem.46939": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es stieg ein tr\u00fcber Nebelwind vom Rheine,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stieg ein tr\u00fcber Nebelwind vom Rheine,", "tokens": ["Es", "stieg", "ein", "tr\u00fc\u00b7ber", "Ne\u00b7bel\u00b7wind", "vom", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf dessen Fitt'chen kam herangeflogen", "tokens": ["Auf", "des\u00b7sen", "Fitt'\u00b7chen", "kam", "her\u00b7an\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Nachtgew\u00f6lk am deutschen Himmelsbogen,", "tokens": ["Ein", "Nacht\u00b7ge\u00b7w\u00f6lk", "am", "deut\u00b7schen", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Darob verfinstert wurden alle Haine.", "tokens": ["Da\u00b7rob", "ver\u00b7fins\u00b7tert", "wur\u00b7den", "al\u00b7le", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Freiheit, die im Maiensonnenscheine", "tokens": ["Die", "Frei\u00b7heit", ",", "die", "im", "Mai\u00b7en\u00b7son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lustwandeln ging an den krystallnen Wogen,", "tokens": ["Lust\u00b7wan\u00b7deln", "ging", "an", "den", "krys\u00b7tall\u00b7nen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "++-+---+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Sah's und erschrak und fl\u00fcchtete betrogen", "tokens": ["Sah's", "und", "er\u00b7schrak", "und", "fl\u00fcch\u00b7te\u00b7te", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "KON", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zur tiefsten Grotte, da\u00df sie einsam weine.", "tokens": ["Zur", "tiefs\u00b7ten", "Grot\u00b7te", ",", "da\u00df", "sie", "ein\u00b7sam", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nun hat ein starker Nordwind sich erhoben,", "tokens": ["Nun", "hat", "ein", "star\u00b7ker", "Nord\u00b7wind", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und hat mit scharfem Grimm das nebelgraue", "tokens": ["Und", "hat", "mit", "schar\u00b7fem", "Grimm", "das", "ne\u00b7bel\u00b7grau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NE", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gew\u00f6lk zur\u00fcck vom Horizont geschnoben.", "tokens": ["Ge\u00b7w\u00f6lk", "zu\u00b7r\u00fcck", "vom", "Ho\u00b7ri\u00b7zont", "ge\u00b7schno\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nun auf, o Freiheit, deutsche Jungfrau, schaue", "tokens": ["Nun", "auf", ",", "o", "Frei\u00b7heit", ",", "deut\u00b7sche", "Jung\u00b7frau", ",", "schau\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "PTKVZ", "$,", "FM", "NN", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Getrost du wieder, wie vordem, nach oben,", "tokens": ["Ge\u00b7trost", "du", "wie\u00b7der", ",", "wie", "vor\u00b7dem", ",", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PWAV", "ADV", "$,", "APPR", "ADV", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Aus blauem Aug' empor zum Himmelsblaue.", "tokens": ["Aus", "blau\u00b7em", "Aug'", "em\u00b7por", "zum", "Him\u00b7mels\u00b7blau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}