{"textgrid.poem.56309": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Invalid im Irrenhaus", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leipzig, Leipzig! arger Boden,", "tokens": ["Leip\u00b7zig", ",", "Leip\u00b7zig", "!", "ar\u00b7ger", "Bo\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmach f\u00fcr Unbill schafftest du.", "tokens": ["Schmach", "f\u00fcr", "Un\u00b7bill", "schaff\u00b7test", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freiheit! hie\u00df es, vorw\u00e4rts, vorw\u00e4rts!", "tokens": ["Frei\u00b7heit", "!", "hie\u00df", "es", ",", "vor\u00b7w\u00e4rts", ",", "vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "ADV", "$,", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trankst mein rotes Blut, wozu?", "tokens": ["Trankst", "mein", "ro\u00b7tes", "Blut", ",", "wo\u00b7zu", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freiheit! rief ich, vorw\u00e4rts, vorw\u00e4rts!", "tokens": ["Frei\u00b7heit", "!", "rief", "ich", ",", "vor\u00b7w\u00e4rts", ",", "vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "ADV", "$,", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ein Tor nicht alles glaubt!", "tokens": ["Was", "ein", "Tor", "nicht", "al\u00b7les", "glaubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von schwerem S\u00e4belstreiche", "tokens": ["Und", "von", "schwe\u00b7rem", "S\u00e4\u00b7bel\u00b7strei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward gespalten mir das Haupt.", "tokens": ["Ward", "ge\u00b7spal\u00b7ten", "mir", "das", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich lag, und abw\u00e4rts w\u00e4lzte", "tokens": ["Und", "ich", "lag", ",", "und", "ab\u00b7w\u00e4rts", "w\u00e4lz\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unheilschwanger sich die Schlacht,", "tokens": ["Un\u00b7heil\u00b7schwan\u00b7ger", "sich", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber mich und \u00fcber Leichen", "tokens": ["\u00dc\u00b7ber", "mich", "und", "\u00fc\u00b7ber", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sank die kalte, finstre Nacht.", "tokens": ["Sank", "die", "kal\u00b7te", ",", "finst\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aufgewacht zu grausen Schmerzen,", "tokens": ["Auf\u00b7ge\u00b7wacht", "zu", "grau\u00b7sen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brennt die Wunde mehr und mehr;", "tokens": ["Brennt", "die", "Wun\u00b7de", "mehr", "und", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich liege hier gebunden,", "tokens": ["Und", "ich", "lie\u00b7ge", "hier", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grimm'ge W\u00e4chter um mich her.", "tokens": ["Grim\u00b7m'\u00b7ge", "W\u00e4ch\u00b7ter", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Schrei ich w\u00fctend noch nach Freiheit,", "tokens": ["Schrei", "ich", "w\u00fc\u00b7tend", "noch", "nach", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem bluterkauften Gl\u00fcck,", "tokens": ["Nach", "dem", "blu\u00b7ter\u00b7kauf\u00b7ten", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Peitscht der W\u00e4chter mit der Peitsche", "tokens": ["Peitscht", "der", "W\u00e4ch\u00b7ter", "mit", "der", "Peit\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich in schn\u00f6de Ruh zur\u00fcck.", "tokens": ["Mich", "in", "schn\u00f6\u00b7de", "Ruh", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}