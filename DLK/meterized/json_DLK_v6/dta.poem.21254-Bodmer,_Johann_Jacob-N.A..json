{"dta.poem.21254": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198486", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein auserwehltes Licht! Acht Tage sind nur hin,", "tokens": ["Mein", "au\u00b7ser\u00b7wehl\u00b7tes", "Licht", "!", "Acht", "Ta\u00b7ge", "sind", "nur", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "CARD", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So werd ich laut und frey den theuren Eyd beschweren,", "tokens": ["So", "werd", "ich", "laut", "und", "frey", "den", "theu\u00b7ren", "Eyd", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich dir ewig treu und ganz dein eigen bin.", "tokens": ["Da\u00df", "ich", "dir", "e\u00b7wig", "treu", "und", "ganz", "dein", "ei\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ADJD", "KON", "ADV", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich freue mich darauf! ", "tokens": ["Ich", "freu\u00b7e", "mich", "da\u00b7rauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}