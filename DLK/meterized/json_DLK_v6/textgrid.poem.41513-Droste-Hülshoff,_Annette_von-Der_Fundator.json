nicht", "der", "Turm", "wie", "'ne", "La\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verhauchend, dunstig, aus der Ferne!", "tokens": ["Ver\u00b7hau\u00b7chend", ",", "duns\u00b7tig", ",", "aus", "der", "Fer\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie steigt der blaue Duft im Rohr,", "tokens": ["Wie", "steigt", "der", "blau\u00b7e", "Duft", "im", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und rollt sich am Gesims empor!", "tokens": ["Und", "rollt", "sich", "am", "Ge\u00b7sims", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie seltsam blinken heut die Sterne!", "tokens": ["Wie", "selt\u00b7sam", "blin\u00b7ken", "heut", "die", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch ha! \u2013 er blinzt, er spannt das Aug',", "tokens": ["Doch", "ha", "!", "\u2013", "er", "blinzt", ",", "er", "spannt", "das", "Aug'", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "me