{"dta.poem.17247": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "97.  Auf beiden seyn ist gut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Den Him\u0304el w\u00fcntsch\u2019 ich mir/ Lieb\u2019 aber auch die Erden:", "tokens": ["Den", "Him\u0304el", "w\u00fcnt\u00b7sch'", "ich", "mir", "/", "Lieb'", "a\u00b7ber", "auch", "die", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$(", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Denn auf derselbigen kan ich GOtt n\u00e4her werden.", "tokens": ["Denn", "auf", "der\u00b7sel\u00b7bi\u00b7gen", "kan", "ich", "Gott", "n\u00e4\u00b7her", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "VMFIN", "PPER", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}