{"dta.poem.18585": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "B ey einer  G arten- L ust", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Nehmet ihr getreuen Winde", "tokens": ["Neh\u00b7met", "ihr", "ge\u00b7treu\u00b7en", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine stillen Seufftzer hinn/", "tokens": ["Mei\u00b7ne", "stil\u00b7len", "Seufft\u00b7zer", "hinn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringet sie dem Engels Kinde", "tokens": ["Brin\u00b7get", "sie", "dem", "En\u00b7gels", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem ich recht gewogen bin.", "tokens": ["Dem", "ich", "recht", "ge\u00b7wo\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Saget/ diese Bothen wissen/", "tokens": ["Sa\u00b7get", "/", "die\u00b7se", "Bo\u00b7then", "wis\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PDAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich ihm verschweigen m\u00fcssen.", "tokens": ["Was", "ich", "ihm", "ver\u00b7schwei\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schreibet doch ihr Lorberbletter/", "tokens": ["Schrei\u00b7bet", "doch", "ihr", "Lor\u00b7berb\u00b7let\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich ihm nicht schreiben kan.", "tokens": ["Was", "ich", "ihm", "nicht", "schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das kein Liebes-Sturm noch Wetter", "tokens": ["Das", "kein", "Lie\u00b7bes\u00b7Sturm", "noch", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Treue was gethan:", "tokens": ["Mei\u00b7ner", "Treu\u00b7e", "was", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PWS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn er nun vorbey will gehen/", "tokens": ["Wenn", "er", "nun", "vor\u00b7bey", "will", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Last ihn sein vergn\u00fcgen sehen.", "tokens": ["Last", "ihn", "sein", "ver\u00b7gn\u00fc\u00b7gen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Darff ich ihn nicht selber K\u00fcssen/", "tokens": ["Darff", "ich", "ihn", "nicht", "sel\u00b7ber", "K\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ichs euch ihr Blumen thun.", "tokens": ["Will", "ichs", "euch", "ihr", "Blu\u00b7men", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er euch nun will begr\u00fcssen/", "tokens": ["Wenn", "er", "euch", "nun", "will", "be\u00b7gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auf euren Bette ruhn!", "tokens": ["Und", "auf", "eu\u00b7ren", "Bet\u00b7te", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach so k\u00fcsset mein Verlangen", "tokens": ["Ach", "so", "k\u00fcs\u00b7set", "mein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tausend mahl auf seine Wangen.", "tokens": ["Tau\u00b7send", "mahl", "auf", "sei\u00b7ne", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun ihr Winde/ Bl\u00e4tter/ Rosen", "tokens": ["Nun", "ihr", "Win\u00b7de", "/", "Bl\u00e4t\u00b7ter", "/", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "PPOSAT", "NN", "$(", "NN", "$(", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprecht zu meiner Sehnsucht ja.", "tokens": ["Sprecht", "zu", "mei\u00b7ner", "Sehn\u00b7sucht", "ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4umet nicht ihn lieb zu kosen/", "tokens": ["S\u00e4u\u00b7met", "nicht", "ihn", "lieb", "zu", "ko\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn er ist schon selber da.", "tokens": ["Denn", "er", "ist", "schon", "sel\u00b7ber", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch di\u00df sey ihm vorgeschrieben/", "tokens": ["Doch", "di\u00df", "sey", "ihm", "vor\u00b7ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Itzo schweigen/ und doch lieben.", "tokens": ["It\u00b7zo", "schwei\u00b7gen", "/", "und", "doch", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$(", "KON", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}