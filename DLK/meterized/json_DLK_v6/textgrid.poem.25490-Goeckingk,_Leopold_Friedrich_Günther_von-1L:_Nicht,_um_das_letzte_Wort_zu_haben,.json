{"textgrid.poem.25490": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht, um das letzte Wort zu haben,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht, um das letzte Wort zu haben,", "tokens": ["Nicht", ",", "um", "das", "letz\u00b7te", "Wort", "zu", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUI", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "(rechthaberei ist mir ein Greul!", "tokens": ["(", "recht\u00b7ha\u00b7be\u00b7rei", "ist", "mir", "ein", "Greul", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Auch ist der Zwist ja l\u00e4ngst begraben,", "tokens": ["Auch", "ist", "der", "Zwist", "ja", "l\u00e4ngst", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Wunde l\u00e4ngstens heil!)", "tokens": ["Und", "mei\u00b7ne", "Wun\u00b7de", "l\u00e4ngs\u00b7tens", "heil", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Blo\u00df unsrer Freundin", "tokens": ["Blo\u00df", "uns\u00b7rer", "Freun\u00b7din"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die sehnlich w\u00fcnscht, ich m\u00f6chte allen,", "tokens": ["Die", "sehn\u00b7lich", "w\u00fcnscht", ",", "ich", "m\u00f6ch\u00b7te", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "PPER", "VMFIN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vorz\u00fcglich einem Mann' wie dir,", "tokens": ["Vor\u00b7z\u00fcg\u00b7lich", "ei\u00b7nem", "Mann'", "wie", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So rein erscheinen, als wie ihr,", "tokens": ["So", "rein", "er\u00b7schei\u00b7nen", ",", "als", "wie", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,", "KOUS", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mu\u00df ich noch ein Paar Verse wagen,", "tokens": ["Mu\u00df", "ich", "noch", "ein", "Paar", "Ver\u00b7se", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Wird in den kalten Wintertagen", "tokens": ["Wird", "in", "den", "kal\u00b7ten", "Win\u00b7ter\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Des Lebens, gleich es schwerer mir,", "tokens": ["Des", "Le\u00b7bens", ",", "gleich", "es", "schwe\u00b7rer", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nach Reimen, wie nach Wild, zu jagen.", "tokens": ["Nach", "Rei\u00b7men", ",", "wie", "nach", "Wild", ",", "zu", "ja\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "APPR", "NE", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wer in der Denkart stimmt, wie wir,", "tokens": ["Wer", "in", "der", "Den\u00b7kart", "stimmt", ",", "wie", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der wird sich leicht im Streit' vertragen.", "tokens": ["Der", "wird", "sich", "leicht", "im", "Streit'", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Doch unser Beispiel, wie mich deucht,", "tokens": ["Doch", "un\u00b7ser", "Bei\u00b7spiel", ",", "wie", "mich", "deucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Kann J\u00fcnglingen die nach uns kommen,", "tokens": ["Kann", "J\u00fcng\u00b7lin\u00b7gen", "die", "nach", "uns", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "APPR", "PPER", "VVINF", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Wenn l\u00e4ngst wir nicht mehr sind, vielleicht", "tokens": ["Wenn", "l\u00e4ngst", "wir", "nicht", "mehr", "sind", ",", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "PPER", "PTKNEG", "ADV", "VAFIN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mehr noch als unsre Schriften, frommen.", "tokens": ["Mehr", "noch", "als", "uns\u00b7re", "Schrif\u00b7ten", ",", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "PPOSAT", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "F\u00fcr dich, den Anwald keuscher Musen", "tokens": ["F\u00fcr", "dich", ",", "den", "An\u00b7wald", "keu\u00b7scher", "Mu\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und weiser Unabh\u00e4ngigkeit,", "tokens": ["Und", "wei\u00b7ser", "Un\u00b7ab\u00b7h\u00e4n\u00b7gig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Gl\u00fcht ein Gef\u00fchl in meinem Busen,", "tokens": ["Gl\u00fcht", "ein", "Ge\u00b7f\u00fchl", "in", "mei\u00b7nem", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Das sicher keinen Zwist erneut;", "tokens": ["Das", "si\u00b7cher", "kei\u00b7nen", "Zwist", "er\u00b7neut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und wer ist mehr als ich bereit,", "tokens": ["Und", "wer", "ist", "mehr", "als", "ich", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADV", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Dir alles, alles einzur\u00e4umen?", "tokens": ["Dir", "al\u00b7les", ",", "al\u00b7les", "ein\u00b7zu\u00b7r\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Befolgt' ich selbst nicht, was du lehrst?", "tokens": ["Be\u00b7folgt'", "ich", "selbst", "nicht", ",", "was", "du", "lehrst", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Nur da\u00df du mich mit meinen Reimen", "tokens": ["Nur", "da\u00df", "du", "mich", "mit", "mei\u00b7nen", "Rei\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Zu hoch stellst, neben ", "tokens": ["Zu", "hoch", "stellst", ",", "ne\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PTKA", "ADJD", "VVFIN", "$,", "APPR"], "meter": "-++--", "measure": "unknown.measure.di"}, "line.28": {"text": "Zu g\u00fctig meine Lieder ehrst:", "tokens": ["Zu", "g\u00fc\u00b7tig", "mei\u00b7ne", "Lie\u00b7der", "ehrst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die\u00df lie\u00df ich wahrlich mir nicht tr\u00e4umen!", "tokens": ["Die\u00df", "lie\u00df", "ich", "wahr\u00b7lich", "mir", "nicht", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "H\u00e4tt' ", "tokens": ["H\u00e4tt'"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Kein F\u00fcrst in ganz Europa h\u00e4tte,", "tokens": ["Kein", "F\u00fcrst", "in", "ganz", "Eu\u00b7ro\u00b7pa", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADV", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Selbst mit des goldnen Vlie\u00dfes Kette,", "tokens": ["Selbst", "mit", "des", "gold\u00b7nen", "Vlie\u00b7\u00dfes", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die Leyer aus der Hand gebracht.", "tokens": ["Die", "Le\u00b7yer", "aus", "der", "Hand", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.34": {"text": "Doch ohn' es nur im Traum' zu w\u00e4hnen,", "tokens": ["Doch", "ohn'", "es", "nur", "im", "Traum'", "zu", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Tauscht' ich sie dennoch unter Thr\u00e4nen", "tokens": ["Tauscht'", "ich", "sie", "den\u00b7noch", "un\u00b7ter", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Mit einem Aktenb\u00fcndel aus.", "tokens": ["Mit", "ei\u00b7nem", "Ak\u00b7ten\u00b7b\u00fcn\u00b7del", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Des Beifalls lockte zwar sie wenig", "tokens": ["Des", "Bei\u00b7falls", "lock\u00b7te", "zwar", "sie", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Von andern, glaubt' ich, nur heraus,", "tokens": ["Von", "an\u00b7dern", ",", "glaubt'", "ich", ",", "nur", "he\u00b7raus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "VVFIN", "PPER", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Doch war, ich selbst, durch sie zu Haus", "tokens": ["Doch", "war", ",", "ich", "selbst", ",", "durch", "sie", "zu", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "$,", "PPER", "ADV", "$,", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Weit gl\u00fccklicher, als selbst der K\u00f6nig.", "tokens": ["Weit", "gl\u00fcck\u00b7li\u00b7cher", ",", "als", "selbst", "der", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Was half's, sie noch mit Thr\u00e4nen netzen?", "tokens": ["Was", "ha\u00b7lf's", ",", "sie", "noch", "mit", "Thr\u00e4\u00b7nen", "net\u00b7zen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Es mu\u00dft' einmal geschieden seyn;", "tokens": ["Es", "mu\u00dft'", "ein\u00b7mal", "ge\u00b7schie\u00b7den", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "So folgt' ich denn des Zwangs Gesetzen.", "tokens": ["So", "folgt'", "ich", "denn", "des", "Zwangs", "Ge\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Mit Ruh' und Leyer, meinen Sch\u00e4tzen,", "tokens": ["Mit", "Ruh'", "und", "Le\u00b7yer", ",", "mei\u00b7nen", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Lief ach! das Gl\u00fcck nun querfeldein.", "tokens": ["Lief", "ach", "!", "das", "Gl\u00fcck", "nun", "quer\u00b7fel\u00b7dein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "XY", "$.", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Umsonst ist's, dacht' ich, nachzusetzen,", "tokens": ["Um\u00b7sonst", "ist's", ",", "dacht'", "ich", ",", "nach\u00b7zu\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "VVFIN", "PPER", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Du holst es doch nicht wieder ein.", "tokens": ["Du", "holst", "es", "doch", "nicht", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Ich sah ein Heer von Sorgen kommen,", "tokens": ["Ich", "sah", "ein", "Heer", "von", "Sor\u00b7gen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Das schob sich zwischen mich und sie.", "tokens": ["Das", "schob", "sich", "zwi\u00b7schen", "mich", "und", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Nun! frommt die Zukunft mir auch nie,", "tokens": ["Nun", "!", "frommt", "die", "Zu\u00b7kunft", "mir", "auch", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ART", "NN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Durch dich wird sie doch andern frommen. \u2013", "tokens": ["Durch", "dich", "wird", "sie", "doch", "an\u00b7dern", "from\u00b7men", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ADJA", "ADJA", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Ein tr\u00f6stend Spiel der Phantasie!", "tokens": ["Ein", "tr\u00f6s\u00b7tend", "Spiel", "der", "Phan\u00b7ta\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Du selbst, o ", "tokens": ["Du", "selbst", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "FM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.54": {"text": "Zuletzt geopfert Kraft und Zeit;", "tokens": ["Zu\u00b7letzt", "ge\u00b7o\u00b7pfert", "Kraft", "und", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Doch rechn' ich dir es zum Verrathe", "tokens": ["Doch", "rechn'", "ich", "dir", "es", "zum", "Ver\u00b7ra\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "An deiner Unabh\u00e4ngigkeit?", "tokens": ["An", "dei\u00b7ner", "Un\u00b7ab\u00b7h\u00e4n\u00b7gig\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Was du geduldet und gewagt,", "tokens": ["Was", "du", "ge\u00b7dul\u00b7det", "und", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Mu\u00df dir ein solches Denkmal stiften,", "tokens": ["Mu\u00df", "dir", "ein", "sol\u00b7ches", "Denk\u00b7mal", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.59": {"text": "Das selbst noch \u00fcber deine Schriften,", "tokens": ["Das", "selbst", "noch", "\u00fc\u00b7ber", "dei\u00b7ne", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "(und das ist wahrlich viel 