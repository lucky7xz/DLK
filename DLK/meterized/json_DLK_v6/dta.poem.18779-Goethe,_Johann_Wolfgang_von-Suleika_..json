{"dta.poem.18779": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Suleika .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ja! von m\u00e4chtig holden Blicken,", "tokens": ["Ja", "!", "von", "m\u00e4ch\u00b7tig", "hol\u00b7den", "Bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von l\u00e4chlendem Entz\u00fccken", "tokens": ["Wie", "von", "l\u00e4ch\u00b7len\u00b7dem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von Z\u00e4hnen blendend klar.", "tokens": ["Und", "von", "Z\u00e4h\u00b7nen", "blen\u00b7dend", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Moschusduftend Lockenschlangen,", "tokens": ["Mo\u00b7schus\u00b7duf\u00b7tend", "Lo\u00b7cken\u00b7schlan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Augenwimpern reizumhangen,", "tokens": ["Au\u00b7gen\u00b7wim\u00b7pern", "rei\u00b7zum\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tausendf\u00e4ltige Gefahr!", "tokens": ["Tau\u00b7send\u00b7f\u00e4l\u00b7ti\u00b7ge", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denke nun wie von so langem", "tokens": ["Den\u00b7ke", "nun", "wie", "von", "so", "lan\u00b7gem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "KOKOM", "APPR", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Prophezeyt Suleika war.", "tokens": ["Pro\u00b7phe\u00b7zeyt", "Sul\u00b7ei\u00b7ka", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}