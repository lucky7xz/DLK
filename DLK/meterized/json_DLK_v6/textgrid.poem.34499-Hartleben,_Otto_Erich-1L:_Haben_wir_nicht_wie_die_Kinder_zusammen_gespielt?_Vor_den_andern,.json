{"textgrid.poem.34499": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Haben wir nicht wie die Kinder zusammen gespielt? Vor den andern,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Haben wir nicht wie die Kinder zusammen gespielt? Vor den andern,", "tokens": ["Ha\u00b7ben", "wir", "nicht", "wie", "die", "Kin\u00b7der", "zu\u00b7sam\u00b7men", "ge\u00b7spielt", "?", "Vor", "den", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "KOKOM", "ART", "NN", "ADV", "VVPP", "$.", "APPR", "ART", "ADJA", "$,"], "meter": "+--+--+--+--++-+-", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "ja, vor uns selber gethan, wie in einander verliebt?", "tokens": ["ja", ",", "vor", "uns", "sel\u00b7ber", "ge\u00b7than", ",", "wie", "in", "ein\u00b7an\u00b7der", "ver\u00b7liebt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "PPER", "ADV", "VVPP", "$,", "PWAV", "APPR", "PRF", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ach, und noch heute \u2013 gedenk ich des braunen, blau\u00e4ugigen K\u00f6pfchens,", "tokens": ["Ach", ",", "und", "noch", "heu\u00b7te", "\u2013", "ge\u00b7denk", "ich", "des", "brau\u00b7nen", ",", "blau\u00b7\u00e4u\u00b7gi\u00b7gen", "K\u00f6pf\u00b7chens", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "ADV", "$(", "VVIMP", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "---+--+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "fass ich es immer noch nicht, dass ich nicht wirklich verliebt.", "tokens": ["fass", "ich", "es", "im\u00b7mer", "noch", "nicht", ",", "dass", "ich", "nicht", "wirk\u00b7lich", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "---+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}