{"textgrid.poem.41296": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "La-Motte", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Houdart, den ich mir zum Muster nie erlesen,", "tokens": ["Der", "Hou\u00b7dart", ",", "den", "ich", "mir", "zum", "Mus\u00b7ter", "nie", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nicht so gro\u00df, auch nicht so klein gewesen,", "tokens": ["Ist", "nicht", "so", "gro\u00df", ",", "auch", "nicht", "so", "klein", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "PTKNEG", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Fontenell' und Rousseau ihn gemacht.", "tokens": ["Als", "Fon\u00b7te\u00b7nell'", "und", "Rous\u00b7se\u00b7au", "ihn", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Sein Tadel wird noch jetzt von vielen nachgeschrieben,", "tokens": ["Sein", "Ta\u00b7del", "wird", "noch", "jetzt", "von", "vie\u00b7len", "nach\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die blos die Kunst des Mitbejahens \u00fcben,", "tokens": ["Die", "blos", "die", "Kunst", "des", "Mit\u00b7be\u00b7ja\u00b7hens", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und lachen, wenn ein andrer lacht.", "tokens": ["Und", "la\u00b7chen", ",", "wenn", "ein", "an\u00b7drer", "lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was Houdart ist, hat Voltair' uns gezeiget:", "tokens": ["Was", "Hou\u00b7dart", "ist", ",", "hat", "Vol\u00b7tair'", "uns", "ge\u00b7zei\u00b7get", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VAFIN", "$,", "VAFIN", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr kleinen Unterrichter, schweiget.", "tokens": ["Ihr", "klei\u00b7nen", "Un\u00b7ter\u00b7rich\u00b7ter", ",", "schwei\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}