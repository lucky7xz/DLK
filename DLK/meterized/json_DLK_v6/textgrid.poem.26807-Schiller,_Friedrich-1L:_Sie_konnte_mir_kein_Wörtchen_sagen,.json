{"textgrid.poem.26807": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie konnte mir kein W\u00f6rtchen sagen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie konnte mir kein W\u00f6rtchen sagen,", "tokens": ["Sie", "konn\u00b7te", "mir", "kein", "W\u00f6rt\u00b7chen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu viele Lauscher waren wach,", "tokens": ["Zu", "vie\u00b7le", "Lau\u00b7scher", "wa\u00b7ren", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Blick nur durft ich sch\u00fcchtern fragen,", "tokens": ["Den", "Blick", "nur", "durft", "ich", "sch\u00fcch\u00b7tern", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wohl verstand ich, was er sprach.", "tokens": ["Und", "wohl", "ver\u00b7stand", "ich", ",", "was", "er", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Leis komm ich her in deine Stille,", "tokens": ["Leis", "komm", "ich", "her", "in", "dei\u00b7ne", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du sch\u00f6n belaubtes Buchenzelt,", "tokens": ["Du", "sch\u00f6n", "be\u00b7laub\u00b7tes", "Bu\u00b7chen\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verbirg in deiner gr\u00fcnen H\u00fclle", "tokens": ["Ver\u00b7birg", "in", "dei\u00b7ner", "gr\u00fc\u00b7nen", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Liebenden dem Aug der Welt.", "tokens": ["Die", "Lie\u00b7ben\u00b7den", "dem", "Aug", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von ferne mit verworrnem Sausen", "tokens": ["Von", "fer\u00b7ne", "mit", "ver\u00b7worr\u00b7nem", "Sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeitet der gesch\u00e4ftge Tag,", "tokens": ["Ar\u00b7bei\u00b7tet", "der", "ge\u00b7sch\u00e4ft\u00b7ge", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch der Stimmen hohles Brausen", "tokens": ["Und", "durch", "der", "Stim\u00b7men", "hoh\u00b7les", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erkenn ich schwerer H\u00e4mmer Schlag.", "tokens": ["Er\u00b7kenn", "ich", "schwe\u00b7rer", "H\u00e4m\u00b7mer", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sauer ringt die kargen Lose", "tokens": ["So", "sau\u00b7er", "ringt", "die", "kar\u00b7gen", "Lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Mensch dem harten Himmel ab,", "tokens": ["Der", "Mensch", "dem", "har\u00b7ten", "Him\u00b7mel", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch leicht erworben, aus dem Scho\u00dfe", "tokens": ["Doch", "leicht", "er\u00b7wor\u00b7ben", ",", "aus", "dem", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der G\u00f6tter f\u00e4llt das Gl\u00fcck herab.", "tokens": ["Der", "G\u00f6t\u00b7ter", "f\u00e4llt", "das", "Gl\u00fcck", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df ja die Menschen nie es h\u00f6ren,", "tokens": ["Da\u00df", "ja", "die", "Men\u00b7schen", "nie", "es", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie treue Lieb uns still begl\u00fcckt!", "tokens": ["Wie", "treu\u00b7e", "Lieb", "uns", "still", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie k\u00f6nnen nur die Freude st\u00f6ren,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nur", "die", "Freu\u00b7de", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil Freude nie sie selbst entz\u00fcckt.", "tokens": ["Weil", "Freu\u00b7de", "nie", "sie", "selbst", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Welt wird nie das Gl\u00fcck erlauben,", "tokens": ["Die", "Welt", "wird", "nie", "das", "Gl\u00fcck", "er\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Beute wird es nur gehascht,", "tokens": ["Als", "Beu\u00b7te", "wird", "es", "nur", "ge\u00b7hascht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Entwenden mu\u00dft dus oder rauben,", "tokens": ["Ent\u00b7wen\u00b7den", "mu\u00dft", "dus", "o\u00b7der", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NE", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Eh dich die Mi\u00dfgunst \u00fcberrascht.", "tokens": ["Eh", "dich", "die", "Mi\u00df\u00b7gunst", "\u00fc\u00b7berr\u00b7ascht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Leis auf den Zehen kommts geschlichen,", "tokens": ["Leis", "auf", "den", "Ze\u00b7hen", "kommts", "ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stille liebt es und die Nacht,", "tokens": ["Die", "Stil\u00b7le", "liebt", "es", "und", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit schnellen F\u00fc\u00dfen ists entwichen,", "tokens": ["Mit", "schnel\u00b7len", "F\u00fc\u00b7\u00dfen", "ists", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo des Verr\u00e4ters Auge wacht.", "tokens": ["Wo", "des", "Ver\u00b7r\u00e4\u00b7ters", "Au\u00b7ge", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O schlinge dich, du sanfte Quelle,", "tokens": ["O", "schlin\u00b7ge", "dich", ",", "du", "sanf\u00b7te", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein breiter Strom um uns herum,", "tokens": ["Ein", "brei\u00b7ter", "Strom", "um", "uns", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und drohend mit emp\u00f6rter Welle", "tokens": ["Und", "dro\u00b7hend", "mit", "em\u00b7p\u00f6r\u00b7ter", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verteidige dies Heiligtum!", "tokens": ["Ver\u00b7tei\u00b7di\u00b7ge", "dies", "Hei\u00b7lig\u00b7tum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}