{"dta.poem.4091": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Mein!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "R\u00e4der, stellt eu'r Brausen ein!", "tokens": ["R\u00e4\u00b7der", ",", "stellt", "eu'r", "Brau\u00b7sen", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "All ihr muntern Waldv\u00f6gelein,", "tokens": ["All", "ihr", "mun\u00b7tern", "Wald\u00b7v\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gro\u00df und klein,", "tokens": ["Gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Endet eure Melodein!", "tokens": ["En\u00b7det", "eu\u00b7re", "Me\u00b7lo\u00b7dein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch den Hain", "tokens": ["Durch", "den", "Hain"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Aus und ein", "tokens": ["Aus", "und", "ein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "KON", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Schalle heut' ", "tokens": ["Schal\u00b7le", "heut'"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Die geliebte M\u00fcllerin ist ", "tokens": ["Die", "ge\u00b7lieb\u00b7te", "M\u00fcl\u00b7le\u00b7rin", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.9": {"text": "Fr\u00fchling, sind das alle deine Bl\u00fcmelein?", "tokens": ["Fr\u00fch\u00b7ling", ",", "sind", "das", "al\u00b7le", "dei\u00b7ne", "Bl\u00fc\u00b7me\u00b7lein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ART", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Sonne, hast du keinen hellern Schein?", "tokens": ["Son\u00b7ne", ",", "hast", "du", "kei\u00b7nen", "hel\u00b7lern", "Schein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Ach, so mu\u00df ich ganz allein,", "tokens": ["Ach", ",", "so", "mu\u00df", "ich", "ganz", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Mit dem seligen Worte ", "tokens": ["Mit", "dem", "se\u00b7li\u00b7gen", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Unverstanden in der weiten Sch\u00f6pfung sein!", "tokens": ["Un\u00b7ver\u00b7stan\u00b7den", "in", "der", "wei\u00b7ten", "Sch\u00f6p\u00b7fung", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}