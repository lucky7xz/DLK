{"textgrid.poem.32744": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Lied einer Kostg\u00e4ngerin, an eine Nonne", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wurdest Mutter mir, als ich", "tokens": ["Du", "wur\u00b7dest", "Mut\u00b7ter", "mir", ",", "als", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Teure sah erblassen;", "tokens": ["Die", "Teu\u00b7re", "sah", "er\u00b7blas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun soll ich, ach, auf ewig dich,", "tokens": ["Nun", "soll", "ich", ",", "ach", ",", "auf", "e\u00b7wig", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ITJ", "$,", "APPR", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und diesen Ort verlassen!", "tokens": ["Und", "die\u00b7sen", "Ort", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aus einer Welt, wo Trug und Tand", "tokens": ["Aus", "ei\u00b7ner", "Welt", ",", "wo", "Trug", "und", "Tand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein wildes Reich verbreitet,", "tokens": ["Sein", "wil\u00b7des", "Reich", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ward ich, an eines Engels Hand,", "tokens": ["Ward", "ich", ",", "an", "ei\u00b7nes", "En\u00b7gels", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dir hieher geleitet.", "tokens": ["Zu", "dir", "hie\u00b7her", "ge\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da lehrtest du mein Herz allein", "tokens": ["Da", "lehr\u00b7test", "du", "mein", "Herz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Jesu Liebe trachten,", "tokens": ["Nach", "Je\u00b7su", "Lie\u00b7be", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und aller Erde goldnen Schein", "tokens": ["Und", "al\u00b7ler", "Er\u00b7de", "gold\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr eiteln Flitter achten.", "tokens": ["F\u00fcr", "ei\u00b7teln", "Flit\u00b7ter", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O, wie so oft die Seele mir", "tokens": ["O", ",", "wie", "so", "oft", "die", "See\u00b7le", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADV", "ADV", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In hei\u00dfer Flamme gl\u00fchte,", "tokens": ["In", "hei\u00b7\u00dfer", "Flam\u00b7me", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich in stiller Nacht mit dir", "tokens": ["Wenn", "ich", "in", "stil\u00b7ler", "Nacht", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Seinem Kreuze kniete!", "tokens": ["Vor", "Sei\u00b7nem", "Kreu\u00b7ze", "knie\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Von Lieb' und Inbrunst angef\u00fcllt,", "tokens": ["Von", "Lieb'", "und", "In\u00b7brunst", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vernahm ich deine Lehren,", "tokens": ["Ver\u00b7nahm", "ich", "dei\u00b7ne", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sah sich Seiner Mutter Bild", "tokens": ["Und", "sah", "sich", "Sei\u00b7ner", "Mut\u00b7ter", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im deinigen verkl\u00e4ren.", "tokens": ["Im", "dei\u00b7ni\u00b7gen", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es kam zu mir im Traum, es glich", "tokens": ["Es", "kam", "zu", "mir", "im", "Traum", ",", "es", "glich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Milde deinen Mienen;", "tokens": ["An", "Mil\u00b7de", "dei\u00b7nen", "Mie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wies auf ein Kreuz, und warnte mich,", "tokens": ["Wies", "auf", "ein", "Kreuz", ",", "und", "warn\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mehr der Welt zu dienen!", "tokens": ["Nicht", "mehr", "der", "Welt", "zu", "die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und, ach! ich soll die Warnung nicht", "tokens": ["Und", ",", "ach", "!", "ich", "soll", "die", "War\u00b7nung", "nicht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "PPER", "VMFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hochgelobten h\u00f6ren;", "tokens": ["Der", "Hoch\u00b7ge\u00b7lob\u00b7ten", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Soll, wider meinen Gott und Pflicht,", "tokens": ["Soll", ",", "wi\u00b7der", "mei\u00b7nen", "Gott", "und", "Pflicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dieser Zelle kehren!", "tokens": ["Aus", "die\u00b7ser", "Zel\u00b7le", "keh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Soll in die Welt, auf deren Pfad", "tokens": ["Soll", "in", "die", "Welt", ",", "auf", "de\u00b7ren", "Pfad"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So leicht die Tugend gleitet,", "tokens": ["So", "leicht", "die", "Tu\u00b7gend", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo nicht dein Beispiel, nicht dein Rat", "tokens": ["Wo", "nicht", "dein", "Bei\u00b7spiel", ",", "nicht", "dein", "Rat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich, als ein Engel, leitet!", "tokens": ["Mich", ",", "als", "ein", "En\u00b7gel", ",", "lei\u00b7tet", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O bitte du f\u00fcr mich! Du bist", "tokens": ["O", "bit\u00b7te", "du", "f\u00fcr", "mich", "!", "Du", "bist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An ihn als Braut verm\u00e4hlet;", "tokens": ["An", "ihn", "als", "Braut", "ver\u00b7m\u00e4h\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bitt ihn, da\u00df mein er nicht vergi\u00dft,", "tokens": ["Bitt", "ihn", ",", "da\u00df", "mein", "er", "nicht", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihn mein Herz verfehlt!", "tokens": ["Wenn", "ihn", "mein", "Herz", "ver\u00b7fehlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}