{"dta.poem.3826": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Zeilen,  \n An einen Ehe-Herrn/ bey fr\u00fchzeitigen Ableben seiner gelieb-  \n ten  Eleonoren.  Jm Nahmen eines andern 1742. ver-  \n fertiget.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "O Freund! wie zittert Hertz und Brust,", "tokens": ["O", "Freund", "!", "wie", "zit\u00b7tert", "Hertz", "und", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hast du deines Hertzens Lust,", "tokens": ["So", "hast", "du", "dei\u00b7nes", "Hert\u00b7zens", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hast du nun ", "tokens": ["So", "hast", "du", "nun"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den frommen Ehe-Schatz verlohren.", "tokens": ["Den", "from\u00b7men", "E\u00b7he\u00b7Schatz", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie? rei\u00dft der Tod das feste Band", "tokens": ["Wie", "?", "rei\u00dft", "der", "Tod", "das", "fes\u00b7te", "Band"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der starck verkn\u00fcpfften Brust und Hand;", "tokens": ["Der", "starck", "ver\u00b7kn\u00fcpff\u00b7ten", "Brust", "und", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mag auch ein Schmertz den deinen gleichen!", "tokens": ["Mag", "auch", "ein", "Schmertz", "den", "dei\u00b7nen", "glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O! k\u00f6nnte meine Feder schweigen.", "tokens": ["O", "!", "k\u00f6nn\u00b7te", "mei\u00b7ne", "Fe\u00b7der", "schwei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Wehmuth hemmet jeden Satz,", "tokens": ["Die", "Weh\u00b7muth", "hem\u00b7met", "je\u00b7den", "Satz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja ich bejammre deinen Schatz,", "tokens": ["Ja", "ich", "be\u00b7jamm\u00b7re", "dei\u00b7nen", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Muster redlich frommer Tugend,", "tokens": ["Das", "Mus\u00b7ter", "red\u00b7lich", "from\u00b7mer", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Spiegel angenehmster Jugend.", "tokens": ["Den", "Spie\u00b7gel", "an\u00b7ge\u00b7nehms\u00b7ter", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr mehr als christlich Christenthum", "tokens": ["Ihr", "mehr", "als", "christ\u00b7lich", "Chris\u00b7ten\u00b7thum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "KOKOM", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erwarb ihr Ehre, Lob und Ruhm.", "tokens": ["Er\u00b7warb", "ihr", "Eh\u00b7re", ",", "Lob", "und", "Ruhm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Di\u00df mu\u00df ihr auch in sp\u00e4ten Zeiten", "tokens": ["Di\u00df", "mu\u00df", "ihr", "auch", "in", "sp\u00e4\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein ewig Denckmahl zubereiten.", "tokens": ["Ein", "e\u00b7wig", "Denck\u00b7mahl", "zu\u00b7be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O! wie begl\u00fcckt ist nicht der Geist,", "tokens": ["O", "!", "wie", "be\u00b7gl\u00fcckt", "ist", "nicht", "der", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VVPP", "VAFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich aus Kedars Wohnung rei\u00dft.", "tokens": ["Der", "sich", "aus", "Ke\u00b7dars", "Woh\u00b7nung", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Scheint uns der C\u00f6rper gleich verlohren,", "tokens": ["Scheint", "uns", "der", "C\u00f6r\u00b7per", "gleich", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Tode wird er neu gebohren.", "tokens": ["Jm", "To\u00b7de", "wird", "er", "neu", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Man stirbt zu neu belebter Krafft,", "tokens": ["Man", "stirbt", "zu", "neu", "be\u00b7leb\u00b7ter", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKA", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ewig Wohl und Leben schafft.", "tokens": ["Die", "e\u00b7wig", "Wohl", "und", "Le\u00b7ben", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So bringt das ob wohl bange Scheiden,", "tokens": ["So", "bringt", "das", "ob", "wohl", "ban\u00b7ge", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "KOUS", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das sch\u00f6nste Paradie", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Pa\u00b7ra\u00b7die"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein Kind, dein eintzig frommer Sohn,", "tokens": ["Dein", "Kind", ",", "dein", "eint\u00b7zig", "from\u00b7mer", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchrt mit dir gleichen Jammer-Thon;", "tokens": ["F\u00fchrt", "mit", "dir", "glei\u00b7chen", "Jam\u00b7mer\u00b7Thon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er thr\u00e4nt, er wimmert, \u00e4chtzt und klaget,", "tokens": ["Er", "thr\u00e4nt", ",", "er", "wim\u00b7mert", ",", "\u00e4chtzt", "und", "kla\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil man dem armen Sch\u00e4fgen saget,", "tokens": ["Weil", "man", "dem", "ar\u00b7men", "Sch\u00e4f\u00b7gen", "sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df die, so ihn so treu gepflegt,", "tokens": ["Da\u00df", "die", ",", "so", "ihn", "so", "treu", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In stillen Bogen hingelegt;", "tokens": ["In", "stil\u00b7len", "Bo\u00b7gen", "hin\u00b7ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In den betr\u00fcbten Jammer-Bogen,", "tokens": ["In", "den", "be\u00b7tr\u00fcb\u00b7ten", "Jam\u00b7mer\u00b7Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der ihm sein Mutter-Hertz entzogen.", "tokens": ["Der", "ihm", "sein", "Mut\u00b7ter\u00b7Hertz", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O sehr gerechtes Trauer-Lied!", "tokens": ["O", "sehr", "ge\u00b7rech\u00b7tes", "Trau\u00b7e\u00b7rLied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr jammert; ach ich weine mit.", "tokens": ["Ihr", "jam\u00b7mert", ";", "ach", "ich", "wei\u00b7ne", "mit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "XY", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Wunder! wenn an unsern Wangen", "tokens": ["Was", "Wun\u00b7der", "!", "wenn", "an", "un\u00b7sern", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$.", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die frommen Liebes-Zeichen hangen.", "tokens": ["Die", "from\u00b7men", "Lie\u00b7bes\u00b7Zei\u00b7chen", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dich kr\u00e4nckt dein banger Wittwer-Stand,", "tokens": ["Dich", "kr\u00e4nckt", "dein", "ban\u00b7ger", "Witt\u00b7wer\u00b7Stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich kr\u00e4nckt das arme Liebes-Pfand,", "tokens": ["Dich", "kr\u00e4nckt", "das", "ar\u00b7me", "Lie\u00b7bes\u00b7Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O! setz den traurigen Gedancken", "tokens": ["O", "!", "setz", "den", "trau\u00b7ri\u00b7gen", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gerechtes Ziel, und Maa\u00df, und Schrancken.", "tokens": ["Ge\u00b7rech\u00b7tes", "Ziel", ",", "und", "Maa\u00df", ",", "und", "Schran\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ergieb dich der Gelassenheit,", "tokens": ["Er\u00b7gieb", "dich", "der", "Ge\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott, Himmel, Schickung, Gl\u00fcck und Zeit,", "tokens": ["Gott", ",", "Him\u00b7mel", ",", "Schi\u00b7ckung", ",", "Gl\u00fcck", "und", "Zeit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Di\u00df wird nach GOttes Vater-Willen", "tokens": ["Di\u00df", "wird", "nach", "Got\u00b7tes", "Va\u00b7ter\u00b7Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die \u00fcberh\u00e4uffte Wehmuth stillen.", "tokens": ["Die", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7te", "Weh\u00b7muth", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bezwinge die bethr\u00e4nte Pein,", "tokens": ["Be\u00b7zwin\u00b7ge", "die", "be\u00b7thr\u00e4n\u00b7te", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott selbsten wird dein Retter seyn,", "tokens": ["Gott", "selbs\u00b7ten", "wird", "dein", "Ret\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir werden sie in jenen Auen", "tokens": ["Wir", "wer\u00b7den", "sie", "in", "je\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dereinst mit Freuden wieder schauen.", "tokens": ["De\u00b7reinst", "mit", "Freu\u00b7den", "wie\u00b7der", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}