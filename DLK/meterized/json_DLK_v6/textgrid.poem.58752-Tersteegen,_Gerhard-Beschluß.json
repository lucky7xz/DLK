{"textgrid.poem.58752": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "Beschlu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, was der Nutzen soll von diesem allem sein:", "tokens": ["H\u00f6rt", ",", "was", "der", "Nut\u00b7zen", "soll", "von", "die\u00b7sem", "al\u00b7lem", "sein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "VMFIN", "APPR", "PDAT", "PIS", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da\u00df man ohn' Vorbehalt soll Gottes sein und lieben!", "tokens": ["Da\u00df", "man", "ohn'", "Vor\u00b7be\u00b7halt", "soll", "Got\u00b7tes", "sein", "und", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VMFIN", "NN", "VAINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein doppelter Gewinn uns davon kommet ein,", "tokens": ["Ein", "dop\u00b7pel\u00b7ter", "Ge\u00b7winn", "uns", "da\u00b7von", "kom\u00b7met", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PAV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der uns bis dahin noch war unbekannt geblieben.", "tokens": ["Der", "uns", "bis", "da\u00b7hin", "noch", "war", "un\u00b7be\u00b7kannt", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PAV", "ADV", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dann, wenn wir lieben Gott, der unser Punkt der Ruh,", "tokens": ["Dann", ",", "wenn", "wir", "lie\u00b7ben", "Gott", ",", "der", "un\u00b7ser", "Punkt", "der", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "NN", "$,", "PRELS", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann werden wir in ihm die h\u00f6chste Ruhe finden;", "tokens": ["Dann", "wer\u00b7den", "wir", "in", "ihm", "die", "h\u00f6chs\u00b7te", "Ru\u00b7he", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil auch die reine Lieb' sonst keine Lieb' l\u00e4\u00dft zu,", "tokens": ["Weil", "auch", "die", "rei\u00b7ne", "Lieb'", "sonst", "kei\u00b7ne", "Lieb'", "l\u00e4\u00dft", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df, was an der Lieb' gebrechlich ist, verschwinden.", "tokens": ["So", "mu\u00df", ",", "was", "an", "der", "Lieb'", "ge\u00b7brech\u00b7lich", "ist", ",", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PRELS", "APPR", "ART", "NN", "ADJD", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im h\u00f6chsten Wesen sich das Herz zur Ruhe legt,", "tokens": ["Im", "h\u00f6chs\u00b7ten", "We\u00b7sen", "sich", "das", "Herz", "zur", "Ru\u00b7he", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im sch\u00f6nsten Vorwurf, der die Liebe an sich ziehet,", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Vor\u00b7wurf", ",", "der", "die", "Lie\u00b7be", "an", "sich", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der in sich selbst zwar bleibt und nimmer sich bewegt,", "tokens": ["Der", "in", "sich", "selbst", "zwar", "bleibt", "und", "nim\u00b7mer", "sich", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "ADV", "VVFIN", "KON", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Weisheit doch regiert, was man geschaffen siehet.", "tokens": ["Mit", "Weis\u00b7heit", "doch", "re\u00b7giert", ",", "was", "man", "ge\u00b7schaf\u00b7fen", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$,", "PRELS", "PIS", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sein zarter Liebeszug zieht stets das Herze an;", "tokens": ["Sein", "zar\u00b7ter", "Lie\u00b7bes\u00b7zug", "zieht", "stets", "das", "Her\u00b7ze", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zug ist unsichtbar und doch so kr\u00e4ftig neiget,", "tokens": ["Der", "Zug", "ist", "un\u00b7sicht\u00b7bar", "und", "doch", "so", "kr\u00e4f\u00b7tig", "nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df, ob ihn zwar der Sinn so grob nicht f\u00fchlen kann,", "tokens": ["Da\u00df", ",", "ob", "ihn", "zwar", "der", "Sinn", "so", "grob", "nicht", "f\u00fch\u00b7len", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er sich doch m\u00e4chtiger als selbst der Tod erzeiget.", "tokens": ["Er", "sich", "doch", "m\u00e4ch\u00b7ti\u00b7ger", "als", "selbst", "der", "Tod", "er\u00b7zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "ADJA", "KOKOM", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wir unterwerfen uns denn seiner Herrschaft gar", "tokens": ["Wir", "un\u00b7ter\u00b7wer\u00b7fen", "uns", "denn", "sei\u00b7ner", "Herr\u00b7schaft", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trotz aller Sinne Wut; den Grund sie nicht verletzen.", "tokens": ["Trotz", "al\u00b7ler", "Sin\u00b7ne", "Wut", ";", "den", "Grund", "sie", "nicht", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$.", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Anfang leidet man wie eine Marter zwar,", "tokens": ["Im", "An\u00b7fang", "lei\u00b7det", "man", "wie", "ei\u00b7ne", "Mar\u00b7ter", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "KOKOM", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Liebe tausendfach es endlich wird ersetzen!", "tokens": ["Die", "Lie\u00b7be", "tau\u00b7send\u00b7fach", "es", "end\u00b7lich", "wird", "er\u00b7set\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}