{"textgrid.poem.58470": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer Jesus folget nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Jesus folget nicht,", "tokens": ["Wer", "Je\u00b7sus", "fol\u00b7get", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der bleibet ohne Licht;", "tokens": ["Der", "blei\u00b7bet", "oh\u00b7ne", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du kannst den Schall zwar h\u00f6ren,", "tokens": ["Du", "kannst", "den", "Schall", "zwar", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch wer sich nicht bestrebt,", "tokens": ["Doch", "wer", "sich", "nicht", "be\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df er wie Jesus lebt,", "tokens": ["Da\u00df", "er", "wie", "Je\u00b7sus", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Fa\u00dft nimmer seine Lehren.", "tokens": ["Fa\u00dft", "nim\u00b7mer", "sei\u00b7ne", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}