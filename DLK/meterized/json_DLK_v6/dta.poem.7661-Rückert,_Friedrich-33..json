{"dta.poem.7661": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du dich anders willst als all die andern kleiden,", "tokens": ["Wenn", "du", "dich", "an\u00b7ders", "willst", "als", "all", "die", "an\u00b7dern", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VMFIN", "KOKOM", "PIAT", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sagt man, da\u00df du dich auszeichnest unbescheiden.", "tokens": ["So", "sagt", "man", ",", "da\u00df", "du", "dich", "aus\u00b7zeich\u00b7nest", "un\u00b7be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "PPER", "PRF", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und sagst du, da\u00df du's nur thust aus Bequemlichkeit,", "tokens": ["Und", "sagst", "du", ",", "da\u00df", "du's", "nur", "thust", "aus", "Be\u00b7quem\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wisse, da\u00df man die noch minder dir verzeiht.", "tokens": ["So", "wis\u00b7se", ",", "da\u00df", "man", "die", "noch", "min\u00b7der", "dir", "ver\u00b7zeiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["AD