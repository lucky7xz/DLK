{"dta.poem.3617": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "7.  Erziehung durch Leidenschaft .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Zorn, du Abgrund des Verderbens,               ", "tokens": ["O", "Zorn", ",", "du", "Ab\u00b7grund", "des", "Ver\u00b7der\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du unbarmherziger Tyrann;", "tokens": ["Du", "un\u00b7barm\u00b7her\u00b7zi\u00b7ger", "Ty\u00b7rann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du frissest, t\u00f6dtest sonder Sterben,", "tokens": ["Du", "fris\u00b7sest", ",", "t\u00f6d\u00b7test", "son\u00b7der", "Ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und brennest stets von neuem an;", "tokens": ["Und", "bren\u00b7nest", "stets", "von", "neu\u00b7em", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer da ger\u00e4th in deine Haft", "tokens": ["Wer", "da", "ge\u00b7r\u00e4\u00b7th", "in", "dei\u00b7ne", "Haft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Bekommt der H\u00f6lle Eigenschaft.", "tokens": ["Be\u00b7kommt", "der", "H\u00f6l\u00b7le", "Ei\u00b7gen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach w\u00e4ren wir verwahret blieben,", "tokens": ["Ach", "w\u00e4\u00b7ren", "wir", "ver\u00b7wah\u00b7ret", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor deiner strengen Widrigkeit;", "tokens": ["Vor", "dei\u00b7ner", "stren\u00b7gen", "Wid\u00b7rig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie selig w\u00e4ren wir im Lieben,", "tokens": ["Wie", "se\u00b7lig", "w\u00e4\u00b7ren", "wir", "im", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fc\u00dften nicht, was Ungleichheit", "tokens": ["Und", "w\u00fc\u00df\u00b7ten", "nicht", ",", "was", "Un\u00b7gleich\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWS", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Im Guten und im B\u00f6sen sey,", "tokens": ["Im", "Gu\u00b7ten", "und", "im", "B\u00f6\u00b7sen", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00e4ren wir des Zornes frey.", "tokens": ["So", "w\u00e4\u00b7ren", "wir", "des", "Zor\u00b7nes", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O da\u00df wir doch wohl m\u00f6gten fassen,", "tokens": ["O", "da\u00df", "wir", "doch", "wohl", "m\u00f6g\u00b7ten", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Woher der Grimm entsprungen sey;", "tokens": ["Wo\u00b7her", "der", "Grimm", "ent\u00b7sprun\u00b7gen", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und st\u00fcnden in der Lieb gelassen,", "tokens": ["Und", "st\u00fcn\u00b7den", "in", "der", "Lieb", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hielten uns des Zornes frey;", "tokens": ["Und", "hiel\u00b7ten", "uns", "des", "Zor\u00b7nes", "frey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Hochmuth und die Eigenheit", "tokens": ["Der", "Hoch\u00b7muth", "und", "die", "Ei\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erregen Zorn und Grimmigkeit.", "tokens": ["Er\u00b7re\u00b7gen", "Zorn", "und", "Grim\u00b7mig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df mich aus Eigenheit ausgehen,", "tokens": ["La\u00df", "mich", "aus", "Ei\u00b7gen\u00b7heit", "aus\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und aller Selbheit sterben ab;", "tokens": ["Und", "al\u00b7ler", "Selb\u00b7heit", "ster\u00b7ben", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lieb hei\u00df in mir auferstehen,", "tokens": ["Die", "Lieb", "hei\u00df", "in", "mir", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und allen Zorn schick in das Grab;", "tokens": ["Und", "al\u00b7len", "Zorn", "schick", "in", "das", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df keine Noth mir mehr setz zu,", "tokens": ["Da\u00df", "kei\u00b7ne", "Noth", "mir", "mehr", "setz", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Kein Widerwille brech die Ruh.", "tokens": ["Kein", "Wi\u00b7der\u00b7wil\u00b7le", "brech", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Liebe, die nicht ist ihr eigen,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "nicht", "ist", "ihr", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sich in allem macht gemein;", "tokens": ["Die", "sich", "in", "al\u00b7lem", "macht", "ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In mir sich la\u00df in Demuth zeigen,", "tokens": ["In", "mir", "sich", "la\u00df", "in", "De\u00b7muth", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df mich ein Kind der Liebe seyn;", "tokens": ["La\u00df", "mich", "ein", "Kind", "der", "Lie\u00b7be", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der alten Schlange Kopf zerbrich", "tokens": ["Der", "al\u00b7ten", "Schlan\u00b7ge", "Kopf", "zer\u00b7brich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In mir und dann erkenne dich.", "tokens": ["In", "mir", "und", "dann", "er\u00b7ken\u00b7ne", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo ist o Liebe deine Tiefe,", "tokens": ["Wo", "ist", "o", "Lie\u00b7be", "dei\u00b7ne", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Urgrund deiner Wunderkraft;", "tokens": ["Der", "Ur\u00b7grund", "dei\u00b7ner", "Wun\u00b7der\u00b7kraft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seel, komm ein einzig Tr\u00f6pflein pr\u00fcfe", "tokens": ["Seel", ",", "komm", "ein", "ein\u00b7zig", "Tr\u00f6pf\u00b7lein", "pr\u00fc\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von dieser Wirkungseigenschaft.", "tokens": ["Von", "die\u00b7ser", "Wir\u00b7kungs\u00b7ei\u00b7gen\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O wer in diesem tiefen Meer", "tokens": ["O", "wer", "in", "die\u00b7sem", "tie\u00b7fen", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich einem Tr\u00f6pflein sich verl\u00f6r!", "tokens": ["Gleich", "ei\u00b7nem", "Tr\u00f6pf\u00b7lein", "sich", "ver\u00b7l\u00f6r", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}