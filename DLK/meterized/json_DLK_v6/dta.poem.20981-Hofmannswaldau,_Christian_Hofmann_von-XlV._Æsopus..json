{"dta.poem.20981": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XlV.  \n \u00c6sopus.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Den ungeschickten Leib bedeckte mein Verstand/", "tokens": ["Den", "un\u00b7ge\u00b7schick\u00b7ten", "Leib", "be\u00b7deck\u00b7te", "mein", "Ver\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Ruhm lief fliegende durch aller V\u00f6lcker Land.", "tokens": ["Mein", "Ruhm", "lief", "flie\u00b7gen\u00b7de", "durch", "al\u00b7ler", "V\u00f6l\u00b7cker", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und hab ich Zung und Mund/ Lufft/ Erd/ und See", "tokens": ["Und", "hab", "ich", "Zung", "und", "Mund", "/", "Lufft", "/", "Erd", "/", "und", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "KON", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "gegeben/", "tokens": ["ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So mu\u00df ich billich auch auff allen Zungen leben.", "tokens": ["So", "mu\u00df", "ich", "bil\u00b7lich", "auch", "auff", "al\u00b7len", "Zun\u00b7gen", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}