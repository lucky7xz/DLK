{"textgrid.poem.59235": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schalkhafte Augen reizend aufgeschlagen,", "tokens": ["Schalk\u00b7haf\u00b7te", "Au\u00b7gen", "rei\u00b7zend", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Brust emp\u00f6rt, die W\u00fcnsche zu verschweigen,", "tokens": ["Die", "Brust", "em\u00b7p\u00f6rt", ",", "die", "W\u00fcn\u00b7sche", "zu", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieht man den leichten Zelter dich besteigen,", "tokens": ["Sieht", "man", "den", "leich\u00b7ten", "Zel\u00b7ter", "dich", "be\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Lust und Scherzen durch den Lenz zu jagen.", "tokens": ["Nach", "Lust", "und", "Scher\u00b7zen", "durch", "den", "Lenz", "zu", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu jung, des Lebens Ernste zu entsagen \u2013", "tokens": ["Zu", "jung", ",", "des", "Le\u00b7bens", "Erns\u00b7te", "zu", "ent\u00b7sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kann ich nicht l\u00e4nger spielen nun und schweigen,", "tokens": ["Kann", "ich", "nicht", "l\u00e4n\u00b7ger", "spie\u00b7len", "nun", "und", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer Herrlichs f\u00fchlt, der mu\u00df sich herrlich zeigen,", "tokens": ["Wer", "Herr\u00b7lichs", "f\u00fchlt", ",", "der", "mu\u00df", "sich", "herr\u00b7lich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "ART", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Ruhen ist ein ewig frisches Wagen.", "tokens": ["Mein", "Ru\u00b7hen", "ist", "ein", "e\u00b7wig", "fri\u00b7sches", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00df mich, solang noch trunken unsre Augen,", "tokens": ["La\u00df", "mich", ",", "so\u00b7lang", "noch", "trun\u00b7ken", "uns\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VMFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein'n bl\u00fchnden Kranz aus den vergangnen Stunden", "tokens": ["Ein'n", "bl\u00fchn\u00b7den", "Kranz", "aus", "den", "ver\u00b7gang\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir heiter um die wei\u00dfe Stirne winden;", "tokens": ["Dir", "hei\u00b7ter", "um", "die", "wei\u00b7\u00dfe", "Stir\u00b7ne", "win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Frag nicht dann, was mich deinem Arm entwunden,", "tokens": ["Frag", "nicht", "dann", ",", "was", "mich", "dei\u00b7nem", "Arm", "ent\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "$,", "PWS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dr\u00fcck fest den Kranz nur in die muntern Augen,", "tokens": ["Dr\u00fcck", "fest", "den", "Kranz", "nur", "in", "die", "mun\u00b7tern", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Haupt will auch und soll den seinen finden!", "tokens": ["Mein", "Haupt", "will", "auch", "und", "soll", "den", "sei\u00b7nen", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "KON", "VMFIN", "ART", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}