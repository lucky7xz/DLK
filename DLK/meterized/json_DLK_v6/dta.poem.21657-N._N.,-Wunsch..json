{"dta.poem.21657": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.71", "af:0.28"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Amseln singen,", "tokens": ["Die", "Am\u00b7seln", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mit sanften Schwingen", "tokens": ["Mit", "sanf\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "S\u00e4uselt der Wind.", "tokens": ["S\u00e4u\u00b7selt", "der", "Wind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dem Tod entronnen!", "tokens": ["Dem", "Tod", "ent\u00b7ron\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Du Licht der Sonnen,", "tokens": ["Du", "Licht", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Labe mich lind!", "tokens": ["La\u00b7be", "mich", "lind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "O bitt\u2019res Leiden,", "tokens": ["O", "bitt'\u00b7res", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So jung zu scheiden", "tokens": ["So", "jung", "zu", "schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In\u2019s Todtenland!", "tokens": ["In's", "Tod\u00b7ten\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Geknickte Saaten,", "tokens": ["Ge\u00b7knick\u00b7te", "Saa\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Gemordete Thaten,", "tokens": ["Ge\u00b7mor\u00b7de\u00b7te", "Tha\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nimmer genannt.", "tokens": ["Nim\u00b7mer", "ge\u00b7nannt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wie flammt\u2019s im Blute", "tokens": ["Wie", "flammt's", "im", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Von dr\u00e4ngendem Muthe", "tokens": ["Von", "dr\u00e4n\u00b7gen\u00b7dem", "Mu\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und edler Kraft!", "tokens": ["Und", "ed\u00b7ler", "Kraft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dank, g\u00fctige G\u00f6tter,", "tokens": ["Dank", ",", "g\u00fc\u00b7ti\u00b7ge", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da\u00df nicht das Wetter", "tokens": ["Da\u00df", "nicht", "das", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mich hingerafft.", "tokens": ["Mich", "hin\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "O, la\u00df mich leben,", "tokens": ["O", ",", "la\u00df", "mich", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gewappnet zu streben", "tokens": ["Ge\u00b7wapp\u00b7net", "zu", "stre\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nach Lieb\u2019 und Licht,", "tokens": ["Nach", "Lieb'", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bis einst in Frieden", "tokens": ["Bis", "einst", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Dem Kampfesm\u00fcden", "tokens": ["Dem", "Kamp\u00b7fes\u00b7m\u00fc\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Das Auge bricht.", "tokens": ["Das", "Au\u00b7ge", "bricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}