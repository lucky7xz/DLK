{"dta.poem.10288": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Der rote Mantel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.71", "nl:0.28"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Nis Hinrichsen von Heistrupgaard,             ", "tokens": ["Nis", "Hin\u00b7rich\u00b7sen", "von", "Heis\u00b7trup\u00b7gaard", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Hardesvogt von B\u00fclderupgaard,", "tokens": ["Der", "Har\u00b7des\u00b7vogt", "von", "B\u00fcl\u00b7de\u00b7rup\u00b7gaard", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "War klug und wahr im Rate.", "tokens": ["War", "klug", "und", "wahr", "im", "Ra\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Hengst sprang zwanzig Ellen weit,", "tokens": ["Sein", "Hengst", "sprang", "zwan\u00b7zig", "El\u00b7len", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gespickt mit Pfeilen war sein Kleid,", "tokens": ["Ge\u00b7spickt", "mit", "Pfei\u00b7len", "war", "sein", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Sonntag Jubilate.", "tokens": ["Am", "Sonn\u00b7tag", "Ju\u00b7bi\u00b7la\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der alte K\u00f6nig Gorm ist tot,", "tokens": ["Der", "al\u00b7te", "K\u00f6\u00b7nig", "Gorm", "ist", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da war im Reiche gro\u00dfe Not,", "tokens": ["Da", "war", "im", "Rei\u00b7che", "gro\u00b7\u00dfe", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer soll nun K\u00f6nig werden.", "tokens": ["Wer", "soll", "nun", "K\u00f6\u00b7nig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den J\u00fcngsten, Gilm, liebt Volk und Land,", "tokens": ["Den", "J\u00fcng\u00b7sten", ",", "Gilm", ",", "liebt", "Volk", "und", "Land", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Andre, Skjalm, ist unbekannt,", "tokens": ["Der", "And\u00b7re", ",", "Sk\u00b7jalm", ",", "ist", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der schweift umher auf Erden.", "tokens": ["Der", "schweift", "um\u00b7her", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch als er h\u00f6rt des K\u00f6nigs End\u2019,", "tokens": ["Doch", "als", "er", "h\u00f6rt", "des", "K\u00f6\u00b7nigs", "End'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flugs hat er auch die Stirn gewend\u2019t,", "tokens": ["Flugs", "hat", "er", "auch", "die", "Stirn", "ge\u00b7wend't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist zu Haus schon heute.", "tokens": ["Und", "ist", "zu", "Haus", "schon", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der J\u00fcngste aber schreit ihn an,", "tokens": ["Der", "J\u00fcngs\u00b7te", "a\u00b7ber", "schreit", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was willst du hier, du fremder Mann,", "tokens": ["Was", "willst", "du", "hier", ",", "du", "frem\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich kennen nicht die Leute.", "tokens": ["Dich", "ken\u00b7nen", "nicht", "die", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was, rief der \u00c4lteste mit Grimm,", "tokens": ["Was", ",", "rief", "der", "\u00c4l\u00b7tes\u00b7te", "mit", "Grimm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Kobold, du, und das w\u00e4r\u2019 schlimm,", "tokens": ["Du", "Ko\u00b7bold", ",", "du", ",", "und", "das", "w\u00e4r'", "schlimm", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PPER", "$,", "KON", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch h\u00f6re, was ich sage.", "tokens": ["Doch", "h\u00f6\u00b7re", ",", "was", "ich", "sa\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nis Hinrichsen, wie dir bekannt,", "tokens": ["Nis", "Hin\u00b7rich\u00b7sen", ",", "wie", "dir", "be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "PWAV", "PPER", "PTKVZ", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ist Vicek\u00f6nig hier im Land,", "tokens": ["Ist", "Vi\u00b7ce\u00b7k\u00f6\u00b7nig", "hier", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der schlichte unsre Klage.", "tokens": ["Der", "schlich\u00b7te", "uns\u00b7re", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nis zog die Hakennase kraus", "tokens": ["Nis", "zog", "die", "Ha\u00b7ken\u00b7na\u00b7se", "kraus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wettert zornig: Ei, der Daus.", "tokens": ["Und", "wet\u00b7tert", "zor\u00b7nig", ":", "Ei", ",", "der", "Daus", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor \u00c4rger wurd\u2019 er gelbe.", "tokens": ["Vor", "\u00c4r\u00b7ger", "wurd'", "er", "gel\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn mach\u2019 ich Skjalm die Sache recht,", "tokens": ["Denn", "mach'", "ich", "Sk\u00b7jalm", "die", "Sa\u00b7che", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So mach\u2019 ich Gilm die Sache schlecht,", "tokens": ["So", "mach'", "ich", "Gilm", "die", "Sa\u00b7che", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und umgekehrt dasselbe.", "tokens": ["Und", "um\u00b7ge\u00b7kehrt", "das\u00b7sel\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PDAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Teufel hol\u2019 den Kronenzwist,", "tokens": ["Der", "Teu\u00b7fel", "hol'", "den", "Kro\u00b7nen\u00b7zwist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bitt\u2019 mir aus ein Halbjahr Frist,", "tokens": ["Ich", "bitt'", "mir", "aus", "ein", "Halb\u00b7jahr", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wird vielleicht gelingen.", "tokens": ["Es", "wird", "viel\u00b7leicht", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stark f\u00fcttern lie\u00df er seinen Rock,", "tokens": ["Stark", "f\u00fct\u00b7tern", "lie\u00df", "er", "sei\u00b7nen", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00fcbte \u00fcber Stein und Stock", "tokens": ["Und", "\u00fcb\u00b7te", "\u00fc\u00b7ber", "Stein", "und", "Stock"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein milchwei\u00df Pferd im Springen.", "tokens": ["Sein", "milch\u00b7wei\u00df", "Pferd", "im", "Sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In Urneh\u00f6ved war die Wahl,", "tokens": ["In", "Ur\u00b7ne\u00b7h\u00f6\u00b7ved", "war", "die", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es warten dort in Helm und Stahl,", "tokens": ["Es", "war\u00b7ten", "dort", "in", "Helm", "und", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Skjalm, Gilm, und ihre Ritter.", "tokens": ["Sk\u00b7jalm", ",", "Gilm", ",", "und", "ih\u00b7re", "Rit\u00b7ter", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nis kam und schrie von Weitem schon:", "tokens": ["Nis", "kam", "und", "schrie", "von", "Wei\u00b7tem", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gilm blieb im Land, daf\u00fcr den Thron. \u2014", "tokens": ["Gilm", "blieb", "im", "Land", ",", "da\u00b7f\u00fcr", "den", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,", "PAV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kehrt, fort wie Ungewitter.", "tokens": ["Kehrt", ",", "fort", "wie", "Un\u00b7ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Heraus die Plempen, schlagt ihn tot,", "tokens": ["He\u00b7raus", "die", "Plem\u00b7pen", ",", "schlagt", "ihn", "tot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Br\u00fcllt heiser Skjalm, Schockschwerenot,", "tokens": ["Br\u00fcllt", "hei\u00b7ser", "Sk\u00b7jalm", ",", "Schock\u00b7schwe\u00b7re\u00b7not", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und la\u00dft die Pfeile schwirren.", "tokens": ["Und", "la\u00dft", "die", "Pfei\u00b7le", "schwir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es braust die Jagd wie Wettergraus,", "tokens": ["Es", "braust", "die", "Jagd", "wie", "Wet\u00b7ter\u00b7graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Nis ist immer weit voraus,", "tokens": ["Doch", "Nis", "ist", "im\u00b7mer", "weit", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und l\u00e4\u00dft sich nicht beirren.", "tokens": ["Und", "l\u00e4\u00dft", "sich", "nicht", "be\u00b7ir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Heissa, in rasendem Galopp,", "tokens": ["Heis\u00b7sa", ",", "in", "ra\u00b7sen\u00b7dem", "Ga\u00b7lopp", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein Wagen wegquer, dr\u00fcber, hopp,", "tokens": ["Ein", "Wa\u00b7gen", "weg\u00b7quer", ",", "dr\u00fc\u00b7ber", ",", "hopp", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PAV", "$,", "ITJ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es zaudern schon die Letzten.", "tokens": ["Es", "zau\u00b7dern", "schon", "die", "Letz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein dicker roter Mantel bl\u00e4ht,", "tokens": ["Sein", "di\u00b7cker", "ro\u00b7ter", "Man\u00b7tel", "bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von tausend Pfeilen \u00fcbers\u00e4t,", "tokens": ["Von", "tau\u00b7send", "Pfei\u00b7len", "\u00fc\u00b7bers\u00b7\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weit ab die Hund\u2019, die hetzten.", "tokens": ["Weit", "ab", "die", "Hund'", ",", "die", "hetz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Den roten Mantel hing er auf", "tokens": ["Den", "ro\u00b7ten", "Man\u00b7tel", "hing", "er", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An einer Marmors\u00e4ule Knauf", "tokens": ["An", "ei\u00b7ner", "Mar\u00b7mor\u00b7s\u00e4u\u00b7le", "Knauf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In hohen Tempelhallen.", "tokens": ["In", "ho\u00b7hen", "Tem\u00b7pel\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Urgro\u00dfvater fand ihn noch,", "tokens": ["Mein", "Ur\u00b7gro\u00df\u00b7va\u00b7ter", "fand", "ihn", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah von ihm kein \u00d6senloch,", "tokens": ["Ich", "sah", "von", "ihm", "kein", "\u00d6\u00b7sen\u00b7loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er ist in Staub zerfallen.", "tokens": ["Er", "ist", "in", "Staub", "zer\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}