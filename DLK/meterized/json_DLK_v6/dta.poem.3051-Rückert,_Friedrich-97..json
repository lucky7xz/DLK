{"dta.poem.3051": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beim Hauch des Morgens und der Mittern\u00e4chte Schauer", "tokens": ["Beim", "Hauch", "des", "Mor\u00b7gens", "und", "der", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADV", "KON", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlt' ich die Trauer, da\u00df die Welt hat keine Dauer;", "tokens": ["F\u00fchlt'", "ich", "die", "Trau\u00b7er", ",", "da\u00df", "die", "Welt", "hat", "kei\u00b7ne", "Dau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df wir am Anfang schon dem End' entgegen gehn,", "tokens": ["Da\u00df", "wir", "am", "An\u00b7fang", "schon", "dem", "End'", "ent\u00b7ge\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und doch am Ende noch beim Anfang immer stehn.", "tokens": ["Und", "doch", "am", "En\u00b7de", "noch", "beim", "An\u00b7fang", "im\u00b7mer", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADV", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bald haben wir's verwacht, bald haben wir's vertr\u00e4umt,", "tokens": ["Bald", "ha\u00b7ben", "wir's", "ver\u00b7wacht", ",", "bald", "ha\u00b7ben", "wir's", "ver\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$,", "ADV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie s\u00e4umend Tag und Nacht, das Gl\u00fcck ist stets vers\u00e4umt.", "tokens": ["Nie", "s\u00e4u\u00b7mend", "Tag", "und", "Nacht", ",", "das", "Gl\u00fcck", "ist", "stets", "ver\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie uns zuschauerhaft vorbeigeht schauerlich", "tokens": ["Wie", "uns", "zu\u00b7schau\u00b7er\u00b7haft", "vor\u00b7bei\u00b7geht", "schau\u00b7er\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt undauerhaft, ist wohl bedauerlich.", "tokens": ["Die", "Welt", "un\u00b7dau\u00b7er\u00b7haft", ",", "ist", "wohl", "be\u00b7dau\u00b7er\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wohl zu bedauern sind leichtsinnige Vertrauer,", "tokens": ["Wohl", "zu", "be\u00b7dau\u00b7ern", "sind", "leicht\u00b7sin\u00b7ni\u00b7ge", "Ver\u00b7trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAFIN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die hier ins Lustspiel gehn, und finden das der Trauer.", "tokens": ["Die", "hier", "ins", "Lust\u00b7spiel", "gehn", ",", "und", "fin\u00b7den", "das", "der", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVINF", "$,", "KON", "VVFIN", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und zwei nur sind begl\u00fcckt, der den kein Trug ber\u00fcckt,", "tokens": ["Und", "zwei", "nur", "sind", "be\u00b7gl\u00fcckt", ",", "der", "den", "kein", "Trug", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADV", "VAFIN", "VVPP", "$,", "PRELS", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der dem es gen\u00fcgt, da\u00df ihn ein Trug begl\u00fcckt.", "tokens": ["Und", "der", "dem", "es", "ge\u00b7n\u00fcgt", ",", "da\u00df", "ihn", "ein", "Trug", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}