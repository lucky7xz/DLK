{"textgrid.poem.67292": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Habsburgs Kaiserhallen Versammelte,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Habsburgs Kaiserhallen Versammelte,", "tokens": ["In", "Habs\u00b7burgs", "Kai\u00b7ser\u00b7hal\u00b7len", "Ver\u00b7sam\u00b7mel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gott gr\u00fc\u00df' Euch, deutsche M\u00e4nner! Geheftet starrt", "tokens": ["Gott", "gr\u00fc\u00df'", "Euch", ",", "deut\u00b7sche", "M\u00e4n\u00b7ner", "!", "Ge\u00b7hef\u00b7tet", "starrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "NN", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Des Vaterlandes Aug' auf Euch, ihr", "tokens": ["Des", "Va\u00b7ter\u00b7lan\u00b7des", "Aug'", "auf", "Euch", ",", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "$,", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Waltenden H\u00fcter des Seyns und Werdens.", "tokens": ["Wal\u00b7ten\u00b7den", "H\u00fc\u00b7ter", "des", "Seyns", "und", "Wer\u00b7dens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wann dr\u00f6hnten jemal so auf des Rathes Tisch", "tokens": ["Wann", "dr\u00f6hn\u00b7ten", "je\u00b7mal", "so", "auf", "des", "Ra\u00b7thes", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verh\u00e4ngni\u00dfschwanger, furchtbar ums\u00e4uselt \u2013 ach", "tokens": ["Ver\u00b7h\u00e4ng\u00b7ni\u00df\u00b7schwan\u00b7ger", ",", "furcht\u00b7bar", "um\u00b7s\u00e4u\u00b7selt", "\u2013", "ach"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJD", "VVPP", "$(", "XY"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vernehmt der Warnungsschauer Fl\u00fcstern! \u2013", "tokens": ["Ver\u00b7nehmt", "der", "War\u00b7nungs\u00b7schau\u00b7er", "Fl\u00fcs\u00b7tern", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rollend Entscheidung, die Schicksalsw\u00fcrfel?", "tokens": ["Rol\u00b7lend", "Ent\u00b7schei\u00b7dung", ",", "die", "Schick\u00b7sals\u00b7w\u00fcr\u00b7fel", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Der V\u00e4ter Ahnherrn, wenn sie bei'm Festgelag'", "tokens": ["Der", "V\u00e4\u00b7ter", "Ahn\u00b7herrn", ",", "wenn", "sie", "bei'm", "Fest\u00b7ge\u00b7lag'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Eichenschatten ruhten, des Uhres Horn", "tokens": ["Im", "Ei\u00b7chen\u00b7schat\u00b7ten", "ruh\u00b7ten", ",", "des", "Uh\u00b7res", "Horn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ART", "NN", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umherging; wie sich schimmernd heben", "tokens": ["Um\u00b7her\u00b7ging", ";", "wie", "sich", "schim\u00b7mernd", "he\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "PWAV", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stern und Gestirn an der blauen W\u00f6lbung,", "tokens": ["Stern", "und", "Ge\u00b7stirn", "an", "der", "blau\u00b7en", "W\u00f6l\u00b7bung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Empor so strahlten ihnen aus Heldensinn", "tokens": ["Em\u00b7por", "so", "strahl\u00b7ten", "ih\u00b7nen", "aus", "Hel\u00b7den\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gedanken auf Gedanken, es rastete", "tokens": ["Ge\u00b7dan\u00b7ken", "auf", "Ge\u00b7dan\u00b7ken", ",", "es", "ras\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jedoch der Arm, bis erst bei kaltem", "tokens": ["Je\u00b7doch", "der", "Arm", ",", "bis", "erst", "bei", "kal\u00b7tem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ernste sich senkte die Pr\u00fcfungswage.", "tokens": ["Erns\u00b7te", "sich", "senk\u00b7te", "die", "Pr\u00fc\u00b7fungs\u00b7wa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Uns unsre Sieg' entflammten zu Nektarrausch,", "tokens": ["Uns", "uns\u00b7re", "Sieg'", "ent\u00b7flamm\u00b7ten", "zu", "Nek\u00b7tar\u00b7rausch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Babels Einzug! huben die Fittige", "tokens": ["Und", "Ba\u00b7bels", "Ein\u00b7zug", "!", "hu\u00b7ben", "die", "Fit\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des trunknen Geistes \u2013 Ha! und st\u00fcrzten", "tokens": ["Des", "trunk\u00b7nen", "Geis\u00b7tes", "\u2013", "Ha", "!", "und", "st\u00fcrz\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ITJ", "$.", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht, nun gel\u00f6s't von der schn\u00f6den Fessel,", "tokens": ["Nicht", ",", "nun", "ge\u00b7l\u00f6s't", "von", "der", "schn\u00f6\u00b7den", "Fes\u00b7sel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "ADV", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "In unsre Feierbecher beim Lebehoch", "tokens": ["In", "uns\u00b7re", "Fei\u00b7er\u00b7be\u00b7cher", "beim", "Le\u00b7be\u00b7hoch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des deutschen Rheines, Vater Johannesberg", "tokens": ["Des", "deut\u00b7schen", "Rhei\u00b7nes", ",", "Va\u00b7ter", "Jo\u00b7han\u00b7nes\u00b7berg"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NE", "$,", "NN", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit seinem Rebenwald, und ihr in", "tokens": ["Mit", "sei\u00b7nem", "Re\u00b7ben\u00b7wald", ",", "und", "ihr", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Goldenen Str\u00f6mungen, Hochheims H\u00fcgel? \u2013", "tokens": ["Gol\u00b7de\u00b7nen", "Str\u00f6\u00b7mun\u00b7gen", ",", "Hoch\u00b7heims", "H\u00fc\u00b7gel", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Fein n\u00fcchtern, Muse! Nahe mit frommen Sinn", "tokens": ["Fein", "n\u00fcch\u00b7tern", ",", "Mu\u00b7se", "!", "Na\u00b7he", "mit", "from\u00b7men", "Sinn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "NN", "$.", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem ernsten Kreise, deines Berufes, o", "tokens": ["Dem", "erns\u00b7ten", "Krei\u00b7se", ",", "dei\u00b7nes", "Be\u00b7ru\u00b7fes", ",", "o"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "FM"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des hohen kundig! Heil'ge Zauber", "tokens": ["Des", "ho\u00b7hen", "kun\u00b7dig", "!", "Heil'\u00b7ge", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "ADJD", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weih'n dir die Lippe zu G\u00f6tterspr\u00fcchen.", "tokens": ["Weih'n", "dir", "die", "Lip\u00b7pe", "zu", "G\u00f6t\u00b7ter\u00b7spr\u00fc\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Geleitest du nicht Bitten und S\u00fchnungen,", "tokens": ["Ge\u00b7lei\u00b7test", "du", "nicht", "Bit\u00b7ten", "und", "S\u00fch\u00b7nun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gel\u00fcbd' und Dankesopfer und Tempelsang", "tokens": ["Ge\u00b7l\u00fcbd'", "und", "Dan\u00b7ke\u00b7sop\u00b7fer", "und", "Tem\u00b7pel\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gen Himmel? Flammt nur deiner Schwester,", "tokens": ["Gen", "Him\u00b7mel", "?", "Flammt", "nur", "dei\u00b7ner", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir, Uranide, nicht auch der Sternkranz?", "tokens": ["Dir", ",", "U\u00b7ra\u00b7ni\u00b7de", ",", "nicht", "auch", "der", "Stern\u00b7kranz", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Heut' athme Segensw\u00fcnsche die sanfte Brust,", "tokens": ["Heut'", "ath\u00b7me", "Se\u00b7gens\u00b7w\u00fcn\u00b7sche", "die", "sanf\u00b7te", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In W\u00fcnschen lisple leise das Saitenspiel,", "tokens": ["In", "W\u00fcn\u00b7schen", "lisp\u00b7le", "lei\u00b7se", "das", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Segen fleh', in ihm der Inbrunst", "tokens": ["Und", "Se\u00b7gen", "fleh'", ",", "in", "ihm", "der", "In\u00b7brunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Perle, der Blick, den nicht hemmt die Wolke:", "tokens": ["Per\u00b7le", ",", "der", "Blick", ",", "den", "nicht", "hemmt", "die", "Wol\u00b7ke", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Ach, da\u00df herab Sie schwebe, die Spenderinn", "tokens": ["Ach", ",", "da\u00df", "her\u00b7ab", "Sie", "schwe\u00b7be", ",", "die", "Spen\u00b7de\u00b7rinn"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des wahren Rathes, da\u00df sie zum Heiligthum", "tokens": ["Des", "wah\u00b7ren", "Ra\u00b7thes", ",", "da\u00df", "sie", "zum", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Halle widme, zu Astr\u00e4a's", "tokens": ["Die", "Hal\u00b7le", "wid\u00b7me", ",", "zu", "As\u00b7tr\u00e4\u00b7a's"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Priestern Teutonia's H\u00e4upter weihe,", "tokens": ["Pries\u00b7tern", "Teu\u00b7to\u00b7nia's", "H\u00e4up\u00b7ter", "wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Allvaters Erstgeborne, die W\u00e4chterinn", "tokens": ["All\u00b7va\u00b7ters", "Erst\u00b7ge\u00b7bor\u00b7ne", ",", "die", "W\u00e4ch\u00b7te\u00b7rinn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Urgesetzes, Sie die Erleuchterinn,", "tokens": ["Des", "Ur\u00b7ge\u00b7set\u00b7zes", ",", "Sie", "die", "Er\u00b7leuch\u00b7te\u00b7rinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die hohe Weisheit, da\u00df sie z\u00fcnd' in", "tokens": ["Die", "ho\u00b7he", "Weis\u00b7heit", ",", "da\u00df", "sie", "z\u00fcnd'", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "APPR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihnen des Lichts und des Rechtes Flamme!", "tokens": ["Ih\u00b7nen", "des", "Lichts", "und", "des", "Rech\u00b7tes", "Flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Dann waltet Eintracht, schirmt in der Bundeshand", "tokens": ["Dann", "wal\u00b7tet", "Ein\u00b7tracht", ",", "schirmt", "in", "der", "Bun\u00b7des\u00b7hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die festumschlungnen, ewigverbr\u00fcderten", "tokens": ["Die", "fes\u00b7tum\u00b7schlung\u00b7nen", ",", "e\u00b7wig\u00b7ver\u00b7br\u00fc\u00b7der\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Geschosse, nur dem Feind verletzbar,", "tokens": ["Ge\u00b7schos\u00b7se", ",", "nur", "dem", "Feind", "ver\u00b7letz\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sich gesondert die Pfeile l\u00f6sen.", "tokens": ["Wenn", "sich", "ge\u00b7son\u00b7dert", "die", "Pfei\u00b7le", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVPP", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "O dann erstehet, bl\u00fchend in Lenzeskraft,", "tokens": ["O", "dann", "er\u00b7ste\u00b7het", ",", "bl\u00fc\u00b7hend", "in", "Len\u00b7zes\u00b7kraft", ","], "token_info": ["wor