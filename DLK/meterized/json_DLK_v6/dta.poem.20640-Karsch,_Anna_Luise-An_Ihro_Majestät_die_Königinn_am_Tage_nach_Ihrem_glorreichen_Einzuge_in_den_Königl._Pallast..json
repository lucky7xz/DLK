{"dta.poem.20640": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n Ihro Majest\u00e4t  \n  die K\u00f6niginn  \n am Tage  \n nach Ihrem glorreichen Einzuge in den K\u00f6nigl.  \n Pallast.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe Dich, Vortrefflichste Gekr\u00f6nte,                 ", "tokens": ["Ich", "sa\u00b7he", "Dich", ",", "Vor\u00b7treff\u00b7lichs\u00b7te", "Ge\u00b7kr\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und \u00fcber Deinen Wagen flog", "tokens": ["Und", "\u00fc\u00b7ber", "Dei\u00b7nen", "Wa\u00b7gen", "flog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Engel, der die Frau vers\u00f6hnte,", "tokens": ["Ein", "En\u00b7gel", ",", "der", "die", "Frau", "ver\u00b7s\u00f6hn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die wider ", "tokens": ["Die", "wi\u00b7der"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ich sahe Dich herauf, in Deine goldne Zimmer", "tokens": ["Ich", "sa\u00b7he", "Dich", "her\u00b7auf", ",", "in", "Dei\u00b7ne", "gold\u00b7ne", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Getragen, und es warf der Abendsonne Glanz", "tokens": ["Ge\u00b7tra\u00b7gen", ",", "und", "es", "warf", "der", "A\u00b7bend\u00b7son\u00b7ne", "Glanz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir einen purpurfarbnen Schimmer", "tokens": ["Dir", "ei\u00b7nen", "pur\u00b7purf\u00b7arb\u00b7nen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf den Pallast, und als das Volk zu Spiel und Tanz", "tokens": ["Auf", "den", "Pal\u00b7last", ",", "und", "als", "das", "Volk", "zu", "Spiel", "und", "Tanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "KOUS", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Mit Jubelrufen lief, und tausend Reyhen machte;", "tokens": ["Mit", "Ju\u00b7bel\u00b7ru\u00b7fen", "lief", ",", "und", "tau\u00b7send", "Rey\u00b7hen", "mach\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da ward in seinem nur geborgten kleinen Prachte", "tokens": ["Da", "ward", "in", "sei\u00b7nem", "nur", "ge\u00b7borg\u00b7ten", "klei\u00b7nen", "Prach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der halbe Mond besch\u00e4mt, und bl\u00e4sser im Gesicht.", "tokens": ["Der", "hal\u00b7be", "Mond", "be\u00b7sch\u00e4mt", ",", "und", "bl\u00e4s\u00b7ser", "im", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Rings um Dich her war alles Licht.", "tokens": ["Rings", "um", "Dich", "her", "war", "al\u00b7les", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APZR", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Es zitterten die Fackelflammen", "tokens": ["Es", "zit\u00b7ter\u00b7ten", "die", "Fa\u00b7ckel\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00f6n in der Luft, das Volk flog noch einmal zusammen;", "tokens": ["Sch\u00f6n", "in", "der", "Luft", ",", "das", "Volk", "flog", "noch", "ein\u00b7mal", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und \u00fcber alle Sph\u00e4ren drang", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7le", "Sph\u00e4\u00b7ren", "drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das jauchzende Geschrey vom Frieden.", "tokens": ["Das", "jauch\u00b7zen\u00b7de", "Ge\u00b7schrey", "vom", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Gott selber, der den Streit der K\u00f6nige entschieden,", "tokens": ["Gott", "sel\u00b7ber", ",", "der", "den", "Streit", "der", "K\u00f6\u00b7ni\u00b7ge", "ent\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vernahm den besten Lobgesang.", "tokens": ["Ver\u00b7nahm", "den", "bes\u00b7ten", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Armuth ungezwungnes Weinen,", "tokens": ["Der", "Ar\u00b7muth", "un\u00b7ge\u00b7zwung\u00b7nes", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr freudig O! ihr dankend Ach!", "tokens": ["Ihr", "freu\u00b7dig", "O", "!", "ihr", "dan\u00b7kend", "Ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "$.", "PPER", "ADJD", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Klang lieblich in sein Ohr, als wenn auf gr\u00fcnem Dach", "tokens": ["Klang", "lieb\u00b7lich", "in", "sein", "Ohr", ",", "als", "wenn", "auf", "gr\u00fc\u00b7nem", "Dach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$,", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der bunten Erde sich die V\u00f6gel fr\u00fch vereinen,", "tokens": ["Der", "bun\u00b7ten", "Er\u00b7de", "sich", "die", "V\u00f6\u00b7gel", "fr\u00fch", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und singend vor Gew\u00fcrm und Korn", "tokens": ["Und", "sin\u00b7gend", "vor", "Ge\u00b7w\u00fcrm", "und", "Korn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ihm danken, da\u00df der Ton von ihren Simphonien", "tokens": ["Ihm", "dan\u00b7ken", ",", "da\u00df", "der", "Ton", "von", "ih\u00b7ren", "Sim\u00b7pho\u00b7ni\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bis \u00fcber Wolken dringt, wo tausend Sonnen gl\u00fchen.", "tokens": ["Bis", "\u00fc\u00b7ber", "Wol\u00b7ken", "dringt", ",", "wo", "tau\u00b7send", "Son\u00b7nen", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "VVFIN", "$,", "PWAV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So klang das Lobgeschrey dem Gotte, der den Zorn", "tokens": ["So", "klang", "das", "Lob\u00b7ge\u00b7schrey", "dem", "Got\u00b7te", ",", "der", "den", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erdeng\u00f6tter lie\u00df entbrennen,", "tokens": ["Der", "Er\u00b7den\u00b7g\u00f6t\u00b7ter", "lie\u00df", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu strafen seine Welt, in der die Bosheit stieg;", "tokens": ["Zu", "stra\u00b7fen", "sei\u00b7ne", "Welt", ",", "in", "der", "die", "Bos\u00b7heit", "stieg", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zu bezeugen, da\u00df den Sieg", "tokens": ["Und", "zu", "be\u00b7zeu\u00b7gen", ",", "da\u00df", "den", "Sieg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von ihm allein erwarten k\u00f6nnen", "tokens": ["Von", "ihm", "al\u00b7lein", "er\u00b7war\u00b7ten", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVINF", "VMINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Helden, wenn ihr Herr umringt,", "tokens": ["Die", "Hel\u00b7den", ",", "wenn", "ihr", "Herr", "um\u00b7ringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von aufgeschwollnen Feindeskr\u00e4ften", "tokens": ["Von", "auf\u00b7ge\u00b7schwoll\u00b7nen", "Fein\u00b7des\u00b7kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf allen Seiten ist, und t\u00e4glich in Gesch\u00e4ften", "tokens": ["Auf", "al\u00b7len", "Sei\u00b7ten", "ist", ",", "und", "t\u00e4g\u00b7lich", "in", "Ge\u00b7sch\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "$,", "KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des W\u00fcrgens, mit dem Tode ringt.", "tokens": ["Des", "W\u00fcr\u00b7gens", ",", "mit", "dem", "To\u00b7de", "ringt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu matt, o ", "tokens": ["Zu", "matt", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["PTKA", "ADJD", "$,", "FM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Nachahmer eines Pindars singt;", "tokens": ["Nac\u00b7hah\u00b7mer", "ei\u00b7nes", "Pin\u00b7dars", "singt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey ", "tokens": ["Bey"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wenn mitten aus dem Pomp der Seligen herab", "tokens": ["Wenn", "mit\u00b7ten", "aus", "dem", "Pomp", "der", "Se\u00b7li\u00b7gen", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Schatten eurer M\u00fctter blicken,", "tokens": ["Die", "Schat\u00b7ten", "eu\u00b7rer", "M\u00fct\u00b7ter", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sich umarmen und den Himmel mit Entz\u00fccken", "tokens": ["Und", "sich", "um\u00b7ar\u00b7men", "und", "den", "Him\u00b7mel", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "VVINF", "KON", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mehr f\u00fchlen, weil er Friede gab.", "tokens": ["Mehr", "f\u00fch\u00b7len", ",", "weil", "er", "Frie\u00b7de", "gab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}