{"textgrid.poem.57775": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Machangel, lieber Machangelbaum,", "tokens": ["Ma\u00b7chan\u00b7gel", ",", "lie\u00b7ber", "Ma\u00b7chan\u00b7gel\u00b7baum", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Trauern komm ich her;", "tokens": ["In", "Trau\u00b7ern", "komm", "ich", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich tr\u00e4umte einen b\u00f6sen Traum,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "ei\u00b7nen", "b\u00f6\u00b7sen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herze ist mir schwer.", "tokens": ["Das", "Her\u00b7ze", "ist", "mir", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Myrtenstock trug Bl\u00fcmelein", "tokens": ["Mein", "Myr\u00b7ten\u00b7stock", "trug", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie das Blut so rot,", "tokens": ["Als", "wie", "das", "Blut", "so", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist krank der Herzgeliebte mein,", "tokens": ["Ist", "krank", "der", "Herz\u00b7ge\u00b7lieb\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oder ist er am Ende tot?", "tokens": ["O\u00b7der", "ist", "er", "am", "En\u00b7de", "tot", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Dein Herzgeliebter im fernen Land", "tokens": ["Dein", "Herz\u00b7ge\u00b7lieb\u00b7ter", "im", "fer\u00b7nen", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist krank nicht und nicht tot;", "tokens": ["Ist", "krank", "nicht", "und", "nicht", "tot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKNEG", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat sein Lieben zugewandt", "tokens": ["Er", "hat", "sein", "Lie\u00b7ben", "zu\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einem M\u00e4gdlein rosenrot.", "tokens": ["Ei\u00b7nem", "M\u00e4gd\u00b7lein", "ro\u00b7sen\u00b7rot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einem rosenroten M\u00e4gdelein,", "tokens": ["Ei\u00b7nem", "ro\u00b7sen\u00b7ro\u00b7ten", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Das ist sein ganzes Gl\u00fcck;", "tokens": ["Das", "ist", "sein", "gan\u00b7zes", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr dich mu\u00df er gestorben sein,", "tokens": ["F\u00fcr", "dich", "mu\u00df", "er", "ge\u00b7stor\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er kehrt nicht mehr zur\u00fcck.", "tokens": ["Er", "kehrt", "nicht", "mehr", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn er mir die Treue brach,", "tokens": ["Und", "wenn", "er", "mir", "die", "Treu\u00b7e", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So will ich schlafen bei dir;", "tokens": ["So", "will", "ich", "schla\u00b7fen", "bei", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wir schlafen bis zum j\u00fcngsten Tag,", "tokens": ["Wir", "schla\u00b7fen", "bis", "zum", "j\u00fcng\u00b7sten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Deinen Schatten \u00fcber mir.", "tokens": ["Dei\u00b7nen", "Schat\u00b7ten", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Es wird dann bl\u00fchn auf meinem Grab", "tokens": ["Es", "wird", "dann", "bl\u00fchn", "auf", "mei\u00b7nem", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blume Vergi\u00dfnichtmein;", "tokens": ["Die", "Blu\u00b7me", "Ver\u00b7gi\u00df\u00b7nicht\u00b7mein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df ich ihn nicht vergessen hab,", "tokens": ["Da\u00df", "ich", "ihn", "nicht", "ver\u00b7ges\u00b7sen", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll sie ein Zeichen sein.", "tokens": ["Soll", "sie", "ein", "Zei\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}