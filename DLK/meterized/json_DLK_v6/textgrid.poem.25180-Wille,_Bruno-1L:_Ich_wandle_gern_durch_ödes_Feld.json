{"textgrid.poem.25180": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wandle gern durch \u00f6des Feld", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wandle gern durch \u00f6des Feld", "tokens": ["Ich", "wand\u00b7le", "gern", "durch", "\u00f6\u00b7des", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei abendkaltem Brausen.", "tokens": ["Bei", "a\u00b7bend\u00b7kal\u00b7tem", "Brau\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus Wolkenballen Dunkel f\u00e4llt,", "tokens": ["Aus", "Wol\u00b7ken\u00b7bal\u00b7len", "Dun\u00b7kel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stoppel\u00e4cker sausen,", "tokens": ["Die", "Stop\u00b7pel\u00b7\u00e4\u00b7cker", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Dornbusch duckt sich, zornumtost,", "tokens": ["Der", "Dorn\u00b7busch", "duckt", "sich", ",", "zorn\u00b7um\u00b7tost", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verdorrte Bl\u00e4tter erschauern...", "tokens": ["Ver\u00b7dorr\u00b7te", "Bl\u00e4t\u00b7ter", "er\u00b7schau\u00b7ern", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "O d\u00fcstrer Trost,", "tokens": ["O", "d\u00fcst\u00b7rer", "Trost", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Wenn Wolke, Busch und Haide mit mir trauern!", "tokens": ["Wenn", "Wol\u00b7ke", ",", "Busch", "und", "Hai\u00b7de", "mit", "mir", "trau\u00b7ern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NE", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}