{"textgrid.poem.31305": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Katharina von Frankreich", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sollt ihm Maine und Anjou", "tokens": ["Man", "sollt", "ihm", "Mai\u00b7ne", "und", "An\u00b7jou"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "NE", "KON", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00dcbergeben.", "tokens": ["\u00dc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was wei\u00df ich, was er alles", "tokens": ["Was", "wei\u00df", "ich", ",", "was", "er", "al\u00b7les"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWS", "PPER", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mocht erstreben.", "tokens": ["Mocht", "er\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und jetzt begehrt er nichts mehr,", "tokens": ["Und", "jetzt", "be\u00b7gehrt", "er", "nichts", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Als die eine \u2013", "tokens": ["Als", "die", "ei\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ihr Menschen, eine Brust her,", "tokens": ["Ihr", "Men\u00b7schen", ",", "ei\u00b7ne", "Brust", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df ich weine!", "tokens": ["Da\u00df", "ich", "wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}