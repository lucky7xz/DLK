{"textgrid.poem.31477": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Vberschw\u00e4nglichs Gut! O Vnausdenklichkeit!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vberschw\u00e4nglichs Gut! O Vnausdenklichkeit!", "tokens": ["O", "Vber\u00b7schw\u00e4ng\u00b7lichs", "Gut", "!", "O", "Vn\u00b7aus\u00b7denk\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "$.", "NE", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Begierigs Wunder-wol / an uns dich zu bezeigen!", "tokens": ["Be\u00b7gie\u00b7rigs", "Wun\u00b7der\u00b7wol", "/", "an", "uns", "dich", "zu", "be\u00b7zei\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "je mehr dein Werk in mir sich preiset / mu\u00df ich schweigen.", "tokens": ["je", "mehr", "dein", "Werk", "in", "mir", "sich", "prei\u00b7set", "/", "mu\u00df", "ich", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "APPR", "PPER", "PRF", "VVFIN", "$(", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Freud-Geniessung l\u00e4sst der R\u00fchmung keine Zeit.", "tokens": ["Die", "Freu\u00b7dGe\u00b7ni\u00b7es\u00b7sung", "l\u00e4sst", "der", "R\u00fch\u00b7mung", "kei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "War ist es / tausendmal das du viel mehr bereit /", "tokens": ["War", "ist", "es", "/", "tau\u00b7send\u00b7mal", "das", "du", "viel", "mehr", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VAFIN", "PPER", "$(", "ADV", "ART", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "als zu begehren ich / dein G\u00fct' in mich zuneigen.", "tokens": ["als", "zu", "be\u00b7geh\u00b7ren", "ich", "/", "dein", "G\u00fct'", "in", "mich", "zu\u00b7nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVINF", "PPER", "$(", "PPOSAT", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Strom schtest schnell herab / die Seuffzer langsam steigen:", "tokens": ["Dein", "Strom", "schtest", "schnell", "her\u00b7ab", "/", "die", "Seuff\u00b7zer", "lang\u00b7sam", "stei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADV", "$(", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ich kriech mit meiner Bitt / du fliegest mit der Freud.", "tokens": ["ich", "kriech", "mit", "mei\u00b7ner", "Bitt", "/", "du", "flie\u00b7gest", "mit", "der", "Freud", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du ausgesch\u00fctte Lieb / recht Herz-entdecktes Wesen!", "tokens": ["Du", "aus\u00b7ge\u00b7sch\u00fct\u00b7te", "Lieb", "/", "recht", "Her\u00b7zent\u00b7deck\u00b7tes", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wie wall-und flammestu / das du dich nur beweist!", "tokens": ["wie", "wall\u00b7\u00b7und", "flam\u00b7mes\u00b7tu", "/", "das", "du", "dich", "nur", "be\u00b7weist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "$(", "PRELS", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "du brichst schier / vor Begier da\u00df wir nur bald genesen.", "tokens": ["du", "brichst", "schier", "/", "vor", "Be\u00b7gier", "da\u00df", "wir", "nur", "bald", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "APPR", "NN", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie hungert doch die Gnad / bi\u00df wir mit Lust gespeist!", "tokens": ["Wie", "hun\u00b7gert", "doch", "die", "Gnad", "/", "bi\u00df", "wir", "mit", "Lust", "ge\u00b7speist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "dem Ewig-wohlen wohl ist schier zusagen weh /", "tokens": ["dem", "E\u00b7wig\u00b7woh\u00b7len", "wohl", "ist", "schier", "zu\u00b7sa\u00b7gen", "weh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "bi\u00df sein Lieb-\u00fcberflu\u00df auf uns Lust-\u00fcbergeh.", "tokens": ["bi\u00df", "sein", "Lie\u00b7b\u00fcber\u00b7flu\u00df", "auf", "uns", "Lust\u00b7\u00fcber\u00b7geh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "NE", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}