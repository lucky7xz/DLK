{"textgrid.poem.25386": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: In diesen N\u00e4chten friert mein Blut", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesen N\u00e4chten friert mein Blut", "tokens": ["In", "die\u00b7sen", "N\u00e4ch\u00b7ten", "friert", "mein", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nach deinem Leib, Geliebte.", "tokens": ["nach", "dei\u00b7nem", "Leib", ",", "Ge\u00b7lieb\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O, meine Sehnsucht ist wie dunkles Wasser", "tokens": ["O", ",", "mei\u00b7ne", "Sehn\u00b7sucht", "ist", "wie", "dunk\u00b7les", "Was\u00b7ser"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "VAFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "aufgestaut vor Schleusentoren,", "tokens": ["auf\u00b7ge\u00b7staut", "vor", "Schleu\u00b7sen\u00b7to\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In Mittagsstille hingelagert", "tokens": ["In", "Mit\u00b7tags\u00b7stil\u00b7le", "hin\u00b7ge\u00b7la\u00b7gert"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "reglos lauernd,", "tokens": ["reg\u00b7los", "lau\u00b7ernd", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Begierig, auszubrechen.", "tokens": ["Be\u00b7gie\u00b7rig", ",", "aus\u00b7zu\u00b7bre\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sommersturm,", "tokens": ["Som\u00b7mer\u00b7sturm", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Der schwer im Hinterhalt geladner Wolken h\u00e4lt.", "tokens": ["Der", "schwer", "im", "Hin\u00b7ter\u00b7halt", "ge\u00b7lad\u00b7ner", "Wol\u00b7ken", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wann kommst du, Blitz,", "tokens": ["Wann", "kommst", "du", ",", "Blitz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Der ihn entfacht,", "tokens": ["Der", "ihn", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "mit Lust befrachtet, F\u00e4hre,", "tokens": ["mit", "Lust", "be\u00b7frach\u00b7tet", ",", "F\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die weit der Wehre starre Schenkel", "tokens": ["Die", "weit", "der", "Weh\u00b7re", "star\u00b7re", "Schen\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "von sich sperrt? Ich will", "tokens": ["von", "sich", "sperrt", "?", "Ich", "will"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRF", "VVFIN", "$.", "PPER", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Dich zu mir in die Kissen tragen", "tokens": ["Dich", "zu", "mir", "in", "die", "Kis\u00b7sen", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "so wie Garben jungen Klees", "tokens": ["so", "wie", "Gar\u00b7ben", "jun\u00b7gen", "Klees"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "In aufgelockert Land.", "tokens": ["In", "auf\u00b7ge\u00b7lo\u00b7ckert", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Ich bin der G\u00e4rtner,", "tokens": ["Ich", "bin", "der", "G\u00e4rt\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Der weich dich niederbettet.", "tokens": ["Der", "weich", "dich", "nie\u00b7der\u00b7bet\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Wolke, die", "tokens": ["Wol\u00b7ke", ",", "die"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "PRELS"], "meter": "+--", "measure": "dactylic.init"}, "line.21": {"text": "Dich \u00fcbersprengt,", "tokens": ["Dich", "\u00fc\u00b7bers\u00b7prengt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "und Luft, die dich umschlie\u00dft.", "tokens": ["und", "Luft", ",", "die", "dich", "um\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "In deine Erde", "tokens": ["In", "dei\u00b7ne", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "will ich meine irre Glut vergraben und", "tokens": ["will", "ich", "mei\u00b7ne", "ir\u00b7re", "Glut", "ver\u00b7gra\u00b7ben", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "KON"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "Sehns\u00fcchtig bl\u00fchend", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "bl\u00fc\u00b7hend"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "\u00fcber deinem Leibe auferstehn.", "tokens": ["\u00fc\u00b7ber", "dei\u00b7nem", "Lei\u00b7be", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}