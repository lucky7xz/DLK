{"textgrid.poem.62708": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "In ein Stamm-Buch", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer das Gesetz-Buch derer Christen aus seinen Augen setzt, und will", "tokens": ["Wer", "das", "Ge\u00b7setz\u00b7Buch", "de\u00b7rer", "Chris\u00b7ten", "aus", "sei\u00b7nen", "Au\u00b7gen", "setzt", ",", "und", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "NN", "PDS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "VMFIN"], "meter": "-+-++--+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Sich gern an seinen Feinden r\u00e4chen, der rathe ihm beherzt und still:", "tokens": ["Sich", "gern", "an", "sei\u00b7nen", "Fein\u00b7den", "r\u00e4\u00b7chen", ",", "der", "ra\u00b7the", "ihm", "be\u00b7herzt", "und", "still", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Er soll ein Alchymiste werden: Und prei\u00df ihm eine Gattin an,", "tokens": ["Er", "soll", "ein", "Al\u00b7chy\u00b7mis\u00b7te", "wer\u00b7den", ":", "Und", "prei\u00df", "ihm", "ei\u00b7ne", "Gat\u00b7tin", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$.", "KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Die keine Tugenden besitzet; so hat er schon genug gethan.", "tokens": ["Die", "kei\u00b7ne", "Tu\u00b7gen\u00b7den", "be\u00b7sit\u00b7zet", ";", "so", "hat", "er", "schon", "ge\u00b7nug", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}