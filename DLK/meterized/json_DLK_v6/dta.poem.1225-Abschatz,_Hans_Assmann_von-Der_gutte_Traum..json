{"dta.poem.1225": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der gutte Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gl\u00fccke lacht/", "tokens": ["Mein", "Gl\u00fc\u00b7cke", "lacht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Melinde spielt mit angenehmen Blicken/", "tokens": ["Me\u00b7lin\u00b7de", "spielt", "mit", "an\u00b7ge\u00b7neh\u00b7men", "Bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr holder Mund giebt Worte/ die entz\u00fccken/", "tokens": ["Ihr", "hol\u00b7der", "Mund", "giebt", "Wor\u00b7te", "/", "die", "ent\u00b7z\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "$(", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich k\u00fcsse sie bey tunckler Mitternacht/", "tokens": ["Ich", "k\u00fcs\u00b7se", "sie", "bey", "tunck\u00b7ler", "Mit\u00b7ter\u00b7nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein Gl\u00fccke lacht.", "tokens": ["Mein", "Gl\u00fc\u00b7cke", "lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mir traumt wohl nicht:", "tokens": ["Mir", "traumt", "wohl", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich seh ihr Bild um meine Ruhstatt spielen/", "tokens": ["Ich", "seh", "ihr", "Bild", "um", "mei\u00b7ne", "Ruh\u00b7statt", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6r ihre Sprach/ und misse nichts als F\u00fchlen.", "tokens": ["H\u00f6r", "ih\u00b7re", "Sprach", "/", "und", "mis\u00b7se", "nichts", "als", "F\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach Schade/ da\u00df das Beste noch gebricht!", "tokens": ["Ach", "Scha\u00b7de", "/", "da\u00df", "das", "Bes\u00b7te", "noch", "ge\u00b7bricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "$(", "KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir traumt wohl nicht.", "tokens": ["Mir", "traumt", "wohl", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Es wird wohl seyn:", "tokens": ["Es", "wird", "wohl", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Hoffnung speist nicht stets mit leeren Schalen.", "tokens": ["Die", "Hoff\u00b7nung", "speist", "nicht", "stets", "mit", "lee\u00b7ren", "Scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erblickt man nur der Morgenr\u00f6the Stralen/", "tokens": ["Er\u00b7blickt", "man", "nur", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Stra\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So folget auch der nahen Sonne Schein.", "tokens": ["So", "fol\u00b7get", "auch", "der", "na\u00b7hen", "Son\u00b7ne", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es wird wohl seyn.", "tokens": ["Es", "wird", "wohl", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}