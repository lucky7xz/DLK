{"textgrid.poem.35783": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbes rinnen alle Bronnen", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbes rinnen alle Bronnen", "tokens": ["\u00bb", "es", "rin\u00b7nen", "al\u00b7le", "Bron\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zusammen in das Meer,", "tokens": ["Zu\u00b7sam\u00b7men", "in", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn sie hin geronnen,", "tokens": ["Und", "wenn", "sie", "hin", "ge\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So kehren sie daher.\u00ab", "tokens": ["So", "keh\u00b7ren", "sie", "da\u00b7her", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne kommt und schwindet,", "tokens": ["Die", "Son\u00b7ne", "kommt", "und", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist stets dasselbe Licht,", "tokens": ["Ist", "stets", "das\u00b7sel\u00b7be", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob sie uns fr\u00f6hlich findet,", "tokens": ["Ob", "sie", "uns", "fr\u00f6h\u00b7lich", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob unser Auge bricht.", "tokens": ["Ob", "un\u00b7ser", "Au\u00b7ge", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dieselben Sterne scheinen", "tokens": ["Die\u00b7sel\u00b7ben", "Ster\u00b7ne", "schei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns wieder jede Nacht,", "tokens": ["Uns", "wie\u00b7der", "je\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob wir in Jammer weinen,", "tokens": ["Ob", "wir", "in", "Jam\u00b7mer", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob sel'ger Traum uns lacht.", "tokens": ["Ob", "sel'\u00b7ger", "Traum", "uns", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es f\u00fchren uns die Jahre", "tokens": ["Es", "f\u00fch\u00b7ren", "uns", "die", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Freude und durch Graus;", "tokens": ["Durch", "Freu\u00b7de", "und", "durch", "Graus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heut Wiege, morgen Bahre", "tokens": ["Heut", "Wie\u00b7ge", ",", "mor\u00b7gen", "Bah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist unser Schlummerhaus.", "tokens": ["Ist", "un\u00b7ser", "Schlum\u00b7mer\u00b7haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So la\u00df uns heiter gr\u00fc\u00dfen", "tokens": ["So", "la\u00df", "uns", "hei\u00b7ter", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Leben wie den Tod", "tokens": ["Das", "Le\u00b7ben", "wie", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fromm die Augen schlie\u00dfen", "tokens": ["Und", "fromm", "die", "Au\u00b7gen", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im letzten Abendroth.", "tokens": ["Im", "letz\u00b7ten", "A\u00b7ben\u00b7droth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir werden auferstehen", "tokens": ["Wir", "wer\u00b7den", "auf\u00b7er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im neuen Morgenglanz", "tokens": ["Im", "neu\u00b7en", "Mor\u00b7gen\u00b7glanz"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Ihn, den Vater, sehen", "tokens": ["Und", "Ihn", ",", "den", "Va\u00b7ter", ",", "se\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im ew'gen Strahlenkranz.", "tokens": ["Im", "ew'\u00b7gen", "Strah\u00b7len\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}