{"textgrid.poem.36430": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sechstes Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich flehe dich um Wunden und um Male", "tokens": ["Ich", "fle\u00b7he", "dich", "um", "Wun\u00b7den", "und", "um", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von deinen H\u00e4nden, die mich heilig sprechen.", "tokens": ["Von", "dei\u00b7nen", "H\u00e4n\u00b7den", ",", "die", "mich", "hei\u00b7lig", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sollst das Glied, das du gesaugt, zerbrechen.", "tokens": ["Du", "sollst", "das", "Glied", ",", "das", "du", "ge\u00b7saugt", ",", "zer\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das steif geragt in deine Kathedrale.", "tokens": ["Das", "steif", "ge\u00b7ragt", "in", "dei\u00b7ne", "Ka\u00b7thed\u00b7ra\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Schl\u00fcrf' aus den Quell, der einst in wei\u00dfen B\u00e4chen", "tokens": ["Schl\u00fcr\u00b7f'", "aus", "den", "Quell", ",", "der", "einst", "in", "wei\u00b7\u00dfen", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deinen Kelch gespritzt beim Bachanale! ...", "tokens": ["In", "dei\u00b7nen", "Kelch", "ge\u00b7spritzt", "beim", "Ba\u00b7cha\u00b7na\u00b7le", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gie\u00df jetzt die letzte Kraft in deine Schale.", "tokens": ["Gie\u00df", "jetzt", "die", "letz\u00b7te", "Kraft", "in", "dei\u00b7ne", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An meinem Blute magst du dich bezechen! ...", "tokens": ["An", "mei\u00b7nem", "Blu\u00b7te", "magst", "du", "dich", "be\u00b7ze\u00b7chen", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nimm scharfe Peitschen und gegl\u00fchte Zwingen.", "tokens": ["Nimm", "schar\u00b7fe", "Peit\u00b7schen", "und", "ge\u00b7gl\u00fch\u00b7te", "Zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlag' fester zu und qu\u00e4le meine Hoden! ...", "tokens": ["Schlag'", "fes\u00b7ter", "zu", "und", "qu\u00e4\u00b7le", "mei\u00b7ne", "Ho\u00b7den", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df tiefsten Schmerz das h\u00f6chste Gl\u00fcck mir bringen.", "tokens": ["La\u00df", "tiefs\u00b7ten", "Schmerz", "das", "h\u00f6chs\u00b7te", "Gl\u00fcck", "mir", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein St\u00f6hnen preist dich br\u00fcnstiger als meine Oden.", "tokens": ["Mein", "St\u00f6h\u00b7nen", "preist", "dich", "br\u00fcns\u00b7ti\u00b7ger", "als", "mei\u00b7ne", "O\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn die letzten Schreie dich umklingen", "tokens": ["Und", "wenn", "die", "letz\u00b7ten", "Schrei\u00b7e", "dich", "um\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6rst du den Dank vom seligen Rhapsoden.", "tokens": ["H\u00f6rst", "du", "den", "Dank", "vom", "se\u00b7li\u00b7gen", "Rhap\u00b7so\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}}}}