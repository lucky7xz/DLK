{"textgrid.poem.56736": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Immer nicht an Mond und Sterne", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer nicht an Mond und Sterne", "tokens": ["Im\u00b7mer", "nicht", "an", "Mond", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mag ich meine Blicke h\u00e4ngen \u2013:", "tokens": ["mag", "ich", "mei\u00b7ne", "Bli\u00b7cke", "h\u00e4n\u00b7gen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach man kann mit Mond und Sternen,", "tokens": ["Ach", "man", "kann", "mit", "Mond", "und", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolken, Felsen, W\u00e4ldern, B\u00e4chen", "tokens": ["Wol\u00b7ken", ",", "Fel\u00b7sen", ",", "W\u00e4l\u00b7dern", ",", "B\u00e4\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "allzuleichtlich kokettieren,", "tokens": ["all\u00b7zu\u00b7leicht\u00b7lich", "ko\u00b7ket\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "hat man solch ein schelmisch Weibchen", "tokens": ["hat", "man", "solch", "ein", "schel\u00b7misch", "Weib\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "PIAT", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "stets um sich wie Phanta Sia.", "tokens": ["stets", "um", "sich", "wie", "Phan\u00b7ta", "Sia", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "KOKOM", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Darum senk ich heut bescheiden", "tokens": ["Da\u00b7rum", "senk", "ich", "heut", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meine Augen in die Tiefe.", "tokens": ["mei\u00b7ne", "Au\u00b7gen", "in", "die", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier und da ein H\u00fcttenlichtlein;", "tokens": ["Hier", "und", "da", "ein", "H\u00fct\u00b7ten\u00b7licht\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "auch ein Feuer, dran sich Hirten", "tokens": ["auch", "ein", "Feu\u00b7er", ",", "dran", "sich", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PAV", "PRF", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "n\u00e4chtliche Kartoffeln braten \u2013", "tokens": ["n\u00e4cht\u00b7li\u00b7che", "Kar\u00b7tof\u00b7feln", "bra\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wenig sonst im dunklen Grunde.", "tokens": ["we\u00b7nig", "sonst", "im", "dunk\u00b7len", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch! da drunten seh ich eine", "tokens": ["Doch", "!", "da", "drun\u00b7ten", "seh", "ich", "ei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$.", "ADV", "ADV", "VVFIN", "PPER", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "goldgeschuppte Schlange kriechen ...", "tokens": ["gold\u00b7ge\u00b7schupp\u00b7te", "Schlan\u00b7ge", "krie\u00b7chen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hochromantisches Ersp\u00e4hnis!", "tokens": ["Hoc\u00b7hro\u00b7man\u00b7ti\u00b7sches", "Er\u00b7sp\u00e4h\u00b7nis", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommst du wieder, trautes Gestern,", "tokens": ["Kommst", "du", "wie\u00b7der", ",", "trau\u00b7tes", "Ge\u00b7stern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "da die Drachen mit den K\u00fchen", "tokens": ["da", "die", "Dra\u00b7chen", "mit", "den", "K\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "friedlich auf den Almen grasten,", "tokens": ["fried\u00b7lich", "auf", "den", "Al\u00b7men", "gras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn sie nicht grad Flammen speien", "tokens": ["wenn", "sie", "nicht", "grad", "Flam\u00b7men", "spei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "oder Ritter fressen mu\u00dften \u2013", "tokens": ["o\u00b7der", "Rit\u00b7ter", "fres\u00b7sen", "mu\u00df\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "da der Lindwurm in den Engpa\u00df", "tokens": ["da", "der", "Lind\u00b7wurm", "in", "den", "Eng\u00b7pa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-++-++-", "measure": "unknown.measure.penta"}, "line.8": {"text": "seinen Boa-Hals hinabhing", "tokens": ["sei\u00b7nen", "Boa\u00b7Hals", "hi\u00b7nab\u00b7hing"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "und mit gr\u00fcnem Augenaufschlag", "tokens": ["und", "mit", "gr\u00fc\u00b7nem", "Au\u00b7gen\u00b7auf\u00b7schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dame, Knapp und Maultier schmauste \u2013", "tokens": ["Da\u00b7me", ",", "Knapp", "und", "Maul\u00b7tier", "schmaus\u00b7te", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "kommst du wieder, trautes Gestern?", "tokens": ["kommst", "du", "wie\u00b7der", ",", "trau\u00b7tes", "Ge\u00b7stern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Eitle Frage! Dieses Schuppen-", "tokens": ["Eit\u00b7le", "Fra\u00b7ge", "!", "Die\u00b7ses", "Schup\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PDAT", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unget\u00fcm da drunten ist ein", "tokens": ["Un\u00b7ge\u00b7t\u00fcm", "da", "drun\u00b7ten", "ist", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "VAFIN", "ART"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "ganz modernes Fabelwesen,", "tokens": ["ganz", "mo\u00b7der\u00b7nes", "Fa\u00b7bel\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "uners\u00e4ttlich zwar, wie jene", "tokens": ["un\u00b7er\u00b7s\u00e4tt\u00b7lich", "zwar", ",", "wie", "je\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADV", "$,", "PWAV", "PDS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "alten Schlangen, doch auch wieder", "tokens": ["al\u00b7ten", "Schlan\u00b7gen", ",", "doch", "auch", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "jenem braven Walfisch \u00e4hnlich,", "tokens": ["je\u00b7nem", "bra\u00b7ven", "Wal\u00b7fisch", "\u00e4hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "der dem Jonas nur auf Tage", "tokens": ["der", "dem", "Jo\u00b7nas", "nur", "auf", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "seinen Bauch zur Herberg anbot.", "tokens": ["sei\u00b7nen", "Bauch", "zur", "Her\u00b7berg", "an\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Feuerwurm, ich gr\u00fc\u00dfe froh dich", "tokens": ["Feu\u00b7er\u00b7wurm", ",", "ich", "gr\u00fc\u00b7\u00dfe", "froh", "dich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJD", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "von den Stufen meines Schlosses!", "tokens": ["von", "den", "Stu\u00b7fen", "mei\u00b7nes", "Schlos\u00b7ses", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ob mancher dich auch schm\u00e4he,", "tokens": ["Denn", "ob", "man\u00b7cher", "dich", "auch", "schm\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "als den St\u00f6rer stiller Lande,", "tokens": ["als", "den", "St\u00f6\u00b7rer", "stil\u00b7ler", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und die gelben Humpeldrachen,", "tokens": ["und", "die", "gel\u00b7ben", "Hum\u00b7pel\u00b7dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "die noch bliesen, noch nicht pfiffen,", "tokens": ["die", "noch", "blie\u00b7sen", ",", "noch", "nicht", "pfif\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "wiederw\u00fcnschte, \u2013 ich bekenne,", "tokens": ["wie\u00b7der\u00b7w\u00fcnschte", ",", "\u2013", "ich", "be\u00b7ken\u00b7ne", ","], "token_info": ["word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "$(", "PPER", "VVFIN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "da\u00df ich stolz bin, dich zu schauen.", "tokens": ["da\u00df", "ich", "stolz", "bin", ",", "dich", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00f6her schl\u00e4gt mir oft das Herze,", "tokens": ["H\u00f6\u00b7her", "schl\u00e4gt", "mir", "oft", "das", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "seh ich dich auf schmalen Pfaden", "tokens": ["seh", "ich", "dich", "auf", "schma\u00b7len", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "deine Wucht in leichter Grazie", "tokens": ["dei\u00b7ne", "Wucht", "in", "leich\u00b7ter", "Gra\u00b7zie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "mit dem Flug der V\u00f6gel messen", "tokens": ["mit", "dem", "Flug", "der", "V\u00f6\u00b7gel", "mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "und mit Triumphatorpose", "tokens": ["und", "mit", "Tri\u00b7um\u00b7pha\u00b7tor\u00b7po\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "hallend durch die N\u00e4chte tragen.", "tokens": ["hal\u00b7lend", "durch", "die", "N\u00e4ch\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sinnbild bist du mir und Gleichnis", "tokens": ["Sinn\u00b7bild", "bist", "du", "mir", "und", "Gleich\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPER", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geistessiegs ob Stoffestr\u00e4gheit!", "tokens": ["Geis\u00b7tes\u00b7siegs", "ob", "Stof\u00b7fes\u00b7tr\u00e4g\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleichnis bist du neuer Zeit mir,", "tokens": ["Gleich\u00b7nis", "bist", "du", "neu\u00b7er", "Zeit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die, jahrtausendalter Kr\u00e4fte", "tokens": ["die", ",", "jahr\u00b7tau\u00b7sen\u00b7dal\u00b7ter", "Kr\u00e4f\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["ART", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Erbin, Sammlerin, sie spielend", "tokens": ["Er\u00b7bin", ",", "Samm\u00b7le\u00b7rin", ",", "sie", "spie\u00b7lend"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "zwingt und formt, beherrscht und leitet!", "tokens": ["zwingt", "und", "formt", ",", "be\u00b7herrscht", "und", "lei\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Andre Zeiten, andre Drachen,", "tokens": ["And\u00b7re", "Zei\u00b7ten", ",", "and\u00b7re", "Dra\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "andre Drachen, andre M\u00e4rchen,", "tokens": ["and\u00b7re", "Dra\u00b7chen", ",", "and\u00b7re", "M\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "andre M\u00e4rchen, andre M\u00fctter,", "tokens": ["and\u00b7re", "M\u00e4r\u00b7chen", ",", "and\u00b7re", "M\u00fct\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "andre M\u00fctter, andre Jugend,", "tokens": ["and\u00b7re", "M\u00fct\u00b7ter", ",", "and\u00b7re", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "