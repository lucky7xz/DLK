{"dta.poem.3158": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Postbot' in ein Haus mit zweien Briefen rennt,", "tokens": ["Der", "Post\u00b7bot'", "in", "ein", "Haus", "mit", "zwei\u00b7en", "Brie\u00b7fen", "rennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versiegelten, davon er nicht den Inhalt kennt.", "tokens": ["Ver\u00b7sie\u00b7gel\u00b7ten", ",", "da\u00b7von", "er", "nicht", "den", "In\u00b7halt", "kennt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Er merkt auch nicht darauf, ob au\u00dfen in den Siegeln", "tokens": ["Er", "merkt", "auch", "nicht", "da\u00b7rauf", ",", "ob", "au\u00b7\u00dfen", "in", "den", "Sie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PAV", "$,", "KOUS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich die Verschiedenheit des Inhalts m\u00f6ge spiegeln.", "tokens": ["Sich", "die", "Ver\u00b7schie\u00b7den\u00b7heit", "des", "In\u00b7halts", "m\u00f6\u00b7ge", "spie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der eine freudenroth gesiegelt meldet Lust,", "tokens": ["Der", "ei\u00b7ne", "freu\u00b7den\u00b7roth", "ge\u00b7sie\u00b7gelt", "mel\u00b7det", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "VVPP", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der andre trauerschwarz verk\u00fcndiget Verlust.", "tokens": ["Der", "and\u00b7re", "trau\u00b7er\u00b7schwarz", "ver\u00b7k\u00fcn\u00b7di\u00b7get", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Bote gibt sie ab, nimmt daf\u00fcr in Empfang", "tokens": ["Der", "Bo\u00b7te", "gibt", "sie", "ab", ",", "nimmt", "da\u00b7f\u00fcr", "in", "Emp\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PAV", "APPR", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Lohn, und setzet fort gleichg\u00fcltig seinen Gang.", "tokens": ["Den", "Lohn", ",", "und", "set\u00b7zet", "fort", "gleich\u00b7g\u00fcl\u00b7tig", "sei\u00b7nen", "Gang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PTKVZ", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er fragt nicht, wie sie nun sich werden hier vertragen,", "tokens": ["Er", "fragt", "nicht", ",", "wie", "sie", "nun", "sich", "wer\u00b7den", "hier", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "PRF", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ja vertr\u00e4glich auch im Postfelleisen lagen.", "tokens": ["Die", "ja", "ver\u00b7tr\u00e4g\u00b7lich", "auch", "im", "Post\u00b7fel\u00b7lei\u00b7sen", "la\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So bringt das Schicksal oft zusammen Lust und Schmerz,", "tokens": ["So", "bringt", "das", "Schick\u00b7sal", "oft", "zu\u00b7sam\u00b7men", "Lust", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fragt nicht, wie sie sich vertragen um ein Herz.", "tokens": ["Und", "fragt", "nicht", ",", "wie", "sie", "sich", "ver\u00b7tra\u00b7gen", "um", "ein", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}