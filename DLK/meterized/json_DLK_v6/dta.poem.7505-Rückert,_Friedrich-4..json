{"dta.poem.7505": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Furcht vor keinem hegt, Furcht keinem auch erregt,", "tokens": ["Wer", "Furcht", "vor", "kei\u00b7nem", "hegt", ",", "Furcht", "kei\u00b7nem", "auch", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PIS", "VVFIN", "$,", "NN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht den furchtbaren Tod von keiner Furcht bewegt.", "tokens": ["Sieht", "den", "furcht\u00b7ba\u00b7ren", "Tod", "von", "kei\u00b7ner", "Furcht", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Wer keine Luft verst\u00f6rt, wen keine Luft beth\u00f6rt,", "tokens": ["Wer", "kei\u00b7ne", "Luft", "ver\u00b7st\u00f6rt", ",", "wen", "kei\u00b7ne", "Luft", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,", "PWS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erlangt die h\u00f6chste Luft, wo alle Luft aufh\u00f6rt.", "tokens": ["Er\u00b7langt", "die", "h\u00f6chs\u00b7te", "Luft", ",", "wo", "al\u00b7le", "Luft", "auf\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wem hoch und niedrig gleich, gleichviel ist hart und weich,", "tokens": ["Wem", "hoch", "und", "nied\u00b7rig", "gleich", ",", "gleich\u00b7viel", "ist", "hart", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "ADV", "$,", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleichgiltig reich und arm, der ist in Armuth reich.", "tokens": ["Gleich\u00b7gil\u00b7tig", "reich", "und", "arm", ",", "der", "ist", "in", "Ar\u00b7muth", "reich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$,", "PRELS", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Wer Lieb' mit Lieb' umfa\u00dft, und selbst den Ha\u00df nicht ha\u00dft,", "tokens": ["Wer", "Lieb'", "mit", "Lieb'", "um\u00b7fa\u00dft", ",", "und", "selbst", "den", "Ha\u00df", "nicht", "ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "VVFIN", "$,", "KON", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ist zu Hause dort, hier auf der Welt ein Gast.", "tokens": ["Der", "ist", "zu", "Hau\u00b7se", "dort", ",", "hier", "auf", "der", "Welt", "ein", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ADV", "$,", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}