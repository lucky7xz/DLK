{"textgrid.poem.58037": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "75. Wer verliert, der findet", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo du dich find'st, da mu\u00dft du immer dich verlieren", "tokens": ["Wo", "du", "dich", "fin\u00b7d'st", ",", "da", "mu\u00dft", "du", "im\u00b7mer", "dich", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Und alle Lust und Lieb' allein in Got einf\u00fchren;", "tokens": ["Und", "al\u00b7le", "Lust", "und", "Lieb'", "al\u00b7lein", "in", "Got", "ein\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je treuer du verlierst, je mehr du stets gewinnst", "tokens": ["Je", "treu\u00b7er", "du", "ver\u00b7lierst", ",", "je", "mehr", "du", "stets", "ge\u00b7winnst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und, wenn du gar bist arm, in Gott viel Reichtum find'st.", "tokens": ["Und", ",", "wenn", "du", "gar", "bist", "arm", ",", "in", "Gott", "viel", "Reich\u00b7tum", "find'", "st."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "VAFIN", "ADJD", "$,", "APPR", "NN", "PIAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}