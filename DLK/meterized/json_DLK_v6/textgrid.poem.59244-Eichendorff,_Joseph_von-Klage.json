{"textgrid.poem.59244": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Klage", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O k\u00f6nnt ich mich niederlegen", "tokens": ["O", "k\u00f6nnt", "ich", "mich", "nie\u00b7der\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit in den tiefsten Wald,", "tokens": ["Weit", "in", "den", "tiefs\u00b7ten", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu H\u00e4upten den guten Degen,", "tokens": ["Zu", "H\u00e4up\u00b7ten", "den", "gu\u00b7ten", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der noch von den V\u00e4tern alt,", "tokens": ["Der", "noch", "von", "den", "V\u00e4\u00b7tern", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und d\u00fcrft von allem nichts sp\u00fcren", "tokens": ["Und", "d\u00fcrft", "von", "al\u00b7lem", "nichts", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PIS", "PIS", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In dieser dummen Zeit,", "tokens": ["In", "die\u00b7ser", "dum\u00b7men", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was sie da unten hantieren,", "tokens": ["Was", "sie", "da", "un\u00b7ten", "han\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von Gott verlassen, zerstreut;", "tokens": ["Von", "Gott", "ver\u00b7las\u00b7sen", ",", "zer\u00b7streut", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Von f\u00fcrstlichen Taten und Werken,", "tokens": ["Von", "f\u00fcrst\u00b7li\u00b7chen", "Ta\u00b7ten", "und", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von alter Ehre und Pracht,", "tokens": ["Von", "al\u00b7ter", "Eh\u00b7re", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und was die Seele mag st\u00e4rken,", "tokens": ["Und", "was", "die", "See\u00b7le", "mag", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vertr\u00e4umend die lange Nacht!", "tokens": ["Ver\u00b7tr\u00e4u\u00b7mend", "die", "lan\u00b7ge", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Denn eine Zeit wird kommen,", "tokens": ["Denn", "ei\u00b7ne", "Zeit", "wird", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da macht der Herr ein End,", "tokens": ["Da", "macht", "der", "Herr", "ein", "End", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wird den Falschen genommen", "tokens": ["Da", "wird", "den", "Fal\u00b7schen", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr unechtes Regiment.", "tokens": ["Ihr", "un\u00b7ech\u00b7tes", "Re\u00b7gi\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn wie die Erze vom Hammer,", "tokens": ["Denn", "wie", "die", "Er\u00b7ze", "vom", "Ham\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So wird das lockre Geschlecht", "tokens": ["So", "wird", "das", "lock\u00b7re", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gehaun sein von Not und Jammer", "tokens": ["Ge\u00b7haun", "sein", "von", "Not", "und", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAINF", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu festem Eisen recht.", "tokens": ["Zu", "fes\u00b7tem", "Ei\u00b7sen", "recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da wird Aurora tagen", "tokens": ["Da", "wird", "Au\u00b7ro\u00b7ra", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hoch \u00fcber den Wald hinauf,", "tokens": ["Hoch", "\u00fc\u00b7ber", "den", "Wald", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da gibt's was zu singen und schlagen,", "tokens": ["Da", "gibt's", "was", "zu", "sin\u00b7gen", "und", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da wacht, ihr Getreuen, auf.", "tokens": ["Da", "wacht", ",", "ihr", "Ge\u00b7treu\u00b7en", ",", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}