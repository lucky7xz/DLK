{"textgrid.poem.24106": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Schmerz ist ein Schmied.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schmerz ist ein Schmied.", "tokens": ["Der", "Schmerz", "ist", "ein", "Schmied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Sein Hammer ist hart;", "tokens": ["Sein", "Ham\u00b7mer", "ist", "hart", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Von fliegenden Flammen", "tokens": ["Von", "flie\u00b7gen\u00b7den", "Flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ist hei\u00df sein Heerd;", "tokens": ["Ist", "hei\u00df", "sein", "Heerd", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Seinen Blasebalg bl\u00e4ht", "tokens": ["Sei\u00b7nen", "Bla\u00b7se\u00b7balg", "bl\u00e4ht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Ein sto\u00dfender Sturm", "tokens": ["Ein", "sto\u00b7\u00dfen\u00b7der", "Sturm"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Von wilden Gewalten.", "tokens": ["Von", "wil\u00b7den", "Ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Er h\u00e4mmert die Herzen", "tokens": ["Er", "h\u00e4m\u00b7mert", "die", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Und schweist sie mit schweren", "tokens": ["Und", "schweist", "sie", "mit", "schwe\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Und harten Hieben", "tokens": ["Und", "har\u00b7ten", "Hie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Zu festem Gef\u00fcge.", "tokens": ["Zu", "fes\u00b7tem", "Ge\u00b7f\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Gut, gut schmiedet der Schmerz.", "tokens": ["Gut", ",", "gut", "schmie\u00b7det", "der", "Schmerz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Kein Sturm zerst\u00f6rt,", "tokens": ["Kein", "Sturm", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Kein Frost zerfri\u00dft,", "tokens": ["Kein", "Frost", "zer\u00b7fri\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Kein Rost zerrei\u00dft,", "tokens": ["Kein", "Rost", "zer\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Was der Schmerz geschmiedet.", "tokens": ["Was", "der", "Schmerz", "ge\u00b7schmie\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}