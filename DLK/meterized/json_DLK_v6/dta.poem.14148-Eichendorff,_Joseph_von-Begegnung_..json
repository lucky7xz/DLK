{"dta.poem.14148": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Begegnung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Am Dorfe dann auf gr\u00fcner Au", "tokens": ["Am", "Dor\u00b7fe", "dann", "auf", "gr\u00fc\u00b7ner", "Au"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begannen sie den Reigen", "tokens": ["Be\u00b7gan\u00b7nen", "sie", "den", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und durch den Schall der Geigen", "tokens": ["Und", "durch", "den", "Schall", "der", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lacht' laut die junge Frau,", "tokens": ["Lacht'", "laut", "die", "jun\u00b7ge", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Stimmlein klang so eigen,", "tokens": ["Ihr", "Stimm\u00b7lein", "klang", "so", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich wu\u00dfte nicht, wie mir gescheh'n \u2014", "tokens": ["Ich", "wu\u00df\u00b7te", "nicht", ",", "wie", "mir", "ge\u00b7scheh'n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da wandt' sie sich in wildem Dreh'n.", "tokens": ["Da", "wandt'", "sie", "sich", "in", "wil\u00b7dem", "Dreh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}