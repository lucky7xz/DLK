{"textgrid.poem.25225": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gedenke mein, wenn Morgenrot die Tore", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedenke mein, wenn Morgenrot die Tore", "tokens": ["Ge\u00b7den\u00b7ke", "mein", ",", "wenn", "Mor\u00b7gen\u00b7rot", "die", "To\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Throngemach der Sonne leis erschlie\u00dft;", "tokens": ["Zum", "Thron\u00b7ge\u00b7mach", "der", "Son\u00b7ne", "leis", "er\u00b7schlie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gedenke mein, wenn dir im Sternenflore", "tokens": ["Ge\u00b7den\u00b7ke", "mein", ",", "wenn", "dir", "im", "Ster\u00b7nen\u00b7flo\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die feierstille Nacht vor\u00fcberflie\u00dft;", "tokens": ["Die", "fei\u00b7er\u00b7stil\u00b7le", "Nacht", "vor\u00b7\u00fc\u00b7berf\u00b7lie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn bei der Freude Ruf die Pulse rascher fliegen,", "tokens": ["Wenn", "bei", "der", "Freu\u00b7de", "Ruf", "die", "Pul\u00b7se", "ra\u00b7scher", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn Abendschatten dich in sanfte Tr\u00e4ume wiegen.", "tokens": ["Wenn", "A\u00b7bend\u00b7schat\u00b7ten", "dich", "in", "sanf\u00b7te", "Tr\u00e4u\u00b7me", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O geh hinaus, zu lauschen,", "tokens": ["O", "geh", "hin\u00b7aus", ",", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was W\u00e4lder heimlich rauschen:", "tokens": ["Was", "W\u00e4l\u00b7der", "heim\u00b7lich", "rau\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gedenke mein!", "tokens": ["Ge\u00b7den\u00b7ke", "mein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Gedenke mein, wenn das Gebot der Sterne", "tokens": ["Ge\u00b7den\u00b7ke", "mein", ",", "wenn", "das", "Ge\u00b7bot", "der", "Ster\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus diesem Arm dich unerbittlich wand;", "tokens": ["Aus", "die\u00b7sem", "Arm", "dich", "un\u00b7er\u00b7bitt\u00b7lich", "wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn mich das Heimweh in der kalten Ferne", "tokens": ["Wenn", "mich", "das", "Heim\u00b7weh", "in", "der", "kal\u00b7ten", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach dir verzehrt, du einzig Heimatland.", "tokens": ["Nach", "dir", "ver\u00b7zehrt", ",", "du", "ein\u00b7zig", "Hei\u00b7mat\u00b7land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denk an mein Lebewohl, an unsre Z\u00e4hrenfluten;", "tokens": ["Denk", "an", "mein", "Le\u00b7be\u00b7wohl", ",", "an", "uns\u00b7re", "Z\u00e4h\u00b7ren\u00b7flu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht Meere zwischen uns ersticken treue Gluten,", "tokens": ["Nicht", "Mee\u00b7re", "zwi\u00b7schen", "uns", "er\u00b7sti\u00b7cken", "treu\u00b7e", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPR", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und meines Herzens Schlagen", "tokens": ["Und", "mei\u00b7nes", "Her\u00b7zens", "Schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Soll zuckend noch dir sagen:", "tokens": ["Soll", "zu\u00b7ckend", "noch", "dir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gedenke mein!", "tokens": ["Ge\u00b7den\u00b7ke", "mein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Gedenke mein, wenn in der Erdenk\u00fchle", "tokens": ["Ge\u00b7den\u00b7ke", "mein", ",", "wenn", "in", "der", "Er\u00b7den\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich tr\u00e4umend ruh, und eine Blume sprie\u00dft", "tokens": ["Ich", "tr\u00e4u\u00b7mend", "ruh", ",", "und", "ei\u00b7ne", "Blu\u00b7me", "sprie\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Einsam und z\u00e4rtlich aus dem Rasenpf\u00fchle;", "tokens": ["Ein\u00b7sam", "und", "z\u00e4rt\u00b7lich", "aus", "dem", "Ra\u00b7sen\u00b7pf\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Du ahnest, was die Knospe keusch umschlie\u00dft.", "tokens": ["Du", "ah\u00b7nest", ",", "was", "die", "Knos\u00b7pe", "keusch", "um\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein Auge sieht mich nicht, doch soll geheimes Leben,", "tokens": ["Dein", "Au\u00b7ge", "sieht", "mich", "nicht", ",", "doch", "soll", "ge\u00b7hei\u00b7mes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein treuer Schwestergeist, dem Blumenkelch entschweben", "tokens": ["Ein", "treu\u00b7er", "Schwes\u00b7ter\u00b7geist", ",", "dem", "Blu\u00b7men\u00b7kelch", "ent\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und horch, in Nacht und Schweigen", "tokens": ["Und", "horch", ",", "in", "Nacht", "und", "Schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu dir sich seufzend neigen:", "tokens": ["Zu", "dir", "sich", "seuf\u00b7zend", "nei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gedenke mein!", "tokens": ["Ge\u00b7den\u00b7ke", "mein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}