{"dta.poem.2962": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Lorberkranz ist, wo er dir erscheint, ein Zeichen", "tokens": ["\u201e", "der", "Lor\u00b7ber\u00b7kranz", "ist", ",", "wo", "er", "dir", "er\u00b7scheint", ",", "ein", "Zei\u00b7chen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "VAFIN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Leidens mehr als Gl\u00fccks.\u201c La\u00df dir zum Troste reichen,", "tokens": ["Des", "Lei\u00b7dens", "mehr", "als", "Gl\u00fccks", ".", "\u201c", "La\u00df", "dir", "zum", "Tros\u00b7te", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOUS", "NN", "$.", "$(", "VVIMP", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn es dich tr\u00f6sten kann, des alten Meisters Wort,", "tokens": ["Wenn", "es", "dich", "tr\u00f6s\u00b7ten", "kann", ",", "des", "al\u00b7ten", "Meis\u00b7ters", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und strebe, wenn du mu\u00dft, nur nach dem Kranze dort!", "tokens": ["Und", "stre\u00b7be", ",", "wenn", "du", "mu\u00dft", ",", "nur", "nach", "dem", "Kran\u00b7ze", "dort", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich m\u00f6chte, w\u00e4r' es auch in meine Hand gegeben,", "tokens": ["Ich", "m\u00f6ch\u00b7te", ",", "w\u00e4r'", "es", "auch", "in", "mei\u00b7ne", "Hand", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des eignen Kampfes nicht, o Freund, dich \u00fcberheben.", "tokens": ["Des", "eig\u00b7nen", "Kamp\u00b7fes", "nicht", ",", "o", "Freund", ",", "dich", "\u00fc\u00b7ber\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,", "FM", "NN", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Geh nur, wie ich sie gieng, mit Gott die Dornenbahn,", "tokens": ["Geh", "nur", ",", "wie", "ich", "sie", "gieng", ",", "mit", "Gott", "die", "Dor\u00b7nen\u00b7bahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du zum Lohne willst die Dornenkron' empfahn.", "tokens": ["Wenn", "du", "zum", "Loh\u00b7ne", "willst", "die", "Dor\u00b7nen\u00b7kron'", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VMFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch von dem Martyrthum la\u00df dir noch eines sagen:", "tokens": ["Doch", "von", "dem", "Mar\u00b7tyr\u00b7thum", "la\u00df", "dir", "noch", "ei\u00b7nes", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVIMP", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur Einer ward zum Heil der Welt ans Kreuz geschlagen;", "tokens": ["Nur", "Ei\u00b7ner", "ward", "zum", "Heil", "der", "Welt", "ans", "Kreuz", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du aber, wenn man nun ans kritische dich schl\u00e4gt,", "tokens": ["Du", "a\u00b7ber", ",", "wenn", "man", "nun", "ans", "kri\u00b7ti\u00b7sche", "dich", "schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PIS", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh zu, ob es der Welt, ob dir ein Heil es tr\u00e4gt!", "tokens": ["Sieh", "zu", ",", "ob", "es", "der", "Welt", ",", "ob", "dir", "ein", "Heil", "es", "tr\u00e4gt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}