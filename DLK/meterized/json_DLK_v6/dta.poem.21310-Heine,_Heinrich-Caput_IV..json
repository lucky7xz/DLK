{"dta.poem.21310": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput IV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da kam der Luther, und er hat", "tokens": ["Da", "kam", "der", "Lu\u00b7ther", ",", "und", "er", "hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein gro\u00dfes \u201eHalt!\u201c gesprochen -", "tokens": ["Sein", "gro\u00b7\u00dfes", "\u201e", "Halt", "!", "\u201c", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$(", "VVIMP", "$.", "$(", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seit jenem Tage blieb der Bau", "tokens": ["Seit", "je\u00b7nem", "Ta\u00b7ge", "blieb", "der", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Domes unterbrochen.", "tokens": ["Des", "Do\u00b7mes", "un\u00b7ter\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er ward nicht vollendet \u2013 und das ist gut.", "tokens": ["Er", "ward", "nicht", "voll\u00b7en\u00b7det", "\u2013", "und", "das", "ist", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$(", "KON", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn eben die Nichtvollendung", "tokens": ["Denn", "e\u00b7ben", "die", "Nicht\u00b7vol\u00b7len\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Macht ihn zum Denkmahl von Deutschlands Kraft", "tokens": ["Macht", "ihn", "zum", "Denk\u00b7mahl", "von", "Deutschlands", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "APPR", "NE", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und protestantischer Sendung.", "tokens": ["Und", "pro\u00b7tes\u00b7tan\u00b7ti\u00b7scher", "Sen\u00b7dung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ihr armen Schelme vom Domverein,", "tokens": ["Ihr", "ar\u00b7men", "Schel\u00b7me", "vom", "Dom\u00b7ver\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr wollt mit schwachen H\u00e4nden", "tokens": ["Ihr", "wollt", "mit", "schwa\u00b7chen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fortsetzen das unterbrochene Werk,", "tokens": ["Fort\u00b7set\u00b7zen", "das", "un\u00b7ter\u00b7bro\u00b7che\u00b7ne", "Werk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die alte Zwingburg vollenden!", "tokens": ["Und", "die", "al\u00b7te", "Zwing\u00b7burg", "voll\u00b7en\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Oh th\u00f6richter Wahn! Vergebens wird", "tokens": ["Oh", "th\u00f6\u00b7rich\u00b7ter", "Wahn", "!", "Ver\u00b7ge\u00b7bens", "wird"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "$.", "ADV", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gesch\u00fcttelt der Klingelbeutel,", "tokens": ["Ge\u00b7sch\u00fct\u00b7telt", "der", "Klin\u00b7gel\u00b7beu\u00b7tel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gebettelt bei Ketzern und Juden sogar;", "tokens": ["Ge\u00b7bet\u00b7telt", "bei", "Ket\u00b7zern", "und", "Ju\u00b7den", "so\u00b7gar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ist alles fruchtlos und eitel.", "tokens": ["Ist", "al\u00b7les", "frucht\u00b7los", "und", "ei\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}