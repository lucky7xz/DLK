{"textgrid.poem.64340": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "61. Grabschrift eines J\u00fcnglings", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pflanzet Blumen umher: zwar schleu\u00dft der Winter die Blumen;", "tokens": ["Pflan\u00b7zet", "Blu\u00b7men", "um\u00b7her", ":", "zwar", "schleu\u00dft", "der", "Win\u00b7ter", "die", "Blu\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber mit tauender Hand \u00f6ffnet sie wieder der Lenz.", "tokens": ["A\u00b7ber", "mit", "tau\u00b7en\u00b7der", "Hand", "\u00f6ff\u00b7net", "sie", "wie\u00b7der", "der", "Lenz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Weinet mit l\u00e4chelndem Blick: ich ruh' im Scho\u00dfe der Erde;", "tokens": ["Wei\u00b7net", "mit", "l\u00e4\u00b7cheln\u00b7dem", "Blick", ":", "ich", "ruh'", "im", "Scho\u00b7\u00dfe", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Der die Blumen erweckt, wecket zum Leben auch mich.", "tokens": ["Der", "die", "Blu\u00b7men", "er\u00b7weckt", ",", "we\u00b7cket", "zum", "Le\u00b7ben", "auch", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,", "VVFIN", "APPRART", "NN", "ADV", "PPER", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}