{"textgrid.poem.35382": {"metadata": {"author": {"name": "Stramm, August", "birth": "N.A.", "death": "N.A."}, "title": "Verhalten", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Augen schwingen in deinen Br\u00fcsten", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "schwin\u00b7gen", "in", "dei\u00b7nen", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Dein Haupt beugt glutrot weichen Schatten", "tokens": ["Dein", "Haupt", "beugt", "glut\u00b7rot", "wei\u00b7chen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drauf!", "tokens": ["Drauf", "!"], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Der Atem sch\u00e4migt hemmend", "tokens": ["Der", "A\u00b7tem", "sch\u00e4\u00b7migt", "hem\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Gewoge.", "tokens": ["Das", "Ge\u00b7wo\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Mich krallt die Gier", "tokens": ["Mich", "krallt", "die", "Gier"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und herbe D\u00fcnste bluten", "tokens": ["Und", "her\u00b7be", "D\u00fcns\u00b7te", "blu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In seinen Ketten", "tokens": ["In", "sei\u00b7nen", "Ket\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "R\u00fcttelt", "tokens": ["R\u00fct\u00b7telt"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Der Verstand.", "tokens": ["Der", "Ver\u00b7stand", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Fein", "tokens": ["Fein"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Knifft die Scheu die Lippen l\u00e4chelnd", "tokens": ["Knifft", "die", "Scheu", "die", "Lip\u00b7pen", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "K\u00e4lter!", "tokens": ["K\u00e4l\u00b7ter", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Mein Arm nur", "tokens": ["Mein", "Arm", "nur"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Fa\u00dft", "tokens": ["Fa\u00dft"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Im Schwung", "tokens": ["Im", "Schwung"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Dich", "tokens": ["Dich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Hei\u00dfer hei\u00df!", "tokens": ["Hei\u00b7\u00dfer", "hei\u00df", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+", 