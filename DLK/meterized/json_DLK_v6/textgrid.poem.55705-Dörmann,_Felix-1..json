{"textgrid.poem.55705": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die schimmernde Pracht Deiner Glieder,", "tokens": ["Die", "schim\u00b7mern\u00b7de", "Pracht", "Dei\u00b7ner", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich hab' sie voll Andacht geschaut,", "tokens": ["Ich", "hab'", "sie", "voll", "An\u00b7dacht", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch vor der umnachteten Seele", "tokens": ["Doch", "vor", "der", "um\u00b7nach\u00b7te\u00b7ten", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Hat mir gegraut.", "tokens": ["Hat", "mir", "ge\u00b7graut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich h\u00e4tte dich gerne, so gerne", "tokens": ["Ich", "h\u00e4t\u00b7te", "dich", "ger\u00b7ne", ",", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Emporgerissen zum Licht;", "tokens": ["Em\u00b7por\u00b7ge\u00b7ris\u00b7sen", "zum", "Licht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Du aber \u2013 wild und trotzig \u2013", "tokens": ["Du", "a\u00b7ber", "\u2013", "wild", "und", "trot\u00b7zig", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du wolltest nicht.", "tokens": ["Du", "woll\u00b7test", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und doch war Deine Liebe", "tokens": ["Und", "doch", "war", "Dei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gewaltig, hei\u00df und echt \u2013 \u2013 \u2013", "tokens": ["Ge\u00b7wal\u00b7tig", ",", "hei\u00df", "und", "echt", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber lie\u00df Dich fallen, \u2013", "tokens": ["Ich", "a\u00b7ber", "lie\u00df", "Dich", "fal\u00b7len", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und das war schlecht.", "tokens": ["Und", "das", "war", "schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}