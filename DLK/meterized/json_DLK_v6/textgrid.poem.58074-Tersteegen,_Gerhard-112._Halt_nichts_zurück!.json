{"textgrid.poem.58074": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "112. Halt nichts zur\u00fcck!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gib deinen liebsten Benjamin", "tokens": ["Gib", "dei\u00b7nen", "liebs\u00b7ten", "Ben\u00b7ja\u00b7min"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Gottes H\u00e4nde willig hin,", "tokens": ["In", "Got\u00b7tes", "H\u00e4n\u00b7de", "wil\u00b7lig", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst bleibt die Angst in deiner Seelen,", "tokens": ["Sonst", "bleibt", "die", "Angst", "in", "dei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00dft bedr\u00fcckt dich immer qu\u00e4len!", "tokens": ["Und", "mu\u00dft", "be\u00b7dr\u00fcckt", "dich", "im\u00b7mer", "qu\u00e4\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}