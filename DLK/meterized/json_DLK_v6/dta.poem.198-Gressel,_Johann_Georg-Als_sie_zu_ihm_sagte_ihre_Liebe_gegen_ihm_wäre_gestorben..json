{"dta.poem.198": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie zu ihm sagte ihre Liebe gegen ihm  \n w\u00e4re gestorben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sterbe schon/ nun deine Gunst versenckt", "tokens": ["Ich", "ster\u00b7be", "schon", "/", "nun", "dei\u00b7ne", "Gunst", "ver\u00b7senckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jm Grabe ist; denn kan es wol geschehen/", "tokens": ["Jm", "Gra\u00b7be", "ist", ";", "denn", "kan", "es", "wol", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$.", "KON", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ohne Hertz zu leben man gedenckt?", "tokens": ["Da\u00df", "oh\u00b7ne", "Hertz", "zu", "le\u00b7ben", "man", "ge\u00b7denckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PTKZU", "VVINF", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ich soll hier mein Hertz begraben sehen/", "tokens": ["Und", "ich", "soll", "hier", "mein", "Hertz", "be\u00b7gra\u00b7ben", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das dir geschenckt der ", "tokens": ["Das", "dir", "ge\u00b7schenckt", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVPP", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ich sterbe schon.", "tokens": ["Ich", "ster\u00b7be", "schon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Asterie! gr\u00fcnt keine Hoffnung mehr?", "tokens": ["As\u00b7te\u00b7rie", "!", "gr\u00fcnt", "kei\u00b7ne", "Hoff\u00b7nung", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Schl\u00e4gt der Blitz die starcken Eichen nieder?", "tokens": ["Schl\u00e4gt", "der", "Blitz", "die", "star\u00b7cken", "Ei\u00b7chen", "nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Da doch ein Strauch st\u00fcrmt es gleich noch sehr", "tokens": ["Da", "doch", "ein", "Strauch", "st\u00fcrmt", "es", "gleich", "noch", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Auffrecht beh\u00e4lt die gantz geringen Glieder;", "tokens": ["Auf\u00b7frecht", "be\u00b7h\u00e4lt", "die", "gantz", "ge\u00b7rin\u00b7gen", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Begehr doch nicht/ da\u00df ich vor Angst vergeh", "tokens": ["Be\u00b7gehr", "doch", "nicht", "/", "da\u00df", "ich", "vor", "Angst", "ver\u00b7geh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PTKNEG", "$(", "KOUS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Asterie.", "tokens": ["As\u00b7te\u00b7rie", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}