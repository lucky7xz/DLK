{"dta.poem.4404": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Schatten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie uns\u2019re C\u00f6rperlichen Augen,", "tokens": ["Wie", "un\u00b7s'\u00b7re", "C\u00f6r\u00b7per\u00b7li\u00b7chen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wofern sie recht sehn, was sie sehn,", "tokens": ["Wo\u00b7fern", "sie", "recht", "sehn", ",", "was", "sie", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jm Schatten Licht zu finden taugen:", "tokens": ["Jm", "Schat\u00b7ten", "Licht", "zu", "fin\u00b7den", "tau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So kann ein geistiges Gesicht,", "tokens": ["So", "kann", "ein", "geis\u00b7ti\u00b7ges", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn wir die Creatur ergr\u00fcnden,", "tokens": ["Wenn", "wir", "die", "Crea\u00b7tur", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch ein allgegenw\u00e4rtigs Licht", "tokens": ["Auch", "ein", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tigs", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Selbst in den dunkeln C\u00f6rpern finden.", "tokens": ["Selbst", "in", "den", "dun\u00b7keln", "C\u00f6r\u00b7pern", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}