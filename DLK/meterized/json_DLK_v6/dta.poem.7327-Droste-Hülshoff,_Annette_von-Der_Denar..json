{"dta.poem.7327": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Denar.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "O! \u00fcber deinen K\u00f6nig! ganz dir gleich,", "tokens": ["O", "!", "\u00fc\u00b7ber", "dei\u00b7nen", "K\u00f6\u00b7nig", "!", "ganz", "dir", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPOSAT", "NN", "$.", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du glattgeschlagner Lumpen, o, sein Reich", "tokens": ["Du", "glatt\u00b7ge\u00b7schlag\u00b7ner", "Lum\u00b7pen", ",", "o", ",", "sein", "Reich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "FM", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Inselchen, de\u00df k\u00e4rglichen Tribut", "tokens": ["Das", "In\u00b7sel\u00b7chen", ",", "de\u00df", "k\u00e4rg\u00b7li\u00b7chen", "Tri\u00b7but"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Lucull in ", "tokens": ["Lu\u00b7cull", "in"], "token_info": ["word", "word"], "pos": ["NE", "APPR"], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Gebannt in ", "tokens": ["Ge\u00b7bannt", "in"], "token_info": ["word", "word"], "pos": ["VVPP", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "In der Egypterf\u00fcrstin Locken wand.", "tokens": ["In", "der", "E\u00b7gyp\u00b7ter\u00b7f\u00fcrs\u00b7tin", "Lo\u00b7cken", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du, zitternd vor Satrapenblicke, fahl", "tokens": ["Du", ",", "zit\u00b7ternd", "vor", "Sa\u00b7tra\u00b7pen\u00b7bli\u00b7cke", ",", "fahl"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "$,", "VVPP", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "W\u00e4rst du zerst\u00e4ubt vor seiner Augen Strahl,", "tokens": ["W\u00e4rst", "du", "zer\u00b7st\u00e4ubt", "vor", "sei\u00b7ner", "Au\u00b7gen", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn langsam \u00fcber's Forum, im Triumpf", "tokens": ["Wenn", "lang\u00b7sam", "\u00fc\u00b7ber's", "Fo\u00b7rum", ",", "im", "Tri\u00b7umpf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Das Viergespann ihn rollte; h\u00f6rst du dumpf,", "tokens": ["Das", "Vier\u00b7ge\u00b7spann", "ihn", "roll\u00b7te", ";", "h\u00f6rst", "du", "dumpf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$.", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wie halberwachten Donner oder Sp\u00fclen", "tokens": ["Wie", "hal\u00b7ber\u00b7wach\u00b7ten", "Don\u00b7ner", "o\u00b7der", "Sp\u00fc\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Brandung, P\u00f6belwoge ziehn und w\u00fchlen,", "tokens": ["Der", "Bran\u00b7dung", ",", "P\u00f6\u00b7bel\u00b7wo\u00b7ge", "ziehn", "und", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Um die Quadriga summend, wie im Nahn", "tokens": ["Um", "die", "Quad\u00b7ri\u00b7ga", "sum\u00b7mend", ",", "wie", "im", "Nahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVPP", "$,", "PWAV", "APPRART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Pr\u00fcft seine Stimme murrend der Orkan?", "tokens": ["Pr\u00fcft", "sei\u00b7ne", "Stim\u00b7me", "mur\u00b7rend", "der", "Or\u00b7kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "\u201eheil, C\u00e4sar, Heil!\u201c um seine kahle Stirn", "tokens": ["\u201e", "heil", ",", "C\u00e4\u00b7sar", ",", "Heil", "!", "\u201c", "um", "sei\u00b7ne", "kah\u00b7le", "Stirn"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "NE", "$,", "NN", "$.", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "Ragt Lorbeer, wie die Ficht' um Klippenfirn;", "tokens": ["Ragt", "Lor\u00b7beer", ",", "wie", "die", "Ficht'", "um", "Klip\u00b7pen\u00b7firn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Er l\u00e4chelt, und aus seinem L\u00e4cheln flie\u00dfet", "tokens": ["Er", "l\u00e4\u00b7chelt", ",", "und", "aus", "sei\u00b7nem", "L\u00e4\u00b7cheln", "flie\u00b7\u00dfet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ein leise schl\u00e4fernd Gift, o Roma, dir,", "tokens": ["Ein", "lei\u00b7se", "schl\u00e4\u00b7fernd", "Gift", ",", "o", "Ro\u00b7ma", ",", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,", "FM", "FM", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Sein halbgeschlossnes Auge F\u00e4den schie\u00dfet,", "tokens": ["Sein", "halb\u00b7ge\u00b7schloss\u00b7nes", "Au\u00b7ge", "F\u00e4\u00b7den", "schie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ein unzerrei\u00dfbar Netz. \u2014 Geb\u00fcckt und stier,", "tokens": ["Ein", "un\u00b7zer\u00b7rei\u00df\u00b7bar", "Netz", ".", "Ge\u00b7b\u00fcckt", "und", "stier", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$.", "$(", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Zerzausten Haares, vor den Rossen klirrt", "tokens": ["Zer\u00b7zaus\u00b7ten", "Haa\u00b7res", ",", "vor", "den", "Ros\u00b7sen", "klirrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Endloser Gallierzug, die Fesseln schleifen,", "tokens": ["End\u00b7lo\u00b7ser", "Gal\u00b7li\u00b7er\u00b7zug", ",", "die", "Fes\u00b7seln", "schlei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.23": {"text": "Und aus der P\u00f6belwelle gellt und schwirrt", "tokens": ["Und", "aus", "der", "P\u00f6\u00b7bel\u00b7wel\u00b7le", "gellt", "und", "schwirrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Gezisch, Gejubel, Cymbelklang und Pfeifen.", "tokens": ["Ge\u00b7zisch", ",", "Ge\u00b7ju\u00b7bel", ",", "Cym\u00b7bel\u00b7klang", "und", "Pfei\u00b7fen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Denare fliegen aus des Siegers Hand,", "tokens": ["De\u00b7na\u00b7re", "flie\u00b7gen", "aus", "des", "Sie\u00b7gers", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Ha, wie es krabbelt im Arenasand! \u2014", "tokens": ["Ha", ",", "wie", "es", "krab\u00b7belt", "im", "A\u00b7re\u00b7na\u00b7sand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.27": {"text": "Der Imperator nickt und klingelt fort.", "tokens": ["Der", "Im\u00b7pe\u00b7ra\u00b7tor", "nickt", "und", "klin\u00b7gelt", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "Noch lieg' ich unber\u00fchrt im Byssusbeutel, \u2014", "tokens": ["Noch", "lieg'", "ich", "un\u00b7be\u00b7r\u00fchrt", "im", "Bys\u00b7sus\u00b7beu\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Was steigt so schwarz am Kapitole dort?", "tokens": ["Was", "steigt", "so", "schwarz", "am", "Ka\u00b7pi\u00b7to\u00b7le", "dort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Es dunkelt, dunkelt; \u2014 \u00fcber C\u00e4sars Scheitel", "tokens": ["Es", "dun\u00b7kelt", ",", "dun\u00b7kelt", ";", "\u00fc\u00b7ber", "C\u00e4\u00b7sars", "Schei\u00b7tel"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$.", "$(", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ein Riesenaar mit Fl\u00fcgelrauschen steigt,", "tokens": ["Ein", "Rie\u00b7se\u00b7naar", "mit", "Fl\u00fc\u00b7gel\u00b7rau\u00b7schen", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Die Sonne schwindet, \u2014 doch ein Leuchten streicht", "tokens": ["Die", "Son\u00b7ne", "schwin\u00b7det", ",", "doch", "ein", "Leuch\u00b7ten", "streicht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "$(", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Um der Liktoren Beile, \u2014 wieder itzt \u2014", "tokens": ["Um", "der", "Lik\u00b7to\u00b7ren", "Bei\u00b7le", ",", "wie\u00b7der", "itzt"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,", "$(", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Sie zucken, schwenken sich \u2014 es blitzt! \u2014 es blitzt!", "tokens": ["Sie", "zu\u00b7cken", ",", "schwen\u00b7ken", "sich", "es", "blitzt", "!", "es", "blitzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PRF", "$(", "PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}