{"textgrid.poem.57202": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[ich habe den Menschen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe den MENSCHEN gesehn in seiner tiefsten Gestalt,", "tokens": ["Ich", "ha\u00b7be", "den", "MeN\u00b7SCHEN", "ge\u00b7sehn", "in", "sei\u00b7ner", "tiefs\u00b7ten", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "ich kenne die Welt bis auf den Grundgehalt.", "tokens": ["ich", "ken\u00b7ne", "die", "Welt", "bis", "auf", "den", "Grund\u00b7ge\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, da\u00df Liebe, Liebe ihr tiefster Sinn,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "Lie\u00b7be", ",", "Lie\u00b7be", "ihr", "tiefs\u00b7ter", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "$,", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "und da\u00df ich da, um immer mehr zu lieben, bin.", "tokens": ["und", "da\u00df", "ich", "da", ",", "um", "im\u00b7mer", "mehr", "zu", "lie\u00b7ben", ",", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "KOUI", "ADV", "ADV", "PTKZU", "VVINF", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich breite die Arme aus, wie ER getan,", "tokens": ["Ich", "brei\u00b7te", "die", "Ar\u00b7me", "aus", ",", "wie", "Er", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "ich m\u00f6chte die ganze Welt, wie ER, umfahn.", "tokens": ["ich", "m\u00f6ch\u00b7te", "die", "gan\u00b7ze", "Welt", ",", "wie", "Er", ",", "um\u00b7fahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "$,", "KOUI", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}