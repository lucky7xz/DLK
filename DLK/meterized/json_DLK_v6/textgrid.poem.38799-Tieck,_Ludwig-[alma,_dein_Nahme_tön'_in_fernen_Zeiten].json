{"textgrid.poem.38799": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[alma, dein Nahme t\u00f6n' in fernen Zeiten]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alma, dein Nahme t\u00f6n' in fernen Zeiten", "tokens": ["Al\u00b7ma", ",", "dein", "Nah\u00b7me", "t\u00f6n'", "in", "fer\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von s\u00fc\u00dfen Lippen, Herzen soll er r\u00fchren,", "tokens": ["Von", "s\u00fc\u00b7\u00dfen", "Lip\u00b7pen", ",", "Her\u00b7zen", "soll", "er", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn k\u00fcnftge Dichter ihre Spr\u00fcche zieren", "tokens": ["Wenn", "k\u00fcnft\u00b7ge", "Dich\u00b7ter", "ih\u00b7re", "Spr\u00fc\u00b7che", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nennen sie dich zum Schlu\u00df der Seltenheiten.", "tokens": ["Nen\u00b7nen", "sie", "dich", "zum", "Schlu\u00df", "der", "Sel\u00b7ten\u00b7hei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Du willst, Holdseelge, ferner s\u00fc\u00df mich leiten,", "tokens": ["Du", "willst", ",", "Hold\u00b7seel\u00b7ge", ",", "fer\u00b7ner", "s\u00fc\u00df", "mich", "lei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit mir der Dichtkunst Fl\u00fcgelro\u00df regieren,", "tokens": ["Mit", "mir", "der", "Dicht\u00b7kunst", "Fl\u00fc\u00b7gel\u00b7ro\u00df", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begeistert es durch Wunderland' zu f\u00fchren,", "tokens": ["Be\u00b7geis\u00b7tert", "es", "durch", "Wun\u00b7der\u00b7land'", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Lieder fliegen fort in helle Weiten.", "tokens": ["Und", "Lie\u00b7der", "flie\u00b7gen", "fort", "in", "hel\u00b7le", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vieles vergeht, nicht was ich dann will singen,", "tokens": ["Vie\u00b7les", "ver\u00b7geht", ",", "nicht", "was", "ich", "dann", "will", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PTKNEG", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Was mir in's Herz die Liebe selbst geschrieben,", "tokens": ["Was", "mir", "in's", "Herz", "die", "Lie\u00b7be", "selbst", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was aust\u00f6nen goldbeschwingte Reime.", "tokens": ["Und", "was", "aus\u00b7t\u00f6\u00b7nen", "gold\u00b7be\u00b7schwing\u00b7te", "Rei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Empfinden Liebende die spielnden Tr\u00e4ume,", "tokens": ["Emp\u00b7fin\u00b7den", "Lie\u00b7ben\u00b7de", "die", "spieln\u00b7den", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Sie fragen: wer hat so gef\u00fchlt das Lieben?", "tokens": ["Sie", "fra\u00b7gen", ":", "wer", "hat", "so", "ge\u00b7f\u00fchlt", "das", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PWS", "VAFIN", "ADV", "VVPP", "ART", "ADJA", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So wird mit deinem auch mein Nahme klingen.", "tokens": ["So", "wird", "mit", "dei\u00b7nem", "auch", "mein", "Nah\u00b7me", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}