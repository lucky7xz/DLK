{"textgrid.poem.58560": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "598. Von der g\u00f6ttlichen Erleuchtung im Grunde der Seele, allen Weltgelehrten unbekannt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich versteh oft im Herzensgrund", "tokens": ["Ich", "ver\u00b7steh", "oft", "im", "Her\u00b7zens\u00b7grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mehr, denn kein Mensch je lehren kunnt,", "tokens": ["Mehr", ",", "denn", "kein", "Mensch", "je", "leh\u00b7ren", "kunnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KON", "PIAT", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Stillesein vom wahren Licht;", "tokens": ["Durch", "Stil\u00b7le\u00b7sein", "vom", "wah\u00b7ren", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O blieb' ich dein, weil solch's geschicht,", "tokens": ["O", "blieb'", "ich", "dein", ",", "weil", "solch's", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was g\u00f6ttlich hoher Gaben viel", "tokens": ["Was", "g\u00f6tt\u00b7lich", "ho\u00b7her", "Ga\u00b7ben", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00e4tt' ich zuletzt! Doch wie Gott will.", "tokens": ["H\u00e4tt'", "ich", "zu\u00b7letzt", "!", "Doch", "wie", "Gott", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "KON", "PWAV", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum versenk dich zu aller Zeit", "tokens": ["Drum", "ver\u00b7senk", "dich", "zu", "al\u00b7ler", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "In die niederst' Dem\u00fctigkeit", "tokens": ["In", "die", "nie\u00b7der\u00b7st'", "De\u00b7m\u00fc\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Unter Gott und all' Kreatur!", "tokens": ["Un\u00b7ter", "Gott", "und", "all'", "Kre\u00b7a\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Das ist f\u00fcrwahr die Weisheit nur,", "tokens": ["Das", "ist", "f\u00fcr\u00b7wahr", "die", "Weis\u00b7heit", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Drin alle K\u00fcnst' beschlossen werden,", "tokens": ["Drin", "al\u00b7le", "K\u00fcnst'", "be\u00b7schlos\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Zur Seligkeit n\u00f6tig auf Erden.", "tokens": ["Zur", "Se\u00b7lig\u00b7keit", "n\u00f6\u00b7tig", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}