{"textgrid.poem.47076": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebchen hat das Herz sich abgeschlossen,", "tokens": ["Mein", "Lieb\u00b7chen", "hat", "das", "Herz", "sich", "ab\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schl\u00fcssel drauf geworfen in die See.", "tokens": ["Den", "Schl\u00fcs\u00b7sel", "drauf", "ge\u00b7wor\u00b7fen", "in", "die", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort h\u00e4ngt er tief, wo die Korallen sprossen,", "tokens": ["Dort", "h\u00e4ngt", "er", "tief", ",", "wo", "die", "Ko\u00b7ral\u00b7len", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergebens taucht nach ihm hinab mein Weh.", "tokens": ["Ver\u00b7ge\u00b7bens", "taucht", "nach", "ihm", "hin\u00b7ab", "mein", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}