: "Da von andern Bluhmen mehr recht ein bunter Kranz", "tokens": ["Da", "von", "an\u00b7dern", "Bluh\u00b7men", "mehr", "recht", "ein", "bun\u00b7ter", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "ihn kr\u00e4nzte,", "tokens": ["ihn", "kr\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Da die Himmel-blauen Korn-gelben Bluhmen und Ca-", "tokens": ["Da", "die", "Him\u00b7mel\u00b7blau\u00b7en", "Korn\u00b7gel\u00b7ben", "Bluh\u00b7men", "und", "Ca"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "KON", "TRUNC"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "millen,", "tokens": ["mil\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Nebst der Purpur-farbnen Radel, beyde Seiten Strich-weis\u2019", "tokens": ["Nebst", "der", "Pur\u00b7pur\u00b7fa\u00b7rb\u00b7nen", "Ra\u00b7del", ",", "bey\u00b7de", "Sei\u00b7ten", "Strich\u00b7weis'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PIAT", "NN", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "f\u00fcllen,", "tokens": ["f\u00fcl\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Die daselbst sich gleichsam selbst hinges\u00e4t zu haben schienen,", "tokens": ["Die", "da\u00b7selbst", "sich", "gleich\u00b7sam", "selbst", "hin\u00b7ge\u00b7s\u00e4t", "zu", "ha\u00b7ben", "schie\u00b7nen", ","], "token_