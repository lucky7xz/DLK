{"dta.poem.4190": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Sch\u00f6nheit der Welt  \n beym  \n  Sonnenschein nach dem Regen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie so sch\u00f6n hat die Natur itzt den Erdkreis ausge-", "tokens": ["Wie", "so", "sch\u00f6n", "hat", "die", "Na\u00b7tur", "itzt", "den", "Erd\u00b7kreis", "aus\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "schm\u00fccket!", "tokens": ["schm\u00fc\u00b7cket", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Fast von jedem Gegenwurf wird der Geist durchs Aug\u2019", "tokens": ["Fast", "von", "je\u00b7dem", "Ge\u00b7gen\u00b7wurf", "wird", "der", "Geist", "durchs", "Aug'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "erquicket.", "tokens": ["er\u00b7quic\u00b7ket", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die noch frische neue Bluhmen, das noch junge Laub und", "tokens": ["Die", "noch", "fri\u00b7sche", "neu\u00b7e", "Bluh\u00b7men", ",", "das", "noch", "jun\u00b7ge", "Laub", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Gras,", "tokens": ["Gras", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Da es eben erst geregnet, und noch alles glatt und na\u00df,", "tokens": ["Da", "es", "e\u00b7ben", "erst", "ge\u00b7reg\u00b7net", ",", "und", "noch", "al\u00b7les", "glatt", "und", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,", "KON", "ADV", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Doch zugleich vom Licht der Sonnen, als von einer Himmels-", "tokens": ["Doch", "zu\u00b7gleich", "vom", "Licht", "der", "Son\u00b7nen", ",", "als", "von", "ei\u00b7ner", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "$,", "KOUS", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Fluht,", "tokens": ["Fluht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Ueberflossen und bestrahlt ist; steht in einer bunten Gluht.", "tokens": ["Ue\u00b7berf\u00b7los\u00b7sen", "und", "be\u00b7strahlt", "ist", ";", "steht", "in", "ei\u00b7ner", "bun\u00b7ten", "Gluht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAFIN", "$.", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Alles Gr\u00fcne gl\u00e4nzet auch; alles Roht\u2019 in Bluhmen", "tokens": ["Al\u00b7les", "Gr\u00fc\u00b7ne", "gl\u00e4n\u00b7zet", "auch", ";", "al\u00b7les", "Roht'", "in", "Bluh\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$.", "PIAT", "NN", "APPR", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "gl\u00fchet;", "tokens": ["gl\u00fc\u00b7het", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Alles Weisse blitzt und schimmert. Seht, wie dorten Gelb", "tokens": ["Al\u00b7les", "Weis\u00b7se", "blitzt", "und", "schim\u00b7mert", ".", "Seht", ",", "wie", "dor\u00b7ten", "Gelb"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "KON", "VVFIN", "$.", "VVFIN", "$,", "PWAV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "und Blau", "tokens": ["und", "Blau"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Lieblich scheinen, spielen, funkeln! Aber mehr noch dort", "tokens": ["Lieb\u00b7lich", "schei\u00b7nen", ",", "spie\u00b7len", ",", "fun\u00b7keln", "!", "A\u00b7ber", "mehr", "noch", "dort"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$.", "KON", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "die Au,", "tokens": ["die", "Au", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Als worauf es wirklich l\u00e4\u00dft, als wenn in dem klaren", "tokens": ["Als", "wo\u00b7rauf", "es", "wirk\u00b7lich", "l\u00e4\u00dft", ",", "als", "wenn", "in", "dem", "kla\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PWAV", "PPER", "ADJD", "VVFIN", "$,", "KOKOM", "KOUS", "APPR", "ART", "ADJA"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Thau", "tokens": ["Thau"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Perlen aus dem Orient \u00fcberall darauf gespr\u00fcet,", "tokens": ["Per\u00b7len", "aus", "dem", "O\u00b7rient", "\u00fc\u00b7be\u00b7rall", "da\u00b7rauf", "ge\u00b7spr\u00fcet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "Diamanten drauf ges\u00e4t, nebst noch andern Edelsteinen,", "tokens": ["Di\u00b7a\u00b7man\u00b7ten", "drauf", "ge\u00b7s\u00e4t", ",", "nebst", "noch", "an\u00b7dern", "E\u00b7del\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "VVPP", "$,", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "(welches Gleichni\u00df von Juwelen, wenn man diese Pracht", "tokens": ["(", "wel\u00b7ches", "Gleich\u00b7ni\u00df", "von", "Ju\u00b7we\u00b7len", ",", "wenn", "man", "die\u00b7se", "Pracht"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAT", "NN", "APPR", "NN", "$,", "KOUS", "PIS", "PDAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.22": {"text": "ermi\u00dft,", "tokens": ["er\u00b7mi\u00dft", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "W\u00e4r es sonst auch gleich zu tadeln, hier doch nicht zu tadeln", "tokens": ["W\u00e4r", "es", "sonst", "auch", "gleich", "zu", "ta\u00b7deln", ",", "hier", "doch", "nicht", "zu", "ta\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$,", "ADV", "ADV", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.24": {"text": "ist)", "tokens": ["ist", ")"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$("], "meter": "-", "measure": "single.down"}, "line.25": {"text": "In gef\u00e4rbter W\u00e4sserung, voller Licht und Feuer, scheinen.", "tokens": ["In", "ge\u00b7f\u00e4rb\u00b7ter", "W\u00e4s\u00b7se\u00b7rung", ",", "vol\u00b7ler", "Licht", "und", "Feu\u00b7er", ",", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Da ich hier nun bey so hellem, lieblichen und sch\u00f6nen", "tokens": ["Da", "ich", "hier", "nun", "bey", "so", "hel\u00b7lem", ",", "lieb\u00b7li\u00b7chen", "und", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "ADV", "ADJA", "$,", "ADJA", "KON", "ADJA"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wetter,", "tokens": ["Wet\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Durch ein klein Geb\u00fcsch von Erlen, langsam hin und wieder", "tokens": ["Durch", "ein", "klein", "Ge\u00b7b\u00fcsch", "von", "Er\u00b7len", ",", "lang\u00b7sam", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "NN", "APPR", "NN", "$,", "ADJD", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "geh,", "tokens": ["geh", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und die noch vom Regen feuchte, klebrichte, gesteifte", "tokens": ["Und", "die", "noch", "vom", "Re\u00b7gen", "feuch\u00b7te", ",", "kleb\u00b7rich\u00b7te", ",", "ge\u00b7steif\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ART", "ADV", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "$,", "ADJA"], "meter": "--+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Bl\u00e4tter,", "tokens": ["Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Die verschiedentlich durchadert, und woran sich oft das", "tokens": ["Die", "ver\u00b7schie\u00b7dent\u00b7lich", "durc\u00b7ha\u00b7dert", ",", "und", "wo\u00b7ran", "sich", "oft", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "$,", "KON", "PWAV", "PRF", "ADV", "ART"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Licht,", "tokens": ["Licht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Durch die kleinen Tief- und H\u00f6hen, als in kleinen Spiegeln,", "tokens": ["Durch", "die", "klei\u00b7nen", "Tie\u00b7f", "und", "H\u00f6\u00b7hen", ",", "als", "in", "klei\u00b7nen", "Spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "TRUNC", "KON", "NN", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "bricht,", "tokens": ["bricht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Mit betrachtendem Vergn\u00fcgen, und mit stillem Geiste", "tokens": ["Mit", "be\u00b7trach\u00b7ten\u00b7dem", "Ver\u00b7gn\u00fc\u00b7gen", ",", "und", "mit", "stil\u00b7lem", "Geis\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "seh;", "tokens": ["seh", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Sah ich, mit vermehrter Lust, \u00f6fters kleine blaue Thiere,", "tokens": ["Sah", "ich", ",", "mit", "ver\u00b7mehr\u00b7ter", "Lust", ",", "\u00f6f\u00b7ters", "klei\u00b7ne", "blau\u00b7e", "Thie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Wie lebendige Sapphire,", "tokens": ["Wie", "le\u00b7ben\u00b7di\u00b7ge", "Sap\u00b7phi\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.15": {"text": "Mit gef\u00e4rbt-durchsichtigem, klar- und schimmerndem Ge-", "tokens": ["Mit", "ge\u00b7f\u00e4rb\u00b7tdurch\u00b7sich\u00b7ti\u00b7gem", ",", "kla\u00b7r", "und", "schim\u00b7mern\u00b7dem", "Ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "TRUNC", "KON", "ADJA", "NN"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "fieder,", "tokens": ["fie\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Hin und wieder,", "tokens": ["Hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "In ver\u00e4nderlichen, leichten, stillen und doch regen Z\u00fcgen,", "tokens": ["In", "ver\u00b7\u00e4n\u00b7der\u00b7li\u00b7chen", ",", "leich\u00b7ten", ",", "stil\u00b7len", "und", "doch", "re\u00b7gen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "$,", "ADJA", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Scherzen, schw\u00e4rmen, schweben, fliegen,", "tokens": ["Scher\u00b7zen", ",", "schw\u00e4r\u00b7men", ",", "schwe\u00b7ben", ",", "flie\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Bald sich heben, bald sich setzen,", "tokens": ["Bald", "sich", "he\u00b7ben", ",", "bald", "sich", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVINF", "$,", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Bald sich trennen, bald sich wieder, durch den Trieb der Liebe,", "tokens": ["Bald", "sich", "tren\u00b7nen", ",", "bald", "sich", "wie\u00b7der", ",", "durch", "den", "Trieb", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVINF", "$,", "ADV", "PRF", "ADV", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.22": {"text": "f\u00fcgen,", "tokens": ["f\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Und, so viel ihr Aeussers zeiget, sich recht inniglich ergetzen.", "tokens": ["Und", ",", "so", "viel", "ihr", "A\u00b7e\u00b7us\u00b7sers", "zei\u00b7get", ",", "sich", "recht", "in\u00b7nig\u00b7lich", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Aber, wie vermehrt sich noch mein hiedurch erregt Ver-", "tokens": ["A\u00b7ber", ",", "wie", "ver\u00b7mehrt", "sich", "noch", "mein", "hie\u00b7durch", "er\u00b7regt", "Ver"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ADJD", "PRF", "ADV", "PPOSAT", "PAV", "VVPP", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "gn\u00fcgen,", "tokens": ["gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Da ich in der, durchs Getrayde, gleich-gezogenen Allee,", "tokens": ["Da", "ich", "in", "der", ",", "durchs", "Ge\u00b7tray\u00b7de", ",", "gleich\u00b7ge\u00b7zo\u00b7ge\u00b7nen", "Al\u00b7lee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "$,", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "Unvermuhtet rohten Mohn, deren Bluhmen, wie Rubi-", "tokens": ["Un\u00b7ver\u00b7muh\u00b7tet", "roh\u00b7ten", "Mohn", ",", "de\u00b7ren", "Bluh\u00b7men", ",", "wie", "Ru\u00b7bi"], "token_info": ["word", 