{"textgrid.poem.50555": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "20. Landhauss des Cantzlers Bacon de Verulam", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als einst ", "tokens": ["Als", "einst"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Auf seinen ", "tokens": ["Auf", "sei\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und zu ihm sagte: Sie eracht'", "tokens": ["Und", "zu", "ihm", "sag\u00b7te", ":", "Sie", "er\u00b7acht'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Hauss ", "tokens": ["Das", "Hauss"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ich habe nicht, sprach er, ", "tokens": ["Ich", "ha\u00b7be", "nicht", ",", "sprach", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Woll aber ihr ", "tokens": ["Woll", "a\u00b7ber", "ihr"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}}}}}