{"textgrid.poem.31658": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "30. Aus dem Lateinischen versetzt", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "A und O du grosser Gott /", "tokens": ["A", "und", "O", "du", "gros\u00b7ser", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "NE", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "mein Gott / mein Gott in der Noht!", "tokens": ["mein", "Gott", "/", "mein", "Gott", "in", "der", "Noht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "welches Krafft kan alles machen /", "tokens": ["wel\u00b7ches", "Krafft", "kan", "al\u00b7les", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VMFIN", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dessen Sinn wei\u00df alle Sachen /", "tokens": ["des\u00b7sen", "Sinn", "wei\u00df", "al\u00b7le", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELAT", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "dessen Seyn das H\u00f6chste Gut /", "tokens": ["des\u00b7sen", "Seyn", "das", "H\u00f6chs\u00b7te", "Gut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "der auch lauter Gutes thut;", "tokens": ["der", "auch", "lau\u00b7ter", "Gu\u00b7tes", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "ob- und unter allen schwebend /", "tokens": ["ob", "und", "un\u00b7ter", "al\u00b7len", "schwe\u00b7bend", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "APPR", "PIAT", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "ausser-und in allen lebend /", "tokens": ["aus\u00b7ser\u00b7\u00b7und", "in", "al\u00b7len", "le\u00b7bend", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "unter allen ungeschm\u00e4ht /", "tokens": ["un\u00b7ter", "al\u00b7len", "un\u00b7ge\u00b7schm\u00e4ht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "ober allen unerh\u00f6ht;", "tokens": ["o\u00b7ber", "al\u00b7len", "un\u00b7er\u00b7h\u00f6ht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "inner aller unverschlossen /", "tokens": ["in\u00b7ner", "al\u00b7ler", "un\u00b7ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "ausser allen unverstossen /", "tokens": ["aus\u00b7ser", "al\u00b7len", "un\u00b7ver\u00b7stos\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "\u00fcber alls mit Herrsch-Gewalt /", "tokens": ["\u00fc\u00b7ber", "alls", "mit", "Herr\u00b7schGe\u00b7walt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "untersich als Aufenthalt;", "tokens": ["un\u00b7ter\u00b7sich", "als", "Auf\u00b7ent\u00b7halt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "alls begreiffend' ausser allen /", "tokens": ["alls", "be\u00b7greif\u00b7fend'", "aus\u00b7ser", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "drinn erf\u00fcllend nachgefallen /", "tokens": ["drinn", "er\u00b7f\u00fcl\u00b7lend", "nach\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "wird darinnen nicht gedr\u00e4ngt /", "tokens": ["wird", "da\u00b7rin\u00b7nen", "nicht", "ge\u00b7dr\u00e4ngt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "noch heraussen was verl\u00e4ngt;", "tokens": ["noch", "her\u00b7aus\u00b7sen", "was", "ver\u00b7l\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "PWS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "unten unbetr\u00fcbt im walten /", "tokens": ["un\u00b7ten", "un\u00b7be\u00b7tr\u00fcbt", "im", "wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "oben auch ununterhalten /", "tokens": ["o\u00b7ben", "auch", "un\u00b7un\u00b7ter\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Welt-bewegend unbewegt /", "tokens": ["Welt\u00b7be\u00b7we\u00b7gend", "un\u00b7be\u00b7wegt", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "h\u00e4tst all Oerter ungehegt;", "tokens": ["h\u00e4tst", "all", "O\u00b7er\u00b7ter", "un\u00b7ge\u00b7hegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "wandelst unverwandt die Zeiten /", "tokens": ["wan\u00b7delst", "un\u00b7ver\u00b7wandt", "die", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "fl\u00fcchtigs stillst ohn Fl\u00fcchtigkeiten;", "tokens": ["fl\u00fcch\u00b7tigs", "stillst", "ohn", "Fl\u00fcch\u00b7tig\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "n\u00f6tige noch \u00e4usre Krafft", "tokens": ["n\u00f6\u00b7ti\u00b7ge", "noch", "\u00e4us\u00b7re", "Krafft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "endert dein' Ureigenschafft.", "tokens": ["en\u00b7dert", "dein'", "Ur\u00b7ei\u00b7gen\u00b7schafft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.27": {"text": "Was vorbey / was soll geschehen /", "tokens": ["Was", "vor\u00b7bey", "/", "was", "soll", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$(", "PWS", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "kanstualls vor Augen sehen:", "tokens": ["kan\u00b7stu\u00b7alls", "vor", "Au\u00b7gen", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "ja es ist dir allzeit heut /", "tokens": ["ja", "es", "ist", "dir", "all\u00b7zeit", "heut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "unzertheilte Ewigkeit!", "tokens": ["un\u00b7zer\u00b7theil\u00b7te", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "deine Vorsicht / wacht in allen.", "tokens": ["dei\u00b7ne", "Vor\u00b7sicht", "/", "wacht", "in", "al\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Schaffst auch alles nach gefallen.", "tokens": ["Schaffst", "auch", "al\u00b7les", "nach", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "APPR", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Zu dem Bild des H\u00f6chsten Sinn /", "tokens": ["Zu", "dem", "Bild", "des", "H\u00f6chs\u00b7ten", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "ordnest die Urwesen hin.", "tokens": ["ord\u00b7n