{"textgrid.poem.39763": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "8. Im Kursaal", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun gar hinein zur gro\u00dfen Welt,", "tokens": ["Nun", "gar", "hin\u00b7ein", "zur", "gro\u00b7\u00dfen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihren grellen Saal!", "tokens": ["In", "ih\u00b7ren", "grel\u00b7len", "Saal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Wandrer, was dein Herz erhellt,", "tokens": ["O", "Wand\u00b7rer", ",", "was", "dein", "Herz", "er\u00b7hellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Such's nicht im Kerzenstral!", "tokens": ["Such's", "nicht", "im", "Ker\u00b7zen\u00b7stral", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und doch \u2013 was fesselt mich denn hier?", "tokens": ["Und", "doch", "\u2013", "was", "fes\u00b7selt", "mich", "denn", "hier", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum verweil' ich gern?", "tokens": ["Wa\u00b7rum", "ver\u00b7weil'", "ich", "gern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was wird es ruhig still in mir", "tokens": ["Was", "wird", "es", "ru\u00b7hig", "still", "in", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie unter Mond und Stern?", "tokens": ["Wie", "un\u00b7ter", "Mond", "und", "Stern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach, in dem brausenden Gew\u00fchl,", "tokens": ["Ach", ",", "in", "dem", "brau\u00b7sen\u00b7den", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wund von der L\u00fcge Schmerz,", "tokens": ["Wund", "von", "der", "L\u00fc\u00b7ge", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Fand pl\u00f6tzlich ich ein ernst Gef\u00fchl,", "tokens": ["Fand", "pl\u00f6tz\u00b7lich", "ich", "ein", "ernst", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wahrheit spendend Herz.", "tokens": ["Ein", "Wahr\u00b7heit", "spen\u00b7dend", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie mitten in dem d\u00fcrren Sand", "tokens": ["Wie", "mit\u00b7ten", "in", "dem", "d\u00fcr\u00b7ren", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Quell dem Waller springt,", "tokens": ["Ein", "Quell", "dem", "Wal\u00b7ler", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie er sich von der Felsenwand", "tokens": ["Wie", "er", "sich", "von", "der", "Fel\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Dorn die Rose ringt:", "tokens": ["Aus", "Dorn", "die", "Ro\u00b7se", "ringt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So perlt aus einem Auge klar", "tokens": ["So", "perlt", "aus", "ei\u00b7nem", "Au\u00b7ge", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir frische Lebensflut,", "tokens": ["Mir", "fri\u00b7sche", "Le\u00b7bens\u00b7flut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So quillt von ros'gem Lippenpaar", "tokens": ["So", "quillt", "von", "ros'\u00b7gem", "Lip\u00b7pen\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir Geistes Duft und Glut.", "tokens": ["Mir", "Geis\u00b7tes", "Duft", "und", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mir ist, als h\u00e4tt' in Einsamkeit", "tokens": ["Mir", "ist", ",", "als", "h\u00e4tt'", "in", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich betend mich erquickt,", "tokens": ["Ich", "be\u00b7tend", "mich", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Engelshand giebt mir Geleit,", "tokens": ["Und", "En\u00b7gels\u00b7hand", "giebt", "mir", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mich kein Trug umstrickt.", "tokens": ["Da\u00df", "mich", "kein", "Trug", "um\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Aus den erf\u00fcllten Hallen fort", "tokens": ["Aus", "den", "er\u00b7f\u00fcll\u00b7ten", "Hal\u00b7len", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wandr' ich hinaus ins Feld,", "tokens": ["Wandr'", "ich", "hin\u00b7aus", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie waren mir ein stiller Port,", "tokens": ["Sie", "wa\u00b7ren", "mir", "ein", "stil\u00b7ler", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab' Dank, du gro\u00dfe Welt!", "tokens": ["Hab'", "Dank", ",", "du", "gro\u00b7\u00dfe", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}