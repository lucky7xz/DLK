{"textgrid.poem.28803": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann auff Tag nicht k\u00e4me Nacht, w\u00fcrden wir gar bald erliegen;", "tokens": ["Wann", "auff", "Tag", "nicht", "k\u00e4\u00b7me", "Nacht", ",", "w\u00fcr\u00b7den", "wir", "gar", "bald", "er\u00b7lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PTKNEG", "VVFIN", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auch der Tod geht darum vor, da\u00df wir rechter leben m\u00fcgen.", "tokens": ["Auch", "der", "Tod", "geht", "da\u00b7rum", "vor", ",", "da\u00df", "wir", "rech\u00b7ter", "le\u00b7ben", "m\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PAV", "PTKVZ", "$,", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "--++-+-+-+-+-+-", "measure": "anapaest.init"}}}}}