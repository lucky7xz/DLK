{"textgrid.poem.41325": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Unvermuthete Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Malthin, den J\u00fcngling, fragt Macrin,", "tokens": ["Mal\u00b7thin", ",", "den", "J\u00fcng\u00b7ling", ",", "fragt", "Ma\u00b7crin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Rechtsgelehrsamkeit, Amt, Milz und Alter steift:", "tokens": ["Den", "Rechts\u00b7ge\u00b7lehr\u00b7sam\u00b7keit", ",", "Amt", ",", "Milz", "und", "Al\u00b7ter", "steift", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie nennst du einen Kerl, sprich, sprich, wie nennst du ihn,", "tokens": ["Wie", "nennst", "du", "ei\u00b7nen", "Kerl", ",", "sprich", ",", "sprich", ",", "wie", "nennst", "du", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den man im Ehebruch ergreift?", "tokens": ["Den", "man", "im", "E\u00b7heb\u00b7ruch", "er\u00b7greift", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich nenn' ihn langsam, spricht Malthin.", "to