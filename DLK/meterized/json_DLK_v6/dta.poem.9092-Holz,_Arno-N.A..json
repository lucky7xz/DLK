{"dta.poem.9092": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lachend in die Siegesallee", "tokens": ["La\u00b7chend", "in", "die", "Sie\u00b7ge\u00b7sal\u00b7lee"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "schwenkt ein M\u00e4dchenpensionat.", "tokens": ["schwenkt", "ein", "M\u00e4d\u00b7chen\u00b7pen\u00b7si\u00b7o\u00b7nat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Donnerwetter, sind die chic!", "tokens": ["Don\u00b7ner\u00b7wet\u00b7ter", ",", "sind", "die", "chic", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wippende, gr\u00fcnblau schillernde Changeantschirme,", "tokens": ["Wip\u00b7pen\u00b7de", ",", "gr\u00fcn\u00b7blau", "schil\u00b7lern\u00b7de", "Chan\u00b7ge\u00b7ant\u00b7schir\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "lange, buttergelbe schwedische Handschuhe,", "tokens": ["lan\u00b7ge", ",", "but\u00b7ter\u00b7gel\u00b7be", "schwe\u00b7di\u00b7sche", "Hand\u00b7schu\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+--++-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "sich bauschende, silbergraue, von roten Tulpen durchflammte Velvetblousen.", "tokens": ["sich", "bau\u00b7schen\u00b7de", ",", "sil\u00b7ber\u00b7grau\u00b7e", ",", "von", "ro\u00b7ten", "Tul\u00b7pen", "durch\u00b7flamm\u00b7te", "Vel\u00b7vet\u00b7blou\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "NE", "$."], "meter": "-+--+-+--+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Drei junge Leutnants drehn ihre Schnurrb\u00e4rte.", "tokens": ["Drei", "jun\u00b7ge", "Leut\u00b7nants", "drehn", "ih\u00b7re", "Schnurr\u00b7b\u00e4r\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "CARD", "PPOSAT", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Monocles.", "tokens": ["Mo\u00b7no\u00b7cles", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Die Kavalkade am\u00fcsiert sich.", "tokens": ["Die", "Ka\u00b7val\u00b7ka\u00b7de", "a\u00b7m\u00fc\u00b7siert", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "F\u00fcnfzig braune, trappelnde Strandschuhe,", "tokens": ["F\u00fcnf\u00b7zig", "brau\u00b7ne", ",", "trap\u00b7peln\u00b7de", "Strand\u00b7schu\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-++-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "f\u00fcnfundzwanzig klingelnde Bettelarmb\u00e4nder.", "tokens": ["f\u00fcn\u00b7fund\u00b7zwan\u00b7zig", "klin\u00b7geln\u00b7de", "Bet\u00b7tel\u00b7arm\u00b7b\u00e4n\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Links,", "tokens": ["Links", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "hinter ihnen drein,", "tokens": ["hin\u00b7ter", "ih\u00b7nen", "drein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "die Blicke kohlschwarz,", "tokens": ["die", "Bli\u00b7cke", "kohl\u00b7schwarz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "ihr Drache.", "tokens": ["ihr", "Dra\u00b7che", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Wehe!", "tokens": ["We\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Wie die Sonne durch die B\u00e4ume goldne Kringel wirft . . . ", "tokens": ["Wie", "die", "Son\u00b7ne", "durch", "die", "B\u00e4u\u00b7me", "gold\u00b7ne", "Krin\u00b7gel", "wirft", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.11": {"line.1": {"text": "Ach was!", "tokens": ["Ach", "was", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "PWS", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.12": {"line.1": {"text": "Und ich kriege die Sch\u00f6nste, die sich nicht str\u00e4ubt, um die Taille,", "tokens": ["Und", "ich", "krie\u00b7ge", "die", "Sch\u00f6ns\u00b7te", ",", "die", "sich", "nicht", "str\u00e4ubt", ",", "um", "die", "Tail\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "PTKNEG", "VVFIN", "$,", "KOUI", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "\u2014 die ganze Gesellschaft stiebt kreischend auseinander,", "tokens": ["die", "gan\u00b7ze", "Ge\u00b7sell\u00b7schaft", "stiebt", "krei\u00b7schend", "aus\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Huuch! die alte Anstandsglucke f\u00e4llt in Ohnmacht \u2014", "tokens": ["Huuch", "!", "die", "al\u00b7te", "An\u00b7stands\u00b7glu\u00b7cke", "f\u00e4llt", "in", "Ohn\u00b7macht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.4": {"text": "und rufe:", "tokens": ["und", "ru\u00b7fe", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.13": {"line.1": {"text": "M\u00e4dchen, entg\u00fcrtet euch und tanzt nackt zwischen Schwertern!", "tokens": ["M\u00e4d\u00b7chen", ",", "ent\u00b7g\u00fcr\u00b7tet", "euch", "und", "tanzt", "nackt", "zwi\u00b7schen", "Schwer\u00b7tern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}