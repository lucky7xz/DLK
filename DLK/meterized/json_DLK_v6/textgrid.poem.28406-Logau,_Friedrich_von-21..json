{"textgrid.poem.28406": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Wort geh\u00f6rt zum Brot und auch das Brot zum Worte;", "tokens": ["Das", "Wort", "ge\u00b7h\u00f6rt", "zum", "Brot", "und", "auch", "das", "Brot", "zum", "Wor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es k\u00fcmmt uns beydes zu herab au\u00df einem Orte.", "tokens": ["Es", "k\u00fcmmt", "uns", "bey\u00b7des", "zu", "her\u00b7ab", "au\u00df", "ei\u00b7nem", "Or\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott, der du beydes gibst, wend ab de\u00df Wortes Noth,", "tokens": ["Gott", ",", "der", "du", "bey\u00b7des", "gibst", ",", "wend", "ab", "de\u00df", "Wor\u00b7tes", "Noth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "So mangelt uns auch nicht die Nothdurfft an dem Brot.", "tokens": ["So", "man\u00b7gelt", "uns", "auch", "nicht", "die", "Noth\u00b7durfft", "an", "dem", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", 