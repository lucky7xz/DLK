{"textgrid.poem.42993": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Leere Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lie\u00df ein Huhn sich braten.", "tokens": ["Es", "lie\u00df", "ein", "Huhn", "sich", "bra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich roch es. Doch es lockte nicht.", "tokens": ["Ich", "roch", "es", ".", "Doch", "es", "lock\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich gr\u00fc\u00dften zwei Soldaten.", "tokens": ["Mich", "gr\u00fc\u00df\u00b7ten", "zwei", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie hatten kein Gesicht.", "tokens": ["Sie", "hat\u00b7ten", "kein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schritt an Licht und Scheinen", "tokens": ["Ich", "schritt", "an", "Licht", "und", "Schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vorbei. Und schritt. Und schritt vorbei.", "tokens": ["Vor\u00b7bei", ".", "Und", "schritt", ".", "Und", "schritt", "vor\u00b7bei", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "VVFIN", "$.", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah ein M\u00e4dchen weinen.", "tokens": ["Ich", "sah", "ein", "M\u00e4d\u00b7chen", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch meine Brille ging entzwei.", "tokens": ["Doch", "mei\u00b7ne", "Bril\u00b7le", "ging", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Bogen strich die Geige.", "tokens": ["Ein", "Bo\u00b7gen", "strich", "die", "Gei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Stumme tranken Luft.", "tokens": ["Und", "Stum\u00b7me", "tran\u00b7ken", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich streiften nasse Zweige.", "tokens": ["Mich", "streif\u00b7ten", "nas\u00b7se", "Zwei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und irgend jemand sagte \u00bbSchuft\u00ab.", "tokens": ["Und", "ir\u00b7gend", "je\u00b7mand", "sag\u00b7te", "\u00bb", "Schuft", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "ADV", "PIS", "VVFIN", "$(", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bin beinah \u00fcberfahren.", "tokens": ["Bin", "bei\u00b7nah", "\u00fc\u00b7berf\u00b7ah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Auto hat mich ausgelacht.", "tokens": ["Das", "Au\u00b7to", "hat", "mich", "aus\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo meine Freunde wohl waren", "tokens": ["Wo", "mei\u00b7ne", "Freun\u00b7de", "wohl", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VAFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In dieser gottvergessenen Nacht?", "tokens": ["In", "die\u00b7ser", "gott\u00b7ver\u00b7ges\u00b7se\u00b7nen", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}