{"textgrid.poem.36081": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XlII. Freundestrost.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Reue hinkt an Kr\u00fccken, und kann nicht eilig sein;", "tokens": ["Die", "Reu\u00b7e", "hinkt", "an", "Kr\u00fc\u00b7cken", ",", "und", "kann", "nicht", "ei\u00b7lig", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "KON", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Voran eilt That gar fl\u00fcchtig, doch Reu schleicht hinterdrein.", "tokens": ["Vo\u00b7ran", "eilt", "That", "gar", "fl\u00fcch\u00b7tig", ",", "doch", "Reu", "schleicht", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "ADJD", "$,", "ADV", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es ist ein' alte Mutter von vielem Weinen blind,", "tokens": ["Es", "ist", "ein'", "al\u00b7te", "Mut\u00b7ter", "von", "vie\u00b7lem", "Wei\u00b7nen", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIS", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von ihrem Kind geleitet, und Bessrung heisst ihr Kind.", "tokens": ["Von", "ih\u00b7rem", "Kind", "ge\u00b7lei\u00b7tet", ",", "und", "Bess\u00b7rung", "heisst", "ihr", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wo mit ihrem Kindlein die lahme Reue geht,", "tokens": ["Und", "wo", "mit", "ih\u00b7rem", "Kin\u00b7dlein", "die", "lah\u00b7me", "Reu\u00b7e", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und an die Pforten anklopft, da kommt sie meist zu sp\u00e4t,", "tokens": ["Und", "an", "die", "Pfor\u00b7ten", "an\u00b7klopft", ",", "da", "kommt", "sie", "meist", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und \u00f6ffnet wer aus Mitleid auch noch dem M\u00fctterlein,", "tokens": ["Und", "\u00f6ff\u00b7net", "wer", "aus", "Mit\u00b7leid", "auch", "noch", "dem", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "APPR", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das Kind \u2013 ist schwer zu pflegen, sie lassen's ungern ein.", "tokens": ["Das", "Kind", "\u2013", "ist", "schwer", "zu", "pfle\u00b7gen", ",", "sie", "las\u00b7sen's", "un\u00b7gern", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Da sie denn beide wandeln am Bettelstab durchs Land,", "tokens": ["Da", "sie", "denn", "bei\u00b7de", "wan\u00b7deln", "am", "Bet\u00b7tel\u00b7stab", "durchs", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Blinde treu geleitet von ihrer Tochter Hand.", "tokens": ["Die", "Blin\u00b7de", "treu", "ge\u00b7lei\u00b7tet", "von", "ih\u00b7rer", "Toch\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wer sie sieht, die Arme, gehn mit der blassen Maid,", "tokens": ["Und", "wer", "sie", "sieht", ",", "die", "Ar\u00b7me", ",", "gehn", "mit", "der", "blas\u00b7sen", "Maid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der fl\u00fcstert still und schaudernd: Dort wandeln Reu und Leid. \u2013", "tokens": ["Der", "fl\u00fcs\u00b7tert", "still", "und", "schau\u00b7dernd", ":", "Dort", "wan\u00b7deln", "Reu", "und", "Leid", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "ADJD", "$.", "ADV", "VVFIN", "NE", "KON", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit Blicken tiefen Grames sitzt Faustus kummervoll;", "tokens": ["Mit", "Bli\u00b7cken", "tie\u00b7fen", "Gra\u00b7mes", "sitzt", "Faus\u00b7tus", "kum\u00b7mer\u00b7voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "NE", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht eine Z\u00e4hre lindernd aus seinen Augen quoll.", "tokens": ["Nicht", "ei\u00b7ne", "Z\u00e4h\u00b7re", "lin\u00b7dernd", "aus", "sei\u00b7nen", "Au\u00b7gen", "quoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nicht eine Hoffnung l\u00e4chelnd durchleuchtet sein Gehirn,", "tokens": ["Nicht", "ei\u00b7ne", "Hoff\u00b7nung", "l\u00e4\u00b7chelnd", "durch\u00b7leuch\u00b7tet", "sein", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Den Wolkenflor der Trauer durchblitzt nicht ein Gestirn.", "tokens": ["Den", "Wol\u00b7ken\u00b7flor", "der", "Trau\u00b7er", "durch\u00b7blitzt", "nicht", "ein", "Ge\u00b7stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Nacht ist um ihn, recht schwarze, recht schauerliche Nacht,", "tokens": ["Nacht", "ist", "um", "ihn", ",", "recht", "schwar\u00b7ze", ",", "recht", "schau\u00b7er\u00b7li\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$,", "ADJD", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und in ihm Nacht so graunvoll, wie tief im Bergesschacht,", "tokens": ["Und", "in", "ihm", "Nacht", "so", "graun\u00b7voll", ",", "wie", "tief", "im", "Ber\u00b7ges\u00b7schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "ADV", "ADJD", "$,", "PWAV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wo durch die feuchten Kl\u00fcfte der Schwaden bl\u00e4ulich zischt,", "tokens": ["Wo", "durch", "die", "feuch\u00b7ten", "Kl\u00fcf\u00b7te", "der", "Schwa\u00b7den", "bl\u00e4u\u00b7lich", "zischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und von dem dunstgen Gifte des Steigers Licht erlischt.", "tokens": ["Und", "von", "dem", "dunst\u00b7gen", "Gif\u00b7te", "des", "Stei\u00b7gers", "Licht", "er\u00b7lischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Es geht ein grauer Berggeist durch manchen dunkeln Stollen,", "tokens": ["Es", "geht", "ein", "grau\u00b7er", "Berg\u00b7geist", "durch", "man\u00b7chen", "dun\u00b7keln", "Stol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Oft h\u00f6rt man in den Teufen ihn donner\u00e4hnlich rollen.", "tokens": ["Oft", "h\u00f6rt", "man", "in", "den", "Teu\u00b7fen", "ihn", "don\u00b7ner\u00b7\u00e4hn\u00b7lich", "rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Oft warnt er treu vor Ungl\u00fcck die Knappen dann, die Guten,", "tokens": ["Oft", "warnt", "er", "treu", "vor", "Un\u00b7gl\u00fcck", "die", "Knap\u00b7pen", "dann", ",", "die", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Oft z\u00fcchtigt er die B\u00f6sen mit unsichtbaren Ruthen.", "tokens": ["Oft", "z\u00fcch\u00b7tigt", "er", "die", "B\u00f6\u00b7sen", "mit", "un\u00b7sicht\u00b7ba\u00b7ren", "Ru\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--++--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Berggeist heisst ", "tokens": ["Der", "Berg\u00b7geist", "heisst"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O h\u00e4tten wir nur immer des treuen Warners Acht!", "tokens": ["O", "h\u00e4t\u00b7ten", "wir", "nur", "im\u00b7mer", "des", "treu\u00b7en", "War\u00b7ners", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "CARD", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch weil er oftmals still scheint, und weil er lange schont,", "tokens": ["Doch", "weil", "er", "oft\u00b7mals", "still", "scheint", ",", "und", "weil", "er", "lan\u00b7ge", "schont", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,", "KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Schreckt uns nicht mehr sein Mahnen; wir werden sein gewohnt. \u2013", "tokens": ["Schreckt", "uns", "nicht", "mehr", "sein", "Mah\u00b7nen", ";", "wir", "wer\u00b7den", "sein", "ge\u00b7wohnt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "VVPP", "$.", "$("], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Nicht Klagen mehr hat Faustus, nicht Worte mehr des Leides;", "tokens": ["Nicht", "Kla\u00b7gen", "mehr", "hat", "Faus\u00b7tus", ",", "nicht", "Wor\u00b7te", "mehr", "des", "Lei\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "VAFIN", "NE", "$,", "PTKNEG", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ach, Wort und Klage tr\u00f6sten, sein Schmerz versagt ihm beides.", "tokens": ["Ach", ",", "Wort", "und", "Kla\u00b7ge", "tr\u00f6s\u00b7ten", ",", "sein", "Schmerz", "ver\u00b7sagt", "ihm", "bei\u00b7des", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "KON", "NN", "VVINF", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Im Marmorsarg des Busens versteinert liegt sein Schmerz;", "tokens": ["Im", "Mar\u00b7mor\u00b7sarg", "des", "Bu\u00b7sens", "ver\u00b7stei\u00b7nert", "liegt", "sein", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Stumm sitzt er, stumm und reglos, gleich einem Bild aus Erz.", "tokens": ["Stumm", "sitzt", "er", ",", "stumm", "und", "reg\u00b7los", ",", "gleich", "ei\u00b7nem", "Bild", "aus", "Erz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Und Wagner naht, und mit ihm ein alter treuer Mann,", "tokens": ["Und", "Wag\u00b7ner", "naht", ",", "und", "mit", "ihm", "ein", "al\u00b7ter", "treu\u00b7er", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "KON", "APPR", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der einst f\u00fcr bessre Regung des Magus Sinn gewann.", "tokens": ["Der", "einst", "f\u00fcr", "bess\u00b7re", "Re\u00b7gung", "des", "Ma\u00b7gus", "Sinn", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dess Herz f\u00fcr jedes Leiden mit sanfter W\u00e4rme schlug,", "tokens": ["Dess", "Herz", "f\u00fcr", "je\u00b7des", "Lei\u00b7den", "mit", "sanf\u00b7ter", "W\u00e4r\u00b7me", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der mitleidvolle Tr\u00f6stung f\u00fcr ihn im Munde trug.", "tokens": ["Der", "mit\u00b7leid\u00b7vol\u00b7le", "Tr\u00f6s\u00b7tung", "f\u00fcr", "ihn", "im", "Mun\u00b7de", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbnicht also Faustus, also nicht gebt Euch hin dem Gram!", "tokens": ["\u00bb", "nicht", "al\u00b7so", "Faus\u00b7tus", ",", "al\u00b7so", "nicht", "gebt", "Euch", "hin", "dem", "Gram", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADV", "NE", "$,", "ADV", "PTKNEG", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Die Himmelslieb' errettet oft S\u00fcnder wundersam.", "tokens": ["Die", "Him\u00b7mels\u00b7lieb'", "er\u00b7ret\u00b7tet", "oft", "S\u00fcn\u00b7der", "wun\u00b7der\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob Ihr auch tief versunken im Lasterelendspfuhl,", "tokens": ["Ob", "Ihr", "auch", "tief", "ver\u00b7sun\u00b7ken", "im", "Las\u00b7te\u00b7re\u00b7lends\u00b7pfuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hat doch Erbarmung inne den Weltenrichterstuhl.\u00ab", "tokens": ["Hat", "doch", "Er\u00b7bar\u00b7mung", "in\u00b7ne", "den", "Wel\u00b7ten\u00b7rich\u00b7ter\u00b7stuhl", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbsie giesst des Zornes Schalen nicht flammend auf die Welt,", "tokens": ["\u00bb", "sie", "giesst", "des", "Zor\u00b7nes", "Scha\u00b7len", "nicht", "flam\u00b7mend", "auf", "die", "Welt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sonst w\u00fcrden, ach, wir alle Verdammten zugesellt.", "tokens": ["Sonst", "w\u00fcr\u00b7den", ",", "ach", ",", "wir", "al\u00b7le", "Ver\u00b7damm\u00b7ten", "zu\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ITJ", "$,", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob noch so schrecklich lohe der H\u00f6llenflammen Gluth,", "tokens": ["Ob", "noch", "so", "schreck\u00b7lich", "lo\u00b7he", "der", "H\u00f6l\u00b7len\u00b7flam\u00b7men", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie l\u00f6scht den Brand der H\u00f6lle mit der Erl\u00f6sung Blut!\u00ab", "tokens": ["Sie", "l\u00f6scht", "den", "Brand", "der", "H\u00f6l\u00b7le", "mit", "der", "Er\u00b7l\u00f6\u00b7sung", "Blut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "\u00bbund allen S\u00fcndern t\u00f6net ja die Verk\u00fcndigung:", "tokens": ["\u00bb", "und", "al\u00b7len", "S\u00fcn\u00b7dern", "t\u00f6\u00b7net", "ja", "die", "Ver\u00b7k\u00fcn\u00b7di\u00b7gung", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf wahre Reu und Busse wird Euch Ents\u00fcndigung!", "tokens": ["Auf", "wah\u00b7re", "Reu", "und", "Bus\u00b7se", "wird", "Euch", "Ent\u00b7s\u00fcn\u00b7di\u00b7gung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NE", "KON", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Reue lass Dein Schwert sein das gen den Feind Du kehrst,", "tokens": ["Die", "Reu\u00b7e", "lass", "Dein", "Schwert", "sein", "das", "gen", "den", "Feind", "Du", "kehrst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PPOSAT", "ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit dem Du seinen Angriff, den heftgen, von Dir wehrst!\u00ab", "tokens": ["Mit", "dem", "Du", "sei\u00b7nen", "An\u00b7griff", ",", "den", "heft\u00b7gen", ",", "von", "Dir", "wehrst", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "APPR", "PPER", "ADV", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u203aer h\u00f6rt Euch nicht!\u2039 spricht Wagner: \u00bbFruchtlos bleibt Freundesm\u00fchn,", "tokens": ["\u203a", "er", "h\u00f6rt", "Euch", "nicht", "!", "\u2039", "spricht", "Wag\u00b7ner", ":", "\u00bb", "Frucht\u00b7los", "bleibt", "Freun\u00b7des\u00b7m\u00fchn", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "VVFIN", "NN", "$.", "$(", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aus seinem dumpfen Br\u00fcten den Geist emporzuziehn.", "tokens": ["Aus", "sei\u00b7nem", "dum\u00b7pfen", "Br\u00fc\u00b7ten", "den", "Geist", "em\u00b7por\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er will nicht Tr\u00f6stung, sucht nicht an Freundesbrust sein Heil;", "tokens": ["Er", "will", "nicht", "Tr\u00f6s\u00b7tung", ",", "sucht", "nicht", "an", "Freun\u00b7des\u00b7brust", "sein", "Heil", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "$,", "VVFIN", "PTKNEG", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zu tief in seinem Busen sitzt der Verzweiflung Pfeil.\u00ab", "tokens": ["Zu", "tief", "in", "sei\u00b7nem", "Bu\u00b7sen", "sitzt", "der", "Ver\u00b7zwei\u00b7flung", "Pfeil", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Wohl gar ein k\u00f6stlich Ding ists um Freudestrost im Harm;", "tokens": ["Wohl", "gar", "ein", "k\u00f6st\u00b7lich", "Ding", "ists", "um", "Freu\u00b7des\u00b7trost", "im", "Harm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJD", "NN", "VAFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Freundschaft k\u00fcsst den Erstarrten an ihrem Busen warm.", "tokens": ["Freund\u00b7schaft", "k\u00fcsst", "den", "Er\u00b7starr\u00b7ten", "an", "ih\u00b7rem", "Bu\u00b7sen", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Freundschaft tr\u00e4uft milden Balsam in manche wunde Brust,", "tokens": ["Freund\u00b7schaft", "tr\u00e4uft", "mil\u00b7den", "Bal\u00b7sam", "in", "man\u00b7che", "wun\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Freundschaft scheut selbst nicht Opfer, bringt jedes dar mit Lust.", "tokens": ["Freund\u00b7schaft", "scheut", "selbst", "nicht", "Op\u00b7fer", ",", "bringt", "je\u00b7des", "dar", "mit", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKNEG", "NN", "$,", "VVFIN", "PIAT", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Wohl dem, den Freundesliebe in ihren Armen h\u00e4lt,", "tokens": ["Wohl", "dem", ",", "den", "Freun\u00b7des\u00b7lie\u00b7be", "in", "ih\u00b7ren", "Ar\u00b7men", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihn st\u00fctzet, wenn er strauchelt, ihm aufhilft, wenn er f\u00e4llt,", "tokens": ["Ihn", "st\u00fct\u00b7zet", ",", "wenn", "er", "strau\u00b7chelt", ",", "ihm", "auf\u00b7hilft", ",", "wenn", "er", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihn warnet, wenn er abirrt vom Pfad, der sicher f\u00fchrt,", "tokens": ["Ihn", "war\u00b7net", ",", "wenn", "er", "a\u00b7birrt", "vom", "Pfad", ",", "der", "si\u00b7cher", "f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Freundesstimme gern h\u00f6rt, gern Freundesrath erk\u00fcrt.", "tokens": ["Der", "Freun\u00b7dess\u00b7tim\u00b7me", "gern", "h\u00f6rt", ",", "gern", "Freun\u00b7des\u00b7rath", "er\u00b7k\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "ADV", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Ein lautres Gold ist Freundschaft, ein seltner Edelstein;", "tokens": ["Ein", "laut\u00b7res", "Gold", "ist", "Freund\u00b7schaft", ",", "ein", "selt\u00b7ner", "E\u00b7del\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zwei Muschelschalen schliessen die Wunderperle ein;", "tokens": ["Zwei", "Mu\u00b7schel\u00b7scha\u00b7len", "schlies\u00b7sen", "die", "Wun\u00b7der\u00b7per\u00b7le", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Schalen sind zwei Herzen, die fest zusammen halten;", "tokens": ["Die", "Scha\u00b7len", "sind", "zwei", "Her\u00b7zen", ",", "die", "fest", "zu\u00b7sam\u00b7men", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "NN", "$,", "PRELS", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wer ihren Hort will rauben, muss erst die Herzen spalten.", "tokens": ["Wer", "ih\u00b7ren", "Hort", "will", "rau\u00b7ben", ",", "muss", "erst", "die", "Her\u00b7zen", "spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "V