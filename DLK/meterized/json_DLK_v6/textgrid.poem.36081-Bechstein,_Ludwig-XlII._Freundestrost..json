inen ein heiligreiner Geist;", "tokens": ["Freund\u00b7schaft", "ist", "al\u00b7len", "Rei\u00b7nen", "ein", "hei\u00b7li\u00b7grei\u00b7ner", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und der ist zu beweinen, der Freundschaft von sich weist.", "tokens": ["Und", "der", "ist", "zu", "be\u00b7wei\u00b7nen", ",", "der", "Freund\u00b7schaft", "von", "sich", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "PTKZU", "VVINF", "$,", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Faust weist den Engel von sich, der liebreich vor ihn trat,", "tokens": ["Faust", "weist", "den", "En\u00b7gel", "von", "sich", ",", "der", "lieb\u00b7reich", "vor", "ihn", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PRF", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der mit der Trostesfackel sich seiner Nacht genaht.", "tokens": ["Der", "mit", "der", "Tros\u00b7tes\u00b7fa\u00b7ckel", "sich", "sei\u00b7ner", "Nacht", "ge\u00b7naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.19": {"line.1": {"text": "\u00bbhinweg mit Euerm Wortkram! Mich \u2013 s\u00fchnet keine Welt!", "tokens": ["\u00bb", "hin\u00b7weg", "mit", "Eu\u00b7erm", "Wort\u00b7kram", "!", "Mich", "\u2013", "s\u00fch\u00b7net", "kei\u00b7ne", 