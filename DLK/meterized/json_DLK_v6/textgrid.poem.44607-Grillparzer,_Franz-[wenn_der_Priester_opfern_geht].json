{"textgrid.poem.44607": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wenn der Priester opfern geht]", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Priester opfern geht,", "tokens": ["Wenn", "der", "Pries\u00b7ter", "op\u00b7fern", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht er mit reinen H\u00e4nden,", "tokens": ["Geht", "er", "mit", "rei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer nicht des Lebens Schmutz verschm\u00e4ht,", "tokens": ["Wer", "nicht", "des", "Le\u00b7bens", "Schmutz", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird nie das Edle vollenden.", "tokens": ["Wird", "nie", "das", "Ed\u00b7le", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Drum, ist dein Dasein dem Volk geweiht,", "tokens": ["Drum", ",", "ist", "dein", "Da\u00b7sein", "dem", "Volk", "ge\u00b7weiht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Begabst sie mit Menschheitsrechten,", "tokens": ["Be\u00b7gabst", "sie", "mit", "Menschheits\u00b7rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verb\u00fcnde dich nicht zu gleicher Zeit", "tokens": ["Ver\u00b7b\u00fcn\u00b7de", "dich", "nicht", "zu", "glei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach au\u00dfenhin mit den Schlechten.", "tokens": ["Nach", "au\u00b7\u00dfen\u00b7hin", "mit", "den", "Schlech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Damit nicht, wenn dein Werk vollbracht,", "tokens": ["Da\u00b7mit", "nicht", ",", "wenn", "dein", "Werk", "voll\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sklaven zur Freiheit kamen,", "tokens": ["Die", "Skla\u00b7ven", "zur", "Frei\u00b7heit", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Vorteilkundigen, die du gemacht,", "tokens": ["Die", "Vor\u00b7teil\u00b7kun\u00b7di\u00b7gen", ",", "die", "du", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Versuchen dich nachzuahmen.", "tokens": ["Ver\u00b7su\u00b7chen", "dich", "nach\u00b7zu\u00b7ah\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVIZU", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}