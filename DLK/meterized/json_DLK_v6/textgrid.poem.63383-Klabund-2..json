{"textgrid.poem.63383": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Regen l\u00e4uft an den H\u00e4usern entlang", "tokens": ["Der", "Re\u00b7gen", "l\u00e4uft", "an", "den", "H\u00e4u\u00b7sern", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPO"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie tausend silberne K\u00e4fer.", "tokens": ["Wie", "tau\u00b7send", "sil\u00b7ber\u00b7ne", "K\u00e4\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fahles Licht f\u00e4llt kupfern in mein Zimmer.", "tokens": ["Fah\u00b7les", "Licht", "f\u00e4llt", "kup\u00b7fern", "in", "mein", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ein Mann mit Holzbein singt auf dem Hinterhof:", "tokens": ["Ein", "Mann", "mit", "Holz\u00b7bein", "singt", "auf", "dem", "Hin\u00b7ter\u00b7hof", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Lang, lang ist's her \u2013", "tokens": ["Lang", ",", "lang", "ist's", "her", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie w\u00e4hrte kurz des Sommers hei\u00dfes Gl\u00fcck.", "tokens": ["Wie", "w\u00e4hr\u00b7te", "kurz", "des", "Som\u00b7mers", "hei\u00b7\u00dfes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So kurz wie zwischen Ku\u00df und Ku\u00df ein Hauch.", "tokens": ["So", "kurz", "wie", "zwi\u00b7schen", "Ku\u00df", "und", "Ku\u00df", "ein", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich morgens meine Haare str\u00e4hle,", "tokens": ["Wenn", "ich", "mor\u00b7gens", "mei\u00b7ne", "Haa\u00b7re", "str\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Entdecke ich immer mehr wei\u00dfe", "tokens": ["Ent\u00b7de\u00b7cke", "ich", "im\u00b7mer", "mehr", "wei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zwischen den schwarzen und grauen.", "tokens": ["Zwi\u00b7schen", "den", "schwar\u00b7zen", "und", "grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Leiser schl\u00e4gt das Herz von Tag zu Tag:", "tokens": ["Lei\u00b7ser", "schl\u00e4gt", "das", "Herz", "von", "Tag", "zu", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Die Abendglocke hinter den W\u00e4ldern.", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cke", "hin\u00b7ter", "den", "W\u00e4l\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie war vergebens alles, was ich tat:", "tokens": ["Wie", "war", "ver\u00b7ge\u00b7bens", "al\u00b7les", ",", "was", "ich", "tat", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Traum der Nacht, im Anbeginn des Tags.", "tokens": ["Im", "Traum", "der", "Nacht", ",", "im", "An\u00b7be\u00b7ginn", "des", "Tags", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich traute, vertraute Gott, dem Bruder,", "tokens": ["Ich", "trau\u00b7te", ",", "ver\u00b7trau\u00b7te", "Gott", ",", "dem", "Bru\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der mir mein Gut stahl,", "tokens": ["Der", "mir", "mein", "Gut", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mein Gutes und meine G\u00fcte.", "tokens": ["Mein", "Gu\u00b7tes", "und", "mei\u00b7ne", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Tenne dr\u00f6hnt.", "tokens": ["Die", "Ten\u00b7ne", "dr\u00f6hnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Sie dreschen volles Stroh und leere Worte.", "tokens": ["Sie", "dre\u00b7schen", "vol\u00b7les", "Stroh", "und", "lee\u00b7re", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es riecht beim Bauern nach eingekochten Zwetschgen.", "tokens": ["Es", "riecht", "beim", "Bau\u00b7ern", "nach", "ein\u00b7ge\u00b7koch\u00b7ten", "Zwetschgen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Abends nach des Tages Arbeit liest er in der Bibel:", "tokens": ["A\u00b7bends", "nach", "des", "Ta\u00b7ges", "Ar\u00b7beit", "liest", "er", "in", "der", "Bi\u00b7bel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Alles ist Liebe!", "tokens": ["Al\u00b7les", "ist", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und pr\u00fcgelt sein schwangeres Weib.", "tokens": ["Und", "pr\u00fc\u00b7gelt", "sein", "schwan\u00b7ge\u00b7res", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Der Briefbote bringt nur Verzweiflung ins Haus.", "tokens": ["Der", "Brief\u00b7bo\u00b7te", "bringt", "nur", "Ver\u00b7zwei\u00b7flung", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Meine alte Tante verkauft ihr letztes, ein rostiges Klavier.", "tokens": ["Mei\u00b7ne", "al\u00b7te", "Tan\u00b7te", "ver\u00b7kauft", "ihr", "letz\u00b7tes", ",", "ein", "ros\u00b7ti\u00b7ges", "Kla\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Sie spielt noch einmal mit kn\u00f6chrigen Fingern", "tokens": ["Sie", "spielt", "noch", "ein\u00b7mal", "mit", "kn\u00f6ch\u00b7ri\u00b7gen", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Lied ihrer Jugend:", "tokens": ["Das", "Lied", "ih\u00b7rer", "Ju\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Lang, lang ist's her \u2013", "tokens": ["Lang", ",", "lang", "ist's", "her", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}