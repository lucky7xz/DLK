{"textgrid.poem.43814": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Papa, mein St\u00fchlchen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Papa, mein St\u00fchlchen,", "tokens": ["Pa\u00b7pa", ",", "mein", "St\u00fchl\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das trauert gar sehr,", "tokens": ["Das", "trau\u00b7ert", "gar", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein liebes St\u00fchlchen", "tokens": ["Mein", "lie\u00b7bes", "St\u00fchl\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Hat keine Beine mehr.", "tokens": ["Hat", "kei\u00b7ne", "Bei\u00b7ne", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Papa, mein Tischchen", "tokens": ["Pa\u00b7pa", ",", "mein", "Tischchen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ist schief und krumm:", "tokens": ["Ist", "schief", "und", "krumm", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Will ich es stellen,", "tokens": ["Will", "ich", "es", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dann f\u00e4llt es immer um.", "tokens": ["Dann", "f\u00e4llt", "es", "im\u00b7mer", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Papa, und sieh doch!", "tokens": ["Pa\u00b7pa", ",", "und", "sieh", "doch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVIMP", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ach, Alles ist krank:", "tokens": ["Ach", ",", "Al\u00b7les", "ist", "krank", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das St\u00fchlchen, das Tischchen,", "tokens": ["Das", "St\u00fchl\u00b7chen", ",", "das", "Tischchen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Das Schr\u00e4nkchen und die Bank.", "tokens": ["Das", "Schr\u00e4nk\u00b7chen", "und", "die", "Bank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Papa, du mu\u00dft es", "tokens": ["Pa\u00b7pa", ",", "du", "mu\u00dft", "es"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mir machen zurecht,", "tokens": ["Mir", "ma\u00b7chen", "zu\u00b7recht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sonst geht's meinen Hausrath", "tokens": ["Sonst", "geht's", "mei\u00b7nen", "Haus\u00b7rath"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Noch ganz entsetzlich schlecht.", "tokens": ["Noch", "ganz", "ent\u00b7setz\u00b7lich", "schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}