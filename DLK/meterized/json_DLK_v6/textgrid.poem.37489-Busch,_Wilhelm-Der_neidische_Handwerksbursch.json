{"textgrid.poem.37489": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der neidische Handwerksbursch", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das H\u00e4hnerl hier ist f\u00fcr den Dicken.", "tokens": ["Das", "H\u00e4h\u00b7nerl", "hier", "ist", "f\u00fcr", "den", "Di\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Handwerksbursch' f\u00fchlt Magenzwicken.", "tokens": ["Der", "Hand\u00b7werks\u00b7bur\u00b7sch'", "f\u00fchlt", "Ma\u00b7gen\u00b7zwi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Zeitung ist oft int'ressant.", "tokens": ["Die", "Zei\u00b7tung", "ist", "oft", "i\u00b7nt'res\u00b7sant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein H\u00e4hnerl nimmt man gern zur Hand.", "tokens": ["Ein", "H\u00e4h\u00b7nerl", "nimmt", "man", "gern", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Politik ist sehr belehrend.", "tokens": ["Die", "Po\u00b7li\u00b7tik", "ist", "sehr", "be\u00b7leh\u00b7rend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wohlgeruch ist manchmal st\u00f6rend.", "tokens": ["Der", "Wohl\u00b7ge\u00b7ruch", "ist", "manch\u00b7mal", "st\u00f6\u00b7rend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Dicke schmaust, es perlt der Wein;", "tokens": ["Der", "Di\u00b7cke", "schmaust", ",", "es", "perlt", "der", "Wein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Handwerksbursch' schaut neidisch drein.", "tokens": ["Der", "Hand\u00b7werks\u00b7bur\u00b7sch'", "schaut", "nei\u00b7disch", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Der Handwerksbursche unverwandt", "tokens": ["Der", "Hand\u00b7werks\u00b7bur\u00b7sche", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertieft sich in den Gegenstand.", "tokens": ["Ver\u00b7tieft", "sich", "in", "den", "Ge\u00b7gen\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auch das noch! \u2013 Es ist unertr\u00e4glich! \u2013", "tokens": ["Auch", "das", "noch", "!", "\u2013", "Es", "ist", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADV", "$.", "$(", "PPER", "VAFIN", "ADJD", "$.", "$("], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Er fl\u00f6tet so leger als m\u00f6glich.", "tokens": ["Er", "fl\u00f6\u00b7tet", "so", "le\u00b7ger", "als", "m\u00f6g\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Dicke schl\u00fcrft mit viel Gef\u00fchl; \u2013", "tokens": ["Der", "Di\u00b7cke", "schl\u00fcrft", "mit", "viel", "Ge\u00b7f\u00fchl", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Handwerksburschen wird es schw\u00fcl.", "tokens": ["Dem", "Hand\u00b7werks\u00b7bur\u00b7schen", "wird", "es", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er zahlt drei Kreuzer sehr verlegen,", "tokens": ["Er", "zahlt", "drei", "Kreu\u00b7zer", "sehr", "ver\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stolz nimmt sie der Herr Wirt entgegen.", "tokens": ["Stolz", "nimmt", "sie", "der", "Herr", "Wirt", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Drei Taler zahlt der gn\u00e4d'ge Herr.", "tokens": ["Drei", "Ta\u00b7ler", "zahlt", "der", "gn\u00e4d'\u00b7ge", "Herr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ist der Wirt schon h\u00f6flicher. \u2013", "tokens": ["Da", "ist", "der", "Wirt", "schon", "h\u00f6f\u00b7li\u00b7cher", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.10": {"line.1": {"text": "Die Sonne brennt, der Staub der weht,", "tokens": ["Die", "Son\u00b7ne", "brennt", ",", "der", "Staub", "der", "weht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Dicke f\u00e4hrt, der D\u00fcnne geht.", "tokens": ["Der", "Di\u00b7cke", "f\u00e4hrt", ",", "der", "D\u00fcn\u00b7ne", "geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Handwerksbursche froh und frei", "tokens": ["Der", "Hand\u00b7werks\u00b7bur\u00b7sche", "froh", "und", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ruht sanft im duft'gen Wiesenheu.", "tokens": ["Ruht", "sanft", "im", "duft'\u00b7gen", "Wie\u00b7sen\u00b7heu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der Dicke aber \u2013 \u00bbautsch! mein Bein!\u00ab \u2013", "tokens": ["Der", "Di\u00b7cke", "a\u00b7ber", "\u2013", "\u00bb", "autsch", "!", "mein", "Bein", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADV", "$(", "$(", "ADJD", "$.", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat wieder heut' das Zipperlein.", "tokens": ["Hat", "wie\u00b7der", "heut'", "das", "Zip\u00b7per\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}