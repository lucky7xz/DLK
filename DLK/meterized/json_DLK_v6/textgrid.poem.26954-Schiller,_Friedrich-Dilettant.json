{"textgrid.poem.26954": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Dilettant", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ein Vers dir gelingt in einer gebildeten Sprache,", "tokens": ["Weil", "ein", "Vers", "dir", "ge\u00b7lingt", "in", "ei\u00b7ner", "ge\u00b7bil\u00b7de\u00b7ten", "Spra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die f\u00fcr dich dichtet und denkt, glaubst du schon Dichter zu sein.", "tokens": ["Die", "f\u00fcr", "dich", "dich\u00b7tet", "und", "denkt", ",", "glaubst", "du", "schon", "Dich\u00b7ter", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}