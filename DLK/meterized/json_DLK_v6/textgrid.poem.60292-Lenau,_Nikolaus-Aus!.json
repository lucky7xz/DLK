{"textgrid.poem.60292": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Aus!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob jeder Freude seh ich schweben", "tokens": ["Ob", "je\u00b7der", "Freu\u00b7de", "seh", "ich", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Geier bald, der sie bedroht;", "tokens": ["Den", "Gei\u00b7er", "bald", ",", "der", "sie", "be\u00b7droht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich geliebt, gesucht im Leben,", "tokens": ["Was", "ich", "ge\u00b7liebt", ",", "ge\u00b7sucht", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist verloren oder tot.", "tokens": ["Es", "ist", "ver\u00b7lo\u00b7ren", "o\u00b7der", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fort ri\u00df der Tod in seinem Grimme", "tokens": ["Fort", "ri\u00df", "der", "Tod", "in", "sei\u00b7nem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Von meinem Gl\u00fcck die letzte Spur;", "tokens": ["Von", "mei\u00b7nem", "Gl\u00fcck", "die", "letz\u00b7te", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Menschenherz hat keine Stimme", "tokens": ["Das", "Men\u00b7schen\u00b7herz", "hat", "kei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im finstern Rate der Natur.", "tokens": ["Im", "fins\u00b7tern", "Ra\u00b7te", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich will nicht l\u00e4nger t\u00f6richt haschen", "tokens": ["Ich", "will", "nicht", "l\u00e4n\u00b7ger", "t\u00f6\u00b7richt", "ha\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach tr\u00fcber Fluten hellem Schaum,", "tokens": ["Nach", "tr\u00fc\u00b7ber", "Flu\u00b7ten", "hel\u00b7lem", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab aus den Augen mir gewaschen", "tokens": ["Hab", "aus", "den", "Au\u00b7gen", "mir", "ge\u00b7wa\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Tr\u00e4nen scharf den letzten Traum.", "tokens": ["Mit", "Tr\u00e4\u00b7nen", "scharf", "den", "letz\u00b7ten", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}