{"dta.poem.11049": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich spreche nicht von Ungl\u00fccksf\u00e4llen, ich bin allein darauf", "tokens": ["Ich", "spre\u00b7che", "nicht", "von", "Un\u00b7gl\u00fccks\u00b7f\u00e4l\u00b7len", ",", "ich", "bin", "al\u00b7lein", "da\u00b7rauf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "PPER", "VAFIN", "ADV", "PAV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Des Guten besser zu gebrauchen, das sonsten ungef\u00fchlt ver-", "tokens": ["Des", "Gu\u00b7ten", "bes\u00b7ser", "zu", "ge\u00b7brau\u00b7chen", ",", "das", "sons\u00b7ten", "un\u00b7ge\u00b7f\u00fchlt", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Zumalen wir noch \u00fcberdem erwogner Dinge fl\u00fcchtges Rennen,", "tokens": ["Zu\u00b7ma\u00b7len", "wir", "noch", "\u00fc\u00b7ber\u00b7dem", "er\u00b7wog\u00b7ner", "Din\u00b7ge", "fl\u00fccht\u00b7ges", "Ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Durch die ", "tokens": ["Durch", "die"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}}}}}