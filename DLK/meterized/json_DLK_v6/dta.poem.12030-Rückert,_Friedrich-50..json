{"dta.poem.12030": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Regens Tropfen spr\u00fchn, doch wird davon nicht gr\u00fcn", "tokens": ["Des", "Re\u00b7gens", "Trop\u00b7fen", "spr\u00fchn", ",", "doch", "wird", "da\u00b7von", "nicht", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PAV", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Rasen, den versengt der Sommersonne Gl\u00fchn.", "tokens": ["Der", "Ra\u00b7sen", ",", "den", "ver\u00b7sengt", "der", "Som\u00b7mer\u00b7son\u00b7ne", "Gl\u00fchn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Gr\u00e4ser bleiben d\u00fcrr, doch neue sprie\u00dfen drunter,", "tokens": ["Die", "Gr\u00e4\u00b7ser", "blei\u00b7ben", "d\u00fcrr", ",", "doch", "neu\u00b7e", "sprie\u00b7\u00dfen", "drun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMINF", "$,", "ADV", "ADJA", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcbergr\u00fcnen bald die alten frisch und munter.", "tokens": ["Und", "\u00fc\u00b7ber\u00b7gr\u00fc\u00b7nen", "bald", "die", "al\u00b7ten", "frisch", "und", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Getrost, o Herz! dir bringt Verlornes nicht zur\u00fcck", "tokens": ["Ge\u00b7trost", ",", "o", "Herz", "!", "dir", "bringt", "Ver\u00b7lor\u00b7nes", "nicht", "zu\u00b7r\u00fcck"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "NN", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Stunde, doch daf\u00fcr bringt sie ein neues Gl\u00fcck.", "tokens": ["Die", "Stun\u00b7de", ",", "doch", "da\u00b7f\u00fcr", "bringt", "sie", "ein", "neu\u00b7es", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}