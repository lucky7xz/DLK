{"textgrid.poem.64192": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der deutsche Fl\u00fcchtling", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich haus' allein im wilden Wald,", "tokens": ["Ich", "haus'", "al\u00b7lein", "im", "wil\u00b7den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im fernen, fernen Westen;", "tokens": ["Im", "fer\u00b7nen", ",", "fer\u00b7nen", "Wes\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Wolf, den Graub\u00e4r ungestalt", "tokens": ["Den", "Wolf", ",", "den", "Grau\u00b7b\u00e4r", "un\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab' einzig ich zu G\u00e4sten:", "tokens": ["Hab'", "ein\u00b7zig", "ich", "zu", "G\u00e4s\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es nahet mir kein Menschenfu\u00df,", "tokens": ["Es", "na\u00b7het", "mir", "kein", "Men\u00b7schen\u00b7fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es gr\u00fc\u00dfet mich kein Freundesgru\u00df: \u2013", "tokens": ["Es", "gr\u00fc\u00b7\u00dfet", "mich", "kein", "Freun\u00b7des\u00b7gru\u00df", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sturm pfeift in den \u00c4sten.", "tokens": ["Der", "Sturm", "pfeift", "in", "den", "\u00c4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit Gram seh' ich der Wolken Heer,", "tokens": ["Mit", "Gram", "seh'", "ich", "der", "Wol\u00b7ken", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die frei nach Osten streifen:", "tokens": ["Die", "frei", "nach", "Os\u00b7ten", "strei\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Schwalben, die beneid' ich schwer,", "tokens": ["Die", "Schwal\u00b7ben", ",", "die", "be\u00b7neid'", "ich", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die heim nach Deutschland schweifen: \u2013", "tokens": ["Die", "heim", "nach", "Deutschland", "schwei\u00b7fen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PTKVZ", "APPR", "NE", "VVFIN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich denk', wie, wo der Neckar geht,", "tokens": ["Ich", "denk'", ",", "wie", ",", "wo", "der", "Ne\u00b7ckar", "geht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein H\u00fcttlein dicht in Reben steht,", "tokens": ["Ein", "H\u00fctt\u00b7lein", "dicht", "in", "Re\u00b7ben", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dran jetzt die Trauben reifen.", "tokens": ["Dran", "jetzt", "die", "Trau\u00b7ben", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ich denk', wie nun das Dorf entlang", "tokens": ["Ich", "denk'", ",", "wie", "nun", "das", "Dorf", "ent\u00b7lang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "APPO"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Kerz' entfacht an Kerzen,", "tokens": ["Sich", "Ker\u00b7z'", "ent\u00b7facht", "an", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie vor der T\u00fcr am Wiesenhang", "tokens": ["Wie", "vor", "der", "T\u00fcr", "am", "Wie\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die blonden Buben scherzen!", "tokens": ["Die", "blon\u00b7den", "Bu\u00b7ben", "scher\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich denk', wie dort zu dieser Zeit", "tokens": ["Ich", "denk'", ",", "wie", "dort", "zu", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Abendglocke hallet weit: \u2013", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cke", "hal\u00b7let", "weit", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und weh wird mir im Herzen!", "tokens": ["Und", "weh", "wird", "mir", "im", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein einsam Feuer z\u00fcnd' ich an,", "tokens": ["Mein", "ein\u00b7sam", "Feu\u00b7er", "z\u00fcnd'", "ich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schau' in die n\u00e4cht'ge Ferne: \u2013", "tokens": ["Schau'", "in", "die", "n\u00e4cht'\u00b7ge", "Fer\u00b7ne", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier bleib' ich stets ein fremder Mann,", "tokens": ["Hier", "bleib'", "ich", "stets", "ein", "frem\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fremd sind mir selbst die Sterne:", "tokens": ["Fremd", "sind", "mir", "selbst", "die", "Ster\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "O s\u00e4h' ich nur einzigmal", "tokens": ["O", "s\u00e4h'", "ich", "nur", "ein\u00b7zig\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein Vaterhaus im Abendstrahl, \u2013", "tokens": ["Mein", "Va\u00b7ter\u00b7haus", "im", "A\u00b7bend\u00b7strahl", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich st\u00fcrbe \u2013 ach wie gerne! \u2013", "tokens": ["Ich", "st\u00fcr\u00b7be", "\u2013", "ach", "wie", "ger\u00b7ne", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$(", "XY", "KOKOM", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}