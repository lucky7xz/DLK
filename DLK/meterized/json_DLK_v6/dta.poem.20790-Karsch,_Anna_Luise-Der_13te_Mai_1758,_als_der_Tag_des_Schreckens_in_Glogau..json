{"dta.poem.20790": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der 13te Mai 1758,  \n als der                \n  Tag des Schreckens in Glogau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbte! da du dich jedweder Lust verzeihst,               ", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "!", "da", "du", "dich", "jed\u00b7we\u00b7der", "Lust", "ver\u00b7zeihst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf dein Haupt den Staub von deinem Schmucke", "tokens": ["Und", "auf", "dein", "Haupt", "den", "Staub", "von", "dei\u00b7nem", "Schmu\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "streust,", "tokens": ["streust", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Da sing ich einen Tag, der dich voll Jammer machte,", "tokens": ["Da", "sing", "ich", "ei\u00b7nen", "Tag", ",", "der", "dich", "voll", "Jam\u00b7mer", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als deines Scheitels Pracht abscheulich niederkrachte.", "tokens": ["Als", "dei\u00b7nes", "Schei\u00b7tels", "Pracht", "ab\u00b7scheu\u00b7lich", "nie\u00b7der\u00b7krach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich singe Gottes Zorn, der \u00fcber dir entbrannt,", "tokens": ["Ich", "sin\u00b7ge", "Got\u00b7tes", "Zorn", ",", "der", "\u00fc\u00b7ber", "dir", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da als sein Knecht der Sturm sich mit der Gluth", "tokens": ["Da", "als", "sein", "Knecht", "der", "Sturm", "sich", "mit", "der", "Gluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "ART", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "verband.", "tokens": ["ver\u00b7band", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "zerstreuet,", "tokens": ["zer\u00b7streu\u00b7et", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Und lachend hattest du dich zu dem Fest geweihet,", "tokens": ["Und", "la\u00b7chend", "hat\u00b7test", "du", "dich", "zu", "dem", "Fest", "ge\u00b7wei\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das man mit Mayen schm\u00fcckt bis an des Altars Horn,", "tokens": ["Das", "man", "mit", "Ma\u00b7yen", "schm\u00fcckt", "bis", "an", "des", "Al\u00b7tars", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Ahndung sagte dir nichts von des H\u00f6chsten Zorn,", "tokens": ["Die", "Ahn\u00b7dung", "sag\u00b7te", "dir", "nichts", "von", "des", "H\u00f6chs\u00b7ten", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Bis die Erschrockenheit in deinen Gassen heulte,", "tokens": ["Bis", "die", "Er\u00b7schro\u00b7cken\u00b7heit", "in", "dei\u00b7nen", "Gas\u00b7sen", "heul\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und der Zerst\u00f6hrung Flug die Fl\u00fcchtigen ereilte.", "tokens": ["Und", "der", "Zer\u00b7st\u00f6h\u00b7rung", "Flug", "die", "Fl\u00fcch\u00b7ti\u00b7gen", "er\u00b7eil\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Welch ein verwirrt Ger\u00e4usch nahm deine Stra\u00dfen ein!", "tokens": ["Welch", "ein", "ver\u00b7wirrt", "Ge\u00b7r\u00e4usch", "nahm", "dei\u00b7ne", "Stra\u00b7\u00dfen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Befl\u00fcgelt von der Angst lief unter hohlem Schreyn", "tokens": ["Be\u00b7fl\u00fc\u00b7gelt", "von", "der", "Angst", "lief", "un\u00b7ter", "hoh\u00b7lem", "Schreyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein Schwarm von B\u00fcrgern hin, damit er auf die", "tokens": ["Ein", "Schwarm", "von", "B\u00fcr\u00b7gern", "hin", ",", "da\u00b7mit", "er", "auf", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Th\u00fcrme", "tokens": ["Th\u00fcr\u00b7me"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Mit Wasser und mit M\u00fch der Gluth entgegen st\u00fcrme;", "tokens": ["Mit", "Was\u00b7ser", "und", "mit", "M\u00fch", "der", "Gluth", "ent\u00b7ge\u00b7gen", "st\u00fcr\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vereitelt ward die M\u00fch, vergr\u00f6\u00dfert die Gefahr,", "tokens": ["Ver\u00b7ei\u00b7telt", "ward", "die", "M\u00fch", ",", "ver\u00b7gr\u00f6\u00b7\u00dfert", "die", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Unaufhaltsam die Gluth, der B\u00fcrger nahm es wahr.", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "die", "Gluth", ",", "der", "B\u00fcr\u00b7ger", "nahm", "es", "wahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Und nun entschlo\u00df er sich den Seinen zuzulaufen.", "tokens": ["Und", "nun", "ent\u00b7schlo\u00df", "er", "sich", "den", "Sei\u00b7nen", "zu\u00b7zu\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ART", "PPOSS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie, wann ein Sch\u00e4ferstab den vollen Ameishaufen", "tokens": ["Wie", ",", "wann", "ein", "Sch\u00e4\u00b7fer\u00b7stab", "den", "vol\u00b7len", "Am\u00b7eis\u00b7hau\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zerst\u00f6hrerisch durchw\u00fchlt, das sammlende Insekt", "tokens": ["Zer\u00b7st\u00f6h\u00b7re\u00b7risch", "durch\u00b7w\u00fchlt", ",", "das", "samm\u00b7len\u00b7de", "In\u00b7sekt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mit banger Forschbegier des Schreckens Grund", "tokens": ["Mit", "ban\u00b7ger", "Forschbe\u00b7gier", "des", "Schre\u00b7ckens", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "entdeckt,", "tokens": ["ent\u00b7deckt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.28": {"text": "Den Nachbarinnen winkt und um den Stab sich h\u00e4ufet,", "tokens": ["Den", "Nach\u00b7ba\u00b7rin\u00b7nen", "winkt", "und", "um", "den", "Stab", "sich", "h\u00e4u\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dann seinem Vorrath nimmt und schnell die Flucht", "tokens": ["Dann", "sei\u00b7nem", "Vor\u00b7rath", "nimmt", "und", "schnell", "die", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "ergreifet,", "tokens": ["er\u00b7grei\u00b7fet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Und in dem \u00f6den Wald sich andre Wohnung sucht;", "tokens": ["Und", "in", "dem", "\u00f6\u00b7den", "Wald", "sich", "and\u00b7re", "Woh\u00b7nung", "sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PRF", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So wimmelnd, so best\u00fcrzt ergrif dein Volk die Flucht.", "tokens": ["So", "wim\u00b7melnd", ",", "so", "be\u00b7st\u00fcrzt", "er\u00b7grif", "dein", "Volk", "die", "Flucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Nun wankten Wittwen fort, des Mitleids Gegenst\u00e4nde!", "tokens": ["Nun", "wank\u00b7ten", "Witt\u00b7wen", "fort", ",", "des", "Mit\u00b7leids", "Ge\u00b7gen\u00b7st\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und Waysen wanden sich um ihrer M\u00fctter H\u00e4nde;", "tokens": ["Und", "Way\u00b7sen", "wan\u00b7den", "sich", "um", "ih\u00b7rer", "M\u00fct\u00b7ter", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Nun floh der beste Mann, ihm loderte das Haus", "tokens": ["Nun", "floh", "der", "bes\u00b7te", "Mann", ",", "ihm", "lo\u00b7der\u00b7te", "das", "Haus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Schon auf den Nacken nach, die Hitze trieb ihn aus.", "tokens": ["Schon", "auf", "den", "Na\u00b7cken", "nach", ",", "die", "Hit\u00b7ze", "trieb", "ihn", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nur sein geduldig Herz und seiner Gattin Schritte", "tokens": ["Nur", "sein", "ge\u00b7dul\u00b7dig", "Herz", "und", "sei\u00b7ner", "Gat\u00b7tin", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJD", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Begleiteten ihn hin in eine fremde H\u00fctte;", "tokens": ["Be\u00b7glei\u00b7te\u00b7ten", "ihn", "hin", "in", "ei\u00b7ne", "frem\u00b7de", "H\u00fct\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dort brannte Dach und Wand, die Wuth der Flam-", "tokens": ["Dort", "brann\u00b7te", "Dach", "und", "Wand", ",", "die", "Wuth", "der", "Flam"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "men schlug", "tokens": ["men", "schlug"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.41": {"text": "Dem feuchten Bette nach, das einen Kranken trug.", "tokens": ["Dem", "feuch\u00b7ten", "Bet\u00b7te", "nach", ",", "das", "ei\u00b7nen", "Kran\u00b7ken", "trug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die Tr\u00e4ger liefen fort, der Vorwurf vom Erbarmen", "tokens": ["Die", "Tr\u00e4\u00b7ger", "lie\u00b7fen", "fort", ",", "der", "Vor\u00b7wurf", "vom", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Lag in der Ohnmacht da, wie in des Todes Armen;", "tokens": ["Lag", "in", "der", "Ohn\u00b7macht", "da", ",", "wie", "in", "des", "To\u00b7des", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Und weinend ward sein Blick dem Himmel zugewandt,", "tokens": ["Und", "wei\u00b7nend", "ward", "sein", "Blick", "dem", "Him\u00b7mel", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Da er ermuntert sich im fremden Hause fand.", "tokens": ["Da", "er", "er\u00b7mun\u00b7tert", "sich", "im", "frem\u00b7den", "Hau\u00b7se", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Er rief: Ein Wunderwerk hat mich hieher getragen,", "tokens": ["Er", "rief", ":", "Ein", "Wun\u00b7der\u00b7werk", "hat", "mich", "hie\u00b7her", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und h\u00f6rte rund um sich vom Tag des Schreckens sagen.", "tokens": ["Und", "h\u00f6r\u00b7te", "rund", "um", "sich", "vom", "Tag", "des", "Schre\u00b7ckens", "sa\u00b7gen", "."], "token_info": ["word", 