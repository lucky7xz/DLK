{"textgrid.poem.65256": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Kr\u00e4he", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Kr\u00e4he war mit mir", "tokens": ["Ei\u00b7ne", "Kr\u00e4\u00b7he", "war", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Stadt gezogen,", "tokens": ["Aus", "der", "Stadt", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist bis heute f\u00fcr und f\u00fcr", "tokens": ["Ist", "bis", "heu\u00b7te", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um mein Haupt geflogen.", "tokens": ["Um", "mein", "Haupt", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Kr\u00e4he, wunderliches Thier,", "tokens": ["Kr\u00e4\u00b7he", ",", "wun\u00b7der\u00b7li\u00b7ches", "Thier", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Willst mich nicht verlassen?", "tokens": ["Willst", "mich", "nicht", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meinst wohl bald als Beute hier", "tokens": ["Meinst", "wohl", "bald", "als", "Beu\u00b7te", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "KOUS", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Leib zu fassen?", "tokens": ["Mei\u00b7nen", "Leib", "zu", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nun, es wird nicht weit mehr gehn", "tokens": ["Nun", ",", "es", "wird", "nicht", "weit", "mehr", "gehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem Wanderstabe.", "tokens": ["An", "dem", "Wan\u00b7der\u00b7sta\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kr\u00e4he, la\u00df mich endlich sehn", "tokens": ["Kr\u00e4\u00b7he", ",", "la\u00df", "mich", "end\u00b7lich", "sehn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Treue bis zum Grabe!", "tokens": ["Treu\u00b7e", "bis", "zum", "Gra\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}