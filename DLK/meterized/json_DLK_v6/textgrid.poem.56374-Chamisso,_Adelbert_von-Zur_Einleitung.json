{"textgrid.poem.56374": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Zur Einleitung", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mir im Busen schwoll, mir unbewu\u00dft,", "tokens": ["Was", "mir", "im", "Bu\u00b7sen", "schwoll", ",", "mir", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ADJD", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich konnt es nicht verhindern, ward Gesang;", "tokens": ["Ich", "konnt", "es", "nicht", "ver\u00b7hin\u00b7dern", ",", "ward", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Liede ward mir jede s\u00fc\u00dfe Lust,", "tokens": ["Zum", "Lie\u00b7de", "ward", "mir", "je\u00b7de", "s\u00fc\u00b7\u00dfe", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Liede jeder Schmerz, mit dem ich rang;", "tokens": ["Zum", "Lie\u00b7de", "je\u00b7der", "Schmerz", ",", "mit", "dem", "ich", "rang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Lied erhob aus zornerkrankter Brust", "tokens": ["Das", "Lied", "er\u00b7hob", "aus", "zor\u00b7ner\u00b7krank\u00b7ter", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sich sturmbefl\u00fcgelt in der Zeiten Drang;", "tokens": ["Sich", "sturm\u00b7be\u00b7fl\u00fc\u00b7gelt", "in", "der", "Zei\u00b7ten", "Drang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich h\u00f6rte nur die eigne Stimme rauschen", "tokens": ["Ich", "h\u00f6r\u00b7te", "nur", "die", "eig\u00b7ne", "Stim\u00b7me", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sorgte nicht, man k\u00f6nne mich belauschen.", "tokens": ["Und", "sorg\u00b7te", "nicht", ",", "man", "k\u00f6n\u00b7ne", "mich", "be\u00b7lau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PIS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch ihr, die ich bewundert wie die Sterne", "tokens": ["Doch", "ihr", ",", "die", "ich", "be\u00b7wun\u00b7dert", "wie", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "VVPP", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Himmels \u00fcber mir, so hoch und klar,", "tokens": ["Des", "Him\u00b7mels", "\u00fc\u00b7ber", "mir", ",", "so", "hoch", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die nur entbl\u00f6\u00dften Hauptes aus der Ferne", "tokens": ["Die", "nur", "ent\u00b7bl\u00f6\u00df\u00b7ten", "Haup\u00b7tes", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu gr\u00fc\u00dfen, mir ein Traum des D\u00fcnkels war,", "tokens": ["Zu", "gr\u00fc\u00b7\u00dfen", ",", "mir", "ein", "Traum", "des", "D\u00fcn\u00b7kels", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PPER", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr meine hohen Meister, lauschtet gerne", "tokens": ["Ihr", "mei\u00b7ne", "ho\u00b7hen", "Meis\u00b7ter", ",", "lauschtet", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dem schlichten Laut, aufblickend nahm ich wahr,", "tokens": ["Dem", "schlich\u00b7ten", "Laut", ",", "auf\u00b7bli\u00b7ckend", "nahm", "ich", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So wie des Liedes Wogen ausgebrandet,", "tokens": ["So", "wie", "des", "Lie\u00b7des", "Wo\u00b7gen", "aus\u00b7ge\u00b7bran\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df l\u00e4chelnd ihr im Kreise mich umstandet.", "tokens": ["Da\u00df", "l\u00e4\u00b7chelnd", "ihr", "im", "Krei\u00b7se", "mich", "um\u00b7stan\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und eurem hohen Chor war's mir beschieden,", "tokens": ["Und", "eu\u00b7rem", "ho\u00b7hen", "Chor", "wa\u00b7r's", "mir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Err\u00f6tend fa\u00df ich's nicht, mich anzureihn;", "tokens": ["Er\u00b7r\u00f6\u00b7tend", "fa\u00df", "ich's", "nicht", ",", "mich", "an\u00b7zu\u00b7reihn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PIS", "PTKNEG", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl herrlich ist es, von den Homeriden \u2013", "tokens": ["Wohl", "herr\u00b7lich", "ist", "es", ",", "von", "den", "Ho\u00b7me\u00b7ri\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "APPR", "ART", "NE", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Gr\u00f6\u00dfrer sprach's \u2013 der letzte noch zu sein;", "tokens": ["Ein", "Gr\u00f6\u00df\u00b7rer", "sprach's", "\u2013", "der", "letz\u00b7te", "noch", "zu", "sein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "ADJA", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr schm\u00fccktet mit der Binde mich hienieden,", "tokens": ["Ihr", "schm\u00fcck\u00b7tet", "mit", "der", "Bin\u00b7de", "mich", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich werde nicht das Priestertum entweihn;", "tokens": ["Ich", "wer\u00b7de", "nicht", "das", "Pries\u00b7ter\u00b7tum", "ent\u00b7weihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Ernst, die Liebe wohnen mir im Busen,", "tokens": ["Der", "Ernst", ",", "die", "Lie\u00b7be", "woh\u00b7nen", "mir", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und also schreit ich zum Altar der Musen.", "tokens": ["Und", "al\u00b7so", "schreit", "ich", "zum", "Al\u00b7tar", "der", "Mu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr habet auf die Stufen dieser Halle", "tokens": ["Ihr", "ha\u00b7bet", "auf", "die", "Stu\u00b7fen", "die\u00b7ser", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als W\u00e4chter mich und Herold hingestellt;", "tokens": ["Als", "W\u00e4ch\u00b7ter", "mich", "und", "He\u00b7rold", "hin\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Feste des Gesanges lad ich alle,", "tokens": ["Zum", "Fes\u00b7te", "des", "Ge\u00b7san\u00b7ges", "lad", "ich", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Herein, herein! das deutsche Lied erschalle", "tokens": ["Her\u00b7ein", ",", "her\u00b7ein", "!", "das", "deut\u00b7sche", "Lied", "er\u00b7schal\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vollt\u00f6nig, kr\u00e4ftig in die ernste Welt;", "tokens": ["Voll\u00b7t\u00f6\u00b7nig", ",", "kr\u00e4f\u00b7tig", "in", "die", "erns\u00b7te", "Welt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Herein! du Meister mit der Lorbeer-Krone;", "tokens": ["Her\u00b7ein", "!", "du", "Meis\u00b7ter", "mit", "der", "Lor\u00b7beer\u00b7Kro\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du J\u00fcnger, der noch ringt nach gleichem Lohne.", "tokens": ["Du", "J\u00fcn\u00b7ger", ",", "der", "noch", "ringt", "nach", "glei\u00b7chem", "Loh\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Herein! du J\u00fcnger; zaudre nicht zu neigen", "tokens": ["Her\u00b7ein", "!", "du", "J\u00fcn\u00b7ger", ";", "zaud\u00b7re", "nicht", "zu", "nei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKVZ", "$.", "PPER", "NN", "$.", "VVFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein lock'ges Haupt vor deinen Meistern hier;", "tokens": ["Dein", "lock'\u00b7ges", "Haupt", "vor", "dei\u00b7nen", "Meis\u00b7tern", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir ziemt vor ihnen Ehrfurcht wohl zu zeigen,", "tokens": ["Dir", "ziemt", "vor", "ih\u00b7nen", "Ehr\u00b7furcht", "wohl", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du ringst hinan zu ihrem Lichtrevier;", "tokens": ["Du", "ringst", "hi\u00b7nan", "zu", "ih\u00b7rem", "Licht\u00b7re\u00b7vier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wehte nicht aus ihres Lorbeers Zweigen", "tokens": ["Und", "weh\u00b7te", "nicht", "aus", "ih\u00b7res", "Lor\u00b7beers", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Gottes Sch\u00f6pferatem erst zu dir?", "tokens": ["Des", "Got\u00b7tes", "Sch\u00f6p\u00b7fe\u00b7ra\u00b7tem", "erst", "zu", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bin so wie du, obschon in grauen Haaren,", "tokens": ["Bin", "so", "wie", "du", ",", "ob\u00b7schon", "in", "grau\u00b7en", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein J\u00fcnger nur; vertraue meinen Jahren.", "tokens": ["Ein", "J\u00fcn\u00b7ger", "nur", ";", "ver\u00b7trau\u00b7e", "mei\u00b7nen", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Herein! du Dichterf\u00fcrst in deinem Ruhme,", "tokens": ["Her\u00b7ein", "!", "du", "Dich\u00b7ter\u00b7f\u00fcrst", "in", "dei\u00b7nem", "Ruh\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und la\u00df die M\u00e4chte deiner Lieder walten;", "tokens": ["Und", "la\u00df", "die", "M\u00e4ch\u00b7te", "dei\u00b7ner", "Lie\u00b7der", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beschirme diese du im Heiligtume,", "tokens": ["Be\u00b7schir\u00b7me", "die\u00b7se", "du", "im", "Hei\u00b7lig\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir ziemt die Jugend ehrenvoll zu halten;", "tokens": ["Dir", "ziemt", "die", "Ju\u00b7gend", "eh\u00b7ren\u00b7voll", "zu", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer wei\u00df, ob nicht die erst erschlo\u00dfne Blume", "tokens": ["Wer", "wei\u00df", ",", "ob", "nicht", "die", "erst", "er\u00b7schlo\u00df\u00b7ne", "Blu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PTKNEG", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zur sch\u00f6nern Frucht sich werde noch entfalten?", "tokens": ["Zur", "sch\u00f6\u00b7nern", "Frucht", "sich", "wer\u00b7de", "noch", "ent\u00b7fal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du hast, wie sie, im niedern Wald verborgen", "tokens": ["Du", "hast", ",", "wie", "sie", ",", "im", "nie\u00b7dern", "Wald", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "$,", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gerungen und gestrebt an dei