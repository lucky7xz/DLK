{"textgrid.poem.58918": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "137. Getrost in Gott", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei doch in Gott getrost und froh", "tokens": ["Sei", "doch", "in", "Gott", "ge\u00b7trost", "und", "froh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00df den Mut nicht sinken so!", "tokens": ["Und", "la\u00df", "den", "Mut", "nicht", "sin\u00b7ken", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott selbst will sein der Schwachen St\u00e4rke;", "tokens": ["Gott", "selbst", "will", "sein", "der", "Schwa\u00b7chen", "St\u00e4r\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df ihn nur wirken deine Werke!", "tokens": ["La\u00df", "ihn", "nur", "wir\u00b7ken", "dei\u00b7ne", "Wer\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}