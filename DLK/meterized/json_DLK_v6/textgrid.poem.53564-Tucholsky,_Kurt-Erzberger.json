{"textgrid.poem.53564": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Erzberger", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du guter Mond aus Buttenhausen!", "tokens": ["Du", "gu\u00b7ter", "Mond", "aus", "But\u00b7ten\u00b7hau\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du leuchtest durch den Wolkenflor.", "tokens": ["Du", "leuch\u00b7test", "durch", "den", "Wol\u00b7ken\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn auch die b\u00f6sen St\u00fcrme brausen \u2013", "tokens": ["Wenn", "auch", "die", "b\u00f6\u00b7sen", "St\u00fcr\u00b7me", "brau\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sanft strahlt dein mildes Rund empor.", "tokens": ["sanft", "strahlt", "dein", "mil\u00b7des", "Rund", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ob der ganze Schnee verbrennt,", "tokens": ["Und", "ob", "der", "gan\u00b7ze", "Schnee", "ver\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ob uns ein leiser Zephir f\u00e4chelt \u2013", "tokens": ["ob", "uns", "ein", "lei\u00b7ser", "Ze\u00b7phir", "f\u00e4\u00b7chelt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "wie immer auch das Firmament:", "tokens": ["wie", "im\u00b7mer", "auch", "das", "Fir\u00b7ma\u00b7ment", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Matthias l\u00e4chelt.", "tokens": ["Mat\u00b7thi\u00b7as", "l\u00e4\u00b7chelt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Was hattest du im Krieg zu schuften!", "tokens": ["Was", "hat\u00b7test", "du", "im", "Krieg", "zu", "schuf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du reistest in und aus der Schweiz.", "tokens": ["Du", "reis\u00b7test", "in", "und", "aus", "der", "Schweiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "T\u00e4t wo ein kleines St\u00e4nklein duften,", "tokens": ["T\u00e4t", "wo", "ein", "klei\u00b7nes", "St\u00e4n\u00b7klein", "duf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "du, Lieber, wu\u00dftest es bereits.", "tokens": ["du", ",", "Lie\u00b7ber", ",", "wu\u00df\u00b7test", "es", "be\u00b7reits", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gewi\u00df, du hast den Zimt erkannt,", "tokens": ["Ge\u00b7wi\u00df", ",", "du", "hast", "den", "Zimt", "er\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "hast Tirpitz wacker durchgehechelt . . .", "tokens": ["hast", "Tir\u00b7pitz", "wa\u00b7cker", "durch\u00b7ge\u00b7he\u00b7chelt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "NE", "ADJD", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Trost blieb uns im Weltenbrand:", "tokens": ["Ein", "Trost", "blieb", "uns", "im", "Wel\u00b7ten\u00b7brand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Matthias l\u00e4chelt.", "tokens": ["Mat\u00b7thi\u00b7as", "l\u00e4\u00b7chelt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Was bist du alles schon gewesen!", "tokens": ["Was", "bist", "du", "al\u00b7les", "schon", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PIS", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein wilder Weltannexionist", "tokens": ["Ein", "wil\u00b7der", "Wel\u00b7tan\u00b7ne\u00b7xi\u00b7o\u00b7nist"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "(man kann es leider heut noch lesen),", "tokens": ["(", "man", "kann", "es", "lei\u00b7der", "heut", "noch", "le\u00b7sen", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dann, als es schief ging, Pazifist . . .", "tokens": ["dann", ",", "als", "es", "schief", "ging", ",", "Pa\u00b7zi\u00b7fist", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man sah dich stets mit wem paktieren,", "tokens": ["Man", "sah", "dich", "stets", "mit", "wem", "pak\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "du machtest dich dem Reich bezahlt . . .", "tokens": ["du", "mach\u00b7test", "dich", "dem", "Reich", "be\u00b7zahlt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir wu\u00dften: Uns kann nichts passieren \u2013", "tokens": ["Wir", "wu\u00df\u00b7ten", ":", "Uns", "kann", "nichts", "pas\u00b7sie\u00b7ren", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Matthias strahlt.", "tokens": ["Mat\u00b7thi\u00b7as", "strahlt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Du sanft Gestirn stehst nun am Himmel", "tokens": ["Du", "sanft", "Ge\u00b7stirn", "stehst", "nun", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und \u2013 leider Gottes! \u2013 im Zenit.", "tokens": ["und", "\u2013", "lei\u00b7der", "Got\u00b7tes", "!", "\u2013", "im", "Ze\u00b7nit", "."], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "NN", "$.", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewi\u00df, du bist in dem Gewimmel", "tokens": ["Ge\u00b7wi\u00df", ",", "du", "bist", "in", "dem", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der schlimmste nicht, den man da sieht.", "tokens": ["der", "schlimms\u00b7te", "nicht", ",", "den", "man", "da", "sieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sterne in der hohen Halle,", "tokens": ["Die", "Ster\u00b7ne", "in", "der", "ho\u00b7hen", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die \u00fcbler Kriegsgewinst geeint,", "tokens": ["die", "\u00fcb\u00b7ler", "Kriegs\u00b7ge\u00b7winst", "ge\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "du \u00fcberstrahlst sie alle, alle \u2013 \u2013", "tokens": ["du", "\u00fc\u00b7bers\u00b7trahlst", "sie", "al\u00b7le", ",", "al\u00b7le", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PIAT", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Matthias grinst.", "tokens": ["Mat\u00b7thi\u00b7as", "grinst", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und Deutschland weint.", "tokens": ["Und", "Deutschland", "weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}