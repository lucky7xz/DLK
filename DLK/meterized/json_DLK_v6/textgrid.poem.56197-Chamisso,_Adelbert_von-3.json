{"textgrid.poem.56197": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jammernd sinn ich und sinn immer das Eine nur:", "tokens": ["Jam\u00b7mernd", "sinn", "ich", "und", "sinn", "im\u00b7mer", "das", "Ei\u00b7ne", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "NN", "ADV", "ART", "ART", "ADV", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wonneselig die Hand, welche beseelet, sanft", "tokens": ["Won\u00b7ne\u00b7se\u00b7lig", "die", "Hand", ",", "wel\u00b7che", "be\u00b7see\u00b7let", ",", "sanft"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "ADJD"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gleitend \u00fcber sein Antlitz", "tokens": ["Glei\u00b7tend", "\u00fc\u00b7ber", "sein", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "D\u00fcrft ihm Form und Gestalt verleihn!", "tokens": ["D\u00fcrft", "ihm", "Form", "und", "Ge\u00b7stalt", "ver\u00b7leihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Armes, armes Geh\u00f6r, welches von ferne nur", "tokens": ["Ar\u00b7mes", ",", "ar\u00b7mes", "Ge\u00b7h\u00f6r", ",", "wel\u00b7ches", "von", "fer\u00b7ne", "nur"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PRELS", "APPR", "ADV", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Du zu schl\u00fcrfen den Ton einzig vermagst, ins Herz", "tokens": ["Du", "zu", "schl\u00fcr\u00b7fen", "den", "Ton", "ein\u00b7zig", "ver\u00b7magst", ",", "ins", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKZU", "VVFIN", "ART", "NN", "ADJD", "VVFIN", "$,", "APPRART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ihn nachhallend zu leiten,", "tokens": ["Ihn", "nach\u00b7hal\u00b7lend", "zu", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ob nachhallend, doch wesenlos!", "tokens": ["Ob", "nach\u00b7hal\u00b7lend", ",", "doch", "we\u00b7sen\u00b7los", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}