{"textgrid.poem.39952": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Thurbr\u00fccke bei Bischofszell", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat diesen steinernen Bogen", "tokens": ["Wer", "hat", "die\u00b7sen", "stei\u00b7ner\u00b7nen", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ueber die wilde Thur gezogen?", "tokens": ["Ue\u00b7ber", "die", "wil\u00b7de", "Thur", "ge\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da\u00df der Wandrer die Stra\u00dfe lobet,", "tokens": ["Da\u00df", "der", "Wand\u00b7rer", "die", "Stra\u00b7\u00dfe", "lo\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da\u00df das Wasser vergeblich tobet?", "tokens": ["Da\u00df", "das", "Was\u00b7ser", "ver\u00b7geb\u00b7lich", "to\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "War's ein m\u00e4chtiger F\u00fcrst im Lande,", "tokens": ["Wa\u00b7r's", "ein", "m\u00e4ch\u00b7ti\u00b7ger", "F\u00fcrst", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der den Strom gelegt in Bande?", "tokens": ["Der", "den", "Strom", "ge\u00b7legt", "in", "Ban\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War's ein F\u00fchrer in Kriegestagen,", "tokens": ["Wa\u00b7r's", "ein", "F\u00fch\u00b7rer", "in", "Krie\u00b7ge\u00b7sta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der die Br\u00fccke dem Heer geschlagen?", "tokens": ["Der", "die", "Br\u00fc\u00b7cke", "dem", "Heer", "ge\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Oder richtet' f\u00fcr Mann und Rosse", "tokens": ["O\u00b7der", "rich\u00b7tet'", "f\u00fcr", "Mann", "und", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie der Ritter vom hohen Schlosse,", "tokens": ["Sie", "der", "Rit\u00b7ter", "vom", "ho\u00b7hen", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und inde\u00df sein Haus zerfallen,", "tokens": ["Und", "in\u00b7de\u00df", "sein", "Haus", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ist sein Pfad noch immer zu wallen?", "tokens": ["Ist", "sein", "Pfad", "noch", "im\u00b7mer", "zu", "wal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nein, die Br\u00fccke, die ihr schauet,", "tokens": ["Nein", ",", "die", "Br\u00fc\u00b7cke", ",", "die", "ihr", "schau\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manneswort hat sie nicht erbauet;", "tokens": ["Man\u00b7nes\u00b7wort", "hat", "sie", "nicht", "er\u00b7bau\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Auf ein Wort aus des Weibes Munde", "tokens": ["Auf", "ein", "Wort", "aus", "des", "Wei\u00b7bes", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Stieg sie \u00fcber dem Felsengrunde.", "tokens": ["Stieg", "sie", "\u00fc\u00b7ber", "dem", "Fel\u00b7sen\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Die dort auf der Burg gehauset", "tokens": ["Die", "dort", "auf", "der", "Burg", "ge\u00b7hau\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "H\u00f6rte wie die Woge brauset,", "tokens": ["H\u00f6r\u00b7te", "wie", "die", "Wo\u00b7ge", "brau\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah den Flu\u00df von Waldesquellen", "tokens": ["Sah", "den", "Flu\u00df", "von", "Wal\u00b7des\u00b7quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vom Gusse des Regens schwellen.", "tokens": ["Und", "vom", "Gus\u00b7se", "des", "Re\u00b7gens", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Und den Nachen am steinigen Lande,", "tokens": ["Und", "den", "Na\u00b7chen", "am", "stei\u00b7ni\u00b7gen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Der vom Strande f\u00fchrt zum Strande,", "tokens": ["Der", "vom", "Stran\u00b7de", "f\u00fchrt", "zum", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah sie dr\u00fcben sich drehn und wiegen:", "tokens": ["Sah", "sie", "dr\u00fc\u00b7ben", "sich", "drehn", "und", "wie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wehe, wenn Einer hineingestiegen.", "tokens": ["We\u00b7he", ",", "wenn", "Ei\u00b7ner", "hin\u00b7ein\u00b7ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Ehe gedacht sie den Gedanken,", "tokens": ["E\u00b7he", "ge\u00b7dacht", "sie", "den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sieht sie ihn mit zwei Wandrern schwanken,", "tokens": ["Sieht", "sie", "ihn", "mit", "zwei", "Wand\u00b7rern", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die sie schauet, es sind in Sch\u00f6ne", "tokens": ["Die", "sie", "schau\u00b7et", ",", "es", "sind", "in", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ihre jungen, einzigen S\u00f6hne.", "tokens": ["Ih\u00b7re", "jun\u00b7gen", ",", "ein\u00b7zi\u00b7gen", "S\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Von dem Waidwerk heimgekehret,", "tokens": ["Von", "dem", "Waid\u00b7werk", "heim\u00b7ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finden sie den Strom emp\u00f6ret,", "tokens": ["Fin\u00b7den", "sie", "den", "Strom", "em\u00b7p\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben doch, die r\u00fcstigen Jungen,", "tokens": ["Ha\u00b7ben", "doch", ",", "die", "r\u00fcs\u00b7ti\u00b7gen", "Jun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Kecklich in den Kahn sich geschwungen.", "tokens": ["Keck\u00b7lich", "in", "den", "Kahn", "sich", "ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Doch es lassen sich die Wellen", "tokens": ["Doch", "es", "las\u00b7sen", "sich", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht wie Thiere des Waldes f\u00e4llen,", "tokens": ["Nicht", "wie", "Thie\u00b7re", "des", "Wal\u00b7des", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und nicht half der Mutter Klagen,", "tokens": ["Und", "nicht", "half", "der", "Mut\u00b7ter", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als sie den Kahn sah umgeschlagen.", "tokens": ["Als", "sie", "den", "Kahn", "sah", "um\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie sie nun in langem Harme", "tokens": ["Wie", "sie", "nun", "in", "lan\u00b7gem", "Har\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Breitet' ihre beiden Arme", "tokens": ["Brei\u00b7tet'", "ih\u00b7re", "bei\u00b7den", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei den Wellen, den schaumesbleichen,", "tokens": ["Bei", "den", "Wel\u00b7len", ",", "den", "schau\u00b7mes\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ueber ihrer Kinder Leichen,", "tokens": ["Ue\u00b7ber", "ih\u00b7rer", "Kin\u00b7der", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Mu\u00dfte sie der M\u00fctter gedenken,", "tokens": ["Mu\u00df\u00b7te", "sie", "der", "M\u00fct\u00b7ter", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die noch k\u00f6nnen schau'n versenken", "tokens": ["Die", "noch", "k\u00f6n\u00b7nen", "schau'n", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VMFIN", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den schnell emp\u00f6rten Wogen", "tokens": ["In", "den", "schnell", "em\u00b7p\u00f6r\u00b7ten", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00f6hne, die sie sich erzogen.", "tokens": ["S\u00f6h\u00b7ne", ",", "die", "sie", "sich", "er\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und es werden im Mutterherzen", "tokens": ["Und", "es", "wer\u00b7den", "im", "Mut\u00b7ter\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Leichter ihr die bittern Schmerzen,", "tokens": ["Leich\u00b7ter", "ihr", "die", "bit\u00b7tern", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie Andern kann ersparen", "tokens": ["Wenn", "sie", "An\u00b7dern", "kann", "er\u00b7spa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solches Leid, wie sie's erfahren.", "tokens": ["Sol\u00b7ches", "Leid", ",", "wie", "sie's", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAV", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und noch ehe sie ausgetrauert,", "tokens": ["Und", "noch", "e\u00b7he", "sie", "aus\u00b7ge\u00b7trau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ward gemei\u00dfelt und gemauert,", "tokens": ["Ward", "ge\u00b7mei\u00b7\u00dfelt", "und", "ge\u00b7mau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward der Strom in's Bett gezw\u00e4nget", "tokens": ["Ward", "der", "Strom", "in's", "Bett", "ge\u00b7zw\u00e4n\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die hohe Br\u00fccke gesprenget.", "tokens": ["Und", "die", "ho\u00b7he", "Br\u00fc\u00b7cke", "ge\u00b7spren\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Sah sie dann oft fr\u00f6hliche Knaben", "tokens": ["Sah", "sie", "dann", "oft", "fr\u00f6h\u00b7li\u00b7che", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ueber den Pfad von Steine traben,", "tokens": ["Ue\u00b7ber", "den", "Pfad", "von", "Stei\u00b7ne", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und die sch\u00e4umenden Wasser h\u00f6hnen,", "tokens": ["Und", "die", "sch\u00e4u\u00b7me