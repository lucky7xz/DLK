{"textgrid.poem.66254": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Europa an Mars", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tritt nicht so stolz einher, des Orkus schwarzer Bote,", "tokens": ["Tritt", "nicht", "so", "stolz", "ein\u00b7her", ",", "des", "Or\u00b7kus", "schwar\u00b7zer", "Bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tritt nicht so hoch und stolz daher!", "tokens": ["Tritt", "nicht", "so", "hoch", "und", "stolz", "da\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und suche Menschenopfer, dem Tode", "tokens": ["Und", "su\u00b7che", "Men\u00b7schen\u00b7op\u00b7fer", ",", "dem", "To\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Geweiht zu Land und Meer.", "tokens": ["Ge\u00b7weiht", "zu", "Land", "und", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dich ha\u00dft der Himmel; denn du bist ein Ungeheuer;", "tokens": ["Dich", "ha\u00dft", "der", "Him\u00b7mel", ";", "denn", "du", "bist", "ein", "Un\u00b7ge\u00b7heu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast deine Lust an wilder Wuth,", "tokens": ["Hast", "dei\u00b7ne", "Lust", "an", "wil\u00b7der", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am prasselnden, h\u00fcttenzerst\u00f6renden Feuer", "tokens": ["Am", "pras\u00b7seln\u00b7den", ",", "h\u00fct\u00b7ten\u00b7zer\u00b7st\u00f6\u00b7ren\u00b7den", "Feu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und am zischenden Blut.", "tokens": ["Und", "am", "zi\u00b7schen\u00b7den", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wenn Schaaren vor dir kriechen, wie Gespenster", "tokens": ["Wenn", "Schaa\u00b7ren", "vor", "dir", "krie\u00b7chen", ",", "wie", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVINF", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor Gram und Hunger zur Erde gedr\u00fcckt,", "tokens": ["Vor", "Gram", "und", "Hun\u00b7ger", "zur", "Er\u00b7de", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wenn der arme Greis durch Schindelfenster", "tokens": ["Und", "wenn", "der", "ar\u00b7me", "Greis", "durch", "Schin\u00b7del\u00b7fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gen Himmel um Erbarmen blickt;", "tokens": ["Gen", "Him\u00b7mel", "um", "Er\u00b7bar\u00b7men", "blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn dich verfluchen friedgewohnte B\u00fcrger;", "tokens": ["Wenn", "dich", "ver\u00b7flu\u00b7chen", "fried\u00b7ge\u00b7wohn\u00b7te", "B\u00fcr\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nennt dich Mutter und Braut", "tokens": ["Und", "nennt", "dich", "Mut\u00b7ter", "und", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "M\u00f6rder des Sohns und Br\u00e4utigamsw\u00fcrger:", "tokens": ["M\u00f6r\u00b7der", "des", "Sohns", "und", "Br\u00e4u\u00b7ti\u00b7gams\u00b7w\u00fcr\u00b7ger", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So lachst und spottest du laut.", "tokens": ["So", "lachst", "und", "spot\u00b7test", "du", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Denn dich erg\u00f6tzt ein Schlachtfeld voller Leichen,", "tokens": ["Denn", "dich", "er\u00b7g\u00f6tzt", "ein", "Schlacht\u00b7feld", "vol\u00b7ler", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und der Verzweiflung vorgepre\u00dfter Blick.", "tokens": ["Und", "der", "Ver\u00b7zwei\u00b7flung", "vor\u00b7ge\u00b7pre\u00df\u00b7ter", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Sterbenden Blutathmen, R\u00f6cheln, Keuchen", "tokens": ["Der", "Ster\u00b7ben\u00b7den", "Blu\u00b7tath\u00b7men", ",", "R\u00f6\u00b7cheln", ",", "Keu\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist deinen Ohren Musik.", "tokens": ["Ist", "dei\u00b7nen", "Oh\u00b7ren", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Doch harre nur, der Thronengott im Himmel", "tokens": ["Doch", "har\u00b7re", "nur", ",", "der", "Thro\u00b7nen\u00b7gott", "im", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwingt schon den Donner rachevoll,", "tokens": ["Schwingt", "schon", "den", "Don\u00b7ner", "ra\u00b7che\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der, M\u00f6rder, dich im Sturmget\u00fcmmel", "tokens": ["Der", ",", "M\u00f6r\u00b7der", ",", "dich", "im", "Sturm\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "NN", "$,", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Orkus w\u00e4lzen soll.", "tokens": ["In", "Or\u00b7kus", "w\u00e4l\u00b7zen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}