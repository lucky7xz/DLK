{"dta.poem.1368": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  III.  Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Seele danckt dem HErrn", "tokens": ["Mei\u00b7ne", "See\u00b7le", "danckt", "dem", "Herrn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter reiner Christen Kern/", "tokens": ["Un\u00b7ter", "rei\u00b7ner", "Chris\u00b7ten", "Kern", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Frommen sich berathen/", "tokens": ["Wo", "die", "From\u00b7men", "sich", "be\u00b7ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00df sind unsers GOttes Thaten/", "tokens": ["Gro\u00df", "sind", "un\u00b7sers", "Got\u00b7tes", "Tha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer sie recht erwegen kan", "tokens": ["Wer", "sie", "recht", "er\u00b7we\u00b7gen", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Findet eitel Lust daran.", "tokens": ["Fin\u00b7det", "ei\u00b7tel", "Lust", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was er ordnet/ was er thut/", "tokens": ["Was", "er", "ord\u00b7net", "/", "was", "er", "thut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ist l\u00f6blich/ das ist gutt/", "tokens": ["Das", "ist", "l\u00f6b\u00b7lich", "/", "das", "ist", "gutt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$(", "PDS", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrlich und von grosser St\u00e4rcke", "tokens": ["Herr\u00b7lich", "und", "von", "gros\u00b7ser", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind des Allerh\u00f6chsten Wercke/", "tokens": ["Sind", "des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Wer\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sein Recht und Gerechtigkeit", "tokens": ["Sein", "Recht", "und", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bleiben \u00fcber alle Zeit.", "tokens": ["Blei\u00b7ben", "\u00fc\u00b7ber", "al\u00b7le", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Er/ der HErr/ voll Gnad und Treu/", "tokens": ["Er", "/", "der", "Herr", "/", "voll", "Gnad", "und", "Treu", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "NN", "$(", "ADJD", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Erbarmen immer neu/", "tokens": ["Des", "Er\u00b7bar\u00b7men", "im\u00b7mer", "neu", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat uns/ seiner zu gedencken/", "tokens": ["Hat", "uns", "/", "sei\u00b7ner", "zu", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "PPOSAT", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manche Wohlthat wollen schencken/", "tokens": ["Man\u00b7che", "Wohlt\u00b7hat", "wol\u00b7len", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch ein Denck-Mahl noch zu lezt", "tokens": ["Auch", "ein", "Den\u00b7ck\u00b7Mahl", "noch", "zu", "lezt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "PTKZU", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Seiner Wunder ausgesezt.", "tokens": ["Sei\u00b7ner", "Wun\u00b7der", "aus\u00b7ge\u00b7sezt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ewig h\u00e4lt er seinen Bund/", "tokens": ["E\u00b7wig", "h\u00e4lt", "er", "sei\u00b7nen", "Bund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Speist der Frommen Geist und Mund/", "tokens": ["Speist", "der", "From\u00b7men", "Geist", "und", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Giebt dem Volck/ das ihn will ehren/", "tokens": ["Giebt", "dem", "Volck", "/", "das", "ihn", "will", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "PRELS", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein gewaltig Thun zu h\u00f6ren/", "tokens": ["Sein", "ge\u00b7wal\u00b7tig", "Thun", "zu", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schleust der fremden Heyden Hau\u00df", "tokens": ["Schleust", "der", "frem\u00b7den", "Hey\u00b7den", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht von seiner Erbschafft aus.", "tokens": ["Nicht", "von", "sei\u00b7ner", "Erb\u00b7schafft", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Recht und Warheit \u00fcbet GOtt/", "tokens": ["Recht", "und", "War\u00b7heit", "\u00fc\u00b7bet", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drauff sich gr\u00fcndet sein Gebot/", "tokens": ["Drauff", "sich", "gr\u00fcn\u00b7det", "sein", "Ge\u00b7bot", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treu und redlich soll mans treiben/", "tokens": ["Treu", "und", "red\u00b7lich", "soll", "mans", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VMFIN", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darum mu\u00df es ewig bleiben/", "tokens": ["Da\u00b7rum", "mu\u00df", "es", "e\u00b7wig", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sich Erd und Himmel regt", "tokens": ["Wenn", "sich", "Erd", "und", "Him\u00b7mel", "regt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Steht sein Wort gantz unbewegt.", "tokens": ["Steht", "sein", "Wort", "gantz", "un\u00b7be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Er erl\u00f6set seine Schaar/", "tokens": ["Er", "er\u00b7l\u00f6\u00b7set", "sei\u00b7ne", "Schaar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein Versprechen bleibet wahr/", "tokens": ["Sein", "Ver\u00b7spre\u00b7chen", "blei\u00b7bet", "wahr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig bl\u00fch der Frommen Saamen/", "tokens": ["E\u00b7wig", "bl\u00fch", "der", "From\u00b7men", "Saa\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hehr und herrlich ist sein Nahmen/", "tokens": ["Hehr", "und", "herr\u00b7lich", "ist", "sein", "Nah\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Billich wird sein Prei\u00df erh\u00f6ht", "tokens": ["Bil\u00b7lich", "wird", "sein", "Prei\u00df", "er\u00b7h\u00f6ht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weiter als die Sonne geht.", "tokens": ["Wei\u00b7ter", "als", "die", "Son\u00b7ne", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Furcht des HErren kan allein", "tokens": ["Furcht", "des", "Her\u00b7ren", "kan", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rechter Wei\u00dfheit Anfang seyn.", "tokens": ["Rech\u00b7ter", "Wei\u00df\u00b7heit", "An\u00b7fang", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott gehorchen/ GOtt recht kennen", "tokens": ["Gott", "ge\u00b7hor\u00b7chen", "/", "Gott", "recht", "ken\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$(", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df man wahre Klugheit nennen/", "tokens": ["Mu\u00df", "man", "wah\u00b7re", "Klug\u00b7heit", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer di\u00df lernt mit allem Flei\u00df/", "tokens": ["Wer", "di\u00df", "lernt", "mit", "al\u00b7lem", "Flei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "APPR", "PIS", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dessen Lob kein Ende wei\u00df.", "tokens": ["Des\u00b7sen", "Lob", "kein", "En\u00b7de", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}