{"textgrid.poem.38231": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ich stand an einem Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand an einem Morgen", "tokens": ["Ich", "stand", "an", "ei\u00b7nem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heimlich an einem Ort,", "tokens": ["Heim\u00b7lich", "an", "ei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da h\u00e4tt ich mich verborgen,", "tokens": ["Da", "h\u00e4tt", "ich", "mich", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich h\u00f6rt kl\u00e4gliche Wort,", "tokens": ["Ich", "h\u00f6rt", "kl\u00e4g\u00b7li\u00b7che", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Von einem Fr\u00e4ulein h\u00fcbsch und fein,", "tokens": ["Von", "ei\u00b7nem", "Fr\u00e4u\u00b7lein", "h\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sprach zu ihrem Buhler,", "tokens": ["Sie", "sprach", "zu", "ih\u00b7rem", "Buh\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es mu\u00df geschieden seyn.", "tokens": ["Es", "mu\u00df", "ge\u00b7schie\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herzlieb, ich hab vernommen,", "tokens": ["Herz\u00b7lieb", ",", "ich", "hab", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du wilt von hinnen schier,", "tokens": ["Du", "wilt", "von", "hin\u00b7nen", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann wilt du wieder kommen,", "tokens": ["Wann", "wilt", "du", "wie\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das sollst du sagen mir,", "tokens": ["Das", "sollst", "du", "sa\u00b7gen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Merk mein Feinslieb, was ich dir sag,", "tokens": ["Merk", "mein", "Feins\u00b7lieb", ",", "was", "ich", "dir", "sag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Zukunft thust du fragen,", "tokens": ["Mein", "Zu\u00b7kunft", "thust", "du", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich wei\u00df weder Stund noch Tag.", "tokens": ["Ich", "wei\u00df", "we\u00b7der", "Stund", "noch", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Fr\u00e4ulein weinet sehre,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "wei\u00b7net", "seh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Herz war Trauren voll:", "tokens": ["Ihr", "Herz", "war", "Trau\u00b7ren", "voll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbso gieb mir Weis und Lehre", "tokens": ["\u00bb", "so", "gieb", "mir", "Weis", "und", "Leh\u00b7re"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVIMP", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ich mich halten soll,", "tokens": ["Wie", "ich", "mich", "hal\u00b7ten", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr dich sez ich mein Hab und Gut,", "tokens": ["F\u00fcr", "dich", "sez", "ich", "mein", "Hab", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und willst du hier nun bleiben,", "tokens": ["Und", "willst", "du", "hier", "nun", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich verehr dich in Jahr und Tag.\u00ab", "tokens": ["Ich", "ver\u00b7ehr", "dich", "in", "Jahr", "und", "Tag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Der Knab der sprach aus Muthe,", "tokens": ["Der", "Knab", "der", "sprach", "aus", "Mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Willen ich wohl sp\u00fcr,", "tokens": ["Dein", "Wil\u00b7len", "ich", "wohl", "sp\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verzehr ich dir dein Gute,", "tokens": ["Ver\u00b7zehr", "ich", "dir", "dein", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Jahr ist bald dahin,", "tokens": ["Ein", "Jahr", "ist", "bald", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dennoch mu\u00df es geschieden seyn,", "tokens": ["Den\u00b7noch", "mu\u00df", "es", "ge\u00b7schie\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Ich will dich z\u00e4rtlich bitten,", "tokens": ["Ich", "will", "dich", "z\u00e4rt\u00b7lich", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Setz du dein Willen drein.", "tokens": ["Setz", "du", "dein", "Wil\u00b7len", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Fr\u00e4ulein das schreit Morde!", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "das", "schreit", "Mor\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mord \u00fcber alles Leid:", "tokens": ["Mord", "\u00fc\u00b7ber", "al\u00b7les", "Leid", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmich kr\u00e4nken deine Worte,", "tokens": ["\u00bb", "mich", "kr\u00e4n\u00b7ken", "dei\u00b7ne", "Wor\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herzlieb nicht von mir scheid;", "tokens": ["Herz\u00b7lieb", "nicht", "von", "mir", "scheid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr dich setz ich mein Gut und Ehr,", "tokens": ["F\u00fcr", "dich", "setz", "ich", "mein", "Gut", "und", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und sollt ich mit dir ziehen,", "tokens": ["Und", "sollt", "ich", "mit", "dir", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kein Weg ist mir zu fern.\u00ab", "tokens": ["Kein", "Weg", "ist", "mir", "zu", "fern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}