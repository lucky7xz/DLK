{"textgrid.poem.47317": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Wieg', aus der die Sonnen steigen, o heiliges Meer!", "tokens": ["O", "Wieg'", ",", "aus", "der", "die", "Son\u00b7nen", "stei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O Grab, in das die Sonnen neigen, o heiliges Meer!", "tokens": ["O", "Grab", ",", "in", "das", "die", "Son\u00b7nen", "nei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "O du im Duft der Nacht entfaltend den Spiegel, darein", "tokens": ["O", "du", "im", "Duft", "der", "Nacht", "ent\u00b7fal\u00b7tend", "den", "Spie\u00b7gel", ",", "da\u00b7rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "NE", "APPRART", "NN", "ART", "NN", "VVPP", "ART", "NN", "$,", "PAV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vom Himmel Luna schaut mit Schweigen, o heiliges Meer!", "tokens": ["Vom", "Him\u00b7mel", "Lu\u00b7na", "schaut", "mit", "Schwei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVFIN", "APPR", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "O du in stillen Mittern\u00e4chten mit Wogengesang", "tokens": ["O", "du", "in", "stil\u00b7len", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten", "mit", "Wo\u00b7gen\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Einklingend in der Sterne Reigen, o heiliges Meer!", "tokens": ["Ein\u00b7klin\u00b7gend", "in", "der", "Ster\u00b7ne", "Rei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Die Morgen- und die Abendr\u00f6ten erbl\u00fchen aus dir,", "tokens": ["Die", "Mor\u00b7gen", "und", "die", "A\u00b7bend\u00b7r\u00f6\u00b7ten", "er\u00b7bl\u00fc\u00b7hen", "aus", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ART", "NN", "VVINF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Zwei Rosen deinem Garten eigen, o heiliges Meer!", "tokens": ["Zwei", "Ro\u00b7sen", "dei\u00b7nem", "Gar\u00b7ten", "ei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPOSAT", "NN", "ADJD", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Atmender Busen Amphitrites, der nieder und auf", "tokens": ["At\u00b7men\u00b7der", "Bu\u00b7sen", "Am\u00b7phit\u00b7ri\u00b7tes", ",", "der", "nie\u00b7der", "und", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NE", "$,", "PRELS", "PTKVZ", "KON", "APPR"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Die Wogen sinken l\u00e4\u00dft und steigen, o heiliges Meer!", "tokens": ["Die", "Wo\u00b7gen", "sin\u00b7ken", "l\u00e4\u00dft", "und", "stei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "KON", "VVFIN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Scho\u00df, m\u00fctterlicher, Aphrodites! geb\u00e4re dein Kind,", "tokens": ["Scho\u00df", ",", "m\u00fct\u00b7ter\u00b7li\u00b7cher", ",", "A\u00b7phro\u00b7di\u00b7tes", "!", "ge\u00b7b\u00e4\u00b7re", "dein", "Kind", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "NE", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Um deinen Glanz der Welt zu zeigen, o heiliges Meer!", "tokens": ["Um", "dei\u00b7nen", "Glanz", "der", "Welt", "zu", "zei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Spreng' auf den Fr\u00fchlingskranz der Erde den perlenden Tau!", "tokens": ["Spreng'", "auf", "den", "Fr\u00fch\u00b7lings\u00b7kranz", "der", "Er\u00b7de", "den", "per\u00b7len\u00b7den", "Tau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Denn alle Perlen sind dein eigen, o heiliges Meer!", "tokens": ["Denn", "al\u00b7le", "Per\u00b7len", "sind", "dein", "ei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPOSAT", "ADJD", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Du sammelst alle dir entstammten Najaden der Flur", "tokens": ["Du", "sam\u00b7melst", "al\u00b7le", "dir", "ent\u00b7stamm\u00b7ten", "Na\u00b7ja\u00b7den", "der", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PPER", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Zur\u00fcck zum Nereidenreigen, o heiliges Meer!", "tokens": ["Zu\u00b7r\u00fcck", "zum", "Ne\u00b7rei\u00b7den\u00b7rei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Die Schiffe der Gedanken segeln und sinken in dir;", "tokens": ["Die", "Schif\u00b7fe", "der", "Ge\u00b7dan\u00b7ken", "se\u00b7geln", "und", "sin\u00b7ken", "in", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Atlantis ruht in deinem Schweigen, o heiliges Meer!", "tokens": ["At\u00b7lan\u00b7tis", "ruht", "in", "dei\u00b7nem", "Schwei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Der G\u00f6tterbecher, der gefallen vom hohen Olymp,", "tokens": ["Der", "G\u00f6t\u00b7ter\u00b7be\u00b7cher", ",", "der", "ge\u00b7fal\u00b7len", "vom", "ho\u00b7hen", "O\u00b7lymp", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "H\u00e4ngt tief an den Korallenzweigen, o heiliges Meer!", "tokens": ["H\u00e4ngt", "tief", "an", "den", "Ko\u00b7ral\u00b7len\u00b7zwei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+---+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Ein Taucher in das Meer der Liebe ist ", "tokens": ["Ein", "Tau\u00b7cher", "in", "das", "Meer", "der", "Lie\u00b7be", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Der deinen Glanz der Welt will zeigen, o heiliges Meer!", "tokens": ["Der", "dei\u00b7nen", "Glanz", "der", "Welt", "will", "zei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Als wie der Mond will ich mit Sehnen mich st\u00fcrzen in dich;", "tokens": ["Als", "wie", "der", "Mond", "will", "ich", "mit", "Seh\u00b7nen", "mich", "st\u00fcr\u00b7zen", "in", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VMFIN", "PPER", "APPR", "NN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "La\u00df mich aus dir als Sonne steigen, o heiliges Meer!", "tokens": ["La\u00df", "mich", "aus", "dir", "als", "Son\u00b7ne", "stei\u00b7gen", ",", "o", "hei\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "KOUS", "NN", "VVINF", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}