{"dta.poem.10729": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "O Kron de\u00df Hauset: leid nur gern an in noth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Dv adeliches blut/ der Welt vnd jhres Hauses", "tokens": ["Dv", "a\u00b7de\u00b7li\u00b7ches", "blut", "/", "der", "Welt", "vnd", "jhres", "Hau\u00b7ses"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$(", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Geh m\u00fcssig/ wie du thust/ ", "tokens": ["Geh", "m\u00fcs\u00b7sig", "/", "wie", "du", "thust", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd schlag der Tugent nach/ so wirr man dir nechst Gott/", "tokens": ["Vnd", "schlag", "der", "Tu\u00b7gent", "nach", "/", "so", "wirr", "man", "dir", "nechst", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "$(", "ADV", "ADJD", "PIS", "PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ins k\u016fnfftig schreyen zue: ", "tokens": ["Ins", "k\u016fnf\u00b7ftig", "schre\u00b7yen", "zue", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}