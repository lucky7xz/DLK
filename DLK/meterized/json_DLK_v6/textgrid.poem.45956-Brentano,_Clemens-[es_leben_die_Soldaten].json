{"textgrid.poem.45956": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[es leben die Soldaten]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es leben die Soldaten,", "tokens": ["Es", "le\u00b7ben", "die", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So recht von Gottes Gnaden,", "tokens": ["So", "recht", "von", "Got\u00b7tes", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel ist ihr Zelt,", "tokens": ["Der", "Him\u00b7mel", "ist", "ihr", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Tisch das gr\u00fcne Feld.", "tokens": ["Ihr", "Tisch", "das", "gr\u00fc\u00b7ne", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Bette ist der Rasen,", "tokens": ["Ihr", "Bet\u00b7te", "ist", "der", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trompeter m\u00fcssen blasen,", "tokens": ["Trom\u00b7pe\u00b7ter", "m\u00fcs\u00b7sen", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Guten Morgen, gute Nacht,", "tokens": ["Gu\u00b7ten", "Mor\u00b7gen", ",", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df man mit Lust erwacht.", "tokens": ["Da\u00df", "man", "mit", "Lust", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Wirtsschild ist die Sonne,", "tokens": ["Ihr", "Wirts\u00b7schild", "ist", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Freund die volle Tonne,", "tokens": ["Ihr", "Freund", "die", "vol\u00b7le", "Ton\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Schlafbuhl ist der Mond,", "tokens": ["Ihr", "Schlaf\u00b7buhl", "ist", "der", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der in der Sternschanz wohnt.", "tokens": ["Der", "in", "der", "Stern\u00b7schanz", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Sterne haben Stunden,", "tokens": ["Die", "Ster\u00b7ne", "ha\u00b7ben", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sterne haben Runden", "tokens": ["Die", "Ster\u00b7ne", "ha\u00b7ben", "Run\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und werden abgel\u00f6st,", "tokens": ["Und", "wer\u00b7den", "ab\u00b7ge\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Drum Schildwacht sei getr\u00f6st.", "tokens": ["Drum", "Schild\u00b7wacht", "sei", "ge\u00b7tr\u00f6st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir richten mit dem Schwerte,", "tokens": ["Wir", "rich\u00b7ten", "mit", "dem", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Leib geh\u00f6rt der Erde,", "tokens": ["Der", "Leib", "ge\u00b7h\u00f6rt", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Seel' dem Himmelszelt,", "tokens": ["Die", "Seel'", "dem", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Rock bleibt in der Welt.", "tokens": ["Der", "Rock", "bleibt", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wer f\u00e4llt, der bleibet liegen,", "tokens": ["Wer", "f\u00e4llt", ",", "der", "blei\u00b7bet", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer steht, der kann noch siegen,", "tokens": ["Wer", "steht", ",", "der", "kann", "noch", "sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer \u00fcbrig bleibt, hat Recht,", "tokens": ["Wer", "\u00fcb\u00b7rig", "bleibt", ",", "hat", "Recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und wer entflieht, ist schlecht.", "tokens": ["Und", "wer", "ent\u00b7flieht", ",", "ist", "schlecht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Zum Hassen oder Lieben", "tokens": ["Zum", "Has\u00b7sen", "o\u00b7der", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist alle Welt getrieben,", "tokens": ["Ist", "al\u00b7le", "Welt", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es bleibet keine Wahl,", "tokens": ["Es", "blei\u00b7bet", "kei\u00b7ne", "Wahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Teufel ist neutral.", "tokens": ["Der", "Teu\u00b7fel", "ist", "neut\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Bedienet uns ein Bauer,", "tokens": ["Be\u00b7die\u00b7net", "uns", "ein", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So schmeckt der Wein fast sauer", "tokens": ["So", "schmeckt", "der", "Wein", "fast", "sau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist's ein sch\u00f6ner Schatz", "tokens": ["Doch", "ist's", "ein", "sch\u00f6\u00b7ner", "Schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So kriegt sie einen Schmatz.", "tokens": ["So", "kriegt", "sie", "ei\u00b7nen", "Schmatz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}