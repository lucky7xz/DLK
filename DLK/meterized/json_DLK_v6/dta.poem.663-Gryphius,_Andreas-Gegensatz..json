{"dta.poem.663": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ja! keine zeit/ ", "tokens": ["Ja", "!", "kei\u00b7ne", "zeit", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Soll \u00e4ndern mein gem\u00fctt\u2019 vnd meine Vatersorgen.", "tokens": ["Soll", "\u00e4n\u00b7dern", "mein", "ge\u00b7m\u00fctt'", "vnd", "mei\u00b7ne", "Va\u00b7ter\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "VVPP", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ewigkeit", "tokens": ["Die", "e\u00b7wig\u00b7keit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sol b\u00fcrgin seyn! was ich jhm je versprach/", "tokens": ["Sol", "b\u00fcr\u00b7gin", "seyn", "!", "was", "ich", "jhm", "je", "ver\u00b7sprach", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$.", "PWS", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wird feste stehn/ jtzt wird er vnd hernach ", "tokens": ["Wird", "fes\u00b7te", "stehn", "/", "jtzt", "wird", "er", "vnd", "her\u00b7nach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "VVINF", "$(", "ADV", "VAFIN", "PPER", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr den B\u00fcrgern dieser Erden/ vnd den Geistern jener welt:", "tokens": ["F\u00fcr", "den", "B\u00fcr\u00b7gern", "die\u00b7ser", "Er\u00b7den", "/", "vnd", "den", "Geis\u00b7tern", "je\u00b7ner", "welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$(", "KON", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Mit lob vollen ", "tokens": ["Mit", "lob", "vol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "ben h\u00e4lt. ", "tokens": ["ben", "h\u00e4lt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}