{"textgrid.poem.28250": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hunde, die das Vieh beh\u00fcten,", "tokens": ["Hun\u00b7de", ",", "die", "das", "Vieh", "be\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hunde, die am Bande w\u00fcten,", "tokens": ["Hun\u00b7de", ",", "die", "am", "Ban\u00b7de", "w\u00fc\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hunde, die nach Wilde jagen,", "tokens": ["Hun\u00b7de", ",", "die", "nach", "Wil\u00b7de", "ja\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hunde, welche stehn und tragen,", "tokens": ["Hun\u00b7de", ",", "wel\u00b7che", "stehn", "und", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hunde, die zu Tische schmeicheln,", "tokens": ["Hun\u00b7de", ",", "die", "zu", "Ti\u00b7sche", "schmei\u00b7cheln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hunde, die die Frauen streicheln:", "tokens": ["Hun\u00b7de", ",", "die", "die", "Frau\u00b7en", "strei\u00b7cheln", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Diese Hunde gar zusammen", "tokens": ["Die\u00b7se", "Hun\u00b7de", "gar", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kummen nur au\u00df faulem Stammen.", "tokens": ["Kum\u00b7men", "nur", "au\u00df", "fau\u00b7lem", "Stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber ich bin von den Hunden,", "tokens": ["A\u00b7ber", "ich", "bin", "von", "den", "Hun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Die sich in den Krieg gefunden,", "tokens": ["Die", "sich", "in", "den", "Krieg", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Bleibe nur, wo Helden bleiben,", "tokens": ["Blei\u00b7be", "nur", ",", "wo", "Hel\u00b7den", "blei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wann sie K\u00fch und Pferde treiben,", "tokens": ["Wann", "sie", "K\u00fch", "und", "Pfer\u00b7de", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Habe B\u00fcndn\u00fc\u00df mit den Dieben,", "tokens": ["Ha\u00b7be", "B\u00fcnd\u00b7n\u00fc\u00df", "mit", "den", "Die\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Trag am rauben ein Belieben,", "tokens": ["Trag", "am", "rau\u00b7ben", "ein", "Be\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Pflege, bin ich in Quartiren,", "tokens": ["Pfle\u00b7ge", ",", "bin", "ich", "in", "Quar\u00b7ti\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.16": {"text": "G\u00e4ns und H\u00fcner zuzuf\u00fchren,", "tokens": ["G\u00e4ns", "und", "H\u00fc\u00b7ner", "zu\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Kan die schlauen Bauern suchen,", "tokens": ["Kan", "die", "schlau\u00b7en", "Bau\u00b7ern", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wann sie sich ins Holtz verkruchen;", "tokens": ["Wann", "sie", "sich", "ins", "Holtz", "ver\u00b7kru\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Wann sie nach den Pferden kummen,", "tokens": ["Wann", "sie", "nach", "den", "Pfer\u00b7den", "kum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Die mein Herr hat wo genummen,", "tokens": ["Die", "mein", "Herr", "hat", "wo", "ge\u00b7num\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Kan ich sie von dannen hetzen,", "tokens": ["Kan", "ich", "sie", "von", "dan\u00b7nen", "het\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Da\u00df sie Hut und Schuh versetzen,", "tokens": ["Da\u00df", "sie", "Hut", "und", "Schuh", "ver\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Kan durch Schaden, kan durch Zehren", "tokens": ["Kan", "durch", "Scha\u00b7den", ",", "kan", "durch", "Zeh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "$,", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Helffen Haus und Hof verheeren.", "tokens": ["Helf\u00b7fen", "Haus", "und", "Hof", "ver\u00b7hee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Cavalliers, die kan ich leiden;", "tokens": ["Ca\u00b7val\u00b7liers", ",", "die", "kan", "ich", "lei\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Bauren m\u00fcssen mich vermeiden;", "tokens": ["Bau\u00b7ren", "m\u00fcs\u00b7sen", "mich", "ver\u00b7mei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Bin nun drum in meinem Orden", "tokens": ["Bin", "nun", "drum", "in", "mei\u00b7nem", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Hunde-Cavallier geworden.", "tokens": ["Hun\u00b7de\u00b7Ca\u00b7val\u00b7lier", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}