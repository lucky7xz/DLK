{"textgrid.poem.49940": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Frau Pauline v. Phull-Rieppur", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht f\u00fcr Nacht, mit dem Zw\u00f6lf-Uhr-Schlag, auf gespenstigem Rosse,", "tokens": ["Nacht", "f\u00fcr", "Nacht", ",", "mit", "dem", "Zw\u00f6l\u00b7fUhr\u00b7Schlag", ",", "auf", "ge\u00b7spens\u00b7ti\u00b7gem", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "War der geharnischte Mann sonst vor dem Schlosse zu sehn;", "tokens": ["War", "der", "ge\u00b7har\u00b7nischte", "Mann", "sonst", "vor", "dem", "Schlos\u00b7se", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Grollend dem fremden Geschlecht, das hier statt seiner gebietet,", "tokens": ["Grol\u00b7lend", "dem", "frem\u00b7den", "Ge\u00b7schlecht", ",", "das", "hier", "statt", "sei\u00b7ner", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Sucht' er die Br\u00fccke umsonst, welche zur Pforte gef\u00fchrt.", "tokens": ["Sucht'", "er", "die", "Br\u00fc\u00b7cke", "um\u00b7sonst", ",", "wel\u00b7che", "zur", "Pfor\u00b7te", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "\u2013 Wunder! seitdem ", "tokens": ["\u2013", "Wun\u00b7der", "!", "seit\u00b7dem"], "token_info": ["punct", "word", "punct", "word"], "pos": ["$(", "NN", "$.", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "H\u00e4tte dein liebliches Bild endlich den Alten vers\u00f6hnt?", "tokens": ["H\u00e4t\u00b7te", "dein", "lieb\u00b7li\u00b7ches", "Bild", "end\u00b7lich", "den", "Al\u00b7ten", "ver\u00b7s\u00f6hnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}}}}