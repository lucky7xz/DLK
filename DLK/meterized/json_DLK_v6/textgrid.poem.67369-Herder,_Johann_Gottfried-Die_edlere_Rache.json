{"textgrid.poem.67369": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die edlere Rache", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbauf! r\u00e4che Dich!\u00ab sprach ein gerechter Zorn,", "tokens": ["\u00bb", "auf", "!", "r\u00e4\u00b7che", "Dich", "!", "\u00ab", "sprach", "ein", "ge\u00b7rech\u00b7ter", "Zorn", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "VVFIN", "PPER", "$.", "$(", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der starkbewaffnet mir im Herzen sa\u00df,", "tokens": ["Der", "stark\u00b7be\u00b7waff\u00b7net", "mir", "im", "Her\u00b7zen", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbauf! r\u00e4che Dich und gieb der Welt und Nachwelt", "tokens": ["\u00bb", "auf", "!", "r\u00e4\u00b7che", "Dich", "und", "gieb", "der", "Welt", "und", "Nach\u00b7welt"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$.", "VVFIN", "PPER", "KON", "VVIMP", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu wissen seine Schmach und Deine Unschuld!\u00ab", "tokens": ["Zu", "wis\u00b7sen", "sei\u00b7ne", "Schmach", "und", "Dei\u00b7ne", "Un\u00b7schuld", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ersch\u00fcttert ward mein Geist, wie auf den Klang", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "ward", "mein", "Geist", ",", "wie", "auf", "den", "Klang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der kriegrischen Drommet' ein edles Ro\u00df", "tokens": ["Der", "krie\u00b7gri\u00b7schen", "Drom\u00b7met'", "ein", "ed\u00b7les", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Emporschnaubt und den Sporn verachtet. Doch", "tokens": ["Em\u00b7por\u00b7schnaubt", "und", "den", "Sporn", "ver\u00b7ach\u00b7tet", ".", "Doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "KON", "ART", "NN", "VVFIN", "$.", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein zweiter, edlerer Gedanke stieg", "tokens": ["Ein", "zwei\u00b7ter", ",", "ed\u00b7le\u00b7rer", "Ge\u00b7dan\u00b7ke", "stieg"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In mir empor und hielt den Z\u00fcgel ihm", "tokens": ["In", "mir", "em\u00b7por", "und", "hielt", "den", "Z\u00fc\u00b7gel", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und b\u00e4ndigte mein Herz: \u00bbWie? und Du willst", "tokens": ["Und", "b\u00e4n\u00b7dig\u00b7te", "mein", "Herz", ":", "\u00bb", "Wie", "?", "und", "Du", "willst"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "$(", "PWAV", "$.", "KON", "PPER", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Solch einem Namen, solcher niedern That", "tokens": ["Solch", "ei\u00b7nem", "Na\u00b7men", ",", "sol\u00b7cher", "nie\u00b7dern", "That"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Noch Welt und Leben geben? Nimmermehr!", "tokens": ["Noch", "Welt", "und", "Le\u00b7ben", "ge\u00b7ben", "?", "Nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$.", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Erwarte ruhig, bis die starke Zeit", "tokens": ["Er\u00b7war\u00b7te", "ru\u00b7hig", ",", "bis", "die", "star\u00b7ke", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dich r\u00e4chet und Dir sanft den Schmerz verwischt!\u00ab", "tokens": ["Dich", "r\u00e4\u00b7chet", "und", "Dir", "sanft", "den", "Schmerz", "ver\u00b7wischt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "ADJD", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "An seinem niedern Feinde. Hochgemuth", "tokens": ["An", "sei\u00b7nem", "nie\u00b7dern", "Fein\u00b7de", ".", "Hoch\u00b7ge\u00b7muth"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verachtet er des Neides Schmach \u2013 und schweigt.", "tokens": ["Ver\u00b7ach\u00b7tet", "er", "des", "Nei\u00b7des", "Schmach", "\u2013", "und", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}