{"textgrid.poem.65940": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An die Heimat", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Heimat, bleiches Bild im Winde,", "tokens": ["Ge\u00b7lieb\u00b7te", "Hei\u00b7mat", ",", "blei\u00b7ches", "Bild", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Olivenhain in der Zypressen Hut,", "tokens": ["O\u00b7li\u00b7ven\u00b7hain", "in", "der", "Zyp\u00b7res\u00b7sen", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Beseeltes Meer, so voll Best\u00fcrmungsglut,", "tokens": ["Be\u00b7seel\u00b7tes", "Meer", ",", "so", "voll", "Be\u00b7st\u00fcr\u00b7mungs\u00b7glut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Betrau mit Wohllaut mich \u2013 zum Angebinde,", "tokens": ["Be\u00b7trau", "mit", "Wohl\u00b7laut", "mich", "\u2013", "zum", "An\u00b7ge\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "$(", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem eingeh\u00e4ndigten dir, fremden Kinde!", "tokens": ["Dem", "ein\u00b7ge\u00b7h\u00e4n\u00b7dig\u00b7ten", "dir", ",", "frem\u00b7den", "Kin\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sinnst wie eine Mutter, leuchtend, gut;", "tokens": ["Du", "sinnst", "wie", "ei\u00b7ne", "Mut\u00b7ter", ",", "leuch\u00b7tend", ",", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "$,", "VVPP", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Italia, dirzu strahlt der Stern im Blut,", "tokens": ["I\u00b7ta\u00b7lia", ",", "dir\u00b7zu", "strahlt", "der", "Stern", "im", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PAV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Hoffen blickt dich an, so bleib gelinde!", "tokens": ["Sein", "Hof\u00b7fen", "blickt", "dich", "an", ",", "so", "bleib", "ge\u00b7lin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu hoher Sonne sprossen meine Triebe.", "tokens": ["Zu", "ho\u00b7her", "Son\u00b7ne", "spros\u00b7sen", "mei\u00b7ne", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesund im S\u00fcden, steh ich stark an Wuchs:", "tokens": ["Ge\u00b7sund", "im", "S\u00fc\u00b7den", ",", "steh", "ich", "stark", "an", "Wuchs", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Blieb S\u00fc\u00dfe auch Besinnung blo\u00df an Liebe!", "tokens": ["Blieb", "S\u00fc\u00b7\u00dfe", "auch", "Be\u00b7sin\u00b7nung", "blo\u00df", "an", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gel\u00e4nge mir, als Zier des Kr\u00f6nungsschmucks,", "tokens": ["Ge\u00b7l\u00e4n\u00b7ge", "mir", ",", "als", "Zier", "des", "Kr\u00f6\u00b7nungs\u00b7schmucks", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der hell auf deinen schwarzen Flechten ruht,", "tokens": ["Der", "hell", "auf", "dei\u00b7nen", "schwar\u00b7zen", "Flech\u00b7ten", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Lied, voll Funkelglut, durch dunklen Mut!", "tokens": ["Ein", "Lied", ",", "voll", "Fun\u00b7kel\u00b7glut", ",", "durch", "dunk\u00b7len", "Mut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}