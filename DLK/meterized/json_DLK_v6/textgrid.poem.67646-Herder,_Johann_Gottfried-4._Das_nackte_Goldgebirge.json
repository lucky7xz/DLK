{"textgrid.poem.67646": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4. Das nackte Goldgebirge", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als wenn auch Armuth, tief verh\u00fcllt,", "tokens": ["Als", "wenn", "auch", "Ar\u00b7muth", ",", "tief", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Edles bergen k\u00f6nnte!", "tokens": ["Nicht", "Ed\u00b7les", "ber\u00b7gen", "k\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sieh, jener Felsen, d\u00fcrr und wild,", "tokens": ["Sieh", ",", "je\u00b7ner", "Fel\u00b7sen", ",", "d\u00fcrr", "und", "wild", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er sein Gold Dir g\u00f6nnte!", "tokens": ["Wenn", "er", "sein", "Gold", "Dir", "g\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}