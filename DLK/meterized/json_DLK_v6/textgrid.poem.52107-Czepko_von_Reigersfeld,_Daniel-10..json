{"textgrid.poem.52107": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hienge gerne dir ein rechtes Schand Mahl an,", "tokens": ["Ich", "hien\u00b7ge", "ger\u00b7ne", "dir", "ein", "rech\u00b7tes", "Schand", "Mahl", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun, die Natur die sprach: Ich hab es schon gethan.", "tokens": ["Nun", ",", "die", "Na\u00b7tur", "die", "sprach", ":", "Ich", "hab", "es", "schon", "ge\u00b7than", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "ART", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er ist ein Dieb, er f\u00fchrt das Zeichen an der Stirnen,", "tokens": ["Er", "ist", "ein", "Dieb", ",", "er", "f\u00fchrt", "das", "Zei\u00b7chen", "an", "der", "Stir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann lie\u00df den Titul ab, wenn er sich wird erz\u00fcrnen.", "tokens": ["Dann", "lie\u00df", "den", "Ti\u00b7tul", "ab", ",", "wenn", "er", "sich", "wird", "er\u00b7z\u00fcr\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Natur, ich la\u00df es seyn, der Warheit Senff ist scharff,", "tokens": ["Na\u00b7tur", ",", "ich", "la\u00df", "es", "seyn", ",", "der", "War\u00b7heit", "Senff", "ist", "scharff", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "VAINF", "$,", "ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihm, was er verdient itzt niemand sagen darff,", "tokens": ["Weil", "ihm", ",", "was", "er", "ver\u00b7dient", "itzt", "nie\u00b7mand", "sa\u00b7gen", "darff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "PPER", "VVFIN", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sieht man durch das Land sein Urtheil auffzuh\u00e4ngen,", "tokens": ["So", "sieht", "man", "durch", "das", "Land", "sein", "Ur\u00b7theil", "auff\u00b7zu\u00b7h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er wird ein Edel Mann: Man sol den Dieb gestrengen.", "tokens": ["Er", "wird", "ein", "E\u00b7del", "Mann", ":", "Man", "sol", "den", "Dieb", "ge\u00b7stren\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$.", "PIS", "VMFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}