{"textgrid.poem.36888": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Bu\u00dflied", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dir allein, an dir hab ich ges\u00fcndigt,", "tokens": ["An", "dir", "al\u00b7lein", ",", "an", "dir", "hab", "ich", "ge\u00b7s\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und \u00fcbel oft vor dir getan.", "tokens": ["Und", "\u00fc\u00b7bel", "oft", "vor", "dir", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du siehst die Schuld, die mir den Fluch verk\u00fcndigt;", "tokens": ["Du", "siehst", "die", "Schuld", ",", "die", "mir", "den", "Fluch", "ver\u00b7k\u00fcn\u00b7digt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sieh, Gott, auch meinen Jammer an.", "tokens": ["Sieh", ",", "Gott", ",", "auch", "mei\u00b7nen", "Jam\u00b7mer", "an", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dir ist mein Flehn, mein Seufzen nicht verborgen,", "tokens": ["Dir", "ist", "mein", "Flehn", ",", "mein", "Seuf\u00b7zen", "nicht", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meine Tr\u00e4nen sind vor dir.", "tokens": ["Und", "mei\u00b7ne", "Tr\u00e4\u00b7nen", "sind", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach Gott, mein Gott, wie lange soll ich sorgen?", "tokens": ["Ach", "Gott", ",", "mein", "Gott", ",", "wie", "lan\u00b7ge", "soll", "ich", "sor\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPOSAT", "NN", "$,", "PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie lang entfernst du dich von mir?", "tokens": ["Wie", "lang", "ent\u00b7fernst", "du", "dich", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Herr, handle nicht mit mir nach meinen S\u00fcnden,", "tokens": ["Herr", ",", "hand\u00b7le", "nicht", "mit", "mir", "nach", "mei\u00b7nen", "S\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergilt mir nicht nach meiner Schuld.", "tokens": ["Ver\u00b7gilt", "mir", "nicht", "nach", "mei\u00b7ner", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich suche dich; la\u00df mich dein Antlitz finden,", "tokens": ["Ich", "su\u00b7che", "dich", ";", "la\u00df", "mich", "dein", "Ant\u00b7litz", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Gott der Langmut und Geduld.", "tokens": ["Du", "Gott", "der", "Lang\u00b7mut", "und", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fr\u00fch wollst du mich mit deiner Gnade f\u00fcllen,", "tokens": ["Fr\u00fch", "wollst", "du", "mich", "mit", "dei\u00b7ner", "Gna\u00b7de", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Gott, Vater der Barmherzigkeit.", "tokens": ["Gott", ",", "Va\u00b7ter", "der", "Barm\u00b7her\u00b7zig\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erfreue mich um deines Namens willen;", "tokens": ["Er\u00b7freu\u00b7e", "mich", "um", "dei\u00b7nes", "Na\u00b7mens", "wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du bist ein Gott, der gern erfreut.", "tokens": ["Du", "bist", "ein", "Gott", ",", "der", "gern", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df deinen Weg mich wieder freudig wallen,", "tokens": ["La\u00df", "dei\u00b7nen", "Weg", "mich", "wie\u00b7der", "freu\u00b7dig", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lehre mich dein heilig Recht,", "tokens": ["Und", "leh\u00b7re", "mich", "dein", "hei\u00b7lig", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich t\u00e4glich tun nach deinem Wohlgefallen;", "tokens": ["Mich", "t\u00e4g\u00b7lich", "tun", "nach", "dei\u00b7nem", "Wohl\u00b7ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du bist mein Gott, ich bin dein Knecht.", "tokens": ["Du", "bist", "mein", "Gott", ",", "ich", "bin", "dein", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr, eile du, mein Schutz, mir beizustehen,", "tokens": ["Herr", ",", "ei\u00b7le", "du", ",", "mein", "Schutz", ",", "mir", "bei\u00b7zu\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und leite mich auf ebner Bahn.", "tokens": ["Und", "lei\u00b7te", "mich", "auf", "eb\u00b7ner", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er h\u00f6rt mein Schrein, der Herr erh\u00f6rt mein Flehen,", "tokens": ["Er", "h\u00f6rt", "mein", "Schrein", ",", "der", "Herr", "er\u00b7h\u00f6rt", "mein", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nimmt sich meiner Seelen an.", "tokens": ["Und", "nimmt", "sich", "mei\u00b7ner", "See\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}