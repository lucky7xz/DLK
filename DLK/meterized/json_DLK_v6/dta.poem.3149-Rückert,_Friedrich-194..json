{"dta.poem.3149": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "194.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Steigen ist die Zeit, auch wo sie scheint im Sinken;", "tokens": ["Im", "Stei\u00b7gen", "ist", "die", "Zeit", ",", "auch", "wo", "sie", "scheint", "im", "Sin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ziel, nach dem sie steigt, das hohe seh' ich winken.", "tokens": ["Das", "Ziel", ",", "nach", "dem", "sie", "steigt", ",", "das", "ho\u00b7he", "seh'", "ich", "win\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "ART", "ADJA", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Anh\u00f6hn und Tiefen sind abwechselnd auf der Bahn,", "tokens": ["An\u00b7h\u00f6hn", "und", "Tie\u00b7fen", "sind", "ab\u00b7wech\u00b7selnd", "auf", "der", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Doch jede Senkung ist Erhebung dort hinan:", "tokens": ["Doch", "je\u00b7de", "Sen\u00b7kung", "ist", "Er\u00b7he\u00b7bung", "dort", "hi\u00b7nan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zum Ziel geht jeder Schritt, der vorw\u00e4rts wird gethan.", "tokens": ["Zum", "Ziel", "geht", "je\u00b7der", "Schritt", ",", "der", "vor\u00b7w\u00e4rts", "wird", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN", "$,", "PRELS", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}