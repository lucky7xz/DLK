{"textgrid.poem.44761": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift auf den F.Z.M.K.", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier schied aus diesem Jammertal", "tokens": ["Hier", "schied", "aus", "die\u00b7sem", "Jam\u00b7mer\u00b7tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kriegsmann, wie sonst keiner!", "tokens": ["Ein", "Kriegs\u00b7mann", ",", "wie", "sonst", "kei\u00b7ner", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er bracht es bis zum General", "tokens": ["Er", "bracht", "es", "bis", "zum", "Ge\u00b7ne\u00b7ral"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und blieb doch ein Gemeiner.", "tokens": ["Und", "blieb", "doch", "ein", "Ge\u00b7mei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}