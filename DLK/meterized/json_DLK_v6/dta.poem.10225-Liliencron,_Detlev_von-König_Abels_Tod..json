{"dta.poem.10225": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Abels Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig schl&#228;ft im purpurnen Zelt,             ", "tokens": ["Der", "K\u00f6\u00b7nig", "schl", "&#228;", "ft", "im", "pur\u00b7pur\u00b7nen", "Zelt", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Blauampellicht gefangen h\u00e4lt", "tokens": ["Blau\u00b7am\u00b7pel\u00b7licht", "ge\u00b7fan\u00b7gen", "h\u00e4lt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Vor den Deichen ebben die Wasser dumpf,", "tokens": ["Vor", "den", "Dei\u00b7chen", "eb\u00b7ben", "die", "Was\u00b7ser", "dumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durch die Nacht wiehert ein Pferd. Die Fr\u00f6sch\u2019 im Sumpf,", "tokens": ["Durch", "die", "Nacht", "wie\u00b7hert", "ein", "Pferd", ".", "Die", "Fr\u00f6sch'", "im", "Sumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$.", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}