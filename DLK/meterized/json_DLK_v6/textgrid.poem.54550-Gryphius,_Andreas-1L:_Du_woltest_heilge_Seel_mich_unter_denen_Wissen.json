{"textgrid.poem.54550": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du woltest/ heilge Seel/ mich unter denen Wissen/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du woltest/ heilge Seel/ mich unter denen Wissen/", "tokens": ["Du", "wol\u00b7test", "/", "heil\u00b7ge", "Seel", "/", "mich", "un\u00b7ter", "de\u00b7nen", "Wis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ADJA", "NN", "$(", "PPER", "APPR", "PRELS", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die fern von fremder List/ ausl\u00e4ndscher V\u00f6lcker Pracht/", "tokens": ["Die", "fern", "von", "frem\u00b7der", "List", "/", "aus\u00b7l\u00e4nd\u00b7scher", "V\u00f6l\u00b7cker", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "$(", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sch\u00f6n-geschmincktes Nichts mit grossem Muth verlacht/", "tokens": ["Und", "sch\u00f6n\u00b7ge\u00b7schmin\u00b7ck\u00b7tes", "Nichts", "mit", "gros\u00b7sem", "Muth", "ver\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und deutscher Treu und Brust und Zunge sich befliessen!", "tokens": ["Und", "deut\u00b7scher", "Treu", "und", "Brust", "und", "Zun\u00b7ge", "sich", "be\u00b7flies\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man soll auf deinen Schlu\u00df mich als unsterblich gr\u00fcssen/", "tokens": ["Man", "soll", "auf", "dei\u00b7nen", "Schlu\u00df", "mich", "als", "uns\u00b7terb\u00b7lich", "gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "KOUS", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach F\u00fcrst! zu grosse Huld! Hier ist nichts was die Nacht", "tokens": ["Ach", "F\u00fcrst", "!", "zu", "gros\u00b7se", "Huld", "!", "Hier", "ist", "nichts", "was", "die", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "APPR", "ADJA", "NN", "$.", "ADV", "VAFIN", "PIS", "PWS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Todes pochen kan: Mein bebend Hertze kracht/", "tokens": ["Des", "To\u00b7des", "po\u00b7chen", "kan", ":", "Mein", "be\u00b7bend", "Hert\u00b7ze", "kracht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$.", "PPOSAT", "ADJD", "VVFIN", "ADJD", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Indem es \u00fcberlegt' wie Zeit und Welt verschiessen.", "tokens": ["In\u00b7dem", "es", "\u00fc\u00b7ber\u00b7legt'", "wie", "Zeit", "und", "Welt", "ver\u00b7schies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KOKOM", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dis schrieb ich und erfahr da\u00df du von hinnen seyst/", "tokens": ["Dis", "schrieb", "ich", "und", "er\u00b7fahr", "da\u00df", "du", "von", "hin\u00b7nen", "seyst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "VVFIN", "KOUS", "PPER", "APPR", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und selbst unsterblich dich der Ewigkeit verm\u00e4hlet.", "tokens": ["Und", "selbst", "uns\u00b7terb\u00b7lich", "dich", "der", "E\u00b7wig\u00b7keit", "ver\u00b7m\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hast du zu guter Nacht denn mich/ durchlauchter Geist/", "tokens": ["Hast", "du", "zu", "gu\u00b7ter", "Nacht", "denn", "mich", "/", "durch\u00b7lauch\u00b7ter", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "KON", "PPER", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu dem/ was du ergetzt besitzest/ auserw\u00e4hlet?", "tokens": ["Zu", "dem", "/", "was", "du", "er\u00b7getzt", "be\u00b7sit\u00b7zest", "/", "au\u00b7ser\u00b7w\u00e4h\u00b7let", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "$(", "PWS", "PPER", "VVPP", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich kan der F\u00fcrsten Sch\u00e4tz und G\u00fcter andern g\u00f6nnen/", "tokens": ["Ich", "kan", "der", "F\u00fcrs\u00b7ten", "Sch\u00e4tz", "und", "G\u00fc\u00b7ter", "an\u00b7dern", "g\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "KON", "NN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kein F\u00fcrst hat keinem ie/ was du mir schencken k\u00f6nnen.", "tokens": ["Kein", "F\u00fcrst", "hat", "kei\u00b7nem", "ie", "/", "was", "du", "mir", "schen\u00b7cken", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PIS", "ADV", "$(", "PWS", "PPER", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}