{"dta.poem.19018": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.   Bey der ersten Communion.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "So ist es denn geschehen:", "tokens": ["So", "ist", "es", "denn", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich habe GOtt gesehen:", "tokens": ["Ich", "ha\u00b7be", "Gott", "ge\u00b7se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat sich eingefunden,", "tokens": ["Er", "hat", "sich", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sich mit mir verbunden.", "tokens": ["Und", "sich", "mit", "mir", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er hat mich Liebes-Krancken,", "tokens": ["Er", "hat", "mich", "Lie\u00b7bes\u00b7Kran\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bey seligen Gedancken,", "tokens": ["Bey", "se\u00b7li\u00b7gen", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Zu seinem Tisch geleitet,", "tokens": ["Zu", "sei\u00b7nem", "Tisch", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und theure Kost bereitet.", "tokens": ["Und", "theu\u00b7re", "Kost", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wie danck ichs Christi Liebe,", "tokens": ["Wie", "danck", "ichs", "Chris\u00b7ti", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIS", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die, aus dem treusten Triebe,", "tokens": ["Die", ",", "aus", "dem", "treus\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Sich, um mich zu erheben,", "tokens": ["Sich", ",", "um", "mich", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ins Niedrige gegeben!", "tokens": ["Ins", "Nied\u00b7ri\u00b7ge", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wie danck ichs seinem Hertzen,", "tokens": ["Wie", "danck", "ichs", "sei\u00b7nem", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Das so viel herbe Schmertzen", "tokens": ["Das", "so", "viel", "her\u00b7be", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "F\u00fcr mich, der sie verschuldet,", "tokens": ["F\u00fcr", "mich", ",", "der", "sie", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Aus lauter Lieb\u2019 erdultet!", "tokens": ["Aus", "lau\u00b7ter", "Lieb'", "er\u00b7dul\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Wie danck ichs seinem Leiden,", "tokens": ["Wie", "danck", "ichs", "sei\u00b7nem", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Dem Ursprung meiner Freuden:", "tokens": ["Dem", "Ur\u00b7sprung", "mei\u00b7ner", "Freu\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Wie danck ichs seinem Stehnen,", "tokens": ["Wie", "danck", "ichs", "sei\u00b7nem", "Steh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Und hei\u00df-vergo\u00dfnen Thr\u00e4nen!", "tokens": ["Und", "hei\u00df\u00b7ver\u00b7go\u00df\u00b7nen", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Wie danck ichs seinem D\u00fcrsten,", "tokens": ["Wie", "danck", "ichs", "sei\u00b7nem", "D\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da ihm, dem Lebens-F\u00fcrsten,", "tokens": ["Da", "ihm", ",", "dem", "Le\u00b7bens\u00b7F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die Zung am Gaumen klebte,", "tokens": ["Die", "Zung", "am", "Gau\u00b7men", "kleb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Und mich die Krafft belebte!", "tokens": ["Und", "mich", "die", "Krafft", "be\u00b7leb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Wie danck ichs seinem Sterben;", "tokens": ["Wie", "danck", "ichs", "sei\u00b7nem", "Ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Es t\u00f6dtet mein Verderben:", "tokens": ["Es", "t\u00f6d\u00b7tet", "mein", "Ver\u00b7der\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Sein letztes Angst-Geth\u00f6ne", "tokens": ["Sein", "letz\u00b7tes", "Angst\u00b7Get\u00b7h\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Klingt meinen Ohren sch\u00f6ne.", "tokens": ["Klingt", "mei\u00b7nen", "Oh\u00b7ren", "sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Die Fahrt ans Grabes Schwelle,", "tokens": ["Die", "Fahrt", "ans", "Gra\u00b7bes", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Und zu der Th\u00fcr der H\u00f6lle,", "tokens": ["Und", "zu", "der", "Th\u00fcr", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Bewahrt mich vor den Schl\u00fcnden,", "tokens": ["Be\u00b7wahrt", "mich", "vor", "den", "Schl\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Die nimmer zu ergr\u00fcnden.", "tokens": ["Die", "nim\u00b7mer", "zu", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Du Hertz-vertraute Liebe,", "tokens": ["Du", "Hertz\u00b7ver\u00b7trau\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Entz\u00fcnde meine Triebe,", "tokens": ["Ent\u00b7z\u00fcn\u00b7de", "mei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Damit sie, ohne Schweigen,", "tokens": ["Da\u00b7mit", "sie", ",", "oh\u00b7ne", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Von deiner Tugend zeugen.", "tokens": ["Von", "dei\u00b7ner", "Tu\u00b7gend", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "La\u00df Christi Tod und Sterben,", "tokens": ["La\u00df", "Chris\u00b7ti", "Tod", "und", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Sein ritterlichs Erwerben", "tokens": ["Sein", "rit\u00b7ter\u00b7lichs", "Er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Der hart-gebundnen Seelen,", "tokens": ["Der", "har\u00b7tge\u00b7bund\u00b7nen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Mich \u00f6ffentlich erzehlen.", "tokens": ["Mich", "\u00f6f\u00b7fent\u00b7lich", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Und nach dem Liebes-Mahle,", "tokens": ["Und", "nach", "dem", "Lie\u00b7bes\u00b7Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Gib, da\u00df ich dir bezahle", "tokens": ["Gib", ",", "da\u00df", "ich", "dir", "be\u00b7zah\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Die seelige Gel\u00fcbte,", "tokens": ["Die", "see\u00b7li\u00b7ge", "Ge\u00b7l\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Darinn sich JEsus \u00fcbte!", "tokens": ["Da\u00b7rinn", "sich", "Je\u00b7sus", "\u00fcb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Es werd an mir gesehen", "tokens": ["Es", "werd", "an", "mir", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Sein Tod und Auferstehen,", "tokens": ["Sein", "Tod", "und", "Auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Sein Kampf und Uberwinden,", "tokens": ["Sein", "Kampf", "und", "U\u00b7ber\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Sein Suchen und sein Finden!", "tokens": ["Sein", "Su\u00b7chen", "und", "sein", "Fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}