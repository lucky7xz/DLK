{"textgrid.poem.64100": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam in der weiten W\u00fcste!", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam in der weiten W\u00fcste!", "tokens": ["Ein\u00b7sam", "in", "der", "wei\u00b7ten", "W\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fern der Atlas, starr und stumm,", "tokens": ["Fern", "der", "At\u00b7las", ",", "starr", "und", "stumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Pfad und ohne Wasser,", "tokens": ["Oh\u00b7ne", "Pfad", "und", "oh\u00b7ne", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fehde, Feinde, Tod ringsum!", "tokens": ["Feh\u00b7de", ",", "Fein\u00b7de", ",", "Tod", "ring\u00b7sum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weit versprengt von meinem Stamme,", "tokens": ["Weit", "ver\u00b7sprengt", "von", "mei\u00b7nem", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einz'ger Freund mein treues Ro\u00df,", "tokens": ["Einz'\u00b7ger", "Freund", "mein", "treu\u00b7es", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Heimat ist der Sattel,", "tokens": ["Mei\u00b7ne", "Hei\u00b7mat", "ist", "der", "Sat\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All' mein Hausrat mein Gescho\u00df!", "tokens": ["All'", "mein", "Haus\u00b7rat", "mein", "Ge\u00b7scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dennoch zagt nicht meine Seele,", "tokens": ["Den\u00b7noch", "zagt", "nicht", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedem Schrecknis biet' ich Spott:", "tokens": ["Je\u00b7dem", "Schreck\u00b7nis", "biet'", "ich", "Spott", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es w\u00f6lbt auch ob der W\u00fcste", "tokens": ["Denn", "es", "w\u00f6lbt", "auch", "ob", "der", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir ein Himmelszelt mein Gott.", "tokens": ["Mir", "ein", "Him\u00b7mels\u00b7zelt", "mein", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und sein Auge sieht mich auf dem", "tokens": ["Und", "sein", "Au\u00b7ge", "sieht", "mich", "auf", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Teppich des Gebetes stehn:", "tokens": ["Tep\u00b7pich", "des", "Ge\u00b7be\u00b7tes", "stehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-----+", "measure": "dactylic.init"}, "line.3": {"text": "Allah, du bist mein Beschirmer,", "tokens": ["Al\u00b7lah", ",", "du", "bist", "mein", "Be\u00b7schir\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dein Wille mu\u00df geschehn!", "tokens": ["Und", "dein", "Wil\u00b7le", "mu\u00df", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dich bekennt einst alle Menschheit", "tokens": ["Dich", "be\u00b7kennt", "einst", "al\u00b7le", "Menschheit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 In den heil'gen B\u00fcchern steht's! \u2013", "tokens": ["\u2013", "In", "den", "heil'\u00b7gen", "B\u00fc\u00b7chern", "steht's", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es wird die ganze Erde", "tokens": ["Und", "es", "wird", "die", "gan\u00b7ze", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem Teppich des Gebets!", "tokens": ["Zu", "dem", "Tep\u00b7pich", "des", "Ge\u00b7bets", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}