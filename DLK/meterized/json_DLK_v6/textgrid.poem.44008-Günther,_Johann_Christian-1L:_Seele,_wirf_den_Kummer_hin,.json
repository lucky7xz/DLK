{"textgrid.poem.44008": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seele, wirf den Kummer hin,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seele, wirf den Kummer hin,", "tokens": ["See\u00b7le", ",", "wirf", "den", "Kum\u00b7mer", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Hoheit nachzudencken,", "tokens": ["Dei\u00b7ner", "Ho\u00b7heit", "nach\u00b7zu\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und las dir den freyen Sinn", "tokens": ["Und", "las", "dir", "den", "frey\u00b7en", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durch des Leibes Last nicht kr\u00e4ncken;", "tokens": ["Durch", "des", "Lei\u00b7bes", "Last", "nicht", "kr\u00e4n\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese B\u00fcrde, so man tr\u00e4gt,", "tokens": ["Die\u00b7se", "B\u00fcr\u00b7de", ",", "so", "man", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird in kurzem abgelegt.", "tokens": ["Wird", "in", "kur\u00b7zem", "ab\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gefangenschaft vergeht,", "tokens": ["Die", "Ge\u00b7fan\u00b7gen\u00b7schaft", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stahl und Fe\u00dfel m\u00fc\u00dfen brechen;", "tokens": ["Stahl", "und", "Fe\u00b7\u00dfel", "m\u00fc\u00b7\u00dfen", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsers Lebens Alphabet", "tokens": ["Un\u00b7sers", "Le\u00b7bens", "Al\u00b7pha\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ist ja noch wohl auszusprechen,", "tokens": ["Ist", "ja", "noch", "wohl", "aus\u00b7zu\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Macht doch auch die ganze Zeit", "tokens": ["Macht", "doch", "auch", "die", "gan\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Keinen Punct der Ewigkeit.", "tokens": ["Kei\u00b7nen", "Punct", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sclaven werden endlich frey", "tokens": ["Scla\u00b7ven", "wer\u00b7den", "end\u00b7lich", "frey"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Kercker aufgebrochen,", "tokens": ["Und", "der", "Ker\u00b7cker", "auf\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn des Todes Tyranney", "tokens": ["Wenn", "des", "To\u00b7des", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren Feinden Hohn gesprochen;", "tokens": ["Ih\u00b7ren", "Fein\u00b7den", "Hohn", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, der l\u00e4ngste Richterstab", "tokens": ["Ja", ",", "der", "l\u00e4ngs\u00b7te", "Rich\u00b7ter\u00b7stab"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Reichet selten bis ins Grab.", "tokens": ["Rei\u00b7chet", "sel\u00b7ten", "bis", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Heiden m\u00f6gen mit der Gruft", "tokens": ["Hei\u00b7den", "m\u00f6\u00b7gen", "mit", "der", "Gruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren Hofnungsport verschlie\u00dfen", "tokens": ["Ih\u00b7ren", "Hof\u00b7nungs\u00b7port", "ver\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, wenn das Verh\u00e4ngn\u00fc\u00df ruft,", "tokens": ["Und", ",", "wenn", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "ruft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Thr\u00e4nen vor Verdru\u00df vergie\u00dfen,", "tokens": ["Thr\u00e4\u00b7nen", "vor", "Ver\u00b7dru\u00df", "ver\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil sie dieser Wahn betriegt,", "tokens": ["Weil", "sie", "die\u00b7ser", "Wahn", "be\u00b7triegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df der Geist zugleich verfliegt.", "tokens": ["Da\u00df", "der", "Geist", "zu\u00b7gleich", "ver\u00b7fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unser Glaube bricht die Bahn", "tokens": ["Un\u00b7ser", "Glau\u00b7be", "bricht", "die", "Bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Kirchhof in das Leben.", "tokens": ["Durch", "den", "Kirch\u00b7hof", "in", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer die Welt nicht gr\u00fc\u00dfen kan,", "tokens": ["Wer", "die", "Welt", "nicht", "gr\u00fc\u00b7\u00dfen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lernt ihr zeitlich Abschied geben;", "tokens": ["Lernt", "ihr", "zeit\u00b7lich", "Ab\u00b7schied", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn er glaubet, da\u00df der Geist", "tokens": ["Denn", "er", "glau\u00b7bet", ",", "da\u00df", "der", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich der Sterbligkeit entrei\u00dft.", "tokens": ["Sich", "der", "Ster\u00b7blig\u00b7keit", "ent\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun wohlan, ich bin bereit,", "tokens": ["Nun", "wo\u00b7hlan", ",", "ich", "bin", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Glieder hinzulegen;", "tokens": ["Mei\u00b7ne", "Glie\u00b7der", "hin\u00b7zu\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn des Todes Bitterkeit", "tokens": ["Denn", "des", "To\u00b7des", "Bit\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchret uns auf Dornenwegen", "tokens": ["F\u00fch\u00b7ret", "uns", "auf", "Dor\u00b7nen\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In des Himmels Rosenfeld,", "tokens": ["In", "des", "Him\u00b7mels", "Ro\u00b7sen\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die Wollust Tafel h\u00e4lt.", "tokens": ["Wo", "die", "Wol\u00b7lust", "Ta\u00b7fel", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}