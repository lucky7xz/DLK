{"dta.poem.16109": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "163.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Sch\u00f6pfrad sch\u00f6pft sich matt, und Athem sch\u00f6pft es kaum;", "tokens": ["Das", "Sch\u00f6pf\u00b7rad", "sch\u00f6pft", "sich", "matt", ",", "und", "A\u00b7them", "sch\u00f6pft", "es", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$,", "KON", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh, seine Sch\u00f6pfung ist die Gr\u00fcne rings im Raum.", "tokens": ["Sieh", ",", "sei\u00b7ne", "Sch\u00f6p\u00b7fung", "ist", "die", "Gr\u00fc\u00b7ne", "rings", "im", "Raum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf seine Sch\u00f6pfung wird das Sch\u00f6pfrad stolz und eitel,", "tokens": ["Auf", "sei\u00b7ne", "Sch\u00f6p\u00b7fung", "wird", "das", "Sch\u00f6pf\u00b7rad", "stolz", "und", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Sch\u00f6pfer f\u00fchlt es sich und hebet hoch die Scheitel.", "tokens": ["Als", "Sch\u00f6p\u00b7fer", "f\u00fchlt", "es", "sich", "und", "he\u00b7bet", "hoch", "die", "Schei\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch, trank die Saat sich satt, zieht man das Sch\u00f6pfrad nieder;", "tokens": ["Doch", ",", "trank", "die", "Saat", "sich", "satt", ",", "zieht", "man", "das", "Sch\u00f6pf\u00b7rad", "nie\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Staube liegt sein Haupt, im Schmutze seine Glieder.", "tokens": ["Im", "Stau\u00b7be", "liegt", "sein", "Haupt", ",", "im", "Schmut\u00b7ze", "sei\u00b7ne", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}