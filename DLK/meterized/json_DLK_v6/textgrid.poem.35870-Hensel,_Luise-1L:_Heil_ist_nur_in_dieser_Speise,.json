{"textgrid.poem.35870": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heil ist nur in dieser Speise,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil ist nur in dieser Speise,", "tokens": ["Heil", "ist", "nur", "in", "die\u00b7ser", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Woll'st des Hungers Qualen heben;", "tokens": ["Woll'st", "des", "Hun\u00b7gers", "Qua\u00b7len", "he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weit und schwer ist meine Reise.", "tokens": ["Weit", "und", "schwer", "ist", "mei\u00b7ne", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Trinken, da\u00df mein Herz gesunde!", "tokens": ["Trin\u00b7ken", ",", "da\u00df", "mein", "Herz", "ge\u00b7sun\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast dies D\u00fcrsten mir gegeben:", "tokens": ["Hast", "dies", "D\u00fcrs\u00b7ten", "mir", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heile nun der Sehnsucht Wunde.", "tokens": ["Hei\u00b7le", "nun", "der", "Sehn\u00b7sucht", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In die Brust, die todesschwache,", "tokens": ["In", "die", "Brust", ",", "die", "to\u00b7des\u00b7schwa\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df erstarkt die Pulse beben", "tokens": ["Da\u00df", "er\u00b7starkt", "die", "Pul\u00b7se", "be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Odem neu erwache!", "tokens": ["Und", "der", "O\u00b7dem", "neu", "er\u00b7wa\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nieder in des Todes Schatten!", "tokens": ["Nie\u00b7der", "in", "des", "To\u00b7des", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf zu Dir ja will ich schweben,", "tokens": ["Auf", "zu", "Dir", "ja", "will", "ich", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df die Schwingen nicht ermatten!", "tokens": ["La\u00df", "die", "Schwin\u00b7gen", "nicht", "er\u00b7mat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bringe meiner Seele Frieden,", "tokens": ["Brin\u00b7ge", "mei\u00b7ner", "See\u00b7le", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sich hell die Blicke heben,", "tokens": ["Da\u00df", "sich", "hell", "die", "Bli\u00b7cke", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so lang den Tag gemieden!", "tokens": ["Die", "so", "lang", "den", "Tag", "ge\u00b7mie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ohne Dich m\u00fc\u00dft' ich verderben:", "tokens": ["Oh\u00b7ne", "Dich", "m\u00fc\u00dft'", "ich", "ver\u00b7der\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mir Erd' und Himmel geben", "tokens": ["Was", "mir", "Erd'", "und", "Him\u00b7mel", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Au\u00dfer Dir, w\u00e4r' qualvoll Sterben.", "tokens": ["Au\u00b7\u00dfer", "Dir", ",", "w\u00e4r'", "qual\u00b7voll", "Ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VAFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}