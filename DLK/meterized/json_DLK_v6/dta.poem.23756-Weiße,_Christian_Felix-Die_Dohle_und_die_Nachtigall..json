{"dta.poem.23756": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Dohle und die Nachtigall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Kleiner Schreyhals sage mir,", "tokens": ["Klei\u00b7ner", "Schrey\u00b7hals", "sa\u00b7ge", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ey, wie k\u00f6mmts, da\u00df Menschen dir", "tokens": ["Ey", ",", "wie", "k\u00f6mmts", ",", "da\u00df", "Men\u00b7schen", "dir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "NE", "$,", "KOUS", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So viel Beyfall gern gew\u00e4hren?", "tokens": ["So", "viel", "Bey\u00b7fall", "gern", "ge\u00b7w\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleichwohl schweigt oft dein Gesang:", "tokens": ["Gleich\u00b7wohl", "schweigt", "oft", "dein", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich! ich schwatze Tage lang,", "tokens": ["Ich", "!", "ich", "schwat\u00b7ze", "Ta\u00b7ge", "lang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PPER", "VVFIN", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mich w\u00fcnscht kein Mensch zu h\u00f6ren!", "tokens": ["Und", "mich", "w\u00fcnscht", "kein", "Mensch", "zu", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00f6mmt es denn aufs Schwatzen an?", "tokens": ["K\u00f6mmt", "es", "denn", "aufs", "Schwat\u00b7zen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem, der niemals schweigen kann,", "tokens": ["Dem", ",", "der", "nie\u00b7mals", "schwei\u00b7gen", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird kein Kluger Lob gew\u00e4hren.", "tokens": ["Wird", "kein", "Klu\u00b7ger", "Lob", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du sprichst ohne Unterla\u00df,", "tokens": ["Du", "sprichst", "oh\u00b7ne", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jmmer einerley, und was?", "tokens": ["Jm\u00b7mer", "ei\u00b7ner\u00b7ley", ",", "und", "was", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KON", "PWS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nichts. Das w\u00fcnscht man auch zu h\u00f6ren!", "tokens": ["Nichts", ".", "Das", "w\u00fcnscht", "man", "auch", "zu", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "PDS", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}