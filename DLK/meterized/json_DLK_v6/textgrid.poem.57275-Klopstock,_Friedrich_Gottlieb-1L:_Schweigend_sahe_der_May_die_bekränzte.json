{"textgrid.poem.57275": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schweigend sahe der May die bekr\u00e4nzte", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigend sahe der May die bekr\u00e4nzte", "tokens": ["Schwei\u00b7gend", "sa\u00b7he", "der", "May", "die", "be\u00b7kr\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Leichtwehende Lock' im Silberbach;", "tokens": ["Leicht\u00b7we\u00b7hen\u00b7de", "Lock'", "im", "Sil\u00b7ber\u00b7bach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "R\u00f6thlich war sein Kranz, wie des Aufgangs,", "tokens": ["R\u00f6th\u00b7lich", "war", "sein", "Kranz", ",", "wie", "des", "Auf\u00b7gangs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Er sah sich, und l\u00e4chelte sanft.", "tokens": ["Er", "sah", "sich", ",", "und", "l\u00e4\u00b7chel\u00b7te", "sanft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "W\u00fcthend kam ein Orkan am Gebirg' her!", "tokens": ["W\u00fct\u00b7hend", "kam", "ein", "Or\u00b7kan", "am", "Ge\u00b7bir\u00b7g'", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die Esche, die Tann', und Eiche brach,", "tokens": ["Die", "E\u00b7sche", ",", "die", "Tann'", ",", "und", "Ei\u00b7che", "brach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und mit Felsen st\u00fcrzte der Ahorn", "tokens": ["Und", "mit", "Fel\u00b7sen", "st\u00fcrz\u00b7te", "der", "A\u00b7horn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Vom bebenden Haupt des Gebirgs.", "tokens": ["Vom", "be\u00b7ben\u00b7den", "Haupt", "des", "Ge\u00b7birgs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ruhig schlummert' am Bache der May ein,", "tokens": ["Ru\u00b7hig", "schlum\u00b7mert'", "am", "Ba\u00b7che", "der", "May", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Liess rasen den lauten Donnersturm!", "tokens": ["Liess", "ra\u00b7sen", "den", "lau\u00b7ten", "Don\u00b7ner\u00b7sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Lauscht', und schlief, beweht von der Bl\u00fcthe,", "tokens": ["Lauscht'", ",", "und", "schlief", ",", "be\u00b7weht", "von", "der", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und wachte mit Hesperus auf.", "tokens": ["Und", "wach\u00b7te", "mit", "Hes\u00b7pe\u00b7rus", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Jetzo f\u00fchlst du noch nichts von dem Elend,", "tokens": ["Jet\u00b7zo", "f\u00fchlst", "du", "noch", "nichts", "von", "dem", "E\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wie Grazien lacht das Leben dir.", "tokens": ["Wie", "Gra\u00b7zi\u00b7en", "lacht", "das", "Le\u00b7ben", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf, und wafne dich mit der Weisheit!", "tokens": ["Auf", ",", "und", "waf\u00b7ne", "dich", "mit", "der", "Weis\u00b7heit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Denn, J\u00fcngling, die Blume verbl\u00fcht!", "tokens": ["Denn", ",", "J\u00fcng\u00b7ling", ",", "die", "Blu\u00b7me", "ver\u00b7bl\u00fcht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}