{"dta.poem.20565": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Erster Tag der Schlacht von Bautzen,  \n  am  20.  Mai  1813.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Neun Uhr fr\u00fch, auf Schmochtitzer H\u00f6h\u2019", "tokens": ["Neun", "Uhr", "fr\u00fch", ",", "auf", "Schmoch\u00b7tit\u00b7zer", "H\u00f6h'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Napoleon besah die N&#228;h'. &#8212;", "tokens": ["Na\u00b7po\u00b7le\u00b7on", "be\u00b7sah", "die", "N", "&#228;", "h'", ".", "&#8212;"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "punct", "XML_entity"], "pos": ["NE", "VVFIN", "ART", "XY", "$(", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Um Mittagszeit begann die Schlacht,", "tokens": ["Um", "Mit\u00b7tags\u00b7zeit", "be\u00b7gann", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo das Gesch\u00fctz den Anfang macht.", "tokens": ["Wo", "das", "Ge\u00b7sch\u00fctz", "den", "An\u00b7fang", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es f\u00fchlt sich Kaiser klar und frei,", "tokens": ["Es", "f\u00fchlt", "sich", "Kai\u00b7ser", "klar", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df Er nicht wartet erst auf ", "tokens": ["Da\u00df", "Er", "nicht", "war\u00b7tet", "erst", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ADV", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Sein Kampfro\u00df l\u00e4\u00dft sich nicht mehr z\u00fcgeln,", "tokens": ["Sein", "Kamp\u00b7fro\u00df", "l\u00e4\u00dft", "sich", "nicht", "mehr", "z\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ganz \u00fcberfl\u00fcgeln", "tokens": ["Ganz", "\u00fc\u00b7berf\u00b7l\u00fc\u00b7geln"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Will Feind Er. \u2014 Der soll j\u00e4hlings weichen", "tokens": ["Will", "Feind", "Er", ".", "Der", "soll", "j\u00e4h\u00b7lings", "wei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "PPER", "$.", "$(", "PDS", "VMFIN", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und Er Sich Siegespalme reichen! \u2014", "tokens": ["Und", "Er", "Sich", "Sie\u00b7ge\u00b7spal\u00b7me", "rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "PRF", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Bald", "tokens": ["Bald"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Sah man ", "tokens": ["Sah", "man"], "token_info": ["word", "word"], "pos": ["VVFIN", "PIS"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Marmont und Oudinot vorr&#252;cken", "tokens": ["Mar\u00b7mont", "und", "Ou\u00b7di\u00b7not", "vorr", "&#252;", "cken"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["NE", "KON", "NE", "NE", "$(", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.14": {"text": "Ueber Br\u00fccken.", "tokens": ["Ue\u00b7ber", "Br\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Muth\u2019gen und festen Widerstand", "tokens": ["Muth'\u00b7gen", "und", "fes\u00b7ten", "Wi\u00b7der\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Vom Wirtemberger Prinz ", "tokens": ["Vom", "Wir\u00b7tem\u00b7ber\u00b7ger", "Prinz"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+---+", "measure": "dactylic.init"}, "line.18": {"text": "Mannhaft sah man den Letzt\u2019ren stehn,", "tokens": ["Mann\u00b7haft", "sah", "man", "den", "Letzt'\u00b7ren", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.19": {"text": "Bis endlich doppelte Gewalt,", "tokens": ["Bis", "end\u00b7lich", "dop\u00b7pel\u00b7te", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Theils ", "tokens": ["Theils"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Jhn dr\u00e4ngte.", "tokens": ["Jhn", "dr\u00e4ng\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Erst jetzt ", "tokens": ["Erst", "jetzt"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Auf diese Weise also lenkte", "tokens": ["Auf", "die\u00b7se", "Wei\u00b7se", "al\u00b7so", "lenk\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Napoleon des Feindes Sinn", "tokens": ["Na\u00b7po\u00b7le\u00b7on", "des", "Fein\u00b7des", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nach dessen linkem Fl\u00fcgel hin.", "tokens": ["Nach", "des\u00b7sen", "lin\u00b7kem", "Fl\u00fc\u00b7gel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die Bundesmacht w\u00e4hnt, hier sei Noth,", "tokens": ["Die", "Bun\u00b7des\u00b7macht", "w\u00e4hnt", ",", "hier", "sei", "Noth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Nur dieser Fl\u00fcgel sei bedroht! \u2014", "tokens": ["Nur", "die\u00b7ser", "Fl\u00fc\u00b7gel", "sei", "be\u00b7droht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Um zu verst\u00e4rken hier Gewalt,", "tokens": ["Um", "zu", "ver\u00b7st\u00e4r\u00b7ken", "hier", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Schickt hin man Truppen vom R\u00fcckhalt.", "tokens": ["Schickt", "hin", "man", "Trup\u00b7pen", "vom", "R\u00fcck\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Vergessen ward, da\u00df hierdurch schlage", "tokens": ["Ver\u00b7ges\u00b7sen", "ward", ",", "da\u00df", "hier\u00b7durch", "schla\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "KOUS", "PAV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Man schw\u00e4cher sich am n\u00e4chsten Tage!", "tokens": ["Man", "schw\u00e4\u00b7cher", "sich", "am", "n\u00e4chs\u00b7ten", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Diebitsch, Emanuel und Eugen", "tokens": ["Die\u00b7bitsch", ",", "E\u00b7ma\u00b7nu\u00b7el", "und", "Eu\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NE", "KON", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Nahmen aufs Neu, man mu\u00df gestehn,", "tokens": ["Nah\u00b7men", "aufs", "Neu", ",", "man", "mu\u00df", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Mit Ruhm, bei m\u00f6rderischem Feuer", "tokens": ["Mit", "Ruhm", ",", "bei", "m\u00f6r\u00b7de\u00b7ri\u00b7schem", "Feu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Falkenberg, Pielitz und Mehltheuer! \u2014", "tokens": ["Fal\u00b7ken\u00b7berg", ",", "Pie\u00b7litz", "und", "Mehl\u00b7theu\u00b7er", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "$.", "$("], "meter": "+--+--++-", "measure": "dactylic.di.plus"}, "line.36": {"text": "Den Kaiser Ru\u00dflands, Preu\u00dfens K\u00f6nig,", "tokens": ["Den", "Kai\u00b7ser", "Ru\u00df\u00b7lands", ",", "Preu\u00b7\u00dfens", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Die Zwei besonders noch erw\u00e4hn\u2019 ich.", "tokens": ["Die", "Zwei", "be\u00b7son\u00b7ders", "noch", "er\u00b7w\u00e4hn'", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Von Jhrem hocherlauchten Haupte", "tokens": ["Von", "Ih\u00b7rem", "ho\u00b7cher\u00b7lauch\u00b7ten", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Ging Wunsch aus, da\u00df man fest behaupte", "tokens": ["Ging", "Wunsch", "aus", ",", "da\u00df", "man", "fest", "be\u00b7haup\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PTKVZ", "$,", "KOUS", "PIS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Die Stellung, die man eingenommen! \u2014", "tokens": ["Die", "Stel\u00b7lung", ",", "die", "man", "ein\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Nur steter Angriff k\u00f6nne frommen! \u2014", "tokens": ["Nur", "ste\u00b7ter", "An\u00b7griff", "k\u00f6n\u00b7ne", "from\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Rings um Sie tobten die Gefahren,", "tokens": ["Rings", "um", "Sie", "tob\u00b7ten", "die", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Sie und ", "tokens": ["Sie", "und"], "token_info": ["word", "word"], "pos": ["PPER", "KON"], "meter": "+-", "measure": "trochaic.single"}, "line.44": {"text": "Befanden sich im linken Fl\u00fcgel,", "tokens": ["Be\u00b7fan\u00b7den", "sich", "im", "lin\u00b7ken", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Wo jeder H\u00fcgel", "tokens": ["Wo", "je\u00b7der", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.46": {"text": "Vertheidigt ward mit festem Muth. \u2014", "tokens": ["Ver\u00b7thei\u00b7digt", "ward", "mit", "fes\u00b7tem", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Mit langem Federbusch am Hut", "tokens": ["Mit", "lan\u00b7gem", "Fe\u00b7der\u00b7busch", "am", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Ritt ", "tokens": ["Ritt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.49": {"text": "Kart\u00e4tschenhagel zischte mit,", "tokens": ["Kar\u00b7t\u00e4t\u00b7schen\u00b7ha\u00b7gel", "zischte", "mit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.50": {"text": "Ein gro\u00df\u2019 Gefolge um ihn her", "tokens": ["Ein", "gro\u00df'", "Ge\u00b7fol\u00b7ge", "um", "ihn", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Und dabei fragt\u2019 in Sorgen Er:", "tokens": ["Und", "da\u00b7bei", "fragt'", "in", "Sor\u00b7gen", "Er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "APPR", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Ist noch Mein Kaiser an dem Ort,", "tokens": ["Ist", "noch", "Mein", "Kai\u00b7ser", "an", "dem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Wo j\u00fcngst Er war? O! reit\u2019 Er fort!", "tokens": ["Wo", "j\u00fcngst", "Er", "war", "?", "O", "!", "reit'", "Er", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VAFIN", "$.", "NE", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Kanonenkugeln sausen dort!", "tokens": ["Ka\u00b7no\u00b7nen\u00b7ku\u00b7geln", "sau\u00b7sen", "dort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Zu seinen Kriegern rief er: Steht!", "tokens": ["Zu", "sei\u00b7nen", "Krie\u00b7gern", "rief", "er", ":", "Steht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$.", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Es blickt auf Euch die Majest\u00e4t! \u2014", "tokens": ["Es", "blickt", "auf", "Euch", "die", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Um 10 Uhr endet gegen Nacht", "tokens": ["Um", "10", "Uhr", "en\u00b7det", "ge\u00b7gen", "Nacht"], "token_info": ["word", "number", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.58": {"text": "Der erste Tag von Bautzen\u2019s Schlacht. \u2014", "tokens": ["Der", "ers\u00b7te", "Tag", "von", "Baut\u00b7zen's", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Doch ehe sich zu Ende neigt", "tokens": ["Doch", "e\u00b7he", "sich", "zu", "En\u00b7de", "neigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Dieser an Blut so schwere Tag,", "tokens": ["Die\u00b7ser", "an", "Blut", "so", "schwe\u00b7re", "Ta