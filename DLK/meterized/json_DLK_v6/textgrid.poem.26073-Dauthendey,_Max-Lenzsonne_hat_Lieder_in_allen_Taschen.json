{"textgrid.poem.26073": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Lenzsonne hat Lieder in allen Taschen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kastanienknospen wiegt der Wind,", "tokens": ["Kas\u00b7ta\u00b7ni\u00b7enk\u00b7nos\u00b7pen", "wiegt", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und frisches Gras am Weg sich biegt,", "tokens": ["Und", "fri\u00b7sches", "Gras", "am", "Weg", "sich", "biegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drinnen die Sonne sich m\u00fcde liegt.", "tokens": ["Drin\u00b7nen", "die", "Son\u00b7ne", "sich", "m\u00fc\u00b7de", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Den ganzen Tag am Flu\u00df sie sa\u00df", "tokens": ["Den", "gan\u00b7zen", "Tag", "am", "Flu\u00df", "sie", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah den Wellen zu, die sich haschen,", "tokens": ["Und", "sah", "den", "Wel\u00b7len", "zu", ",", "die", "sich", "ha\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und singt sich Lieder an der Stra\u00df',", "tokens": ["Und", "singt", "sich", "Lie\u00b7der", "an", "der", "Stra\u00df'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die in den Wellen, den raschen, sind.", "tokens": ["Die", "in", "den", "Wel\u00b7len", ",", "den", "ra\u00b7schen", ",", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VAFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Lenzsonne hat Lieder in allen Taschen,", "tokens": ["Lenz\u00b7son\u00b7ne", "hat", "Lie\u00b7der", "in", "al\u00b7len", "Ta\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "++--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Die steckt sie ins Mieder manch sch\u00f6nem Kind.", "tokens": ["Die", "steckt", "sie", "ins", "Mie\u00b7der", "manch", "sch\u00f6\u00b7nem", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}