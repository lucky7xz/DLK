{"textgrid.poem.64899": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir z\u00fcrnen, da\u00df du mich verlassen? \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir z\u00fcrnen, da\u00df du mich verlassen? \u2013", "tokens": ["Dir", "z\u00fcr\u00b7nen", ",", "da\u00df", "du", "mich", "ver\u00b7las\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Himmel, nein! wie sollt' ich das?", "tokens": ["Beim", "Him\u00b7mel", ",", "nein", "!", "wie", "sollt'", "ich", "das", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PTKANT", "$.", "PWAV", "VMFIN", "PPER", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War's deine Schuld, mich nicht zu fassen?", "tokens": ["Wa\u00b7r's", "dei\u00b7ne", "Schuld", ",", "mich", "nicht", "zu", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Verdient ein blinder Irrthum Ha\u00df?", "tokens": ["Ver\u00b7di\u00b7ent", "ein", "blin\u00b7der", "Irr\u00b7thum", "Ha\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Bes\u00e4\u00dfe dein Gem\u00fcth die Schwingen,", "tokens": ["Be\u00b7s\u00e4\u00b7\u00dfe", "dein", "Ge\u00b7m\u00fcth", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu schweben auf des meinen Spur,", "tokens": ["Zu", "schwe\u00b7ben", "auf", "des", "mei\u00b7nen", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann lie\u00dfest du mich dir entringen", "tokens": ["Dann", "lie\u00b7\u00dfest", "du", "mich", "dir", "ent\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinem eignen Leben nur!", "tokens": ["Mit", "dei\u00b7nem", "eig\u00b7nen", "Le\u00b7ben", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wen also h\u00e4tt' ich anzuklagen?", "tokens": ["Wen", "al\u00b7so", "h\u00e4tt'", "ich", "an\u00b7zu\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, da\u00df dein Herz so schwach und klein?", "tokens": ["Dich", ",", "da\u00df", "dein", "Herz", "so", "schwach", "und", "klein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Davon kannst du die Schuld nicht tragen!", "tokens": ["Da\u00b7von", "kannst", "du", "die", "Schuld", "nicht", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie du's empfangen, blieb es dein.", "tokens": ["Wie", "du's", "emp\u00b7fan\u00b7gen", ",", "blieb", "es", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fahr hin! als der Vergebung Bl\u00fcthe", "tokens": ["Fahr", "hin", "!", "als", "der", "Ver\u00b7ge\u00b7bung", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "KOUS", "ART", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Rankt sich der Wunsch noch himmelan,", "tokens": ["Rankt", "sich", "der", "Wunsch", "noch", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da\u00df Gott fortan dein Gl\u00fcck beh\u00fcte,", "tokens": ["Da\u00df", "Gott", "for\u00b7tan", "dein", "Gl\u00fcck", "be\u00b7h\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil's meine Liebe nicht mehr kann.", "tokens": ["Weil's", "mei\u00b7ne", "Lie\u00b7be", "nicht", "mehr", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKNEG", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}