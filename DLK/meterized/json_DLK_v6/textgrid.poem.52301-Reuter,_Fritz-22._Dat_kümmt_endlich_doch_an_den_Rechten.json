{"textgrid.poem.52301": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "22. Dat k\u00fcmmt endlich doch an den Rechten", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De oll Postmeister M\u00f6ller fr\u00f6ggt", "tokens": ["De", "oll", "Post\u00b7meis\u00b7ter", "M\u00f6l\u00b7ler", "fr\u00f6ggt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Jungen, de de Breiw utdr\u00f6ggt:", "tokens": ["Den", "Jun\u00b7gen", ",", "de", "de", "Breiw", "ut\u00b7dr\u00f6ggt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbhest du de Breiw besorgt, Jehann?\u00ab", "tokens": ["\u00bb", "hest", "du", "de", "Breiw", "be\u00b7sorgt", ",", "Je\u00b7hann", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "NE", "NE", "VVFIN", "$,", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbja, Herr!\u00ab \u2013 \u00bbOk den, de an", "tokens": ["\u00bb", "ja", ",", "Herr", "!", "\u00ab", "\u2013", "\u00bb", "Ok", "den", ",", "de", "an"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PTKANT", "$,", "NN", "$.", "$(", "$(", "$(", "NN", "ART", "$,", "NE", "PTKVZ"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Den Jehann Krischan Engel wir,", "tokens": ["Den", "Je\u00b7hann", "Krisc\u00b7han", "En\u00b7gel", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "NE", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "De bi den Snider Block is in de Lihr?", "tokens": ["De", "bi", "den", "Sni\u00b7der", "Block", "is", "in", "de", "Lihr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NN", "FM", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hest du sin Wahnung endlich funnen?\u00ab", "tokens": ["Hest", "du", "sin", "Wah\u00b7nung", "end\u00b7lich", "fun\u00b7nen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbja, Herr\u00ab, antwurt de Burs, nahdem hei sick besunnen,", "tokens": ["\u00bb", "ja", ",", "Herr", "\u00ab", ",", "ant\u00b7wurt", "de", "Burs", ",", "nah\u00b7dem", "hei", "sick", "be\u00b7sun\u00b7nen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "$(", "$,", "VVFIN", "NE", "NE", "$,", "FM", "FM", "FM", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u00bbja, Herr. Doch mit den ollen Breiw", "tokens": ["\u00bb", "ja", ",", "Herr", ".", "Doch", "mit", "den", "ol\u00b7len", "Breiw"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "NN", "$.", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dor gung mi dat tauirst ganz eklich scheiw;", "tokens": ["Dor", "gung", "mi", "dat", "tau\u00b7irst", "ganz", "ek\u00b7lich", "scheiw", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ART", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "De Sak, de was sihr bisterig,", "tokens": ["De", "Sak", ",", "de", "was", "sihr", "bis\u00b7te\u00b7rig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Denn in de Laagerstrat, dor wahnt hei nich", "tokens": ["Denn", "in", "de", "Laa\u00b7ger\u00b7strat", ",", "dor", "wahnt", "hei", "nich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NE", "$,", "ADV", "VVPP", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Un wahnt en En'n lang wider an den Strand;", "tokens": ["Un", "wahnt", "en", "En'n", "lang", "wi\u00b7der", "an", "den", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "ADJD", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Un wahnt nich rechtsch \u2013 ne! linker Hand;", "tokens": ["Un", "wahnt", "nich", "rechtsch", "\u2013", "ne", "!", "lin\u00b7ker", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADJD", "$(", "NE", "$.", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Un wahnt ok nich in't dr\u00fcdde Stock \u2013", "tokens": ["Un", "wahnt", "ok", "nich", "in't", "dr\u00fcd\u00b7de", "Stock", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Ne! hei wahnt unnen in en Keller;", "tokens": ["Ne", "!", "hei", "wahnt", "un\u00b7nen", "in", "en", "Kel\u00b7ler", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sin Meister is nich Snider Block,", "tokens": ["Sin", "Meis\u00b7ter", "is", "nich", "Sni\u00b7der", "Block", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "PTKNEG", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sin Meister, de heit Snider Teller;", "tokens": ["Sin", "Meis\u00b7ter", ",", "de", "heit", "Sni\u00b7der", "Tel\u00b7ler", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NE", "NE", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.19": {"text": "Hei s\u00fclwst, hei heit nich Krischan Engel \u2013", "tokens": ["Hei", "s\u00fclwst", ",", "hei", "heit", "nich", "Krisc\u00b7han", "En\u00b7gel", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "PTKNEG", "NE", "NE", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Ne! hei heit Ann'meriken D\u00fcrten Rist,", "tokens": ["Ne", "!", "hei", "heit", "Ann'\u00b7me\u00b7ri\u00b7ken", "D\u00fcr\u00b7ten", "Rist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "NE", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Un't is ok keinen Sniderbengel \u2013", "tokens": ["Un't", "is", "ok", "kei\u00b7nen", "Sni\u00b7der\u00b7ben\u00b7gel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.nl", "FM.nl", "FM.nl", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Ne! Herr, 'ne olle Waschfru is't.\u00ab", "tokens": ["Ne", "!", "Herr", ",", "'", "ne", "ol\u00b7le", "Waschfru", "is", "'", "t.", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "abbreviation", "punct"], "pos": ["NE", "$.", "NN", "$,", "$(", "FM", "FM", "FM", "FM", "$(", "NE", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}