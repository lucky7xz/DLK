{"textgrid.poem.46261": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von meinem hochzeittag, freitag den 13. September 1616", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein freien mu\u00df ja wol gedeihen,", "tokens": ["Mein", "frei\u00b7en", "mu\u00df", "ja", "wol", "ge\u00b7dei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "weil mir die himmel, der lieb lohn,", "tokens": ["weil", "mir", "die", "him\u00b7mel", ",", "der", "lieb", "lohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "der tugend und der sch\u00f6nheit kron,", "tokens": ["der", "tu\u00b7gend", "und", "der", "sch\u00f6n\u00b7heit", "kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zu geben, so vil hilf verleihen.", "tokens": ["zu", "ge\u00b7ben", ",", "so", "vil", "hilf", "ver\u00b7lei\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein hochzeit oder ein freitag", "tokens": ["Ein", "hoch\u00b7zeit", "o\u00b7der", "ein", "frei\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "ist gnug ein andres paar zu binden,", "tokens": ["ist", "gnug", "ein", "and\u00b7res", "paar", "zu", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "da sich zu meiner lieb austrag", "tokens": ["da", "sich", "zu", "mei\u00b7ner", "lieb", "aus\u00b7trag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "mu\u00df ein dreifacher freitag finden.", "tokens": ["mu\u00df", "ein", "drei\u00b7fa\u00b7cher", "frei\u00b7tag", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du, Freytag, gabest, nach gebrauch,", "tokens": ["Du", ",", "Frey\u00b7tag", ",", "ga\u00b7best", ",", "nach", "ge\u00b7brauch", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "mir die braut, liebend und geliebet,", "tokens": ["mir", "die", "braut", ",", "lie\u00b7bend", "und", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "VVFIN", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "an einen tag, der freitag auch,", "tokens": ["an", "ei\u00b7nen", "tag", ",", "der", "frei\u00b7tag", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "dadurch ich werd nach wunsch enttr\u00fcbet.", "tokens": ["da\u00b7durch", "ich", "werd", "nach", "wunsch", "ent\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VAFIN", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Also weil meiner lieb und treu,", "tokens": ["Al\u00b7so", "weil", "mei\u00b7ner", "lieb", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJD", "KON", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.14": {"text": "wie immer lang, mich nicht gereuet,", "tokens": ["wie", "im\u00b7mer", "lang", ",", "mich", "nicht", "ge\u00b7reu\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "dreif\u00e4ltiglich dreimal getreu", "tokens": ["drei\u00b7f\u00e4l\u00b7tig\u00b7lich", "drei\u00b7mal", "ge\u00b7treu"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.16": {"text": "werd ich entfreiet und gefreiet.", "tokens": ["werd", "ich", "ent\u00b7frei\u00b7et", "und", "ge\u00b7frei\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}