{"textgrid.poem.66929": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00fchend im Schicksalszwang", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fchend im Schicksalszwang", "tokens": ["Gl\u00fc\u00b7hend", "im", "Schick\u00b7sals\u00b7zwang"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sah ich dich, Deutschland.", "tokens": ["Sah", "ich", "dich", ",", "Deutschland", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie dein Schild geh\u00e4mmert", "tokens": ["Wie", "dein", "Schild", "ge\u00b7h\u00e4m\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Klang mein Lied.", "tokens": ["Klang", "mein", "Lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Mein Herzblut sang", "tokens": ["Mein", "Herz\u00b7blut", "sang"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sieg durch Treue,", "tokens": ["Sieg", "durch", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Sieg durch Treue,", "tokens": ["Sieg", "durch", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Neugeburt deiner Welt,", "tokens": ["Neu\u00b7ge\u00b7burt", "dei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Glutengel\u00e4utert, dir zu.", "tokens": ["Glu\u00b7ten\u00b7ge\u00b7l\u00e4u\u00b7tert", ",", "dir", "zu", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Dunkel im Schicksalsgang", "tokens": ["Dun\u00b7kel", "im", "Schick\u00b7sals\u00b7gang"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Seh ich dich, Deutschland.", "tokens": ["Seh", "ich", "dich", ",", "Deutschland", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie dein Tag erd\u00e4mmert,", "tokens": ["Wie", "dein", "Tag", "er\u00b7d\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "T\u00f6nt mein Lied.", "tokens": ["T\u00f6nt", "mein", "Lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Blut braust Gesang:", "tokens": ["Blut", "braust", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Sieg durch Freiheit,", "tokens": ["Sieg", "durch", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Sieg durch Freiheit!", "tokens": ["Sieg", "durch", "Frei\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Singt ersch\u00fcttert mein Herz,", "tokens": ["Singt", "er\u00b7sch\u00fct\u00b7tert", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Donnernde Walstatt, dir zu.", "tokens": ["Don\u00b7nern\u00b7de", "Wals\u00b7tatt", ",", "dir", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}