{"dta.poem.20339": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n Beschreibung vollkommener  \n Sch\u00f6nheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein haar so k\u00fchnlich trotz der Berenice spricht.", "tokens": ["Ein", "haar", "so", "k\u00fchn\u00b7lich", "trotz", "der", "Be\u00b7re\u00b7ni\u00b7ce", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein mund/ der rosen f\u00fchrt und perlen in sich heget.", "tokens": ["Ein", "mund", "/", "der", "ro\u00b7sen", "f\u00fchrt", "und", "per\u00b7len", "in", "sich", "he\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "VVFIN", "KON", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein z\u00fcnglein/ so ein gifft vor tausend hertzen tr\u00e4get.", "tokens": ["Ein", "z\u00fcn\u00b7glein", "/", "so", "ein", "gifft", "vor", "tau\u00b7send", "hert\u00b7zen", "tr\u00e4\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwo br\u00fcste/ wo rubin durch alabaster bricht.", "tokens": ["Zwo", "br\u00fcs\u00b7te", "/", "wo", "ru\u00b7bin", "durch", "a\u00b7la\u00b7bas\u00b7ter", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PWAV", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein hals/ der schwanen schnee weit weit zur\u00fccke sticht.", "tokens": ["Ein", "hals", "/", "der", "schwa\u00b7nen", "schnee", "weit", "weit", "zu\u00b7r\u00fc\u00b7cke", "sticht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "ADJD", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zwey wangen/ wo die pracht der Flora sich beweget.", "tokens": ["Zwey", "wan\u00b7gen", "/", "wo", "die", "pracht", "der", "Flo\u00b7ra", "sich", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "PWAV", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein blick/ der blitze f\u00fchrt und m\u00e4nner niederleget.", "tokens": ["Ein", "blick", "/", "der", "blit\u00b7ze", "f\u00fchrt", "und", "m\u00e4n\u00b7ner", "nie\u00b7der\u00b7le\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zwey arme/ derer krafft offt leuen hingericht.", "tokens": ["Zwey", "ar\u00b7me", "/", "de\u00b7rer", "krafft", "offt", "leu\u00b7en", "hin\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "$(", "PDS", "NN", "ADV", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein hertz/ aus welchem nichts als mein verderben qvillet.", "tokens": ["Ein", "hertz", "/", "aus", "wel\u00b7chem", "nichts", "als", "mein", "ver\u00b7der\u00b7ben", "qvil\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PIS", "KOKOM", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein wort/ so himmlisch ist/ und mich verdammen kan/", "tokens": ["Ein", "wort", "/", "so", "himm\u00b7lisch", "ist", "/", "und", "mich", "ver\u00b7dam\u00b7men", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJD", "VAFIN", "$(", "KON", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zwey h\u00e4nde/ derer grimm mich in den bann gethan/", "tokens": ["Zwey", "h\u00e4n\u00b7de", "/", "de\u00b7rer", "grimm", "mich", "in", "den", "bann", "ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "PDS", "VVFIN", "PRF", "APPR", "ART", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und durch ein s\u00fcsses gifft die seele selbst umh\u00fcllet.", "tokens": ["Und", "durch", "ein", "s\u00fcs\u00b7ses", "gifft", "die", "see\u00b7le", "selbst", "um\u00b7h\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein zierrath/ wie es scheint/ im paradi\u00df gemacht/", "tokens": ["Ein", "zier\u00b7rath", "/", "wie", "es", "scheint", "/", "im", "pa\u00b7ra\u00b7di\u00df", "ge\u00b7macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "VVFIN", "$(", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hat mich um meinen witz und meine freyheit bracht.", "tokens": ["Hat", "mich", "um", "mei\u00b7nen", "witz", "und", "mei\u00b7ne", "frey\u00b7heit", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}