{"dta.poem.15862": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Gegend k\u00f6nnte mir ganz anspruchlos gefallen,", "tokens": ["Die", "Ge\u00b7gend", "k\u00f6nn\u00b7te", "mir", "ganz", "an\u00b7spruch\u00b7los", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie als \u00fcbersch\u00f6n nicht w\u00e4r verschrien von allen.", "tokens": ["Wenn", "sie", "als", "\u00fc\u00b7bersc\u00b7h\u00f6n", "nicht", "w\u00e4r", "ver\u00b7schri\u00b7en", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "ADJD", "PTKNEG", "VAFIN", "VVPP", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun macht die Augen, was sie suchten und nicht finden,", "tokens": ["Nun", "macht", "die", "Au\u00b7gen", ",", "was", "sie", "such\u00b7ten", "und", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch f\u00fcr das Sch\u00f6ne, das sich wirklich fand, erblinden.", "tokens": ["Auch", "f\u00fcr", "das", "Sch\u00f6\u00b7ne", ",", "das", "sich", "wirk\u00b7lich", "fand", ",", "er\u00b7blin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gern lie\u00df' ich euern Mann das was er werth ist gelten;", "tokens": ["Gern", "lie\u00df'", "ich", "eu\u00b7ern", "Mann", "das", "was", "er", "werth", "ist", "gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "PRELS", "PPER", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihr ihn \u00fcbersch\u00e4tzt, mu\u00df ich ihn leider schelten.", "tokens": ["Weil", "ihr", "ihn", "\u00fc\u00b7bersc\u00b7h\u00e4tzt", ",", "mu\u00df", "ich", "ihn", "lei\u00b7der", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,", "VMFIN", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}