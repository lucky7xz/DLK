{"textgrid.poem.41030": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Im Grase", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich's so wohl im Grase liegt", "tokens": ["Wie", "sich's", "so", "wohl", "im", "Gra\u00b7se", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Kraut und Blumend\u00fcften,", "tokens": ["Bei", "Kraut", "und", "Blu\u00b7men\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn \u00fcber uns ein Vogel fliegt", "tokens": ["Wenn", "\u00fc\u00b7ber", "uns", "ein", "Vo\u00b7gel", "fliegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In goldnen Himmelsl\u00fcften.", "tokens": ["In", "gold\u00b7nen", "Him\u00b7mel\u00b7sl\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da kann man wahrlich denken nicht,", "tokens": ["Da", "kann", "man", "wahr\u00b7lich", "den\u00b7ken", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man bald liegen werde", "tokens": ["Da\u00df", "man", "bald", "lie\u00b7gen", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "VVINF", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tief unten ohne Sang und Licht", "tokens": ["Tief", "un\u00b7ten", "oh\u00b7ne", "Sang", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Wurzeln in der Erde.", "tokens": ["Bei", "Wur\u00b7zeln", "in", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man denkt nur an des Himmels Schein", "tokens": ["Man", "denkt", "nur", "an", "des", "Him\u00b7mels", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und an den Vogel drinnen,", "tokens": ["Und", "an", "den", "Vo\u00b7gel", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denkt: Gott wird wohl so gn\u00e4dig sein,", "tokens": ["Denkt", ":", "Gott", "wird", "wohl", "so", "gn\u00e4\u00b7dig", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "VAFIN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir das auch gewinnen.", "tokens": ["Da\u00df", "wir", "das", "auch", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}