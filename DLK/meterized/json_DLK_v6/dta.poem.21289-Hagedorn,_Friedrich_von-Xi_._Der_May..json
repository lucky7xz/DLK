{"dta.poem.21289": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Xi .  \n  Der May.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Nachtigall reizende Lieder", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", "rei\u00b7zen\u00b7de", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ert\u00f6nen und locken schon wieder", "tokens": ["Er\u00b7t\u00f6\u00b7nen", "und", "lo\u00b7cken", "schon", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die fr\u00f6hlichsten Stunden ins Jahr.", "tokens": ["Die", "fr\u00f6h\u00b7lichs\u00b7ten", "Stun\u00b7den", "ins", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Nun singet die steigende Lerche,", "tokens": ["Nun", "sin\u00b7get", "die", "stei\u00b7gen\u00b7de", "Ler\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Nun klappern die reisenden St\u00f6rche,", "tokens": ["Nun", "klap\u00b7pern", "die", "rei\u00b7sen\u00b7den", "St\u00f6r\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Nun schwatzet der gaukelnde Staar.", "tokens": ["Nun", "schwat\u00b7zet", "der", "gau\u00b7keln\u00b7de", "Staar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Wie munter sind Sch\u00e4fer und Herde!", "tokens": ["Wie", "mun\u00b7ter", "sind", "Sch\u00e4\u00b7fer", "und", "Her\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NE", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie lieblich bebl\u00fcmt sich die Erde!", "tokens": ["Wie", "lieb\u00b7lich", "be\u00b7bl\u00fcmt", "sich", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie lebhaft ist itzo die Welt!", "tokens": ["Wie", "leb\u00b7haft", "ist", "it\u00b7zo", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tauben verdoppeln die K\u00fcsse,", "tokens": ["Die", "Tau\u00b7ben", "ver\u00b7dop\u00b7peln", "die", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Entrich besuchet die Fl\u00fcsse,", "tokens": ["Der", "Ent\u00b7rich", "be\u00b7su\u00b7chet", "die", "Fl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der lustige Sperling sein Feld.", "tokens": ["Der", "lus\u00b7ti\u00b7ge", "Sper\u00b7ling", "sein", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wie gleichet doch Zephyr der Floren!", "tokens": ["Wie", "glei\u00b7chet", "doch", "Ze\u00b7phyr", "der", "Flo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie haben sich weislich erkohren,", "tokens": ["Sie", "ha\u00b7ben", "sich", "weis\u00b7lich", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie w\u00e4hlen den Wechsel zur Pflicht.", "tokens": ["Sie", "w\u00e4h\u00b7len", "den", "Wech\u00b7sel", "zur", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Er flattert um Sprossen und Garben;", "tokens": ["Er", "flat\u00b7tert", "um", "Spros\u00b7sen", "und", "Gar\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie liebet unz\u00e4hlige Farben;", "tokens": ["Sie", "lie\u00b7bet", "un\u00b7z\u00e4h\u00b7li\u00b7ge", "Far\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und Eifersucht trennet sie nicht.", "tokens": ["Und", "Ei\u00b7fer\u00b7sucht", "tren\u00b7net", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Nun heben sich Binsen und Keime,", "tokens": ["Nun", "he\u00b7ben", "sich", "Bin\u00b7sen", "und", "Kei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nun kleiden die Bl\u00e4tter die B\u00e4ume,", "tokens": ["Nun", "klei\u00b7den", "die", "Bl\u00e4t\u00b7ter", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nun schwindet des Winters Gestalt;", "tokens": ["Nun", "schwin\u00b7det", "des", "Win\u00b7ters", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Nun rauschen lebendige Quellen", "tokens": ["Nun", "rau\u00b7schen", "le\u00b7ben\u00b7di\u00b7ge", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und tr\u00e4nken mit spielenden Wellen", "tokens": ["Und", "tr\u00e4n\u00b7ken", "mit", "spie\u00b7len\u00b7den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Triften, den Anger, den Wald.", "tokens": ["Die", "Trif\u00b7ten", ",", "den", "An\u00b7ger", ",", "den", "Wald", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Wie buhlerisch, wie so gelinde", "tokens": ["Wie", "buh\u00b7le\u00b7risch", ",", "wie", "so", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erw\u00e4rmen die westlichen Winde", "tokens": ["Er\u00b7w\u00e4r\u00b7men", "die", "west\u00b7li\u00b7chen", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das Ufer, den H\u00fcgel, die Gruft!", "tokens": ["Das", "U\u00b7fer", ",", "den", "H\u00fc\u00b7gel", ",", "die", "Gruft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die jugendlich scherzende Liebe", "tokens": ["Die", "ju\u00b7gend\u00b7lich", "scher\u00b7zen\u00b7de", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Empfindet die Reizung der Triebe,", "tokens": ["Emp\u00b7fin\u00b7det", "die", "Rei\u00b7zung", "der", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Empfindet die schmeichelnde Luft.", "tokens": ["Emp\u00b7fin\u00b7det", "die", "schmei\u00b7cheln\u00b7de", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Nun stellt sich die Dorfschaft in Reihen,", "tokens": ["Nun", "stellt", "sich", "die", "Dorf\u00b7schaft", "in", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nun rufen euch eure Schallmeyen,", "tokens": ["Nun", "ru\u00b7fen", "euch", "eu\u00b7re", "Schall\u00b7me\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jhr stampfenden T\u00e4nzer, hervor.", "tokens": ["Ihr", "stamp\u00b7fen\u00b7den", "T\u00e4n\u00b7zer", ",", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Jhr springet auf gr\u00fcnender Wiese,", "tokens": ["Ihr", "sprin\u00b7get", "auf", "gr\u00fc\u00b7nen\u00b7der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Bauerknecht hebet die Liese,", "tokens": ["Der", "Bau\u00b7er\u00b7knecht", "he\u00b7bet", "die", "Lie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In hurtiger Wendung, empor.", "tokens": ["In", "hur\u00b7ti\u00b7ger", "Wen\u00b7dung", ",", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Nicht fr\u00f6hlicher, weidlicher, k\u00fchner", "tokens": ["Nicht", "fr\u00f6h\u00b7li\u00b7cher", ",", "weid\u00b7li\u00b7cher", ",", "k\u00fch\u00b7ner"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PTKNEG", "ADJD", "$,", "ADJA", "$,", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Schwang vormals der braune Sabiner", "tokens": ["Schwang", "vor\u00b7mals", "der", "brau\u00b7ne", "Sa\u00b7bi\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mit m\u00e4nnlicher Freyheit den Hut.", "tokens": ["Mit", "m\u00e4nn\u00b7li\u00b7cher", "Frey\u00b7heit", "den", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "O reizet die St\u00e4dte zum Neide,", "tokens": ["O", "rei\u00b7zet", "die", "St\u00e4d\u00b7te", "zum", "Nei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Jhr D\u00f6rfer voll h\u00fcpfender Freude!", "tokens": ["Ihr", "D\u00f6r\u00b7fer", "voll", "h\u00fcp\u00b7fen\u00b7der", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Was gleichet dem Land-Volk an Muth?", "tokens": ["Was", "glei\u00b7chet", "dem", "Lan\u00b7dVolk", "an", "Muth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}