{"dta.poem.4382": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Z\u00fcgel des Hochmuhts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oft glaubt der Mensch, da\u00df die Natur und er, zwey", "tokens": ["Oft", "glaubt", "der", "Mensch", ",", "da\u00df", "die", "Na\u00b7tur", "und", "er", ",", "zwey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "KON", "PPER", "$,", "CARD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "eigne Wesen seyn,", "tokens": ["eig\u00b7ne", "We\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die, wo nicht gleich, doch wenigstens an Geist und Kr\u00e4f-", "tokens": ["Die", ",", "wo", "nicht", "gleich", ",", "doch", "we\u00b7nigs\u00b7tens", "an", "Geist", "und", "Kr\u00e4f"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "PTKNEG", "ADV", "$,", "ADV", "ADV", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ten \u00fcberein,", "tokens": ["ten", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und mehrentheils zusammenstimmen. Doch nein! zuwei-", "tokens": ["Und", "meh\u00b7ren\u00b7theils", "zu\u00b7sam\u00b7mens\u00b7tim\u00b7men", ".", "Doch", "nein", "!", "zu\u00b7wei"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVINF", "$.", "KON", "PTKANT", "$.", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "len glaubt er kaum,", "tokens": ["len", "glaubt", "er", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df er nicht weit noch \u00fcber sie mit seines Geistes Kr\u00e4ften", "tokens": ["Da\u00df", "er", "nicht", "weit", "noch", "\u00fc\u00b7ber", "sie", "mit", "sei\u00b7nes", "Geis\u00b7tes", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "gehe,", "tokens": ["ge\u00b7he", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und da\u00df sie (wie verschiedne Vorw\u00fcrf\u2019) ihm g\u00e4nzlich", "tokens": ["Und", "da\u00df", "sie", "(", "wie", "ver\u00b7schied\u00b7ne", "Vor\u00b7w\u00fcr\u00b7f'", ")", "ihm", "g\u00e4nz\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "$(", "KOKOM", "ADJA", "NN", "$(", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "zu Gebohte stehe,", "tokens": ["zu", "Ge\u00b7boh\u00b7te", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Er l\u00e4\u00dft, in der geschwollnen Meynung, ihr kaum an der", "tokens": ["Er", "l\u00e4\u00dft", ",", "in", "der", "ge\u00b7schwoll\u00b7nen", "Mey\u00b7nung", ",", "ihr", "kaum", "an", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Regierung Raum.", "tokens": ["Re\u00b7gie\u00b7rung", "Raum", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Allein, wenn wir die Ordnung, Wirkung und ihre Hand-", "tokens": ["Al\u00b7lein", ",", "wenn", "wir", "die", "Ord\u00b7nung", ",", "Wir\u00b7kung", "und", "ih\u00b7re", "Han\u00b7d"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NN", "$,", "NN", "KON", "PPOSAT", "TRUNC"], "meter": "---+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "lung \u00fcberlegen,", "tokens": ["lung", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Den b\u00fcndigen Zusammenhang, ihr Thun, das nimmer", "tokens": ["Den", "b\u00fcn\u00b7di\u00b7gen", "Zu\u00b7sam\u00b7men\u00b7hang", ",", "ihr", "Thun", ",", "das", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "fehlt, erwegen;", "tokens": ["fehlt", ",", "er\u00b7we\u00b7gen", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "So kommt der Mensch mit allem Wissen, und allen seinen", "tokens": ["So", "kommt", "der", "Mensch", "mit", "al\u00b7lem", "Wis\u00b7sen", ",", "und", "al\u00b7len", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIS", "NN", "$,", "KON", "PIAT", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Werken mir", "tokens": ["Wer\u00b7ken", "mir"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Nur als ein Werkzeug, das von GOtt f\u00fcr die Natur erle-", "tokens": ["Nur", "als", "ein", "Werk\u00b7zeug", ",", "das", "von", "Gott", "f\u00fcr", "die", "Na\u00b7tur", "er\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "$,", "PRELS", "APPR", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.8": {"text": "sen, f\u00fcr,", "tokens": ["sen", ",", "f\u00fcr", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Das sie zu ihrem Zweck gebraucht, das grosse Ganze zu", "tokens": ["Das", "sie", "zu", "ih\u00b7rem", "Zweck", "ge\u00b7braucht", ",", "das", "gros\u00b7se", "Gan\u00b7ze", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "erhalten", "tokens": ["er\u00b7hal\u00b7ten"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "In der ihr von der ew\u2019gen Weisheit gegebnen Ordnung,", "tokens": ["In", "der", "ihr", "von", "der", "ew'\u00b7gen", "Weis\u00b7heit", "ge\u00b7geb\u00b7nen", "Ord\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-----+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Maa\u00df und Ziel.", "tokens": ["Maa\u00df", "und", "Ziel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "So fahr denn fort, o kluges Werkzeug, in deinem hie-", "tokens": ["So", "fahr", "denn", "fort", ",", "o", "klu\u00b7ges", "Werk\u00b7zeug", ",", "in", "dei\u00b7nem", "hie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "sigen Verwalten,", "tokens": ["si\u00b7gen", "Ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So viel, als dir geb\u00fchret, von dir zu halten, aber nicht", "tokens": ["So", "viel", ",", "als", "dir", "ge\u00b7b\u00fch\u00b7ret", ",", "von", "dir", "zu", "hal\u00b7ten", ",", "a\u00b7ber", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,", "ADV", "PTKNEG"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "zu viel!", "tokens": ["zu", "viel", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKA", "PIS", "$."], "meter": "-+", "measure": "iambic.single"}}}}}