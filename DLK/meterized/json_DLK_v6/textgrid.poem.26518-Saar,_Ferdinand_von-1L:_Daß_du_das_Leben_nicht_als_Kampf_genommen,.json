{"textgrid.poem.26518": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df du das Leben nicht als Kampf genommen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du das Leben nicht als Kampf genommen,", "tokens": ["Da\u00df", "du", "das", "Le\u00b7ben", "nicht", "als", "Kampf", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur als der Kr\u00e4fte holdes Wechselspiel;", "tokens": ["Nur", "als", "der", "Kr\u00e4f\u00b7te", "hol\u00b7des", "Wech\u00b7sel\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du, in heil'gen Gluthen still entglommen,", "tokens": ["Da\u00df", "du", ",", "in", "heil'\u00b7gen", "Glu\u00b7then", "still", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur sanften Fluges wolltest an das Ziel;", "tokens": ["Nur", "sanf\u00b7ten", "Flu\u00b7ges", "woll\u00b7test", "an", "das", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df du, nach Hohem strebend, dich verblutet", "tokens": ["Da\u00df", "du", ",", "nach", "Ho\u00b7hem", "stre\u00b7bend", ",", "dich", "ver\u00b7blu\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "NE", "VVPP", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An kleinen Sorgen, die du nie bezwangst,", "tokens": ["An", "klei\u00b7nen", "Sor\u00b7gen", ",", "die", "du", "nie", "be\u00b7zwangst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und so, im Innersten von Licht durchfluthet,", "tokens": ["Und", "so", ",", "im", "In\u00b7ners\u00b7ten", "von", "Licht", "durch\u00b7flut\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit dunklen M\u00e4chten stets vergebens rangst;", "tokens": ["Mit", "dunk\u00b7len", "M\u00e4ch\u00b7ten", "stets", "ver\u00b7ge\u00b7bens", "rangst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df du selbst Jenen, die dich da entweihen,", "tokens": ["Da\u00df", "du", "selbst", "Je\u00b7nen", ",", "die", "dich", "da", "ent\u00b7wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDS", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In G\u00fcte dich geopfert und Geduld \u2013", "tokens": ["In", "G\u00fc\u00b7te", "dich", "ge\u00b7o\u00b7pfert", "und", "Ge\u00b7duld", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "KON", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df du nicht hassen konntest, nur verzeihen:", "tokens": ["Da\u00df", "du", "nicht", "has\u00b7sen", "konn\u00b7test", ",", "nur", "ver\u00b7zei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das war dein Schicksal, Herz \u2013 und deine Schuld!", "tokens": ["Das", "war", "dein", "Schick\u00b7sal", ",", "Herz", "\u2013", "und", "dei\u00b7ne", "Schuld", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "NN", "$(", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}