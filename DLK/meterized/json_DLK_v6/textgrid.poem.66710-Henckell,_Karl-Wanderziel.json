{"textgrid.poem.66710": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wanderziel", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist das Gl\u00fcck? Im Wechsel ein Sichfinden,", "tokens": ["Was", "ist", "das", "Gl\u00fcck", "?", "Im", "Wech\u00b7sel", "ein", "Sich\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Sichverlieren Immerwiederkehr", "tokens": ["Im", "Sich\u00b7ver\u00b7lie\u00b7ren", "Im\u00b7mer\u00b7wie\u00b7der\u00b7kehr"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Mittelpunkt, wo sich die Kr\u00e4fte binden,", "tokens": ["Zum", "Mit\u00b7tel\u00b7punkt", ",", "wo", "sich", "die", "Kr\u00e4f\u00b7te", "bin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die r\u00e4tselhaft sich tauschen kreuz und quer.", "tokens": ["Die", "r\u00e4t\u00b7sel\u00b7haft", "sich", "tau\u00b7schen", "kreuz", "und", "quer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie wandelt unabl\u00e4ssig das Empfinden", "tokens": ["Wie", "wan\u00b7delt", "un\u00b7ab\u00b7l\u00e4s\u00b7sig", "das", "Emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Welt sich drinnen! Bald gewitterschwer,", "tokens": ["Der", "Welt", "sich", "drin\u00b7nen", "!", "Bald", "ge\u00b7wit\u00b7ter\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "$.", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald wie Fanfaren goldnen Lichts. Entschwinden", "tokens": ["Bald", "wie", "Fan\u00b7fa\u00b7ren", "gold\u00b7nen", "Lichts", ".", "Ent\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "KOKOM", "NN", "ADJA", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Kommen ist wie Ebb' und Flut im Meer.", "tokens": ["Und", "Kom\u00b7men", "ist", "wie", "Ebb'", "und", "Flut", "im", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "KOKOM", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Eh ich's begriff mit seinem gro\u00dfen Schwanken,", "tokens": ["Eh", "ich's", "be\u00b7griff", "mit", "sei\u00b7nem", "gro\u00b7\u00dfen", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00dft' ich vor Zweifel an mir selbst erkranken,", "tokens": ["Mu\u00dft'", "ich", "vor", "Zwei\u00b7fel", "an", "mir", "selbst", "er\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis ich des Ausgleichs holde Macht erfuhr", "tokens": ["Bis", "ich", "des", "Aus\u00b7gleichs", "hol\u00b7de", "Macht", "er\u00b7fuhr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und fand, ein Wandrer durch die fernsten Gaue,", "tokens": ["Und", "fand", ",", "ein", "Wand\u00b7rer", "durch", "die", "ferns\u00b7ten", "Gau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die feste Wartburg, der ich tief vertraue,", "tokens": ["Die", "fes\u00b7te", "Wart\u00b7burg", ",", "der", "ich", "tief", "ver\u00b7trau\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den sichern Turm der eigenen Natur.", "tokens": ["Den", "si\u00b7chern", "Turm", "der", "ei\u00b7ge\u00b7nen", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}