{"textgrid.poem.49182": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "5. An eine Jungfrau", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umb alles Gut unnd Geldt in diesem gantzen Lande", "tokens": ["Umb", "al\u00b7les", "Gut", "unnd", "Geldt", "in", "die\u00b7sem", "gant\u00b7zen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "KON", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erzehl' ich weder euch noch andern was zu Schande", "tokens": ["Er\u00b7zehl'", "ich", "we\u00b7der", "euch", "noch", "an\u00b7dern", "was", "zu", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "PPER", "ADV", "ADJA", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wei\u00df gewi\u00dflich auch, da\u00df niemand sprechen kan,", "tokens": ["Und", "wei\u00df", "ge\u00b7wi\u00df\u00b7lich", "auch", ",", "da\u00df", "nie\u00b7mand", "spre\u00b7chen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich hab au\u00df Feindschafft ihm was Leides angethan.", "tokens": ["Ich", "hab", "au\u00df", "Feind\u00b7schafft", "ihm", "was", "Lei\u00b7des", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPER", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr m\u00f6get aber doch darneben k\u00fchnlich gleuben,", "tokens": ["Ihr", "m\u00f6\u00b7get", "a\u00b7ber", "doch", "dar\u00b7ne\u00b7ben", "k\u00fchn\u00b7lich", "gleu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PAV", "ADJD", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df ich ohn euch, Gott Lob, wol werd' im Leben bleiben,", "tokens": ["Da\u00df", "ich", "ohn", "euch", ",", "Gott", "Lob", ",", "wol", "werd'", "im", "Le\u00b7ben", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$,", "NN", "NN", "$,", "ADV", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wil derenthalben auch mich nimmer unterstehn", "tokens": ["Wil", "de\u00b7rent\u00b7hal\u00b7ben", "auch", "mich", "nim\u00b7mer", "un\u00b7ter\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PAV", "ADV", "PPER", "ADV", "VVINF"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Von wegen eurer Gunst mit L\u00fcgen umbzugehn.", "tokens": ["Von", "we\u00b7gen", "eu\u00b7rer", "Gunst", "mit", "L\u00fc\u00b7gen", "um\u00b7bzu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+--++", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Di\u00df alles la\u00df ich euch die Hofeleut' erzeigen,", "tokens": ["Di\u00df", "al\u00b7les", "la\u00df", "ich", "euch", "die", "Ho\u00b7fe\u00b7leut'", "er\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVIMP", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die pr\u00e4chtig Berg hinan mit Reden k\u00f6nnen steigen", "tokens": ["Die", "pr\u00e4ch\u00b7tig", "Berg", "hi\u00b7nan", "mit", "Re\u00b7den", "k\u00f6n\u00b7nen", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ADV", "APPR", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und jedes Wort auffziehn nicht ohne grossen Schein,", "tokens": ["Und", "je\u00b7des", "Wort", "auff\u00b7ziehn", "nicht", "oh\u00b7ne", "gros\u00b7sen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVIZU", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auff da\u00df sie so bey euch in Gnaden m\u00f6gen seyn.", "tokens": ["Auff", "da\u00df", "sie", "so", "bey", "euch", "in", "Gna\u00b7den", "m\u00f6\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "APPR", "PPER", "APPR", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Sie thun wol einen Eyd, nicht dennoch ohne Lachen,", "tokens": ["Sie", "thun", "wol", "ei\u00b7nen", "Eyd", ",", "nicht", "den\u00b7noch", "oh\u00b7ne", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df euer' Augen auch die Sternen finster machen", "tokens": ["Da\u00df", "eu\u00b7er'", "Au\u00b7gen", "auch", "die", "Ster\u00b7nen", "fins\u00b7ter", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und da\u00df sie heller sind denn alles Firmament,", "tokens": ["Und", "da\u00df", "sie", "hel\u00b7ler", "sind", "denn", "al\u00b7les", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ja, da\u00df die Sonne selbst auch nicht so hefftig brent.", "tokens": ["Ja", ",", "da\u00df", "die", "Son\u00b7ne", "selbst", "auch", "nicht", "so", "heff\u00b7tig", "brent", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie schweren hoch und sehr, da\u00df Gott euch au\u00dferlesen", "tokens": ["Sie", "schwe\u00b7ren", "hoch", "und", "sehr", ",", "da\u00df", "Gott", "euch", "au\u00b7\u00dfer\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADV", "$,", "KOUS", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vor aller Zierligkeit und allen sch\u00f6nen Wesen,", "tokens": ["Vor", "al\u00b7ler", "Zier\u00b7lig\u00b7keit", "und", "al\u00b7len", "sch\u00f6\u00b7nen", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und sagen, selig sey das Jahr und denn die Zeit,", "tokens": ["Und", "sa\u00b7gen", ",", "se\u00b7lig", "sey", "das", "Jahr", "und", "denn", "die", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ADJD", "VAFIN", "ART", "NN", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In der ihr grosse Ziehr der Welt geboren seydt.", "tokens": ["In", "der", "ihr", "gros\u00b7se", "Ziehr", "der", "Welt", "ge\u00b7bo\u00b7ren", "seydt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie sprechen wol darbey, da\u00df ihr mit euren Blicken", "tokens": ["Sie", "spre\u00b7chen", "wol", "dar\u00b7bey", ",", "da\u00df", "ihr", "mit", "eu\u00b7ren", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein h\u00e4rter Hertz als Stein verm\u00f6get zu entz\u00fccken,", "tokens": ["Ein", "h\u00e4r\u00b7ter", "Hertz", "als", "Stein", "ver\u00b7m\u00f6\u00b7get", "zu", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df au\u00df America die beste Specerey", "tokens": ["Da\u00df", "au\u00df", "A\u00b7me\u00b7ri\u00b7ca", "die", "bes\u00b7te", "Spe\u00b7ce\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Mit eurem Athem weit nicht zu vergleichen sey,", "tokens": ["Mit", "eu\u00b7rem", "A\u00b7them", "weit", "nicht", "zu", "ver\u00b7glei\u00b7chen", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da\u00df solche H\u00e4nde nicht gemahlet werden k\u00f6ndten,", "tokens": ["Da\u00df", "sol\u00b7che", "H\u00e4n\u00b7de", "nicht", "ge\u00b7mah\u00b7let", "wer\u00b7den", "k\u00f6nd\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PTKNEG", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df gegen ihnen Schnee zu gleichen sey der Tinten,", "tokens": ["Da\u00df", "ge\u00b7gen", "ih\u00b7nen", "Schnee", "zu", "glei\u00b7chen", "sey", "der", "Tin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "NN", "PTKZU", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df jedes Z\u00e4hnlein sey ein k\u00f6stlicher Demant,", "tokens": ["Da\u00df", "je\u00b7des", "Z\u00e4hn\u00b7lein", "sey", "ein", "k\u00f6st\u00b7li\u00b7cher", "De\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "An welches die Natur all' ihre Kunst gewandt,", "tokens": ["An", "wel\u00b7ches", "die", "Na\u00b7tur", "all'", "ih\u00b7re", "Kunst", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PIAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und da\u00df die Lippen auch, so mehr als Rosen bl\u00fchen,", "tokens": ["Und", "da\u00df", "die", "Lip\u00b7pen", "auch", ",", "so", "mehr", "als", "Ro\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "$,", "ADV", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Weit, weit den edelsten Corallen vorz\u00fcziehen:", "tokens": ["Weit", ",", "weit", "den", "e\u00b7dels\u00b7ten", "Co\u00b7ral\u00b7len", "vor\u00b7z\u00fc\u00b7zie\u00b7hen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df Haar (ich glaube nicht, da\u00df es von Hertzen k\u00f6mpt)", "tokens": ["Da\u00df", "Haar", "(", "ich", "glau\u00b7be", "nicht", ",", "da\u00df", "es", "von", "Hert\u00b7zen", "k\u00f6mpt", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ein jeglicher vor Gold und beste Perlen nimpt.", "tokens": ["Ein", "jeg\u00b7li\u00b7cher", "vor", "Gold", "und", "bes\u00b7te", "Per\u00b7len", "nimpt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie setzen wohl hinzu, wenn sie euch reden h\u00f6ren,", "tokens": ["Sie", "set\u00b7zen", "wohl", "hin\u00b7zu", ",", "wenn", "sie", "euch", "re\u00b7den", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Da\u00df auch ein jedes Wort starck sey, sie zu versehren,", "tokens": ["Da\u00df", "auch", "ein", "je\u00b7des", "Wort", "starck", "sey", ",", "sie", "zu", "ver\u00b7seh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "PIAT", "NN", "ADJD", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und da\u00df der starcke Mars durch eurer Zungen Schein", "tokens": ["Und", "da\u00df", "der", "star\u00b7cke", "Mars", "durch", "eu\u00b7rer", "Zun\u00b7gen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die Waffen abzuthun bereitet w\u00fcrde seyn.", "tokens": ["Die", "Waf\u00b7fen", "ab\u00b7zu\u00b7thun", "be\u00b7rei\u00b7tet", "w\u00fcr\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "VVPP", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Geliebet euch hernach von Venus was zu singen,", "tokens": ["Ge\u00b7lie\u00b7bet", "euch", "her\u00b7nach", "von", "Ve\u00b7nus", "was", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Winde k\u00f6nnet ihr mit eurer Stimme zwingen,", "tokens": ["Die", "Win\u00b7de", "k\u00f6n\u00b7net", "ihr", "mit", "eu\u00b7rer", "Stim\u00b7me", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und wenn ihr weiter auch euch zu der Lauten findt,", "tokens": ["Und", "wenn", "ihr", "wei\u00b7ter", "auch", "euch", "zu", "der", "Lau\u00b7ten", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ist Orpheus ungelehrt und gegen euch ein Kindt.", "tokens": ["Ist", "Or\u00b7pheus", "un\u00b7ge\u00b7lehrt", "und", "ge\u00b7gen", "euch", "ein", "Kindt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJD", "KON", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wann ihr zu Felde kompt, wohin man euch sieht gehen,", "tokens": ["Wann", "ihr", "zu", "Fel\u00b7de", "kompt", ",", "wo\u00b7hin", "man", "euch", "sieht", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "