{"textgrid.poem.63718": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Im S\u00fcden", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist die Luft so reingestimmt,", "tokens": ["Ist", "die", "Luft", "so", "rein\u00b7ge\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeden Mi\u00dfklang zu vers\u00f6hnen?", "tokens": ["Je\u00b7den", "Mi\u00df\u00b7klang", "zu", "ver\u00b7s\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will doch alles lieblich t\u00f6nen,", "tokens": ["Will", "doch", "al\u00b7les", "lieb\u00b7lich", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mein lauschend Ohr vernimmt.", "tokens": ["Was", "mein", "lau\u00b7schend", "Ohr", "ver\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich, um den die Fessel schlang", "tokens": ["Ich", ",", "um", "den", "die", "Fes\u00b7sel", "schlang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUI", "ART", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Leidenschaft, \u2013 an dieser St\u00e4tte", "tokens": ["Lei\u00b7den\u00b7schaft", ",", "\u2013", "an", "die\u00b7ser", "St\u00e4t\u00b7te"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$,", "$(", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klirr' ich nur mit meiner Kette,", "tokens": ["Klirr'", "ich", "nur", "mit", "mei\u00b7ner", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und schon klingt es wie Gesang.", "tokens": ["Und", "schon", "klingt", "es", "wie", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}