{"textgrid.poem.37097": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Daphne", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Birg die schmachtenden Augen,", "tokens": ["Birg", "die", "schmach\u00b7ten\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wo die G\u00f6tter der Liebe", "tokens": ["Wo", "die", "G\u00f6t\u00b7ter", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ihre Pfeile vergolden,", "tokens": ["Ih\u00b7re", "Pfei\u00b7le", "ver\u00b7gol\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Birg die schmachtenden Augen mir!", "tokens": ["Birg", "die", "schmach\u00b7ten\u00b7den", "Au\u00b7gen", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Birg den bebenden Busen,", "tokens": ["Birg", "den", "be\u00b7ben\u00b7den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wo die G\u00f6tter der Liebe", "tokens": ["Wo", "die", "G\u00f6t\u00b7ter", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auf den H\u00fcgeln sich betten,", "tokens": ["Auf", "den", "H\u00fc\u00b7geln", "sich", "bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Birg den bebenden Busen mir!", "tokens": ["Birg", "den", "be\u00b7ben\u00b7den", "Bu\u00b7sen", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Birg die purpurne Wange!", "tokens": ["Birg", "die", "pur\u00b7pur\u00b7ne", "Wan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sonst ersink ich dem Taumel,", "tokens": ["Sonst", "er\u00b7sink", "ich", "dem", "Tau\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und zerk\u00fc\u00dfe den Busen,", "tokens": ["Und", "zer\u00b7k\u00fc\u00b7\u00dfe", "den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die purpurne Wange dir!", "tokens": ["Und", "die", "pur\u00b7pur\u00b7ne", "Wan\u00b7ge", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "$."], "meter": "--++-+-+", "measure": "anapaest.init"}}}}}