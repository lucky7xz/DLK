{"textgrid.poem.49228": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Fackeln dieser Welt, ihr gro\u00dfes Wolkenfeuer,", "tokens": ["Ihr", "Fa\u00b7ckeln", "die\u00b7ser", "Welt", ",", "ihr", "gro\u00b7\u00dfes", "Wol\u00b7ken\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Lichter in der Luft, ihr Himmels\u00e4ugelein,", "tokens": ["Ihr", "Lich\u00b7ter", "in", "der", "Luft", ",", "ihr", "Him\u00b7mel\u00b7s\u00e4u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fchrt mich zur Liebsten hin; komt ihr mir nicht zu Steuer,", "tokens": ["F\u00fchrt", "mich", "zur", "Liebs\u00b7ten", "hin", ";", "komt", "ihr", "mir", "nicht", "zu", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$.", "VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So wird mein brennend Herz' anstatt der Sternen sein.", "tokens": ["So", "wird", "mein", "bren\u00b7nend", "Her\u00b7z'", "an\u00b7statt", "der", "Ster\u00b7nen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}