{"dta.poem.15917": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "122.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es \u00e4rgerte mich wol, da\u00df von den braunen Haaren", "tokens": ["Es", "\u00e4r\u00b7ger\u00b7te", "mich", "wol", ",", "da\u00df", "von", "den", "brau\u00b7nen", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So viel seit ein'ger Zeit mir grau geworden waren.", "tokens": ["So", "viel", "seit", "ein'\u00b7ger", "Zeit", "mir", "grau", "ge\u00b7wor\u00b7den", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "PPER", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun aber freu' ich mich, da\u00df bei den grauen doch", "tokens": ["Nun", "a\u00b7ber", "freu'", "ich", "mich", ",", "da\u00df", "bei", "den", "grau\u00b7en", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "APPR", "ART", "ADJA", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So viele braune sind geblieben immer noch.", "tokens": ["So", "vie\u00b7le", "brau\u00b7ne", "sind", "ge\u00b7blie\u00b7ben", "im\u00b7mer", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "VAFIN", "VVPP", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wann die grauen nun die braunen \u00fcberwiegen,", "tokens": ["Und", "wann", "die", "grau\u00b7en", "nun", "die", "brau\u00b7nen", "\u00fc\u00b7berw\u00b7ie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird es mich endlich freun, ein reines Grau zu kriegen.", "tokens": ["Wird", "es", "mich", "end\u00b7lich", "freun", ",", "ein", "rei\u00b7nes", "Grau", "zu", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVINF", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Als Knabe betet' ich, und jetzo werd' es wahr:", "tokens": ["Als", "Kna\u00b7be", "be\u00b7tet'", "ich", ",", "und", "jet\u00b7zo", "werd'", "es", "wahr", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "$,", "KON", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201egib, da\u00df ich tragen mag mit Ehr'n ein graues Haar!\u201c", "tokens": ["\u201e", "gib", ",", "da\u00df", "ich", "tra\u00b7gen", "mag", "mit", "Ehr'n", "ein", "grau\u00b7es", "Haar", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "APPR", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}