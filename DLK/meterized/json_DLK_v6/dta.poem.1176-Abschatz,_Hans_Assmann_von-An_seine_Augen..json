{"dta.poem.1176": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "An seine Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Augen/ h\u00f6ret auff Silvinden zu beschauen!", "tokens": ["Ihr", "Au\u00b7gen", "/", "h\u00f6\u00b7ret", "auff", "Sil\u00b7vin\u00b7den", "zu", "be\u00b7schau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Hertze/ welches sie kennt besser weder ihr/", "tokens": ["Mein", "Hert\u00b7ze", "/", "wel\u00b7ches", "sie", "kennt", "bes\u00b7ser", "we\u00b7der", "ihr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$(", "PWS", "PPER", "VVFIN", "ADJD", "KON", "PPOSAT", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sagt mir/ da\u00df eure Lust wird sein mein Ungel\u00fccke.", "tokens": ["Sagt", "mir", "/", "da\u00df", "eu\u00b7re", "Lust", "wird", "sein", "mein", "Un\u00b7ge\u00b7l\u00fc\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "VAFIN", "PPOSAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es zwinget die Begier/", "tokens": ["Es", "zwin\u00b7get", "die", "Be\u00b7gier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Halt eure Stralen auch zur\u00fccke/", "tokens": ["Halt", "eu\u00b7re", "Stra\u00b7len", "auch", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und h\u00f6ret auff Silvinden zu beschauen?", "tokens": ["Und", "h\u00f6\u00b7ret", "auff", "Sil\u00b7vin\u00b7den", "zu", "be\u00b7schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr Augen/ eure Blicke", "tokens": ["Ihr", "Au\u00b7gen", "/", "eu\u00b7re", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gerathen in Verdacht:", "tokens": ["Ge\u00b7ra\u00b7then", "in", "Ver\u00b7dacht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nehmt euch f\u00fcr Ungel\u00fccke/", "tokens": ["Nehmt", "euch", "f\u00fcr", "Un\u00b7ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das eure K\u00fcnheit macht/", "tokens": ["Das", "eu\u00b7re", "K\u00fcn\u00b7heit", "macht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hinfort genau in Acht.", "tokens": ["Hin\u00b7fort", "ge\u00b7nau", "in", "Acht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man saget/ da\u00df ihr spielet", "tokens": ["Man", "sa\u00b7get", "/", "da\u00df", "ihr", "spie\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$(", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach der Verliebten Art/", "tokens": ["Nach", "der", "Ver\u00b7lieb\u00b7ten", "Art", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wiewohl ichs nie gef\u00fchlet/", "tokens": ["Wie\u00b7wohl", "ichs", "nie", "ge\u00b7f\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eurer Stralen Fahrt", "tokens": ["Und", "eu\u00b7rer", "Stra\u00b7len", "Fahrt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auff Rosen-Wangen paart.", "tokens": ["Auff", "Ro\u00b7sen\u00b7Wan\u00b7gen", "paart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Entdeckt nicht Unbekandten", "tokens": ["Ent\u00b7deckt", "nicht", "Un\u00b7be\u00b7kand\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ihr itzund allein", "tokens": ["Was", "ihr", "it\u00b7zund", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Solt meiner Amaranthen/", "tokens": ["Solt", "mei\u00b7ner", "A\u00b7ma\u00b7ran\u00b7then", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch dunckler Farben Schein", "tokens": ["Durch", "dunck\u00b7ler", "Far\u00b7ben", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ins Hertze schreiben ein.", "tokens": ["Ins", "Hert\u00b7ze", "schrei\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Lasst sie von ferne wissen/", "tokens": ["Lasst", "sie", "von", "fer\u00b7ne", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was dieser treue Mund/", "tokens": ["Was", "die\u00b7ser", "treu\u00b7e", "Mund", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Fall sie zu bek\u00fcssen", "tokens": ["Im", "Fall", "sie", "zu", "be\u00b7k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm m\u00f6chte seyn vergunt/", "tokens": ["Ihm", "m\u00f6ch\u00b7te", "seyn", "ver\u00b7gunt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr w\u00fcrde machen kund.", "tokens": ["Ihr", "w\u00fcr\u00b7de", "ma\u00b7chen", "kund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bringt mir Bericht zur\u00fccke/", "tokens": ["Bringt", "mir", "Be\u00b7richt", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was zu erwarten sey/", "tokens": ["Was", "zu", "er\u00b7war\u00b7ten", "sey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ob auch ihre Blicke", "tokens": ["Und", "ob", "auch", "ih\u00b7re", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich/ sonder Heucheley/", "tokens": ["Sich", "/", "son\u00b7der", "Heu\u00b7che\u00b7ley", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PRF", "$(", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem Hertzen nahen bey.", "tokens": ["Dem", "Hert\u00b7zen", "na\u00b7hen", "bey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich will mit Willen tragen", "tokens": ["Ich", "will", "mit", "Wil\u00b7len", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die auffgelegte Schuld/", "tokens": ["Die", "auff\u00b7ge\u00b7leg\u00b7te", "Schuld", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht \u00fcber Unrecht klagen/", "tokens": ["Nicht", "\u00fc\u00b7ber", "Un\u00b7recht", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo Amaranthens Huld", "tokens": ["Wo", "A\u00b7ma\u00b7ran\u00b7thens", "Huld"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist meiner Blicke Sold.", "tokens": ["Ist", "mei\u00b7ner", "Bli\u00b7cke", "Sold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}