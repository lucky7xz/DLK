{"dta.poem.19481": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Vor der Ernte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In reiner Nacht die Sichel geht", "tokens": ["In", "rei\u00b7ner", "Nacht", "die", "Si\u00b7chel", "geht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht ein leis Get\u00f6n,", "tokens": ["Und", "macht", "ein", "leis", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im reifen Korne wogt und weht", "tokens": ["Im", "rei\u00b7fen", "Kor\u00b7ne", "wogt", "und", "weht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rauscht und w\u00fchlt der F\u00f6hn.", "tokens": ["Und", "rauscht", "und", "w\u00fchlt", "der", "F\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie wandert voller Melodie", "tokens": ["Sie", "wan\u00b7dert", "vol\u00b7ler", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch\u00fcber durch das Land,", "tokens": ["Hoch\u00b7\u00fc\u00b7ber", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fr\u00fch morgen schwingt die Schnitt'rin sie", "tokens": ["Fr\u00fch", "mor\u00b7gen", "schwingt", "die", "Schnitt'\u00b7rin", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mit sonnenbrauner Hand.", "tokens": ["Mit", "son\u00b7nen\u00b7brau\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}