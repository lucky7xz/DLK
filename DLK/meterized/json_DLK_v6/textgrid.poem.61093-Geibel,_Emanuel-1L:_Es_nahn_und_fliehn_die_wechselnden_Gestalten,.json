{"textgrid.poem.61093": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es nahn und fliehn die wechselnden Gestalten,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es nahn und fliehn die wechselnden Gestalten,", "tokens": ["Es", "nahn", "und", "fliehn", "die", "wech\u00b7seln\u00b7den", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was wir kaum im Herzen liebgewannen,", "tokens": ["Und", "was", "wir", "kaum", "im", "Her\u00b7zen", "lieb\u00b7ge\u00b7wan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Ferne f\u00fchrt es neidisch uns von dannen,", "tokens": ["Die", "Fer\u00b7ne", "f\u00fchrt", "es", "nei\u00b7disch", "uns", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Lauf der Stunden mu\u00df es rasch veralten.", "tokens": ["Im", "Lauf", "der", "Stun\u00b7den", "mu\u00df", "es", "rasch", "ver\u00b7al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da greift der K\u00fcnstler in des Schicksals Walten:", "tokens": ["Da", "greift", "der", "K\u00fcnst\u00b7ler", "in", "des", "Schick\u00b7sals", "Wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Zaubrer, wei\u00df er Raum und Zeit zu bannen,", "tokens": ["Ein", "Zaub\u00b7rer", ",", "wei\u00df", "er", "Raum", "und", "Zeit", "zu", "ban\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er wei\u00df den Augenblick, den wir umspannen,", "tokens": ["Er", "wei\u00df", "den", "Au\u00b7gen\u00b7blick", ",", "den", "wir", "um\u00b7span\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In lichten Farben selig festzuhalten.", "tokens": ["In", "lich\u00b7ten", "Far\u00b7ben", "se\u00b7lig", "fest\u00b7zu\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So hast nun du mit sch\u00f6pfrischem Gem\u00fcte", "tokens": ["So", "hast", "nun", "du", "mit", "sch\u00f6pf\u00b7ri\u00b7schem", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sch\u00f6nste Ros' auf Hellas' sch\u00f6nen Auen", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Ros'", "auf", "Hel\u00b7las'", "sch\u00f6\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dahingebannt in ew'ger Jugendbl\u00fcte.", "tokens": ["Da\u00b7hin\u00b7ge\u00b7bannt", "in", "ew'\u00b7ger", "Ju\u00b7gend\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und staunend wird es noch der Enkel schauen,", "tokens": ["Und", "stau\u00b7nend", "wird", "es", "noch", "der", "En\u00b7kel", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies Angesicht voll Majest\u00e4t und G\u00fcte,", "tokens": ["Dies", "An\u00b7ge\u00b7sicht", "voll", "Ma\u00b7jes\u00b7t\u00e4t", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die K\u00f6nigin der Griechen und der Frauen.", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "der", "Grie\u00b7chen", "und", "der", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}