{"textgrid.poem.38004": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schwimm hin, schwimm her du Ringlein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts sch\u00f6neres kann mich erfreuen,", "tokens": ["Nichts", "sch\u00f6\u00b7ne\u00b7res", "kann", "mich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn es der Sommer angeht,", "tokens": ["Als", "wenn", "es", "der", "Som\u00b7mer", "an\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da bl\u00fchen die Rosen im Mayen,", "tokens": ["Da", "bl\u00fc\u00b7hen", "die", "Ro\u00b7sen", "im", "Ma\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Trompeter die blasen ins Feld.", "tokens": ["Trom\u00b7pe\u00b7ter", "die", "bla\u00b7sen", "ins", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Trompeter die haben's geblasen;", "tokens": ["Trom\u00b7pe\u00b7ter", "die", "ha\u00b7ben's", "ge\u00b7bla\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soldaten marschieren in's Feld,", "tokens": ["Sol\u00b7da\u00b7ten", "mar\u00b7schie\u00b7ren", "in's", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie ziehen dem Feinde entgegen,", "tokens": ["Sie", "zie\u00b7hen", "dem", "Fein\u00b7de", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zum Streite wohl sind sie bestellt.", "tokens": ["Zum", "Strei\u00b7te", "wohl", "sind", "sie", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dort drunten in's Kaisers Schlo\u00dfgarten,", "tokens": ["Dort", "drun\u00b7ten", "in's", "Kai\u00b7sers", "Schlo\u00df\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da stehet ein Feigenbaum,", "tokens": ["Da", "ste\u00b7het", "ein", "Fei\u00b7gen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da m\u00fcssen wir alle ablegen", "tokens": ["Da", "m\u00fcs\u00b7sen", "wir", "al\u00b7le", "ab\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Pistolen und S\u00e4belgezeug.", "tokens": ["Pis\u00b7to\u00b7len", "und", "S\u00e4\u00b7bel\u00b7ge\u00b7zeug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Ach Sch\u00e4tzel was hab ich erfahren,", "tokens": ["Ach", "Sch\u00e4t\u00b7zel", "was", "hab", "ich", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PWS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df du jetzt willst reisen von hier,", "tokens": ["Da\u00df", "du", "jetzt", "willst", "rei\u00b7sen", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "APPR", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Willst reisen in's fremde Land nause,", "tokens": ["Willst", "rei\u00b7sen", "in's", "frem\u00b7de", "Land", "nau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wann kommst du wieder zu mir?", "tokens": ["Wann", "kommst", "du", "wie\u00b7der", "zu", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Und da ich im fremden Land drausen war,", "tokens": ["Und", "da", "ich", "im", "frem\u00b7den", "Land", "drau\u00b7sen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "ADJA", "NN", "ADV", "VAFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gedacht ich gleich wieder nach Haus;", "tokens": ["Ge\u00b7dacht", "ich", "gleich", "wie\u00b7der", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach w\u00e4r ich zu Hause geblieben,", "tokens": ["Ach", "w\u00e4r", "ich", "zu", "Hau\u00b7se", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und h\u00e4tte gehalten mein Wort!", "tokens": ["Und", "h\u00e4t\u00b7te", "ge\u00b7hal\u00b7ten", "mein", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Und als ich wieder nach Hause kam,", "tokens": ["Und", "als", "ich", "wie\u00b7der", "nach", "Hau\u00b7se", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Feins Liebchen stand unter der Th\u00fcr;", "tokens": ["Feins", "Lieb\u00b7chen", "stand", "un\u00b7ter", "der", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Gott gr\u00fc\u00df dich du H\u00fcbsche, du Feine,", "tokens": ["Gott", "gr\u00fc\u00df", "dich", "du", "H\u00fcb\u00b7sche", ",", "du", "Fei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von Herzen gefallest du mir!", "tokens": ["Von", "Her\u00b7zen", "ge\u00b7fal\u00b7lest", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Ich brauche dir nicht zu gefallen,", "tokens": ["Ich", "brau\u00b7che", "dir", "nicht", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe schon l\u00e4ngst einen Mann,", "tokens": ["Ich", "ha\u00b7be", "schon", "l\u00e4ngst", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dazu einen h\u00fcbschen und feinen,", "tokens": ["Da\u00b7zu", "ei\u00b7nen", "h\u00fcb\u00b7schen", "und", "fei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "KON", "ADJA", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der mich wohl ern\u00e4hren kann.", "tokens": ["Der", "mich", "wohl", "er\u00b7n\u00e4h\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Was zog er aus seiner Tasche?", "tokens": ["Was", "zog", "er", "aus", "sei\u00b7ner", "Ta\u00b7sche", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Messer war scharf und war spitz;", "tokens": ["Ein", "Mes\u00b7ser", "war", "scharf", "und", "war", "spitz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VAFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er stach es feins Liebchen ins Herze,", "tokens": ["Er", "stach", "es", "feins", "Lieb\u00b7chen", "ins", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "APPRART", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das rothe Blut gegen ihn spritzt.", "tokens": ["Das", "ro\u00b7the", "Blut", "ge\u00b7gen", "ihn", "spritzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er zog es gleich wieder herause,", "tokens": ["Er", "zog", "es", "gleich", "wie\u00b7der", "her\u00b7au\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Blute da war es so roth,", "tokens": ["Vom", "Blu\u00b7te", "da", "war", "es", "so", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Hast du nun gelitten die Schmerzen,", "tokens": ["Hast", "du", "nun", "ge\u00b7lit\u00b7ten", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "So will ich auch leiden den Tod.", "tokens": ["So", "will", "ich", "auch", "lei\u00b7den", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Da nun das Liebchen gestorben,", "tokens": ["Da", "nun", "das", "Lieb\u00b7chen", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo begrabt man sie denn hin?", "tokens": ["Wo", "be\u00b7grabt", "man", "sie", "denn", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In ihres Vaters Schlo\u00dfgarten,", "tokens": ["In", "ih\u00b7res", "Va\u00b7ters", "Schlo\u00df\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Wo wei\u00dfe Lilien bl\u00fchn.", "tokens": ["Wo", "wei\u00b7\u00dfe", "Li\u00b7li\u00b7en", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Was zog er da von seinem Finger?", "tokens": ["Was", "zog", "er", "da", "von", "sei\u00b7nem", "Fin\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ringlein, das war von Gold,", "tokens": ["Ein", "Rin\u00b7glein", ",", "das", "war", "von", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er warf es sogleich in das Wasser,", "tokens": ["Er", "warf", "es", "sog\u00b7leich", "in", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wellen, die geben den Schein.", "tokens": ["Die", "Wel\u00b7len", ",", "die", "ge\u00b7ben", "den", "Schein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Schwimm hin, schwimm hin, du Ringlein,", "tokens": ["Schwimm", "hin", ",", "schwimm", "hin", ",", "du", "Rin\u00b7glein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schwimm hin in das Meer hinein,", "tokens": ["Schwimm", "hin", "in", "das", "Meer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gr\u00fc\u00df mir mein Vater und Mutter,", "tokens": ["Und", "gr\u00fc\u00df", "mir", "mein", "Va\u00b7ter", "und", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und sag, ich komm nimmermehr heim.", "tokens": ["Und", "sag", ",", "ich", "komm", "nim\u00b7mer\u00b7mehr", "heim", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}