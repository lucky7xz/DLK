{"textgrid.poem.46192": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber den tod Herrn Christians des dapferen herzogen und starken heldens von Braunschweig", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, tod, bist, zweifels ohn, ohn augen und verstand,", "tokens": ["Du", ",", "tod", ",", "bist", ",", "zwei\u00b7fels", "ohn", ",", "ohn", "au\u00b7gen", "und", "ver\u00b7stand", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "$,", "KOUS", "APPR", "$,", "KOUI", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weil den verzagten du, dich fliehend, nicht nachlaufest,", "tokens": ["weil", "den", "ver\u00b7zag\u00b7ten", "du", ",", "dich", "flie\u00b7hend", ",", "nicht", "nach\u00b7lau\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "PPER", "$,", "PPER", "VVPP", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+-+-+++-", "measure": "iambic.septa.invert"}, "line.3": {"text": "weil du die dapfersten am ersten hinweg raufest,", "tokens": ["weil", "du", "die", "dap\u00b7fers\u00b7ten", "am", "ers\u00b7ten", "hin\u00b7weg", "rau\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "APPRART", "ADJA", "APZR", "VVFIN", "$,"], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "darunder dieser held, des teutschen reichs wohlstand;", "tokens": ["da\u00b7run\u00b7der", "die\u00b7ser", "held", ",", "des", "teut\u00b7schen", "reichs", "wohl\u00b7stand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PDS", "VVFIN", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "dan h\u00e4tt ihn dein gesicht und witz nur k\u00f6nnen sehen,", "tokens": ["dan", "h\u00e4tt", "ihn", "dein", "ge\u00b7sicht", "und", "witz", "nur", "k\u00f6n\u00b7nen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "VVPP", "KON", "VVIMP", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "so h\u00e4ttest, sparend ihn, du m\u00fcssen selbs gestehen,", "tokens": ["so", "h\u00e4t\u00b7test", ",", "spa\u00b7rend", "ihn", ",", "du", "m\u00fcs\u00b7sen", "selbs", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ADJD", "PPER", "$,", "PPER", "VMFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da\u00df mehr volkreich dein reich durch sein, dan deine hand.", "tokens": ["da\u00df", "mehr", "vol\u00b7kreich", "dein", "reich", "durch", "sein", ",", "dan", "dei\u00b7ne", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPOSAT", "ADJD", "APPR", "PPOSAT", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}