{"dta.poem.18437": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber Clementis  XI  wegen der Jta-  \n li\u00e4nischen kriegs-troublen vergo\u00df-  \n ne thr\u00e4nen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist nicht Petro nur in dem verl\u00e4ugnen gleich/", "tokens": ["Du", "bist", "nicht", "Pe\u00b7tro", "nur", "in", "dem", "ver\u00b7l\u00e4ug\u00b7nen", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NE", "ADV", "APPR", "ART", "ADJA", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst auch offt wie er an bittern thr\u00e4nen reich.", "tokens": ["Du", "wirst", "auch", "offt", "wie", "er", "an", "bit\u00b7tern", "thr\u00e4\u00b7nen", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KOKOM", "PPER", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wo die \u00e4hnlichkeit soll recht vollkommen scheinen;", "tokens": ["Doch", "wo", "die", "\u00e4hn\u00b7lich\u00b7keit", "soll", "recht", "voll\u00b7kom\u00b7men", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VMFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So must du nicht in Rom und im Palaste weinen;", "tokens": ["So", "must", "du", "nicht", "in", "Rom", "und", "im", "Pa\u00b7las\u00b7te", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "APPR", "NE", "KON", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vieleicht vertreiben noch die krieges-knechte dich/", "tokens": ["Vie\u00b7leicht", "ver\u00b7trei\u00b7ben", "noch", "die", "krie\u00b7ge\u00b7sknech\u00b7te", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sprechen! Geh hinaus und weine bitterlich.", "tokens": ["Und", "spre\u00b7chen", "!", "Geh", "hin\u00b7aus", "und", "wei\u00b7ne", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "NE", "PTKVZ", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}