{"textgrid.poem.46567": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Dieses Kleidchen lilienwei\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieses Kleidchen lilienwei\u00df", "tokens": ["Die\u00b7ses", "Kleid\u00b7chen", "li\u00b7li\u00b7en\u00b7wei\u00df"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Stand der Ros' am sch\u00f6nsten an;", "tokens": ["Stand", "der", "Ros'", "am", "sch\u00f6ns\u00b7ten", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schm\u00fcckte sie sich selbst mit Flei\u00df,", "tokens": ["Schm\u00fcck\u00b7te", "sie", "sich", "selbst", "mit", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hatt' ich meine Freude dran.", "tokens": ["Hatt'", "ich", "mei\u00b7ne", "Freu\u00b7de", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In den Sommertagen hei\u00df", "tokens": ["In", "den", "Som\u00b7mer\u00b7ta\u00b7gen", "hei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War das leichte wohlgethan;", "tokens": ["War", "das", "leich\u00b7te", "wohl\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des kalten Winters Eis", "tokens": ["In", "des", "kal\u00b7ten", "Win\u00b7ters", "Eis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt' ich ihr's nicht legen an.", "tokens": ["Wollt'", "ich", "ih\u00b7r's", "nicht", "le\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich vertr\u00f6stet' ihr Gehei\u00df:", "tokens": ["Ich", "ver\u00b7tr\u00f6s\u00b7tet'", "ihr", "Ge\u00b7hei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sommers wird dir's angethan!", "tokens": ["Som\u00b7mers", "wird", "dir's", "an\u00b7ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun, ohne da\u00df sie's wei\u00df,", "tokens": ["Und", "nun", ",", "oh\u00b7ne", "da\u00df", "sie's", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zieh' ich's ihr im Winter an", "tokens": ["Zieh'", "ich's", "ihr", "im", "Win\u00b7ter", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPOSAT", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meine Rose lilienwei\u00df", "tokens": ["Mei\u00b7ne", "Ro\u00b7se", "li\u00b7li\u00b7en\u00b7wei\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "H\u00fcpft nicht mehr auf gr\u00fcnem Plan;", "tokens": ["H\u00fcpft", "nicht", "mehr", "auf", "gr\u00fc\u00b7nem", "Plan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem bethr\u00e4nten Lilienrei\u00df", "tokens": ["Dem", "be\u00b7thr\u00e4n\u00b7ten", "Li\u00b7li\u00b7en\u00b7rei\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sei der Himmel aufgethan!", "tokens": ["Sei", "der", "Him\u00b7mel", "auf\u00b7ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn er wahr ist, der Verhei\u00df:", "tokens": ["Wenn", "er", "wahr", "ist", ",", "der", "Ver\u00b7hei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Saat in Thr\u00e4nen eingethan", "tokens": ["Saat", "in", "Thr\u00e4\u00b7nen", "ein\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erntet Lust einst; nun ich wei\u00df,", "tokens": ["Ern\u00b7tet", "Lust", "einst", ";", "nun", "ich", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "$.", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich Lust einst ernten kann.", "tokens": ["Da\u00df", "ich", "Lust", "einst", "ern\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}