{"dta.poem.21227": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Die Eroberung der Stadt  \n Regenspurg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sehn am str\u00e4nden her die Tonaw stiller gehn/", "tokens": ["Wir", "sehn", "am", "str\u00e4n\u00b7den", "her", "die", "To\u00b7naw", "stil\u00b7ler", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "APZR", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach ", "tokens": ["Nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ob sie verwundet sind/ die siht man leichter trauren/", "tokens": ["Ob", "sie", "ver\u00b7wun\u00b7det", "sind", "/", "die", "siht", "man", "leich\u00b7ter", "trau\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$(", "ART", "VVFIN", "PIS", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "O Horn/ jhr rechte Faust der letzten Rach vnd Krafft/", "tokens": ["O", "Horn", "/", "jhr", "rech\u00b7te", "Faust", "der", "letz\u00b7ten", "Rach", "vnd", "Krafft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die in den Finnen brennt/ de\u00df ", "tokens": ["Die", "in", "den", "Fin\u00b7nen", "brennt", "/", "de\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vmb sonst ist ewer Witz die Tonaw zuentsetzen/", "tokens": ["Vmb", "sonst", "ist", "e\u00b7wer", "Witz", "die", "To\u00b7naw", "zu\u00b7ent\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie starck es ewer Belt vnd sein Bootes schafft.", "tokens": ["Wie", "starck", "es", "e\u00b7wer", "Belt", "vnd", "sein", "Boo\u00b7tes", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Vnd schawt der Gassen schaar eilt s\u00e4mtlich an das Thor/", "tokens": ["Vnd", "schawt", "der", "Gas\u00b7sen", "schaar", "eilt", "s\u00e4mt\u00b7lich", "an", "das", "Thor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "VVFIN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd ehrt ", "tokens": ["Vnd", "ehrt"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die St\u00fccke thun es nicht/ da\u00df Sie die Schl\u00fcssel bringen.", "tokens": ["Die", "St\u00fc\u00b7cke", "thun", "es", "nicht", "/", "da\u00df", "Sie", "die", "Schl\u00fcs\u