{"textgrid.poem.38019": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Zierlichkeit des Sch\u00e4ferlebens", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts kann auf Erden", "tokens": ["Nichts", "kann", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Verglichen werden", "tokens": ["Ver\u00b7gli\u00b7chen", "wer\u00b7den"], "token_info": ["word", "word"], "pos": ["VVPP", "VAINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Sch\u00e4fers Lust,", "tokens": ["Der", "Sch\u00e4\u00b7fers", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Auf gr\u00fcnen Heiden,", "tokens": ["Auf", "gr\u00fc\u00b7nen", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Verbl\u00fcmten Weiden,", "tokens": ["Ver\u00b7bl\u00fcm\u00b7ten", "Wei\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Giebts wahre Freuden,", "tokens": ["Giebts", "wah\u00b7re", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mir ists bewust.", "tokens": ["Mir", "ists", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Bey k\u00fchlen Bronnen,", "tokens": ["Bey", "k\u00fch\u00b7len", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bey heisser Sonnen", "tokens": ["Bey", "heis\u00b7ser", "Son\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Bestrahlet seyn,", "tokens": ["Be\u00b7strah\u00b7let", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ohn Furcht der Waffen", "tokens": ["Ohn", "Furcht", "der", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Im Gr\u00fcnen schlafen,", "tokens": ["Im", "Gr\u00fc\u00b7nen", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bey meinen Schafen", "tokens": ["Bey", "mei\u00b7nen", "Scha\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ist Freud allein.", "tokens": ["Ist", "Freud", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Bald geh ich leyren,", "tokens": ["Bald", "geh", "ich", "ley\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bald wieder feyren,", "tokens": ["Bald", "wie\u00b7der", "fey\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durch tiefe Thal,", "tokens": ["Durch", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dann mu\u00df ich springen", "tokens": ["Dann", "mu\u00df", "ich", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mich ganz aussingen,", "tokens": ["Mich", "ganz", "aus\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Thut wieder klingen", "tokens": ["Thut", "wie\u00b7der", "klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der Echo Schall.", "tokens": ["Der", "E\u00b7cho", "Schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ums Sch\u00e4ferleben", "tokens": ["Ums", "Sch\u00e4\u00b7fer\u00b7le\u00b7ben"], "token_info": ["word", "word"], "pos": ["KOUI", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Soll man gern geben,", "tokens": ["Soll", "man", "gern", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich wei\u00df nicht was,", "tokens": ["Ich", "wei\u00df", "nicht", "was", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PIS", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ich tausch mit keinem", "tokens": ["Ich", "tausch", "mit", "kei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und schlaf bey meinen", "tokens": ["Und", "schlaf", "bey", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Herzliebsten Sch\u00e4fchen", "tokens": ["Herz\u00b7liebs\u00b7ten", "Sch\u00e4f\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Im gr\u00fcnen Gras.", "tokens": ["Im", "gr\u00fc\u00b7nen", "Gras", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}