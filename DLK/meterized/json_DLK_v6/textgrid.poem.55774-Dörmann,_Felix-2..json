{"textgrid.poem.55774": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Asche vergrub ich die Stirn und in Koth,", "tokens": ["In", "A\u00b7sche", "ver\u00b7grub", "ich", "die", "Stirn", "und", "in", "Koth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den W\u00f6lfen im Walde zum Fra\u00df ich mich bot,", "tokens": ["Den", "W\u00f6l\u00b7fen", "im", "Wal\u00b7de", "zum", "Fra\u00df", "ich", "mich", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPRART", "NN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Den St\u00fcrmen der Winternacht gab ich mich preis,", "tokens": ["Den", "St\u00fcr\u00b7men", "der", "Win\u00b7ter\u00b7nacht", "gab", "ich", "mich", "preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich w\u00e4lzte die Glieder durch Felsen und Eis;", "tokens": ["Ich", "w\u00e4lz\u00b7te", "die", "Glie\u00b7der", "durch", "Fel\u00b7sen", "und", "Eis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ich beugte den Willen und brach meinen Stolz,", "tokens": ["Ich", "beug\u00b7te", "den", "Wil\u00b7len", "und", "brach", "mei\u00b7nen", "Stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schlug selber die Seele an's Marterholz,", "tokens": ["Schlug", "sel\u00b7ber", "die", "See\u00b7le", "an's", "Mar\u00b7ter\u00b7holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Ich kroch vor der Herde \u2013 und das ist die Qual,", "tokens": ["Ich", "kroch", "vor", "der", "Her\u00b7de", "\u2013", "und", "das", "ist", "die", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Seele rothgl\u00fchendes Schandenmal.", "tokens": ["Der", "See\u00b7le", "roth\u00b7gl\u00fc\u00b7hen\u00b7des", "Schan\u00b7den\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}