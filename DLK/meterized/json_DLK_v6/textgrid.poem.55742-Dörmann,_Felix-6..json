{"textgrid.poem.55742": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag ist langsam verronnen,", "tokens": ["Der", "Tag", "ist", "lang\u00b7sam", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Nacht bricht endlich herein,", "tokens": ["Die", "Nacht", "bricht", "end\u00b7lich", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu seliger Liebe Wonnen", "tokens": ["Zu", "se\u00b7li\u00b7ger", "Lie\u00b7be", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Leuchtet der Sterne Schein.", "tokens": ["Leuch\u00b7tet", "der", "Ster\u00b7ne", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wie sind Deine schneeigen Glieder", "tokens": ["Wie", "sind", "Dei\u00b7ne", "schne\u00b7e\u00b7i\u00b7gen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Vom Feuer der Liebe durchloht, \u2013", "tokens": ["Vom", "Feu\u00b7er", "der", "Lie\u00b7be", "durch\u00b7loht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und wieder k\u00fcss' ich und wieder", "tokens": ["Und", "wie\u00b7der", "k\u00fcss'", "ich", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KON", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die Lippen so hei\u00df und so rot.", "tokens": ["Die", "Lip\u00b7pen", "so", "hei\u00df", "und", "so", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Den Haaren die Funken entstieben,", "tokens": ["Den", "Haa\u00b7ren", "die", "Fun\u00b7ken", "ent\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Wenn zitternd die Hand sie durchw\u00fchlt,", "tokens": ["Wenn", "zit\u00b7ternd", "die", "Hand", "sie", "durch\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Ja, Du kannst k\u00fcssen und lieben,", "tokens": ["Ja", ",", "Du", "kannst", "k\u00fcs\u00b7sen", "und", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Wie Du hat noch keine gef\u00fchlt.", "tokens": ["Wie", "Du", "hat", "noch", "kei\u00b7ne", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ADV", "PIAT", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Gefangen nimmst Du die Sinne", "tokens": ["Ge\u00b7fan\u00b7gen", "nimmst", "Du", "die", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Das Herz und den Geist und den Leib,", "tokens": ["Das", "Herz", "und", "den", "Geist", "und", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Du bist die F\u00fcrstin der Minne,", "tokens": ["Du", "bist", "die", "F\u00fcrs\u00b7tin", "der", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Du liebegewaltiges Weib. \u2013", "tokens": ["Du", "lie\u00b7be\u00b7ge\u00b7wal\u00b7ti\u00b7ges", "Weib", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.18": {"text": "Schon brennt die Ampel tr\u00fcber,", "tokens": ["Schon", "brennt", "die", "Am\u00b7pel", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Schon zeigt sich Fr\u00fchrotschein \u2013", "tokens": ["Schon", "zeigt", "sich", "Fr\u00fch\u00b7rot\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "W\u00e4r' erst der Tag vor\u00fcber", "tokens": ["W\u00e4r'", "erst", "der", "Tag", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Und br\u00e4ch' die Nacht herein.", "tokens": ["Und", "br\u00e4ch'", "die", "Nacht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}