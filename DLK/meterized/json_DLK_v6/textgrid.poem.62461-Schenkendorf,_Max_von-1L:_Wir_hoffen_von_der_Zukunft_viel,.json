{"textgrid.poem.62461": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir hoffen von der Zukunft viel,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir hoffen von der Zukunft viel,", "tokens": ["Wir", "hof\u00b7fen", "von", "der", "Zu\u00b7kunft", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Recht soll wiederkehren,", "tokens": ["Das", "Recht", "soll", "wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4nger nicht der Willk\u00fcr Spiel", "tokens": ["Und", "l\u00e4n\u00b7ger", "nicht", "der", "Will\u00b7k\u00fcr", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das deutsche Volk entehren.", "tokens": ["Das", "deut\u00b7sche", "Volk", "en\u00b7teh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dir nicht, entartetes Geschlecht,", "tokens": ["Dir", "nicht", ",", "ent\u00b7ar\u00b7te\u00b7tes", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir wird das nicht verk\u00fcndigt,", "tokens": ["Dir", "wird", "das", "nicht", "ver\u00b7k\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hast mit fremder Magd und Knecht", "tokens": ["Du", "hast", "mit", "frem\u00b7der", "Magd", "und", "Knecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich gar zu schwer vers\u00fcndigt.", "tokens": ["Dich", "gar", "zu", "schwer", "ver\u00b7s\u00fcn\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So wandle ferner blind und taub,", "tokens": ["So", "wand\u00b7le", "fer\u00b7ner", "blind", "und", "taub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu bl\u00f6de selbst zum Hoffen;", "tokens": ["Zu", "bl\u00f6\u00b7de", "selbst", "zum", "Hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sei jedes Dr\u00e4ngers guter Raub", "tokens": ["Sei", "je\u00b7des", "Dr\u00e4n\u00b7gers", "gu\u00b7ter", "Raub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und stets dem Welschen offen.", "tokens": ["Und", "stets", "dem", "Wel\u00b7schen", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch aus der Ferne steigen schon", "tokens": ["Doch", "aus", "der", "Fer\u00b7ne", "stei\u00b7gen", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kinder frei geboren,", "tokens": ["Die", "Kin\u00b7der", "frei", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die hat sich Gott im h\u00f6chsten Thron", "tokens": ["Die", "hat", "sich", "Gott", "im", "h\u00f6chs\u00b7ten", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PRF", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinem Volk erkoren.", "tokens": ["Zu", "sei\u00b7nem", "Volk", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die sogen an der Freiheit Brust,", "tokens": ["Die", "so\u00b7gen", "an", "der", "Frei\u00b7heit", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon unterm Mutterherzen", "tokens": ["Schon", "un\u00b7term", "Mut\u00b7ter\u00b7her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Empfanden sie die Siegeslust", "tokens": ["Emp\u00b7fan\u00b7den", "sie", "die", "Sie\u00b7ges\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch des Landes Schmerzen.", "tokens": ["Und", "auch", "des", "Lan\u00b7des", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die schauen erst im rechten Glanz", "tokens": ["Die", "schau\u00b7en", "erst", "im", "rech\u00b7ten", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum wir alle werben,", "tokens": ["Wa\u00b7rum", "wir", "al\u00b7le", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sollen Waffen, sollen Kranz,", "tokens": ["Die", "sol\u00b7len", "Waf\u00b7fen", ",", "sol\u00b7len", "Kranz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die blut'gen, von uns erben.", "tokens": ["Die", "blut'\u00b7gen", ",", "von", "uns", "er\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Drum wollen wir die Gegenwart", "tokens": ["Drum", "wol\u00b7len", "wir", "die", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit rechter Treu verwalten,", "tokens": ["Mit", "rech\u00b7ter", "Treu", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und was die junge Welt erharrt,", "tokens": ["Und", "was", "die", "jun\u00b7ge", "Welt", "er\u00b7harrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versuchen zu gestalten.", "tokens": ["Ver\u00b7su\u00b7chen", "zu", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir haben lang ans Kreuz gelegt", "tokens": ["Wir", "ha\u00b7ben", "lang", "ans", "Kreuz", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Willen wie die Ehren,", "tokens": ["Den", "Wil\u00b7len", "wie", "die", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was auch die H\u00f6lle noch erregt,", "tokens": ["Was", "auch", "die", "H\u00f6l\u00b7le", "noch", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag f\u00fcrder uns nicht st\u00f6ren.", "tokens": ["Mag", "f\u00fcr\u00b7der", "uns", "nicht", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wir streuen froh den Samen aus,", "tokens": ["Wir", "streu\u00b7en", "froh", "den", "Sa\u00b7men", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die edlen K\u00f6rner fallen:", "tokens": ["Die", "ed\u00b7len", "K\u00f6r\u00b7ner", "fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir werden zu des Vaters Haus", "tokens": ["Wir", "wer\u00b7den", "zu", "des", "Va\u00b7ters", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ohne Schande wallen.", "tokens": ["Doch", "oh\u00b7ne", "Schan\u00b7de", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}