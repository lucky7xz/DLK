{"textgrid.poem.59377": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann Lenzesstrahlen golden niederrinnen,", "tokens": ["Wann", "Len\u00b7zes\u00b7strah\u00b7len", "gol\u00b7den", "nie\u00b7der\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieht man die Scharen losgebunden ziehen,", "tokens": ["Sieht", "man", "die", "Scha\u00b7ren", "los\u00b7ge\u00b7bun\u00b7den", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Waldrevier, dem neu der Schmuck geliehen,", "tokens": ["Im", "Wald\u00b7re\u00b7vier", ",", "dem", "neu", "der", "Schmuck", "ge\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die lust'ge Jagd nach Lieb und Scherz beginnen.", "tokens": ["Die", "lust'\u00b7ge", "Jagd", "nach", "Lieb", "und", "Scherz", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den S\u00e4nger will der Fr\u00fchling gar umspinnen,", "tokens": ["Den", "S\u00e4n\u00b7ger", "will", "der", "Fr\u00fch\u00b7ling", "gar", "um\u00b7spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er, der Geliebteste, darf nicht entfliehen,", "tokens": ["Er", ",", "der", "Ge\u00b7lieb\u00b7tes\u00b7te", ",", "darf", "nicht", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "F\u00fchlt rings ein Lied durch alle Farben ziehen,", "tokens": ["F\u00fchlt", "rings", "ein", "Lied", "durch", "al\u00b7le", "Far\u00b7ben", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ihn so lockend nimmer l\u00e4\u00dft von hinnen.", "tokens": ["Das", "ihn", "so", "lo\u00b7ckend", "nim\u00b7mer", "l\u00e4\u00dft", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gefangen so, sitzt er viel sel'ge Jahre;", "tokens": ["Ge\u00b7fan\u00b7gen", "so", ",", "sitzt", "er", "viel", "sel'\u00b7ge", "Jah\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Des Einsamen spottet des P\u00f6bels Scherzen,", "tokens": ["Des", "Ein\u00b7sa\u00b7men", "spot\u00b7tet", "des", "P\u00f6\u00b7bels", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der aller Glorie m\u00f6chte Lieb entkleiden.", "tokens": ["Der", "al\u00b7ler", "Glo\u00b7rie", "m\u00f6ch\u00b7te", "Lieb", "ent\u00b7klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch er gr\u00fc\u00dft fr\u00f6hlich alle, wie sie fahren,", "tokens": ["Doch", "er", "gr\u00fc\u00dft", "fr\u00f6h\u00b7lich", "al\u00b7le", ",", "wie", "sie", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PIS", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mutig sagt er zu den s\u00fc\u00dfen Schmerzen:", "tokens": ["Und", "mu\u00b7tig", "sagt", "er", "zu", "den", "s\u00fc\u00b7\u00dfen", "Schmer\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbgern sterb ich bald, wollt ihr von mir je scheiden!\u00ab", "tokens": ["\u00bb", "gern", "sterb", "ich", "bald", ",", "wollt", "ihr", "von", "mir", "je", "schei\u00b7den", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "$,", "VMFIN", "PPER", "APPR", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}