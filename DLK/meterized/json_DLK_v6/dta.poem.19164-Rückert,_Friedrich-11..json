{"dta.poem.19164": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist der Vorzug wol der menschlichen Vernunft", "tokens": ["Was", "ist", "der", "Vor\u00b7zug", "wol", "der", "menschli\u00b7chen", "Ver\u00b7nunft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Vor allen Trieben, die besitzt der Thiere Zunft?", "tokens": ["Vor", "al\u00b7len", "Trie\u00b7ben", ",", "die", "be\u00b7sitzt", "der", "Thie\u00b7re", "Zunft", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Thuts nicht der menschlichen in allen St\u00fccken gleich", "tokens": ["Thuts", "nicht", "der", "menschli\u00b7chen", "in", "al\u00b7len", "St\u00fc\u00b7cken", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "APPR", "PIAT", "NN", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Naturvernunft und Kunst, an Wunderwerken reich?", "tokens": ["Na\u00b7tur\u00b7ver\u00b7nunft", "und", "Kunst", ",", "an", "Wun\u00b7der\u00b7wer\u00b7ken", "reich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Mensch kann feiner als der Seidenwurm nicht spinnen,", "tokens": ["Der", "Mensch", "kann", "fei\u00b7ner", "als", "der", "Sei\u00b7den\u00b7wurm", "nicht", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "KOKOM", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und k\u00fcnstlicher nicht baun als Immen goldne Zinnen.", "tokens": ["Und", "k\u00fcnst\u00b7li\u00b7cher", "nicht", "baun", "als", "Im\u00b7men", "gold\u00b7ne", "Zin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VVINF", "KOKOM", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und nicht gelernt ist das, geerbt ists vom Geschlecht;", "tokens": ["Und", "nicht", "ge\u00b7lernt", "ist", "das", ",", "ge\u00b7erbt", "ists", "vom", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "VAFIN", "PDS", "$,", "VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der j\u00fcngste Biber baut gleich wie der \u00e4lt'ste recht.", "tokens": ["Der", "j\u00fcngs\u00b7te", "Bi\u00b7ber", "baut", "gleich", "wie", "der", "\u00e4lt'\u00b7ste", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die uranf\u00e4ngliche Naturvollkommenheit", "tokens": ["Die", "u\u00b7ran\u00b7f\u00e4ng\u00b7li\u00b7che", "Na\u00b7tur\u00b7voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nie vollkommener geworden durch die Zeit.", "tokens": ["Ist", "nie", "voll\u00b7kom\u00b7me\u00b7ner", "ge\u00b7wor\u00b7den", "durch", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VAPP", "APPR", "ART", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Und dieses ist, was der Vollkommenheit gebricht;", "tokens": ["Und", "die\u00b7ses", "ist", ",", "was", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "ge\u00b7bricht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vollkommnungsf\u00e4higkeit fehlt nur dem Menschen nicht.", "tokens": ["Voll\u00b7komm\u00b7nungs\u00b7f\u00e4\u00b7hig\u00b7keit", "fehlt", "nur", "dem", "Men\u00b7schen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Die junge Spinne spinnt nur wie die alte spann,", "tokens": ["Die", "jun\u00b7ge", "Spin\u00b7ne", "spinnt", "nur", "wie", "die", "al\u00b7te", "spann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "KOKOM", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indes der Menschensinn stets neu Geweb ersann.", "tokens": ["In\u00b7des", "der", "Men\u00b7schen\u00b7sinn", "stets", "neu", "Ge\u00b7web", "er\u00b7sann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Vom Vater erbt ers nicht, vom Meister kann ers lernen,", "tokens": ["Vom", "Va\u00b7ter", "erbt", "ers", "nicht", ",", "vom", "Meis\u00b7ter", "kann", "ers", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PTKNEG", "$,", "APPRART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ausgelernt von ihm mit Freiheit sich entfernen.", "tokens": ["Und", "aus\u00b7ge\u00b7lernt", "von", "ihm", "mit", "Frei\u00b7heit", "sich", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "PPER", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die Freiheit voll Gefahr ist jedes Irrthums Spiel,", "tokens": ["Die", "Frei\u00b7heit", "voll", "Ge\u00b7fahr", "ist", "je\u00b7des", "Irr\u00b7thums", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indes der sichre Trieb nothwendig geht zum Ziel.", "tokens": ["In\u00b7des", "der", "sich\u00b7re", "Trieb", "noth\u00b7wen\u00b7dig", "geht", "zum", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Doch ists ein niedres Ziel vor jenem, das erreichen", "tokens": ["Doch", "ists", "ein", "nie\u00b7dres", "Ziel", "vor", "je\u00b7nem", ",", "das", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "$,", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mensch will, soll und kann, mag es auch stets entweichen;", "tokens": ["Der", "Mensch", "will", ",", "soll", "und", "kann", ",", "mag", "es", "auch", "stets", "ent\u00b7wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "VMFIN", "KON", "VMFIN", "$,", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Wo Kunstbehendigkeit und Thatverst\u00e4ndigkeit", "tokens": ["Wo", "Kunst\u00b7be\u00b7hen\u00b7dig\u00b7keit", "und", "That\u00b7ver\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm wird in h\u00f6hrer Art Naturnothwendigkeit.", "tokens": ["Ihm", "wird", "in", "h\u00f6h\u00b7rer", "Art", "Na\u00b7tur\u00b7noth\u00b7wen\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}