{"textgrid.poem.36425": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um meiner Mannheit Tiefgang auszuloten,", "tokens": ["Um", "mei\u00b7ner", "Mann\u00b7heit", "Tief\u00b7gang", "aus\u00b7zu\u00b7lo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ging ich mit nacktem Glied zu Keuschgesinnten.", "tokens": ["Ging", "ich", "mit", "nack\u00b7tem", "Glied", "zu", "Keuschge\u00b7sinn\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ich glaubte, diese deutlichste der Finten", "tokens": ["Ich", "glaub\u00b7te", ",", "die\u00b7se", "deut\u00b7lichs\u00b7te", "der", "Fin\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PDAT", "ADJA", "ART", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Sei zwingender als Zahlen oder Zoten.", "tokens": ["Sei", "zwin\u00b7gen\u00b7der", "als", "Zah\u00b7len", "o\u00b7der", "Zo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich trat zu M\u00e4dchen unversehns von hinten,", "tokens": ["Ich", "trat", "zu", "M\u00e4d\u00b7chen", "un\u00b7ver\u00b7sehns", "von", "hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sprach sanft sie an und spielte den Zeloten.", "tokens": ["Sprach", "sanft", "sie", "an", "und", "spiel\u00b7te", "den", "Ze\u00b7lo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann fragt' ich pl\u00f6tzlich, wann sie denn den roten", "tokens": ["Dann", "fragt'", "ich", "pl\u00f6tz\u00b7lich", ",", "wann", "sie", "denn", "den", "ro\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gewaltherrn h\u00e4tten, und wie lang sie minnten.", "tokens": ["Ge\u00b7walt\u00b7herrn", "h\u00e4t\u00b7ten", ",", "und", "wie", "lang", "sie", "minn\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KON", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie sehn verdrehten Auges auf den Stecken,", "tokens": ["Sie", "sehn", "ver\u00b7dreh\u00b7ten", "Au\u00b7ges", "auf", "den", "Ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ihnen doch galant entgegensteht.", "tokens": ["Der", "ih\u00b7nen", "doch", "ga\u00b7lant", "ent\u00b7ge\u00b7gen\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich hebe sie, darauf zu st\u00fclpsen.", "tokens": ["Ich", "he\u00b7be", "sie", ",", "da\u00b7rauf", "zu", "st\u00fclp\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zuerst wohl w\u00fcrgen, schreien sie, und r\u00fclpsen,", "tokens": ["Zu\u00b7erst", "wohl", "w\u00fcr\u00b7gen", ",", "schrei\u00b7en", "sie", ",", "und", "r\u00fclp\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann flie\u00dft die Lust, und alles Weh vergeht.", "tokens": ["Dann", "flie\u00dft", "die", "Lust", ",", "und", "al\u00b7les", "Weh", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis sie zutiefst gekitzelt drauf verrecken.", "tokens": ["Bis", "sie", "zu\u00b7tiefst", "ge\u00b7kit\u00b7zelt", "drauf", "ver\u00b7re\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VVPP", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}