{"dta.poem.18907": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von H\u00f6chstermelter K\u00f6nigl. Mayt.  \n Gustav dem Grossen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df billich ist sein Nam/ als dessen wehrt so", "tokens": ["Gro\u00df", "bil\u00b7lich", "ist", "sein", "Nam", "/", "als", "des\u00b7sen", "wehrt", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VAFIN", "PPOSAT", "NN", "$(", "KOKOM", "PDS", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gro\u00df/", "tokens": ["gro\u00df", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Da\u00df Fridenszeit sein haupt zu herschen vnd zu", "tokens": ["Da\u00df", "Fri\u00b7dens\u00b7zeit", "sein", "haupt", "zu", "her\u00b7schen", "vnd", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "KON", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "rahten/", "tokens": ["rah\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Da\u00df Kriegszeit seine faust durch grosse Helden-", "tokens": ["Da\u00df", "Kriegs\u00b7zeit", "sei\u00b7ne", "faust", "durch", "gros\u00b7se", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "thaten", "tokens": ["tha\u00b7ten"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Der Helden Phoenix jn bewehrten stehts gletchlo\u00df.", "tokens": ["Der", "Hel\u00b7den", "Phoe\u00b7nix", "jn", "be\u00b7wehr\u00b7ten", "stehts", "gletc\u00b7hlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein wehrters haupt noch hand/ gewafnet oder blo\u00df/", "tokens": ["Kein", "wehr\u00b7ters", "haupt", "noch", "hand", "/", "ge\u00b7waf\u00b7net", "o\u00b7der", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "NN", "$(", "VVPP", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trug jemahls Scepter/ Cron/ schwert/ zierd der", "tokens": ["Trug", "je\u00b7mahls", "Scep\u00b7ter", "/", "Cron", "/", "schwert", "/", "zierd", "der"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "NN", "$(", "NE", "$(", "VVFIN", "$(", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Potentaten;", "tokens": ["Po\u00b7ten\u00b7ta\u00b7ten", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Von keinem edlern blut/ belebend die Soldaten/", "tokens": ["Von", "kei\u00b7nem", "ed\u00b7lern", "blut", "/", "be\u00b7le\u00b7bend", "die", "Sol\u00b7da\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$(", "VVPP", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sah man jemahls schamroht der erden blaiche", "tokens": ["Sah", "man", "je\u00b7mahls", "scham\u00b7roht", "der", "er\u00b7den", "blai\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "VVFIN", "ART", "NN", "ADJA"], "meter": "+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "scho\u00df.", "tokens": ["scho\u00df", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Auch kont kein andrer Held/ wz Er gethan/ verwalte\u0304/", "tokens": ["Auch", "kont", "kein", "an\u00b7drer", "Held", "/", "wz", "Er", "ge\u00b7than", "/", "ver\u00b7wal\u00b7t\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "ADJA", "NN", "$(", "PWS", "PPER", "VVPP", "$(", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch vnnachthunlich sein in sicherheit vnd noht/", "tokens": ["Noch", "vn\u00b7nach\u00b7thun\u00b7lich", "sein", "in", "si\u00b7cher\u00b7heit", "vnd", "noht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df weder Sig noch fall sein Kriegsheer kont", "tokens": ["Da\u00df", "we\u00b7der", "Sig", "noch", "fall", "sein", "Kriegs\u00b7heer", "kont"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KON", "NE", "ADV", "NN", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "erkalten.", "tokens": ["er\u00b7kal\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "O wonder! Er allein sigreich durch Got/ f\u00fcr Got", "tokens": ["O", "won\u00b7der", "!", "Er", "al\u00b7lein", "sig\u00b7reich", "durch", "Got", "/", "f\u00fcr", "Got"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "PPER", "ADV", "ADJD", "APPR", "NN", "$(", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergiessend nu sein blut/ hat vor/ in/ nach dem", "tokens": ["Ver\u00b7gies\u00b7send", "nu", "sein", "blut", "/", "hat", "vor", "/", "in", "/", "nach", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVPP", "ADV", "PPOSAT", "NN", "$(", "VAFIN", "APPR", "$(", "APPR", "$(", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tod/", "tokens": ["Tod", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Stehts vnvergleichlich ", "tokens": ["Stehts", "vn\u00b7ver\u00b7gleich\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Cron erhalten!", "tokens": ["Cron", "er\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}