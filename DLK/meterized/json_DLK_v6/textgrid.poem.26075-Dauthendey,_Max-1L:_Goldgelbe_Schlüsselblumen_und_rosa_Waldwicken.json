{"textgrid.poem.26075": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Goldgelbe Schl\u00fcsselblumen und rosa Waldwicken", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldgelbe Schl\u00fcsselblumen und rosa Waldwicken", "tokens": ["Gold\u00b7gel\u00b7be", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7men", "und", "ro\u00b7sa", "Wald\u00b7wi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+--+-+--+-+--", "measure": "iambic.penta.invert"}, "line.2": {"text": "Kommen ins Zimmer mit l\u00e4ndlichen Blicken.", "tokens": ["Kom\u00b7men", "ins", "Zim\u00b7mer", "mit", "l\u00e4nd\u00b7li\u00b7chen", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Veilchen und samtne Osterblumen mit silbrigem Schimmer", "tokens": ["Veil\u00b7chen", "und", "samt\u00b7ne", "Os\u00b7ter\u00b7blu\u00b7men", "mit", "silb\u00b7ri\u00b7gem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Bringen die Luft vom Berg, wo Gr\u00e4ser nicken,", "tokens": ["Brin\u00b7gen", "die", "Luft", "vom", "Berg", ",", "wo", "Gr\u00e4\u00b7ser", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und alle rufen: \u00bbFr\u00fchling bleibt es jetzt immer.\u00ab", "tokens": ["Und", "al\u00b7le", "ru\u00b7fen", ":", "\u00bb", "Fr\u00fch\u00b7ling", "bleibt", "es", "jetzt", "im\u00b7mer", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVINF", "$.", "$(", "NN", "VVFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "H\u00f6rst keine Uhr und keinen Holzwurm mehr ticken,", "tokens": ["H\u00f6rst", "kei\u00b7ne", "Uhr", "und", "kei\u00b7nen", "Holz\u00b7wurm", "mehr", "ti\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Alle Tage unsterblichen Atem dir schicken.", "tokens": ["Al\u00b7le", "Ta\u00b7ge", "uns\u00b7terb\u00b7li\u00b7chen", "A\u00b7tem", "dir", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Das Herz wird zur Fl\u00f6te; drauf spielt jede Stund'", "tokens": ["Das", "Herz", "wird", "zur", "Fl\u00f6\u00b7te", ";", "drauf", "spielt", "je\u00b7de", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$.", "PAV", "VVFIN", "PIAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Deiner Liebsten woll\u00fcstiger Mund.", "tokens": ["Dei\u00b7ner", "Liebs\u00b7ten", "wol\u00b7l\u00fcs\u00b7ti\u00b7ger", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}