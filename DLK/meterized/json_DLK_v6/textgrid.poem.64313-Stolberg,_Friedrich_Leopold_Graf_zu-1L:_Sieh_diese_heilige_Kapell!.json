{"textgrid.poem.64313": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh diese heilige Kapell!", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh diese heilige Kapell!", "tokens": ["Sieh", "die\u00b7se", "hei\u00b7li\u00b7ge", "Ka\u00b7pell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier ward geboren Wilhelm Tell!", "tokens": ["Hier", "ward", "ge\u00b7bo\u00b7ren", "Wil\u00b7helm", "Tell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier, wo der Altar Gottes steht,", "tokens": ["Hier", ",", "wo", "der", "Al\u00b7tar", "Got\u00b7tes", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stand seiner Eltern Ehebett!", "tokens": ["Stand", "sei\u00b7ner", "El\u00b7tern", "E\u00b7he\u00b7bett", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit Mutter-Freuden freute sich", "tokens": ["Mit", "Mut\u00b7ter\u00b7Freu\u00b7den", "freu\u00b7te", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die liebe Mutter inniglich,", "tokens": ["Die", "lie\u00b7be", "Mut\u00b7ter", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gedachte nicht an ihren Schmerz,", "tokens": ["Ge\u00b7dach\u00b7te", "nicht", "an", "ih\u00b7ren", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hielt den Knaben an ihr Herz;", "tokens": ["Und", "hielt", "den", "Kna\u00b7ben", "an", "ihr", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die sprach zu Gott: Er sei dein Knecht,", "tokens": ["Die", "sprach", "zu", "Gott", ":", "Er", "sei", "dein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei stark und mutig und gerecht!", "tokens": ["Sei", "stark", "und", "mu\u00b7tig", "und", "ge\u00b7recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott aber dacht': ich thue mehr", "tokens": ["Gott", "a\u00b7ber", "dacht'", ":", "ich", "thue", "mehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Durch ihn als durch ein ganzes Heer.", "tokens": ["Durch", "ihn", "als", "durch", "ein", "gan\u00b7zes", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er gab dem Knaben warmes Blut,", "tokens": ["Er", "gab", "dem", "Kna\u00b7ben", "war\u00b7mes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Rosses Kraft, des Adlers Mut,", "tokens": ["Des", "Ros\u00b7ses", "Kraft", ",", "des", "Ad\u00b7lers", "Mut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Felsen-Nacken freien Sinn,", "tokens": ["Im", "Fel\u00b7sen\u00b7Nacken", "frei\u00b7en", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Falken Aug' und Feuer drin!", "tokens": ["Des", "Fal\u00b7ken", "Aug'", "und", "Feu\u00b7er", "drin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dem Worte sein und der Natur", "tokens": ["Dem", "Wor\u00b7te", "sein", "und", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAINF", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertraute Gott das Kn\u00e4blein nur,", "tokens": ["Ver\u00b7trau\u00b7te", "Gott", "das", "Kn\u00e4\u00b7blein", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sich der Felsenstrom ergeu\u00dft,", "tokens": ["Wo", "sich", "der", "Fel\u00b7sen\u00b7strom", "er\u00b7geu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erhub sich fr\u00fch sein Helden-Geist.", "tokens": ["Er\u00b7hub", "sich", "fr\u00fch", "sein", "Hel\u00b7den\u00b7Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er wu\u00dfte nicht, da\u00df seine Hand,", "tokens": ["Er", "wu\u00df\u00b7te", "nicht", ",", "da\u00df", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Gott gest\u00e4rkt, sein Vaterland", "tokens": ["Durch", "Gott", "ge\u00b7st\u00e4rkt", ",", "sein", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erretten w\u00fcrde von der Schmach", "tokens": ["Er\u00b7ret\u00b7ten", "w\u00fcr\u00b7de", "von", "der", "Schmach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Knechtschaft, deren Joch er brach.", "tokens": ["Der", "Knecht\u00b7schaft", ",", "de\u00b7ren", "Joch", "er", "brach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Ruder und die Gemsenjagd", "tokens": ["Das", "Ru\u00b7der", "und", "die", "Gem\u00b7sen\u00b7jagd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat seine Glieder stark gemacht!", "tokens": ["Hat", "sei\u00b7ne", "Glie\u00b7der", "stark", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er scherzte fr\u00fch mit der Gefahr,", "tokens": ["Er", "scherz\u00b7te", "fr\u00fch", "mit", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wu\u00dfte nicht, wie gro\u00df er war!", "tokens": ["Und", "wu\u00df\u00b7te", "nicht", ",", "wie", "gro\u00df", "er", "war", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}