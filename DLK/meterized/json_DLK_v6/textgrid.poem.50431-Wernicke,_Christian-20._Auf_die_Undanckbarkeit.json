{"textgrid.poem.50431": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "20. Auf die Undanckbarkeit", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du scheusslich Laster dieser Zeit,", "tokens": ["Du", "scheuss\u00b7lich", "Las\u00b7ter", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Solt' ich aus meinem Buch unangemerckt dich lassen,", "tokens": ["Solt'", "ich", "aus", "mei\u00b7nem", "Buch", "un\u00b7an\u00b7ge\u00b7merckt", "dich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du schn\u00f6der ", "tokens": ["Du", "schn\u00f6\u00b7der"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Solt' ich verwogner Weis' auf dich die Feder spitzen,", "tokens": ["Solt'", "ich", "ver\u00b7wog\u00b7ner", "Weis'", "auf", "dich", "die", "Fe\u00b7der", "spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "APPR", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da dich ", "tokens": ["Da", "dich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Und bist darum von mir unangefochten blieben,", "tokens": ["Und", "bist", "da\u00b7rum", "von", "mir", "un\u00b7an\u00b7ge\u00b7foch\u00b7ten", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil du die ", "tokens": ["Weil", "du", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+--", "measure": "dactylic.init"}}}}}