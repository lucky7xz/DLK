{"textgrid.poem.44067": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Heiland, der du von der List", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Heiland, der du von der List", "tokens": ["Mein", "Hei\u00b7land", ",", "der", "du", "von", "der", "List"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der tollen Welt gepr\u00fcfet bist", "tokens": ["Der", "tol\u00b7len", "Welt", "ge\u00b7pr\u00fc\u00b7fet", "bist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stets zu ihrer Schand entgangen,", "tokens": ["Und", "stets", "zu", "ih\u00b7rer", "Schand", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verleih mir deiner Wei\u00dfheit Licht,", "tokens": ["Ver\u00b7leih", "mir", "dei\u00b7ner", "Wei\u00df\u00b7heit", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit mich Schein und Vorwiz nicht", "tokens": ["Da\u00b7mit", "mich", "Schein", "und", "Vor\u00b7wiz", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch ihr verstelltes Wesen fangen.", "tokens": ["Durch", "ihr", "ver\u00b7stell\u00b7tes", "We\u00b7sen", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie leichtlich strauchelt die Vernunft", "tokens": ["Wie", "leicht\u00b7lich", "strau\u00b7chelt", "die", "Ver\u00b7nunft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gar auch bey gelehrter Zunft,", "tokens": ["So", "gar", "auch", "bey", "ge\u00b7lehr\u00b7ter", "Zunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn nicht dein Geist den Wiz begleitet.", "tokens": ["Wenn", "nicht", "dein", "Geist", "den", "Wiz", "be\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man f\u00e4hrt oft \u00fcber Berg und See,", "tokens": ["Man", "f\u00e4hrt", "oft", "\u00fc\u00b7ber", "Berg", "und", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch fehlt der Aufgang aus der H\u00f6h,", "tokens": ["Doch", "fehlt", "der", "Auf\u00b7gang", "aus", "der", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wird man stets in Nacht geleitet.", "tokens": ["So", "wird", "man", "stets", "in", "Nacht", "ge\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es theilet deine G\u00fctigkeit", "tokens": ["Es", "thei\u00b7let", "dei\u00b7ne", "G\u00fc\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gaben mit viel Unterscheid:", "tokens": ["Die", "Ga\u00b7ben", "mit", "viel", "Un\u00b7ter\u00b7scheid", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der eine lehrt, der andre sch\u00fczet,", "tokens": ["Der", "ei\u00b7ne", "lehrt", ",", "der", "and\u00b7re", "sch\u00fc\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem wird der Scepter zugedacht", "tokens": ["Dem", "wird", "der", "Scep\u00b7ter", "zu\u00b7ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und jener in den Staub gebracht,", "tokens": ["Und", "je\u00b7ner", "in", "den", "Staub", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den man zur Fruchtbarkeit beschwizet.", "tokens": ["Den", "man", "zur", "Frucht\u00b7bar\u00b7keit", "be\u00b7schwi\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Gieb mir nur, was dir wohlgef\u00e4llt,", "tokens": ["Gieb", "mir", "nur", ",", "was", "dir", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und las mich allzeit auf der Welt", "tokens": ["Und", "las", "mich", "all\u00b7zeit", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch jeglichem das Seine geben.", "tokens": ["Auch", "jeg\u00b7li\u00b7chem", "das", "Sei\u00b7ne", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PDS", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott nimmt, was mein Verm\u00f6gen kan,", "tokens": ["Gott", "nimmt", ",", "was", "mein", "Ver\u00b7m\u00f6\u00b7gen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein redlich Herz vor Opfer an,", "tokens": ["Ein", "red\u00b7lich", "Herz", "vor", "Op\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Obrigkeit mein Blut und Leben.", "tokens": ["Die", "Ob\u00b7rig\u00b7keit", "mein", "Blut", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie ruhig fahr ich einmahl hin,", "tokens": ["Wie", "ru\u00b7hig", "fahr", "ich", "ein\u00b7mahl", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich bey mir versichert bin,", "tokens": ["Wenn", "ich", "bey", "mir", "ver\u00b7si\u00b7chert", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da\u00df ich dem Nechsten treu gedienet.", "tokens": ["Da\u00df", "ich", "dem", "Nechs\u00b7ten", "treu", "ge\u00b7die\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schm\u00fcckt dieser Ruhm den Leichenstein,", "tokens": ["Schm\u00fcckt", "die\u00b7ser", "Ruhm", "den", "Lei\u00b7chen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So will ich mehr zufrieden seyn,", "tokens": ["So", "will", "ich", "mehr", "zu\u00b7frie\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als de\u00dfen Schwerd von Lorbeern gr\u00fcnet.", "tokens": ["Als", "de\u00b7\u00dfen", "Schwerd", "von", "Lor\u00b7beern", "gr\u00fc\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}