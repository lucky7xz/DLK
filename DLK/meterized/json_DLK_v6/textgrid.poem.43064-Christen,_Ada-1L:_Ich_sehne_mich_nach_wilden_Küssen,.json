{"textgrid.poem.43064": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sehne mich nach wilden K\u00fcssen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehne mich nach wilden K\u00fcssen,", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "wil\u00b7den", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach wollusthei\u00dfen Fieberschauern;", "tokens": ["Nach", "wol\u00b7lust\u00b7hei\u00b7\u00dfen", "Fie\u00b7ber\u00b7schau\u00b7ern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will die Nacht am hellen Tag", "tokens": ["Ich", "will", "die", "Nacht", "am", "hel\u00b7len", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht schon in banger Qual durchtrauern.", "tokens": ["Nicht", "schon", "in", "ban\u00b7ger", "Qual", "durch\u00b7trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch schl\u00e4gt mein Herz mit raschem Drang,", "tokens": ["Noch", "schl\u00e4gt", "mein", "Herz", "mit", "ra\u00b7schem", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch brennt die Wang' in Jugendgluthen \u2013", "tokens": ["Noch", "brennt", "die", "Wang'", "in", "Ju\u00b7gend\u00b7glu\u00b7then", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Steh' still, l\u00f6sch' aus mit einem Mal!", "tokens": ["Steh'", "still", ",", "l\u00f6sch'", "aus", "mit", "ei\u00b7nem", "Mal", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur nicht so tropfenweis verbluten!", "tokens": ["Nur", "nicht", "so", "trop\u00b7fen\u00b7weis", "ver\u00b7blu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}