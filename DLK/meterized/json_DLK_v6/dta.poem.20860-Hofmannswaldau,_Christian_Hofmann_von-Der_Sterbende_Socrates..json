{"dta.poem.20860": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df euch nicht m\u00f6ge schaden k\u00f6nnen/", "tokens": ["Da\u00df", "euch", "nicht", "m\u00f6\u00b7ge", "scha\u00b7den", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Ebentheuer d\u00fcnner Lufft/", "tokens": ["Das", "E\u00b7bent\u00b7heu\u00b7er", "d\u00fcn\u00b7ner", "Lufft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So blo\u00df die Furcht der schwachen Sinnen/", "tokens": ["So", "blo\u00df", "die", "Furcht", "der", "schwa\u00b7chen", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Torheit hat herbey gerufft/", "tokens": ["Aus", "Tor\u00b7heit", "hat", "her\u00b7bey", "ge\u00b7rufft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So di\u00df was Untersuchung heist/", "tokens": ["So", "di\u00df", "was", "Un\u00b7ter\u00b7su\u00b7chung", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PRELS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Jrrthum nicht zu Boden reist/", "tokens": ["Den", "Jrr\u00b7thum", "nicht", "zu", "Bo\u00b7den", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So mu\u00df ein ander Mittel kommen.", "tokens": ["So", "mu\u00df", "ein", "an\u00b7der", "Mit\u00b7tel", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Man greiffe Schwur und Segen an/", "tokens": ["Man", "greif\u00b7fe", "Schwur", "und", "Se\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVFIN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Damit die Wurtzel sey benommen/", "tokens": ["Da\u00b7mit", "die", "Wurt\u00b7zel", "sey", "be\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Torheit so uns schaden kan.", "tokens": ["Der", "Tor\u00b7heit", "so", "uns", "scha\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}