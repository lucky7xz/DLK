{"textgrid.poem.30806": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Preussen kan mit Jammer tr\u00e4ncken und mit Elend einen speisen.", "tokens": ["Preus\u00b7sen", "kan", "mit", "Jam\u00b7mer", "tr\u00e4n\u00b7cken", "und", "mit", "E\u00b7lend", "ei\u00b7nen", "spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "VVINF", "KON", "APPR", "NN", "ART", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "O, wir d\u00fcrffen nicht in Preussen! k\u00fcnnens einem hier erweisen.", "tokens": ["O", ",", "wir", "d\u00fcrf\u00b7fen", "nicht", "in", "Preus\u00b7sen", "!", "k\u00fcn\u00b7nens", "ei\u00b7nem", "hier", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "PTKNEG", "APPR", "NN", "$.", "ADV", "ART", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}