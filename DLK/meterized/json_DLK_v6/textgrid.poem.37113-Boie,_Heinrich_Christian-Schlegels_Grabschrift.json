{"textgrid.poem.37113": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Schlegels Grabschrift", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er starb der Genius vom tragischen Cothurne.", "tokens": ["Er", "starb", "der", "Ge\u00b7nius", "vom", "tra\u00b7gi\u00b7schen", "Cot\u00b7hur\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch liegt Melpomene geb\u00fcckt auf seiner Urne,", "tokens": ["Noch", "liegt", "Mel\u00b7po\u00b7me\u00b7ne", "ge\u00b7b\u00fcckt", "auf", "sei\u00b7ner", "Ur\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Giebt ihren Lieblingen nur selten einen Blick", "tokens": ["Giebt", "ih\u00b7ren", "Lieb\u00b7lin\u00b7gen", "nur", "sel\u00b7ten", "ei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "ART", "NN"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und denkt an Schlegeln stets zur\u00fcck.", "tokens": ["Und", "denkt", "an", "Schle\u00b7geln", "stets", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}