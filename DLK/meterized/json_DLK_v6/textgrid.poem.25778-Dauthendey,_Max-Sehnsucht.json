{"textgrid.poem.25778": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihre Arme umschlingen den Mondenschein", "tokens": ["Ih\u00b7re", "Ar\u00b7me", "um\u00b7schlin\u00b7gen", "den", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und ringen nach den Sternen,", "tokens": ["Und", "rin\u00b7gen", "nach", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen w\u00fchlen sich in die Nacht,", "tokens": ["Die", "Au\u00b7gen", "w\u00fch\u00b7len", "sich", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In kalte leblose Fernen.", "tokens": ["In", "kal\u00b7te", "leb\u00b7lo\u00b7se", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und es umkrallt die bettelnde Hand", "tokens": ["Und", "es", "um\u00b7krallt", "die", "bet\u00b7teln\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Den tauben Stein, den toten Sand,", "tokens": ["Den", "tau\u00b7ben", "Stein", ",", "den", "to\u00b7ten", "Sand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zermalmt von verzweifeltem Sehnen.", "tokens": ["Zer\u00b7malmt", "von", "ver\u00b7zwei\u00b7fel\u00b7tem", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Ertrinkend in Sehnsucht und Tr\u00e4nen.", "tokens": ["Er\u00b7trin\u00b7kend", "in", "Sehn\u00b7sucht", "und", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}