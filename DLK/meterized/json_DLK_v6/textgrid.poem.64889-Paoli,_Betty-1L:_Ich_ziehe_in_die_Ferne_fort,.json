{"textgrid.poem.64889": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ziehe in die Ferne fort,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ziehe in die Ferne fort,", "tokens": ["Ich", "zie\u00b7he", "in", "die", "Fer\u00b7ne", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In andre, fremde Gr\u00fcnde;", "tokens": ["In", "and\u00b7re", ",", "frem\u00b7de", "Gr\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn es auch nicht besser dort,", "tokens": ["Und", "wenn", "es", "auch", "nicht", "bes\u00b7ser", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich's nur anders finde.", "tokens": ["Wenn", "ich's", "nur", "an\u00b7ders", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn dort nur nicht so finster-grau", "tokens": ["Wenn", "dort", "nur", "nicht", "so", "fins\u00b7ter\u00b7grau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wolken ob mir hangen,", "tokens": ["Die", "Wol\u00b7ken", "ob", "mir", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn nicht, wie hier, gestellt zur Schau,", "tokens": ["Wenn", "nicht", ",", "wie", "hier", ",", "ge\u00b7stellt", "zur", "Schau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "PWAV", "ADV", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gemeinheit, Bosheit prangen!", "tokens": ["Ge\u00b7mein\u00b7heit", ",", "Bos\u00b7heit", "pran\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn ich nur von der St\u00e4tte fern,", "tokens": ["Wenn", "ich", "nur", "von", "der", "St\u00e4t\u00b7te", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die Thr\u00e4nen nur befeuchten;", "tokens": ["Die", "Thr\u00e4\u00b7nen", "nur", "be\u00b7feuch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo unterging mein Hoffnungsstern", "tokens": ["Wo", "un\u00b7ter\u00b7ging", "mein", "Hoff\u00b7nungs\u00b7stern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach allzukurzem Leuchten.", "tokens": ["Nach", "all\u00b7zu\u00b7kur\u00b7zem", "Leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn ich nur fern dem todten See", "tokens": ["Wenn", "ich", "nur", "fern", "dem", "tod\u00b7ten", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von th\u00f6richt eitlem Trachten,", "tokens": ["Von", "th\u00f6\u00b7richt", "eit\u00b7lem", "Trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jene Augen nicht mehr seh',", "tokens": ["Und", "je\u00b7ne", "Au\u00b7gen", "nicht", "mehr", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die mich so elend machten.", "tokens": ["Die", "mich", "so", "e\u00b7lend", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}