{"dta.poem.14228": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied und Wiedersehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da rauscht's lebendig drau\u00dfen in den B\u00e4umen,", "tokens": ["Da", "rauscht's", "le\u00b7ben\u00b7dig", "drau\u00b7\u00dfen", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Osten langen purpurrothe Streifen,", "tokens": ["Aus", "Os\u00b7ten", "lan\u00b7gen", "pur\u00b7pur\u00b7ro\u00b7the", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hoch Lerchenlieder durch das Zwielicht schweifen \u2014", "tokens": ["Hoch", "Ler\u00b7chen\u00b7lie\u00b7der", "durch", "das", "Zwie\u00b7licht", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du hebst das bl\u00fch'nde K\u00f6pfchen hold aus Tr\u00e4umen.", "tokens": ["Du", "hebst", "das", "bl\u00fch'n\u00b7de", "K\u00f6pf\u00b7chen", "hold", "aus", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}