{"textgrid.poem.39811": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "6. Biston", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorbereitet", "tokens": ["Vor\u00b7be\u00b7rei\u00b7tet"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sind die Geschicke der Welt.", "tokens": ["Sind", "die", "Ge\u00b7schi\u00b7cke", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "In allen Zonen dr\u00e4ngt sich aus dem Boden", "tokens": ["In", "al\u00b7len", "Zo\u00b7nen", "dr\u00e4ngt", "sich", "aus", "dem", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Saat hervor,", "tokens": ["Die", "Saat", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Decket mit ihrem Sammte", "tokens": ["De\u00b7cket", "mit", "ih\u00b7rem", "Samm\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die Erd', als einem Festgewand,", "tokens": ["Die", "Erd'", ",", "als", "ei\u00b7nem", "Fest\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und harrt des befruchtenden Donners.", "tokens": ["Und", "harrt", "des", "be\u00b7fruch\u00b7ten\u00b7den", "Don\u00b7ners", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Wen in den z\u00f6gernden Himmel", "tokens": ["Wen", "in", "den", "z\u00f6\u00b7gern\u00b7den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sendet die Erde hinauf", "tokens": ["Sen\u00b7det", "die", "Er\u00b7de", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Zum Vater", "tokens": ["Zum", "Va\u00b7ter"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Mit dem Flehen der V\u00f6lker,", "tokens": ["Mit", "dem", "Fle\u00b7hen", "der", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Da\u00df ihm gefalle zu lenken", "tokens": ["Da\u00df", "ihm", "ge\u00b7fal\u00b7le", "zu", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Seiner Allwissenheit Stral", "tokens": ["Sei\u00b7ner", "All\u00b7wis\u00b7sen\u00b7heit", "Stral"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Auf des Menschengeschlechts arbeitende Flur.", "tokens": ["Auf", "des", "Men\u00b7schen\u00b7ge\u00b7schlechts", "ar\u00b7bei\u00b7ten\u00b7de", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und zu senden schaffende Allmacht?", "tokens": ["Und", "zu", "sen\u00b7den", "schaf\u00b7fen\u00b7de", "All\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--++", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Einer aus seines K\u00f6niges Rat", "tokens": ["Ei\u00b7ner", "aus", "sei\u00b7nes", "K\u00f6\u00b7ni\u00b7ges", "Rat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Steht auf.", "tokens": ["Steht", "auf", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Kaum erh\u00f6het, r\u00e4umt er", "tokens": ["Kaum", "er\u00b7h\u00f6\u00b7het", ",", "r\u00e4umt", "er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Den ersten Platz.", "tokens": ["Den", "ers\u00b7ten", "Platz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Erschrocken sehen's,", "tokens": ["Er\u00b7schro\u00b7cken", "se\u00b7hen's", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Denn sie liebten ihn, die Menschen;", "tokens": ["Denn", "sie", "lieb\u00b7ten", "ihn", ",", "die", "Men\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch bei der Wellen Triumphlied,", "tokens": ["Doch", "bei", "der", "Wel\u00b7len", "Tri\u00b7ump\u00b7hlied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Die sein Eiland umschlingen,", "tokens": ["Die", "sein", "Ei\u00b7land", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Wandelt hinauf er zu Gott.", "tokens": ["Wan\u00b7delt", "hin\u00b7auf", "er", "zu", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Vor des H\u00f6chsten Throne", "tokens": ["Vor", "des", "H\u00f6chs\u00b7ten", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wirft er sich nieder und spricht:", "tokens": ["Wirft", "er", "sich", "nie\u00b7der", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "\u00bbbegonnen ist, o Herr, dein Werk!", "tokens": ["\u00bb", "be\u00b7gon\u00b7nen", "ist", ",", "o", "Herr", ",", "dein", "Werk", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die in der V\u00f6lker irrenden H\u00e4nden", "tokens": ["Die", "in", "der", "V\u00f6l\u00b7ker", "ir\u00b7ren\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Lange geschwankt,", "tokens": ["Lan\u00b7ge", "ge\u00b7schwankt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Gefa\u00dft hab' ich die Fackel", "tokens": ["Ge\u00b7fa\u00dft", "hab'", "ich", "die", "Fa\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Habe sie hoch gehoben in die Luft.", "tokens": ["Ha\u00b7be", "sie", "hoch", "ge\u00b7ho\u00b7ben", "in", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Sie z\u00fcndet! riefen die Thoren,", "tokens": ["Sie", "z\u00fcn\u00b7det", "!", "rie\u00b7fen", "die", "Tho\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Aber sie leuchtete nur.", "tokens": ["A\u00b7ber", "sie", "leuch\u00b7te\u00b7te", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Ein S\u00e4mann ging ich aus", "tokens": ["Ein", "S\u00e4\u00b7mann", "ging", "ich", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In ihrem Scheine,", "tokens": ["In", "ih\u00b7rem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Warf in langdurchw\u00fchlten,", "tokens": ["Warf", "in", "lang\u00b7durch\u00b7w\u00fchl\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Lockeren Boden", "tokens": ["Lo\u00b7cke\u00b7ren", "Bo\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "K\u00f6rner des Heils.", "tokens": ["K\u00f6r\u00b7ner", "des", "Heils", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Sprie\u00dfen sollte sie", "tokens": ["Sprie\u00b7\u00dfen", "soll\u00b7te", "sie"], "token_info": ["word", "word", "word"], "pos": ["NN", "VMFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Den Geschlechtern der Erde allen,", "tokens": ["Den", "Ge\u00b7schlech\u00b7tern", "der", "Er\u00b7de", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PIAT", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Deiner Freiheit k\u00f6stliche Frucht.", "tokens": ["Dei\u00b7ner", "Frei\u00b7heit", "k\u00f6st\u00b7li\u00b7che", "Frucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Frei im geselligen Tausch", "tokens": ["Frei", "im", "ge\u00b7sel\u00b7li\u00b7gen", "Tausch"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "M\u00f6gen die Sch\u00e4tze des Erdballs", "tokens": ["M\u00f6\u00b7gen", "die", "Sch\u00e4t\u00b7ze", "des", "Erd\u00b7balls"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Rollen von Lande zu Land;", "tokens": ["Rol\u00b7len", "von", "Lan\u00b7de", "zu", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Frei wandle das vern\u00fcnftige Wort,", "tokens": ["Frei", "wand\u00b7le", "das", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ge", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Frei gl\u00fche der fromme Glaube", "tokens": ["Frei", "gl\u00fc\u00b7he", "der", "from\u00b7me", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "In jeder Menschenbrust;", "tokens": ["In", "je\u00b7der", "Men\u00b7schen\u00b7brust", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Frei diene der B\u00fcrger dem Gesetz,", "tokens": ["Frei", "die\u00b7ne", "der", "B\u00fcr\u00b7ger", "dem", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Jede Fessel falle,", "tokens": ["Je\u00b7de", "Fes\u00b7sel", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Von der neuen Welt jungbrausenden Str\u00f6men", "tokens": ["Von", "der", "neu\u00b7en", "Welt", "jung\u00b7brau\u00b7sen\u00b7den", "Str\u00f6\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Bis zu des Eurotas versiegender Flut.", "tokens": ["Bis", "zu", "des", "Eu\u00b7ro\u00b7tas", "ver\u00b7sie\u00b7gen\u00b7der", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "Nicht geraubt, wie der Titanensohn,", "tokens": ["Nicht", "ge\u00b7raubt", ",", "wie", "der", "Ti\u00b7ta\u00b7nen\u00b7sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hab' ich dein Licht;", "tokens": ["Hab'", "ich", "dein", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf dein eigen Gehei\u00df", "tokens": ["Auf", "dein", "ei\u00b7gen", "Ge\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Hielt ich's den V\u00f6lkern vor,", "tokens": ["Hielt", "ich's", "den", "V\u00f6l\u00b7kern", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und der Erde besorgte,", "tokens": ["Und", "der", "Er\u00b7de", "be\u00b7sorg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Zweifelnde Herrscher", "tokens": ["Zwei\u00b7feln\u00b7de", "Herr\u00b7scher"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Haben mir, trauend, Gnade genickt,", "tokens": ["Ha\u00b7ben", "mir", ",", "trau\u00b7end", ",", "Gna\u00b7de", "ge\u00b7nickt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJD", "$,", "NN", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Haben gef\u00fcget die m\u00e4chtigen H\u00e4nde", "tokens": ["Ha\u00b7ben", "ge\u00b7f\u00fc\u00b7get", "die", "m\u00e4ch\u00b7ti\u00b7gen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Zu dreifaltigem, heiligem,", "tokens": ["Zu", "drei\u00b7fal\u00b7ti\u00b7gem", ",", "hei\u00b7li\u00b7gem", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.10": {"text": "Freiheitspendendem Bund.", "tokens": ["Frei\u00b7heits\u00b7pen\u00b7den\u00b7dem", "Bund", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Und jetzo fleh' ich:", "tokens": ["Und", "jet\u00b7zo", "fleh'", "ich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "La\u00df nicht umsonst sein", "tokens": ["La\u00df", "nicht", "um\u00b7sonst", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP"