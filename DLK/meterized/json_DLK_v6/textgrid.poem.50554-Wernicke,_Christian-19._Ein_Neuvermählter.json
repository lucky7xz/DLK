{"textgrid.poem.50554": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "19. Ein Neuverm\u00e4hlter", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als einen, der sich nur den ", "tokens": ["Als", "ei\u00b7nen", ",", "der", "sich", "nur", "den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "$,", "PRELS", "PRF", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit einer, welcher nichts an Schein und Tugend fehlt',", "tokens": ["Mit", "ei\u00b7ner", ",", "wel\u00b7cher", "nichts", "an", "Schein", "und", "Tu\u00b7gend", "fehlt'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Was habt ihr, fragten sie, ", "tokens": ["Was", "habt", "ihr", ",", "frag\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nichts, sagt' er, ", "tokens": ["Nichts", ",", "sagt'", "er", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Um mich ein wenig ", "tokens": ["Um", "mich", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PRF", "ART", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}}}}}