{"dta.poem.6178": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die richtige Verunfft selbst saget,", "tokens": ["Die", "rich\u00b7ti\u00b7ge", "Ver\u00b7unfft", "selbst", "sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird sie darinn um Rath gefraget:", "tokens": ["Wird", "sie", "da\u00b7rinn", "um", "Rath", "ge\u00b7fra\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df alle Dinge, die wir sehn,", "tokens": ["Da\u00df", "al\u00b7le", "Din\u00b7ge", ",", "die", "wir", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf eine Weis\u2019 allein geschehn.", "tokens": ["Auf", "ei\u00b7ne", "Weis'", "al\u00b7lein", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weg ", "tokens": ["Weg"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Es sey der eitlen W\u00f6rter Tand", "tokens": ["Es", "sey", "der", "eit\u00b7len", "W\u00f6r\u00b7ter", "Tand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sammt den Erfindungen verbannt,", "tokens": ["Sammt", "den", "Er\u00b7fin\u00b7dun\u00b7gen", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Worinn man nichts bekamit-nichts deutliches entdecket.", "tokens": ["Wo\u00b7rinn", "man", "nichts", "be\u00b7ka\u00b7mit\u00b7nichts", "deut\u00b7li\u00b7ches", "ent\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "VVFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}