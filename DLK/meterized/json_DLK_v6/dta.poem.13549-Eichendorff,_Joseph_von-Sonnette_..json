{"dta.poem.13549": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie bald l\u00e4g' unten alles Bange, Tr\u00fcbe,", "tokens": ["Wie", "bald", "l\u00e4g'", "un\u00b7ten", "al\u00b7les", "Ban\u00b7ge", ",", "Tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "PIAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du strebtest lauschend, blicktest nicht mehr nieder,", "tokens": ["Du", "streb\u00b7test", "lau\u00b7schend", ",", "blick\u00b7test", "nicht", "mehr", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6her winket stets der Br\u00fcder Liebe:", "tokens": ["Und", "h\u00f6\u00b7her", "win\u00b7ket", "stets", "der", "Br\u00fc\u00b7der", "Lie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}