{"textgrid.poem.40402": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der Ichthyosaurus", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rauscht in den Schachtelhalmen,", "tokens": ["Es", "rauscht", "in", "den", "Schach\u00b7tel\u00b7hal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verd\u00e4chtig leuchtet das Meer,", "tokens": ["Ver\u00b7d\u00e4ch\u00b7tig", "leuch\u00b7tet", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da schwimmt mit Tr\u00e4nen im Auge", "tokens": ["Da", "schwimmt", "mit", "Tr\u00e4\u00b7nen", "im", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Ichthyosaurus daher.", "tokens": ["Ein", "Ich\u00b7thy\u00b7o\u00b7sau\u00b7rus", "da\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ihn jammert der Zeiten Verderbnis,", "tokens": ["Ihn", "jam\u00b7mert", "der", "Zei\u00b7ten", "Ver\u00b7derb\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Denn ein sehr bedenklicher Ton", "tokens": ["Denn", "ein", "sehr", "be\u00b7denk\u00b7li\u00b7cher", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "War neuerlich eingerissen", "tokens": ["War", "neu\u00b7er\u00b7lich", "ein\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In der Liasformation.", "tokens": ["In", "der", "Li\u00b7as\u00b7for\u00b7ma\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbder Plesiosaurus, der Alte,", "tokens": ["\u00bb", "der", "Ple\u00b7si\u00b7o\u00b7sau\u00b7rus", ",", "der", "Al\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Er jubelt in Saus und Braus,", "tokens": ["Er", "ju\u00b7belt", "in", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Pterodaktylus selber", "tokens": ["Der", "Pter\u00b7o\u00b7dak\u00b7ty\u00b7lus", "sel\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Flog neulich betrunken nach Haus.", "tokens": ["Flog", "neu\u00b7lich", "be\u00b7trun\u00b7ken", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Der Iguanodon, der L\u00fcmmel,", "tokens": ["Der", "I\u00b7gua\u00b7no\u00b7don", ",", "der", "L\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wird frecher zu jeglicher Frist,", "tokens": ["Wird", "fre\u00b7cher", "zu", "jeg\u00b7li\u00b7cher", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Schon hat er am hellen Tage", "tokens": ["Schon", "hat", "er", "am", "hel\u00b7len", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Ichthyosaura gek\u00fc\u00dft.", "tokens": ["Die", "Ich\u00b7thy\u00b7o\u00b7sau\u00b7ra", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Mir ahnt eine Weltkatastrophe,", "tokens": ["Mir", "ahnt", "ei\u00b7ne", "Welt\u00b7ka\u00b7tast\u00b7ro\u00b7phe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "So kann es ja l\u00e4nger nicht gehn;", "tokens": ["So", "kann", "es", "ja", "l\u00e4n\u00b7ger", "nicht", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Was soll aus dem Lias noch werden,", "tokens": ["Was", "soll", "aus", "dem", "Lias", "noch", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ART", "NN", "ADV", "VAINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn solche Dinge geschehn?\u00ab", "tokens": ["Wenn", "sol\u00b7che", "Din\u00b7ge", "ge\u00b7schehn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "So klagte der Ichthyosaurus,", "tokens": ["So", "klag\u00b7te", "der", "Ich\u00b7thy\u00b7o\u00b7sau\u00b7rus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ward es ihm kreidig zumut';", "tokens": ["Da", "ward", "es", "ihm", "krei\u00b7dig", "zu\u00b7mut'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sein letzter Seufzer verhallte", "tokens": ["Sein", "letz\u00b7ter", "Seuf\u00b7zer", "ver\u00b7hall\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Qualmen und Zischen der Flut.", "tokens": ["Im", "Qual\u00b7men", "und", "Zi\u00b7schen", "der", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Es starb zu derselbigen Stunde", "tokens": ["Es", "starb", "zu", "der\u00b7sel\u00b7bi\u00b7gen", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die ganze Saurierei,", "tokens": ["Die", "gan\u00b7ze", "Sau\u00b7ri\u00b7e\u00b7rei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie kamen zu tief in die Kreide,", "tokens": ["Sie", "ka\u00b7men", "zu", "tief", "in", "die", "Krei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da war es nat\u00fcrlich vorbei.", "tokens": ["Da", "war", "es", "na\u00b7t\u00fcr\u00b7lich", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Und der uns hat gesungen", "tokens": ["Und", "der", "uns", "hat", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dies petrefaktische Lied,", "tokens": ["Dies", "pe\u00b7tre\u00b7fak\u00b7ti\u00b7sche", "Lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der fand's als fossiles Albumblatt", "tokens": ["Der", "fan\u00b7d's", "als", "fos\u00b7si\u00b7les", "Al\u00b7bum\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "KOUS", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auf einem Koprolith.", "tokens": ["Auf", "ei\u00b7nem", "Ko\u00b7pro\u00b7lith", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}