{"dta.poem.11642": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Einer verehrten Frau  \n  zum Geburtstage , mit einem Blumenstock.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, an solchen Tagen sey es Pflicht,", "tokens": ["Man", "sagt", ",", "an", "sol\u00b7chen", "Ta\u00b7gen", "sey", "es", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "PIAT", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich selber einen Spiegel vorzuhalten;", "tokens": ["Sich", "sel\u00b7ber", "ei\u00b7nen", "Spie\u00b7gel", "vor\u00b7zu\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bring' ihn Dir; verschm\u00e4h' dies Bl\u00fcmchen nicht,", "tokens": ["Ich", "bring'", "ihn", "Dir", ";", "ver\u00b7schm\u00e4h'", "dies", "Bl\u00fcm\u00b7chen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "PDS", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es soll Dir Deinen eignen Werth entfalten.", "tokens": ["Es", "soll", "Dir", "Dei\u00b7nen", "eig\u00b7nen", "Werth", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh' der bescheidenen Reseda Bl\u00fcthe,", "tokens": ["Sieh'", "der", "be\u00b7schei\u00b7de\u00b7nen", "Re\u00b7se\u00b7da", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Ein Bild der Menschenfreundlichkeit,", "tokens": ["Ein", "Bild", "der", "Men\u00b7schen\u00b7freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ohne Prunk, voll innerer Herzensg\u00fcte,", "tokens": ["Die", "oh\u00b7ne", "Prunk", ",", "voll", "in\u00b7ne\u00b7rer", "Her\u00b7zens\u00b7g\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Wohlgeruch der th\u00e4t'gen Liebe streut.", "tokens": ["Den", "Wohl\u00b7ge\u00b7ruch", "der", "th\u00e4t'\u00b7gen", "Lie\u00b7be", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}