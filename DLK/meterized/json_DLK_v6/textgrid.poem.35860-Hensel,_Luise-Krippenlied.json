{"textgrid.poem.35860": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Krippenlied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist das doch ein holdes Kind,", "tokens": ["Was", "ist", "das", "doch", "ein", "hol\u00b7des", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das man hier in der Krippe find't?", "tokens": ["Das", "man", "hier", "in", "der", "Krip\u00b7pe", "find't", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ach, solch ein s\u00fc\u00dfes Kindelein,", "tokens": ["Ach", ",", "solch", "ein", "s\u00fc\u00b7\u00dfes", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mu\u00df gewi\u00df vom Himmel sein.", "tokens": ["Das", "mu\u00df", "ge\u00b7wi\u00df", "vom", "Him\u00b7mel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Frau, die bei der Krippe kniet", "tokens": ["Die", "Frau", ",", "die", "bei", "der", "Krip\u00b7pe", "kniet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und selig auf das Kindlein sieht,", "tokens": ["Und", "se\u00b7lig", "auf", "das", "Kin\u00b7dlein", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist Maria, hehr und rein;", "tokens": ["Das", "ist", "Ma\u00b7ria", ",", "hehr", "und", "rein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr mag recht wohl im Herzen sein.", "tokens": ["Ihr", "mag", "recht", "wohl", "im", "Her\u00b7zen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Mann, der zu der Seite steht", "tokens": ["Der", "Mann", ",", "der", "zu", "der", "Sei\u00b7te", "steht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und still hinauf zum Himmel fleht,", "tokens": ["Und", "still", "hin\u00b7auf", "zum", "Him\u00b7mel", "fleht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das mu\u00df der fromme Joseph sein,", "tokens": ["Das", "mu\u00df", "der", "from\u00b7me", "Jo\u00b7se\u00b7ph", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NE", "VAINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der thut sich auch des Kindleins freun.", "tokens": ["Der", "thut", "sich", "auch", "des", "Kin\u00b7dleins", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und was dort in der Ecke liegt", "tokens": ["Und", "was", "dort", "in", "der", "E\u00b7cke", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nach dem Kindlein schaut vergn\u00fcgt,", "tokens": ["Und", "nach", "dem", "Kin\u00b7dlein", "schaut", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Oechslein und ein Eselein,", "tokens": ["Ein", "O\u00b7echs\u00b7lein", "und", "ein", "E\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Das m\u00f6gen gute Thierlein sein.", "tokens": ["Das", "m\u00f6\u00b7gen", "gu\u00b7te", "Thier\u00b7lein", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die dort kommen fromm und gut", "tokens": ["Und", "die", "dort", "kom\u00b7men", "fromm", "und", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit langem Stab und rundem Hut,", "tokens": ["Mit", "lan\u00b7gem", "Stab", "und", "run\u00b7dem", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist der Hirten fromme Schaar,", "tokens": ["Das", "ist", "der", "Hir\u00b7ten", "from\u00b7me", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die bringen ihre Gaben dar.", "tokens": ["Die", "brin\u00b7gen", "ih\u00b7re", "Ga\u00b7ben", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und was den Stall so helle macht", "tokens": ["Und", "was", "den", "Stall", "so", "hel\u00b7le", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was so lieblich singt und lacht,", "tokens": ["Und", "was", "so", "lieb\u00b7lich", "singt", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sind die lichten Engelein,", "tokens": ["Das", "sind", "die", "lich\u00b7ten", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die schau'n zu Th\u00fcr und Fenster ein.", "tokens": ["Die", "schau'n", "zu", "Th\u00fcr", "und", "Fens\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und die dort kommen ganz von fern", "tokens": ["Und", "die", "dort", "kom\u00b7men", "ganz", "von", "fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "VVFIN", "ADV", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gl\u00e4ubig schauen nach dem Stern,", "tokens": ["Und", "gl\u00e4u\u00b7big", "schau\u00b7en", "nach", "dem", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sind der heil'gen K\u00f6n'ge drei", "tokens": ["Das", "sind", "der", "heil'\u00b7gen", "K\u00f6n'\u00b7ge", "drei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Weihrauch, Gold und Spezerei.", "tokens": ["Mit", "Weih\u00b7rauch", ",", "Gold", "und", "Spe\u00b7ze\u00b7rei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und ob dem H\u00fcttlein flammt ein Stern,", "tokens": ["Und", "ob", "dem", "H\u00fctt\u00b7lein", "flammt", "ein", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der leuchtet nah und leuchtet fern,", "tokens": ["Der", "leuch\u00b7tet", "nah", "und", "leuch\u00b7tet", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er scheinet auch durch unsre Zeit", "tokens": ["Er", "schei\u00b7net", "auch", "durch", "uns\u00b7re", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leuchtet bis in Ewigkeit.", "tokens": ["Und", "leuch\u00b7tet", "bis", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sei hochgelobt, du dunkle Zell'!", "tokens": ["Sei", "hoch\u00b7ge\u00b7lobt", ",", "du", "dunk\u00b7le", "Zell'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch dich die ganze Welt wird hell.", "tokens": ["Durch", "dich", "die", "gan\u00b7ze", "Welt", "wird", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Klein Kindlein in der Jungfrau Schoo\u00df,", "tokens": ["Klein", "Kin\u00b7dlein", "in", "der", "Jung\u00b7frau", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie bist Du so unendlich gro\u00df!", "tokens": ["Wie", "bist", "Du", "so", "un\u00b7end\u00b7lich", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}