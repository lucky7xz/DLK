{"textgrid.poem.50690": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch ", "tokens": ["Durch"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Wird ein ", "tokens": ["Wird", "ein"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Steht aber dieser nur auf Schrauben,", "tokens": ["Steht", "a\u00b7ber", "die\u00b7ser", "nur", "auf", "Schrau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDAT", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wird er insgemein durch jene ganz zerst\u00f6rt.", "tokens": ["So", "wird", "er", "ins\u00b7ge\u00b7mein", "durch", "je\u00b7ne", "ganz", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PDS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Weltweisheit wirkt in dem Glauben", "tokens": ["Die", "Welt\u00b7weis\u00b7heit", "wirkt", "in", "dem", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das, was bei einem Feu'r der Wind ausrichten kann:", "tokens": ["Das", ",", "was", "bei", "ei\u00b7nem", "Feu'r", "der", "Wind", "aus\u00b7rich\u00b7ten", "kann", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}