{"textgrid.poem.55076": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Zigeunerlied", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Nebelgeriesel, im tiefen Schnee,", "tokens": ["Im", "Ne\u00b7bel\u00b7ge\u00b7rie\u00b7sel", ",", "im", "tie\u00b7fen", "Schnee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im wilden Wald, in der Winternacht,", "tokens": ["Im", "wil\u00b7den", "Wald", ",", "in", "der", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich h\u00f6rte der W\u00f6lfe Hungergeheul,", "tokens": ["Ich", "h\u00f6r\u00b7te", "der", "W\u00f6l\u00b7fe", "Hun\u00b7ger\u00b7ge\u00b7heul", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich' h\u00f6rte der Eulen Geschrei:", "tokens": ["Ich'", "h\u00f6r\u00b7te", "der", "Eu\u00b7len", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wille wau wau wau!", "tokens": ["Wil\u00b7le", "wau", "wau", "wau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ITJ", "ITJ", "ITJ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wille wo wo wo!", "tokens": ["Wil\u00b7le", "wo", "wo", "wo", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PWAV", "PWAV", "PWAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wito hu!", "tokens": ["Wi\u00b7to", "hu", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ich scho\u00df einmal eine Katz am Zaun,", "tokens": ["Ich", "scho\u00df", "ein\u00b7mal", "ei\u00b7ne", "Katz", "am", "Zaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Anne, der Hex, ihre schwarze, liebe Katz;", "tokens": ["Der", "An\u00b7ne", ",", "der", "Hex", ",", "ih\u00b7re", "schwar\u00b7ze", ",", "lie\u00b7be", "Katz", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da kamen des Nachts sieben Werw\u00f6lf zu mir,", "tokens": ["Da", "ka\u00b7men", "des", "Nachts", "sie\u00b7ben", "Wer\u00b7w\u00f6lf", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "CARD", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Waren sieben, sieben Weiber vom Dorf.", "tokens": ["Wa\u00b7ren", "sie\u00b7ben", ",", "sie\u00b7ben", "Wei\u00b7ber", "vom", "Dorf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "CARD", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Wille wau wau wau!", "tokens": ["Wil\u00b7le", "wau", "wau", "wau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ITJ", "ITJ", "ITJ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wille wo wo wo!", "tokens": ["Wil\u00b7le", "wo", "wo", "wo", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PWAV", "PWAV", "PWAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wito hu!", "tokens": ["Wi\u00b7to", "hu", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Ich kannte sie all, ich kannte sie wohl,", "tokens": ["Ich", "kann\u00b7te", "sie", "all", ",", "ich", "kann\u00b7te", "sie", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Anne, die Ursel, die K\u00e4th,", "tokens": ["Die", "An\u00b7ne", ",", "die", "Ur\u00b7sel", ",", "die", "K\u00e4\u00b7th", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Liese, die Barbe, die Ev, die Beth;", "tokens": ["Die", "Lie\u00b7se", ",", "die", "Bar\u00b7be", ",", "die", "Ev", ",", "die", "Be\u00b7th", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "NE", "$,", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie heulten im Kreise mich an.", "tokens": ["Sie", "heul\u00b7ten", "im", "Krei\u00b7se", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wille wau wau wau!", "tokens": ["Wil\u00b7le", "wau", "wau", "wau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ITJ", "ITJ", "ITJ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wille wo wo wo!", "tokens": ["Wil\u00b7le", "wo", "wo", "wo", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PWAV", "PWAV", "PWAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wito hu!", "tokens": ["Wi\u00b7to", "hu", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Da nannt ich sie alle bei Namen laut:", "tokens": ["Da", "nannt", "ich", "sie", "al\u00b7le", "bei", "Na\u00b7men", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIS", "APPR", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbwas willst du, Anne? was willst du, Beth?\u00ab", "tokens": ["\u00bb", "was", "willst", "du", ",", "An\u00b7ne", "?", "was", "willst", "du", ",", "Be\u00b7th", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "$,", "NE", "$.", "PWS", "VMFIN", "PPER", "$,", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da r\u00fcttelten sie sich, da sch\u00fcttelten sie sich", "tokens": ["Da", "r\u00fct\u00b7tel\u00b7ten", "sie", "sich", ",", "da", "sch\u00fct\u00b7tel\u00b7ten", "sie", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und liefen und heulten davon.", "tokens": ["Und", "lie\u00b7fen", "und", "heul\u00b7ten", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wille wau wau wau!", "tokens": ["Wil\u00b7le", "wau", "wau", "wau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ITJ", "ITJ", "ITJ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wille wo wo wo!", "tokens": ["Wil\u00b7le", "wo", "wo", "wo", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PWAV", "PWAV", "PWAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wito hu!", "tokens": ["Wi\u00b7to", "hu", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}