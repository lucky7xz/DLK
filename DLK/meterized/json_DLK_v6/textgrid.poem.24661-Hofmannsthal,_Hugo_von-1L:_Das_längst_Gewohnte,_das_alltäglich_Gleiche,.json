{"textgrid.poem.24661": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das l\u00e4ngst Gewohnte, das allt\u00e4glich Gleiche,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das l\u00e4ngst Gewohnte, das allt\u00e4glich Gleiche,", "tokens": ["Das", "l\u00e4ngst", "Ge\u00b7wohn\u00b7te", ",", "das", "all\u00b7t\u00e4g\u00b7lich", "Glei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "$,", "PRELS", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Auge adelt mirs zum Zauberreiche:", "tokens": ["Mein", "Au\u00b7ge", "a\u00b7delt", "mirs", "zum", "Zau\u00b7ber\u00b7rei\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es singt der Sturm sein grollend Lied f\u00fcr mich,", "tokens": ["Es", "singt", "der", "Sturm", "sein", "grol\u00b7lend", "Lied", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "ADJD", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr mich ergl\u00fcht die Rose, rauscht die Eiche.", "tokens": ["F\u00fcr", "mich", "er\u00b7gl\u00fcht", "die", "Ro\u00b7se", ",", "rauscht", "die", "Ei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Sonne spielt auf goldnem Frauenhaar", "tokens": ["Die", "Son\u00b7ne", "spielt", "auf", "gold\u00b7nem", "Frau\u00b7en\u00b7haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr mich \u2013 und Mondlicht auf dem stillen Teiche.", "tokens": ["F\u00fcr", "mich", "\u2013", "und", "Mond\u00b7licht", "auf", "dem", "stil\u00b7len", "Tei\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Seele les ich aus dem stummen Blick,", "tokens": ["Die", "See\u00b7le", "les", "ich", "aus", "dem", "stum\u00b7men", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und zu mir spricht die Stirn, die schweigend bleiche.", "tokens": ["Und", "zu", "mir", "spricht", "die", "Stirn", ",", "die", "schwei\u00b7gend", "blei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zum Traume sag ich: \u00bbBleib bei mir, sei wahr!\u00ab", "tokens": ["Zum", "Trau\u00b7me", "sag", "ich", ":", "\u00bb", "Bleib", "bei", "mir", ",", "sei", "wahr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "$(", "NN", "APPR", "PPER", "$,", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und zu der Wirklichkeit: \u00bbSei Traum, entweiche!\u00ab", "tokens": ["Und", "zu", "der", "Wirk\u00b7lich\u00b7keit", ":", "\u00bb", "Sei", "Traum", ",", "ent\u00b7wei\u00b7che", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$.", "$(", "VAFIN", "NN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das Wort, das Andern Scheidem\u00fcnze ist,", "tokens": ["Das", "Wort", ",", "das", "An\u00b7dern", "Schei\u00b7de\u00b7m\u00fcn\u00b7ze", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mir ists der Bilderquell, der flimmernd reiche.", "tokens": ["Mir", "ists", "der", "Bil\u00b7der\u00b7quell", ",", "der", "flim\u00b7mernd", "rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Was ich erkenne, ist mein Eigentum,", "tokens": ["Was", "ich", "er\u00b7ken\u00b7ne", ",", "ist", "mein", "Ei\u00b7gen\u00b7tum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und lieblich locket, was ich ", "tokens": ["Und", "lieb\u00b7lich", "lo\u00b7cket", ",", "was", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "PWS", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Der Rausch ist s\u00fc\u00df, den Geistertrank entflammt,", "tokens": ["Der", "Rausch", "ist", "s\u00fc\u00df", ",", "den", "Geis\u00b7ter\u00b7trank", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und s\u00fc\u00df ist die Erschlaffung auch, die weiche.", "tokens": ["Und", "s\u00fc\u00df", "ist", "die", "Er\u00b7schlaf\u00b7fung", "auch", ",", "die", "wei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "So tiefe Welten tun sich oft mir auf,", "tokens": ["So", "tie\u00b7fe", "Wel\u00b7ten", "tun", "sich", "oft", "mir", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PRF", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Da\u00df ich drein glanzgeblendet, z\u00f6gernd schleiche,", "tokens": ["Da\u00df", "ich", "drein", "glanz\u00b7ge\u00b7blen\u00b7det", ",", "z\u00f6\u00b7gernd", "schlei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und einen goldnen Reigen schlingt um mich", "tokens": ["Und", "ei\u00b7nen", "gold\u00b7nen", "Rei\u00b7gen", "schlingt", "um", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Das l\u00e4ngst Gewohnte, das allt\u00e4glich Gleiche.", "tokens": ["Das", "l\u00e4ngst", "Ge\u00b7wohn\u00b7te", ",", "das", "all\u00b7t\u00e4g\u00b7lich", "Glei\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "$,", "PRELS", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}