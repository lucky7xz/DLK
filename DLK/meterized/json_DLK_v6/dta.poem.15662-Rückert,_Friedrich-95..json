{"dta.poem.15662": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei scheinen sich so nah, und kommen nie zusammen;", "tokens": ["Zwei", "schei\u00b7nen", "sich", "so", "nah", ",", "und", "kom\u00b7men", "nie", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PRF", "ADV", "ADJD", "$,", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwei andre finden sich, die aus der Ferne stammen.", "tokens": ["Zwei", "and\u00b7re", "fin\u00b7den", "sich", ",", "die", "aus", "der", "Fer\u00b7ne", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "PIS", "VVFIN", "PRF", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ists? Wie Linien verhalten sich die Seelen;", "tokens": ["Was", "ists", "?", "Wie", "Li\u00b7ni\u00b7en", "ver\u00b7hal\u00b7ten", "sich", "die", "See\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "PWAV", "NE", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwei haben Neigungen, zwei bilden Parallelen.", "tokens": ["Zwei", "ha\u00b7ben", "Nei\u00b7gun\u00b7gen", ",", "zwei", "bil\u00b7den", "Par\u00b7al\u00b7le\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "NN", "$,", "CARD", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Gleichg\u00fcltig laufen die stets aneinander hin,", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "lau\u00b7fen", "die", "stets", "an\u00b7ein\u00b7an\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jene begegnen sich zuletzt in Einem Sinn.", "tokens": ["Je\u00b7ne", "be\u00b7geg\u00b7nen", "sich", "zu\u00b7letzt", "in", "Ei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}