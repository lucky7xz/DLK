{"textgrid.poem.54152": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Rote Signale", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vier Treppen links im Hinterhause", "tokens": ["Vier", "Trep\u00b7pen", "links", "im", "Hin\u00b7ter\u00b7hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "als Oberhaupt und Haustyrann", "tokens": ["als", "O\u00b7ber\u00b7haupt", "und", "Haus\u00b7ty\u00b7rann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "herrscht der Familienvater Krause", "tokens": ["herrscht", "der", "Fa\u00b7mi\u00b7li\u00b7en\u00b7va\u00b7ter", "Krau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und sieht sich seine Bude an:", "tokens": ["und", "sieht", "sich", "sei\u00b7ne", "Bu\u00b7de", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Een Haufen Nipps, \u2013 der Sch\u00f6nheit wejen \u2013", "tokens": ["E\u00b7en", "Hau\u00b7fen", "Nipps", ",", "\u2013", "der", "Sch\u00f6n\u00b7heit", "we\u00b7jen", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,", "$(", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "zwee Engel \u00fcberm Ehebett", "tokens": ["zwee", "En\u00b7gel", "\u00fc\u00b7berm", "E\u00b7he\u00b7bett"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "mit joldbesticktem Morjensejen", "tokens": ["mit", "jold\u00b7be\u00b7stick\u00b7tem", "Mor\u00b7jen\u00b7se\u00b7jen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "und eene Venus im Klosett.", "tokens": ["und", "e\u00b7e\u00b7ne", "Ve\u00b7nus", "im", "Klo\u00b7sett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Und neben Milit\u00e4randenken", "tokens": ["Und", "ne\u00b7ben", "Mi\u00b7li\u00b7t\u00e4\u00b7ran\u00b7den\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "mit sch\u00f6nem schwarzwei\u00dfrotem Band", "tokens": ["mit", "sch\u00f6\u00b7nem", "schwarz\u00b7wei\u00df\u00b7ro\u00b7tem", "Band"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "und Hochzeits- und Vereinsjeschenken", "tokens": ["und", "Hoch\u00b7zeits", "und", "Ver\u00b7eins\u00b7je\u00b7schen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "h\u00e4ngt einsam Lenin an der Wand.", "tokens": ["h\u00e4ngt", "ein\u00b7sam", "Le\u00b7nin", "an", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Blo\u00df wenn er an der Theke steht,", "tokens": ["Blo\u00df", "wenn", "er", "an", "der", "The\u00b7ke", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "ist Krause Sozialist:", "tokens": ["ist", "Krau\u00b7se", "So\u00b7zi\u00b7a\u00b7list", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "da schwitzt er Klassenk\u00e4mpfertum", "tokens": ["da", "schwitzt", "er", "Klas\u00b7sen\u00b7k\u00e4mp\u00b7fer\u00b7tum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "und schimpft uff B\u00fcrgermist.", "tokens": ["und", "schimpft", "uff", "B\u00fcr\u00b7ger\u00b7mist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Sonst fri\u00dft er sich ne Plauze ran,", "tokens": ["Sonst", "fri\u00dft", "er", "sich", "ne", "Plau\u00b7ze", "ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "ist fromm und gottergeben . . .", "tokens": ["ist", "fromm", "und", "got\u00b7ter\u00b7ge\u00b7ben", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Doch kommts nicht auf die Schnauze an,", "tokens": ["Doch", "kommts", "nicht", "auf", "die", "Schnau\u00b7ze", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "ihr m\u00fc\u00dft auch danach leben!", "tokens": ["ihr", "m\u00fc\u00dft", "auch", "da\u00b7nach", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}