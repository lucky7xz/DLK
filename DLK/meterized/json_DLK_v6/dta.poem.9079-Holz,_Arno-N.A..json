{"dta.poem.9079": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nes, gr\u00fcnes, weiches Gras.", "tokens": ["Sch\u00f6\u00b7nes", ",", "gr\u00fc\u00b7nes", ",", "wei\u00b7ches", "Gras", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drin liege ich.", "tokens": ["Drin", "lie\u00b7ge", "ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mitten zwischen Butterblumen!", "tokens": ["Mit\u00b7ten", "zwi\u00b7schen", "But\u00b7ter\u00b7blu\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ueber mir,", "tokens": ["Ue\u00b7ber", "mir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "warm,", "tokens": ["warm", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "der Himmel:", "tokens": ["der", "Him\u00b7mel", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "ein weites, zitterndes Weiss,", "tokens": ["ein", "wei\u00b7tes", ",", "zit\u00b7tern\u00b7des", "Weiss", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "das mir die Augen langsam, ganz langsam,", "tokens": ["das", "mir", "die", "Au\u00b7gen", "lang\u00b7sam", ",", "ganz", "lang\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "schliesst.", "tokens": ["schliesst", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Wehende Luft, . . . ein zartes Summen.", "tokens": ["We\u00b7hen\u00b7de", "Luft", ",", ".", ".", ".", "ein", "zar\u00b7tes", "Sum\u00b7men", "."], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "$.", "$.", "$.", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Nun bin ich fern", "tokens": ["Nun", "bin", "ich", "fern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "von jeder Welt,", "tokens": ["von", "je\u00b7der", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "ein sanftes Roth erf\u00fcllt mich ganz,", "tokens": ["ein", "sanf\u00b7tes", "Roth", "er\u00b7f\u00fcllt", "mich", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und deutlich sp\u00fcr ich,", "tokens": ["und", "deut\u00b7lich", "sp\u00fcr", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "wie die Sonne mir durchs Blut rinnt \u2014", "tokens": ["wie", "die", "Son\u00b7ne", "mir", "durchs", "Blut", "rinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "minutenlang.", "tokens": ["mi\u00b7nu\u00b7ten\u00b7lang", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Versunken Alles. Nur noch ich.", "tokens": ["Ver\u00b7sun\u00b7ken", "Al\u00b7les", ".", "Nur", "noch", "ich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$.", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Selig.", "tokens": ["Se\u00b7lig", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}