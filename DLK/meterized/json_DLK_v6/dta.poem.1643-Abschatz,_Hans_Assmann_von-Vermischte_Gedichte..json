{"dta.poem.1643": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer durch der andern Fallen ", "tokens": ["Wer", "durch", "der", "an\u00b7dern", "Fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu hohen Ehren steigt/", "tokens": ["Zu", "ho\u00b7hen", "Eh\u00b7ren", "steigt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist zu dem Fall geneigt:", "tokens": ["Ist", "zu", "dem", "Fall", "ge\u00b7neigt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Himmel wachet \u00fcber allen.", "tokens": ["Der", "Him\u00b7mel", "wa\u00b7chet", "\u00fc\u00b7ber", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Manch Ungl\u00fcck kan auch Kron und Thron vergallen.", "tokens": ["Manch", "Un\u00b7gl\u00fcck", "kan", "auch", "Kron", "und", "Thron", "ver\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer sich durch fremden Schaden", "tokens": ["Wer", "sich", "durch", "frem\u00b7den", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hoch an das Brett will ziehn/", "tokens": ["Hoch", "an", "das", "Brett", "will", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00e4llt vor der Zeit dahin.", "tokens": ["F\u00e4llt", "vor", "der", "Zeit", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Hertze mit viel Schuld beladen", "tokens": ["Ein", "Hert\u00b7ze", "mit", "viel", "Schuld", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "APPR", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bleibt bey dem Gl\u00fcck nicht lange Zeit in Gnaden.", "tokens": ["Bleibt", "bey", "dem", "Gl\u00fcck", "nicht", "lan\u00b7ge", "Zeit", "in", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKNEG", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}