{"textgrid.poem.67423": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Fritz Jacobi", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Mensch, ein armer S\u00fcnder;", "tokens": ["Ich", "bin", "ein", "Mensch", ",", "ein", "ar\u00b7mer", "S\u00fcn\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geh nicht, Jacobi, ins Gericht!", "tokens": ["Geh", "nicht", ",", "Ja\u00b7co\u00b7bi", ",", "ins", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "NE", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich denke nichts und schreib' noch minder,", "tokens": ["Ich", "den\u00b7ke", "nichts", "und", "schreib'", "noch", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil mir's an Zeit und Muth gebricht.", "tokens": ["Weil", "mir's", "an", "Zeit", "und", "Muth", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum denke Du und schreib so mehr!", "tokens": ["Drum", "den\u00b7ke", "Du", "und", "schreib", "so", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist doch eins, die L\u00e4ng' und Quer.", "tokens": ["Es", "ist", "doch", "eins", ",", "die", "L\u00e4ng'", "und", "Quer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}