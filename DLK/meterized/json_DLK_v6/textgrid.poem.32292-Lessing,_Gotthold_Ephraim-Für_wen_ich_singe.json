{"textgrid.poem.32292": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr wen ich singe", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich singe nicht f\u00fcr kleine Knaben,", "tokens": ["Ich", "sin\u00b7ge", "nicht", "f\u00fcr", "klei\u00b7ne", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die voller Stolz zur Schule gehn,", "tokens": ["Die", "vol\u00b7ler", "Stolz", "zur", "Schu\u00b7le", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und den Ovid in H\u00e4nden haben,", "tokens": ["Und", "den", "O\u00b7vid", "in", "H\u00e4n\u00b7den", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den ihre Lehrer nicht verstehn.", "tokens": ["Den", "ih\u00b7re", "Leh\u00b7rer", "nicht", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich singe nicht f\u00fcr euch, ihr Richter,", "tokens": ["Ich", "sin\u00b7ge", "nicht", "f\u00fcr", "euch", ",", "ihr", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr voll spitz'ger Gr\u00fcndlichkeit", "tokens": ["Die", "ihr", "voll", "spitz'\u00b7ger", "Gr\u00fcnd\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein unertr\u00e4glich Joch dem Dichter,", "tokens": ["Ein", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "Joch", "dem", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und euch die Muster selber seid.", "tokens": ["Und", "euch", "die", "Mus\u00b7ter", "sel\u00b7ber", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich singe nicht den k\u00fchnen Geistern,", "tokens": ["Ich", "sin\u00b7ge", "nicht", "den", "k\u00fch\u00b7nen", "Geis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die nur Homer und Milton reizt;", "tokens": ["Die", "nur", "Ho\u00b7mer", "und", "Mil\u00b7ton", "reizt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "KON", "NE", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Weil man den unersch\u00f6pften Meistern", "tokens": ["Weil", "man", "den", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Meis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lorbeern nur umsonst begeizt.", "tokens": ["Die", "Lor\u00b7beern", "nur", "um\u00b7sonst", "be\u00b7geizt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich singe nicht, durch Stolz gedrungen,", "tokens": ["Ich", "sin\u00b7ge", "nicht", ",", "durch", "Stolz", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr dich, mein deutsches Vaterland.", "tokens": ["F\u00fcr", "dich", ",", "mein", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fcrchte jene L\u00e4sterzungen,", "tokens": ["Ich", "f\u00fcrch\u00b7te", "je\u00b7ne", "L\u00e4s\u00b7ter\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die dich bis an den Pol verbannt.", "tokens": ["Die", "dich", "bis", "an", "den", "Pol", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich singe nicht f\u00fcr fremde Reiche.", "tokens": ["Ich", "sin\u00b7ge", "nicht", "f\u00fcr", "frem\u00b7de", "Rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie k\u00e4m' mir solch ein Ehrgeiz ein?", "tokens": ["Wie", "k\u00e4m'", "mir", "solch", "ein", "Ehr\u00b7geiz", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sind verwegne Autorstreiche.", "tokens": ["Das", "sind", "ver\u00b7weg\u00b7ne", "Au\u00b7tor\u00b7strei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mag nicht \u00fcbersetzet sein.", "tokens": ["Ich", "mag", "nicht", "\u00fc\u00b7bers\u00b7et\u00b7zet", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich singe nicht f\u00fcr fromme Schwestern,", "tokens": ["Ich", "sin\u00b7ge", "nicht", "f\u00fcr", "from\u00b7me", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die nie der Liebe Reiz gewinnt,", "tokens": ["Die", "nie", "der", "Lie\u00b7be", "Reiz", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die, wenn wir munter singen, l\u00e4stern,", "tokens": ["Die", ",", "wenn", "wir", "mun\u00b7ter", "sin\u00b7gen", ",", "l\u00e4s\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADJD", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir nicht alle Schmolcken sind.", "tokens": ["Da\u00df", "wir", "nicht", "al\u00b7le", "Schmol\u00b7cken", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich singe nur f\u00fcr euch, ihr Br\u00fcder,", "tokens": ["Ich", "sin\u00b7ge", "nur", "f\u00fcr", "euch", ",", "ihr", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr den Wein erhebt, wie ich.", "tokens": ["Die", "ihr", "den", "Wein", "er\u00b7hebt", ",", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr euch, f\u00fcr euch sind meine Lieder.", "tokens": ["F\u00fcr", "euch", ",", "f\u00fcr", "euch", "sind", "mei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Singt ihr sie nach: o Gl\u00fcck f\u00fcr mich!", "tokens": ["Singt", "ihr", "sie", "nach", ":", "o", "Gl\u00fcck", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$.", "FM", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich singe nur f\u00fcr meine Sch\u00f6ne,", "tokens": ["Ich", "sin\u00b7ge", "nur", "f\u00fcr", "mei\u00b7ne", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O muntre Phyllis, nur f\u00fcr dich.", "tokens": ["O", "mun\u00b7tre", "Phyl\u00b7lis", ",", "nur", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr dich, f\u00fcr dich sind meine T\u00f6ne.", "tokens": ["F\u00fcr", "dich", ",", "f\u00fcr", "dich", "sind", "mei\u00b7ne", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stehn sie dir an, so k\u00fcsse mich.", "tokens": ["Stehn", "sie", "dir", "an", ",", "so", "k\u00fcs\u00b7se", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}