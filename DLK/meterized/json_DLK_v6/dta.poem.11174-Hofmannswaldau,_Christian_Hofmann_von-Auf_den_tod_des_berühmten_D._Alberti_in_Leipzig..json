{"dta.poem.11174": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den tod des ber\u00fchmten D. Alberti  \n in Leipzig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich da ich deiner grufft die grabschrifft setzen will,", "tokens": ["Gleich", "da", "ich", "dei\u00b7ner", "grufft", "die", "grab\u00b7schrifft", "set\u00b7zen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So stie\u00df Apollo mich, und wolt es gar nicht haben.", "tokens": ["So", "stie\u00df", "A\u00b7pol\u00b7lo", "mich", ",", "und", "wolt", "es", "gar", "nicht", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPER", "$,", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch er bedachte sich; Schreib, sprach er, nur so viel:", "tokens": ["Doch", "er", "be\u00b7dach\u00b7te", "sich", ";", "Schreib", ",", "sprach", "er", ",", "nur", "so", "viel", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "$.", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit dir liegt Schlesien in Meissenland begraben.", "tokens": ["Mit", "dir", "liegt", "Schle\u00b7si\u00b7en", "in", "Meis\u00b7sen\u00b7land", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NE", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}