{"textgrid.poem.54558": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Held/ den die Tapfferkeit/ Held den die Kunst zu kriegen/", "tokens": ["Held", "/", "den", "die", "Tapf\u00b7fer\u00b7keit", "/", "Held", "den", "die", "Kunst", "zu", "krie\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ART", "NN", "$(", "NN", "ART", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Der treffliche Verstand/ des alten Hauses Pracht/", "tokens": ["Der", "treff\u00b7li\u00b7che", "Ver\u00b7stand", "/", "des", "al\u00b7ten", "Hau\u00b7ses", "Pracht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und das verkn\u00fcpffte Gl\u00fcck mit Tugend/ herrlich macht/", "tokens": ["Und", "das", "ver\u00b7kn\u00fcpff\u00b7te", "Gl\u00fcck", "mit", "Tu\u00b7gend", "/", "herr\u00b7lich", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "$(", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der durch Blut gesucht in Flamm und Blut zu siegen;", "tokens": ["Und", "der", "durch", "Blut", "ge\u00b7sucht", "in", "Flamm", "und", "Blut", "zu", "sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "VVPP", "APPR", "NE", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schau an in seinem Blut gest\u00fcrtzt vom Throne liegen/", "tokens": ["Schau", "an", "in", "sei\u00b7nem", "Blut", "ge\u00b7st\u00fcrtzt", "vom", "Thro\u00b7ne", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Britten hohes Haupt/ indem das Reich erkracht/", "tokens": ["Der", "Brit\u00b7ten", "ho\u00b7hes", "Haupt", "/", "in\u00b7dem", "das", "Reich", "er\u00b7kracht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "KOUS", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das eines Henckers Beil auffs K\u00f6nigs Hals gebracht/", "tokens": ["Das", "ei\u00b7nes", "Hen\u00b7ckers", "Beil", "auffs", "K\u00f6\u00b7nigs", "Hals", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "APPRART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit dessen Seel auch Ruh/ und Zucht/ und Recht verfliegen/", "tokens": ["Mit", "des\u00b7sen", "Seel", "auch", "Ruh", "/", "und", "Zucht", "/", "und", "Recht", "ver\u00b7flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "NN", "$(", "KON", "NN", "$(", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die todte Majest\u00e4t/ die auf das Mord-Klotz f\u00e4llt/", "tokens": ["Die", "tod\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t", "/", "die", "auf", "das", "Mord\u00b7Klotz", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Beschw\u00e4rtzt das weisse Land/ und schreckt die grosse Welt/", "tokens": ["Be\u00b7schw\u00e4rtzt", "das", "weis\u00b7se", "Land", "/", "und", "schreckt", "die", "gros\u00b7se", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die sich von Britten sucht weit mehr denn vor zu schneiden/", "tokens": ["Die", "sich", "von", "Brit\u00b7ten", "sucht", "weit", "mehr", "denn", "vor", "zu", "schnei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "ADJD", "ADV", "ADV", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Unschuld/ die den Geist in solchem Hohn auffgiebt/", "tokens": ["Die", "Un\u00b7schuld", "/", "die", "den", "Geist", "in", "sol\u00b7chem", "Hohn", "auff\u00b7giebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erfordert was gerecht/ und rechte Waffen liebt/", "tokens": ["Er\u00b7for\u00b7dert", "was", "ge\u00b7recht", "/", "und", "rech\u00b7te", "Waf\u00b7fen", "liebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "ADJD", "$(", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zu r\u00e4chen diesen Fall. Heer Schwerdter aus der Scheiden!", "tokens": ["Zu", "r\u00e4\u00b7chen", "die\u00b7sen", "Fall", ".", "Heer", "Schwerd\u00b7ter", "aus", "der", "Schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "NN", "$.", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}