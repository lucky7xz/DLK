{"textgrid.poem.65419": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Nachgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie das Meer noch braust am Morgen, wenn zu Nacht ein Sturm geweht,", "tokens": ["Wie", "das", "Meer", "noch", "braust", "am", "Mor\u00b7gen", ",", "wenn", "zu", "Nacht", "ein", "Sturm", "ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "$,", "KOUS", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie ihr lange nach dem Regen noch die Blume zittern seht,", "tokens": ["Wie", "ihr", "lan\u00b7ge", "nach", "dem", "Re\u00b7gen", "noch", "die", "Blu\u00b7me", "zit\u00b7tern", "seht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Also wogt es ganze Tage mir im Herzen tief und hoch,", "tokens": ["Al\u00b7so", "wogt", "es", "gan\u00b7ze", "Ta\u00b7ge", "mir", "im", "Her\u00b7zen", "tief", "und", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "PPER", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wenn mir deine Lock' im Traume streifend um den Busen flog.", "tokens": ["Wenn", "mir", "dei\u00b7ne", "Lock'", "im", "Trau\u00b7me", "strei\u00b7fend", "um", "den", "Bu\u00b7sen", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}