{"dta.poem.23556": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lapidarstil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist das Deutsch schon so verdorben,", "tokens": ["Ist", "das", "Deutsch", "schon", "so", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df man's kaum noch schreiben kann?", "tokens": ["Da\u00df", "man's", "kaum", "noch", "schrei\u00b7ben", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder ist es ausgestorben,", "tokens": ["O\u00b7der", "ist", "es", "aus\u00b7ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df man's spricht nur dann und wann?", "tokens": ["Da\u00df", "man's", "spricht", "nur", "dann", "und", "wann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "ADV", "KON", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Oder habet ihr vernommen,", "tokens": ["O\u00b7der", "ha\u00b7bet", "ihr", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df es bald zu Ende geht?", "tokens": ["Da\u00df", "es", "bald", "zu", "En\u00b7de", "geht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Zeiten n\u00e4chstens kommen,", "tokens": ["Da\u00df", "die", "Zei\u00b7ten", "n\u00e4chs\u00b7tens", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo kein Mensch mehr deutsch versteht?", "tokens": ["Wo", "kein", "Mensch", "mehr", "deutsch", "ver\u00b7steht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedes Denkmal wird frisieret", "tokens": ["Je\u00b7des", "Denk\u00b7mal", "wird", "fri\u00b7sie\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Von der Philologen Hand,", "tokens": ["Von", "der", "Phi\u00b7lo\u00b7lo\u00b7gen", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und so haben sie beschmieret", "tokens": ["Und", "so", "ha\u00b7ben", "sie", "be\u00b7schmie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erz und Stein und Tisch und Wand.", "tokens": ["Erz", "und", "Stein", "und", "Tisch", "und", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo man hinschaut, strotzt und glotzet", "tokens": ["Wo", "man", "hin\u00b7schaut", ",", "strotzt", "und", "glot\u00b7zet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVPP", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eine Inschrift in Latein,", "tokens": ["Ei\u00b7ne", "In\u00b7schrift", "in", "La\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Die sich trotzig hat schmarotzet", "tokens": ["Die", "sich", "trot\u00b7zig", "hat", "schma\u00b7rot\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADJD", "VAFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das Denkmal mit hinein.", "tokens": ["In", "das", "Denk\u00b7mal", "mit", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deutsches Volk, du musst studieren", "tokens": ["Deut\u00b7sches", "Volk", ",", "du", "musst", "stu\u00b7die\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vor allem das Latein,", "tokens": ["Und", "vor", "al\u00b7lem", "das", "La\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Niemals kannst du sonst capieren", "tokens": ["Nie\u00b7mals", "kannst", "du", "sonst", "ca\u00b7pie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dein eigner Ruhm soll sein!", "tokens": ["Was", "dein", "eig\u00b7ner", "Ruhm", "soll", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}