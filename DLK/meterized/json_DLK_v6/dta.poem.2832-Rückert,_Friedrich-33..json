{"dta.poem.2832": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00e4mpe wappne sich, eh er zum Kampfe geht;", "tokens": ["Der", "K\u00e4m\u00b7pe", "wapp\u00b7ne", "sich", ",", "eh", "er", "zum", "Kamp\u00b7fe", "geht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist zu sp\u00e4t, wann er in Feindes Mitten steht.", "tokens": ["Es", "ist", "zu", "sp\u00e4t", ",", "wann", "er", "in", "Fein\u00b7des", "Mit\u00b7ten", "steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PWAV", "PPER", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So mit Grunds\u00e4tzen magst du wappnen dich und schirmen", "tokens": ["So", "mit", "Grund\u00b7s\u00e4t\u00b7zen", "magst", "du", "wapp\u00b7nen", "dich", "und", "schir\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Vor Leidenschaften, eh sie selber dich best\u00fcrmen.", "tokens": ["Vor", "Lei\u00b7den\u00b7schaf\u00b7ten", ",", "eh", "sie", "sel\u00b7ber", "dich", "be\u00b7st\u00fcr\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Oft leider wird auch so, was du bei kaltem Blut", "tokens": ["Oft", "lei\u00b7der", "wird", "auch", "so", ",", "was", "du", "bei", "kal\u00b7tem", "Blut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ADV", "$,", "PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir nahmest vor als Schild, zerschmelzen in der Glut.", "tokens": ["Dir", "nah\u00b7mest", "vor", "als", "Schild", ",", "zer\u00b7schmel\u00b7zen", "in", "der", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KOUS", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}