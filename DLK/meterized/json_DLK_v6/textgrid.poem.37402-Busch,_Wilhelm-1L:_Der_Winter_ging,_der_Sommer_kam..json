{"textgrid.poem.37402": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Winter ging, der Sommer kam.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Winter ging, der Sommer kam.", "tokens": ["Der", "Win\u00b7ter", "ging", ",", "der", "Som\u00b7mer", "kam", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er bringt aufs neue wieder", "tokens": ["Er", "bringt", "aufs", "neu\u00b7e", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den vielbeliebten Wunderkram", "tokens": ["Den", "viel\u00b7be\u00b7lieb\u00b7ten", "Wun\u00b7der\u00b7kram"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Blumen und der Lieder.", "tokens": ["Der", "Blu\u00b7men", "und", "der", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie das so wechselt Jahr um Jahr,", "tokens": ["Wie", "das", "so", "wech\u00b7selt", "Jahr", "um", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Betracht ich fast mit Sorgen.", "tokens": ["Be\u00b7tracht", "ich", "fast", "mit", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was lebte, starb, was ist, es war,", "tokens": ["Was", "leb\u00b7te", ",", "starb", ",", "was", "ist", ",", "es", "war", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "$,", "PWS", "VAFIN", "$,", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und heute wird zu morgen.", "tokens": ["Und", "heu\u00b7te", "wird", "zu", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Stets mu\u00df die Bildnerin Natur", "tokens": ["Stets", "mu\u00df", "die", "Bild\u00b7ne\u00b7rin", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den alten Ton ben\u00fctzen", "tokens": ["Den", "al\u00b7ten", "Ton", "be\u00b7n\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Haus und Garten, Wald und Flur", "tokens": ["In", "Haus", "und", "Gar\u00b7ten", ",", "Wald", "und", "Flur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihren neuen Skizzen.", "tokens": ["Zu", "ih\u00b7ren", "neu\u00b7en", "Skiz\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}