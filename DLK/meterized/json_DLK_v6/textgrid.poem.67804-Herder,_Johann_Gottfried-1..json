{"textgrid.poem.67804": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traurend tief sa\u00df Don Diego,", "tokens": ["Trau\u00b7rend", "tief", "sa\u00df", "Don", "Die\u00b7go", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl war keiner je so traurig;", "tokens": ["Wohl", "war", "kei\u00b7ner", "je", "so", "trau\u00b7rig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gramvoll dacht er Tag' und N\u00e4chte", "tokens": ["Gram\u00b7voll", "dacht", "er", "Tag'", "und", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur an seines Hauses Schmach.", "tokens": ["Nur", "an", "sei\u00b7nes", "Hau\u00b7ses", "Schmach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An die Schmach des edlen alten", "tokens": ["An", "die", "Schmach", "des", "ed\u00b7len", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tapfern Hauses der von Lainez,", "tokens": ["Tap\u00b7fern", "Hau\u00b7ses", "der", "von", "Lai\u00b7nez", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das die Inigos an Ruhme,", "tokens": ["Das", "die", "I\u00b7ni\u00b7gos", "an", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Abarcos \u00fcbertraf.", "tokens": ["Die", "A\u00b7bar\u00b7cos", "\u00fc\u00b7bert\u00b7raf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tief gekr\u00e4nket, schwach vor Alter,", "tokens": ["Tief", "ge\u00b7kr\u00e4n\u00b7ket", ",", "schwach", "vor", "Al\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlt' er nahe sich dem Grabe,", "tokens": ["F\u00fchlt'", "er", "na\u00b7he", "sich", "dem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da indes sein Feind Don Gormaz", "tokens": ["Da", "in\u00b7des", "sein", "Feind", "Don", "Gor\u00b7maz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "NE", "NE"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.4": {"text": "Ohne Gegner triumphiert.", "tokens": ["Oh\u00b7ne", "Geg\u00b7ner", "tri\u00b7um\u00b7phiert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sonder Schlaf und sonder Speise,", "tokens": ["Son\u00b7der", "Schlaf", "und", "son\u00b7der", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4get er die Augen nieder,", "tokens": ["Schl\u00e4\u00b7get", "er", "die", "Au\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tritt nicht \u00fcber seine Schwelle,", "tokens": ["Tritt", "nicht", "\u00fc\u00b7ber", "sei\u00b7ne", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spricht mit seinen Freunden nicht,", "tokens": ["Spricht", "mit", "sei\u00b7nen", "Freun\u00b7den", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6ret nicht der Freunde Zuspruch,", "tokens": ["H\u00f6\u00b7ret", "nicht", "der", "Freun\u00b7de", "Zu\u00b7spruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sie kommen, ihn zu tr\u00f6sten;", "tokens": ["Wenn", "sie", "kom\u00b7men", ",", "ihn", "zu", "tr\u00f6s\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Atem des Entehrten,", "tokens": ["Denn", "der", "A\u00b7tem", "des", "Ent\u00b7ehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaubt' er, sch\u00e4nde seinen Freund.", "tokens": ["Glaubt'", "er", ",", "sch\u00e4n\u00b7de", "sei\u00b7nen", "Freund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Endlich sch\u00fcttelt er die B\u00fcrde", "tokens": ["End\u00b7lich", "sch\u00fct\u00b7telt", "er", "die", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Los des grausam-stummen Grames,", "tokens": ["Los", "des", "grau\u00b7sam\u00b7stum\u00b7men", "Gra\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4sset kommen seine S\u00f6hne,", "tokens": ["L\u00e4s\u00b7set", "kom\u00b7men", "sei\u00b7ne", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber spricht zu ihnen nicht;", "tokens": ["A\u00b7ber", "spricht", "zu", "ih\u00b7nen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bindet ihrer aller H\u00e4nde", "tokens": ["Bin\u00b7det", "ih\u00b7rer", "al\u00b7ler", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ernst und fest mit starken Banden;", "tokens": ["Ernst", "und", "fest", "mit", "star\u00b7ken", "Ban\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle, Tr\u00e4nen in den Augen,", "tokens": ["Al\u00b7le", ",", "Tr\u00e4\u00b7nen", "in", "den", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flehen um Barmherzigkeit.", "tokens": ["Fle\u00b7hen", "um", "Barm\u00b7her\u00b7zig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Fast schon ist er ohne Hoffnung,", "tokens": ["Fast", "schon", "ist", "er", "oh\u00b7ne", "Hoff\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als der j\u00fcngste seiner S\u00f6hne,", "tokens": ["Als", "der", "j\u00fcngs\u00b7te", "sei\u00b7ner", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Don Rodrigo, seinem Mute", "tokens": ["Don", "Rod\u00b7ri\u00b7go", ",", "sei\u00b7nem", "Mu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freud und Hoffnung wiedergab.", "tokens": ["Freud", "und", "Hoff\u00b7nung", "wie\u00b7der\u00b7gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mit entflammten Tigeraugen", "tokens": ["Mit", "ent\u00b7flamm\u00b7ten", "Ti\u00b7ger\u00b7au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt er von dem Vater r\u00fcckw\u00e4rts;", "tokens": ["Tritt", "er", "von", "dem", "Va\u00b7ter", "r\u00fcck\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbvater\u00ab, spricht er, \u00bbIhr vergesset,", "tokens": ["\u00bb", "va\u00b7ter", "\u00ab", ",", "spricht", "er", ",", "\u00bb", "Ihr", "ver\u00b7ges\u00b7set", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer Ihr seid und wer ich bin.", "tokens": ["Wer", "Ihr", "seid", "und", "wer", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "KON", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "H\u00e4tt ich nicht aus Euren H\u00e4nden", "tokens": ["H\u00e4tt", "ich", "nicht", "aus", "Eu\u00b7ren", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Waffenwehr empfangen,", "tokens": ["Mei\u00b7ne", "Waf\u00b7fen\u00b7wehr", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahndet ich mit einem Dolche", "tokens": ["Ahn\u00b7det", "ich", "mit", "ei\u00b7nem", "Dol\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mir jetzt gebotne Schmach.\u00ab", "tokens": ["Die", "mir", "jetzt", "ge\u00b7bot\u00b7ne", "Schmach", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Str\u00f6mend flossen Freudentr\u00e4nen", "tokens": ["Str\u00f6\u00b7mend", "flos\u00b7sen", "Freu\u00b7den\u00b7tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die v\u00e4terlichen Wangen;", "tokens": ["Auf", "die", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbdu\u00ab, sprach er, den Sohn umarmend,", "tokens": ["\u00bb", "du", "\u00ab", ",", "sprach", "er", ",", "den", "Sohn", "um\u00b7ar\u00b7mend", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$(", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbdu, Rodrigo, bist mein Sohn.", "tokens": ["\u00bb", "du", ",", "Rod\u00b7ri\u00b7go", ",", "bist", "mein", "Sohn", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "NE", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Ruhe gibt dein Zorn mir wieder,", "tokens": ["Ru\u00b7he", "gibt", "dein", "Zorn", "mir", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Schmerzen heilt dein Unmut.", "tokens": ["Mei\u00b7ne", "Schmer\u00b7zen", "heilt", "dein", "Un\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gegen mich nicht, deinen Vater,", "tokens": ["Ge\u00b7gen", "mich", "nicht", ",", "dei\u00b7nen", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen unsres Hauses Feind", "tokens": ["Ge\u00b7gen", "uns\u00b7res", "Hau\u00b7ses", "Feind"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Hebe sich dein Arm!\u00ab \u2013 \u00bbWo ist er?\u00ab", "tokens": ["He\u00b7be", "sich", "dein", "Arm", "!", "\u00ab", "\u2013", "\u00bb", "Wo", "ist", "er", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$.", "$(", "$(", "$(", "PWAV", "VAFIN", "PPER", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief Rodrigo, \u00bbwer entehret", "tokens": ["Rief", "Rod\u00b7ri\u00b7go", ",", "\u00bb", "wer", "en\u00b7teh\u00b7ret"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "$(", "PWS", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Unser Haus?\u00ab Er lie\u00df dem Vater", "tokens": ["Un\u00b7ser", "Haus", "?", "\u00ab", "Er", "lie\u00df", "dem", "Va\u00b7ter"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum, es zu erz\u00e4hlen, Zeit.", "tokens": ["Kaum", ",", "es", "zu", "er\u00b7z\u00e4h\u00b7len", ",", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "PTKZU", "VVINF", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}