{"textgrid.poem.48548": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Auf die Weise des 101. Psalms", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mich mit mir nun ganz besprochen.", "tokens": ["Ich", "ha\u00b7be", "mich", "mit", "mir", "nun", "ganz", "be\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der leichten Welt ihr Gutsein oder Pochen", "tokens": ["Der", "leich\u00b7ten", "Welt", "ihr", "Gut\u00b7sein", "o\u00b7der", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "soll mich forthin nicht weder krank noch froh", "tokens": ["soll", "mich", "for\u00b7thin", "nicht", "we\u00b7der", "krank", "noch", "froh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "KON", "ADJD", "ADV", "ADJD"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "mehr machen so.", "tokens": ["mehr", "ma\u00b7chen", "so", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Was ist es mehr mit allen ihren G\u00fcnsten,", "tokens": ["Was", "ist", "es", "mehr", "mit", "al\u00b7len", "ih\u00b7ren", "G\u00fcns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die, Nebeln gleich und leichten leeren D\u00fcnsten,", "tokens": ["die", ",", "Ne\u00b7beln", "gleich", "und", "leich\u00b7ten", "lee\u00b7ren", "D\u00fcns\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "ADV", "KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so bald sie nur und eh' sie fast entstehn,", "tokens": ["so", "bald", "sie", "nur", "und", "eh'", "sie", "fast", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "KON", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "so bald vergehn?", "tokens": ["so", "bald", "ver\u00b7gehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So kan mir auch ihr Zorn auf mehr nicht schaden,", "tokens": ["So", "kan", "mir", "auch", "ihr", "Zorn", "auf", "mehr", "nicht", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "als da\u00df er mir zerrei\u00dft des Lebens Faden.", "tokens": ["als", "da\u00df", "er", "mir", "zer\u00b7rei\u00dft", "des", "Le\u00b7bens", "Fa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Not, Armut, Pein und was sie T\u00f6tlichs dreut", "tokens": ["Not", ",", "Ar\u00b7mut", ",", "Pein", "und", "was", "sie", "T\u00f6t\u00b7lichs", "dreut"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "PWS", "PPER", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "scheint nur ein Leid.", "tokens": ["scheint", "nur", "ein", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Sie mag mich denn gleich sieden oder braten,", "tokens": ["Sie", "mag", "mich", "denn", "gleich", "sie\u00b7den", "o\u00b7der", "bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "sie mu\u00df mich doch der Seelen lassen raten,", "tokens": ["sie", "mu\u00df", "mich", "doch", "der", "See\u00b7len", "las\u00b7sen", "ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die ihren Grimm, wie sauer sie ihn macht,", "tokens": ["die", "ih\u00b7ren", "Grimm", ",", "wie", "sau\u00b7er", "sie", "ihn", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NE", "$,", "PWAV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "doch nur verlacht.", "tokens": ["doch", "nur", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Gott, der ist der, der nicht nur hie mit N\u00f6ten,", "tokens": ["Gott", ",", "der", "ist", "der", ",", "der", "nicht", "nur", "hie", "mit", "N\u00f6\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VAFIN", "ART", "$,", "PRELS", "PTKNEG", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "der dort auch kan mit H\u00f6llen\u00e4ngsten t\u00f6ten,", "tokens": ["der", "dort", "auch", "kan", "mit", "H\u00f6l\u00b7len\u00b7\u00e4ngs\u00b7ten", "t\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da Leib und Geist durch einen Tod verdirbt,", "tokens": ["da", "Leib", "und", "Geist", "durch", "ei\u00b7nen", "Tod", "ver\u00b7dirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der nimmer stirbt.", "tokens": ["der", "nim\u00b7mer", "stirbt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "La\u00df keine Furcht, als deine, mich erschrecken,", "tokens": ["La\u00df", "kei\u00b7ne", "Furcht", ",", "als", "dei\u00b7ne", ",", "mich", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "$,", "KOUS", "PPOSAT", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "o der du uns vom Tode wirst erwecken!", "tokens": ["o", "der", "du", "uns", "vom", "To\u00b7de", "wirst", "er\u00b7we\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ART", "PPER", "PRF", "APPRART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df, H\u00f6chster, mich bewegen keine Lust,", "tokens": ["La\u00df", ",", "H\u00f6chs\u00b7ter", ",", "mich", "be\u00b7we\u00b7gen", "kei\u00b7ne", "Lust", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "als die du tust!", "tokens": ["als", "die", "du", "tust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Enthalte mich in diesen tr\u00fcben Zeiten,", "tokens": ["Ent\u00b7hal\u00b7te", "mich", "in", "die\u00b7sen", "tr\u00fc\u00b7ben", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da Gut und B\u00f6s in vollen L\u00e4rmen streiten,", "tokens": ["da", "Gut", "und", "B\u00f6s", "in", "vol\u00b7len", "L\u00e4r\u00b7men", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da falscher Schein mit wahrer Sch\u00f6nheit kriegt", "tokens": ["da", "fal\u00b7scher", "Schein", "mit", "wah\u00b7rer", "Sch\u00f6n\u00b7heit", "kriegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und fast wie siegt!", "tokens": ["und", "fast", "wie", "siegt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "O wahre Zier, itzt lenke meine Sinnen,", "tokens": ["O", "wah\u00b7re", "Zier", ",", "itzt", "len\u00b7ke", "mei\u00b7ne", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df sie nur dich alleine lieb gewinnen!", "tokens": ["da\u00df", "sie", "nur", "dich", "al\u00b7lei\u00b7ne", "lieb", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Itzt wend auf nichts als dich nur meinen Mut,", "tokens": ["Itzt", "wend", "auf", "nichts", "als", "dich", "nur", "mei\u00b7nen", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PIS", "KOKOM", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "o h\u00f6chstes Gut!", "tokens": ["o", "h\u00f6chs\u00b7tes", "Gut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Ich will forthin mit streitenden Gebeten", "tokens": ["Ich", "will", "for\u00b7thin", "mit", "strei\u00b7ten\u00b7den", "Ge\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die b\u00f6se Welt zu meinen F\u00fc\u00dfen treten,", "tokens": ["die", "b\u00f6\u00b7se", "Welt", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "will blos auf den mein Herze stellen hin,", "tokens": ["will", "blos", "auf", "den", "mein", "Her\u00b7ze", "stel\u00b7len", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "von dem ich bin.", "tokens": ["von", "dem", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Er meint es gut und will mich nur versuchen,", "tokens": ["Er", "meint", "es", "gut", "und", "will", "mich", "nur", "ver\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ob ich ihn wol will segnen oder fluchen;", "tokens": ["ob", "ich", "ihn", "wol", "will", "seg\u00b7nen", "o\u00b7der", "flu\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "er ist gerecht, wird mehr nicht, als ich kan,", "tokens": ["er", "ist", "ge\u00b7recht", ",", "wird", "mehr", "nicht", ",", "als", "ich", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VAFIN", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mir muten an.", "tokens": ["mir", "mu\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Was er legt auf, das hilft er selbsten tragen", "tokens": ["Was", "er", "legt", "auf", ",", "das", "hilft", "er", "selbs\u00b7ten", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "PTKVZ", "$,", "PDS", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und macht es so mit allen unsern Plagen,", "tokens": ["und", "macht", "es", "so", "mit", "al\u00b7len", "un\u00b7sern", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df endlich anders man nicht sagen kan", "tokens": ["da\u00df", "end\u00b7lich", "an\u00b7ders", "man", "nicht", "sa\u00b7gen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "PIS", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "als: wol getan!", "tokens": ["als", ":", "wol", "ge\u00b7tan", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$.", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}