{"dta.poem.8030": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edarum ward ein Weg betreten", "tokens": ["\u201e", "da\u00b7rum", "ward", "ein", "Weg", "be\u00b7tre\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PAV", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4ngst von Pilgern, S\u00e4ngern, Wappnern,", "tokens": ["L\u00e4ngst", "von", "Pil\u00b7gern", ",", "S\u00e4n\u00b7gern", ",", "Wapp\u00b7nern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum ward ein Schlo\u00df erbauet,", "tokens": ["Da\u00b7rum", "ward", "ein", "Schlo\u00df", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Herrlich, an des Weges Rande,", "tokens": ["Herr\u00b7lich", ",", "an", "des", "We\u00b7ges", "Ran\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum schaute von den Zinnen", "tokens": ["Da\u00b7rum", "schau\u00b7te", "von", "den", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis auf mich wohl manche Dame:", "tokens": ["Bis", "auf", "mich", "wohl", "man\u00b7che", "Da\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Weil der sch\u00f6nste, k\u00fchnste Ritter", "tokens": ["Weil", "der", "sch\u00f6ns\u00b7te", ",", "k\u00fchns\u00b7te", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sollte hier vor\u00fcberfahren.", "tokens": ["Soll\u00b7te", "hier", "vor\u00b7\u00fc\u00b7berf\u00b7ah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wehe nun! es ist erf\u00fcllt,", "tokens": ["We\u00b7he", "nun", "!", "es", "ist", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Was so lange ward erharret.", "tokens": ["Was", "so", "lan\u00b7ge", "ward", "er\u00b7har\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Weh! die Augen werden brechen,", "tokens": ["Weh", "!", "die", "Au\u00b7gen", "wer\u00b7den", "bre\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Die so hohen Adel sahen.", "tokens": ["Die", "so", "ho\u00b7hen", "A\u00b7del", "sa\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Weh! die Mauern werden sinken,", "tokens": ["Weh", "!", "die", "Mau\u00b7ern", "wer\u00b7den", "sin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Drin des Rosses Tritt verhallet.", "tokens": ["Drin", "des", "Ros\u00b7ses", "Tritt", "ver\u00b7hal\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Weh! der Pfad, den er verlie\u00df,", "tokens": ["Weh", "!", "der", "Pfad", ",", "den", "er", "ver\u00b7lie\u00df", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Wird vergehn in hohem Grase.\u201c", "tokens": ["Wird", "ver\u00b7gehn", "in", "ho\u00b7hem", "Gra\u00b7se", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "VVINF", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}