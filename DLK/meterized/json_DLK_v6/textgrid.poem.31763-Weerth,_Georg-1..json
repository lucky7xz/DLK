{"textgrid.poem.31763": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein sch\u00f6ner Ding ist auf der Welt,", "tokens": ["Kein", "sch\u00f6\u00b7ner", "Ding", "ist", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als seine Feinde zu bei\u00dfen,", "tokens": ["Als", "sei\u00b7ne", "Fein\u00b7de", "zu", "bei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als \u00fcber all die plumpen Geselln", "tokens": ["Als", "\u00fc\u00b7ber", "all", "die", "plum\u00b7pen", "Ge\u00b7selln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Seine lustigen Witze zu rei\u00dfen.", "tokens": ["Sei\u00b7ne", "lus\u00b7ti\u00b7gen", "Wit\u00b7ze", "zu", "rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So dacht ich und stimmte die Saiten schon:", "tokens": ["So", "dacht", "ich", "und", "stimm\u00b7te", "die", "Sai\u00b7ten", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da ward ich versetzt in Ruhstand.", "tokens": ["Da", "ward", "ich", "ver\u00b7setzt", "in", "Ruh\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus war der Spa\u00df; die heil'ge Stadt K\u00f6ln", "tokens": ["Aus", "war", "der", "Spa\u00df", ";", "die", "heil'\u00b7ge", "Stadt", "K\u00f6ln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ward erkl\u00e4rt in Belagerungszustand.", "tokens": ["Ward", "er\u00b7kl\u00e4rt", "in", "Be\u00b7la\u00b7ge\u00b7rungs\u00b7zu\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NE", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Von Bajonetten starrte die Stadt", "tokens": ["Von", "Ba\u00b7jo\u00b7net\u00b7ten", "starr\u00b7te", "die", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie ein Stachelschwein. Rings um den Neumarkt", "tokens": ["Wie", "ein", "Sta\u00b7chel\u00b7schwein", ".", "Rings", "um", "den", "Neu\u00b7markt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$.", "NN", "APPR", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wogten die preu\u00dfischen Erzengel bis", "tokens": ["Wog\u00b7ten", "die", "preu\u00b7\u00dfi\u00b7schen", "Er\u00b7zen\u00b7gel", "bis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "APPR"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Zum Hahnentor und zum Heumarkt.", "tokens": ["Zum", "Hah\u00b7nen\u00b7tor", "und", "zum", "Heu\u00b7markt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und ein Leutnant zog vor unsere T\u00fcr", "tokens": ["Und", "ein", "Leut\u00b7nant", "zog", "vor", "un\u00b7se\u00b7re", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In kriegerischer Begleitung", "tokens": ["In", "krie\u00b7ge\u00b7ri\u00b7scher", "Be\u00b7glei\u00b7tung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und proklamierte trommelnd den Tod", "tokens": ["Und", "pro\u00b7kla\u00b7mier\u00b7te", "trom\u00b7melnd", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Neuen Rheinischen Zeitung. \u2013", "tokens": ["Der", "Neu\u00b7en", "Rhei\u00b7ni\u00b7schen", "Zei\u00b7tung", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Da griff ich zum Stab, und ich eilte fort,", "tokens": ["Da", "griff", "ich", "zum", "Stab", ",", "und", "ich", "eil\u00b7te", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Brust voller Kummer und \u00c4rger.", "tokens": ["Die", "Brust", "vol\u00b7ler", "Kum\u00b7mer", "und", "\u00c4r\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Zu Herrn Soherr nach Bingen floh ich; dort trinkt", "tokens": ["Zu", "Herrn", "So\u00b7herr", "nach", "Bin\u00b7gen", "floh", "ich", ";", "dort", "trinkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "NN", "APPR", "NN", "VVFIN", "PPER", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Man vorz\u00fcglichen Scharlachberger.", "tokens": ["Man", "vor\u00b7z\u00fcg\u00b7li\u00b7chen", "Schar\u00b7lach\u00b7ber\u00b7ger", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Herr Soherr, der ist ein fr\u00f6hlicher Mann,", "tokens": ["Herr", "So\u00b7herr", ",", "der", "ist", "ein", "fr\u00f6h\u00b7li\u00b7cher", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und im ganzen Lande wird sich", "tokens": ["Und", "im", "gan\u00b7zen", "Lan\u00b7de", "wird", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VAFIN", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kein Scharlachberger finden wie der", "tokens": ["Kein", "Schar\u00b7lach\u00b7ber\u00b7ger", "fin\u00b7den", "wie", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVINF", "KOKOM", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Herrn Soherr von sechsundvierzig.", "tokens": ["Des", "Herrn", "So\u00b7herr", "von", "sech\u00b7sund\u00b7vier\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr Soherr ist vierundsechzig alt,", "tokens": ["Herr", "So\u00b7herr", "ist", "vie\u00b7rund\u00b7sech\u00b7zig", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "CARD", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und sein Wein ist von sechsundvierzig;", "tokens": ["Und", "sein", "Wein", "ist", "von", "sech\u00b7sund\u00b7vier\u00b7zig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "CARD", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Er duftet nach Veilchen und Rosen und schmeckt", "tokens": ["Er", "duf\u00b7tet", "nach", "Veil\u00b7chen", "und", "Ro\u00b7sen", "und", "schmeckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie die Liebe erquickend und w\u00fcrzig.", "tokens": ["Wie", "die", "Lie\u00b7be", "er\u00b7qui\u00b7ckend", "und", "w\u00fcr\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}