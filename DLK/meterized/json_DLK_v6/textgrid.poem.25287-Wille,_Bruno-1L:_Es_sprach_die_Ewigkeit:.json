{"textgrid.poem.25287": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es sprach die Ewigkeit:", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sprach die Ewigkeit:", "tokens": ["Es", "sprach", "die", "E\u00b7wig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbnur still, ihr Kindlein, ruht!", "tokens": ["\u00bb", "nur", "still", ",", "ihr", "Kin\u00b7dlein", ",", "ruht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bewahrt vor allem Streit,", "tokens": ["Be\u00b7wahrt", "vor", "al\u00b7lem", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt Gottes Fleisch und Blut.\u00ab", "tokens": ["Bleibt", "Got\u00b7tes", "Fleisch", "und", "Blut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch ein Geschrei erwacht:", "tokens": ["Doch", "ein", "Ge\u00b7schrei", "er\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbla\u00df uns geboren werden!\u00ab/", "tokens": ["\u00bb", "la\u00df", "uns", "ge\u00b7bo\u00b7ren", "wer\u00b7den", "!", "\u00ab", "/"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "VVPP", "VAINF", "$.", "$(", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "So wurden Tag und Nacht,", "tokens": ["So", "wur\u00b7den", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Luft, Wasser, Himmel, Erden.", "tokens": ["Luft", ",", "Was\u00b7ser", ",", "Him\u00b7mel", ",", "Er\u00b7den", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Menschenkindlein sog", "tokens": ["Das", "Men\u00b7schen\u00b7kin\u00b7dlein", "sog"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit Auge, Mund und Ohr.", "tokens": ["Mit", "Au\u00b7ge", ",", "Mund", "und", "Ohr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sondergier betrog,", "tokens": ["Die", "Son\u00b7der\u00b7gier", "be\u00b7trog", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df es sein Herz verlor.", "tokens": ["Da\u00df", "es", "sein", "Herz", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von Habsucht ausgef\u00fcllt,", "tokens": ["Von", "Hab\u00b7sucht", "aus\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Denkt es der Herkunft kaum;", "tokens": ["Denkt", "es", "der", "Her\u00b7kunft", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Heimat liegt verh\u00fcllt,", "tokens": ["Die", "Hei\u00b7mat", "liegt", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vergessen wie ein Traum.", "tokens": ["Ver\u00b7ges\u00b7sen", "wie", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn es r\u00fcckw\u00e4rts lauscht,", "tokens": ["Und", "wenn", "es", "r\u00fcck\u00b7w\u00e4rts", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gr\u00fc\u00dft keine Mutter mehr;", "tokens": ["Gr\u00fc\u00dft", "kei\u00b7ne", "Mut\u00b7ter", "mehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nur ein Garten rauscht,", "tokens": ["Und", "nur", "ein", "Gar\u00b7ten", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein wogend Wipfelheer.", "tokens": ["Ein", "wo\u00b7gend", "Wip\u00b7fel\u00b7heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit lichtem Schwerte droht", "tokens": ["Mit", "lich\u00b7tem", "Schwer\u00b7te", "droht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein W\u00e4chter vor der Pforte.", "tokens": ["Ein", "W\u00e4ch\u00b7ter", "vor", "der", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Blitz sein Auge loht;", "tokens": ["Wie", "Blitz", "sein", "Au\u00b7ge", "loht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie Donner seine Worte:", "tokens": ["Wie", "Don\u00b7ner", "sei\u00b7ne", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbim Heim der Ewigkeit", "tokens": ["\u00bb", "im", "Heim", "der", "E\u00b7wig\u00b7keit"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "War einer bei dem andern.", "tokens": ["War", "ei\u00b7ner", "bei", "dem", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die unrastvolle Zeit", "tokens": ["Die", "un\u00b7rast\u00b7vol\u00b7le", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "L\u00e4\u00dft euch entfremdet wandern.", "tokens": ["L\u00e4\u00dft", "euch", "ent\u00b7frem\u00b7det", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O W\u00fcste Einsamkeit,", "tokens": ["O", "W\u00fcs\u00b7te", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo jeder einzeln irrt!", "tokens": ["Wo", "je\u00b7der", "ein\u00b7zeln", "irrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6lker sind entzweit,", "tokens": ["Die", "V\u00f6l\u00b7ker", "sind", "ent\u00b7zweit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Sprachen sind verwirrt.", "tokens": ["Die", "Spra\u00b7chen", "sind", "ver\u00b7wirrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und weil um Rache schreit", "tokens": ["Und", "weil", "um", "Ra\u00b7che", "schreit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vergossnes Bruderblut,", "tokens": ["Ver\u00b7goss\u00b7nes", "Bru\u00b7der\u00b7blut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun denn, ihr M\u00f6rder, seid", "tokens": ["Nun", "denn", ",", "ihr", "M\u00f6r\u00b7der", ",", "seid"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Einander H\u00f6llenglut!\u00ab", "tokens": ["Ein\u00b7an\u00b7der", "H\u00f6l\u00b7len\u00b7glut", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "So grollt der Rachegeist.", "tokens": ["So", "grollt", "der", "Ra\u00b7che\u00b7geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch horch, der Garten Eden,", "tokens": ["Doch", "horch", ",", "der", "Gar\u00b7ten", "E\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er s\u00e4uselt und verhei\u00dft:", "tokens": ["Er", "s\u00e4u\u00b7selt", "und", "ver\u00b7hei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbherbei! Ich heile jeden!", "tokens": ["\u00bb", "her\u00b7bei", "!", "Ich", "hei\u00b7le", "je\u00b7den", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "PPER", "VVFIN", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Erl\u00f6sung wird beschert,", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "wird", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn ihr, der W\u00fcste leid,", "tokens": ["Wenn", "ihr", ",", "der", "W\u00fcs\u00b7te", "leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch reuevoll bekehrt", "tokens": ["Euch", "reu\u00b7e\u00b7voll", "be\u00b7kehrt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur treuen Ewigkeit.", "tokens": ["Zur", "treu\u00b7en", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Herbei, ihr Zagen! Kommt", "tokens": ["Her\u00b7bei", ",", "ihr", "Za\u00b7gen", "!", "Kommt"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "$.", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An meine Gartenmauer!", "tokens": ["An", "mei\u00b7ne", "Gar\u00b7ten\u00b7mau\u00b7er", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu eurem Troste frommt", "tokens": ["Zu", "eu\u00b7rem", "Tros\u00b7te", "frommt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der ahnungsvolle Schauer.", "tokens": ["Der", "ah\u00b7nungs\u00b7vol\u00b7le", "Schau\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wenn meine Wipfel raunen", "tokens": ["Wenn", "mei\u00b7ne", "Wip\u00b7fel", "rau\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Nachtigallen singen,", "tokens": ["Und", "Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will euch vor s\u00fc\u00dfem Staunen", "tokens": ["Will", "euch", "vor", "s\u00fc\u00b7\u00dfem", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das volle Herz zerspringen.", "tokens": ["Das", "vol\u00b7le", "Herz", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und so sich zwei vereinen", "tokens": ["Und", "so", "sich", "zwei", "ver\u00b7ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PRF", "CARD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Lieben und Erbarmen,", "tokens": ["In", "Lie\u00b7ben", "und", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da halten sie mit Weinen", "tokens": ["Da", "hal\u00b7ten", "sie", "mit", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Eden in den Armen.\u00ab", "tokens": ["Ihr", "E\u00b7den", "in", "den", "Ar\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}