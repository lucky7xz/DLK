{"textgrid.poem.41036": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An Gr\u00e4fin Fernande von Pappenheim in Tegernsee", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Tegernsee! du Aue voller Frieden,", "tokens": ["O", "Te\u00b7gern\u00b7see", "!", "du", "Au\u00b7e", "vol\u00b7ler", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit deiner Wasser, deiner W\u00e4lder Pracht!", "tokens": ["Mit", "dei\u00b7ner", "Was\u00b7ser", ",", "dei\u00b7ner", "W\u00e4l\u00b7der", "Pracht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ja, w\u00e4re dich zu sehen mir beschieden,", "tokens": ["Ja", ",", "w\u00e4\u00b7re", "dich", "zu", "se\u00b7hen", "mir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PPER", "PTKZU", "VVINF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch eh' mein Auge sinkt in bange Nacht?", "tokens": ["Noch", "eh'", "mein", "Au\u00b7ge", "sinkt", "in", "ban\u00b7ge", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wird dies nimmer dem Erkrankten, M\u00fcden,", "tokens": ["Und", "wird", "dies", "nim\u00b7mer", "dem", "Er\u00b7krank\u00b7ten", ",", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "ADV", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Werd' ihm ein Licht im Innern angefacht,", "tokens": ["Werd'", "ihm", "ein", "Licht", "im", "In\u00b7nern", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das ihm in eines Maitags Sonnenhelle", "tokens": ["Das", "ihm", "in", "ei\u00b7nes", "Mai\u00b7tags", "Son\u00b7nen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dein wunderreiches Bild herspiegelt schnelle,", "tokens": ["Dein", "wun\u00b7der\u00b7rei\u00b7ches", "Bild", "her\u00b7spie\u00b7gelt", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch in ihm, ach! vor allen eine Stelle.", "tokens": ["Doch", "in", "ihm", ",", "ach", "!", "vor", "al\u00b7len", "ei\u00b7ne", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "ITJ", "$.", "APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor allen, ja soll hell das Licht ihm bringen", "tokens": ["Vor", "al\u00b7len", ",", "ja", "soll", "hell", "das", "Licht", "ihm", "brin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "$,", "ADV", "VMFIN", "ADJD", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das liebe Haus am Berge angebaut.", "tokens": ["Das", "lie\u00b7be", "Haus", "am", "Ber\u00b7ge", "an\u00b7ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drin wird er h\u00f6ren Nachtigallen singen,", "tokens": ["Drin", "wird", "er", "h\u00f6\u00b7ren", "Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bekannte Stimmen, ach! so lieb, so traut!", "tokens": ["Be\u00b7kann\u00b7te", "Stim\u00b7men", ",", "ach", "!", "so", "lieb", ",", "so", "traut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ITJ", "$.", "ADV", "ADJD", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann wird aus seinem wunden Herzen dringen,", "tokens": ["Dann", "wird", "aus", "sei\u00b7nem", "wun\u00b7den", "Her\u00b7zen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verstummt ihr Lied, der Sehnsucht Klagelaut.", "tokens": ["Ver\u00b7stummt", "ihr", "Lied", ",", "der", "Sehn\u00b7sucht", "Kla\u00b7ge\u00b7laut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Magnetisch werd' zu ihnen der getragen,", "tokens": ["Mag\u00b7ne\u00b7tisch", "werd'", "zu", "ih\u00b7nen", "der", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "ART", "VVPP", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Da\u00df sie verwundert dort, \u00bbwas t\u00f6nt so?\u00ab fragen,", "tokens": ["Da\u00df", "sie", "ver\u00b7wun\u00b7dert", "dort", ",", "\u00bb", "was", "t\u00f6nt", "so", "?", "\u00ab", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$,", "$(", "PWS", "VVFIN", "ADV", "$.", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "\u00bbdas klang ja, wie zu Weinsberg Harfen klagen.\u00ab", "tokens": ["\u00bb", "das", "klang", "ja", ",", "wie", "zu", "Weins\u00b7berg", "Har\u00b7fen", "kla\u00b7gen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "ADV", "$,", "PWAV", "APPR", "NE", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}