{"dta.poem.3376": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Was  \n bleibet und was schwindet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es rinnt der Sand der Stunden;", "tokens": ["Es", "rinnt", "der", "Sand", "der", "Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es rauscht der Jahre Fl\u00fcgel.", "tokens": ["Es", "rauscht", "der", "Jah\u00b7re", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Zukunft heilge Siegel", "tokens": ["Der", "Zu\u00b7kunft", "heil\u00b7ge", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bricht jeder Augenblick.", "tokens": ["Bricht", "je\u00b7der", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Schlossen Schlossen jagen,", "tokens": ["Wie", "Schlos\u00b7sen", "Schlos\u00b7sen", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Fluthen Fluthen schlagen,", "tokens": ["Wie", "Flut\u00b7hen", "Flut\u00b7hen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So rollt der Strom der Zeiten;", "tokens": ["So", "rollt", "der", "Strom", "der", "Zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Gott ruft ihn zur\u00fcck.", "tokens": ["Kein", "Gott", "ruft", "ihn", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es kreisst der Zeiten Strudel,", "tokens": ["Es", "kreisst", "der", "Zei\u00b7ten", "Stru\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und reisst des Menschen Freuden", "tokens": ["Und", "reisst", "des", "Men\u00b7schen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und seine tausend Leiden", "tokens": ["Und", "sei\u00b7ne", "tau\u00b7send", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinen Schlund hinab.", "tokens": ["In", "sei\u00b7nen", "Schlund", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hast du verjauchzt Secunden?", "tokens": ["Hast", "du", "ver\u00b7jauchzt", "Se\u00b7cun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hast du verjammert Stunden? \u2014", "tokens": ["Hast", "du", "ver\u00b7jam\u00b7mert", "Stun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dein Jauchzen und dein Jammern", "tokens": ["Dein", "Jauch\u00b7zen", "und", "dein", "Jam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verschliesst das stumme Grab.", "tokens": ["Ver\u00b7schliesst", "das", "stum\u00b7me", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Ros' erbl\u00fcht am Morgen.", "tokens": ["Die", "Ros'", "er\u00b7bl\u00fcht", "am", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie str\u00f6men ihre D\u00fcfte!", "tokens": ["Wie", "str\u00f6\u00b7men", "ih\u00b7re", "D\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Hauch durchw\u00fcrzt die L\u00fcfte;", "tokens": ["Ihr", "Hauch", "durch\u00b7w\u00fcrzt", "die", "L\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Abend welkt sie hin.", "tokens": ["Am", "A\u00b7bend", "welkt", "sie", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es lockt im Mayenschatten", "tokens": ["Es", "lockt", "im", "Ma\u00b7yen\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Nachtigal den Gatten.", "tokens": ["Die", "Nach\u00b7ti\u00b7gal", "den", "Gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der May entflieht, und traurig", "tokens": ["Der", "May", "ent\u00b7flieht", ",", "und", "trau\u00b7rig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erstummt die S\u00e4ngerin.", "tokens": ["Er\u00b7stummt", "die", "S\u00e4n\u00b7ge\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hoch klingt des Dichters Harfe;", "tokens": ["Hoch", "klingt", "des", "Dich\u00b7ters", "Har\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sie schmelzt die rohe Jugend,", "tokens": ["Sie", "schmelzt", "die", "ro\u00b7he", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entflammt zu hoher Tugend,", "tokens": ["Ent\u00b7flammt", "zu", "ho\u00b7her", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und st\u00e4rkt zu Edelthat.", "tokens": ["Und", "st\u00e4rkt", "zu", "E\u00b7delt\u00b7hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Wandrer kommt im Lenzen,", "tokens": ["Der", "Wand\u00b7rer", "kommt", "im", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sein gr\u00fcnend Grab zu kr\u00e4nzen \u2014", "tokens": ["Sein", "gr\u00fc\u00b7nend", "Grab", "zu", "kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Umsonst! denn niemand kennet", "tokens": ["Um\u00b7sonst", "!", "denn", "nie\u00b7mand", "ken\u00b7net"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Edeln Ruhestatt.", "tokens": ["Des", "E\u00b7deln", "Ru\u00b7he\u00b7statt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Von Durst nach Ruhm und Liebe,", "tokens": ["Von", "Durst", "nach", "Ruhm", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Wein der Lebensfreuden,", "tokens": ["Vom", "Wein", "der", "Le\u00b7bens\u00b7freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Heldenmuth zu leiden \u2014", "tokens": ["Vom", "Hel\u00b7den\u00b7muth", "zu", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie flammt des J\u00fcnglings Blick!", "tokens": ["Wie", "flammt", "des", "J\u00fcng\u00b7lings", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom Morgen saust ein L\u00fcftchen,", "tokens": ["Vom", "Mor\u00b7gen", "saust", "ein", "L\u00fcft\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Mittag weht ein D\u00fcftchen,", "tokens": ["Vom", "Mit\u00b7tag", "weht", "ein", "D\u00fcft\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Umhaucht den Starken \u2014 Pl\u00f6tzlich", "tokens": ["Um\u00b7haucht", "den", "Star\u00b7ken", "Pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$(", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Erlischt sein Flammenblick!", "tokens": ["Er\u00b7lischt", "sein", "Flam\u00b7men\u00b7blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In ihrer Myrtenkrone,", "tokens": ["In", "ih\u00b7rer", "Myr\u00b7ten\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In lilienweisser Seide,", "tokens": ["In", "li\u00b7li\u00b7en\u00b7weis\u00b7ser", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In br\u00e4utlichem Geschmeide,", "tokens": ["In", "br\u00e4ut\u00b7li\u00b7chem", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wem bl\u00fcht die junge Braut?", "tokens": ["Wem", "bl\u00fcht", "die", "jun\u00b7ge", "Braut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es flammt die Mittagsschw\u00fcle,", "tokens": ["Es", "flammt", "die", "Mit\u00b7tags\u00b7schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es weht die Abendk\u00fchle \u2014", "tokens": ["Es", "weht", "die", "A\u00b7bend\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und in die kalten Arme", "tokens": ["Und", "in", "die", "kal\u00b7ten", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nimmt Tod die holde Braut.", "tokens": ["Nimmt", "Tod", "die", "hol\u00b7de", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Zeder trozt den St\u00fcrmen;", "tokens": ["Die", "Ze\u00b7der", "trozt", "den", "St\u00fcr\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es trotzt der Fels den Wogen.", "tokens": ["Es", "trotzt", "der", "Fels", "den", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es f\u00e4hrt am Himmelsbegen", "tokens": ["Es", "f\u00e4hrt", "am", "Him\u00b7mels\u00b7be\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonn' in stolzer Pracht.", "tokens": ["Die", "Sonn'", "in", "stol\u00b7zer", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die hohe Zeder splittert;", "tokens": ["Die", "ho\u00b7he", "Ze\u00b7der", "split\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der stolze Fels verwittert.", "tokens": ["Der", "stol\u00b7ze", "Fels", "ver\u00b7wit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Einst sinkst du, goldne Sonne,", "tokens": ["Einst", "sinkst", "du", ",", "gold\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und kehrst nicht aus der Nacht.", "tokens": ["Und", "kehrst", "nicht", "aus", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mit diamantnem Griffel", "tokens": ["Mit", "di\u00b7a\u00b7mant\u00b7nem", "Grif\u00b7fel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward es in Erz geschrieben:", "tokens": ["Ward", "es", "in", "Erz", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ewas Staub ist, soll zerstieben;", "tokens": ["\u201e", "was", "Staub", "ist", ",", "soll", "zer\u00b7stie\u00b7ben", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "VAFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ewas athmet, soll verwehn;", "tokens": ["\u201e", "was", "ath\u00b7met", ",", "soll", "ver\u00b7wehn", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201edie vollste Kraft ermatten;", "tokens": ["\u201e", "die", "volls\u00b7te", "Kraft", "er\u00b7mat\u00b7ten", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201eder lichte Tag in Schatten,", "tokens": ["\u201e", "der", "lich\u00b7te", "Tag", "in", "Schat\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201edie Sch\u00f6nheit und die Jugend", "tokens": ["\u201e", "die", "Sch\u00f6n\u00b7heit", "und", "die", "Ju\u00b7gend"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201ein Moder \u00fcbergehn!\u201c", "tokens": ["\u201e", "in", "Mo\u00b7der", "\u00fc\u00b7ber\u00b7gehn", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Welkt, Lorbeern meines Hauptes!", "tokens": ["Welkt", ",", "Lor\u00b7beern", "mei\u00b7nes", "Haup\u00b7tes", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bekr\u00e4nzt mich, Weid' und Wermuth!", "tokens": ["Be\u00b7kr\u00e4nzt", "mich", ",", "Weid'", "und", "Wer\u00b7muth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Seele w\u00f6lke Schwermuth,", "tokens": ["Die", "See\u00b7le", "w\u00f6l\u00b7ke", "Schwer\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Auge Thr\u00e4nenfluth!", "tokens": ["Das", "Au\u00b7ge", "Thr\u00e4\u00b7nen\u00b7fluth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verbl\u00fchn wird Idens Bl\u00fcthe;", "tokens": ["Ver\u00b7bl\u00fchn", "wird", "I\u00b7dens", "Bl\u00fc\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Blick voll reiner G\u00fcte,", "tokens": ["Dein", "Blick", "voll", "rei\u00b7ner", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ellwina, wird ermatten,", "tokens": ["Ell\u00b7wi\u00b7na", ",", "wird", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erl\u00f6schen meine Gluth!", "tokens": ["Er\u00b7l\u00f6\u00b7schen", "mei\u00b7ne", "Gluth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Klagt, Saiten, \u00e4chzt, ihr Weiden \u2014", "tokens": ["Klagt", ",", "Sai\u00b7ten", ",", "\u00e4chzt", ",", "ihr", "Wei\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch nein, erjauchzt in Psalmen!", "tokens": ["Doch", "nein", ",", "er\u00b7jauchzt", "in", "Psal\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Rauscht, Edens ewge Palmen!", "tokens": ["Rauscht", ",", "E\u00b7dens", "ew\u00b7ge", "Pal\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mag seyn, dass Staub zerstiebe!", "tokens": ["Mag", "seyn", ",", "dass", "Staub", "zer\u00b7stie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Eins, weiss ich, kann nicht sterben;", "tokens": ["Eins", ",", "weiss", "ich", ",", "kann", "nicht", "ster\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "$,", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Eins trotzet dem Verderben,", "tokens": ["Eins", "trot\u00b7zet", "dem", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Eins spottet der Verwesung \u2014", "tokens": ["Eins", "spot\u00b7tet", "der", "Ver\u00b7we\u00b7sung"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Geist, der Tugend liebt!", "tokens": ["Ein", "Geist", ",", "der", "Tu\u00b7gend", "liebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ein Geist voll reiner Tugend,", "tokens": ["Ein", "Geist", "voll", "rei\u00b7ner", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll Einfalt und voll Liebe,", "tokens": ["Voll", "Ein\u00b7falt", "und", "voll", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bezwungen nie vom Triebe,", "tokens": ["Be\u00b7zwun\u00b7gen", "nie", "vom", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt ewig jung und sch\u00f6n;", "tokens": ["Bleibt", "e\u00b7wig", "jung", "und", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist Hauch des Mundes Gottes,", "tokens": ["Ist", "Hauch", "des", "Mun\u00b7des", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist Blirz der Flamme Gottes,", "tokens": ["Ist", "Blirz", "der", "Flam\u00b7me", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ist Abglanz seines Lichtes,", "tokens": ["Ist", "Ab\u00b7glanz", "sei\u00b7nes", "Lich\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kann ewig nicht vergehn.", "tokens": ["Kann", "e\u00b7wig", "nicht", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er stammt nicht von hienieden;", "tokens": ["Er", "stammt", "nicht", "von", "hien\u00b7ie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er wird nicht dir zum Raube,", "tokens": ["Er", "wird", "nicht", "dir", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verwesung, gleich dem Staube,", "tokens": ["Ver\u00b7we\u00b7sung", ",", "gleich", "dem", "Stau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dran ihn sein Sch\u00f6pfer band.", "tokens": ["Dran", "ihn", "sein", "Sch\u00f6p\u00b7fer", "band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sieht den Staub verfliegen,", "tokens": ["Er", "sieht", "den", "Staub", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Sturmwind um ihn kriegen,", "tokens": ["Den", "Sturm\u00b7wind", "um", "ihn", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erhebt sich, m\u00e4chtig, schwingt sich", "tokens": ["Er\u00b7hebt", "sich", ",", "m\u00e4ch\u00b7tig", ",", "schwingt", "sich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "ADJD", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hoch in sein Vaterland.", "tokens": ["Hoch", "in", "sein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sein Vaterland ist droben.", "tokens": ["Sein", "Va\u00b7ter\u00b7land", "ist", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "w