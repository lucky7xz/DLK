{"dta.poem.10237": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Heidebilder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Tiefeinsamkeit spannt weit die sch\u00f6nen Fl&#252;gel,             ", "tokens": ["Tie\u00b7fein\u00b7sam\u00b7keit", "spannt", "weit", "die", "sch\u00f6\u00b7nen", "Fl", "&#252;", "gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$(", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Weit \u00fcber stille Felder aus.", "tokens": ["Weit", "\u00fc\u00b7ber", "stil\u00b7le", "Fel\u00b7der", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ferne K\u00fcsten grenzen graue H\u00fcgel,", "tokens": ["Wie", "fer\u00b7ne", "K\u00fcs\u00b7ten", "gren\u00b7zen", "grau\u00b7e", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sch\u00fctzen vor dem Menschengraus.", "tokens": ["Sie", "sch\u00fct\u00b7zen", "vor", "dem", "Men\u00b7schen\u00b7graus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Fr\u00fchling rauscht in mittern\u00e4chtiger Stunde", "tokens": ["Im", "Fr\u00fch\u00b7ling", "rauscht", "in", "mit\u00b7ter\u00b7n\u00e4ch\u00b7ti\u00b7ger", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Wildgans hoch in raschem Flug.", "tokens": ["Die", "Wild\u00b7gans", "hoch", "in", "ra\u00b7schem", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das alte Gaukelspiel: in weiter Runde", "tokens": ["Das", "al\u00b7te", "Gau\u00b7kel\u00b7spiel", ":", "in", "wei\u00b7ter", "Run\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6r\u2019 ich Gesang im Wolkenzug.", "tokens": ["H\u00f6r'", "ich", "Ge\u00b7sang", "im", "Wol\u00b7ken\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verschlafen sinkt der Mond in schwarze Gr\u00fcnde,", "tokens": ["Ver\u00b7schla\u00b7fen", "sinkt", "der", "Mond", "in", "schwar\u00b7ze", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begl\u00e4nzt noch einmal Schilf und Rohr.", "tokens": ["Be\u00b7gl\u00e4nzt", "noch", "ein\u00b7mal", "Schilf", "und", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gelangweilt ob so mancher holden S\u00fcnde,", "tokens": ["Ge\u00b7lang\u00b7weilt", "ob", "so", "man\u00b7cher", "hol\u00b7den", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOUS", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verl\u00e4\u00dft er Garten, Wald und Moor.", "tokens": ["Ver\u00b7l\u00e4\u00dft", "er", "Gar\u00b7ten", ",", "Wald", "und", "Moor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}