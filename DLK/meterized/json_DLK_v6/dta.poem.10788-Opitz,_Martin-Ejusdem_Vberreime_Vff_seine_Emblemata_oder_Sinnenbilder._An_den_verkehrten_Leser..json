{"dta.poem.10788": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ejusdem   Vberreime/  \n Vff seine Emblemata oder Sinnenbilder.  \n An den verkehrten Leser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sinnenbilder hier sein ledig eigen mein/", "tokens": ["Die", "Sin\u00b7nen\u00b7bil\u00b7der", "hier", "sein", "le\u00b7dig", "ei\u00b7gen", "mein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "ADJD", "ADJD", "PPOSAT", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Drumb mache sie durch kein verkehrt\u2019 au\u00dflegung dein.", "tokens": ["Drumb", "ma\u00b7che", "sie", "durch", "kein", "ver\u00b7kehrt'", "au\u00df\u00b7le\u00b7gung", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}