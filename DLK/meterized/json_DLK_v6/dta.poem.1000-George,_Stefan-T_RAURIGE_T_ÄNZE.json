{"dta.poem.1000": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Trauervolle nacht               ", "tokens": ["Trau\u00b7er\u00b7vol\u00b7le", "nacht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Schwarze sammetdecke d&#228;mpft               ", "tokens": ["Schwar\u00b7ze", "sam\u00b7met\u00b7de\u00b7cke", "d", "&#228;", "mpft"], "token_info": ["word", "word", "word", "XML_entity", "word"], "pos": ["XY", "XY", "XY", "XY", "XY"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schritte im gemach               ", "tokens": ["Schrit\u00b7te", "im", "ge\u00b7mach"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Worin die liebe k&#228;mpft             ", "tokens": ["Wo\u00b7rin", "die", "lie\u00b7be", "k", "&#228;", "mpft"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$(", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Den tod gab ihr dein wunsch               ", "tokens": ["Den", "tod", "gab", "ihr", "dein", "wunsch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nun siehst du bleich und stumm               ", "tokens": ["Nun", "siehst", "du", "bleich", "und", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie auf der bahre ruhn               ", "tokens": ["Sie", "auf", "der", "bah\u00b7re", "ruhn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es stecken lichter drum             ", "tokens": ["Es", "ste\u00b7cken", "lich\u00b7ter", "drum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die lichter brennen ab               ", "tokens": ["Die", "lich\u00b7ter", "bren\u00b7nen", "ab"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVINF", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du eilest blind hinaus               ", "tokens": ["Du", "ei\u00b7lest", "blind", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nachdem die liebe starb &#8212;               ", "tokens": ["Nach\u00b7dem", "die", "lie\u00b7be", "starb", "&#8212;"], "token_info": ["word", "word", "word", "word", "XML_entity"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und weinen schallt im haus.             ", "tokens": ["Und", "wei\u00b7nen", "schallt", "im", "haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}