{"textgrid.poem.37152": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Unentschiedene", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Zauber in dem Klang", "tokens": ["Ei\u00b7nen", "Zau\u00b7ber", "in", "dem", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer Stimme hat Filinde,", "tokens": ["Ih\u00b7rer", "Stim\u00b7me", "hat", "Fi\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich immer nur mit Zwang", "tokens": ["Da\u00df", "ich", "im\u00b7mer", "nur", "mit", "Zwang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich dem s\u00fc\u00dfen Ton entwinde.", "tokens": ["Mich", "dem", "s\u00fc\u00b7\u00dfen", "Ton", "ent\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einen Zauber in dem Blick", "tokens": ["Ei\u00b7nen", "Zau\u00b7ber", "in", "dem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer Augen hat Agathe,", "tokens": ["Ih\u00b7rer", "Au\u00b7gen", "hat", "A\u00b7gat\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich \u00fcber mein Geschick", "tokens": ["Da\u00df", "ich", "\u00fc\u00b7ber", "mein", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In Besorgnis fast gerathe.", "tokens": ["In", "Be\u00b7sorg\u00b7nis", "fast", "ge\u00b7ra\u00b7the", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von den schwestern gleich ger\u00fchrt,", "tokens": ["Von", "den", "schwes\u00b7tern", "gleich", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich keine noch erkoren.", "tokens": ["Hab", "ich", "kei\u00b7ne", "noch", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn mein Auge die verf\u00fchrt,", "tokens": ["Wenn", "mein", "Au\u00b7ge", "die", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt mich jene bei den Ohren.", "tokens": ["H\u00e4lt", "mich", "je\u00b7ne", "bei", "den", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}