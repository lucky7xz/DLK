{"dta.poem.4009": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die scheinheiligen Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr kalten Heuchler, sprecht von den G\u00f6ttern nicht!", "tokens": ["Ihr", "kal\u00b7ten", "Heuch\u00b7ler", ",", "sprecht", "von", "den", "G\u00f6t\u00b7tern", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr habt Verstand, ihr glaubt nicht an Helios", "tokens": ["Ihr", "habt", "Ver\u00b7stand", ",", "ihr", "glaubt", "nicht", "an", "He\u00b7li\u00b7os"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch an den Donnerer und Meergott;", "tokens": ["Noch", "an", "den", "Don\u00b7ne\u00b7rer", "und", "Meer\u00b7gott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Todt ist die Erde, wer mag ihr danken?", "tokens": ["Todt", "ist", "die", "Er\u00b7de", ",", "wer", "mag", "ihr", "dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Getrost ihr G\u00f6tter! zieret ihr doch das Lied,", "tokens": ["Ge\u00b7trost", "ihr", "G\u00f6t\u00b7ter", "!", "zie\u00b7ret", "ihr", "doch", "das", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn schon aus euren Namen die Seele schwamd,", "tokens": ["Wenn", "schon", "aus", "eu\u00b7ren", "Na\u00b7men", "die", "See\u00b7le", "schwamd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ist ein gro\u00dfes Wort vonn\u00f6then,", "tokens": ["Und", "ist", "ein", "gro\u00b7\u00dfes", "Wort", "von\u00b7n\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mutter Natur! so gedenkt man deiner.", "tokens": ["Mut\u00b7ter", "Na\u00b7tur", "!", "so", "ge\u00b7denkt", "man", "dei\u00b7ner", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADV", "VVFIN", "PIS", "PPOSAT", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}