{"textgrid.poem.31771": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die heilige deutsche Reichsarmee", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die heilige deutsche Reichsarmee", "tokens": ["Die", "hei\u00b7li\u00b7ge", "deut\u00b7sche", "Reichs\u00b7ar\u00b7mee"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist auf den Strumpf gekommen:", "tokens": ["Ist", "auf", "den", "Strumpf", "ge\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat aus Schwaben und Hessen sich", "tokens": ["Sie", "hat", "aus", "Schwa\u00b7ben", "und", "Hes\u00b7sen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "PRF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die besten Jungens genommen.", "tokens": ["Die", "bes\u00b7ten", "Jun\u00b7gens", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die hellige deutsche Reichsarmee,", "tokens": ["Die", "hel\u00b7li\u00b7ge", "deut\u00b7sche", "Reichs\u00b7ar\u00b7mee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sollte die Schweiz ber\u00fccken:", "tokens": ["Die", "soll\u00b7te", "die", "Schweiz", "be\u00b7r\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schon r\u00fcckte sie aus, da mu\u00dfte sie, ach,", "tokens": ["Schon", "r\u00fcck\u00b7te", "sie", "aus", ",", "da", "mu\u00df\u00b7te", "sie", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "$,", "ITJ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die verfluchten Hosen noch flicken.", "tokens": ["Die", "ver\u00b7fluch\u00b7ten", "Ho\u00b7sen", "noch", "fli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die heilige deutsche Reichsarmee,", "tokens": ["Die", "hei\u00b7li\u00b7ge", "deut\u00b7sche", "Reichs\u00b7ar\u00b7mee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sollte ganz Limburg fressen:", "tokens": ["Die", "soll\u00b7te", "ganz", "Lim\u00b7burg", "fres\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "NE", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schon r\u00fcckte sie aus, da hatte sie, ach,", "tokens": ["Schon", "r\u00fcck\u00b7te", "sie", "aus", ",", "da", "hat\u00b7te", "sie", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "$,", "ITJ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das verfluchte Pulver vergessen.", "tokens": ["Das", "ver\u00b7fluch\u00b7te", "Pul\u00b7ver", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die heilige deutsche Reichsarmee,", "tokens": ["Die", "hei\u00b7li\u00b7ge", "deut\u00b7sche", "Reichs\u00b7ar\u00b7mee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die war zum Kampfe entschlossen:", "tokens": ["Die", "war", "zum", "Kamp\u00b7fe", "ent\u00b7schlos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da haben die Preu\u00dfen und D\u00e4nen, ach,", "tokens": ["Da", "ha\u00b7ben", "die", "Preu\u00b7\u00dfen", "und", "D\u00e4\u00b7nen", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "NN", "$,", "ITJ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den verfluchten Frieden geschlossen.", "tokens": ["Den", "ver\u00b7fluch\u00b7ten", "Frie\u00b7den", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Die heilige deutsche Reichsarmee,", "tokens": ["Die", "hei\u00b7li\u00b7ge", "deut\u00b7sche", "Reichs\u00b7ar\u00b7mee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sollte auch Wien erl\u00f6sen:", "tokens": ["Die", "soll\u00b7te", "auch", "Wi\u00b7en", "er\u00b7l\u00f6\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "NE", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da ist, ach Gott, der Herr Windischgr\u00e4tz", "tokens": ["Da", "ist", ",", "ach", "Gott", ",", "der", "Herr", "Win\u00b7dischgr\u00e4tz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "XY", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So verflucht bei der Hand gewesen.", "tokens": ["So", "ver\u00b7flucht", "bei", "der", "Hand", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "VAPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Die heilige deutsche Reichsarmee,", "tokens": ["Die", "hei\u00b7li\u00b7ge", "deut\u00b7sche", "Reichs\u00b7ar\u00b7mee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die lebt ohn viele Sorgen:", "tokens": ["Die", "lebt", "ohn", "vie\u00b7le", "Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Landsknechte traun auf den lieben Gott \u2013", "tokens": ["Die", "Lands\u00b7knech\u00b7te", "traun", "auf", "den", "lie\u00b7ben", "Gott", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-++-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Kommst du heute nicht, kommst du morgen.", "tokens": ["Kommst", "du", "heu\u00b7te", "nicht", ",", "kommst", "du", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}