{"dta.poem.16288": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir die Lust noch nicht vergangen ist, den Herden", "tokens": ["Wenn", "dir", "die", "Lust", "noch", "nicht", "ver\u00b7gan\u00b7gen", "ist", ",", "den", "Her\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVPP", "VAFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weltber\u00fchmtheiten auch beigez\u00e4hlt zu werden,", "tokens": ["Der", "Welt\u00b7be\u00b7r\u00fchmt\u00b7hei\u00b7ten", "auch", "bei\u00b7ge\u00b7z\u00e4hlt", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Soll sie dir jetzt vergehn, wo zum ber\u00fchmten Mann", "tokens": ["Soll", "sie", "dir", "jetzt", "ver\u00b7gehn", ",", "wo", "zum", "be\u00b7r\u00fchm\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein M\u00f6rder, frech im Tod wie Leben, werden kann,", "tokens": ["Ein", "M\u00f6r\u00b7der", ",", "frech", "im", "Tod", "wie", "Le\u00b7ben", ",", "wer\u00b7den", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPRART", "NN", "KOKOM", "NN", "$,", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und eine Metze, weil sie seine Metze war,", "tokens": ["Und", "ei\u00b7ne", "Met\u00b7ze", ",", "weil", "sie", "sei\u00b7ne", "Met\u00b7ze", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als eine Sch\u00f6nheit sich darstellt, ein\u00e4ugig zwar.", "tokens": ["Als", "ei\u00b7ne", "Sch\u00f6n\u00b7heit", "sich", "dar\u00b7stellt", ",", "ein\u00b7\u00e4u\u00b7gig", "zwar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$,", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}