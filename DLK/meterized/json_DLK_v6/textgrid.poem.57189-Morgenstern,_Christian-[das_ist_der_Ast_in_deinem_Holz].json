{"textgrid.poem.57189": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[das ist der Ast in deinem Holz]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Ast in deinem Holz,", "tokens": ["Das", "ist", "der", "Ast", "in", "dei\u00b7nem", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "an dem der Hobel h\u00e4ngt und h\u00e4ngt:", "tokens": ["an", "dem", "der", "Ho\u00b7bel", "h\u00e4ngt", "und", "h\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dein Stolz,", "tokens": ["dein", "Stolz", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "der immer wieder dich", "tokens": ["der", "im\u00b7mer", "wie\u00b7der", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "in seine steifen Stiefel zw\u00e4ngt.", "tokens": ["in", "sei\u00b7ne", "stei\u00b7fen", "Stie\u00b7fel", "zw\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du m\u00f6chtest auf den Fl\u00fcgelschuhn", "tokens": ["Du", "m\u00f6ch\u00b7test", "auf", "den", "Fl\u00fc\u00b7gel\u00b7schuhn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "tiefinnerlichster Freiheit fliehn,", "tokens": ["tie\u00b7fin\u00b7ner\u00b7lichs\u00b7ter", "Frei\u00b7heit", "fliehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "doch ihn", "tokens": ["doch", "ihn"], "token_info": ["word", "word"], "pos": ["ADV", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "verdrie\u00dft so bitterlich", "tokens": ["ver\u00b7drie\u00dft", "so", "bit\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "kein ander unabh\u00e4ngig Tun.", "tokens": ["kein", "an\u00b7der", "un\u00b7ab\u00b7h\u00e4n\u00b7gig", "Tun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er h\u00e4lt dich fest: da stehst du starr:", "tokens": ["Er", "h\u00e4lt", "dich", "fest", ":", "da", "stehst", "du", "starr", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "d\u00fcrrknisternd-widerspenstig Holz:", "tokens": ["d\u00fcrr\u00b7knis\u00b7tern\u00b7dwi\u00b7der\u00b7spens\u00b7tig", "Holz", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein Stolz-", "tokens": ["ein", "Stolz"], "token_info": ["word", "word"], "pos": ["ART", "TRUNC"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "verstotzter Stock, ein sich", "tokens": ["ver\u00b7stotz\u00b7ter", "Stock", ",", "ein", "sich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "selbst widriger Hanswurst und Narr.", "tokens": ["selbst", "wid\u00b7ri\u00b7ger", "Hans\u00b7wurst", "und", "Narr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}