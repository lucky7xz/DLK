{"dta.poem.12643": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Roselinden,  \n Als sie einen alten mann nehmen wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schreibe, was vielleicht dir zorn erregen kan,", "tokens": ["Ich", "schrei\u00b7be", ",", "was", "viel\u00b7leicht", "dir", "zorn", "er\u00b7re\u00b7gen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "PPER", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem die jugend dir itzund fast gantz zuwider;", "tokens": ["In\u00b7dem", "die", "ju\u00b7gend", "dir", "it\u00b7zund", "fast", "gantz", "zu\u00b7wi\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wei\u00df ich eben nicht, was sie doch dir gethan,", "tokens": ["Doch", "wei\u00df", "ich", "e\u00b7ben", "nicht", ",", "was", "sie", "doch", "dir", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du fahest ja vorher nicht so auf alle glieder.", "tokens": ["Du", "fa\u00b7hest", "ja", "vor\u00b7her", "nicht", "so", "auf", "al\u00b7le", "glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch itzund ist bey dir der vorsatz fest gestellt,", "tokens": ["Doch", "it\u00b7zund", "ist", "bey", "dir", "der", "vor\u00b7satz", "fest", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein junger mann soll dir, als braut, die lippen k\u00fcssen;", "tokens": ["Kein", "jun\u00b7ger", "mann", "soll", "dir", ",", "als", "braut", ",", "die", "lip\u00b7pen", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "$,", "KOUS", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein alter soll es seyn, der dich zu frieden stellt,", "tokens": ["Ein", "al\u00b7ter", "soll", "es", "seyn", ",", "der", "dich", "zu", "frie\u00b7den", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "VAINF", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und gegen welchen du in liebe wilst zerfliessen.", "tokens": ["Und", "ge\u00b7gen", "wel\u00b7chen", "du", "in", "lie\u00b7be", "wilst", "zer\u00b7flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PWAT", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich wei\u00df nicht, was dir doch mu\u00df im gehirne seyn,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "dir", "doch", "mu\u00df", "im", "ge\u00b7hir\u00b7ne", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "VMFIN", "APPRART", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du pflegtest sonsten ja von den itzt lieben alten,", "tokens": ["Du", "pfleg\u00b7test", "sons\u00b7ten", "ja", "von", "den", "itzt", "lie\u00b7ben", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADV", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die eine junge frau sehr sparsamlich erfreun,", "tokens": ["Die", "ei\u00b7ne", "jun\u00b7ge", "frau", "sehr", "spar\u00b7sam\u00b7lich", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und von derselben gunst nicht eben viel zu halten.", "tokens": ["Und", "von", "der\u00b7sel\u00b7ben", "gunst", "nicht", "e\u00b7ben", "viel", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "PTKNEG", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nun ist das blat gewendt, nun ist ein junger mann,", "tokens": ["Nun", "ist", "das", "blat", "ge\u00b7wendt", ",", "nun", "ist", "ein", "jun\u00b7ger", "mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dir, gleichfalls junges kind! ein dorn in deinen augen;", "tokens": ["Dir", ",", "gleich\u00b7falls", "jun\u00b7ges", "kind", "!", "ein", "dorn", "in", "dei\u00b7nen", "au\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "ADJA", "NN", "$.", "ART", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein alter aber ists, aus dem dein auge kan", "tokens": ["Ein", "al\u00b7ter", "a\u00b7ber", "ists", ",", "aus", "dem", "dein", "au\u00b7ge", "kan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "VAFIN", "$,", "APPR", "ART", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das liebes trieseneth mit vollem schlunge sangen.", "tokens": ["Das", "lie\u00b7bes", "trie\u00b7se\u00b7neth", "mit", "vol\u00b7lem", "schlun\u00b7ge", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was rath hierbey zu thun? ich wei\u00df gewi\u00df nicht wie", "tokens": ["Was", "rath", "hier\u00b7bey", "zu", "thun", "?", "ich", "wei\u00df", "ge\u00b7wi\u00df", "nicht", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "ADV", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "KOKOM"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich dir die phantasie soll aus dem kopffe bringen?", "tokens": ["Ich", "dir", "die", "phan\u00b7ta\u00b7sie", "soll", "aus", "dem", "kopf\u00b7fe", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bist du zu sehr verpflicht, so kost es zuviel m\u00fch,", "tokens": ["Bist", "du", "zu", "sehr", "ver\u00b7pflicht", ",", "so", "kost", "es", "zu\u00b7viel", "m\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und doch beredet dich nicht mein getreues singen.", "tokens": ["Und", "doch", "be\u00b7re\u00b7det", "dich", "nicht", "mein", "ge\u00b7treu\u00b7es", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was fang ich anders an? ich wei\u00df nicht was ich thu,", "tokens": ["Was", "fang", "ich", "an\u00b7ders", "an", "?", "ich", "wei\u00df", "nicht", "was", "ich", "thu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Noch was ich soll vor trost bey diesen zeilen haben;", "tokens": ["Noch", "was", "ich", "soll", "vor", "trost", "bey", "die\u00b7sen", "zei\u00b7len", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VMFIN", "APPR", "NN", "APPR", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er trincket allbereit dir seine liebe zu,", "tokens": ["Er", "trin\u00b7cket", "all\u00b7be\u00b7reit", "dir", "sei\u00b7ne", "lie\u00b7be", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PPOSAT", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und w\u00fcnschet sich nur bald an deiner brust zu laben.", "tokens": ["Und", "w\u00fcn\u00b7schet", "sich", "nur", "bald", "an", "dei\u00b7ner", "brust", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wilst du denn irgend gar valet der erdeu-bahn,", "tokens": ["Wilst", "du", "denn", "ir\u00b7gend", "gar", "va\u00b7let", "der", "er\u00b7deu\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.26": {"text": "Und aller ihrer lust mit deinem alten geben?", "tokens": ["Und", "al\u00b7ler", "ih\u00b7rer", "lust", "mit", "dei\u00b7nem", "al\u00b7ten", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Stimmst du auch irgend schon ein sterbe-liedgen an?", "tokens": ["Stimmst", "du", "auch", "ir\u00b7gend", "schon", "ein", "ster\u00b7be\u00b7lied\u00b7gen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und wilst mit ihm forthin, als wie im closter leben?", "tokens": ["Und", "wilst", "mit", "ihm", "for\u00b7thin", ",", "als", "wie", "im", "clos\u00b7ter", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "PTKVZ", "$,", "KOUS", "KOKOM", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So la\u00df ichs gerne zu, da\u00df du noch auf der welt,", "tokens": ["So", "la\u00df", "ichs", "ger\u00b7ne", "zu", ",", "da\u00df", "du", "noch", "auf", "der", "welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein fleischern engel wilst aus liebes-busse werden:", "tokens": ["Ein", "flei\u00b7schern", "en\u00b7gel", "wilst", "aus", "lie\u00b7bes\u00b7bus\u00b7se", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So gehts, wer sich zu erst zuviel ans lieben h\u00e4lt,", "tokens": ["So", "gehts", ",", "wer", "sich", "zu", "erst", "zu\u00b7viel", "ans", "lie\u00b7ben", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PRF", "APPR", "ADV", "PIS", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der h\u00fcst hernach davor mit heuchelnden gebehrden.", "tokens": ["Der", "h\u00fcst", "her\u00b7nach", "da\u00b7vor", "mit", "heu\u00b7cheln\u00b7den", "ge\u00b7behr\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Zum ersten lachet man die alten ziemlich aus,", "tokens": ["Zum", "ers\u00b7ten", "la\u00b7chet", "man", "die", "al\u00b7ten", "ziem\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PIS", "ART", "ADJA", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die jungen pflegt man auch nicht wenig zu verspotten,", "tokens": ["Die", "jun\u00b7gen", "pflegt", "man", "auch", "nicht", "we\u00b7nig", "zu", "ver\u00b7spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "ADV", "PTKNEG", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Kommt aber ungel\u00fcck in der verliebten hau\u00df,", "tokens": ["Kommt", "a\u00b7ber", "un\u00b7ge\u00b7l\u00fcck", "in", "der", "ver\u00b7lieb\u00b7ten", "hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So denckt man alle brunst auf einmahl auszurotten.", "tokens": ["So", "denckt", "man", "al\u00b7le", "brunst", "auf", "ein\u00b7mahl", "aus\u00b7zu\u00b7rot\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "APPR", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie geht es aber an? jedweder lacht dabey,", "tokens": ["Wie", "geht", "es", "a\u00b7ber", "an", "?", "jed\u00b7we\u00b7der", "lacht", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Zumahl die, so uns offt mehr, als wir selbsten, kennen,", "tokens": ["Zu\u00b7mahl", "die", ",", "so", "uns", "offt", "mehr", ",", "als", "wir", "selbs\u00b7ten", ",", "ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da spottet jedermann denn unser ziemlich frey,", "tokens": ["Da", "spot\u00b7tet", "je\u00b7der\u00b7mann", "denn", "un\u00b7ser", "ziem\u00b7lich", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PPOSAT", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und pflegt uns da bald dort was anders anzubrennen.", "tokens": ["Und", "pflegt", "uns", "da", "bald", "dort", "was", "an\u00b7ders", "an\u00b7zu\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PWS", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Du alter Spatian! solst du nun itzt der hahn,", "tokens": ["Du", "al\u00b7ter", "Spa\u00b7ti\u00b7an", "!", "solst", "du", "nun", "itzt", "der", "hahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Nachdem das beste weg, in diesem korbe heissen?", "tokens": ["Nach\u00b7dem", "das", "bes\u00b7te", "weg", ",", "in", "die\u00b7sem", "kor\u00b7be", "heis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PTKVZ", "$,", "APPR", "PDAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Tritt ja bey leibe nicht auf diese dornen-bahn,", "tokens": ["Tritt", "ja", "bey", "lei\u00b7be", "nicht", "auf", "die\u00b7se", "dor\u00b7nen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "VVFIN", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Man will dir ein geweyh um deinen sch\u00e4del schmeissen.", "tokens": ["Man", "will", "dir", "ein", "ge\u00b7weyh", "um", "dei\u00b7nen", "sch\u00e4\u00b7del", "schmeis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Sie liebt dich itzund zwar; doch dencke, was hier steht,", "tokens": ["Sie", "liebt", "dich", "it\u00b7zund", "zwar", ";", "doch", "den\u00b7cke", ",", "was", "hier", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$.", "ADV", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Hast du sie einmahl nur an dich als frau genommen,", "tokens": ["Hast", "du", "sie", "ein\u00b7mahl", "nur", "an", "dich", "als", "frau", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "PPER", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So dencke, da\u00df hernach sie so wohl extra geht,", "tokens": ["So", "den\u00b7cke", ",", "da\u00df", "her\u00b7nach", "sie", "so", "wohl", "ex\u00b7tra", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ADV", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Als vor, eh du noch bist in diese falle kommen.", "tokens": ["Als", "vor", ",", "eh", "du", "noch", "bist", "in", "die\u00b7se", "fal\u00b7le", "kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "PDAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die katze l\u00e4sset ja ihr liebes mausen nicht;", "tokens": ["Die", "kat\u00b7ze", "l\u00e4s\u00b7set", "ja", "ihr", "lie\u00b7bes", "mau\u00b7sen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die w\u00f6lffe suchen stets, wie vor, im felde, luder;", "tokens": ["Die", "w\u00f6lf\u00b7fe", "su\u00b7chen", "stets", ",", "wie", "vor", ",", "im", "fel\u00b7de", ",", "lu\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "PTKVZ", "$,", "APPRART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und obgleich Rosilis itzund von bu\u00dfe spricht,", "tokens": ["Und", "ob\u00b7gleich", "Ro\u00b7si\u00b7lis", "it\u00b7zund", "von", "bu\u00b7\u00dfe", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So fehlt es ihr itzund am rechten s\u00fcnden-bruder.", "tokens": ["So", "fehlt", "es", "ihr", "it\u00b7zund", "am", "rech\u00b7ten", "s\u00fcn\u00b7den\u00b7bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Weil du, Zach\u00e4us! dir dennoch nicht rathen l\u00e4st,", "tokens": ["Weil", "du", ",", "Zac\u00b7h\u00e4us", "!", "dir", "den\u00b7noch", "nicht", "ra\u00b7then", "l\u00e4st", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "$.", "PPER", "ADV", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.54": {"text": "So geh nur an dein joch, und ziehe dich zu tode,", "tokens": ["So", "geh", "nur", "an", "dein", "joch", ",", "und", "zie\u00b7he", "dich", "zu", "to\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Dann feyret Rosilis ein rechtes freuden-fest,", "tokens": ["Dann", "fey\u00b7ret", "Ro\u00b7si\u00b7lis", "ein", "rech\u00b7tes", "freu\u00b7den\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Um alte m\u00e4nner ist es itzund so die mode.", "tokens": ["Um", "al\u00b7te", "m\u00e4n\u00b7ner", "ist", "es", "it\u00b7zund", "so", "die", "mo\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "ADJA", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Zieh, schimmel! immer hin, bi\u00df da\u00df du liegen bleibst,", "tokens": ["Zieh", ",", "schim\u00b7mel", "!", "im\u00b7mer", "hin", ",", "bi\u00df", "da\u00df", "du", "lie\u00b7gen", "bleibst", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$.", "ADV", "PTKVZ", "$,", "KON", "KOUS", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Ein junges weib, das ist der alten himmels-wagen,", "tokens": ["Ein", "jun\u00b7ges", "weib", ",", "das", "ist", "der", "al\u00b7ten", "him\u00b7mels\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Die ehre bleibet dir, da\u00df du dich einverleibst", "tokens": ["Die", "eh\u00b7re", "blei\u00b7bet", "dir", ",", "da\u00df", "du", "dich", "ein\u00b7ver\u00b7leibst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Der zunfft, die dich hernach auch wird zu grabe tragen.", "tokens": ["Der", "zunfft", ",", "die", "dich", "her\u00b7nach", "auch", "wird", "zu", "gra\u00b7be", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Ich w\u00fcnsche gute nacht, dir alt- und junges paar!", "tokens": ["Ich", "w\u00fcn\u00b7sche", "gu\u00b7te", "nacht", ",", "dir", "al\u00b7t", "und", "jun\u00b7ges", "paar", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "PPER", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.62": {"text": "(den alten mu\u00df ich doch wohl billich oben setzen)", "tokens": ["(", "den", "al\u00b7ten", "mu\u00df", "ich", "doch", "wohl", "bil\u00b7lich", "o\u00b7ben", "set\u00b7zen", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Bestelle dir itzund die lieb-und todes-bahr,", "tokens": ["Be\u00b7stel\u00b7le", "dir", "it\u00b7zund", "die", "lie\u00b7b\u00b7und", "to\u00b7des\u00b7bahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.64": {"text": "Das eine wird die frau die andre dich erg\u00f6tzen.", "tokens": ["Das", "ei\u00b7ne", "wird", "die", "frau", "die", "and\u00b7re", "dich", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ART", "NN", "ART", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}