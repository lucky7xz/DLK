{"textgrid.poem.46606": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[heut ist voll das f\u00fcnfte Jahr]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut ist voll das f\u00fcnfte Jahr,", "tokens": ["Heut", "ist", "voll", "das", "f\u00fcnf\u00b7te", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit die Mutter dich gebar.", "tokens": ["Seit", "die", "Mut\u00b7ter", "dich", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Andre als Geburteswehen", "tokens": ["And\u00b7re", "als", "Ge\u00b7bur\u00b7tes\u00b7we\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat sie heute zu bestehen;", "tokens": ["Hat", "sie", "heu\u00b7te", "zu", "be\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn nicht wendet Gott den Schlag,", "tokens": ["Wenn", "nicht", "wen\u00b7det", "Gott", "den", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser ist dein Sterbetag.", "tokens": ["Die\u00b7ser", "ist", "dein", "Ster\u00b7be\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat sie jene Weh'n ertragen,", "tokens": ["Hat", "sie", "je\u00b7ne", "Weh'n", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird sie diesen nicht verzagen,", "tokens": ["Wird", "sie", "die\u00b7sen", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie es auch sich wenden mag,", "tokens": ["Wie", "es", "auch", "sich", "wen\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ob du neu mit diesem Tag", "tokens": ["Ob", "du", "neu", "mit", "die\u00b7sem", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Uns geschenkt seist, ob verloren,", "tokens": ["Uns", "ge\u00b7schenkt", "seist", ",", "ob", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "$,", "KOUS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und f\u00fcr h\u00f6hre Welt geboren.", "tokens": ["Und", "f\u00fcr", "h\u00f6h\u00b7re", "Welt", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}