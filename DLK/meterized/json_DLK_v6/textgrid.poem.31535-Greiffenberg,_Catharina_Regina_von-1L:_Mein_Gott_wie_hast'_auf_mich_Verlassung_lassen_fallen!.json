{"textgrid.poem.31535": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Gott wie hast' auf mich Verlassung lassen fallen!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott wie hast' auf mich Verlassung lassen fallen!", "tokens": ["Mein", "Gott", "wie", "hast'", "auf", "mich", "Ver\u00b7las\u00b7sung", "las\u00b7sen", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "VAFIN", "APPR", "PPER", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dein ganzes Zornes-Heer jetzt st\u00fcrmet auf mich ein.", "tokens": ["dein", "gan\u00b7zes", "Zor\u00b7nes\u00b7Heer", "jetzt", "st\u00fcr\u00b7met", "auf", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach! du entzeuchst mir ganz den Gott-und Gnadenschein.", "tokens": ["Ach", "!", "du", "ent\u00b7zeuchst", "mir", "ganz", "den", "Got\u00b7t\u00b7und", "Gna\u00b7den\u00b7schein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich bin ein W\u00fcrmlein nur / das Elendst unter allen.", "tokens": ["Ich", "bin", "ein", "W\u00fcrm\u00b7lein", "nur", "/", "das", "E\u00b7lendst", "un\u00b7ter", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$(", "ART", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein s\u00fcsse Labung / sind die herb' und bittern Gallen.", "tokens": ["Mein", "s\u00fcs\u00b7se", "La\u00b7bung", "/", "sind", "die", "herb'", "und", "bit\u00b7tern", "Gal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "VAFIN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch soll mirs eitel Trost und Zucker-Wollust seyn /", "tokens": ["Doch", "soll", "mirs", "ei\u00b7tel", "Trost", "und", "Zu\u00b7cke\u00b7rWol\u00b7lust", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "ADJD", "NN", "KON", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "wann mit der meinen ich vertrieb der Menschen Pein.", "tokens": ["wann", "mit", "der", "mei\u00b7nen", "ich", "ver\u00b7trieb", "der", "Men\u00b7schen", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "VVFIN", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In gr\u00f6sten Schmerzen pflegt mein Herz vor Lieb zu wallen.", "tokens": ["In", "gr\u00f6s\u00b7ten", "Schmer\u00b7zen", "pflegt", "mein", "Herz", "vor", "Lieb", "zu", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich will mich lieber selbst / als sie / verlassen sehn.", "tokens": ["Ich", "will", "mich", "lie\u00b7ber", "selbst", "/", "als", "sie", "/", "ver\u00b7las\u00b7sen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPER", "$(", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd wann ich noch so viel / ja mehr noch / aus solt stehn /", "tokens": ["Vnd", "wann", "ich", "noch", "so", "viel", "/", "ja", "mehr", "noch", "/", "aus", "solt", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "ADV", "$(", "ADV", "ADV", "ADV", "$(", "APPR", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so tauret mich doch nichts: wann sie es nur geniessen.", "tokens": ["so", "tau\u00b7ret", "mich", "doch", "nichts", ":", "wann", "sie", "es", "nur", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "$.", "PWAV", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein' Haupt-Verlassung / sey ihr st\u00e4ter Trostes-Brunn.", "tokens": ["Mein'", "Haup\u00b7tVer\u00b7las\u00b7sung", "/", "sey", "ihr", "st\u00e4\u00b7ter", "Tros\u00b7tes\u00b7Brunn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df sie sie finden st\u00e4ts / mir alle H\u00fclf zerrunn.", "tokens": ["Da\u00df", "sie", "sie", "fin\u00b7den", "st\u00e4ts", "/", "mir", "al\u00b7le", "H\u00fclf", "zer\u00b7runn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VVFIN", "$(", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein Blut soll von mir weg / sie zu erquicken / fliessen.", "tokens": ["Mein", "Blut", "soll", "von", "mir", "weg", "/", "sie", "zu", "er\u00b7qui\u00b7cken", "/", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "PTKVZ", "$(", "PPER", "PTKZU", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}