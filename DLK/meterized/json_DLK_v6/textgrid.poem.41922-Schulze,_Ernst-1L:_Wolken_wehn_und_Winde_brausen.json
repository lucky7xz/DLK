{"textgrid.poem.41922": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wolken wehn und Winde brausen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolken wehn und Winde brausen", "tokens": ["Wol\u00b7ken", "wehn", "und", "Win\u00b7de", "brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Burg im Mondenstrahl,", "tokens": ["Um", "die", "Burg", "im", "Mon\u00b7dens\u00b7trahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die W\u00e4lder zieht das Grausen", "tokens": ["Durch", "die", "W\u00e4l\u00b7der", "zieht", "das", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Schlummer durch das Thal.", "tokens": ["Und", "der", "Schlum\u00b7mer", "durch", "das", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Und ich stehe dir so ferne", "tokens": ["Und", "ich", "ste\u00b7he", "dir", "so", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem ragenden Gestein;", "tokens": ["Auf", "dem", "ra\u00b7gen\u00b7den", "Ge\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die Winde, nur die Sterne", "tokens": ["Nur", "die", "Win\u00b7de", ",", "nur", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnen meine Boten seyn.", "tokens": ["K\u00f6n\u00b7nen", "mei\u00b7ne", "Bo\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Traurig wehn die Wind' hin\u00fcber;", "tokens": ["Trau\u00b7rig", "wehn", "die", "Wind'", "hin\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Sterne leuchten mild;", "tokens": ["Doch", "die", "Ster\u00b7ne", "leuch\u00b7ten", "mild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Boten send' ich lieber,", "tokens": ["Kei\u00b7ne", "Bo\u00b7ten", "send'", "ich", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie bringen dir mein Bild.", "tokens": ["Denn", "sie", "brin\u00b7gen", "dir", "mein", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denkst du bey der Winde Wehen", "tokens": ["Denkst", "du", "bey", "der", "Win\u00b7de", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch an meine Klagen nicht,", "tokens": ["Auch", "an", "mei\u00b7ne", "Kla\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Liebe kannst du sehen", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "kannst", "du", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der Sterne stillem Licht.", "tokens": ["An", "der", "Ster\u00b7ne", "stil\u00b7lem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}