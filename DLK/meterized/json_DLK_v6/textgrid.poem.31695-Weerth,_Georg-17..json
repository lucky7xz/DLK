{"textgrid.poem.31695": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sah in den Wolken das Abendrot,", "tokens": ["Sie", "sah", "in", "den", "Wol\u00b7ken", "das", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da kam ihr Herz in gro\u00dfe Not.", "tokens": ["Da", "kam", "ihr", "Herz", "in", "gro\u00b7\u00dfe", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie machte sich hurtig auf den Weg,", "tokens": ["Sie", "mach\u00b7te", "sich", "hur\u00b7tig", "auf", "den", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl durch den Garten, wohl \u00fcber den Steg.", "tokens": ["Wohl", "durch", "den", "Gar\u00b7ten", ",", "wohl", "\u00fc\u00b7ber", "den", "Steg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Und als sie kam durch den Tannenwald:", "tokens": ["Und", "als", "sie", "kam", "durch", "den", "Tan\u00b7nen\u00b7wald", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie meinte, sie s\u00e4h eine wei\u00dfe Gestalt.", "tokens": ["Sie", "mein\u00b7te", ",", "sie", "s\u00e4h", "ei\u00b7ne", "wei\u00b7\u00dfe", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Und als sie rasch durch die Erlen schritt:", "tokens": ["Und", "als", "sie", "rasch", "durch", "die", "Er\u00b7len", "schritt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie meinte, stets liefe noch jemand mit.", "tokens": ["Sie", "mein\u00b7te", ",", "stets", "lie\u00b7fe", "noch", "je\u00b7mand", "mit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Und als sie sprang an der Weide vorbei:", "tokens": ["Und", "als", "sie", "sprang", "an", "der", "Wei\u00b7de", "vor\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auffuhren die Rosse mit wildem Schrei!", "tokens": ["Auf\u00b7fuh\u00b7ren", "die", "Ros\u00b7se", "mit", "wil\u00b7dem", "Schrei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Und als sie vor\u00fcberjagte am Moor:", "tokens": ["Und", "als", "sie", "vor\u00b7\u00fc\u00b7ber\u00b7jag\u00b7te", "am", "Moor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da huschte ein schwarzer Vogel empor.", "tokens": ["Da", "huschte", "ein", "schwar\u00b7zer", "Vo\u00b7gel", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Und als sie die Lichter im Dorfe schaut':", "tokens": ["Und", "als", "sie", "die", "Lich\u00b7ter", "im", "Dor\u00b7fe", "schaut'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da bellten die Hunde so laut, so laut.", "tokens": ["Da", "bell\u00b7ten", "die", "Hun\u00b7de", "so", "laut", ",", "so", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Und als sie genommen zur Kirche den Lauf:", "tokens": ["Und", "als", "sie", "ge\u00b7nom\u00b7men", "zur", "Kir\u00b7che", "den", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Uhr h\u00f6rte mitten im Schlagen auf.", "tokens": ["Die", "Uhr", "h\u00f6r\u00b7te", "mit\u00b7ten", "im", "Schla\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-++-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Und als sie rannte zur H\u00fctte drauf:", "tokens": ["Und", "als", "sie", "rann\u00b7te", "zur", "H\u00fct\u00b7te", "drauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da flogen die T\u00fcren von selber auf.", "tokens": ["Da", "flo\u00b7gen", "die", "T\u00fc\u00b7ren", "von", "sel\u00b7ber", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "Und als sie fragte, welch Leid geschehn:", "tokens": ["Und", "als", "sie", "frag\u00b7te", ",", "welch", "Leid", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PWAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da wollte kein Mensch ihr Rede stehn.", "tokens": ["Da", "woll\u00b7te", "kein", "Mensch", "ihr", "Re\u00b7de", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Und als sie fragt', wer erschlagen w\u00e4r \u2013", "tokens": ["Und", "als", "sie", "fragt'", ",", "wer", "er\u00b7schla\u00b7gen", "w\u00e4r", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PWS", "VVPP", "VAFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da trug man den toten Knaben daher.", "tokens": ["Da", "trug", "man", "den", "to\u00b7ten", "Kna\u00b7ben", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}