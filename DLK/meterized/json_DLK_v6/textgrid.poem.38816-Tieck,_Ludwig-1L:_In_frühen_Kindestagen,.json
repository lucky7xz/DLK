{"textgrid.poem.38816": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: In fr\u00fchen Kindestagen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In fr\u00fchen Kindestagen,", "tokens": ["In", "fr\u00fc\u00b7hen", "Kin\u00b7des\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus Trutz und frevlem Muth,", "tokens": ["Aus", "Trutz", "und", "frev\u00b7lem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entlief der Burg zu Santen", "tokens": ["Ent\u00b7lief", "der", "Burg", "zu", "San\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Siegfried, ein Recke gut.", "tokens": ["Sieg\u00b7fried", ",", "ein", "Re\u00b7cke", "gut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er kam nach vielem Irren", "tokens": ["Er", "kam", "nach", "vie\u00b7lem", "Ir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In einen fernen Wald,", "tokens": ["In", "ei\u00b7nen", "fer\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah da die gro\u00dfe Schmiede,", "tokens": ["Sah", "da", "die", "gro\u00b7\u00dfe", "Schmie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein trat der Knabe bald.", "tokens": ["Ein", "trat", "der", "Kna\u00b7be", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier wohnt' mit seinen K\u00fcnsten", "tokens": ["Hier", "wohnt'", "mit", "sei\u00b7nen", "K\u00fcns\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mimer, ein Held bekannt,", "tokens": ["Mi\u00b7mer", ",", "ein", "Held", "be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der mit vielen Geh\u00fclfen", "tokens": ["Der", "mit", "vie\u00b7len", "Ge\u00b7h\u00fcl\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schmiedete sch\u00f6n Gewand.", "tokens": ["Schmie\u00b7de\u00b7te", "sch\u00f6n", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Er wirkte edle Schwerdter,", "tokens": ["Er", "wirk\u00b7te", "ed\u00b7le", "Schwerd\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Panzer und Schilde breit,", "tokens": ["Pan\u00b7zer", "und", "Schil\u00b7de", "breit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die kauften werthe Recken", "tokens": ["Die", "kauf\u00b7ten", "wert\u00b7he", "Re\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und K\u00f6n'ge hoch erfreut.", "tokens": ["Und", "K\u00f6n'\u00b7ge", "hoch", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er war ein Held gewaltig,", "tokens": ["Er", "war", "ein", "Held", "ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu ihm trat Siegfried ein,", "tokens": ["Zu", "ihm", "trat", "Sieg\u00b7fried", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wollt' im gr\u00fcnen Walde", "tokens": ["Und", "wollt'", "im", "gr\u00fc\u00b7nen", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mimers Geh\u00fclfe seyn.", "tokens": ["Mi\u00b7mers", "Ge\u00b7h\u00fcl\u00b7fe", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Als gr\u00f6\u00dfer ward der Knabe", "tokens": ["Als", "gr\u00f6\u00b7\u00dfer", "ward", "der", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zeigt' er viel b\u00f6sen Sinn,", "tokens": ["Zeigt'", "er", "viel", "b\u00f6\u00b7sen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er droht' und plagte alle,", "tokens": ["Er", "droht'", "und", "plag\u00b7te", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Meister furchte ihn:", "tokens": ["Der", "Meis\u00b7ter", "furch\u00b7te", "ihn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er stellt' ihn an die Arbeit", "tokens": ["Er", "stellt'", "ihn", "an", "die", "Ar\u00b7beit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An einem Sommertag,", "tokens": ["An", "ei\u00b7nem", "Som\u00b7mer\u00b7tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da nahm Siegfried den Hammer", "tokens": ["Da", "nahm", "Sieg\u00b7fried", "den", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und that so kr\u00e4ftgen Schlag,", "tokens": ["Und", "that", "so", "kr\u00e4ft\u00b7gen", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da\u00df er den Ambos spaltete", "tokens": ["Da\u00df", "er", "den", "Am\u00b7bos", "spal\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlug ihn in den Grund,", "tokens": ["Und", "schlug", "ihn", "in", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darob sie all erschraken", "tokens": ["Da\u00b7rob", "sie", "all", "er\u00b7schra\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00fcnschten zu der Stund,", "tokens": ["Und", "w\u00fcnschten", "zu", "der", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Er w\u00e4re nie gekommen,", "tokens": ["Er", "w\u00e4\u00b7re", "nie", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie hatten sein nicht Noth,", "tokens": ["Sie", "hat\u00b7ten", "sein", "nicht", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "PTKNEG", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie furchten da\u00df der Gro\u00dfe", "tokens": ["Sie", "furch\u00b7ten", "da\u00df", "der", "Gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie alle schl\u00fcge todt.", "tokens": ["Sie", "al\u00b7le", "schl\u00fc\u00b7ge", "todt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ein giftiger Linddrache", "tokens": ["Ein", "gif\u00b7ti\u00b7ger", "Lind\u00b7dra\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort in dem Walde was,", "tokens": ["Dort", "in", "dem", "Wal\u00b7de", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PWS", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vor dessen grimmen Rachen", "tokens": ["Vor", "des\u00b7sen", "grim\u00b7men", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der K\u00fchnste nicht genas.", "tokens": ["Der", "K\u00fchns\u00b7te", "nicht", "ge\u00b7nas", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Mimer in seinen Listen", "tokens": ["Mi\u00b7mer", "in", "sei\u00b7nen", "Lis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dachte mit klugem Sinn:", "tokens": ["Dach\u00b7te", "mit", "klu\u00b7gem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Knab' wird sich nicht fristen;", "tokens": ["Der", "Knab'", "wird", "sich", "nicht", "fris\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sandt' ihn zum Wurme hin.", "tokens": ["Sandt'", "ihn", "zum", "Wur\u00b7me", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da folgt der J\u00fcngling k\u00fchne", "tokens": ["Da", "folgt", "der", "J\u00fcng\u00b7ling", "k\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem anbefohlnen Werke,", "tokens": ["Dem", "an\u00b7be\u00b7fohl\u00b7nen", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ohn' Waffen in der Gr\u00fcne,", "tokens": ["Ohn'", "Waf\u00b7fen", "in", "der", "Gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur in selbsteigner St\u00e4rke.", "tokens": ["Nur", "in", "selbs\u00b7teig\u00b7ner", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der Drache scho\u00df im Grimme", "tokens": ["Der", "Dra\u00b7che", "scho\u00df", "im", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus seiner H\u00f6hle wild,", "tokens": ["Aus", "sei\u00b7ner", "H\u00f6h\u00b7le", "wild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den jungen Ritter schirmten", "tokens": ["Den", "jun\u00b7gen", "Rit\u00b7ter", "schirm\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Baumzweige wie ein Schild.", "tokens": ["Baum\u00b7zwei\u00b7ge", "wie", "ein", "Schild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Damit k\u00e4mpft' er so kr\u00e4ftig", "tokens": ["Da\u00b7mit", "k\u00e4mpft'", "er", "so", "kr\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schlug das Ungeheuer,", "tokens": ["Und", "schlug", "das", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann a\u00df er in dem Walde", "tokens": ["Dann", "a\u00df", "er", "in", "dem", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und z\u00fcndete ein Feuer,", "tokens": ["Und", "z\u00fcn\u00b7de\u00b7te", "ein", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Im Drachenblut er badete,", "tokens": ["Im", "Dra\u00b7chen\u00b7blut", "er", "ba\u00b7de\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00fcrnen ward seine Haut,", "tokens": ["H\u00fcr\u00b7nen", "ward", "sei\u00b7ne", "Haut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Kein Waffen ihm nun schadete,", "tokens": ["Kein", "Waf\u00b7fen", "ihm", "nun", "scha\u00b7de\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie scharf es auf ihn haut.", "tokens": ["Wie", "scharf", "es", "auf", "ihn", "haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "In sehr grimmigen Muthe", "tokens": ["In", "sehr", "grim\u00b7mi\u00b7gen", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ri\u00df er vom Wurm das Haupt,", "tokens": ["Ri\u00df", "er", "vom", "Wurm", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rennt durch Waldesdunkel,", "tokens": ["Und", "rennt", "durch", "Wal\u00b7des\u00b7dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als schon der Meister glaubt", "tokens": ["Als", "schon", "der", "Meis\u00b7ter", "glaubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Er sey im Wald erstorben.", "tokens": ["Er", "sey", "im", "Wald", "ers\u00b7tor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schreien die Gesellen:", "tokens": ["Da", "schrei\u00b7en", "die", "Ge\u00b7sel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sehen Siegfried kommen,", "tokens": ["Wir", "se\u00b7hen", "Sieg\u00b7fried", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der wird uns alle f\u00e4llen!", "tokens": ["Der", "wird", "uns", "al\u00b7le", "f\u00e4l\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Er tr\u00e4gt das Wurmhaupt blutig", "tokens": ["Er", "tr\u00e4gt", "das", "Wurm\u00b7haupt", "blu\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie einen Schildesrand!", "tokens": ["Wie", "ei\u00b7nen", "Schil\u00b7des\u00b7rand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Siegfried trat ein wildmuthig,", "tokens": ["Sieg\u00b7fried", "trat", "ein", "wild\u00b7mut\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie flohn zur Steineswand.", "tokens": ["Sie", "flohn", "zur", "Stei\u00b7nes\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Mimer ging ihm entgegen,", "tokens": ["Mi\u00b7mer", "ging", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Er sah des J\u00fcnglings Wuth,", "tokens": ["Er", "sah", "des", "J\u00fcng\u00b7lings", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um Gnade bat der Degen,", "tokens": ["Um", "Gna\u00b7de", "bat", "der", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Harnisch und Schwerdter gut", "tokens": ["Har\u00b7nisch", "und", "Schwerd\u00b7ter", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "NN", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.20": {"line.1": {"text": "Versprach er fleh'nd dem Werthen:", "tokens": ["Ver\u00b7sprach", "er", "fleh'nd", "dem", "Wert\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Siegfried nichts sagte wieder,", "tokens": ["Sieg\u00b7fried", "nichts", "sag\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Haupt warf er zur Erden", "tokens": ["Das", "Haupt", "warf", "er", "zur", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schlug den Meister nieder.", "tokens": ["Und", "schlug", "den", "Meis\u00b7ter", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Auf sa\u00df er dann zu Rosse,", "tokens": ["Auf", "sa\u00df", "er", "dann", "zu", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nahm ein Sturmgewand,", "tokens": ["Und", "nahm", "ein", "Sturm\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht sucht' er die Genossen,", "tokens": ["Nicht", "sucht'", "er", "die", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weit fuhr er durch das Land.", "tokens": ["Weit", "fuhr", "er", "durch", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}