{"dta.poem.20332": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Flavien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Will das gel\u00fccke denn gantz meine feindin werden?", "tokens": ["Will", "das", "ge\u00b7l\u00fc\u00b7cke", "denn", "gantz", "mei\u00b7ne", "fein\u00b7din", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "KON", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "St\u00fcrmt s\u00fcd/ ost/ nord und west?", "tokens": ["St\u00fcrmt", "s\u00fcd", "/", "ost", "/", "nord", "und", "west", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "VVFIN", "$(", "NE", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bin ich ein gauckel-spiel/ und leichter ball der erden/", "tokens": ["Bin", "ich", "ein", "gau\u00b7ckel\u00b7spiel", "/", "und", "leich\u00b7ter", "ball", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$(", "KON", "ADJD", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Venus fallen l\u00e4st?", "tokens": ["Den", "Ve\u00b7nus", "fal\u00b7len", "l\u00e4st", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will keine sonne mehr mein schwartzes haupt ber\u00fchren?", "tokens": ["Will", "kei\u00b7ne", "son\u00b7ne", "mehr", "mein", "schwart\u00b7zes", "haupt", "be\u00b7r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Umh\u00fcllt mich nichts als nacht?", "tokens": ["Um\u00b7h\u00fcllt", "mich", "nichts", "als", "nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Will das verh\u00e4ngni\u00df mich an einer keite f\u00fchren/", "tokens": ["Will", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "mich", "an", "ei\u00b7ner", "kei\u00b7te", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So mich verzweiffelt macht?", "tokens": ["So", "mich", "ver\u00b7zweif\u00b7felt", "macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Orontes wei\u00df fast nicht was ferner sey zu dencken/", "tokens": ["O\u00b7ron\u00b7tes", "wei\u00df", "fast", "nicht", "was", "fer\u00b7ner", "sey", "zu", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PTKNEG", "PWS", "ADV", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Z\u00fcrnt Venus oder du?", "tokens": ["Z\u00fcrnt", "Ve\u00b7nus", "o\u00b7der", "du", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ach gl\u00e4ub es! Flavia/ mein leiden und dein kr\u00e4ncken", "tokens": ["Ach", "gl\u00e4ub", "es", "!", "Fla\u00b7via", "/", "mein", "lei\u00b7den", "und", "dein", "kr\u00e4n\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "$.", "NE", "$(", "PPOSAT", "ADJA", "KON", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Kommt mir und dir nicht zu;", "tokens": ["Kommt", "mir", "und", "dir", "nicht", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Was hab ich dir gethan/ da\u00df du mich nicht wilst kennen?", "tokens": ["Was", "hab", "ich", "dir", "ge\u00b7than", "/", "da\u00df", "du", "mich", "nicht", "wilst", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVPP", "$(", "KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie heist du meine schuld?", "tokens": ["Wie", "heist", "du", "mei\u00b7ne", "schuld", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Soll mein verbrechen sich mit rechten namen nennen?", "tokens": ["Soll", "mein", "ver\u00b7bre\u00b7chen", "sich", "mit", "rech\u00b7ten", "na\u00b7men", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So rufft es: Lieb und huld/", "tokens": ["So", "rufft", "es", ":", "Lieb", "und", "huld", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Du l\u00e4st mein auge nicht zu deinen gr\u00e4ntzen dringen/", "tokens": ["Du", "l\u00e4st", "mein", "au\u00b7ge", "nicht", "zu", "dei\u00b7nen", "gr\u00e4nt\u00b7zen", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mein auge sonder licht/", "tokens": ["Mein", "au\u00b7ge", "son\u00b7der", "licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Du deckst den sch\u00f6nen mund mit deines schatten schwingen/", "tokens": ["Du", "deckst", "den", "sch\u00f6\u00b7nen", "mund", "mit", "dei\u00b7nes", "schat\u00b7ten", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und kennst mich ferner nicht.", "tokens": ["Und", "kennst", "mich", "fer\u00b7ner", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Das basiliscken-gifft/ der rauch von allen drachen/", "tokens": ["Das", "ba\u00b7si\u00b7lis\u00b7cken\u00b7gifft", "/", "der", "rauch", "von", "al\u00b7len", "dra\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der flederm\u00e4use blut/", "tokens": ["Der", "fle\u00b7der\u00b7m\u00e4u\u00b7se", "blut", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Kan meiner Flavie nicht solchen eckel machen/", "tokens": ["Kan", "mei\u00b7ner", "Fla\u00b7vie", "nicht", "sol\u00b7chen", "ec\u00b7kel", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Als des Orontes hut.", "tokens": ["Als", "des", "O\u00b7ron\u00b7tes", "hut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Doch hab ich dich erz\u00fcrt/ so will ich treulich b\u00fcssen/", "tokens": ["Doch", "hab", "ich", "dich", "er\u00b7z\u00fcrt", "/", "so", "will", "ich", "treu\u00b7lich", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "VVPP", "$(", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es schweret hand und geist.", "tokens": ["Es", "schwe\u00b7ret", "hand", "und", "geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Wie solte nicht mein blut mit reichen str\u00f6hmen fliessen/", "tokens": ["Wie", "sol\u00b7te", "nicht", "mein", "blut", "mit", "rei\u00b7chen", "str\u00f6h\u00b7men", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn du es springen heist.", "tokens": ["Wenn", "du", "es", "sprin\u00b7gen", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Solt ich/ o Flavia! zu deinen f\u00fcssen sterben/", "tokens": ["Solt", "ich", "/", "o", "Fla\u00b7via", "!", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "ster\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "FM", "NE", "$.", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.30": {"text": "So st\u00fcrb ich ohne spott;", "tokens": ["So", "st\u00fcrb", "ich", "oh\u00b7ne", "spott", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Denn liebe/ so nicht kan die gegenlieb erwerben/", "tokens": ["Denn", "lie\u00b7be", "/", "so", "nicht", "kan", "die", "ge\u00b7gen\u00b7lieb", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADV", "PTKNEG", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ist \u00e4rger als der tod.", "tokens": ["Ist", "\u00e4r\u00b7ger", "als", "der", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}