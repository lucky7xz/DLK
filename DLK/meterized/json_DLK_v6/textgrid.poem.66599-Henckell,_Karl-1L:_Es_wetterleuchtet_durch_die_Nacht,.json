{"textgrid.poem.66599": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es wetterleuchtet durch die Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wetterleuchtet durch die Nacht,", "tokens": ["Es", "wet\u00b7ter\u00b7leuch\u00b7tet", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Donner, sie rollen von ferne,", "tokens": ["Die", "Don\u00b7ner", ",", "sie", "rol\u00b7len", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Wolken st\u00fcrmen zur wilden Schlacht,", "tokens": ["Die", "Wol\u00b7ken", "st\u00fcr\u00b7men", "zur", "wil\u00b7den", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und \u00e4ngstlich verl\u00f6schen die Sterne.", "tokens": ["Und", "\u00e4ngst\u00b7lich", "ver\u00b7l\u00f6\u00b7schen", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Es jagt und wettert und kracht und braust,", "tokens": ["Es", "jagt", "und", "wet\u00b7tert", "und", "kracht", "und", "braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie wenn in L\u00fcften der B\u00f6se haust \u2013", "tokens": ["Wie", "wenn", "in", "L\u00fcf\u00b7ten", "der", "B\u00f6\u00b7se", "haust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPR", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Was schmiegst du dich an mich mit Zittern?", "tokens": ["Was", "schmiegst", "du", "dich", "an", "mich", "mit", "Zit\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "He, holla! Mich freut das Gewittern.", "tokens": ["He", ",", "hol\u00b7la", "!", "Mich", "freut", "das", "Ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Kennst du das Leben, mein liebes Kind?", "tokens": ["Kennst", "du", "das", "Le\u00b7ben", ",", "mein", "lie\u00b7bes", "Kind", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ach nein, du t\u00e4ndelst in Tr\u00e4umen.", "tokens": ["Ach", "nein", ",", "du", "t\u00e4n\u00b7delst", "in", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oft st\u00fcrmt durch das Leben der Wirbelwind", "tokens": ["Oft", "st\u00fcrmt", "durch", "das", "Le\u00b7ben", "der", "Wir\u00b7bel\u00b7wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und rei\u00dft an den knorrigsten B\u00e4umen.", "tokens": ["Und", "rei\u00dft", "an", "den", "knor\u00b7rigs\u00b7ten", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Unter Donner und Blitzen, in st\u00fcrmischer Nacht", "tokens": ["Un\u00b7ter", "Don\u00b7ner", "und", "Blit\u00b7zen", ",", "in", "st\u00fcr\u00b7mi\u00b7scher", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Schl\u00e4gt der Mensch mit dem Schicksal die lustige Schlacht.", "tokens": ["Schl\u00e4gt", "der", "Mensch", "mit", "dem", "Schick\u00b7sal", "die", "lus\u00b7ti\u00b7ge", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Was schmiegst du dich an mich mit Zittern?", "tokens": ["Was", "schmiegst", "du", "dich", "an", "mich", "mit", "Zit\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "He, holla! Mich freut das Gewittern.", "tokens": ["He", ",", "hol\u00b7la", "!", "Mich", "freut", "das", "Ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Wie brannte die Sonne so hei\u00df und so dumpf!", "tokens": ["Wie", "brann\u00b7te", "die", "Son\u00b7ne", "so", "hei\u00df", "und", "so", "dumpf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die B\u00e4ume, sie rangen nach Odem;", "tokens": ["Die", "B\u00e4u\u00b7me", ",", "sie", "ran\u00b7gen", "nach", "O\u00b7dem", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nun flutet es feucht, und der d\u00fcrrste Stumpf", "tokens": ["Nun", "flu\u00b7tet", "es", "feucht", ",", "und", "der", "d\u00fcrrs\u00b7te", "Stumpf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Saugt ein den k\u00f6stlichen Brodem.", "tokens": ["Saugt", "ein", "den", "k\u00f6st\u00b7li\u00b7chen", "Bro\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wenn tr\u00e4ge die Sonne das Leben verbrennt,", "tokens": ["Wenn", "tr\u00e4\u00b7ge", "die", "Son\u00b7ne", "das", "Le\u00b7ben", "ver\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Willkommen dann, schlagendes Element!", "tokens": ["Will\u00b7kom\u00b7men", "dann", ",", "schla\u00b7gen\u00b7des", "E\u00b7le\u00b7ment", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "La\u00df ab von Zagen und Zittern,", "tokens": ["La\u00df", "ab", "von", "Za\u00b7gen", "und", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "He, holla! Mich freut das Gewittern.", "tokens": ["He", ",", "hol\u00b7la", "!", "Mich", "freut", "das", "Ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}