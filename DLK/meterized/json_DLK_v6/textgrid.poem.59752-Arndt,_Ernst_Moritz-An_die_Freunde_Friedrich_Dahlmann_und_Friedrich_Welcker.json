{"textgrid.poem.59752": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "An die Freunde Friedrich Dahlmann und Friedrich Welcker", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid gegr\u00fc\u00dft, ihr treuen Alten,", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "treu\u00b7en", "Al\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dem alten Gott vertraun,", "tokens": ["Die", "dem", "al\u00b7ten", "Gott", "ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch des Altertums Gestalten", "tokens": ["Durch", "des", "Al\u00b7ter\u00b7tums", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin auf neue Sch\u00f6pfung schaun.", "tokens": ["Hin", "auf", "neu\u00b7e", "Sch\u00f6p\u00b7fung", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Her die H\u00e4nde auf den Glauben,", "tokens": ["Her", "die", "H\u00e4n\u00b7de", "auf", "den", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sein ", "tokens": ["Der", "sein"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und, wieviel auch Narren schnauben,", "tokens": ["Und", ",", "wie\u00b7viel", "auch", "Nar\u00b7ren", "schnau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch der Ewiggleiche bleibt.", "tokens": ["Doch", "der", "E\u00b7wig\u00b7glei\u00b7che", "bleibt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vaterland und Freiheit haben", "tokens": ["Va\u00b7ter\u00b7land", "und", "Frei\u00b7heit", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir in stillem Streit gesucht,", "tokens": ["Wir", "in", "stil\u00b7lem", "Streit", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollten nicht, da\u00df Kr\u00e4hn und Raben", "tokens": ["Woll\u00b7ten", "nicht", ",", "da\u00df", "Kr\u00e4hn", "und", "Ra\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "$,", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frech bekr\u00e4chzen Adlerflucht.", "tokens": ["Frech", "be\u00b7kr\u00e4ch\u00b7zen", "Ad\u00b7ler\u00b7flucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Haben auf die Adlersiege", "tokens": ["Ha\u00b7ben", "auf", "die", "Ad\u00b7ler\u00b7sie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Fest gehofft und treu geglaubt,", "tokens": ["Fest", "ge\u00b7hofft", "und", "treu", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch fiel in dem schweren Kriege", "tokens": ["Doch", "fiel", "in", "dem", "schwe\u00b7ren", "Krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mancher Tropfen Schwei\u00df vom Haupt.", "tokens": ["Man\u00b7cher", "Trop\u00b7fen", "Schwei\u00df", "vom", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so schaun trotz feiger Tadler", "tokens": ["Und", "so", "schaun", "trotz", "fei\u00b7ger", "Tad\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und trotz feiler Knechte Witz", "tokens": ["Und", "trotz", "fei\u00b7ler", "Knech\u00b7te", "Witz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir von fern den deutschen Adler", "tokens": ["Wir", "von", "fern", "den", "deut\u00b7schen", "Ad\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem alten Donnerblitz.", "tokens": ["Mit", "dem", "al\u00b7ten", "Don\u00b7ner\u00b7blitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, schon saust es und wird kommen \u2013", "tokens": ["Ja", ",", "schon", "saust", "es", "und", "wird", "kom\u00b7men", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "KON", "VAFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutschland, s\u00fc\u00dfes Vaterland!", "tokens": ["Deutschland", ",", "s\u00fc\u00b7\u00dfes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Alle Tapfern, Treuen, Frommen", "tokens": ["Al\u00b7le", "Tap\u00b7fern", ",", "Treu\u00b7en", ",", "From\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind dem Wetter zugewandt.", "tokens": ["Sind", "dem", "Wet\u00b7ter", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schrei' der P\u00f6belschwarm sich heiser,", "tokens": ["Schrei'", "der", "P\u00f6\u00b7bel\u00b7schwarm", "sich", "hei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sich fern zusammenballt,", "tokens": ["Was", "sich", "fern", "zu\u00b7sam\u00b7men\u00b7ballt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem blitzt der Donnerweiser", "tokens": ["Aus", "dem", "blitzt", "der", "Don\u00b7ner\u00b7wei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neuen Lebens Lichtgestalt.", "tokens": ["Neu\u00b7en", "Le\u00b7bens", "Licht\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}