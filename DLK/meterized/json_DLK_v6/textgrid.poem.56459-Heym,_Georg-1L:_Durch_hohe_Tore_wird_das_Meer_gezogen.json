{"textgrid.poem.56459": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch hohe Tore wird das Meer gezogen", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch hohe Tore wird das Meer gezogen", "tokens": ["Durch", "ho\u00b7he", "To\u00b7re", "wird", "das", "Meer", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und goldne Wolkens\u00e4ulen, wo noch s\u00e4umt", "tokens": ["Und", "gold\u00b7ne", "Wol\u00b7ken\u00b7s\u00e4u\u00b7len", ",", "wo", "noch", "s\u00e4umt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PWAV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der sp\u00e4te Tag am hellen Himmelsbogen", "tokens": ["Der", "sp\u00e4\u00b7te", "Tag", "am", "hel\u00b7len", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fern hinab des Meeres Weite tr\u00e4umt.", "tokens": ["Und", "fern", "hin\u00b7ab", "des", "Mee\u00b7res", "Wei\u00b7te", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbvergi\u00df der Traurigkeit, die sich verlor", "tokens": ["\u00bb", "ver\u00b7gi\u00df", "der", "Trau\u00b7rig\u00b7keit", ",", "die", "sich", "ver\u00b7lor"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ins ferne Spiel der Wasser, und der Zeit", "tokens": ["Ins", "fer\u00b7ne", "Spiel", "der", "Was\u00b7ser", ",", "und", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Versunkner Tage. Singt der Wind ins Ohr", "tokens": ["Ver\u00b7sun\u00b7kner", "Ta\u00b7ge", ".", "Singt", "der", "Wind", "ins", "Ohr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dir seine Schwermut, h\u00f6re nicht sein Leid.", "tokens": ["Dir", "sei\u00b7ne", "Schwer\u00b7mut", ",", "h\u00f6\u00b7re", "nicht", "sein", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00df ab von Weinen. Bei den Toten unten", "tokens": ["La\u00df", "ab", "von", "Wei\u00b7nen", ".", "Bei", "den", "To\u00b7ten", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "APPR", "NN", "$.", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Schattenlande werden bald wir wohnen", "tokens": ["Im", "Schat\u00b7ten\u00b7lan\u00b7de", "wer\u00b7den", "bald", "wir", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ewig schlafen in den Tiefen drunten,", "tokens": ["Und", "e\u00b7wig", "schla\u00b7fen", "in", "den", "Tie\u00b7fen", "drun\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In den verborgenen St\u00e4dten der D\u00e4monen.", "tokens": ["In", "den", "ver\u00b7bor\u00b7ge\u00b7nen", "St\u00e4d\u00b7ten", "der", "D\u00e4\u00b7mo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Dort wird uns Einsamkeit die Lider schlie\u00dfen.", "tokens": ["Dort", "wird", "uns", "Ein\u00b7sam\u00b7keit", "die", "Li\u00b7der", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir h\u00f6ren nichts in unserer Hallen R\u00e4umen,", "tokens": ["Wir", "h\u00f6\u00b7ren", "nichts", "in", "un\u00b7se\u00b7rer", "Hal\u00b7len", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Fische nur, die durch die Fenster schie\u00dfen,", "tokens": ["Die", "Fi\u00b7sche", "nur", ",", "die", "durch", "die", "Fens\u00b7ter", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und leisen Wind in den Korallenb\u00e4umen.", "tokens": ["Und", "lei\u00b7sen", "Wind", "in", "den", "Ko\u00b7ral\u00b7len\u00b7b\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir werden immer beieinander bleiben", "tokens": ["Wir", "wer\u00b7den", "im\u00b7mer", "bei\u00b7ein\u00b7an\u00b7der", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im schattenhaften Walde auf dem Grunde.", "tokens": ["Im", "schat\u00b7ten\u00b7haf\u00b7ten", "Wal\u00b7de", "auf", "dem", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die gleiche Woge wird uns dunkel treiben,", "tokens": ["Die", "glei\u00b7che", "Wo\u00b7ge", "wird", "uns", "dun\u00b7kel", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gleiche Tr\u00e4ume trinkt der Ku\u00df vom Munde.", "tokens": ["Und", "glei\u00b7che", "Tr\u00e4u\u00b7me", "trinkt", "der", "Ku\u00df", "vom", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Tod ist sanft. Und die uns niemand gab,", "tokens": ["Der", "Tod", "ist", "sanft", ".", "Und", "die", "uns", "nie\u00b7mand", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KON", "ART", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er gibt uns Heimat. Und er tr\u00e4gt uns weich", "tokens": ["Er", "gibt", "uns", "Hei\u00b7mat", ".", "Und", "er", "tr\u00e4gt", "uns", "weich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In seinem Mantel in das dunkle Grab,", "tokens": ["In", "sei\u00b7nem", "Man\u00b7tel", "in", "das", "dunk\u00b7le", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo viele schlafen schon im stillen Reich.\u00ab", "tokens": ["Wo", "vie\u00b7le", "schla\u00b7fen", "schon", "im", "stil\u00b7len", "Reich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Des Meeres Seele singt am leeren Kahn.", "tokens": ["Des", "Mee\u00b7res", "See\u00b7le", "singt", "am", "lee\u00b7ren", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er treibt davon, ein Spiel den tauben Winden", "tokens": ["Er", "treibt", "da\u00b7von", ",", "ein", "Spiel", "den", "tau\u00b7ben", "Win\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "$,", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Meeres Einsamkeit. Der Ozean", "tokens": ["In", "Mee\u00b7res", "Ein\u00b7sam\u00b7keit", ".", "Der", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "T\u00fcrmt fern sich auf zu schwarzer Nacht, der Blinden.", "tokens": ["T\u00fcrmt", "fern", "sich", "auf", "zu", "schwar\u00b7zer", "Nacht", ",", "der", "Blin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "APPR", "APPR", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.8": {"line.1": {"text": "In hohen Wogen schweift ein Kormoran", "tokens": ["In", "ho\u00b7hen", "Wo\u00b7gen", "schweift", "ein", "Kor\u00b7mo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit gr\u00fcnen Fittichs dunkler Tr\u00e4umerei.", "tokens": ["Mit", "gr\u00fc\u00b7nen", "Fit\u00b7tichs", "dunk\u00b7ler", "Tr\u00e4u\u00b7me\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Darunter ziehn die Toten ihre Bahn.", "tokens": ["Da\u00b7run\u00b7ter", "ziehn", "die", "To\u00b7ten", "ih\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie blasse Blumen treiben sie vorbei.", "tokens": ["Wie", "blas\u00b7se", "Blu\u00b7men", "trei\u00b7ben", "sie", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sie sinken tief. Das Meer schlie\u00dft seinen Mund", "tokens": ["Sie", "sin\u00b7ken", "tief", ".", "Das", "Meer", "schlie\u00dft", "sei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schillert wei\u00df. Der Horizont nur bebt", "tokens": ["Und", "schil\u00b7lert", "wei\u00df", ".", "Der", "Ho\u00b7ri\u00b7zont", "nur", "bebt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN", "$.", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie eines Adlers Flug, der von dem Sund", "tokens": ["Wie", "ei\u00b7nes", "Ad\u00b7lers", "Flug", ",", "der", "von", "dem", "Sund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ins Abendmeer die blaue Schwinge hebt.", "tokens": ["Ins", "A\u00b7bend\u00b7meer", "die", "blau\u00b7e", "Schwin\u00b7ge", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}