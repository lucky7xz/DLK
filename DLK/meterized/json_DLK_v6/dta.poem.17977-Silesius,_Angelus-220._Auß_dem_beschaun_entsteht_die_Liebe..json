{"dta.poem.17977": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "220.  Au\u00df dem beschaun entsteht die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe folgt aufs schaun. Schau an die ewge dinge/", "tokens": ["Die", "Lie\u00b7be", "folgt", "aufs", "schaun", ".", "Schau", "an", "die", "ew\u00b7ge", "din\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "VVINF", "$.", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So liebstu sie als bald und h\u00e4lst sonst alls geringe.", "tokens": ["So", "liebs\u00b7tu", "sie", "als", "bald", "und", "h\u00e4lst", "sonst", "alls", "ge\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ADV", "KON", "VVFIN", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}