{"textgrid.poem.35963": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich, ein Kind des Maien,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich, ein Kind des Maien,", "tokens": ["Ich", ",", "ein", "Kind", "des", "Mai\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bl\u00fch', umhaucht von Duft,", "tokens": ["Bl\u00fch'", ",", "um\u00b7haucht", "von", "Duft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Um mich still zu freuen", "tokens": ["Um", "mich", "still", "zu", "freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "An der stillen Luft.", "tokens": ["An", "der", "stil\u00b7len", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mich zog s\u00fc\u00dfes Streben", "tokens": ["Mich", "zog", "s\u00fc\u00b7\u00dfes", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Aus dem d\u00fcstern Haus,", "tokens": ["Aus", "dem", "d\u00fcs\u00b7tern", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und das zarte Leben", "tokens": ["Und", "das", "zar\u00b7te", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hauch' im Duft ich aus.", "tokens": ["Hauch'", "im", "Duft", "ich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}