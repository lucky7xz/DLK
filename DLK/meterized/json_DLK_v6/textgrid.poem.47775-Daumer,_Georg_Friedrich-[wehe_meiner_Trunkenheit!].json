{"textgrid.poem.47775": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wehe meiner Trunkenheit!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehe meiner Trunkenheit!", "tokens": ["We\u00b7he", "mei\u00b7ner", "Trun\u00b7ken\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wehe meinem Liebebeben!", "tokens": ["We\u00b7he", "mei\u00b7nem", "Lie\u00b7be\u00b7be\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Biete mir f\u00fcr einen Gran", "tokens": ["Bie\u00b7te", "mir", "f\u00fcr", "ei\u00b7nen", "Gran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heuchelei und Schlechtigkeit", "tokens": ["Heu\u00b7che\u00b7lei", "und", "Schlech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausend Tonnen Goldes an \u2013", "tokens": ["Tau\u00b7send", "Ton\u00b7nen", "Gol\u00b7des", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Keinen hegt der Busen mir,", "tokens": ["Kei\u00b7nen", "hegt", "der", "Bu\u00b7sen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Keinen hab' ich hinzugeben.", "tokens": ["Kei\u00b7nen", "hab'", "ich", "hin\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "VAFIN", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn was auch vielleicht dahier", "tokens": ["Denn", "was", "auch", "viel\u00b7leicht", "da\u00b7hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "ADV", "PAV"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.9": {"text": "In des Herzens dunkler Hut", "tokens": ["In", "des", "Her\u00b7zens", "dunk\u00b7ler", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Rastete von jenem Gut,", "tokens": ["Ras\u00b7te\u00b7te", "von", "je\u00b7nem", "Gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Es zerschmolz in Minnegluth,", "tokens": ["Es", "zer\u00b7schmolz", "in", "Min\u00b7ne\u00b7gluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Es zerflo\u00df im Saft der Reben.", "tokens": ["Es", "zer\u00b7flo\u00df", "im", "Saft", "der", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}