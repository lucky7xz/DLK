{"textgrid.poem.59185": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "404. Selig sind, die reines Herzens sind!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer wahre Keuschheit hat erreichet,", "tokens": ["Wer", "wah\u00b7re", "Keuschheit", "hat", "er\u00b7rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem Heiland und der Jungfrau Mutter gleichet;", "tokens": ["Dem", "Hei\u00b7land", "und", "der", "Jung\u00b7frau", "Mut\u00b7ter", "glei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Besitzt du diese Tugend rein,", "tokens": ["Be\u00b7sitzt", "du", "die\u00b7se", "Tu\u00b7gend", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird das Kreuz dir leicht und lieblich sein.", "tokens": ["So", "wird", "das", "Kreuz", "dir", "leicht", "und", "lieb\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}