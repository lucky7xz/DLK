{"textgrid.poem.43532": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werde heiter, mein Gem\u00fcthe,", "tokens": ["Wer\u00b7de", "hei\u00b7ter", ",", "mein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vergi\u00df der Angst und Pein!", "tokens": ["Und", "ver\u00b7gi\u00df", "der", "Angst", "und", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gro\u00df ist Gottes Gnad' und G\u00fcte,", "tokens": ["Gro\u00df", "ist", "Got\u00b7tes", "Gnad'", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00df mu\u00df auch dein Hoffen sein.", "tokens": ["Gro\u00df", "mu\u00df", "auch", "dein", "Hof\u00b7fen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kommt der helle goldne Morgen", "tokens": ["Kommt", "der", "hel\u00b7le", "gold\u00b7ne", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht hervor aus dunkler Nacht?", "tokens": ["Nicht", "her\u00b7vor", "aus", "dunk\u00b7ler", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lag nicht einst in Schnee verborgen", "tokens": ["Lag", "nicht", "einst", "in", "Schnee", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Fr\u00fchlings Bl\u00fcthenpracht?", "tokens": ["Die\u00b7ses", "Fr\u00fch\u00b7lings", "Bl\u00fc\u00b7then\u00b7pracht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch die Finsterni\u00df der Klagen", "tokens": ["Durch", "die", "Fins\u00b7ter\u00b7ni\u00df", "der", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bricht der Freude Morgenstern;", "tokens": ["Bricht", "der", "Freu\u00b7de", "Mor\u00b7gens\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Bald wird auch dein Morgen tagen:", "tokens": ["Bald", "wird", "auch", "dein", "Mor\u00b7gen", "ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gottes G\u00fct' ist nimmer fern.", "tokens": ["Got\u00b7tes", "G\u00fct'", "ist", "nim\u00b7mer", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}