{"textgrid.poem.67416": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das neue Lied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein neues Lied! ein neues Lied!", "tokens": ["Ein", "neu\u00b7es", "Lied", "!", "ein", "neu\u00b7es", "Lied", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesundheit und ein froh Gem\u00fcth!", "tokens": ["Ge\u00b7sund\u00b7heit", "und", "ein", "froh", "Ge\u00b7m\u00fcth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer unser neues Lied nicht kann,", "tokens": ["Wer", "un\u00b7ser", "neu\u00b7es", "Lied", "nicht", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der fang' es heut zu lernen an", "tokens": ["Der", "fang'", "es", "heut", "zu", "ler\u00b7nen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sei zu \u00fcben es bem\u00fcht:", "tokens": ["Und", "sei", "zu", "\u00fc\u00b7ben", "es", "be\u00b7m\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKZU", "VVINF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gesundheit und ein froh Gem\u00fcth!", "tokens": ["Ge\u00b7sund\u00b7heit", "und", "ein", "froh", "Ge\u00b7m\u00fcth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wem weiht sich unser neues Lied?", "tokens": ["Wem", "weiht", "sich", "un\u00b7ser", "neu\u00b7es", "Lied", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6nheit, die das Herz erzieht.", "tokens": ["Der", "Sch\u00f6n\u00b7heit", ",", "die", "das", "Herz", "er\u00b7zieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer solche Sch\u00f6nheit lieb gewann,", "tokens": ["Wer", "sol\u00b7che", "Sch\u00f6n\u00b7heit", "lieb", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der stimme mit uns jauchzend an:", "tokens": ["Der", "stim\u00b7me", "mit", "uns", "jauch\u00b7zend", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie lebe, die unsterblich bl\u00fcht,", "tokens": ["Sie", "le\u00b7be", ",", "die", "uns\u00b7terb\u00b7lich", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sch\u00f6nheit, die das Herz erzieht!", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "die", "das", "Herz", "er\u00b7zieht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihm, der f\u00fcr Recht und Wahrheit gl\u00fcht,", "tokens": ["Ihm", ",", "der", "f\u00fcr", "Recht", "und", "Wahr\u00b7heit", "gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Freund und Feind sich edel m\u00fcht,", "tokens": ["F\u00fcr", "Freund", "und", "Feind", "sich", "e\u00b7del", "m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nie Schlechtes thun und dulden kann,", "tokens": ["Nie", "Schlech\u00b7tes", "thun", "und", "dul\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fecht' ihn auch Ha\u00df und Mi\u00dfgunst an,", "tokens": ["Fecht'", "ihn", "auch", "Ha\u00df", "und", "Mi\u00df\u00b7gunst", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm, Freunde, singen wir dies Lied,", "tokens": ["Ihm", ",", "Freun\u00b7de", ",", "sin\u00b7gen", "wir", "dies", "Lied", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Edelsten, der vor uns bl\u00fcht.", "tokens": ["Dem", "E\u00b7dels\u00b7ten", ",", "der", "vor", "uns", "bl\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Der neuen Zeit, die vor uns bl\u00fcht,", "tokens": ["Der", "neu\u00b7en", "Zeit", ",", "die", "vor", "uns", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Blick, der in die Zukunft sieht.", "tokens": ["Dem", "Blick", ",", "der", "in", "die", "Zu\u00b7kunft", "sieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer f\u00fcr die Nachwelt leben kann,", "tokens": ["Wer", "f\u00fcr", "die", "Nach\u00b7welt", "le\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist, auch verkannt, ein sel'ger Mann;", "tokens": ["Ist", ",", "auch", "ver\u00b7kannt", ",", "ein", "sel'\u00b7ger", "Mann", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADV", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn ehret froh der Zeiten Lied;", "tokens": ["Ihn", "eh\u00b7ret", "froh", "der", "Zei\u00b7ten", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gl\u00fcckauf der Zeit, die vor uns bl\u00fcht!", "tokens": ["Gl\u00fcc\u00b7kauf", "der", "Zeit", ",", "die", "vor", "uns", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Noch einmal stimmet an das Lied", "tokens": ["Noch", "ein\u00b7mal", "stim\u00b7met", "an", "das", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kraft, die Herz an Herzen zieht!", "tokens": ["Der", "Kraft", ",", "die", "Herz", "an", "Her\u00b7zen", "zieht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr weihen wir uns Hand in Hand", "tokens": ["Ihr", "wei\u00b7hen", "wir", "uns", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kn\u00fcpfen ein unl\u00f6sbar Band:", "tokens": ["Und", "kn\u00fcp\u00b7fen", "ein", "un\u00b7l\u00f6s\u00b7bar", "Band", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Der sch\u00f6nsten Kraft, die in uns gl\u00fcht,", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Kraft", ",", "die", "in", "uns", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}