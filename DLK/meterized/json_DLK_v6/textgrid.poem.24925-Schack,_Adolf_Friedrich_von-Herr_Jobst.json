{"textgrid.poem.24925": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Herr Jobst", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaff h\u00e4ngen die Segel, die Wellen ruhn,", "tokens": ["Schlaff", "h\u00e4n\u00b7gen", "die", "Se\u00b7gel", ",", "die", "Wel\u00b7len", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Kein Lufthauch regt die Gew\u00e4sser;", "tokens": ["Kein", "Luft\u00b7hauch", "regt", "die", "Ge\u00b7w\u00e4s\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kein Bissen mehr in den Kasten und Truhn,", "tokens": ["Kein", "Bis\u00b7sen", "mehr", "in", "den", "Kas\u00b7ten", "und", "Truhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Geleert sind Tonnen und F\u00e4sser;", "tokens": ["Ge\u00b7leert", "sind", "Ton\u00b7nen", "und", "F\u00e4s\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nur zehn Matrosen noch leben, nur zehn,", "tokens": ["Nur", "zehn", "Mat\u00b7ro\u00b7sen", "noch", "le\u00b7ben", ",", "nur", "zehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADV", "VVINF", "$,", "ADV", "CARD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und wie sie ringsum die Gestorbenen sehn,", "tokens": ["Und", "wie", "sie", "ring\u00b7sum", "die", "Ge\u00b7stor\u00b7be\u00b7nen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Da denken sie: Jene beneiden wir nun;", "tokens": ["Da", "den\u00b7ken", "sie", ":", "Je\u00b7ne", "be\u00b7nei\u00b7den", "wir", "nun", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "F\u00fcrwahr, sie haben es besser!", "tokens": ["F\u00fcr\u00b7wahr", ",", "sie", "ha\u00b7ben", "es", "bes\u00b7ser", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und der Schiffsherr spricht: \u00bbNicht Trank, noch Brot!", "tokens": ["Und", "der", "Schiffs\u00b7herr", "spricht", ":", "\u00bb", "Nicht", "Trank", ",", "noch", "Brot", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "$(", "PTKNEG", "NN", "$,", "ADV", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Bald ist auch der letzte verschmachtet;", "tokens": ["Bald", "ist", "auch", "der", "letz\u00b7te", "ver\u00b7schmach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Drum werde das Mittel in \u00e4u\u00dferster Not,", "tokens": ["Drum", "wer\u00b7de", "das", "Mit\u00b7tel", "in", "\u00e4u\u00b7\u00dfers\u00b7ter", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das einzige, nicht verachtet!", "tokens": ["Das", "ein\u00b7zi\u00b7ge", ",", "nicht", "ver\u00b7ach\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PTKNEG", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Ein Opfer mu\u00df fallen, \u2013 wohlan, es sei!", "tokens": ["Ein", "Op\u00b7fer", "mu\u00df", "fal\u00b7len", ",", "\u2013", "wo\u00b7hlan", ",", "es", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,", "$(", "ADV", "$,", "PPER", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das Los mag entscheiden \u2013 bringt Zettel herbei!", "tokens": ["Das", "Los", "mag", "ent\u00b7schei\u00b7den", "\u2013", "bringt", "Zet\u00b7tel", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$(", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und wen es trifft, der verfalle dem Tod!", "tokens": ["Und", "wen", "es", "trifft", ",", "der", "ver\u00b7fal\u00b7le", "dem", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Er werde morgen geschlachtet!\u00ab", "tokens": ["Er", "wer\u00b7de", "mor\u00b7gen", "ge\u00b7schlach\u00b7tet", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "O Guter! eh du den Einfall lobst,", "tokens": ["O", "Gu\u00b7ter", "!", "eh", "du", "den", "Ein\u00b7fall", "lobst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was hast du ihn mehr nicht erwogen?", "tokens": ["Was", "hast", "du", "ihn", "mehr", "nicht", "er\u00b7wo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Matrosen rufen: \u00bbHabt recht, Herr Jobst!\u00ab", "tokens": ["Die", "Mat\u00b7ro\u00b7sen", "ru\u00b7fen", ":", "\u00bb", "Habt", "recht", ",", "Herr", "Jobst", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$(", "VAFIN", "ADJD", "$,", "NN", "NE", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kaum wird noch Beratung gepflogen.", "tokens": ["Kaum", "wird", "noch", "Be\u00b7ra\u00b7tung", "ge\u00b7pflo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie mischen die Todeslose, sie ziehn,", "tokens": ["Sie", "mi\u00b7schen", "die", "To\u00b7des\u00b7lo\u00b7se", ",", "sie", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und alle blicken verbl\u00fcfft auf ihn:", "tokens": ["Und", "al\u00b7le", "bli\u00b7cken", "ver\u00b7bl\u00fcfft", "auf", "ihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er selber, der Schiffsherr, fett wie ein Probst,", "tokens": ["Er", "sel\u00b7ber", ",", "der", "Schiffs\u00b7herr", ",", "fett", "wie", "ein", "Probst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Das Los hat er selber gezogen.", "tokens": ["Das", "Los", "hat", "er", "sel\u00b7ber", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Schon ist es die Zeit, wo die Sonne sich neigt", "tokens": ["Schon", "ist", "es", "die", "Zeit", ",", "wo", "die", "Son\u00b7ne", "sich", "neigt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zum flammenden Meeresbecken,", "tokens": ["Zum", "flam\u00b7men\u00b7den", "Mee\u00b7res\u00b7be\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sie raunen: \u00bbWenn sie im Osten steigt,", "tokens": ["Und", "sie", "rau\u00b7nen", ":", "\u00bb", "Wenn", "sie", "im", "Os\u00b7ten", "steigt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dann soll uns der Dicke schmecken!\u00ab", "tokens": ["Dann", "soll", "uns", "der", "Di\u00b7cke", "schme\u00b7cken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Herr Jobst vernimmt das bedrohliche Wort;", "tokens": ["Herr", "Jobst", "ver\u00b7nimmt", "das", "be\u00b7droh\u00b7li\u00b7che", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Er sieht in den Blicken den lauernden Mord;", "tokens": ["Er", "sieht", "in", "den", "Bli\u00b7cken", "den", "lau\u00b7ern\u00b7den", "Mord", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Er lehnt an dem Schiffsrand, starrt und schweigt", "tokens": ["Er", "lehnt", "an", "dem", "Schiffs\u00b7rand", ",", "starrt", "und", "schweigt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und denkt an den Morgen mit Schrecken.", "tokens": ["Und", "denkt", "an", "den", "Mor\u00b7gen", "mit", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Da tritt, in der Rechten das Seherohr,", "tokens": ["Da", "tritt", ",", "in", "der", "Rech\u00b7ten", "das", "Se\u00b7he\u00b7rohr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zu ihm heran ein Matrose", "tokens": ["Zu", "ihm", "he\u00b7ran", "ein", "Mat\u00b7ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und fl\u00fcstert ihm heimlich, heimlich ins Ohr:", "tokens": ["Und", "fl\u00fcs\u00b7tert", "ihm", "heim\u00b7lich", ",", "heim\u00b7lich", "ins", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbihr seid getroffen vom Lose;", "tokens": ["\u00bb", "ihr", "seid", "ge\u00b7trof\u00b7fen", "vom", "Lo\u00b7se", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch kann ich Euch retten, wenn Ihr mir vertraut!", "tokens": ["Doch", "kann", "ich", "Euch", "ret\u00b7ten", ",", "wenn", "Ihr", "mir", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF", "$,", "KOUS", "PPER", "PPER", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das Fernrohr nehmt in die Hand und schaut!\u00ab", "tokens": ["Das", "Fern\u00b7rohr", "nehmt", "in", "die", "Hand", "und", "schaut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zugleich zieht er Feder und Rolle hervor", "tokens": ["Zu\u00b7gleich", "zieht", "er", "Fe\u00b7der", "und", "Rol\u00b7le", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Aus der Tasche der ledernen Hose.", "tokens": ["Aus", "der", "Ta\u00b7sche", "der", "le\u00b7der\u00b7nen", "Ho\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Herr Jobst ist nicht l\u00e4ssig, das Rohr zur Hand", "tokens": ["Herr", "Jobst", "ist", "nicht", "l\u00e4s\u00b7sig", ",", "das", "Rohr", "zur", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "VAFIN", "PTKNEG", "ADJD", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und sp\u00e4hend vors Auge zu nehmen;", "tokens": ["Und", "sp\u00e4\u00b7hend", "vors", "Au\u00b7ge", "zu", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Bald ruft er: \u00bbBei Gott, ich erblicke Land!", "tokens": ["Bald", "ruft", "er", ":", "\u00bb", "Bei", "Gott", ",", "ich", "er\u00b7bli\u00b7cke", "Land", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "APPR", "NN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist's Wahrheit oder ein Schemen?", "tokens": ["Ist's", "Wahr\u00b7heit", "o\u00b7der", "ein", "Sche\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das H\u00e4uslein dort mit dem Gartenzaun,", "tokens": ["Das", "H\u00e4us\u00b7lein", "dort", "mit", "dem", "Gar\u00b7ten\u00b7zaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wei\u00df anget\u00fcncht, doch die Balken braun \u2013", "tokens": ["Wei\u00df", "an\u00b7ge\u00b7t\u00fcncht", ",", "doch", "die", "Bal\u00b7ken", "braun", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADV", "ART", "NN", "ADJD", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "O Himmel, wie ist mir das alles bekannt.", "tokens": ["O", "Him\u00b7mel", ",", "wie", "ist", "mir", "das", "al\u00b7les", "be\u00b7kannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ART", "PIS", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Das ist ja der Hafen von Bremen!", "tokens": ["Das", "ist", "ja", "der", "Ha\u00b7fen", "von", "Bre\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "