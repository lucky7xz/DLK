{"textgrid.poem.25728": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch ich, ich lasse mir zuweilen tr\u00e4umen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch ich, ich lasse mir zuweilen tr\u00e4umen,", "tokens": ["Auch", "ich", ",", "ich", "las\u00b7se", "mir", "zu\u00b7wei\u00b7len", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie jeder, der einmal gereimt,", "tokens": ["Wie", "je\u00b7der", ",", "der", "ein\u00b7mal", "ge\u00b7reimt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mit des Harzes j\u00fcngsten Eichenb\u00e4umen", "tokens": ["Da\u00df", "mit", "des", "Har\u00b7zes", "j\u00fcng\u00b7sten", "Ei\u00b7chen\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Lorbeer einst noch in die Wette keimt.", "tokens": ["Mein", "Lor\u00b7beer", "einst", "noch", "in", "die", "Wet\u00b7te", "keimt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wahrlich nicht, weil ihr, oft herbe Lieder,", "tokens": ["Doch", "wahr\u00b7lich", "nicht", ",", "weil", "ihr", ",", "oft", "her\u00b7be", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Wahn' des S\u00e4ngers s\u00fc\u00dfer w\u00e4rt;", "tokens": ["Im", "Wahn'", "des", "S\u00e4n\u00b7gers", "s\u00fc\u00b7\u00dfer", "w\u00e4rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich schlage gern' besch\u00e4mt die Augen nieder,", "tokens": ["Ich", "schla\u00b7ge", "gern'", "be\u00b7sch\u00e4mt", "die", "Au\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn euch das Ohr des gr\u00f6\u00dfern Meisters h\u00f6rt.", "tokens": ["Wenn", "euch", "das", "Ohr", "des", "gr\u00f6\u00b7\u00dfern", "Meis\u00b7ters", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "S\u00fc\u00df aber ist's, zu tr\u00e4umen, da\u00df von Wesen,", "tokens": ["S\u00fc\u00df", "a\u00b7ber", "ist's", ",", "zu", "tr\u00e4u\u00b7men", ",", "da\u00df", "von", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "$,", "PTKZU", "VVINF", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die ich geliebt, und selbst, Gesang", "tokens": ["Die", "ich", "ge\u00b7liebt", ",", "und", "selbst", ",", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "VVPP", "$,", "KON", "ADV", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nicht dichten konnten, man noch dann wird lesen,", "tokens": ["Nicht", "dich\u00b7ten", "konn\u00b7ten", ",", "man", "noch", "dann", "wird", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "VMFIN", "$,", "PIS", "ADV", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wenn sie und mich vorl\u00e4ngst das Grab verschlang.", "tokens": ["Wenn", "sie", "und", "mich", "vor\u00b7l\u00e4ngst", "das", "Grab", "ver\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So m\u00f6cht' ich auch, du K\u00f6nig aller Hunde!", "tokens": ["So", "m\u00f6cht'", "ich", "auch", ",", "du", "K\u00f6\u00b7nig", "al\u00b7ler", "Hun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "PPER", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "So gerne die Unsterblichkeit", "tokens": ["So", "ger\u00b7ne", "die", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Dir geben! denn in mancher Lebensstunde", "tokens": ["Dir", "ge\u00b7ben", "!", "denn", "in", "man\u00b7cher", "Le\u00b7bens\u00b7stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$.", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Hast schmeichelnd du, mich Traurigen, zerstreut.", "tokens": ["Hast", "schmei\u00b7chelnd", "du", ",", "mich", "Trau\u00b7ri\u00b7gen", ",", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "$,", "PPER", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Du trugst, wenn ich zu lang in tr\u00e4ger Ruhe", "tokens": ["Du", "trugst", ",", "wenn", "ich", "zu", "lang", "in", "tr\u00e4\u00b7ger", "Ru\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKA", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Am Pulte sa\u00df, \u2013 gerad als w\u00e4rest du", "tokens": ["Am", "Pul\u00b7te", "sa\u00df", ",", "\u2013", "ge\u00b7rad", "als", "w\u00e4\u00b7rest", "du"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "$(", "ADV", "KOKOM", "VAFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Mit meinem Arzt' verstanden \u2013 mir die Schuhe", "tokens": ["Mit", "mei\u00b7nem", "Arzt'", "ver\u00b7stan\u00b7den", "\u2013", "mir", "die", "Schu\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$(", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Schon oft von selbst, mich anzukleiden, zu.", "tokens": ["Schon", "oft", "von", "selbst", ",", "mich", "an\u00b7zu\u00b7klei\u00b7den", ",", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADV", "$,", "PPER", "VVIZU", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Als du, wenn ich den Hut ergriff!", "tokens": ["Als", "du", ",", "wenn", "ich", "den", "Hut", "er\u00b7griff", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Vor gro\u00dfer Freude ranntest du die H\u00fchner", "tokens": ["Vor", "gro\u00b7\u00dfer", "Freu\u00b7de", "rann\u00b7test", "du", "die", "H\u00fch\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und Enten um \u2013 und standest, wenn ich pfiff!", "tokens": ["Und", "En\u00b7ten", "um", "\u2013", "und", "stan\u00b7dest", ",", "wenn", "ich", "pfiff", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "$(", "KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und wenn ein Maulwurf unser Beet voll Bohnen", "tokens": ["Und", "wenn", "ein", "Maul\u00b7wurf", "un\u00b7ser", "Beet", "voll", "Boh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Zerw\u00fchlt, so lauerst du ihm auf,", "tokens": ["Zer\u00b7w\u00fchlt", ",", "so", "lau\u00b7erst", "du", "ihm", "auf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Um seine Frevelthat zu lohnen,", "tokens": ["Um", "sei\u00b7ne", "Fre\u00b7vel\u00b7that", "zu", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Geht auch der Abendstern dar\u00fcber auf.", "tokens": ["Geht", "auch", "der", "A\u00b7bends\u00b7tern", "da\u00b7r\u00fc\u00b7ber", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PAV", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Doch meine Wachtel, gutes Thier! spatzieret", "tokens": ["Doch", "mei\u00b7ne", "Wach\u00b7tel", ",", "gu\u00b7tes", "Thier", "!", "spat\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Nach Herzenslust auf dir herum,", "tokens": ["Nach", "Her\u00b7zens\u00b7lust", "auf", "dir", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und picket dich am Ohr', und n\u00e4rrt und schieret", "tokens": ["Und", "pi\u00b7cket", "dich", "am", "Ohr'", ",", "und", "n\u00e4rrt", "und", "schie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Dich ba\u00df; und du? siehst kaum einmal dich um.", "tokens": ["Dich", "ba\u00df", ";", "und", "du", "?", "siehst", "kaum", "ein\u00b7mal", "dich", "um", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$.", "KON", "PPER", "$.", "VVFIN", "ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Dann aber spr\u00fchten deine Augen Feuer,", "tokens": ["Dann", "a\u00b7ber", "spr\u00fch\u00b7ten", "dei\u00b7ne", "Au\u00b7gen", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Wenn Nachts ein Dieb am Laden war;", "tokens": ["Wenn", "Nachts", "ein", "Dieb", "am", "La\u00b7den", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Dein eignes Leben ist dir niemals theuer,", "tokens": ["Dein", "eig\u00b7nes", "Le\u00b7ben", "ist", "dir", "nie\u00b7mals", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Entziehst du mich nur immer der Gefahr.", "tokens": ["Ent\u00b7ziehst", "du", "mich", "nur", "im\u00b7mer", "der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "Wie lohn' ich dir so viele sch\u00f6ne Thaten?", "tokens": ["Wie", "lohn'", "ich", "dir", "so", "vie\u00b7le", "sch\u00f6\u00b7ne", "Tha\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Ein Denkmal will ich einst dir weihn!", "tokens": ["Ein", "Denk\u00b7mal", "will", "ich", "einst", "dir", "weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Doch, besser ist vorher im Leben Braten,", "tokens": ["Doch", ",", "bes\u00b7ser", "ist", "vor\u00b7her", "im", "Le\u00b7ben", "Bra\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "VAFIN", "ADV", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Als nach dem Tode blo\u00df ein Marmorstein.", "tokens": ["Als", "nach", "dem", "To\u00b7de", "blo\u00df", "ein", "Mar\u00b7mor\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}