{"textgrid.poem.66269": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Bei einem Wetter", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolkensammler, der im Himmel thronet,", "tokens": ["Wol\u00b7ken\u00b7samm\u00b7ler", ",", "der", "im", "Him\u00b7mel", "thro\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der so gerne seine Menschen schonet,", "tokens": ["Der", "so", "ger\u00b7ne", "sei\u00b7ne", "Men\u00b7schen", "scho\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sprich zum Wetter, das uns droht:", "tokens": ["Sprich", "zum", "Wet\u00b7ter", ",", "das", "uns", "droht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Werde Segen, und nicht Tod!", "tokens": ["Wer\u00b7de", "Se\u00b7gen", ",", "und", "nicht", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KON", "PTKNEG", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter mancher strohbedeckten H\u00fctte", "tokens": ["Un\u00b7ter", "man\u00b7cher", "stroh\u00b7be\u00b7deck\u00b7ten", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Winselt ja des armen Waisen Bitte:", "tokens": ["Win\u00b7selt", "ja", "des", "ar\u00b7men", "Wai\u00b7sen", "Bit\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Straf uns nicht in deinem Zorn,", "tokens": ["Straf", "uns", "nicht", "in", "dei\u00b7nem", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater, la\u00df uns unser Korn!", "tokens": ["Va\u00b7ter", ",", "la\u00df", "uns", "un\u00b7ser", "Korn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die schwarze Wolke donnertr\u00e4chtig,", "tokens": ["Wenn", "die", "schwar\u00b7ze", "Wol\u00b7ke", "don\u00b7ner\u00b7tr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unsre Saaten zu verw\u00fcsten, m\u00e4chtig", "tokens": ["Uns\u00b7re", "Saa\u00b7ten", "zu", "ver\u00b7w\u00fcs\u00b7ten", ",", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ueber unsre Gauen zeucht;", "tokens": ["Ue\u00b7ber", "uns\u00b7re", "Gau\u00b7en", "zeucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, so werde sie verscheucht.", "tokens": ["Ach", ",", "so", "wer\u00b7de", "sie", "ver\u00b7scheucht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Winke, da\u00df ein Schlauch die Wolke werde,", "tokens": ["Win\u00b7ke", ",", "da\u00df", "ein", "Schlauch", "die", "Wol\u00b7ke", "wer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Segen zu verspritzen auf die Erde;", "tokens": ["Se\u00b7gen", "zu", "ver\u00b7sprit\u00b7zen", "auf", "die", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Leuchte mit des Blitzes Licht", "tokens": ["Leuch\u00b7te", "mit", "des", "Blit\u00b7zes", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Irrenden, nur t\u00f6dte nicht.", "tokens": ["Ir\u00b7ren\u00b7den", ",", "nur", "t\u00f6d\u00b7te", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Standest du nicht auf dem Regenbogen", "tokens": ["Stan\u00b7dest", "du", "nicht", "auf", "dem", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einst vor Noah? sprachest: Euch gewogen", "tokens": ["Einst", "vor", "Noah", "?", "spra\u00b7chest", ":", "Euch", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "NE", "$.", "VVFIN", "$.", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bleib' ich, Wasserfluthgericht", "tokens": ["Bleib'", "ich", ",", "Was\u00b7ser\u00b7fluth\u00b7ge\u00b7richt"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Straf' euch Menschen f\u00fcrder nicht!", "tokens": ["Straf'", "euch", "Men\u00b7schen", "f\u00fcr\u00b7der", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df dies auch von deinem Donner gelten;", "tokens": ["La\u00df", "dies", "auch", "von", "dei\u00b7nem", "Don\u00b7ner", "gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Spar aufs Weltgericht sein grimmes Schelten;", "tokens": ["Spar", "aufs", "Welt\u00b7ge\u00b7richt", "sein", "grim\u00b7mes", "Schel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df des Hagels schwere Wuth", "tokens": ["La\u00df", "des", "Ha\u00b7gels", "schwe\u00b7re", "Wuth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmelzen in der Liebe Gluth.", "tokens": ["Schmel\u00b7zen", "in", "der", "Lie\u00b7be", "Gluth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}