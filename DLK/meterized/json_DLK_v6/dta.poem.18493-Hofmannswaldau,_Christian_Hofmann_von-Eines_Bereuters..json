{"dta.poem.18493": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines Bereuters.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts hat mir auff der welt/ mehr lust als reiten bracht.", "tokens": ["Nichts", "hat", "mir", "auff", "der", "welt", "/", "mehr", "lust", "als", "rei\u00b7ten", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPR", "ART", "NN", "$(", "ADV", "VVFIN", "KOKOM", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich habe manches pferd und reuter gut gemacht/", "tokens": ["Ich", "ha\u00b7be", "man\u00b7ches", "pferd", "und", "reu\u00b7ter", "gut", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "ADJD", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jetzt ist mein fauler leib der maden renne-bahn/", "tokens": ["Jetzt", "ist", "mein", "fau\u00b7ler", "leib", "der", "ma\u00b7den", "ren\u00b7ne\u00b7bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die reiten jetzt herum/ dieweil ich nicht mehr kan.", "tokens": ["Die", "rei\u00b7ten", "jetzt", "he\u00b7rum", "/", "die\u00b7weil", "ich", "nicht", "mehr", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}