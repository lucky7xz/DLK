{"textgrid.poem.66360": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Theon an Wilhelminen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilhelmine, o du Holde!", "tokens": ["Wil\u00b7hel\u00b7mi\u00b7ne", ",", "o", "du", "Hol\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "PPER", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die beim ersten Blick entz\u00fcckt,", "tokens": ["Die", "beim", "ers\u00b7ten", "Blick", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mit seinem reinsten Golde", "tokens": ["Denn", "mit", "sei\u00b7nem", "reins\u00b7ten", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat der Himmel dich geschm\u00fcckt.", "tokens": ["Hat", "der", "Him\u00b7mel", "dich", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6re \u2013 nicht mit Liebesklagen", "tokens": ["H\u00f6\u00b7re", "\u2013", "nicht", "mit", "Lie\u00b7bes\u00b7kla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcll' ich dieses Lied dir an;", "tokens": ["F\u00fcll'", "ich", "die\u00b7ses", "Lied", "dir", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Augen m\u00f6gen sagen,", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "m\u00f6\u00b7gen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was der Mund nicht stammeln kann.", "tokens": ["Was", "der", "Mund", "nicht", "stam\u00b7meln", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deiner Sch\u00f6nheit Pfirsichbl\u00fcthe,", "tokens": ["Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Pfir\u00b7sich\u00b7bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Laune Heiterkeit;", "tokens": ["Dei\u00b7ner", "Lau\u00b7ne", "Hei\u00b7ter\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, dein Herz voll Himmelsg\u00fcte,", "tokens": ["Ach", ",", "dein", "Herz", "voll", "Him\u00b7mels\u00b7g\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das die Unschuld nie entweiht;", "tokens": ["Das", "die", "Un\u00b7schuld", "nie", "ent\u00b7weiht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Augen stilles Feuer,", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "stil\u00b7les", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drin der Liebe Schmachten flimmt,", "tokens": ["Drin", "der", "Lie\u00b7be", "Schmach\u00b7ten", "flimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat, o Traute! meine Leier", "tokens": ["Hat", ",", "o", "Trau\u00b7te", "!", "mei\u00b7ne", "Lei\u00b7er"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "FM", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heut zu deinem Lob gestimmt.", "tokens": ["Heut", "zu", "dei\u00b7nem", "Lob", "ge\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alles t\u00f6nt mir: Wilhelmine!", "tokens": ["Al\u00b7les", "t\u00f6nt", "mir", ":", "Wil\u00b7hel\u00b7mi\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$.", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueberall seh' ich dein Bild,", "tokens": ["Ue\u00b7be\u00b7rall", "seh'", "ich", "dein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie dir aus verkl\u00e4rter Miene", "tokens": ["Wie", "dir", "aus", "ver\u00b7kl\u00e4r\u00b7ter", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiblichzarte Anmuth quillt.", "tokens": ["Weib\u00b7lich\u00b7zar\u00b7te", "An\u00b7muth", "quillt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wilhelmine! hallt's in L\u00fcften,", "tokens": ["Wil\u00b7hel\u00b7mi\u00b7ne", "!", "hallt's", "in", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wilhelmine! seufzt der Bach,", "tokens": ["Wil\u00b7hel\u00b7mi\u00b7ne", "!", "seufzt", "der", "Bach", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "In des Mondes Silberd\u00fcften", "tokens": ["In", "des", "Mon\u00b7des", "Sil\u00b7ber\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seufzt es meine Seele nach.", "tokens": ["Seufzt", "es", "mei\u00b7ne", "See\u00b7le", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wilhelmine, kann dies Wallen,", "tokens": ["Wil\u00b7hel\u00b7mi\u00b7ne", ",", "kann", "dies", "Wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PDS", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Kann dies Klopfen meiner Brust,", "tokens": ["Kann", "dies", "Klop\u00b7fen", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann ein J\u00fcngling dir gefallen,", "tokens": ["Kann", "ein", "J\u00fcng\u00b7ling", "dir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der der Treue sich bewu\u00dft?", "tokens": ["Der", "der", "Treu\u00b7e", "sich", "be\u00b7wu\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, so schaue mit den Blicken", "tokens": ["Ach", ",", "so", "schau\u00b7e", "mit", "den", "Bli\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deiner Engelhuld auf ihn,", "tokens": ["Dei\u00b7ner", "En\u00b7gel\u00b7huld", "auf", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wonnestrahlendes Entz\u00fccken", "tokens": ["Won\u00b7nes\u00b7trah\u00b7len\u00b7des", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rei\u00dft dann seine Seele hin.", "tokens": ["Rei\u00dft", "dann", "sei\u00b7ne", "See\u00b7le", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O in deinen Armen leben,", "tokens": ["O", "in", "dei\u00b7nen", "Ar\u00b7men", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wilhelmine, welch ein Gl\u00fcck!", "tokens": ["Wil\u00b7hel\u00b7mi\u00b7ne", ",", "welch", "ein", "Gl\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Unsichtbare Geister schweben", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7re", "Geis\u00b7ter", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Goldgeschwingt um meinen Blick!", "tokens": ["Gold\u00b7ge\u00b7schwingt", "um", "mei\u00b7nen", "Blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O mit dir! mit dir! \u2013 wie helle", "tokens": ["O", "mit", "dir", "!", "mit", "dir", "!", "\u2013", "wie", "hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "APPR", "PPER", "$.", "APPR", "PPER", "$.", "$(", "KOKOM", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4r's in meinem tr\u00fcben Sinn:", "tokens": ["W\u00e4r's", "in", "mei\u00b7nem", "tr\u00fc\u00b7ben", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00fcpfend, wie die Silberwelle,", "tokens": ["H\u00fcp\u00b7fend", ",", "wie", "die", "Sil\u00b7ber\u00b7wel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tanzte mir dies Leben hin.", "tokens": ["Tanz\u00b7te", "mir", "dies", "Le\u00b7ben", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}