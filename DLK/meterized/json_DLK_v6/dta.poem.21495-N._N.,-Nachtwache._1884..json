{"dta.poem.21495": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Nachtwache.  \n 1884.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Um Haupt und Leib mir wallen", "tokens": ["Um", "Haupt", "und", "Leib", "mir", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dunkle Nebel der Nacht,", "tokens": ["Dunk\u00b7le", "Ne\u00b7bel", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auf Herz und Sinne fallen", "tokens": ["Auf", "Herz", "und", "Sin\u00b7ne", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Finsternisse mit Macht.", "tokens": ["Fins\u00b7ter\u00b7nis\u00b7se", "mit", "Macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die d\u00fcst\u2019ren Wolken schreiten", "tokens": ["Die", "d\u00fcst'\u00b7ren", "Wol\u00b7ken", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drohend \u00fcber das Land,", "tokens": ["Dro\u00b7hend", "\u00fc\u00b7ber", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Schatten vor\u00fcbergleiten", "tokens": ["Schat\u00b7ten", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und fassen mein Gewand.", "tokens": ["Und", "fas\u00b7sen", "mein", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie fassen an meine Seele", "tokens": ["Sie", "fas\u00b7sen", "an", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und greifen in mein Hirn,", "tokens": ["Und", "grei\u00b7fen", "in", "mein", "Hirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O l\u00f6sche in Nacht und Schwele \u2014", "tokens": ["O", "l\u00f6\u00b7sche", "in", "Nacht", "und", "Schwe\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verl\u00f6sche nicht mein Gestirn.", "tokens": ["Ver\u00b7l\u00f6\u00b7sche", "nicht", "mein", "Ge\u00b7stirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "O wasche mit Feuerwellen", "tokens": ["O", "wa\u00b7sche", "mit", "Feu\u00b7er\u00b7wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von meinem Busen die Schuld,", "tokens": ["Von", "mei\u00b7nem", "Bu\u00b7sen", "die", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Str\u00f6m\u2019 \u00fcber mich den hellen", "tokens": ["Str\u00f6m'", "\u00fc\u00b7ber", "mich", "den", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Glanz deiner Gnade und Huld.", "tokens": ["Glanz", "dei\u00b7ner", "Gna\u00b7de", "und", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Ich bin eine zitternde Leuchte,", "tokens": ["Ich", "bin", "ei\u00b7ne", "zit\u00b7tern\u00b7de", "Leuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich bin ein schwaches Rohr \u2014", "tokens": ["Ich", "bin", "ein", "schwa\u00b7ches", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du, schau meiner Augen Feuchte,", "tokens": ["Du", ",", "schau", "mei\u00b7ner", "Au\u00b7gen", "Feuch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gnade f&#252;hr' mich empor!             ", "tokens": ["Gna\u00b7de", "f", "&#252;", "hr'", "mich", "em\u00b7por", "!"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}