{"dta.poem.18962": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "40.  \n  Nichts Jrrdisches  \n Wehrhafft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Geburt ist schlechter Ruhm/", "tokens": ["Ge\u00b7burt", "ist", "schlech\u00b7ter", "Ruhm", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ehr wie ein plitz vergehet/", "tokens": ["Ehr", "wie", "ein", "plitz", "ver\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sch\u00f6nheit ist eine Bluhm/", "tokens": ["Sch\u00f6n\u00b7heit", "ist", "ei\u00b7ne", "Bluhm", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$("], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Reichthumb nicht lang bestehet/", "tokens": ["Reicht\u00b7humb", "nicht", "lang", "be\u00b7ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herrlichkeit ist ein glantz/", "tokens": ["Herr\u00b7lich\u00b7keit", "ist", "ein", "glantz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ruhm wie ein rauch verflieget/", "tokens": ["Ruhm", "wie", "ein", "rauch", "ver\u00b7flie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJD", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wollust ist nur ein Dantz/", "tokens": ["Wol\u00b7lust", "ist", "nur", "ein", "Dantz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Die welt allzeit betrieget.", "tokens": ["Die", "welt", "all\u00b7zeit", "be\u00b7trie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Auff erden/ Herr/ bleib du bey Mir/", "tokens": ["Auff", "er\u00b7den", "/", "Herr", "/", "bleib", "du", "bey", "Mir", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "$(", "VVFIN", "PPER", "APPR", "NE", "$("], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jm Himmel la\u00df mich sein mit Dir!", "tokens": ["Jm", "Him\u00b7mel", "la\u00df", "mich", "sein", "mit", "Dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIMP", "PPER", "PPOSAT", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}