{"textgrid.poem.37070": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Grille", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum zirptest du mich, o b\u00f6se Grille,", "tokens": ["Wa\u00b7rum", "zirp\u00b7test", "du", "mich", ",", "o", "b\u00f6\u00b7se", "Gril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "FM", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Aus dem s\u00fc\u00dfesten Traume? \u2013 Laura sa\u00df mir,", "tokens": ["Aus", "dem", "s\u00fc\u00b7\u00dfes\u00b7ten", "Trau\u00b7me", "?", "\u2013", "Lau\u00b7ra", "sa\u00df", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$(", "NE", "VVFIN", "PPER", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "\u00dcberschattet von Rosen, gegen\u00fcber,", "tokens": ["\u00dc\u00b7bersc\u00b7hat\u00b7tet", "von", "Ro\u00b7sen", ",", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "PTKVZ", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wand aus wei\u00dfen und rothen Blumen Kr\u00e4nze,", "tokens": ["Wand", "aus", "wei\u00b7\u00dfen", "und", "ro\u00b7then", "Blu\u00b7men", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "KON", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sang, wie Engel im Paradiese singen,", "tokens": ["Sang", ",", "wie", "En\u00b7gel", "im", "Pa\u00b7ra\u00b7die\u00b7se", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ach, und l\u00e4chelte, da\u00df mein trunknes Herz mir", "tokens": ["Ach", ",", "und", "l\u00e4\u00b7chel\u00b7te", ",", "da\u00df", "mein", "trunk\u00b7nes", "Herz", "mir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "PPER"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Vor Entz\u00fcckung beinah zerflo\u00dfen w\u00e4re. \u2013", "tokens": ["Vor", "Ent\u00b7z\u00fc\u00b7ckung", "bei\u00b7nah", "zer\u00b7flo\u00b7\u00dfen", "w\u00e4\u00b7re", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "VAFIN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Warum zirptest du mich, o b\u00f6se Grille,", "tokens": ["Wa\u00b7rum", "zirp\u00b7test", "du", "mich", ",", "o", "b\u00f6\u00b7se", "Gril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "FM", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Aus dem s\u00fc\u00dfesten Traume? Flieh mein Lager,", "tokens": ["Aus", "dem", "s\u00fc\u00b7\u00dfes\u00b7ten", "Trau\u00b7me", "?", "Flieh", "mein", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Kleine Zirperin, wecke, wecke Lauren", "tokens": ["Klei\u00b7ne", "Zir\u00b7pe\u00b7rin", ",", "we\u00b7cke", ",", "we\u00b7cke", "Lau\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Aus dem Schlummer. Vielleicht gedenkt sie meiner,", "tokens": ["Aus", "dem", "Schlum\u00b7mer", ".", "Viel\u00b7leicht", "ge\u00b7denkt", "sie", "mei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Beym Erwachen, und seufzet, \u00bbarmer J\u00fcngling,", "tokens": ["Beym", "Er\u00b7wa\u00b7chen", ",", "und", "seuf\u00b7zet", ",", "\u00bb", "ar\u00b7mer", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "$,", "$(", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Warum waltet ein Unstern \u00fcber unsrer", "tokens": ["Wa\u00b7rum", "wal\u00b7tet", "ein", "Uns\u00b7tern", "\u00fc\u00b7ber", "uns\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "PPOSAT"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Liebe? K\u00f6nnt' ich die Deine werden, k\u00f6nnt' ich", "tokens": ["Lie\u00b7be", "?", "K\u00f6nnt'", "ich", "die", "Dei\u00b7ne", "wer\u00b7den", ",", "k\u00f6nnt'", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "VMFIN", "PPER", "ART", "PPOSAT", "VAINF", "$,", "VMFIN", "PPER"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "An dies klopfende M\u00e4dchenherz dich dr\u00fccken,", "tokens": ["An", "dies", "klop\u00b7fen\u00b7de", "M\u00e4d\u00b7chen\u00b7herz", "dich", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Traun, du w\u00fcrdest mich z\u00e4rtlich, z\u00e4rtlich lieben,", "tokens": ["Traun", ",", "du", "w\u00fcr\u00b7dest", "mich", "z\u00e4rt\u00b7lich", ",", "z\u00e4rt\u00b7lich", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Bis zum Grabe mich lieben\u00ab, ach, und weinet", "tokens": ["Bis", "zum", "Gra\u00b7be", "mich", "lie\u00b7ben", "\u00ab", ",", "ach", ",", "und", "wei\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "APPRART", "NN", "PPER", "VVINF", "$(", "$,", "ITJ", "$,", "KON", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Auf ihr K\u00fc\u00dfen das sch\u00f6nste Thr\u00e4nchen, welches", "tokens": ["Auf", "ihr", "K\u00fc\u00b7\u00dfen", "das", "sch\u00f6ns\u00b7te", "Thr\u00e4n\u00b7chen", ",", "wel\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Je ein M\u00e4dchen geweint hat. \u2013 Bleib, o Grille,", "tokens": ["Je", "ein", "M\u00e4d\u00b7chen", "ge\u00b7weint", "hat", ".", "\u2013", "Bleib", ",", "o", "Gril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VAFIN", "$.", "$(", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Keine Z\u00e4hre soll Laurens Auge tr\u00fcben,", "tokens": ["Kei\u00b7ne", "Z\u00e4h\u00b7re", "soll", "Lau\u00b7rens", "Au\u00b7ge", "tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "NN", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Ich will Klagen in deine Klagen wimmern,", "tokens": ["Ich", "will", "Kla\u00b7gen", "in", "dei\u00b7ne", "Kla\u00b7gen", "wim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Will mein trauriges Herz mir leichter weinen.", "tokens": ["Will", "mein", "trau\u00b7ri\u00b7ges", "Herz", "mir", "leich\u00b7ter", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}