{"dta.poem.5452": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Alle Dinge haben zwo Seiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn man, mit rechtem Ueberlegen, die Dinge dieser", "tokens": ["Wenn", "man", ",", "mit", "rech\u00b7tem", "Ue\u00b7ber\u00b7le\u00b7gen", ",", "die", "Din\u00b7ge", "die\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "PDAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Welt betracht't,", "tokens": ["Welt", "be\u00b7tracht't", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So sind sie das nicht, was sie sind; sie sind das, wozu", "tokens": ["So", "sind", "sie", "das", "nicht", ",", "was", "sie", "sind", ";", "sie", "sind", "das", ",", "wo\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "PTKNEG", "$,", "PRELS", "PPER", "VAFIN", "$.", "PPER", "VAFIN", "PDS", "$,", "PWAV"], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "man sie macht.", "tokens": ["man", "sie", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}