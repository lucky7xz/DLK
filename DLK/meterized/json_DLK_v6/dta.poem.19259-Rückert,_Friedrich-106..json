{"dta.poem.19259": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "106.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kennst du den Boten nicht, der dir allein Bericht", "tokens": ["Kennst", "du", "den", "Bo\u00b7ten", "nicht", ",", "der", "dir", "al\u00b7lein", "Be\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von h\u00f6hern Welten bringt? Der Bote hei\u00dfet Licht.", "tokens": ["Von", "h\u00f6\u00b7hern", "Wel\u00b7ten", "bringt", "?", "Der", "Bo\u00b7te", "hei\u00b7\u00dfet", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ist vor ihm der Schall? ein ungest\u00fcmer Prall,", "tokens": ["Was", "ist", "vor", "ihm", "der", "Schall", "?", "ein", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Prall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "ART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der h\u00f6chstens niederkommt vom niedern Wolkenwall.", "tokens": ["Der", "h\u00f6chs\u00b7tens", "nie\u00b7der\u00b7kommt", "vom", "nie\u00b7dern", "Wol\u00b7ken\u00b7wall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was ist vor ihm der Wind? ein wetterwendisch Kind,", "tokens": ["Was", "ist", "vor", "ihm", "der", "Wind", "?", "ein", "wet\u00b7ter\u00b7wen\u00b7disch", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "ART", "NN", "$.", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das \u00fcber Land und Meer f\u00e4hrt st\u00fcrmisch oder lind.", "tokens": ["Das", "\u00fc\u00b7ber", "Land", "und", "Meer", "f\u00e4hrt", "st\u00fcr\u00b7misch", "o\u00b7der", "lind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "KON", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was ist vor ihm der Duft? ein weicher Gru\u00df der Luft,", "tokens": ["Was", "ist", "vor", "ihm", "der", "Duft", "?", "ein", "wei\u00b7cher", "Gru\u00df", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "ART", "NN", "$.", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der deine Sehnsucht nur ins Unbestimmte ruft.", "tokens": ["Der", "dei\u00b7ne", "Sehn\u00b7sucht", "nur", "ins", "Un\u00b7be\u00b7stimm\u00b7te", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hat Schall, Wind oder Duft vom H\u00f6chsten dir gesprochen?", "tokens": ["Hat", "Schall", ",", "Wind", "o\u00b7der", "Duft", "vom", "H\u00f6chs\u00b7ten", "dir", "ge\u00b7spro\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "KON", "NN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hast du das Ewige geh\u00f6rt, gef\u00fchlt, gerochen?", "tokens": ["Hast", "du", "das", "E\u00b7wi\u00b7ge", "ge\u00b7h\u00f6rt", ",", "ge\u00b7f\u00fchlt", ",", "ge\u00b7ro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "VVFIN", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das Licht nur steiget dir aus h\u00f6chsten Sf\u00e4ren nieder,", "tokens": ["Das", "Licht", "nur", "stei\u00b7get", "dir", "aus", "h\u00f6chs\u00b7ten", "Sf\u00e4\u00b7ren", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und steigt mit deinem Blick zu h\u00f6chsten Sf\u00e4ren wieder.", "tokens": ["Und", "steigt", "mit", "dei\u00b7nem", "Blick", "zu", "h\u00f6chs\u00b7ten", "Sf\u00e4\u00b7ren", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Folge nur seiner Spur! Verst\u00e4ndest du es nur,", "tokens": ["Fol\u00b7ge", "nur", "sei\u00b7ner", "Spur", "!", "Ver\u00b7st\u00e4n\u00b7dest", "du", "es", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und unverstanden w\u00e4r dir nichts in der Natur.", "tokens": ["Und", "un\u00b7ver\u00b7stan\u00b7den", "w\u00e4r", "dir", "nichts", "in", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Schon hat der Astronom vom Lichte dort gelernt,", "tokens": ["Schon", "hat", "der", "Ast\u00b7ro\u00b7nom", "vom", "Lich\u00b7te", "dort", "ge\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie weit am Himmelsdom die Kuppeln stehn entfernt.", "tokens": ["Wie", "weit", "am", "Him\u00b7mels\u00b7dom", "die", "Kup\u00b7peln", "stehn", "ent\u00b7fernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Er hat von ihm gelernt die Gr\u00f6\u00dfen und die Bahnen,", "tokens": ["Er", "hat", "von", "ihm", "ge\u00b7lernt", "die", "Gr\u00f6\u00b7\u00dfen", "und", "die", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Ma\u00dfe messen und die Eigenschaften ahnen.", "tokens": ["Die", "Ma\u00b7\u00dfe", "mes\u00b7sen", "und", "die", "Ei\u00b7gen\u00b7schaf\u00b7ten", "ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wei\u00dft du, wieviel Gestalt der Vielgestaltige tr\u00e4gt,", "tokens": ["Wei\u00dft", "du", ",", "wie\u00b7viel", "Ge\u00b7stalt", "der", "Viel\u00b7ge\u00b7stal\u00b7ti\u00b7ge", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Der Mittler, wie und wo er sich ins Mittel schl\u00e4gt?", "tokens": ["Der", "Mitt\u00b7ler", ",", "wie", "und", "wo", "er", "sich", "ins", "Mit\u00b7tel", "schl\u00e4gt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "KON", "PWAV", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Er selbst ist wol der Duft, er selbst ist wol die Luft,", "tokens": ["Er", "selbst", "ist", "wol", "der", "Duft", ",", "er", "selbst", "ist", "wol", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ART", "NN", "$,", "PPER", "ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er selbst ist wol der Schall, den er ins Leben ruft.", "tokens": ["Er", "selbst", "ist", "wol", "der", "Schall", ",", "den", "er", "ins", "Le\u00b7ben", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Hier siehst du unvermerkt in W\u00e4rm' ihn sich verlieren,", "tokens": ["Hier", "siehst", "du", "un\u00b7ver\u00b7merkt", "in", "W\u00e4rm'", "ihn", "sich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Spiel der Farben dort, die seine S\u00e4ume zieren.", "tokens": ["In", "Spiel", "der", "Far\u00b7ben", "dort", ",", "die", "sei\u00b7ne", "S\u00e4u\u00b7me", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Der hier des Fr\u00fchlings Schein, dort Kern von Holz und Stein", "tokens": ["Der", "hier", "des", "Fr\u00fch\u00b7lings", "Schein", ",", "dort", "Kern", "von", "Holz", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "NN", "$,", "ADV", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird im Magnet der Zug und Gegenzug auch seyn;", "tokens": ["Wird", "im", "Mag\u00b7net", "der", "Zug", "und", "Ge\u00b7gen\u00b7zug", "auch", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "KON", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Wird schlagen hier als Puls, und dort elektrisch blitzen,", "tokens": ["Wird", "schla\u00b7gen", "hier", "als", "Puls", ",", "und", "dort", "e\u00b7lekt\u00b7risch", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ADV", "KOUS", "NN", "$,", "KON", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich in alles zu verwandeln Kraft besitzen.", "tokens": ["Und", "sich", "in", "al\u00b7les", "zu", "ver\u00b7wan\u00b7deln", "Kraft", "be\u00b7sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PIS", "PTKZU", "VVINF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Du kanst nicht zweifeln, Geist, es sei ein gro\u00dfer Geist;", "tokens": ["Du", "kanst", "nicht", "zwei\u00b7feln", ",", "Geist", ",", "es", "sei", "ein", "gro\u00b7\u00dfer", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Frag' ist was zu ihm du im Verh\u00e4ltnis seist.", "tokens": ["Die", "Frag'", "ist", "was", "zu", "ihm", "du", "im", "Ver\u00b7h\u00e4lt\u00b7nis", "seist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "APPR", "PPER", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}