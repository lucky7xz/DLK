{"textgrid.poem.58497": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie lange, Simeon, hast du nicht warten m\u00fcssen,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lange, Simeon, hast du nicht warten m\u00fcssen,", "tokens": ["Wie", "lan\u00b7ge", ",", "Si\u00b7me\u00b7on", ",", "hast", "du", "nicht", "war\u00b7ten", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "NE", "$,", "VAFIN", "PPER", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis du das Kind im Arm, dein Herz den Heiland hat!", "tokens": ["Bis", "du", "das", "Kind", "im", "Arm", ",", "dein", "Herz", "den", "Hei\u00b7land", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich will die lange Weil', die kleine M\u00fch verdrie\u00dfen,", "tokens": ["Mich", "will", "die", "lan\u00b7ge", "Weil'", ",", "die", "klei\u00b7ne", "M\u00fch", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich seufze und ich such', ich wart' und werde matt.", "tokens": ["Ich", "seuf\u00b7ze", "und", "ich", "such'", ",", "ich", "wart'", "und", "wer\u00b7de", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nein, nein, ich bleibe treu, ich la\u00df den Treuen walten", "tokens": ["Nein", ",", "nein", ",", "ich", "blei\u00b7be", "treu", ",", "ich", "la\u00df", "den", "Treu\u00b7en", "wal\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sollt' ich dr\u00fcber auch wie Simeon veralten.", "tokens": ["Und", "sollt'", "ich", "dr\u00fc\u00b7ber", "auch", "wie", "Si\u00b7me\u00b7on", "ver\u00b7al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PAV", "ADV", "KOKOM", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}