{"textgrid.poem.49627": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "B\u00fclow", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht wirklich Bernhard mit dem Scheitel", "tokens": ["Geht", "wirk\u00b7lich", "Bern\u00b7hard", "mit", "dem", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und der gepappten Haarfrisur?", "tokens": ["Und", "der", "ge\u00b7papp\u00b7ten", "Haar\u00b7fri\u00b7sur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist all und jede Gr\u00f6\u00dfe eitel?", "tokens": ["Ist", "all", "und", "je\u00b7de", "Gr\u00f6\u00b7\u00dfe", "ei\u00b7tel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "KON", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4gt jedem seine Totenuhr?", "tokens": ["Schl\u00e4gt", "je\u00b7dem", "sei\u00b7ne", "To\u00b7te\u00b7nuhr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind die Erfolge ganz vergessen,", "tokens": ["Sind", "die", "Er\u00b7fol\u00b7ge", "ganz", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Herr von B\u00fclow uns gebracht?", "tokens": ["Die", "Herr", "von", "B\u00fc\u00b7low", "uns", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soupers und Fr\u00fchst\u00fccks, Mittagessen?", "tokens": ["Sou\u00b7pers", "und", "Fr\u00fch\u00b7st\u00fccks", ",", "Mit\u00b7ta\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kurz alles, was er mitgemacht?", "tokens": ["Kurz", "al\u00b7les", ",", "was", "er", "mit\u00b7ge\u00b7macht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er war doch stets in seiner Meinung", "tokens": ["Er", "war", "doch", "stets", "in", "sei\u00b7ner", "Mei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So biegsam wie das Binsenrohr!", "tokens": ["So", "bieg\u00b7sam", "wie", "das", "Bin\u00b7sen\u00b7rohr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt ist er chronische Erscheinung", "tokens": ["Jetzt", "ist", "er", "chro\u00b7ni\u00b7sche", "Er\u00b7schei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und geht! Mir kommt es seltsam vor.", "tokens": ["Und", "geht", "!", "Mir", "kommt", "es", "selt\u00b7sam", "vor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich las die Nachricht beim Salvator", "tokens": ["Ich", "las", "die", "Nach\u00b7richt", "beim", "Sal\u00b7va\u00b7tor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und dachte mir sogleich dazu:", "tokens": ["Und", "dach\u00b7te", "mir", "sog\u00b7leich", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sucht der Bismarck-Imitator", "tokens": ["Wo", "sucht", "der", "Bis\u00b7ma\u00b7rck\u00b7I\u00b7mi\u00b7ta\u00b7tor"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sein grollbelad'nes Friedrichsruh?", "tokens": ["Sein", "groll\u00b7be\u00b7la\u00b7d'\u00b7nes", "Fried\u00b7richs\u00b7ruh", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}