{"dta.poem.18393": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihm alle gewogenheit ins  \n k\u00fcnfftige versagte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach m\u00f6chtestu vor mich so viel erbarmung hegen/", "tokens": ["Ach", "m\u00f6ch\u00b7tes\u00b7tu", "vor", "mich", "so", "viel", "er\u00b7bar\u00b7mung", "he\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wohl mein kranckes hertz verliebte flammen tr\u00e4gt/", "tokens": ["Als", "wohl", "mein", "kran\u00b7ckes", "hertz", "ver\u00b7lieb\u00b7te", "flam\u00b7men", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00fcrde dieses blat dich nicht zum zorn bewegen/", "tokens": ["So", "w\u00fcr\u00b7de", "die\u00b7ses", "blat", "dich", "nicht", "zum", "zorn", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df meine faust hiermit zu deinen f\u00fcssen legt.", "tokens": ["Da\u00df", "mei\u00b7ne", "faust", "hier\u00b7mit", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PAV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die wehmuth w\u00fcrde dir bi\u00df zu der seelen steigen/", "tokens": ["Die", "weh\u00b7muth", "w\u00fcr\u00b7de", "dir", "bi\u00df", "zu", "der", "see\u00b7len", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann du die thr\u00e4nen solst auf meinen wangen sehn/", "tokens": ["Wann", "du", "die", "thr\u00e4\u00b7nen", "solst", "auf", "mei\u00b7nen", "wan\u00b7gen", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wie sich meine brust schon will zum grabe neigen/", "tokens": ["Und", "wie", "sich", "mei\u00b7ne", "brust", "schon", "will", "zum", "gra\u00b7be", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "PPOSAT", "NN", "ADV", "VMFIN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Indem es g\u00e4ntzlich fast um deinen knecht geschehn.", "tokens": ["In\u00b7dem", "es", "g\u00e4ntz\u00b7lich", "fast", "um", "dei\u00b7nen", "knecht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun aber wei\u00df ich dir nicht gnugsam vorzustellen", "tokens": ["Nun", "a\u00b7ber", "wei\u00df", "ich", "dir", "nicht", "gnug\u00b7sam", "vor\u00b7zu\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die centner-schwere last der unersch\u00f6pfften pein/", "tokens": ["Die", "cent\u00b7ner\u00b7schwe\u00b7re", "last", "der", "un\u00b7er\u00b7sch\u00f6pff\u00b7ten", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wormit dein harter schlu\u00df will meine brust verg\u00e4llen/", "tokens": ["Wor\u00b7mit", "dein", "har\u00b7ter", "schlu\u00df", "will", "mei\u00b7ne", "brust", "ver\u00b7g\u00e4l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und mich heist ohne lust und ohn vergn\u00fcgen seyn.", "tokens": ["Und", "mich", "heist", "oh\u00b7ne", "lust", "und", "ohn", "ver\u00b7gn\u00fc\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Selbst meine faust wei\u00df kaum die feder mehr zu f\u00fchren/", "tokens": ["Selbst", "mei\u00b7ne", "faust", "wei\u00df", "kaum", "die", "fe\u00b7der", "mehr", "zu", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Di\u00df blat ist auch zu eng vor meine traurigkeit,", "tokens": ["Di\u00df", "blat", "ist", "auch", "zu", "eng", "vor", "mei\u00b7ne", "trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "ADV", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und was mir etwan noch soll meine geister r\u00fchren/", "tokens": ["Und", "was", "mir", "et\u00b7wan", "noch", "soll", "mei\u00b7ne", "geis\u00b7ter", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Hat angst und bittres weh! mit wermut \u00fcberstreut.", "tokens": ["Hat", "angst", "und", "bitt\u00b7res", "weh", "!", "mit", "wer\u00b7mut", "\u00fc\u00b7bers\u00b7treut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "ADJA", "PTKVZ", "$.", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die tinte will mir nicht/ wie sonst vor diesem/ fliessen.", "tokens": ["Die", "tin\u00b7te", "will", "mir", "nicht", "/", "wie", "sonst", "vor", "die\u00b7sem", "/", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "$(", "KOKOM", "ADV", "APPR", "PDAT", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die reimen \u00fcbergehn ihr fest-gesetztes ziel/", "tokens": ["Die", "rei\u00b7men", "\u00fc\u00b7ber\u00b7gehn", "ihr", "fest\u00b7ge\u00b7setz\u00b7tes", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vor zittern kan die hand nicht recht die sylben schliessen/", "tokens": ["Vor", "zit\u00b7tern", "kan", "die", "hand", "nicht", "recht", "die", "syl\u00b7ben", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VMFIN", "ART", "NN", "PTKNEG", "ADJD", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und kurtz; Ich selber bin des himmels gauckel-spiel.", "tokens": ["Und", "kurtz", ";", "Ich", "sel\u00b7ber", "bin", "des", "him\u00b7mels", "gau\u00b7ckel\u00b7spiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPER", "ADV", "VAFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Darff ich/ Arminde/ dir die wahrheit recht gestehen/", "tokens": ["Darff", "ich", "/", "Ar\u00b7min\u00b7de", "/", "dir", "die", "wahr\u00b7heit", "recht", "ge\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "NE", "$(", "PPER", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So hastu selbst die qual in meiner brust erregt;", "tokens": ["So", "has\u00b7tu", "selbst", "die", "qual", "in", "mei\u00b7ner", "brust", "er\u00b7regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es scheint/ ich soll nicht mehr auf freuden-rosen gehen/", "tokens": ["Es", "scheint", "/", "ich", "soll", "nicht", "mehr", "auf", "freu\u00b7den\u00b7ro\u00b7sen", "ge\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ach! da\u00df der himmel mich mit solcher angst belegt.", "tokens": ["Ach", "!", "da\u00df", "der", "him\u00b7mel", "mich", "mit", "sol\u00b7cher", "angst", "be\u00b7legt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "PRF", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dein schu\u00df/ dein harter schlu\u00df/ der zwinget mich zum sterben/", "tokens": ["Dein", "schu\u00df", "/", "dein", "har\u00b7ter", "schlu\u00df", "/", "der", "zwin\u00b7get", "mich", "zum", "ster\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$(", "ART", "VVFIN", "PPER", "APPRART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was die Erg\u00f6tzung bringt/ greifft mich mit schmertzen an/", "tokens": ["Was", "die", "Er\u00b7g\u00f6t\u00b7zung", "bringt", "/", "greifft", "mich", "mit", "schmert\u00b7zen", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$(", "VVFIN", "PRF", "APPR", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Aus deiner h\u00e4rtigkeit entstehet mein verderben/", "tokens": ["Aus", "dei\u00b7ner", "h\u00e4r\u00b7tig\u00b7keit", "ent\u00b7ste\u00b7het", "mein", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Kein wunder/ da\u00df ich auch fast nicht mehr leben kan!", "tokens": ["Kein", "wun\u00b7der", "/", "da\u00df", "ich", "auch", "fast", "nicht", "mehr", "le\u00b7ben", "kan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "KOUS", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du zeigst mir jetzund nichts als schwartze trauer-kertzen/", "tokens": ["Du", "zeigst", "mir", "je\u00b7tzund", "nichts", "als", "schwart\u00b7ze", "trau\u00b7e\u00b7rkert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "KOKOM", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mein freuden-licht hast du verkehrt in kummer-nacht/", "tokens": ["Mein", "freu\u00b7den\u00b7licht", "hast", "du", "ver\u00b7kehrt", "in", "kum\u00b7mer\u00b7nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Du labest dein gem\u00fcth an meinen seelen-schmertzen/", "tokens": ["Du", "la\u00b7best", "dein", "ge\u00b7m\u00fcth", "an", "mei\u00b7nen", "see\u00b7len\u00b7schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Doch welche freude hat dir meine noth gebracht?", "tokens": ["Doch", "wel\u00b7che", "freu\u00b7de", "hat", "dir", "mei\u00b7ne", "noth", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Soll ich nun ferner nicht den s\u00fcssen thau geniessen/", "tokens": ["Soll", "ich", "nun", "fer\u00b7ner", "nicht", "den", "s\u00fcs\u00b7sen", "thau", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den mir dein rosen-mund vor kurtzem hat gewehrt/", "tokens": ["Den", "mir", "dein", "ro\u00b7sen\u00b7mund", "vor", "kurt\u00b7zem", "hat", "ge\u00b7wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So mu\u00df die seele selbst in bleicher angst zerfliessen/", "tokens": ["So", "mu\u00df", "die", "see\u00b7le", "selbst", "in", "blei\u00b7cher", "angst", "zer\u00b7flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und zeigen/ wie dein schlu\u00df itzt meine brust versehrt.", "tokens": ["Und", "zei\u00b7gen", "/", "wie", "dein", "schlu\u00df", "itzt", "mei\u00b7ne", "brust", "ver\u00b7sehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "KOKOM", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Arminde/ nicht zu streng! du weist/ ich mu\u00df dich lieben/", "tokens": ["Ar\u00b7min\u00b7de", "/", "nicht", "zu", "streng", "!", "du", "weist", "/", "ich", "mu\u00df", "dich", "lie\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PTKNEG", "PTKZU", "VVFIN", "$.", "PPER", "VVFIN", "$(", "PPER", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein trieb der \u00fcber mich und meine kr\u00e4ffte steigt/", "tokens": ["Ein", "trieb", "der", "\u00fc\u00b7ber", "mich", "und", "mei\u00b7ne", "kr\u00e4ff\u00b7te", "steigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein \u00fcberirrdscher zwang hat mich hierzu getrieben/", "tokens": ["Ein", "\u00fc\u00b7be\u00b7rirrd\u00b7scher", "zwang", "hat", "mich", "hier\u00b7zu", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Schau wie mein hertze sich vor deiner gottheit beugt/", "tokens": ["Schau", "wie", "mein", "hert\u00b7ze", "sich", "vor", "dei\u00b7ner