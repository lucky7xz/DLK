{"textgrid.poem.66025": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht dort den Mann mit seiner L\u00f6wenm\u00e4hne,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht dort den Mann mit seiner L\u00f6wenm\u00e4hne,", "tokens": ["Seht", "dort", "den", "Mann", "mit", "sei\u00b7ner", "L\u00f6\u00b7wen\u00b7m\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die F\u00e4uste schl\u00fcgen einen Ochsen nieder.", "tokens": ["Die", "F\u00e4us\u00b7te", "schl\u00fc\u00b7gen", "ei\u00b7nen", "Och\u00b7sen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Dichter ist's, und seine Heldenthaten \u2013", "tokens": ["Ein", "Dich\u00b7ter", "ist's", ",", "und", "sei\u00b7ne", "Hel\u00b7den\u00b7tha\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein B\u00e4ndchen veilchenblauer Liebeslieder.", "tokens": ["Ein", "B\u00e4nd\u00b7chen", "veil\u00b7chen\u00b7blau\u00b7er", "Lie\u00b7bes\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und jenes M\u00e4nnchen, sch\u00fcchtern wie ein M\u00e4dchen,", "tokens": ["Und", "je\u00b7nes", "M\u00e4nn\u00b7chen", ",", "sch\u00fcch\u00b7tern", "wie", "ein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Err\u00f6tend schon, nennt man nur seinen Namen,", "tokens": ["Er\u00b7r\u00f6\u00b7tend", "schon", ",", "nennt", "man", "nur", "sei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "$,", "VVFIN", "PIS", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Zum Helden hat ihn die Natur verdorben,", "tokens": ["Zum", "Hel\u00b7den", "hat", "ihn", "die", "Na\u00b7tur", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er schreibt daf\u00fcr geniale Feuerdramen.", "tokens": ["Er", "schreibt", "da\u00b7f\u00fcr", "ge\u00b7ni\u00b7a\u00b7le", "Feu\u00b7er\u00b7dra\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch dieser mit dem eleganten Wuchse,", "tokens": ["Doch", "die\u00b7ser", "mit", "dem", "e\u00b7leg\u00b7an\u00b7ten", "Wuch\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Damen werden jedesmal ekstatisch:", "tokens": ["Die", "Da\u00b7men", "wer\u00b7den", "je\u00b7des\u00b7mal", "eks\u00b7ta\u00b7tisch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbwelch' sch\u00f6ner Mann! Und welch' Organ! zu himmlisch!\u00ab", "tokens": ["\u00bb", "welch'", "sch\u00f6\u00b7ner", "Mann", "!", "Und", "welch'", "Or\u00b7gan", "!", "zu", "himm\u00b7lisch", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWAT", "ADJA", "NN", "$.", "KON", "PWAT", "NN", "$.", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fondsmakler ist er und ein Held am Skattisch!", "tokens": ["Fonds\u00b7mak\u00b7ler", "ist", "er", "und", "ein", "Held", "am", "Skat\u00b7tisch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}