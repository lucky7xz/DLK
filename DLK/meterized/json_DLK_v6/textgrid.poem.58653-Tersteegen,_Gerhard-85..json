{"textgrid.poem.58653": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Anfang, wenn der Herr sein Volk will aus \u00c4gypten bringen,", "tokens": ["Im", "An\u00b7fang", ",", "wenn", "der", "Herr", "sein", "Volk", "will", "aus", "\u00c4\u00b7gyp\u00b7ten", "brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Mu\u00df er meist durch Gewissensangst und durchs Gesetz sie zwingen;", "tokens": ["Mu\u00df", "er", "meist", "durch", "Ge\u00b7wis\u00b7sen\u00b7sangst", "und", "durchs", "Ge\u00b7setz", "sie", "zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "KON", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Man l\u00e4\u00dft aus Furcht das B\u00f6se nur, man zwingt zum Guten sich,", "tokens": ["Man", "l\u00e4\u00dft", "aus", "Furcht", "das", "B\u00f6\u00b7se", "nur", ",", "man", "zwingt", "zum", "Gu\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "ART", "NN", "ADV", "$,", "PIS", "VVFIN", "APPRART", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Doch h\u00e4lt die Seel' nicht diesen Bund, die Lust sitzt innerlich.", "tokens": ["Doch", "h\u00e4lt", "die", "Seel'", "nicht", "die\u00b7sen", "Bund", ",", "die", "Lust", "sitzt", "in\u00b7ner\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "PDAT", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Herr, schreib mir dein Gesetz ins Herz, gib Kraft und Lust und Liebe,", "tokens": ["Herr", ",", "schreib", "mir", "dein", "Ge\u00b7setz", "ins", "Herz", ",", "gib", "Kraft", "und", "Lust", "und", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,", "VVIMP", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Zu folgen frei und williglich des neuen Geistes Triebe,", "tokens": ["Zu", "fol\u00b7gen", "frei", "und", "wil\u00b7lig\u00b7lich", "des", "neu\u00b7en", "Geis\u00b7tes", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Mach mich des neuen Bundes Kind, sei du mein Gott allein", "tokens": ["Mach", "mich", "des", "neu\u00b7en", "Bun\u00b7des", "Kind", ",", "sei", "du", "mein", "Gott", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Und la\u00df mich ewig dir getreu und ganz der Deine sein!", "tokens": ["Und", "la\u00df", "mich", "e\u00b7wig", "dir", "ge\u00b7treu", "und", "ganz", "der", "Dei\u00b7ne", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "PPER", "ADJD", "KON", "ADV", "ART", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}