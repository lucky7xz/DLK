{"textgrid.poem.58310": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "348. Gott im Herzen tr\u00f6st't in Schmerzen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Seelen, eilet doch, sterbt freudig der Natur", "tokens": ["Ach", "See\u00b7len", ",", "ei\u00b7let", "doch", ",", "sterbt", "freu\u00b7dig", "der", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "VVFIN", "ADV", "$,", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aller Eigenheit, da\u00df ihr Gott in euch findet!", "tokens": ["Und", "al\u00b7ler", "Ei\u00b7gen\u00b7heit", ",", "da\u00df", "ihr", "Gott", "in", "euch", "fin\u00b7det", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Not und Tod gilt nichts als Gott im Herzen nur;", "tokens": ["In", "Not", "und", "Tod", "gilt", "nichts", "als", "Gott", "im", "Her\u00b7zen", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PIS", "KOKOM", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies h\u00e4lt im Feu'r die Prob', wann Schein und Wahn verschwindet.", "tokens": ["Dies", "h\u00e4lt", "im", "Feu'r", "die", "Prob'", ",", "wann", "Schein", "und", "Wahn", "ver\u00b7schwin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "wor