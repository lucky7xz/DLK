{"dta.poem.3224": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ohn' Arbeit w\u00e4r' ein Freudenaufenthalt,", "tokens": ["Die", "Welt", "ohn'", "Ar\u00b7beit", "w\u00e4r'", "ein", "Freu\u00b7den\u00b7au\u00b7fent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit der Arbeit ist sie eine Strafanstalt.", "tokens": ["Und", "mit", "der", "Ar\u00b7beit", "ist", "sie", "ei\u00b7ne", "Stra\u00b7fan\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie mit dem Paradies die Freiheit ward verloren,", "tokens": ["Wie", "mit", "dem", "Pa\u00b7ra\u00b7dies", "die", "Frei\u00b7heit", "ward", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird sie wieder mit dem Paradies geboren.", "tokens": ["So", "wird", "sie", "wie\u00b7der", "mit", "dem", "Pa\u00b7ra\u00b7dies", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wann selbst die Element' erst dienstbar sich bequemen,", "tokens": ["Wann", "selbst", "die", "E\u00b7le\u00b7ment'", "erst", "dienst\u00b7bar", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "ADJD", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem freien Menschen ganz die Arbeit abzunehmen,", "tokens": ["Dem", "frei\u00b7en", "Men\u00b7schen", "ganz", "die", "Ar\u00b7beit", "ab\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann ist, da\u00df sie dem Wink des Zauberstabes dienen,", "tokens": ["Dann", "ist", ",", "da\u00df", "sie", "dem", "Wink", "des", "Zau\u00b7ber\u00b7sta\u00b7bes", "die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Menschengeist nur noch der Lenker der Maschinen.", "tokens": ["Der", "Men\u00b7schen\u00b7geist", "nur", "noch", "der", "Len\u00b7ker", "der", "Ma\u00b7schi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum ringt nur muthig loszul\u00f6then eure Fessel,", "tokens": ["Drum", "ringt", "nur", "mut\u00b7hig", "los\u00b7zu\u00b7l\u00f6\u00b7then", "eu\u00b7re", "Fes\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um aufzuklimmen zum verlornen Herrschaftsessel;", "tokens": ["Um", "auf\u00b7zu\u00b7klim\u00b7men", "zum", "ver\u00b7lor\u00b7nen", "Herr\u00b7schaft\u00b7ses\u00b7sel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.6": {"line.1": {"text": "Wenn erst der Arbeit ihr zum eignen Heil entbehren", "tokens": ["Wenn", "erst", "der", "Ar\u00b7beit", "ihr", "zum", "eig\u00b7nen", "Heil", "ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lernt, und zu Lenkern taugt aus Sklaven der Galeeren.", "tokens": ["Lernt", ",", "und", "zu", "Len\u00b7kern", "taugt", "aus", "Skla\u00b7ven", "der", "Ga\u00b7lee\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "APPR", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}