{"dta.poem.2205": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tirsanders Christenthum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beym Erndten seh ich das Betragen der Menschen", "tokens": ["Beym", "Ernd\u00b7ten", "seh", "ich", "das", "Be\u00b7tra\u00b7gen", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "mit Betr\u00fcbni\u00df an,", "tokens": ["mit", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dann hat die Unerkenntlichkeit und unser Undank keine", "tokens": ["Dann", "hat", "die", "Un\u00b7er\u00b7kennt\u00b7lich\u00b7keit", "und", "un\u00b7ser", "Un\u00b7dank", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Schranken;", "tokens": ["Schran\u00b7ken", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ja, minder, als zur Erndtezeit der Mensch dem Sch\u00f6-", "tokens": ["Ja", ",", "min\u00b7der", ",", "als", "zur", "Ernd\u00b7te\u00b7zeit", "der", "Mensch", "dem", "Sch\u00f6"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "$,", "KOUS", "APPRART", "NN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "pfer danket, kann", "tokens": ["pfer", "dan\u00b7ket", ",", "kann"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "$,", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Bey seinem Korn kein Hammster danken.", "tokens": ["Bey", "sei\u00b7nem", "Korn", "kein", "Hamms\u00b7ter", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}