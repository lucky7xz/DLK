{"dta.poem.13474": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der wandernde Student.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Bei dem angenehmsten Wetter", "tokens": ["Bei", "dem", "an\u00b7ge\u00b7nehms\u00b7ten", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singen alle V\u00f6gelein,", "tokens": ["Sin\u00b7gen", "al\u00b7le", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klatscht der Regen auf die Bl\u00e4tter,", "tokens": ["Klatscht", "der", "Re\u00b7gen", "auf", "die", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sing ich so f\u00fcr mich allein.", "tokens": ["Sing", "ich", "so", "f\u00fcr", "mich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn mein Aug' kann nichts entdecken;", "tokens": ["Denn", "mein", "Aug'", "kann", "nichts", "ent\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Blitz auch grausam gl\u00fcht,", "tokens": ["Wenn", "der", "Blitz", "auch", "grau\u00b7sam", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was im Wandeln k\u00f6nnt' erschrecken", "tokens": ["Was", "im", "Wan\u00b7deln", "k\u00f6nnt'", "er\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein zufriedenes Gem\u00fcth.", "tokens": ["Ein", "zu\u00b7frie\u00b7de\u00b7nes", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Frei von Mammon will ich schreiten", "tokens": ["Frei", "von", "Mam\u00b7mon", "will", "ich", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Feld der Wissenschaft,", "tokens": ["Auf", "dem", "Feld", "der", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sinne ernst und nehm' zu Zeiten", "tokens": ["Sin\u00b7ne", "ernst", "und", "nehm'", "zu", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Mund voll Rebensaft.", "tokens": ["Ei\u00b7nen", "Mund", "voll", "Re\u00b7ben\u00b7saft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bin ich m\u00fcde vom Studieren,", "tokens": ["Bin", "ich", "m\u00fc\u00b7de", "vom", "Stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Wann der Mond tritt sanft herf\u00fcr,", "tokens": ["Wann", "der", "Mond", "tritt", "sanft", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfleg' ich dann zu musiziren", "tokens": ["Pfleg'", "ich", "dann", "zu", "mu\u00b7si\u00b7zi\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor der Allersch\u00f6nsten Th\u00fcr.", "tokens": ["Vor", "der", "Al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Th\u00fcr."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}