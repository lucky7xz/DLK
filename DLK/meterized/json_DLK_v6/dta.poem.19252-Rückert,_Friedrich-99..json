{"dta.poem.19252": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Winde wechseln wol nach jedem Himmelstrich,", "tokens": ["Die", "Win\u00b7de", "wech\u00b7seln", "wol", "nach", "je\u00b7dem", "Him\u00b7mel\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Einer ist der bleibt und ist der Wind an sich:", "tokens": ["Doch", "Ei\u00b7ner", "ist", "der", "bleibt", "und", "ist", "der", "Wind", "an", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "VVFIN", "KON", "VAFIN", "ART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Ostwind, der sogleich die heil'gen Fl\u00fcgel regt,", "tokens": ["Der", "Ost\u00b7wind", ",", "der", "sog\u00b7leich", "die", "heil'\u00b7gen", "Fl\u00fc\u00b7gel", "regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald das Ungest\u00fcm der andern sich gelegt;", "tokens": ["So\u00b7bald", "das", "Un\u00b7ge\u00b7st\u00fcm", "der", "an\u00b7dern", "sich", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Ostwind, der allein, wenn andre aufgest\u00f6rt", "tokens": ["Der", "Ost\u00b7wind", ",", "der", "al\u00b7lein", ",", "wenn", "and\u00b7re", "auf\u00b7ge\u00b7st\u00f6rt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Zufall sind, dem Gang der Sonne selbst geh\u00f6rt;", "tokens": ["Vom", "Zu\u00b7fall", "sind", ",", "dem", "Gang", "der", "Son\u00b7ne", "selbst", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dem Gang des Sonnenlichts, das sich entgegenbreitet", "tokens": ["Dem", "Gang", "des", "Son\u00b7nen\u00b7lichts", ",", "das", "sich", "ent\u00b7ge\u00b7gen\u00b7brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erd' in jedem Nu, wie sie gen Osten schreitet.", "tokens": ["Der", "Erd'", "in", "je\u00b7dem", "Nu", ",", "wie", "sie", "gen", "Os\u00b7ten", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "ADV", "$,", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wol f\u00fchlest du von ihm den Ku\u00df an Stirn und Wange,", "tokens": ["Wol", "f\u00fch\u00b7lest", "du", "von", "ihm", "den", "Ku\u00df", "an", "Stirn", "und", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wenn windstill ist die Luft, bei jedem Sonnaufgange.", "tokens": ["Wenn", "wind\u00b7still", "ist", "die", "Luft", ",", "bei", "je\u00b7dem", "Son\u00b7nauf\u00b7gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Den heil'gen Fr\u00fchhauch la\u00df, eh einer von den vielen", "tokens": ["Den", "heil'\u00b7gen", "Fr\u00fch\u00b7hauch", "la\u00df", ",", "eh", "ei\u00b7ner", "von", "den", "vie\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KOUS", "ART", "APPR", "ART", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tagwinden sich erhebt, dich ahnungsvoll umspielen.", "tokens": ["Tag\u00b7win\u00b7den", "sich", "er\u00b7hebt", ",", "dich", "ah\u00b7nungs\u00b7voll", "um\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "$,", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}