{"dta.poem.7350": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Montag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewenn ich Montags fr\u00fch erwache,", "tokens": ["\u201e", "wenn", "ich", "Mon\u00b7tags", "fr\u00fch", "er\u00b7wa\u00b7che", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird mir's ganz behaglich gleich;", "tokens": ["Wird", "mir's", "ganz", "be\u00b7hag\u00b7lich", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Montag hat so eigne Sache", "tokens": ["Mon\u00b7tag", "hat", "so", "eig\u00b7ne", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem kleinen Wochenreich.", "tokens": ["In", "dem", "klei\u00b7nen", "Wo\u00b7chen\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die Predigt liegt noch ferne,", "tokens": ["Denn", "die", "Pre\u00b7digt", "liegt", "noch", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Sorgen scheinen leicht;", "tokens": ["Al\u00b7le", "Sor\u00b7gen", "schei\u00b7nen", "leicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Keiner k\u00f6mmt am Montag gerne,", "tokens": ["Kei\u00b7ner", "k\u00f6mmt", "am", "Mon\u00b7tag", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sey's zur Trauung, sey's zur Beicht.\u201c", "tokens": ["Sey's", "zur", "Trau\u00b7ung", ",", "sey's", "zur", "Beicht", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eund man darf mir's nicht verdenken,", "tokens": ["\u201e", "und", "man", "darf", "mir's", "nicht", "ver\u00b7den\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIS", "VMFIN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich in des Amtes Frist", "tokens": ["Will", "ich", "in", "des", "Am\u00b7tes", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem ein freies St\u00fcndchen schenken,", "tokens": ["Dem", "ein", "frei\u00b7es", "St\u00fcnd\u00b7chen", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was doch auch zu loben ist.", "tokens": ["Was", "doch", "auch", "zu", "lo\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So erwacht denn, ihr Gesellen", "tokens": ["So", "er\u00b7wacht", "denn", ",", "ihr", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner flei\u00df'gen Jugendzeit!", "tokens": ["Mei\u00b7ner", "flei\u00df'\u00b7gen", "Ju\u00b7gend\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollt' in Reih' und Glied euch stellen,", "tokens": ["Wollt'", "in", "Reih'", "und", "Glied", "euch", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alte Bilder, eingeschneit!\u201c", "tokens": ["Al\u00b7te", "Bil\u00b7der", ",", "ein\u00b7ge\u00b7schneit", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eilion will ich bekriegen,", "tokens": ["\u201e", "i\u00b7li\u00b7on", "will", "ich", "be\u00b7krie\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Horaz auf Reisen geh'n,", "tokens": ["Mit", "Ho\u00b7raz", "auf", "Rei\u00b7sen", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Will mit Alexander siegen", "tokens": ["Will", "mit", "A\u00b7lex\u00b7an\u00b7der", "sie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an Memnons S\u00e4ule steh'n.", "tokens": ["Und", "an", "Mem\u00b7nons", "S\u00e4u\u00b7le", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPR", "NE", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder auch vergn\u00fcgt ergr\u00fcnden,", "tokens": ["O\u00b7der", "auch", "ver\u00b7gn\u00fcgt", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was das Vaterland gebracht,", "tokens": ["Was", "das", "Va\u00b7ter\u00b7land", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mich mit Kant und Wolf verb\u00fcnden,", "tokens": ["Mich", "mit", "Kant", "und", "Wolf", "ver\u00b7b\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zieh'n mit Laudon in die Schlacht.\u201c", "tokens": ["Zieh'n", "mit", "Lau\u00b7don", "in", "die", "Schlacht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf der B\u00fccherleiter traben", "tokens": ["Auf", "der", "B\u00fc\u00b7cher\u00b7lei\u00b7ter", "tra\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh den Pfarrer, lustentbrannt,", "tokens": ["Sieh", "den", "Pfar\u00b7rer", ",", "lus\u00b7tent\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich verschanzen, sich vergraben", "tokens": ["Sich", "ver\u00b7schan\u00b7zen", ",", "sich", "ver\u00b7gra\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PRF", "VVINF", "$,", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter Heft und Foliant.", "tokens": ["Un\u00b7ter", "Heft", "und", "Fo\u00b7li\u00b7ant", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bl\u00e4ttern sieh ihn \u2014 nicken \u2014 sp\u00fcren \u2014", "tokens": ["Bl\u00e4t\u00b7tern", "sieh", "ihn", "ni\u00b7cken", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ganz versunken sitzen dann,", "tokens": ["Ganz", "ver\u00b7sun\u00b7ken", "sit\u00b7zen", "dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df mit einer Linie r\u00fchren", "tokens": ["Da\u00df", "mit", "ei\u00b7ner", "Li\u00b7nie", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Du das Buch magst und den Mann.", "tokens": ["Du", "das", "Buch", "magst", "und", "den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VMFIN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}