{"textgrid.poem.66791": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht den Korpsier stolzieren!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht den Korpsier stolzieren!", "tokens": ["Seht", "den", "Korp\u00b7sier", "stol\u00b7zie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Kolossal! Feudaler Schmi\u00df!", "tokens": ["Ko\u00b7los\u00b7sal", "!", "Feu\u00b7da\u00b7ler", "Sch\u00b7mi\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Was? Du wagst zu protestieren?", "tokens": ["Was", "?", "Du", "wagst", "zu", "pro\u00b7tes\u00b7tie\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steckt den Kerl in Bierverschi\u00df!", "tokens": ["Steckt", "den", "Kerl", "in", "Bier\u00b7ver\u00b7schi\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese Narbe auf der Stirne,", "tokens": ["Die\u00b7se", "Nar\u00b7be", "auf", "der", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ehrenfurche lang und breit,", "tokens": ["Eh\u00b7ren\u00b7fur\u00b7che", "lang", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jedem echten Burschenhirne", "tokens": ["Je\u00b7dem", "ech\u00b7ten", "Bur\u00b7schen\u00b7hir\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sinnbild strammer Schneidigkeit.", "tokens": ["Sinn\u00b7bild", "stram\u00b7mer", "Schnei\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch die Gassen auf und nieder", "tokens": ["Durch", "die", "Gas\u00b7sen", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Protzt der noble Studio:", "tokens": ["Protzt", "der", "nob\u00b7le", "Stu\u00b7dio", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Renommage Blick und Glieder", "tokens": ["Re\u00b7nom\u00b7ma\u00b7ge", "Blick", "und", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum Scheitel des Popo.", "tokens": ["Bis", "zum", "Schei\u00b7tel", "des", "Po\u00b7po", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses K\u00e4ppi, dieses B\u00e4ndchen,", "tokens": ["Die\u00b7ses", "K\u00e4p\u00b7pi", ",", "die\u00b7ses", "B\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese Pose, dieser Schick!", "tokens": ["Die\u00b7se", "Po\u00b7se", ",", "die\u00b7ser", "Schick", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ein repr\u00e4sentables Endchen", "tokens": ["Ein", "re\u00b7pr\u00e4\u00b7sen\u00b7tab\u00b7les", "End\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Haarzopf baumelt im Genick.", "tokens": ["Haar\u00b7zopf", "bau\u00b7melt", "im", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heil dir, Korpsbursch, sch\u00f6nste Bl\u00fcte", "tokens": ["Heil", "dir", ",", "Korps\u00b7bursch", ",", "sch\u00f6ns\u00b7te", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Au dem Riesenbusch der Zeit!", "tokens": ["Au", "dem", "Rie\u00b7sen\u00b7busch", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch mein trunkenes Gem\u00fcte", "tokens": ["Durch", "mein", "trun\u00b7ke\u00b7nes", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Duftet deine Herrlichkeit.", "tokens": ["Duf\u00b7tet", "dei\u00b7ne", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schmi\u00df \u2013 Verschi\u00df \u2013 Bierjunge \u2013 h\u00e4ngen!", "tokens": ["Sch\u00b7mi\u00df", "\u2013", "Ver\u00b7schi\u00df", "\u2013", "Bier\u00b7jun\u00b7ge", "\u2013", "h\u00e4n\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "VVIMP", "$(", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Abgestochen! Juden raus!", "tokens": ["Ab\u00b7ge\u00b7sto\u00b7chen", "!", "Ju\u00b7den", "raus", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Br\u00fcder, preist in Hochges\u00e4ngen", "tokens": ["Br\u00fc\u00b7der", ",", "preist", "in", "Hoch\u00b7ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das erhabne Herrscherhaus!", "tokens": ["Das", "er\u00b7hab\u00b7ne", "Herr\u00b7scher\u00b7haus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}