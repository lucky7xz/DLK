{"dta.poem.21686": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An die Zeitgenossen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vereidet keiner Schule, keiner Rotte", "tokens": ["Ver\u00b7ei\u00b7det", "kei\u00b7ner", "Schu\u00b7le", ",", "kei\u00b7ner", "Rot\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verkauft um schn\u00f6den Hohn und feiles Lob,", "tokens": ["Ver\u00b7kauft", "um", "schn\u00f6\u00b7den", "Hohn", "und", "fei\u00b7les", "Lob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gehorchend einzig dem gewalt'gen Gotte,", "tokens": ["Ge\u00b7hor\u00b7chend", "ein\u00b7zig", "dem", "ge\u00b7walt'\u00b7gen", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wagt' ich zu singen, was die Brust mir hob.", "tokens": ["Wagt'", "ich", "zu", "sin\u00b7gen", ",", "was", "die", "Brust", "mir", "hob", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Katarakte schoss den Felshang nieder;", "tokens": ["Die", "Ka\u00b7ta\u00b7rak\u00b7te", "schoss", "den", "Fels\u00b7hang", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Rauh klangen, herzlich doch, des J\u00fcnglings", "tokens": ["Rauh", "klan\u00b7gen", ",", "herz\u00b7lich", "doch", ",", "des", "J\u00fcng\u00b7lings"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADJD", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gez\u00fcndet durch das Heilige und Hohe,", "tokens": ["Ge\u00b7z\u00fcn\u00b7det", "durch", "das", "Hei\u00b7li\u00b7ge", "und", "Ho\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "KON", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entstoben Funken der verborgnen Glut;", "tokens": ["Ent\u00b7sto\u00b7ben", "Fun\u00b7ken", "der", "ver\u00b7borg\u00b7nen", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Schlechte nur, das Niedrige und Rohe", "tokens": ["Das", "Schlech\u00b7te", "nur", ",", "das", "Nied\u00b7ri\u00b7ge", "und", "Ro\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verschm\u00e4hte z\u00fcrnend die geweihte Wuth.", "tokens": ["Ver\u00b7schm\u00e4h\u00b7te", "z\u00fcr\u00b7nend", "die", "ge\u00b7weih\u00b7te", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sang die Liebe meiner Rosenjugend:", "tokens": ["Ich", "sang", "die", "Lie\u00b7be", "mei\u00b7ner", "Ro\u00b7sen\u00b7ju\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gott, die Natur, die Sch\u00f6nheit und\ndie Tugend.", "tokens": ["Gott", ",", "die", "Na\u00b7tur", ",", "die", "Sch\u00f6n\u00b7heit", "und", "die", "Tu\u00b7gend", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Doch andre Zeiten brachten andre Lehren,", "tokens": ["Doch", "and\u00b7re", "Zei\u00b7ten", "brach\u00b7ten", "and\u00b7re", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein neues Licht entblitzt der alten Nacht.", "tokens": ["Ein", "neu\u00b7es", "Licht", "ent\u00b7blitzt", "der", "al\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man heisst uns neue fremde G\u00f6tter ehren;", "tokens": ["Man", "heisst", "uns", "neu\u00b7e", "frem\u00b7de", "G\u00f6t\u00b7ter", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was heilig war den V\u00e4tern, wird verlacht.", "tokens": ["Was", "hei\u00b7lig", "war", "den", "V\u00e4\u00b7tern", ",", "wird", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "ART", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Euch, meines Liedes inhaltreiche Themen,", "tokens": ["Euch", ",", "mei\u00b7nes", "Lie\u00b7des", "in\u00b7hal\u00b7trei\u00b7che", "The\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verh\u00f6hnen sie als wesenlose Schemen.", "tokens": ["Ver\u00b7h\u00f6h\u00b7nen", "sie", "als", "we\u00b7sen\u00b7lo\u00b7se", "Sche\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u201elasst einmal doch die breitgetretnen", "tokens": ["\u201e", "lasst", "ein\u00b7mal", "doch", "die", "breit\u00b7ge\u00b7tret\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201everlockte Dichter! Thoren, die ihr seyd,", "tokens": ["\u201e", "ver\u00b7lock\u00b7te", "Dich\u00b7ter", "!", "Tho\u00b7ren", ",", "die", "ihr", "seyd", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201ewisst, nur Bedarf prosaischer Naturen", "tokens": ["\u201e", "wisst", ",", "nur", "Be\u00b7darf", "pro\u00b7sa\u00b7i\u00b7scher", "Na\u00b7tu\u00b7ren"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "ADV", "NN", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "\u201esind Gott, die Tugend und die Ewigkeit.", "tokens": ["\u201e", "sind", "Gott", ",", "die", "Tu\u00b7gend", "und", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201ees sch\u00f6pft aus reichern, aus den ", "tokens": ["\u201e", "es", "sch\u00f6pft", "aus", "rei\u00b7chern", ",", "aus", "den"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ADJA", "$,", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201eder Genius, der k\u00fchn sich selbst begriffen.", "tokens": ["\u201e", "der", "Ge\u00b7nius", ",", "der", "k\u00fchn", "sich", "selbst", "be\u00b7grif\u00b7fen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "ADJD", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "\u201everschm\u00e4hend, fremder Macht uns", "tokens": ["\u201e", "ver\u00b7schm\u00e4\u00b7hend", ",", "frem\u00b7der", "Macht", "uns"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVPP", "$,", "ADJA", "NN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201efrohlockend in dem heisserrungnen Licht,", "tokens": ["\u201e", "froh\u00b7lo\u00b7ckend", "in", "dem", "heis\u00b7ser\u00b7rung\u00b7nen", "Licht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201ethun wir Verzicht auf euer ewig Leben,", "tokens": ["\u201e", "thun", "wir", "Ver\u00b7zicht", "auf", "eu\u00b7er", "e\u00b7wig", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201eauf eure Tugend, euren Gott Verzicht.", "tokens": ["\u201e", "auf", "eu\u00b7re", "Tu\u00b7gend", ",", "eu\u00b7ren", "Gott", "Ver\u00b7zicht", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201euns, die wir gar in eignen Feuern gl\u00fchen,", "tokens": ["\u201e", "uns", ",", "die", "wir", "gar", "in", "eig\u00b7nen", "Feu\u00b7ern", "gl\u00fc\u00b7hen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u201erauscht der Krystallstrom \u00e4chter Poesieen.\u201c", "tokens": ["\u201e", "rauscht", "der", "Krys\u00b7tall\u00b7strom", "\u00e4ch\u00b7ter", "Poe\u00b7si\u00b7e\u00b7en", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "So schallen rechts und links der Meister", "tokens": ["So", "schal\u00b7len", "rechts", "und", "links", "der", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der J\u00fcnger Echo hallt sie wiehernd nach,", "tokens": ["Der", "J\u00fcn\u00b7ger", "E\u00b7cho", "hallt", "sie", "wie\u00b7hernd", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00fchsam windet sich durch Moor und", "tokens": ["Und", "m\u00fch\u00b7sam", "win\u00b7det", "sich", "durch", "Moor", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Dichtung dunkler schlammgetr\u00fcbter", "tokens": ["Der", "Dich\u00b7tung", "dunk\u00b7ler", "schlamm\u00b7ge\u00b7tr\u00fcb\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der \u00fcber Goldsand sonst, von Cynoglossen", "tokens": ["Der", "\u00fc\u00b7ber", "Gold\u00b7sand", "sonst", ",", "von", "Cy\u00b7nog\u00b7los\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "NN", "ADV", "$,", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Umduftet, \u00e4therklar dahingeflossen.", "tokens": ["Um\u00b7duf\u00b7tet", ",", "\u00e4t\u00b7her\u00b7klar", "da\u00b7hin\u00b7ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Zum neuen Glauben, zu den neuen", "tokens": ["Zum", "neu\u00b7en", "Glau\u00b7ben", ",", "zu", "den", "neu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich zu bekehren, bleibe fern von mir!", "tokens": ["Mich", "zu", "be\u00b7keh\u00b7ren", ",", "blei\u00b7be", "fern", "von", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ich gesungen hab, hab' ich gesungen.", "tokens": ["Was", "ich", "ge\u00b7sun\u00b7gen", "hab", ",", "hab'", "ich", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir haben auch den Geist des Herrn, auch", "tokens": ["Wir", "ha\u00b7ben", "auch", "den", "Geist", "des", "Herrn", ",", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "wir!", "tokens": ["wir", "!"], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Seyd ewig dann, ihr Genien meiner Jugend,", "tokens": ["Seyd", "e\u00b7wig", "dann", ",", "ihr", "Ge\u00b7ni\u00b7en", "mei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mein hohes Lied: ", "tokens": ["Mein", "ho\u00b7hes", "Lied", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}