{"textgrid.poem.53528": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Silvester", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im niedern Zimmer", "tokens": ["Im", "nie\u00b7dern", "Zim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "zieht sich der Pfeifenrauch in dicken, blauen Schwaden.", "tokens": ["zieht", "sich", "der", "Pfei\u00b7fen\u00b7rauch", "in", "di\u00b7cken", ",", "blau\u00b7en", "Schwa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Der Nachtsturm r\u00fcttelt an den Fensterladen;", "tokens": ["Der", "Nachts\u00b7turm", "r\u00fct\u00b7telt", "an", "den", "Fens\u00b7ter\u00b7la\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die brave Lampe leuchtet mir wie immer.", "tokens": ["die", "bra\u00b7ve", "Lam\u00b7pe", "leuch\u00b7tet", "mir", "wie", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "KOKOM", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie stets gl\u00fcht mir der rote Wein", "tokens": ["Wie", "stets", "gl\u00fcht", "mir", "der", "ro\u00b7te", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "im festen Glase mit dem Kaiserbilde;", "tokens": ["im", "fes\u00b7ten", "Gla\u00b7se", "mit", "dem", "Kai\u00b7ser\u00b7bil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ein stiller Wein \u2013 er mundet mir so milde \u2013", "tokens": ["ein", "stil\u00b7ler", "Wein", "\u2013", "er", "mun\u00b7det", "mir", "so", "mil\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ich tr\u00e4um ins Glas \u2013 was spiegelt sich darein?", "tokens": ["ich", "tr\u00e4um", "ins", "Glas", "\u2013", "was", "spie\u00b7gelt", "sich", "da\u00b7rein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$(", "PWS", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vier lange Jahre.", "tokens": ["Vier", "lan\u00b7ge", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Es hie\u00df sich immer wieder, wieder ducken", "tokens": ["Es", "hie\u00df", "sich", "im\u00b7mer", "wie\u00b7der", ",", "wie\u00b7der", "du\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und schweigen und herunterschlucken.", "tokens": ["und", "schwei\u00b7gen", "und", "her\u00b7un\u00b7ter\u00b7schlu\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mensch war Material und Heeresware.", "tokens": ["Der", "Mensch", "war", "Ma\u00b7te\u00b7ri\u00b7al", "und", "Hee\u00b7res\u00b7wa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Das ist vorbei.", "tokens": ["Das", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was ist uns nun geblieben?", "tokens": ["Was", "ist", "uns", "nun", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo ist das Deutschland, das wir ewig lieben?", "tokens": ["Wo", "ist", "das", "Deutschland", ",", "das", "wir", "e\u00b7wig", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wof\u00fcr die Plackerei?", "tokens": ["Wo\u00b7f\u00fcr", "die", "Pla\u00b7cke\u00b7rei", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "F\u00fcr nichts.", "tokens": ["F\u00fcr", "nichts", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PIS", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ich tue einen Zug \u2013 die Pfeife knastert \u2013", "tokens": ["Ich", "tue", "ei\u00b7nen", "Zug", "\u2013", "die", "Pfei\u00b7fe", "knas\u00b7tert", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was hat man uns gebetet und gepastert \u2013", "tokens": ["Was", "hat", "man", "uns", "ge\u00b7be\u00b7tet", "und", "ge\u00b7pas\u00b7tert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "PPER", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tag des Gerichts!", "tokens": ["Tag", "des", "Ge\u00b7richts", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Und wi\u00dft ihr, wer uns also traf?", "tokens": ["Und", "wi\u00dft", "ihr", ",", "wer", "uns", "al\u00b7so", "traf", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Koksbaron und der Monokeltr\u00e4ger,", "tokens": ["Der", "Koks\u00b7ba\u00b7ron", "und", "der", "Mo\u00b7no\u00b7kel\u00b7tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das B\u00fcrgerlamm und der Karrierej\u00e4ger \u2013", "tokens": ["das", "B\u00fcr\u00b7ger\u00b7lamm", "und", "der", "Kar\u00b7rie\u00b7re\u00b7j\u00e4\u00b7ger", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ihr lagt im Schlaf.", "tokens": ["ihr", "lagt", "im", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "So wacht heut auf!", "tokens": ["So", "wacht", "heut", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wir trugen unser Kreuz und jene ihre Orden \u2013", "tokens": ["Wir", "tru\u00b7gen", "un\u00b7ser", "Kreuz", "und", "je\u00b7ne", "ih\u00b7re", "Or\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PDS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wir sind gesto\u00dfen und getreten worden:", "tokens": ["wir", "sind", "ge\u00b7sto\u00b7\u00dfen", "und", "ge\u00b7tre\u00b7ten", "wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Muschkot, versauf!", "tokens": ["Muschkot", ",", "ver\u00b7sauf", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Verge\u00dft ihr das?", "tokens": ["Ver\u00b7ge\u00dft", "ihr", "das", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Denkt stets daran, wie jene Alten sungen!", "tokens": ["Denkt", "stets", "da\u00b7ran", ",", "wie", "je\u00b7ne", "Al\u00b7ten", "sun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PAV", "$,", "PWAV", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber komm euch in Erinnerungen", "tokens": ["Ich", "a\u00b7ber", "komm", "euch", "in", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ein volles Glas \u2013!", "tokens": ["ein", "vol\u00b7les", "Glas", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}