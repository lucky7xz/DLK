{"textgrid.poem.42173": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Brigitte B.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junges M\u00e4dchen kam nach Baden,", "tokens": ["Ein", "jun\u00b7ges", "M\u00e4d\u00b7chen", "kam", "nach", "Ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Brigitte B. war sie genannt,", "tokens": ["Bri\u00b7git\u00b7te", "B.", "war", "sie", "ge\u00b7nannt", ","], "token_info": ["word", "abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Fand Stellung dort in einem Laden,", "tokens": ["Fand", "Stel\u00b7lung", "dort", "in", "ei\u00b7nem", "La\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sie gut angeschrieben stand.", "tokens": ["Wo", "sie", "gut", "an\u00b7ge\u00b7schrie\u00b7ben", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Dame, schon ein wenig \u00e4lter,", "tokens": ["Die", "Da\u00b7me", ",", "schon", "ein", "we\u00b7nig", "\u00e4l\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War dem Gesch\u00e4fte zugetan,", "tokens": ["War", "dem", "Ge\u00b7sch\u00e4f\u00b7te", "zu\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herr ein h\u00f6herer Angestellter", "tokens": ["Der", "Herr", "ein", "h\u00f6\u00b7he\u00b7rer", "An\u00b7ge\u00b7stell\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der k\u00f6niglichen Eisenbahn.", "tokens": ["Der", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Ei\u00b7sen\u00b7bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Dame sagt nun eines Tages,", "tokens": ["Die", "Da\u00b7me", "sagt", "nun", "ei\u00b7nes", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie man zur Nacht gegessen hat:", "tokens": ["Wie", "man", "zur", "Nacht", "ge\u00b7ges\u00b7sen", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbnimm dies Paket, mein Kind, und trag es", "tokens": ["\u00bb", "nimm", "dies", "Pa\u00b7ket", ",", "mein", "Kind", ",", "und", "trag", "es"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "PDS", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Zu der Baronin vor der Stadt.\u00ab", "tokens": ["Zu", "der", "Ba\u00b7ro\u00b7nin", "vor", "der", "Stadt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf diesem Wege traf Brigitte", "tokens": ["Auf", "die\u00b7sem", "We\u00b7ge", "traf", "Bri\u00b7git\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jedoch ein Individium,", "tokens": ["Je\u00b7doch", "ein", "In\u00b7di\u00b7vi\u00b7di\u00b7um", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das hat an sie nur eine Bitte,", "tokens": ["Das", "hat", "an", "sie", "nur", "ei\u00b7ne", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn nicht, dann bringe er sich um.", "tokens": ["Wenn", "nicht", ",", "dann", "brin\u00b7ge", "er", "sich", "um", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Brigitte, v\u00f6llig unerfahren,", "tokens": ["Bri\u00b7git\u00b7te", ",", "v\u00f6l\u00b7lig", "un\u00b7er\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gab sich ihm mehr aus Mitleid hin.", "tokens": ["Gab", "sich", "ihm", "mehr", "aus", "Mit\u00b7leid", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drauf ging er fort mit ihren Waren", "tokens": ["Drauf", "ging", "er", "fort", "mit", "ih\u00b7ren", "Wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lie\u00df sie in der Lage drin.", "tokens": ["Und", "lie\u00df", "sie", "in", "der", "La\u00b7ge", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie konnt es anfangs gar nicht fassen,", "tokens": ["Sie", "konnt", "es", "an\u00b7fangs", "gar", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann lief sie heulend und gestand,", "tokens": ["Dann", "lief", "sie", "heu\u00b7lend", "und", "ge\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie sich hat verf\u00fchren lassen,", "tokens": ["Da\u00df", "sie", "sich", "hat", "ver\u00b7f\u00fch\u00b7ren", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VAFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was die Madam begreiflich fand.", "tokens": ["Was", "die", "Ma\u00b7dam", "be\u00b7greif\u00b7lich", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df aber dabei die Tourn\u00fcre", "tokens": ["Da\u00df", "a\u00b7ber", "da\u00b7bei", "die", "Tour\u00b7n\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr die Baronin vor der Stadt", "tokens": ["F\u00fcr", "die", "Ba\u00b7ro\u00b7nin", "vor", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gestohlen worden sei, das schn\u00fcre", "tokens": ["Ge\u00b7stoh\u00b7len", "wor\u00b7den", "sei", ",", "das", "schn\u00fc\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAPP", "VAFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz ihr ab, sie hab sie satt.", "tokens": ["Das", "Herz", "ihr", "ab", ",", "sie", "hab", "sie", "satt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Brigitte warf sich vor ihr nieder,", "tokens": ["Bri\u00b7git\u00b7te", "warf", "sich", "vor", "ihr", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sei gewi\u00df nicht mehr so dumm;", "tokens": ["Sie", "sei", "ge\u00b7wi\u00df", "nicht", "mehr", "so", "dumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Abend aber schlief sie wieder", "tokens": ["Den", "A\u00b7bend", "a\u00b7ber", "schlief", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei ihrem Individium.", "tokens": ["Bei", "ih\u00b7rem", "In\u00b7di\u00b7vi\u00b7di\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und als die Herrschaft dann um Pfingsten", "tokens": ["Und", "als", "die", "Herr\u00b7schaft", "dann", "um", "Pfings\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ausflog mit dem Gesangverein,", "tokens": ["Aus\u00b7flog", "mit", "dem", "Ge\u00b7sang\u00b7ver\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lud sie ihn ohne die geringsten", "tokens": ["Lud", "sie", "ihn", "oh\u00b7ne", "die", "ge\u00b7rings\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bedenken abends zu sich ein.", "tokens": ["Be\u00b7den\u00b7ken", "a\u00b7bends", "zu", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sofort lie\u00df er sich alles zeigen,", "tokens": ["So\u00b7fort", "lie\u00df", "er", "sich", "al\u00b7les", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schreibtisch und den Kassenschrank,", "tokens": ["Den", "Schreib\u00b7tisch", "und", "den", "Kas\u00b7sen\u00b7schrank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Macht die Papiere sich zu eigen", "tokens": ["Macht", "die", "Pa\u00b7pie\u00b7re", "sich", "zu", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PRF", "PTKA", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und zollt ihr nicht mal mehr den Dank.", "tokens": ["Und", "zollt", "ihr", "nicht", "mal", "mehr", "den", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Brigitte, als sie nun gesehen,", "tokens": ["Bri\u00b7git\u00b7te", ",", "als", "sie", "nun", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ihr Geliebter angericht',", "tokens": ["Was", "ihr", "Ge\u00b7lieb\u00b7ter", "an\u00b7ge\u00b7richt'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entwich auf unh\u00f6rbaren Zehen", "tokens": ["Ent\u00b7wich", "auf", "un\u00b7h\u00f6r\u00b7ba\u00b7ren", "Ze\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Ehepaar aus dem Gesicht.", "tokens": ["Dem", "E\u00b7he\u00b7paar", "aus", "dem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Vorgestern hat man sie gefangen,", "tokens": ["Vor\u00b7ges\u00b7tern", "hat", "man", "sie", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es l\u00e4\u00dft sich nicht erz\u00e4hlen, wo;", "tokens": ["Es", "l\u00e4\u00dft", "sich", "nicht", "er\u00b7z\u00e4h\u00b7len", ",", "wo", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem J\u00fcngling, der die Tat begangen,", "tokens": ["Dem", "J\u00fcng\u00b7ling", ",", "der", "die", "Tat", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem ging es gestern ebenso.", "tokens": ["Dem", "ging", "es", "ge\u00b7stern", "e\u00b7ben\u00b7so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}}}}