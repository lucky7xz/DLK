{"dta.poem.2845": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wen unerwartet Gl\u00fcck mit Unma\u00df \u00fcbersch\u00fcttet,", "tokens": ["Wen", "un\u00b7er\u00b7war\u00b7tet", "Gl\u00fcck", "mit", "Un\u00b7ma\u00df", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gef\u00f6rdert wird dadurch sein Heil nicht, nur zerr\u00fcttet;", "tokens": ["Ge\u00b7f\u00f6r\u00b7dert", "wird", "da\u00b7durch", "sein", "Heil", "nicht", ",", "nur", "zer\u00b7r\u00fct\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PAV", "PPOSAT", "NN", "PTKNEG", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie \u00fcberstr\u00f6mt mit Oel, statt m\u00e4\u00dfig angefrischt,", "tokens": ["Wie", "\u00fc\u00b7ber\u00b7str\u00f6mt", "mit", "O\u00b7el", ",", "statt", "m\u00e4\u00b7\u00dfig", "an\u00b7ge\u00b7frischt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "NN", "$,", "KOUI", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "An ihrer Lebensf\u00fcll' oft eine Lamp' erlischt.", "tokens": ["An", "ih\u00b7rer", "Le\u00b7bens\u00b7f\u00fcll'", "oft", "ei\u00b7ne", "Lamp'", "er\u00b7lischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "VVP