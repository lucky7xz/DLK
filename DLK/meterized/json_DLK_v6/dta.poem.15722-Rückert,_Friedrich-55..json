{"dta.poem.15722": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wiesehr auch er f\u00fcrs Weib Lieb' und Verehrung hegt,", "tokens": ["Wie\u00b7sehr", "auch", "er", "f\u00fcrs", "Weib", "Lieb'", "und", "Ver\u00b7eh\u00b7rung", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPRART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Der Mann hat immer sich den Vorzug beigelegt.", "tokens": ["Der", "Mann", "hat", "im\u00b7mer", "sich", "den", "Vor\u00b7zug", "bei\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als Erstgeschaffner, als Alleingeschaffner hat", "tokens": ["Als", "Erst\u00b7ge\u00b7schaff\u00b7ner", ",", "als", "Al\u00b7lein\u00b7ge\u00b7schaff\u00b7ner", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "KOUS", "PIS", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sich gef\u00fchlt, aus dem das Weib hervor nur trat.", "tokens": ["Er", "sich", "ge\u00b7f\u00fchlt", ",", "aus", "dem", "das", "Weib", "her\u00b7vor", "nur", "trat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVPP", "$,", "APPR", "PRELS", "ART", "NN", "PTKVZ", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er wu\u00dft' in Staat und Rath den Vorrang zu gewinnen;", "tokens": ["Er", "wu\u00dft'", "in", "Staat", "und", "Rath", "den", "Vor\u00b7rang", "zu", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch hatten Menschen auch, wie Bienen, K\u00f6niginnen.", "tokens": ["Doch", "hat\u00b7ten", "Men\u00b7schen", "auch", ",", "wie", "Bie\u00b7nen", ",", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADV", "$,", "PWAV", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und dienen siehest du im stillen Reich der Pflanze", "tokens": ["Und", "die\u00b7nen", "sie\u00b7hest", "du", "im", "stil\u00b7len", "Reich", "der", "Pflan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel M\u00e4nner einem Weib zu Liebeshof und Kranze.", "tokens": ["Viel", "M\u00e4n\u00b7ner", "ei\u00b7nem", "Weib", "zu", "Lie\u00b7bes\u00b7hof", "und", "Kran\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch viel Insekten sind gefl\u00fcgelt nur, wenn m\u00e4nnlich,", "tokens": ["Doch", "viel", "In\u00b7sek\u00b7ten", "sind", "ge\u00b7fl\u00fc\u00b7gelt", "nur", ",", "wenn", "m\u00e4nn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "ADV", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Vogelm\u00e4nnchen an Gesang und Schmuck erkennlich.", "tokens": ["Und", "Vo\u00b7gel\u00b7m\u00e4nn\u00b7chen", "an", "Ge\u00b7sang", "und", "Schmuck", "er\u00b7kenn\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Im niedersten Gebiet der Thierwelt herrscht ein dritter", "tokens": ["Im", "nie\u00b7ders\u00b7ten", "Ge\u00b7biet", "der", "Thier\u00b7welt", "herrscht", "ein", "drit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stand \u00fcber Mann und Weib, der zweigeschlecht'ge Zwitter.", "tokens": ["Stand", "\u00fc\u00b7ber", "Mann", "und", "Weib", ",", "der", "zwei\u00b7ge\u00b7schlecht'\u00b7ge", "Zwit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Weibchen, in sich selbst befruchtet, m\u00f6gen hecken;", "tokens": ["Die", "Weib\u00b7chen", ",", "in", "sich", "selbst", "be\u00b7fruch\u00b7tet", ",", "m\u00f6\u00b7gen", "he\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRF", "ADV", "VVPP", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die M\u00e4nnchen dienen nur, die Keime zu erwecken.", "tokens": ["Die", "M\u00e4nn\u00b7chen", "die\u00b7nen", "nur", ",", "die", "Kei\u00b7me", "zu", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "So k\u00f6nnt' ein Menschenweib geb\u00e4ren ohne Mann,", "tokens": ["So", "k\u00f6nnt'", "ein", "Men\u00b7schen\u00b7weib", "ge\u00b7b\u00e4\u00b7ren", "oh\u00b7ne", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da aus sich selbst nur Zeus die Tochter zeugen kann.", "tokens": ["Da", "aus", "sich", "selbst", "nur", "Zeus", "die", "Toch\u00b7ter", "zeu\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "ADV", "NE", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die geistige Geburt ist eignes Mannesrecht;", "tokens": ["Die", "geis\u00b7ti\u00b7ge", "Ge\u00b7burt", "ist", "eig\u00b7nes", "Man\u00b7nes\u00b7recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mann ist die Person, das Weib ist das Geschlecht.", "tokens": ["Der", "Mann", "ist", "die", "Per\u00b7son", ",", "das", "Weib", "ist", "das", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "Und die Pers\u00f6nlichkeit, die an sich selbst ihm fehlt,", "tokens": ["Und", "die", "Per\u00b7s\u00f6n\u00b7lich\u00b7keit", ",", "die", "an", "sich", "selbst", "ihm", "fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "PRF", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewinnt das Weib, indem sie sich dem Mann verm\u00e4hlt.", "tokens": ["Ge\u00b7winnt", "das", "Weib", ",", "in\u00b7dem", "sie", "sich", "dem", "Mann", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}