{"dta.poem.2869": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wen man gern anerkennt, der wird gern anerkennen;", "tokens": ["Wen", "man", "gern", "an\u00b7er\u00b7kennt", ",", "der", "wird", "gern", "an\u00b7er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVFIN", "$,", "PRELS", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wem man das Seine g\u00f6nnt, mag Andern Ihres g\u00f6nnen.", "tokens": ["Wem", "man", "das", "Sei\u00b7ne", "g\u00f6nnt", ",", "mag", "An\u00b7dern", "Ih\u00b7res", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ART", "PPOSAT", "VVFIN", "$,", "VMFIN", "ADJA", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn ihr dagegen mir mein Recht nicht wollt zugeben,", "tokens": ["Wenn", "ihr", "da\u00b7ge\u00b7gen", "mir", "mein", "Recht", "nicht", "wollt", "zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PPER", "PPOSAT", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So leugn' ich eures ab, und streit' euch ab das Leben. \u2014", "tokens": ["So", "leugn'", "ich", "eu\u00b7res", "ab", ",", "und", "streit'", "euch", "ab", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von diesem Sinne bin ich selber zwar entfernt,", "tokens": ["Von", "die\u00b7sem", "Sin\u00b7ne", "bin", "ich", "sel\u00b7ber", "zwar", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ihn begreifen hab' ich leider wohl gelernt.", "tokens": ["Doch", "ihn", "be\u00b7grei\u00b7fen", "hab'", "ich", "lei\u00b7der", "wohl", "ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}