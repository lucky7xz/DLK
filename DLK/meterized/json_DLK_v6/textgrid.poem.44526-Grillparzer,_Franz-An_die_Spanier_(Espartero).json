{"textgrid.poem.44526": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An die Spanier (Espartero)", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wohl, fahrt hin, des Ankers seid ihr los,", "tokens": ["Nun", "wohl", ",", "fahrt", "hin", ",", "des", "An\u00b7kers", "seid", "ihr", "los", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war ein plumpes Ding auch, schwer von Eisen,", "tokens": ["Es", "war", "ein", "plum\u00b7pes", "Ding", "auch", ",", "schwer", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daran ein Seil, nur etwa h\u00e4nfen blo\u00df,", "tokens": ["Da\u00b7ran", "ein", "Seil", ",", "nur", "et\u00b7wa", "h\u00e4n\u00b7fen", "blo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "ADV", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das f\u00f6rdert nicht, zur\u00fcckh\u00e4lt, die da reisen.", "tokens": ["Das", "f\u00f6r\u00b7dert", "nicht", ",", "zu\u00b7r\u00fcck\u00b7h\u00e4lt", ",", "die", "da", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "$,", "VVFIN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch hielt er euch, umringt von Klippen, fest,", "tokens": ["Doch", "hielt", "er", "euch", ",", "um\u00b7ringt", "von", "Klip\u00b7pen", ",", "fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "VVPP", "APPR", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da alle Seiten schon des Schiffes krachten,", "tokens": ["Da", "al\u00b7le", "Sei\u00b7ten", "schon", "des", "Schif\u00b7fes", "krach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und auf des Strandrechts kargen \u00dcberrest,", "tokens": ["Und", "auf", "des", "Strand\u00b7rechts", "kar\u00b7gen", "\u00dc\u00b7ber\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Nachbarn ihre H\u00e4nde fertig machten.", "tokens": ["Die", "Nach\u00b7barn", "ih\u00b7re", "H\u00e4n\u00b7de", "fer\u00b7tig", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun gehts nach vorw\u00e4rts und ein jeder r\u00e4t,", "tokens": ["Nun", "gehts", "nach", "vor\u00b7w\u00e4rts", "und", "ein", "je\u00b7der", "r\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "KON", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Kurs bestimmt das Schiffsvolk der Verdecke,", "tokens": ["Den", "Kurs", "be\u00b7stimmt", "das", "Schiffs\u00b7volk", "der", "Ver\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Segel schwellen und der Wimpel weht,", "tokens": ["Die", "Se\u00b7gel", "schwel\u00b7len", "und", "der", "Wim\u00b7pel", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Aug mi\u00dft froh die r\u00fcckgebliebne Strecke.", "tokens": ["Das", "Aug", "mi\u00dft", "froh", "die", "r\u00fcck\u00b7ge\u00b7blieb\u00b7ne", "Stre\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Allein bedenkt, der Sturm bl\u00e4st ohne Ruhn,", "tokens": ["Al\u00b7lein", "be\u00b7denkt", ",", "der", "Sturm", "bl\u00e4st", "oh\u00b7ne", "Ruhn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gekreuzt aus jedem Strich der Windesrose,", "tokens": ["Ge\u00b7kreuzt", "aus", "je\u00b7dem", "Strich", "der", "Win\u00b7des\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Circe und \u00c4olus, Proteus, Neptun,", "tokens": ["Cir\u00b7ce", "und", "\u00c4\u00b7o\u00b7lus", ",", "Pro\u00b7teus", ",", "Nep\u00b7tun", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "NE", "$,", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sie streiten sich um eure k\u00fcnftgen Lose.", "tokens": ["Sie", "strei\u00b7ten", "sich", "um", "eu\u00b7re", "k\u00fcnft\u00b7gen", "Lo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da ist nicht Wahl, als, wenn die Kraft erlahmt,", "tokens": ["Da", "ist", "nicht", "Wahl", ",", "als", ",", "wenn", "die", "Kraft", "er\u00b7lahmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "$,", "KOUS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu sterben wie es ansteht wackern Streitern", "tokens": ["Zu", "ster\u00b7ben", "wie", "es", "an\u00b7steht", "wa\u00b7ckern", "Strei\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KOKOM", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und an den Klippenreihn, woher ihr kamt,", "tokens": ["Und", "an", "den", "Klip\u00b7pen\u00b7reihn", ",", "wo\u00b7her", "ihr", "kamt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit ungeschw\u00e4chtem M\u00e4nnermut zu scheitern.", "tokens": ["Mit", "un\u00b7ge\u00b7schw\u00e4ch\u00b7tem", "M\u00e4n\u00b7ner\u00b7mut", "zu", "schei\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wenn nicht, da\u00df wieder ihr die Sandbank sucht,", "tokens": ["Wenn", "nicht", ",", "da\u00df", "wie\u00b7der", "ihr", "die", "Sand\u00b7bank", "sucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "KOUS", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf der ihr durch Jahrhunderte gestanden,", "tokens": ["Auf", "der", "ihr", "durch", "Jahr\u00b7hun\u00b7der\u00b7te", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Um in der schlammerf\u00fcllten eklen Bucht", "tokens": ["Um", "in", "der", "schlam\u00b7mer\u00b7f\u00fcll\u00b7ten", "ek\u00b7len", "Bucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr weitere Jahrhunderte zu stranden.", "tokens": ["F\u00fcr", "wei\u00b7te\u00b7re", "Jahr\u00b7hun\u00b7der\u00b7te", "zu", "stran\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}