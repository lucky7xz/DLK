{"dta.poem.6475": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rachdem die Lufft bewegt wird und ger\u00fchret;", "tokens": ["Rach\u00b7dem", "die", "Lufft", "be\u00b7wegt", "wird", "und", "ge\u00b7r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So w\u00fcrcket auch der Ton. Er ist ver\u00e4nderlich,", "tokens": ["So", "w\u00fcr\u00b7cket", "auch", "der", "Ton", ".", "Er", "ist", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf ungez\u00e4hlte Art erzeigt er sich.", "tokens": ["Auf", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Art", "er\u00b7zeigt", "er", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Lufft, woferne man sie langsam regt, gebieret", "tokens": ["Die", "Lufft", ",", "wo\u00b7fer\u00b7ne", "man", "sie", "lang\u00b7sam", "regt", ",", "ge\u00b7bie\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Stets einen tieffen Ton. Beweget sie sich schnell;", "tokens": ["Stets", "ei\u00b7nen", "tief\u00b7fen", "Ton", ".", "Be\u00b7we\u00b7get", "sie", "sich", "schnell", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wird derselbe scharff und hell.", "tokens": ["So", "wird", "der\u00b7sel\u00b7be", "scharff", "und", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}