{"textgrid.poem.61778": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Abels Ende", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Abel hatt' einen schweren Traum,", "tokens": ["K\u00f6\u00b7nig", "A\u00b7bel", "hatt'", "ei\u00b7nen", "schwe\u00b7ren", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nicht l\u00e4nger l\u00e4\u00dft's ihn schlafen,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "l\u00e4\u00dft's", "ihn", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er springt vom Bett und tritt hinaus", "tokens": ["Er", "springt", "vom", "Bett", "und", "tritt", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum S\u00f6ller \u00fcberm Hafen.", "tokens": ["Zum", "S\u00f6l\u00b7ler", "\u00fc\u00b7berm", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es scheint der Mond, es rauscht die Schlei", "tokens": ["Es", "scheint", "der", "Mond", ",", "es", "rauscht", "die", "Schlei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dumpfem Wellenschlage;", "tokens": ["Mit", "dum\u00b7pfem", "Wel\u00b7len\u00b7schla\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig starrt hinab, er denkt", "tokens": ["Der", "K\u00f6\u00b7nig", "starrt", "hin\u00b7ab", ",", "er", "denkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schuld vergangner Tage.", "tokens": ["Der", "Schuld", "ver\u00b7gang\u00b7ner", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wie es Eins vom Dome schl\u00e4gt,", "tokens": ["Und", "wie", "es", "Eins", "vom", "Do\u00b7me", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt unten auf den Wogen", "tokens": ["Kommt", "un\u00b7ten", "auf", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gespenstisch aus dem Nebelduft", "tokens": ["Ge\u00b7spens\u00b7tisch", "aus", "dem", "Ne\u00b7bel\u00b7duft"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein stummer Kahn gezogen.", "tokens": ["Ein", "stum\u00b7mer", "Kahn", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er schwebt heran im wei\u00dfen Licht,", "tokens": ["Er", "schwebt", "he\u00b7ran", "im", "wei\u00b7\u00dfen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unh\u00f6rbar geht das Ruder \u2013", "tokens": ["Un\u00b7h\u00f6r\u00b7bar", "geht", "das", "Ru\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbhilf Gott! Der dort am Steuer sitzt,", "tokens": ["\u00bb", "hilf", "Gott", "!", "Der", "dort", "am", "Steu\u00b7er", "sitzt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "NN", "$.", "ART", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist mein toter Bruder!", "tokens": ["Das", "ist", "mein", "to\u00b7ter", "Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Langsam an seinem Halse quillt", "tokens": ["Lang\u00b7sam", "an", "sei\u00b7nem", "Hal\u00b7se", "quillt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das Blut aus breiter Wunde,", "tokens": ["Das", "Blut", "aus", "brei\u00b7ter", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In seinem Haar noch klebt das Schilf,", "tokens": ["In", "sei\u00b7nem", "Haar", "noch", "klebt", "das", "Schilf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schlamm vom Stromesgrunde.", "tokens": ["Der", "Schlamm", "vom", "Stro\u00b7mes\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er stiert mich an mit glas'gem Blick,", "tokens": ["Er", "stiert", "mich", "an", "mit", "glas'\u00b7gem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Blut gerinnt vor Grauen;", "tokens": ["Mein", "Blut", "ge\u00b7rinnt", "vor", "Grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hebt den Arm und winkt und winkt \u2013", "tokens": ["Er", "hebt", "den", "Arm", "und", "winkt", "und", "winkt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weh mir, ich kann's nicht schauen!\u00ab", "tokens": ["Weh", "mir", ",", "ich", "kann's", "nicht", "schau\u00b7en", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Herr Abel st\u00fcrzt zur\u00fcck ins Schlo\u00df:", "tokens": ["Herr", "A\u00b7bel", "st\u00fcrzt", "zu\u00b7r\u00fcck", "ins", "Schlo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbla\u00dft mir den Bischof wecken!\u00ab", "tokens": ["\u00bb", "la\u00dft", "mir", "den", "Bi\u00b7schof", "we\u00b7cken", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er keucht's und birgt sein fiebernd Haupt", "tokens": ["Er", "keucht's", "und", "birgt", "sein", "fie\u00b7bernd", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seines Lagers Decken.", "tokens": ["In", "sei\u00b7nes", "La\u00b7gers", "De\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbfluch dir, Fluch dir, unselig Gold,", "tokens": ["\u00bb", "fluch", "dir", ",", "Fluch", "dir", ",", "un\u00b7se\u00b7lig", "Gold", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "NN", "PPER", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Du K\u00f6nigskron' im Norden!", "tokens": ["Du", "K\u00f6\u00b7nigs\u00b7kron'", "im", "Nor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl hei\u00df ich Abel, doch um dich", "tokens": ["Wohl", "hei\u00df", "ich", "A\u00b7bel", ",", "doch", "um", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kain bin ich worden.", "tokens": ["Zum", "Kain", "bin", "ich", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VAPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Fluch, Purpur, dir! Du glei\u00dftest mir", "tokens": ["Fluch", ",", "Pur\u00b7pur", ",", "dir", "!", "Du", "glei\u00df\u00b7test", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So zaubrisch vor den Sinnen;", "tokens": ["So", "zaub\u00b7risch", "vor", "den", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun sengst du mich wie Feuersglut,", "tokens": ["Nun", "sengst", "du", "mich", "wie", "Feu\u00b7ers\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Qual mu\u00df ich von hinnen.\u00ab", "tokens": ["In", "Qual", "mu\u00df", "ich", "von", "hin\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "APPR", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Was pocht und h\u00e4mmert in der Wand?", "tokens": ["Was", "pocht", "und", "h\u00e4m\u00b7mert", "in", "der", "Wand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das kommt vom Totenwurme.", "tokens": ["Das", "kommt", "vom", "To\u00b7ten\u00b7wur\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was klirrt und klingt? Das Fenster springt", "tokens": ["Was", "klirrt", "und", "klingt", "?", "Das", "Fens\u00b7ter", "springt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weitklaffend auf im Sturme.", "tokens": ["Weit\u00b7klaf\u00b7fend", "auf", "im", "Stur\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und sieh, zwei schwarze Raben ziehn", "tokens": ["Und", "sieh", ",", "zwei", "schwar\u00b7ze", "Ra\u00b7ben", "ziehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "CARD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herein mit heiserem Schreien,", "tokens": ["Her\u00b7ein", "mit", "hei\u00b7se\u00b7rem", "Schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie flattern kreischend um das Bett", "tokens": ["Sie", "flat\u00b7tern", "krei\u00b7schend", "um", "das", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fliegen hinaus zu dreien.", "tokens": ["Und", "flie\u00b7gen", "hin\u00b7aus", "zu", "drei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Der Bischof kommt, er schl\u00e4gt ein Kreuz,", "tokens": ["Der", "Bi\u00b7schof", "kommt", ",", "er", "schl\u00e4gt", "ein", "Kreuz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Raben sieht er fliegen,", "tokens": ["Die", "Ra\u00b7ben", "sieht", "er", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sieht den K\u00f6nig starr und tot", "tokens": ["Er", "sieht", "den", "K\u00f6\u00b7nig", "starr", "und", "tot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seinem Purpur liegen.", "tokens": ["Auf", "sei\u00b7nem", "Pur\u00b7pur", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}