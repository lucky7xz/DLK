{"textgrid.poem.49083": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "41. Shakespeare und Michel Angelo", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Shakespeare und Michel Angelo scheinen h\u00e4ufig dem Sch\u00fcler", "tokens": ["Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "und", "Mi\u00b7chel", "An\u00b7ge\u00b7lo", "schei\u00b7nen", "h\u00e4u\u00b7fig", "dem", "Sch\u00fc\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "NE", "VVFIN", "ADJD", "ART", "NN"], "meter": "+--+--+-+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Leicht erreichbar, doch schwer \u00e4chten Meistern der Kunst.", "tokens": ["Leicht", "er\u00b7reich\u00b7bar", ",", "doch", "schwer", "\u00e4ch\u00b7ten", "Meis\u00b7tern", "der", "Kunst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADV", "ADJD", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Jener beachtet die \u00e4u\u00df're Form nur, doch diese erw\u00e4gen,", "tokens": ["Je\u00b7ner", "be\u00b7ach\u00b7tet", "die", "\u00e4u\u00df'\u00b7re", "Form", "nur", ",", "doch", "die\u00b7se", "er\u00b7w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,", "KON", "PDS", "VVFIN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Dringend in's Inn're zugleich, tiefer liegenden Geist.", "tokens": ["Drin\u00b7gend", "in's", "Inn'\u00b7re", "zu\u00b7gleich", ",", "tie\u00b7fer", "lie\u00b7gen\u00b7den", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}}}}