{"textgrid.poem.58705": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede des \u00dcbersetzers", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ungesuchter Trieb kam neulich in mich dringen", "tokens": ["Ein", "un\u00b7ge\u00b7such\u00b7ter", "Trieb", "kam", "neu\u00b7lich", "in", "mich", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur ungelegnen Zeit, weil Kopf und Augen bl\u00f6d,", "tokens": ["Zur", "un\u00b7ge\u00b7leg\u00b7nen", "Zeit", ",", "weil", "Kopf", "und", "Au\u00b7gen", "bl\u00f6d", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hier Frau Guyon schreibt, sollt' ich ins Deutsche bringen;", "tokens": ["Was", "hier", "Frau", "Gu\u00b7yon", "schreibt", ",", "sollt'", "ich", "ins", "Deut\u00b7sche", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "NE", "VVFIN", "$,", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich tat, was ich gekonnt, ich konnte, was hier steht.", "tokens": ["Ich", "tat", ",", "was", "ich", "ge\u00b7konnt", ",", "ich", "konn\u00b7te", ",", "was", "hier", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "ADJD", "$,", "PPER", "VMFIN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob mein Verdeutschen treu, kann jeder selber sehen,", "tokens": ["Ob", "mein", "Ver\u00b7deut\u00b7schen", "treu", ",", "kann", "je\u00b7der", "sel\u00b7ber", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "$,", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn er Franz\u00f6sisch und der Liebe Sprache kann,", "tokens": ["Wenn", "er", "Fran\u00b7z\u00f6\u00b7sisch", "und", "der", "Lie\u00b7be", "Spra\u00b7che", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "ART", "NN", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein wenig mu\u00df er auch die Reimenkunst verstehen;", "tokens": ["Ein", "we\u00b7nig", "mu\u00df", "er", "auch", "die", "Rei\u00b7men\u00b7kunst", "ver\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch hei\u00dft es Gott nur gut, dann hab' ich's gut getan.", "tokens": ["Doch", "hei\u00dft", "es", "Gott", "nur", "gut", ",", "dann", "hab'", "ich's", "gut", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was du nicht fassen kannst, ist dir auch nicht geschrieben,", "tokens": ["Was", "du", "nicht", "fas\u00b7sen", "kannst", ",", "ist", "dir", "auch", "nicht", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was dich zu Gott nicht f\u00fchrt, verstehest du nicht gut;", "tokens": ["Was", "dich", "zu", "Gott", "nicht", "f\u00fchrt", ",", "ver\u00b7ste\u00b7hest", "du", "nicht", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "PTKNEG", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man wiegt die Worte nicht, wenn Herzen br\u00fcnstig lieben,", "tokens": ["Man", "wiegt", "die", "Wor\u00b7te", "nicht", ",", "wenn", "Her\u00b7zen", "br\u00fcns\u00b7tig", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKNEG", "$,", "KOUS", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Herr, w\u00e4r' ich ganz erf\u00fcllt mit dieser reinen Glut!", "tokens": ["Herr", ",", "w\u00e4r'", "ich", "ganz", "er\u00b7f\u00fcllt", "mit", "die\u00b7ser", "rei\u00b7nen", "Glut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}