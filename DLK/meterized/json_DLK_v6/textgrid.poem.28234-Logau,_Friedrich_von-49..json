{"textgrid.poem.28234": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht zu weit von meinem singen", "tokens": ["Nicht", "zu", "weit", "von", "mei\u00b7nem", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PTKA", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegen Netz und falsche Schlingen;", "tokens": ["Lie\u00b7gen", "Netz", "und", "fal\u00b7sche", "Schlin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die f\u00fcr mir hier hat gelogen,", "tokens": ["Die", "f\u00fcr", "mir", "hier", "hat", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat mich, wie ich euch, betrogen.", "tokens": ["Hat", "mich", ",", "wie", "ich", "euch", ",", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich, die ich gefangen sitze,", "tokens": ["Ich", ",", "die", "ich", "ge\u00b7fan\u00b7gen", "sit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bin nur meinem Herren n\u00fctze.", "tokens": ["Bin", "nur", "mei\u00b7nem", "Her\u00b7ren", "n\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die da wil, die mag verfliegen,", "tokens": ["Die", "da", "wil", ",", "die", "mag", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "$,", "PRELS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die nicht wil, die la\u00df sich kriegen;", "tokens": ["Die", "nicht", "wil", ",", "die", "la\u00df", "sich", "krie\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VMFIN", "$,", "PRELS", "VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wann nur ich die Kost erwerbe,", "tokens": ["Wann", "nur", "ich", "die", "Kost", "er\u00b7wer\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gilt mirs gleiche, wer verterbe.", "tokens": ["Gilt", "mirs", "glei\u00b7che", ",", "wer", "ver\u00b7ter\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "PWS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}