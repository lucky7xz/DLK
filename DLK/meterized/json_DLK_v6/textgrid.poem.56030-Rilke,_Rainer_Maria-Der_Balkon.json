{"textgrid.poem.56030": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Balkon", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von der Enge, oben, des Balkones", "tokens": ["Von", "der", "En\u00b7ge", ",", "o\u00b7ben", ",", "des", "Bal\u00b7ko\u00b7nes"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "angeordnet wie von einem Maler", "tokens": ["an\u00b7ge\u00b7ord\u00b7net", "wie", "von", "ei\u00b7nem", "Ma\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KOKOM", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und gebunden wie zu einem Strau\u00df", "tokens": ["und", "ge\u00b7bun\u00b7den", "wie", "zu", "ei\u00b7nem", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "KOKOM", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "alternder Gesichter und ovaler,", "tokens": ["al\u00b7tern\u00b7der", "Ge\u00b7sich\u00b7ter", "und", "o\u00b7va\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "klar im Abend, sehn sie idealer,", "tokens": ["klar", "im", "A\u00b7bend", ",", "sehn", "sie", "i\u00b7dea\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "r\u00fchrender und wie f\u00fcr immer aus.", "tokens": ["r\u00fch\u00b7ren\u00b7der", "und", "wie", "f\u00fcr", "im\u00b7mer", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PWAV", "APPR", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Diese aneinander angelehnten", "tokens": ["Die\u00b7se", "an\u00b7ein\u00b7an\u00b7der", "an\u00b7ge\u00b7lehn\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwestern, die, als ob sie sich von weit", "tokens": ["Schwes\u00b7tern", ",", "die", ",", "als", "ob", "sie", "sich", "von", "weit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "$,", "KOKOM", "KOUS", "PPER", "PRF", "APPR", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ohne Aussicht nacheinander sehnten,", "tokens": ["oh\u00b7ne", "Aus\u00b7sicht", "na\u00b7ch\u00b7ein\u00b7an\u00b7der", "sehn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "lehnen, Einsamkeit an Einsamkeit;", "tokens": ["leh\u00b7nen", ",", "Ein\u00b7sam\u00b7keit", "an", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "und der Bruder mit dem feierlichen", "tokens": ["und", "der", "Bru\u00b7der", "mit", "dem", "fei\u00b7er\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schweigen, zugeschlossen, voll Geschick,", "tokens": ["Schwei\u00b7gen", ",", "zu\u00b7ge\u00b7schlos\u00b7sen", ",", "voll", "Ge\u00b7schick", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "doch von einem sanften Augenblick", "tokens": ["doch", "von", "ei\u00b7nem", "sanf\u00b7ten", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "mit der Mutter unbemerkt verglichen;", "tokens": ["mit", "der", "Mut\u00b7ter", "un\u00b7be\u00b7merkt", "ver\u00b7gli\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "und dazwischen, abgelebt und l\u00e4nglich,", "tokens": ["und", "da\u00b7zwi\u00b7schen", ",", "ab\u00b7ge\u00b7lebt", "und", "l\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "l\u00e4ngst mit keinem mehr verwandt,", "tokens": ["l\u00e4ngst", "mit", "kei\u00b7nem", "mehr", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "einer Greisin Maske, unzug\u00e4nglich,", "tokens": ["ei\u00b7ner", "Grei\u00b7sin", "Mas\u00b7ke", ",", "un\u00b7zu\u00b7g\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wie im Fallen von der einen Hand", "tokens": ["wie", "im", "Fal\u00b7len", "von", "der", "ei\u00b7nen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "APPR", "ART", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "aufgehalten, w\u00e4hrend eine zweite", "tokens": ["auf\u00b7ge\u00b7hal\u00b7ten", ",", "w\u00e4h\u00b7rend", "ei\u00b7ne", "zwei\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "welkere, als ob sie weitergleite,", "tokens": ["wel\u00b7ke\u00b7re", ",", "als", "ob", "sie", "wei\u00b7ter\u00b7glei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "unten vor den Kleidern h\u00e4ngt zur Seite", "tokens": ["un\u00b7ten", "vor", "den", "Klei\u00b7dern", "h\u00e4ngt", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "von dem Kinder-Angesicht,", "tokens": ["von", "dem", "Kin\u00b7der\u00b7An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "das das Letzte ist, versucht, verblichen,", "tokens": ["das", "das", "Letz\u00b7te", "ist", ",", "ver\u00b7sucht", ",", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "VAFIN", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "von den St\u00e4ben wieder durchgestrichen", "tokens": ["von", "den", "St\u00e4\u00b7ben", "wie\u00b7der", "durch\u00b7ge\u00b7stri\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wie noch unbestimmbar, wie noch nicht.", "tokens": ["wie", "noch", "un\u00b7be\u00b7stimm\u00b7bar", ",", "wie", "noch", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "PWAV", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}