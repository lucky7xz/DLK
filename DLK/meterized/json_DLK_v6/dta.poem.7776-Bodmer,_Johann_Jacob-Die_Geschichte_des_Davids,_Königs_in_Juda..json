, "leicht", "ver\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "So ware Davids Sinn, er stellt ihm f\u00fcr daneben,", "tokens": ["So", "wa\u00b7re", "Da\u00b7vids", "Sinn", ",", "er", "stellt", "ihm", "f\u00fcr", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Wie Gott durch Mosen h\u00e4tt sein theur Gesetz gegeben:", "tokens": ["Wie", "Gott", "durch", "Mo\u00b7sen", "h\u00e4tt", "sein", "theur", "Ge\u00b7setz", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "So w\u00e4r es auch geschehn mit Donner, Feur und Blitz,", "tokens": ["So", "w\u00e4r", "es", "auch", "ge\u00b7schehn", "mit", "Don\u00b7ner", ",", "Feur", "und", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Da\u00df selbst der Sinai gebrannt in seiner Spitz.", "tokens": ["Da\u00df", "selbst", "der", "Si\u00b7nai", "ge\u00b7brannt", "in", "sei\u00b7ner", "Spitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.85": {"text": "Die\u00df edele Gesetz war seine Lust und Freude,", "tokens": ["Die\u00df", "e\u00b7de\u00b7le", "Ge\u00b7setz", "war", "sei\u00b7ne", "Lust", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Daran fand er stets Lust, auch in dem gr\u00f6sten Leyde.", "tokens": ["Da\u00b7ran", "fand", "er", "stets", "Lust", ",", "auch", "in", "dem", "gr\u00f6s\u00b7ten", "Ley\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.87": {"text": "Da denckt er Tag und Nacht mit grosser Sorgfalt auf,", "tokens": ["Da", "denckt", "er", "Tag", "und", "Nacht", "mit", "gros\u00b7ser", "Sorg\u00b7falt", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Setzt es zur Richtschnur sich in seinem Lebenslauf.", "tokens": ["Setzt", "es", "zur", "Richt\u00b7schnur", "sich", "in", "sei\u00b7nem", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}