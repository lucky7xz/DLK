{"dta.poem.7776": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hierauf, weil schon der Tag sich schiene zu verliehren,", "tokens": ["Hier\u00b7auf", ",", "weil", "schon", "der", "Tag", "sich", "schie\u00b7ne", "zu", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "ADV", "ART", "NN", "PRF", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wollt David seine Heerd in ihre H\u00fcrden f\u00fchren,", "tokens": ["Wollt", "Da\u00b7vid", "sei\u00b7ne", "Heerd", "in", "ih\u00b7re", "H\u00fcr\u00b7den", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Trieb sie drauf f\u00fcr sich hin, durch einen tiefen Thal,", "tokens": ["Trieb", "sie", "drauf", "f\u00fcr", "sich", "hin", ",", "durch", "ei\u00b7nen", "tie\u00b7fen", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "APPR", "PRF", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und spielte ihnen f\u00fcr mit seiner Harffen Schall:", "tokens": ["Und", "spiel\u00b7te", "ih\u00b7nen", "f\u00fcr", "mit", "sei\u00b7ner", "Harf\u00b7fen", "Schall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Ort zu seiner Ruh war rund herum beschlossen", "tokens": ["Der", "Ort", "zu", "sei\u00b7ner", "Ruh", "war", "rund", "he\u00b7rum", "be\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "PTKVZ", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit steinigtem Gebirg, woraus mit Anmuth flossen", "tokens": ["Mit", "stei\u00b7nig\u00b7tem", "Ge\u00b7birg", ",", "wo\u00b7raus", "mit", "An\u00b7muth", "flos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der B\u00e4che Silber-Quell, so durch die Klippen fiel,", "tokens": ["Der", "B\u00e4\u00b7che", "Sil\u00b7ber\u00b7Quell", ",", "so", "durch", "die", "Klip\u00b7pen", "fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und eilte mit Ger\u00e4usch zu kommen nach dem Ziel.", "tokens": ["Und", "eil\u00b7te", "mit", "Ge\u00b7r\u00e4usch", "zu", "kom\u00b7men", "nach", "dem", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Di\u00df sausendes Get\u00f6n bracht Anmuth da zu schlafen,", "tokens": ["Di\u00df", "sau\u00b7sen\u00b7des", "Ge\u00b7t\u00f6n", "bracht", "An\u00b7muth", "da", "zu", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Drum hatte David auch mit seinen lieben Schafen,", "tokens": ["Drum", "hat\u00b7te", "Da\u00b7vid", "auch", "mit", "sei\u00b7nen", "lie\u00b7ben", "Scha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NE", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erkieset dieses Thal, das die Natur bebaut", "tokens": ["Er\u00b7kie\u00b7set", "die\u00b7ses", "Thal", ",", "das", "die", "Na\u00b7tur", "be\u00b7baut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit einer Maur, da\u00df er sein Vieh hie sicher schaut.", "tokens": ["Mit", "ei\u00b7ner", "Maur", ",", "da\u00df", "er", "sein", "Vieh", "hie", "si\u00b7cher", "schaut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dann nur ein Eingang war den wilden Thieren offen,", "tokens": ["Dann", "nur", "ein", "Ein\u00b7gang", "war", "den", "wil\u00b7den", "Thie\u00b7ren", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch, da\u00df f\u00fcr deren Wuth man Sicherheit zu hoffen,", "tokens": ["Doch", ",", "da\u00df", "f\u00fcr", "de\u00b7ren", "Wuth", "man", "Si\u00b7cher\u00b7heit", "zu", "hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "APPR", "PRELAT", "NN", "PIS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Lag da sein Sch\u00e4ferhund, des Treu und Wachsamkeit", "tokens": ["Lag", "da", "sein", "Sch\u00e4\u00b7fer\u00b7hund", ",", "des", "Treu", "und", "Wach\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jhn nimmer nicht verlie\u00df, bey Tag-noch Nachtes-Zeit.", "tokens": ["Jhn", "nim\u00b7mer", "nicht", "ver\u00b7lie\u00df", ",", "bey", "Tag\u00b7noch", "Nach\u00b7tes\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVFIN", "$,", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie nun die Heerde sich in ihre H\u00fcrd begeben,", "tokens": ["Wie", "nun", "die", "Heer\u00b7de", "sich", "in", "ih\u00b7re", "H\u00fcrd", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Gieng David in die H\u00f6hl, die ware gleich daneben,", "tokens": ["Gieng", "Da\u00b7vid", "in", "die", "H\u00f6hl", ",", "die", "wa\u00b7re", "gleich", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "PAV", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "In welcher stuhnd sein Bett, in einem Stein gehaut,", "tokens": ["In", "wel\u00b7cher", "stuhnd", "sein", "Bett", ",", "in", "ei\u00b7nem", "Stein", "ge\u00b7haut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das ehmals Esaus Flei\u00df hat selbsten so gebaut,", "tokens": ["Das", "eh\u00b7mals", "E\u00b7saus", "Flei\u00df", "hat", "selbs\u00b7ten", "so", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wann er war auf der Jagd, war hie bey Nacht sein Bleiben,", "tokens": ["Wann", "er", "war", "auf", "der", "Jagd", ",", "war", "hie", "bey", "Nacht", "sein", "Blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "APPR", "ART", "NN", "$,", "VAFIN", "ADV", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Denn nahe hie herum pflag er die Jagd zu treiben,", "tokens": ["Denn", "na\u00b7he", "hie", "he\u00b7rum", "pflag", "er", "die", "Jagd", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APZR", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "In einem weiten Wald, der seine L\u00fcste stillt", "tokens": ["In", "ei\u00b7nem", "wei\u00b7ten", "Wald", ",", "der", "sei\u00b7ne", "L\u00fcs\u00b7te", "stillt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und seine M\u00fch belohnt, mit manchem edlen Wild.", "tokens": ["Und", "sei\u00b7ne", "M\u00fch", "be\u00b7lohnt", ",", "mit", "man\u00b7chem", "ed\u00b7len", "Wild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jezt wohnte David hie, des Jacobs edler Saame,", "tokens": ["Jezt", "wohn\u00b7te", "Da\u00b7vid", "hie", ",", "des", "Ja\u00b7cobs", "ed\u00b7ler", "Saa\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "$,", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der da nach Gottes Schlu\u00df den Seegen Esau nahme.", "tokens": ["Der", "da", "nach", "Got\u00b7tes", "Schlu\u00df", "den", "See\u00b7gen", "E\u00b7sau", "nah\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "NN", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Di\u00df fiele David ein, als er sich legt zur Ruh,", "tokens": ["Di\u00df", "fie\u00b7le", "Da\u00b7vid", "ein", ",", "als", "er", "sich", "legt", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und schlo\u00df er lang darum die Augen noch nicht zu.", "tokens": ["Und", "schlo\u00df", "er", "lang", "da\u00b7rum", "die", "Au\u00b7gen", "noch", "nicht", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PAV", "ART", "NN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Geht es jezt auch nicht so? dacht er in dem Gem\u00fcthe,", "tokens": ["Geht", "es", "jezt", "auch", "nicht", "so", "?", "dacht", "er", "in", "dem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.30": {"text": "Mich hebet Gott empor nach seiner Wunder-G\u00fcte,", "tokens": ["Mich", "he\u00b7bet", "Gott", "em\u00b7por", "nach", "sei\u00b7ner", "Wun\u00b7der\u00b7G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und wehlet mich f\u00fcr Saul, die Erstgeburt ist sein,", "tokens": ["Und", "weh\u00b7let", "mich", "f\u00fcr", "Saul", ",", "die", "Erst\u00b7ge\u00b7burt", "ist", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "$,", "ART", "NN", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der K\u00f6nigliche Thron, doch soll er werden mein.", "tokens": ["Der", "K\u00f6\u00b7nig\u00b7li\u00b7che", "Thron", ",", "doch", "soll", "er", "wer\u00b7den", "mein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "O grosser Wunder-Gott! als Esau ward gebohren,", "tokens": ["O", "gros\u00b7ser", "Wun\u00b7der\u00b7Gott", "!", "als", "E\u00b7sau", "ward", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "KOUS", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "War er verworffen schon, und Jacob war erkohren.", "tokens": ["War", "er", "ver\u00b7worf\u00b7fen", "schon", ",", "und", "Ja\u00b7cob", "war", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ADV", "$,", "KON", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was kan der Mensch dann thun? er ist ja eh erwehlt,", "tokens": ["Was", "kan", "der", "Mensch", "dann", "thun", "?", "er", "ist", "ja", "eh", "er\u00b7wehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$.", "PPER", "VAFIN", "ADV", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und eh verworffen schon, eh er noch auf der Welt.", "tokens": ["Und", "eh", "ver\u00b7worf\u00b7fen", "schon", ",", "eh", "er", "noch", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Ach Gott! du siehst zuvor der Menschen ihre Sinnen,", "tokens": ["Ach", "Gott", "!", "du", "siehst", "zu\u00b7vor", "der", "Men\u00b7schen", "ih\u00b7re", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Eh sie gebohren sind, weist du schon ihre Beginnen,", "tokens": ["Eh", "sie", "ge\u00b7boh\u00b7ren", "sind", ",", "weist", "du", "schon", "ih\u00b7re", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.39": {"text": "Du straffest drum zuvor, was nachmahls wird gethan,", "tokens": ["Du", "straf\u00b7fest", "drum", "zu\u00b7vor", ",", "was", "nach\u00b7mahls", "wird", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "$,", "PRELS", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die S\u00fcnderrott, und l\u00e4st in allem Recht bestahn.", "tokens": ["Die", "S\u00fcn\u00b7der\u00b7rott", ",", "und", "l\u00e4st", "in", "al\u00b7lem", "Recht", "be\u00b7stahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "APPR", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Nun Esau der ist Saul, da ich mich Jacob gleiche,", "tokens": ["Nun", "E\u00b7sau", "der", "ist", "Saul", ",", "da", "ich", "mich", "Ja\u00b7cob", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ART", "VAFIN", "NE", "$,", "KOUS", "PPER", "PRF", "NE", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ich sehe schon zuvor, wie ich ihm gar nicht weiche", "tokens": ["Ich", "se\u00b7he", "schon", "zu\u00b7vor", ",", "wie", "ich", "ihm", "gar", "nicht", "wei\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "PPER", "ADV", "PTKNEG", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "In mancher Noth und Angst, die ihm zu Handen kam,", "tokens": ["In", "man\u00b7cher", "Noth", "und", "Angst", ",", "die", "ihm", "zu", "Han\u00b7den", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Als er f\u00fcr Esaus Grimm die Flucht zu Handen nahm.", "tokens": ["Als", "er", "f\u00fcr", "E\u00b7saus", "Grimm", "die", "Flucht", "zu", "Han\u00b7den", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "NE", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So wird mirs auch noch gehn, ich werde noch erleben", "tokens": ["So", "wird", "mirs", "auch", "noch", "gehn", ",", "ich", "wer\u00b7de", "noch", "er\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "ADV", "ADV", "VVINF", "$,", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Beym Scepter Ungemach, beym Throne Angst und Bebe\u0303.", "tokens": ["Beym", "Scep\u00b7ter", "Un\u00b7ge\u00b7mach", ",", "beym", "Thro\u00b7ne", "Angst", "und", "Be\u00b7b\u1ebd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.47": {"text": "Doch, Herr, ich bin bereit! Ach Herr, ich bin dein Knecht,", "tokens": ["Doch", ",", "Herr", ",", "ich", "bin", "be\u00b7reit", "!", "Ach", "Herr", ",", "ich", "bin", "dein", "Knecht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VAFIN", "ADJD", "$.", "ITJ", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Nach deinem Willen will ich leben, schlecht und recht.", "tokens": ["Nach", "dei\u00b7nem", "Wil\u00b7len", "will", "ich", "le\u00b7ben", ",", "schlecht", "und", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Hiemit schlieff David ein, in seinem Geist zufrieden,", "tokens": ["Hie\u00b7mit", "schlieff", "Da\u00b7vid", "ein", ",", "in", "sei\u00b7nem", "Geist", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Bereit und wohl ger\u00fcst, wozu ihn Gott beschieden,", "tokens": ["Be\u00b7reit", "und", "wohl", "ge\u00b7r\u00fcst", ",", "wo\u00b7zu", "ihn", "Gott", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVPP", "$,", "PWAV", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Als pl\u00f6tzlich sich der Wind in starckes Brausen kehrt,", "tokens": ["Als", "pl\u00f6tz\u00b7lich", "sich", "der", "Wind", "in", "star\u00b7ckes", "Brau\u00b7sen", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und weckt mit Ungest\u00fcm die eingeschlaffne Heerd.", "tokens": ["Und", "weckt", "mit", "Un\u00b7ge\u00b7st\u00fcm", "die", "ein\u00b7ge\u00b7schlaff\u00b7ne", "Heerd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Die vorhin heitre Luft wird dunckel und umzogen,", "tokens": ["Die", "vor\u00b7hin", "heit\u00b7re", "Luft", "wird", "dun\u00b7ckel", "und", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Die Wolcken stossen sich und blasen Wasserwogen,", "tokens": ["Die", "Wol\u00b7cken", "stos\u00b7sen", "sich", "und", "bla\u00b7sen", "Was\u00b7ser\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Der Mond und Sternen Schein bedeckt ihr braunes Kleid,", "tokens": ["Der", "Mond", "und", "Ster\u00b7nen", "Schein", "be\u00b7deckt", "ihr", "brau\u00b7nes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Die Elemente selbst sind mit sich in dem Streit.", "tokens": ["Die", "E\u00b7le\u00b7men\u00b7te", "selbst", "sind", "mit", "sich", "in", "dem", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "PRF", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.57": {"text": "Die Luft die saust und braust, und will dem Wasser wehren,", "tokens": ["Die", "Luft", "die", "saust", "und", "braust", ",", "und", "will", "dem", "Was\u00b7ser", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "VVFIN", "$,", "KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Da\u00df nicht ein Wolckenbruch die Felder m\u00f6g verzehren,", "tokens": ["Da\u00df", "nicht", "ein", "Wol\u00b7cken\u00b7bruch", "die", "Fel\u00b7der", "m\u00f6g", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Doch bricht das Wasser durch, die Wolcken gehn entzwey,", "tokens": ["Doch", "bricht", "das", "Was\u00b7ser", "durch", ",", "die", "Wol\u00b7cken", "gehn", "ent\u00b7zwey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Und wollen so das Feur verl\u00f6schen, wo es sey.", "tokens": ["Und", "wol\u00b7len", "so", "das", "Feur", "ver\u00b7l\u00f6\u00b7schen", ",", "wo", "es", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Das Feur, der schnelle Blitz, zischt doch durch diese L\u00fcfte,", "tokens": ["Das", "Feur", ",", "der", "schnel\u00b7le", "Blitz", ",", "zischt", "doch", "durch", "die\u00b7se", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.62": {"text": "Und acht das Wasser nicht. Der Erden tieffe Kl\u00fcffte", "tokens": ["Und", "acht", "das", "Was\u00b7ser", "nicht", ".", "Der", "Er\u00b7den", "tief\u00b7fe", "Kl\u00fcff\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "CARD", "ART", "NN", "PTKNEG", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Ersch\u00fctteren darob, wie sich der Donnerschall", "tokens": ["Er\u00b7sch\u00fct\u00b7te\u00b7ren", "da\u00b7rob", ",", "wie", "sich", "der", "Don\u00b7ner\u00b7schall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "$,", "PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Hiernach auch h\u00f6ren l\u00e4st, mit schrecklichem Geknall.", "tokens": ["Hier\u00b7nach", "auch", "h\u00f6\u00b7ren", "l\u00e4st", ",", "mit", "schreck\u00b7li\u00b7chem", "Ge\u00b7knall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVINF", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Die Felsen dort umher vermehren dieses Rollen,", "tokens": ["Die", "Fel\u00b7sen", "dort", "um\u00b7her", "ver\u00b7meh\u00b7ren", "die\u00b7ses", "Rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Und scheinen jeden Schlag mit Schlag zu doppeln wolle\u0303,", "tokens": ["Und", "schei\u00b7nen", "je\u00b7den", "Schlag", "mit", "Schlag", "zu", "dop\u00b7peln", "woll\u1ebd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Der Nachtigalen Zunft verkriecht sich in die Stein,", "tokens": ["Der", "Nach\u00b7ti\u00b7ga\u00b7len", "Zunft", "ver\u00b7kriecht", "sich", "in", "die", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Die Schafe wollen nicht mehr in den H\u00fcrden seyn;", "tokens": ["Die", "Scha\u00b7fe", "wol\u00b7len", "nicht", "mehr", "in", "den", "H\u00fcr\u00b7den", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Sie lauffen hin und her, in dieser Felsen Ritzen;", "tokens": ["Sie", "lauf\u00b7fen", "hin", "und", "her", ",", "in", "die\u00b7ser", "Fel\u00b7sen", "Rit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Der treue Hund will auch nicht in der H\u00fctte sitzen,", "tokens": ["Der", "treu\u00b7e", "Hund", "will", "auch", "nicht", "in", "der", "H\u00fct\u00b7te", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.71": {"text": "F\u00fcr solchem starcken Feind ist ihm der Muth nicht da,", "tokens": ["F\u00fcr", "sol\u00b7chem", "star\u00b7cken", "Feind", "ist", "ihm", "der", "Muth", "nicht", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Wo alles sich f\u00fcr scheut, ist auch sein Schrecken nah.", "tokens": ["Wo", "al\u00b7les", "sich", "f\u00fcr", "scheut", ",", "ist", "auch", "sein", "Schre\u00b7cken", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "APPR", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Der David der erwacht von diesem starcken W\u00fcten,", "tokens": ["Der", "Da\u00b7vid", "der", "er\u00b7wacht", "von", "die\u00b7sem", "star\u00b7cken", "W\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Sein Seufzen gieng zu Gott, da\u00df der sich lie\u00df beg\u00fcten,", "tokens": ["Sein", "Seuf\u00b7zen", "gieng", "zu", "Gott", ",", "da\u00df", "der", "sich", "lie\u00df", "be\u00b7g\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,", "KOUS", "ART", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Und nicht in Eifer straft. Er dacht, wie gro\u00df ist der,", "tokens": ["Und", "nicht", "in", "Ei\u00b7fer", "straft", ".", "Er", "dacht", ",", "wie", "gro\u00df", "ist", "der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "ADJD", "$.", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "VAFIN", "ART", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.76": {"text": "Dem da s