{"textgrid.poem.48925": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Die Heilung", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleines Kind mit Fl\u00fcgeln", "tokens": ["Ein", "klei\u00b7nes", "Kind", "mit", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam j\u00fcngst mit muntern Schritten", "tokens": ["Kam", "j\u00fcngst", "mit", "mun\u00b7tern", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In meinen Blumengarten.", "tokens": ["In", "mei\u00b7nen", "Blu\u00b7men\u00b7gar\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ging in alle G\u00e4nge,", "tokens": ["Es", "ging", "in", "al\u00b7le", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sah nach allen Beeten,", "tokens": ["Und", "sah", "nach", "al\u00b7len", "Bee\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und pfl\u00fcckte Rosenknospen,", "tokens": ["Und", "pfl\u00fcck\u00b7te", "Ro\u00b7sen\u00b7knos\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und haschte Schmetterlinge,", "tokens": ["Und", "haschte", "Schmet\u00b7ter\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Die um die Rosen buhlten,", "tokens": ["Die", "um", "die", "Ro\u00b7sen", "buhl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und strich die g\u00f6ldnen St\u00e4ubchen", "tokens": ["Und", "strich", "die", "g\u00f6ld\u00b7nen", "St\u00e4ub\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Von den gesprengten Fl\u00fcgeln.", "tokens": ["Von", "den", "ge\u00b7spreng\u00b7ten", "Fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Itzt wollt es wieder haschen,", "tokens": ["Itzt", "wollt", "es", "wie\u00b7der", "ha\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und hob die Hand behutsam,", "tokens": ["Und", "hob", "die", "Hand", "be\u00b7hut\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und grif, und zischte pl\u00f6tzlich,", "tokens": ["Und", "grif", ",", "und", "zischte", "pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und zog sie schnell zur\u00fccke.", "tokens": ["Und", "zog", "sie", "schnell", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ein Dorn vom Stamm der Rosen", "tokens": ["Ein", "Dorn", "vom", "Stamm", "der", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Stach in den zarten Finger.", "tokens": ["Stach", "in", "den", "zar\u00b7ten", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Es schwang die Hand f\u00fcr Schmerzen,", "tokens": ["Es", "schwang", "die", "Hand", "f\u00fcr", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und sahe nach der Wunde,", "tokens": ["Und", "sa\u00b7he", "nach", "der", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und zog mit sauren Blicken", "tokens": ["Und", "zog", "mit", "sau\u00b7ren", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Den Dorn sich aus der Wunde.", "tokens": ["Den", "Dorn", "sich", "aus", "der", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Ich lauscht ihm gegen \u00fcber,", "tokens": ["Ich", "lauscht", "ihm", "ge\u00b7gen", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Bey Doris in der Laube,", "tokens": ["Bey", "Do\u00b7ris", "in", "der", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.23": {"text": "Und lachte seiner Minen.", "tokens": ["Und", "lach\u00b7te", "sei\u00b7ner", "Mi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Schnell winkt es mit dem Kopfe,", "tokens": ["Schnell", "winkt", "es", "mit", "dem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Und griff nach Pfeil und Bogen,", "tokens": ["Und", "griff", "nach", "Pfeil", "und", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Und sagte leise: Sp\u00f6tter,", "tokens": ["Und", "sag\u00b7te", "lei\u00b7se", ":", "Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Weisst du wie Wunden schmerzen?", "tokens": ["Weisst", "du", "wie", "Wun\u00b7den", "schmer\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Du sollst es bald erfahren;", "tokens": ["Du", "sollst", "es", "bald", "er\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Und zielte mit dem Bogen,", "tokens": ["Und", "ziel\u00b7te", "mit", "dem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Und, eh ich mirs versahe,", "tokens": ["Und", ",", "eh", "ich", "mirs", "ver\u00b7sa\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Stach mir der Pfeil im Herzen.", "tokens": ["Stach", "mir", "der", "Pfeil", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "O, wie ward mir zu Muthe!", "tokens": ["O", ",", "wie", "ward", "mir", "zu", "Mu\u00b7the", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.33": {"text": "Ich sank f\u00fcr Schmerzen nieder,", "tokens": ["Ich", "sank", "f\u00fcr", "Schmer\u00b7zen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Und dachte schnell zu sterben.", "tokens": ["Und", "dach\u00b7te", "schnell", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Doch Doris, meine Taube,", "tokens": ["Doch", "Do\u00b7ris", ",", "mei\u00b7ne", "Tau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Entzog den Pfeil der Wunde,", "tokens": ["Ent\u00b7zog", "den", "Pfeil", "der", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Und salbte sie mit Salben,", "tokens": ["Und", "salb\u00b7te", "sie", "mit", "Sal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Und streichelte sie z\u00e4rtlich,", "tokens": ["Und", "strei\u00b7chel\u00b7te", "sie", "z\u00e4rt\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Und linderte die Schmerzen,", "tokens": ["Und", "lin\u00b7der\u00b7te", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Und so ward ich geheilet.", "tokens": ["Und", "so", "ward", "ich", "ge\u00b7hei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "Hinfort will ich des Losen,", "tokens": ["Hin\u00b7fort", "will", "ich", "des", "Lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Die\u00df wei\u00df ich, nicht mehr spotten.", "tokens": ["Die\u00df", "wei\u00df", "ich", ",", "nicht", "mehr", "spot\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Wenn mich die sch\u00f6ne Doris", "tokens": ["Wenn", "mich", "die", "sch\u00f6\u00b7ne", "Do\u00b7ris"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.44": {"text": "Aus Mittleid nicht geheilet,", "tokens": ["Aus", "Mitt\u00b7leid", "nicht", "ge\u00b7hei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "So w\u00e4r ich schon gestorben.", "tokens": ["So", "w\u00e4r", "ich", "schon", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}