{"textgrid.poem.24532": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Seine sch\u00f6ne Nachbarschafft", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Auge/ mein Verstand/ und mein vergn\u00fcgter Sinn/", "tokens": ["Mein", "Au\u00b7ge", "/", "mein", "Ver\u00b7stand", "/", "und", "mein", "ver\u00b7gn\u00fcg\u00b7ter", "Sinn", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erkennen/ Sch\u00f6nste/ dich vor ihre Nachbarin.", "tokens": ["Er\u00b7ken\u00b7nen", "/", "Sch\u00f6ns\u00b7te", "/", "dich", "vor", "ih\u00b7re", "Nach\u00b7ba\u00b7rin", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur werde mit der Zeit durch deiner Augen Kertzen/", "tokens": ["Nur", "wer\u00b7de", "mit", "der", "Zeit", "durch", "dei\u00b7ner", "Au\u00b7gen", "Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht auch die Nachbarin von meinem freyen Hertzen/", "tokens": ["Nicht", "auch", "die", "Nach\u00b7ba\u00b7rin", "von", "mei\u00b7nem", "frey\u00b7en", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und steck es in den Brand/ sonst ruf' ich Feuer aus:", "tokens": ["Und", "steck", "es", "in", "den", "Brand", "/", "sonst", "ruf'", "ich", "Feu\u00b7er", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein treuer Nachbar komt und l\u00f6scht des andern Hau\u00df.", "tokens": ["Ein", "treu\u00b7er", "Nach\u00b7bar", "komt", "und", "l\u00f6scht", "des", "an\u00b7dern", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}