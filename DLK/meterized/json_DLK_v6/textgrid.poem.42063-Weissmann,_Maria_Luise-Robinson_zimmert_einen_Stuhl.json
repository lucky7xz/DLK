{"textgrid.poem.42063": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Robinson zimmert einen Stuhl", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lehre schwer die unge\u00fcbten H\u00e4nde,", "tokens": ["Ich", "leh\u00b7re", "schwer", "die", "un\u00b7ge\u00b7\u00fcb\u00b7ten", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die von nichts wissen. Und mein Mund ward stumm,", "tokens": ["Die", "von", "nichts", "wis\u00b7sen", ".", "Und", "mein", "Mund", "ward", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "VVINF", "$.", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ob Erinnerung ihm langsam schw\u00e4nde.", "tokens": ["Als", "ob", "E\u00b7rin\u00b7ne\u00b7rung", "ihm", "lang\u00b7sam", "schw\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich hab ein Ding, behindert, schief und krumm,", "tokens": ["Ich", "hab", "ein", "Ding", ",", "be\u00b7hin\u00b7dert", ",", "schief", "und", "krumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VVPP", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gebaut und mu\u00df an ihm nun meine Zunge lehren,", "tokens": ["Ge\u00b7baut", "und", "mu\u00df", "an", "ihm", "nun", "mei\u00b7ne", "Zun\u00b7ge", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VMFIN", "APPR", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die \u00bbStuhl\u00ab sagt. Mit dem kindlichen, erstaunten Klang.", "tokens": ["Die", "\u00bb", "Stuhl", "\u00ab", "sagt", ".", "Mit", "dem", "kind\u00b7li\u00b7chen", ",", "er\u00b7staun\u00b7ten", "Klang", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "$(", "VVFIN", "$.", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und meine gl\u00fcckbet\u00e4ubten H\u00e4nde ehren", "tokens": ["Und", "mei\u00b7ne", "gl\u00fcck\u00b7be\u00b7t\u00e4ub\u00b7ten", "H\u00e4n\u00b7de", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich in dem ersten Werk. Ein Tun gelang", "tokens": ["Sich", "in", "dem", "ers\u00b7ten", "Werk", ".", "Ein", "Tun", "ge\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Grund: ich sitze. (Und das ist fast, wie", "tokens": ["Von", "Grund", ":", "ich", "sit\u00b7ze", ".", "(", "Und", "das", "ist", "fast", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$.", "PPER", "VVFIN", "$.", "$(", "KON", "PDS", "VAFIN", "ADV", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dies einmal war, sehr fr\u00fch: ich bog die Knie.)", "tokens": ["Dies", "ein\u00b7mal", "war", ",", "sehr", "fr\u00fch", ":", "ich", "bog", "die", "Knie", ".", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ADV", "VAFIN", "$,", "ADV", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}