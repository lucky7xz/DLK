{"textgrid.poem.66630": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Solang meine Seele noch leuchtet und bl\u00fcht,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solang meine Seele noch leuchtet und bl\u00fcht,", "tokens": ["So\u00b7lang", "mei\u00b7ne", "See\u00b7le", "noch", "leuch\u00b7tet", "und", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das wonnige Leben, nicht werd' ich es m\u00fcd!", "tokens": ["Das", "won\u00b7ni\u00b7ge", "Le\u00b7ben", ",", "nicht", "werd'", "ich", "es", "m\u00fcd", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKNEG", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich lausche den Rhythmen der rauschenden Welt,", "tokens": ["Ich", "lau\u00b7sche", "den", "Rhyth\u00b7men", "der", "rau\u00b7schen\u00b7den", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die klangvoll am ewigen Strande zerschellt.", "tokens": ["Die", "klang\u00b7voll", "am", "e\u00b7wi\u00b7gen", "Stran\u00b7de", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich weide das Aug' am Geschmeide der Zeit,", "tokens": ["Ich", "wei\u00b7de", "das", "Aug'", "am", "Ge\u00b7schmei\u00b7de", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Das funkelt in dunkler Unendlichkeit,", "tokens": ["Das", "fun\u00b7kelt", "in", "dunk\u00b7ler", "Un\u00b7end\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ich atme der Freiheit Sturmwind,", "tokens": ["Ich", "at\u00b7me", "der", "Frei\u00b7heit", "Sturm\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der die Knechtschaft sch\u00fcttert zugrund,", "tokens": ["Der", "die", "Knecht\u00b7schaft", "sch\u00fct\u00b7tert", "zu\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Ich k\u00fcsse der Wonne Wangen", "tokens": ["Ich", "k\u00fcs\u00b7se", "der", "Won\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Mit zitternden Lippen wund.", "tokens": ["Mit", "zit\u00b7tern\u00b7den", "Lip\u00b7pen", "wund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Solange der Geist mir noch fruchtet und tr\u00e4gt,", "tokens": ["So\u00b7lan\u00b7ge", "der", "Geist", "mir", "noch", "fruch\u00b7tet", "und", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Baum meiner Freude, nicht sei er zers\u00e4gt!", "tokens": ["Der", "Baum", "mei\u00b7ner", "Freu\u00b7de", ",", "nicht", "sei", "er", "zer\u00b7s\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PTKNEG", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich sammle der Denker schwergoldene Saat", "tokens": ["Ich", "samm\u00b7le", "der", "Den\u00b7ker", "schwer\u00b7gol\u00b7de\u00b7ne", "Saat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und mahle den Weizen mit pl\u00e4tscherndem Rad.", "tokens": ["Und", "mah\u00b7le", "den", "Wei\u00b7zen", "mit", "pl\u00e4t\u00b7schern\u00b7dem", "Rad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich k\u00fcre die sch\u00f6nsten Gedanken zum Tanz", "tokens": ["Ich", "k\u00fc\u00b7re", "die", "sch\u00f6ns\u00b7ten", "Ge\u00b7dan\u00b7ken", "zum", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und winde der Wahrheit den schwellenden Kranz.", "tokens": ["Und", "win\u00b7de", "der", "Wahr\u00b7heit", "den", "schwel\u00b7len\u00b7den", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ich gr\u00fc\u00dfe das werdende Gute", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "das", "wer\u00b7den\u00b7de", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Mit hocherhobener Stirn,", "tokens": ["Mit", "ho\u00b7cher\u00b7ho\u00b7be\u00b7ner", "Stirn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Ich feire der reinen Erkenntnis", "tokens": ["Ich", "fei\u00b7re", "der", "rei\u00b7nen", "Er\u00b7kennt\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Hellgl\u00fchende Rosenfirn.", "tokens": ["Hell\u00b7gl\u00fc\u00b7hen\u00b7de", "Ro\u00b7sen\u00b7firn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Solang meine Seele noch leuchtet und bl\u00fcht,", "tokens": ["So\u00b7lang", "mei\u00b7ne", "See\u00b7le", "noch", "leuch\u00b7tet", "und", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Solange der Geist mir noch fruchtet und tr\u00e4gt,", "tokens": ["So\u00b7lan\u00b7ge", "der", "Geist", "mir", "noch", "fruch\u00b7tet", "und", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das wonnige Leben, nicht werd' ich es m\u00fcd,", "tokens": ["Das", "won\u00b7ni\u00b7ge", "Le\u00b7ben", ",", "nicht", "werd'", "ich", "es", "m\u00fcd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKNEG", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Baum meiner Freude, nicht sei er zers\u00e4gt!", "tokens": ["Der", "Baum", "mei\u00b7ner", "Freu\u00b7de", ",", "nicht", "sei", "er", "zer\u00b7s\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PTKNEG", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}