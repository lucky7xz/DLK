{"dta.poem.5327": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sichtbar werd\u2019, und unser Sinn,", "tokens": ["Sicht\u00b7bar", "werd'", ",", "und", "un\u00b7ser", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von derselben Pracht ger\u00fchret,", "tokens": ["Von", "der\u00b7sel\u00b7ben", "Pracht", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den Glantz zuletzt dahin", "tokens": ["Durch", "den", "Glantz", "zu\u00b7letzt", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufgezogen und gef\u00fchret,", "tokens": ["Auf\u00b7ge\u00b7zo\u00b7gen", "und", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Woraus selbst der Sonnen Pracht", "tokens": ["Wo\u00b7raus", "selbst", "der", "Son\u00b7nen", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Erst entsprungen, der die Welt,", "tokens": ["Erst", "ent\u00b7sprun\u00b7gen", ",", "der", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie erschaffen, so erh\u00e4lt,", "tokens": ["Wie", "er\u00b7schaf\u00b7fen", ",", "so", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und so herrlich zubereitet.", "tokens": ["Und", "so", "herr\u00b7lich", "zu\u00b7be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hast du also, kleine Fliege,", "tokens": ["Hast", "du", "al\u00b7so", ",", "klei\u00b7ne", "Flie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da ich mich an dir vergn\u00fcge,", "tokens": ["Da", "ich", "mich", "an", "dir", "ver\u00b7gn\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Selbst zur GOttheit mich geleitet.", "tokens": ["Selbst", "zur", "Got\u00b7theit", "mich", "ge\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}