{"textgrid.poem.67118": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Diotima", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schweigst und duldest, und sie verstehn dich nicht,", "tokens": ["Du", "schweigst", "und", "dul\u00b7dest", ",", "und", "sie", "ver\u00b7stehn", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du heilig Leben! welkest hinweg und schweigst,", "tokens": ["Du", "hei\u00b7lig", "Le\u00b7ben", "!", "wel\u00b7kest", "hin\u00b7weg", "und", "schweigst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn ach, vergebens bei Barbaren", "tokens": ["Denn", "ach", ",", "ver\u00b7ge\u00b7bens", "bei", "Bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Suchst du die Deinen im Sonnenlichte,", "tokens": ["Suchst", "du", "die", "Dei\u00b7nen", "im", "Son\u00b7nen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPOSAT", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die z\u00e4rtlichgro\u00dfen Seelen, die nimmer sind!", "tokens": ["Die", "z\u00e4rt\u00b7lich\u00b7gro\u00b7\u00dfen", "See\u00b7len", ",", "die", "nim\u00b7mer", "sind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch eilt die Zeit. Noch siehet mein sterblich Lied", "tokens": ["Doch", "eilt", "die", "Zeit", ".", "Noch", "sie\u00b7het", "mein", "sterb\u00b7lich", "Lied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Tag, der, Diotima! n\u00e4chst den", "tokens": ["Den", "Tag", ",", "der", ",", "Dio\u00b7ti\u00b7ma", "!", "n\u00e4chst", "den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "NE", "$.", "ADV", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "G\u00f6ttern mit Helden dich nennt, und dir gleicht.", "tokens": ["G\u00f6t\u00b7tern", "mit", "Hel\u00b7den", "dich", "nennt", ",", "und", "dir", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}