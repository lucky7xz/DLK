{"dta.poem.2213": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach der Beschaffenheit der Neigung und aller K\u00f6r-", "tokens": ["Nach", "der", "Be\u00b7schaf\u00b7fen\u00b7heit", "der", "Nei\u00b7gung", "und", "al\u00b7ler", "K\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "PIAT", "NN"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "per auf der Welt,", "tokens": ["per", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ist nicht das, welches sch\u00f6n ist, sch\u00f6n: nur das ist", "tokens": ["Ist", "nicht", "das", ",", "wel\u00b7ches", "sch\u00f6n", "ist", ",", "sch\u00f6n", ":", "nur", "das", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PDS", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ADJD", "$.", "ADV", "PDS", "VAFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "sch\u00f6n, was uns gef\u00e4llt.", "tokens": ["sch\u00f6n", ",", "was", "uns", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}