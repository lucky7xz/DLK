{"textgrid.poem.42068": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ein Leichnam ist ans Land gesp\u00fclt", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gr\u00fc\u00dfte ihn vertraut. Und seinen Namen", "tokens": ["Ich", "gr\u00fc\u00df\u00b7te", "ihn", "ver\u00b7traut", ".", "Und", "sei\u00b7nen", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbtod\u00ab sprach ich br\u00fcderlich. Da hob er sich,", "tokens": ["\u00bb", "tod", "\u00ab", "sprach", "ich", "br\u00fc\u00b7der\u00b7lich", ".", "Da", "hob", "er", "sich", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "VVFIN", "PPER", "ADJD", "$.", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da fing er mich am t\u00f6dlicheren Hamen,", "tokens": ["Da", "fing", "er", "mich", "am", "t\u00f6d\u00b7li\u00b7che\u00b7ren", "Ha\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da warf er Diesen hin, da bog er mich", "tokens": ["Da", "warf", "er", "Die\u00b7sen", "hin", ",", "da", "bog", "er", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An das erstarrte Herz, den Schlag zu suchen,", "tokens": ["An", "das", "er\u00b7starr\u00b7te", "Herz", ",", "den", "Schlag", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schlo\u00df Mund auf Mund, ob er nicht Atem fand,", "tokens": ["Schlo\u00df", "Mund", "auf", "Mund", ",", "ob", "er", "nicht", "A\u00b7tem", "fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da hie\u00df er mich mir rasender zu fluchen...", "tokens": ["Da", "hie\u00df", "er", "mich", "mir", "ra\u00b7sen\u00b7der", "zu", "flu\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich grub ihn ein, der mir der Freund gewesen,", "tokens": ["Ich", "grub", "ihn", "ein", ",", "der", "mir", "der", "Freund", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PPER", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das liebste Du ward Feind: der Teure trug", "tokens": ["Das", "liebs\u00b7te", "Du", "ward", "Feind", ":", "der", "Teu\u00b7re", "trug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VAFIN", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich Jenem an, von dem ich fast genesen;", "tokens": ["Sich", "Je\u00b7nem", "an", ",", "von", "dem", "ich", "fast", "ge\u00b7ne\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er stand beim Tod.", "tokens": ["Er", "stand", "beim", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O, St\u00e4rke, die mich schlug!", "tokens": ["O", ",", "St\u00e4r\u00b7ke", ",", "die", "mich", "schlug", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}