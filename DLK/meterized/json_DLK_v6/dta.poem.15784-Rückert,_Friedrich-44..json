{"dta.poem.15784": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo h\u00f6rt die Heimat auf, und f\u00e4ngt die Fremde an?", "tokens": ["Wo", "h\u00f6rt", "die", "Hei\u00b7mat", "auf", ",", "und", "f\u00e4ngt", "die", "Frem\u00b7de", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es liegt daran, wie weit das Herz ist aufgethan.", "tokens": ["Es", "liegt", "da\u00b7ran", ",", "wie", "weit", "das", "Herz", "ist", "auf\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein enges Herz, das sich verstockt im Winkel hat,", "tokens": ["Ein", "en\u00b7ges", "Herz", ",", "das", "sich", "ver\u00b7stockt", "im", "Win\u00b7kel", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "VVPP", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es findet fremdes Land drei Finger von der Stadt;", "tokens": ["Es", "fin\u00b7det", "frem\u00b7des", "Land", "drei", "Fin\u00b7ger", "von", "der", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein weites aber hat das Fernste sein genannt,", "tokens": ["Ein", "wei\u00b7tes", "a\u00b7ber", "hat", "das", "Ferns\u00b7te", "sein", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VAFIN", "ART", "ADJA", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie vom Himmel wird die bl\u00fchnde Welt umspannt.", "tokens": ["Als\u00b7wie", "vom", "Him\u00b7mel", "wird", "die", "bl\u00fchn\u00b7de", "Welt", "um\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}