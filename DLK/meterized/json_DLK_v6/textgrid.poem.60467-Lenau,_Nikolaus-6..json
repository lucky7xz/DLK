{"textgrid.poem.60467": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ragend steht der blinde F\u00fchrer", "tokens": ["Ra\u00b7gend", "steht", "der", "blin\u00b7de", "F\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziska dort auf seinem Wagen,", "tokens": ["Zis\u00b7ka", "dort", "auf", "sei\u00b7nem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Donnerstimme herrschend,", "tokens": ["Mit", "der", "Don\u00b7ner\u00b7stim\u00b7me", "herr\u00b7schend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die hei\u00dfe Schlacht zu schlagen.", "tokens": ["Wie", "die", "hei\u00b7\u00dfe", "Schlacht", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Steht ein Hauptmann ihm zur Linken", "tokens": ["Steht", "ein", "Haupt\u00b7mann", "ihm", "zur", "Lin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein andrer ihm zur Rechten,", "tokens": ["Und", "ein", "an\u00b7drer", "ihm", "zur", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PPER", "APPRART", "NN", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schildern ihm den Ort getreulich,", "tokens": ["Schil\u00b7dern", "ihm", "den", "Ort", "ge\u00b7treu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo es gilt, den Kampf zu fechten.", "tokens": ["Wo", "es", "gilt", ",", "den", "Kampf", "zu", "fech\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lager, Zahl und Zug der Feinde", "tokens": ["La\u00b7ger", ",", "Zahl", "und", "Zug", "der", "Fein\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Melden sie, da\u00df er befehle;", "tokens": ["Mel\u00b7den", "sie", ",", "da\u00df", "er", "be\u00b7feh\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles schaut er klar im Strahle", "tokens": ["Al\u00b7les", "schaut", "er", "klar", "im", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner lichten Feldherrnseele.", "tokens": ["Sei\u00b7ner", "lich\u00b7ten", "Feld\u00b7herrn\u00b7see\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "In den Tagen, eh der Pfeilschu\u00df", "tokens": ["In", "den", "Ta\u00b7gen", ",", "eh", "der", "Pfeil\u00b7schu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm geraubt das Augenlicht,", "tokens": ["Ihm", "ge\u00b7raubt", "das", "Au\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blickt' er scharf dem Vaterlande", "tokens": ["Blickt'", "er", "scharf", "dem", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ins geliebte Angesicht;", "tokens": ["Ins", "ge\u00b7lieb\u00b7te", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "All die W\u00e4lder, Str\u00f6m und Buchten,", "tokens": ["All", "die", "W\u00e4l\u00b7der", ",", "Str\u00f6m", "und", "Buch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Talgewind' und Bergesr\u00fccken", "tokens": ["Tal\u00b7ge\u00b7wind'", "und", "Ber\u00b7ge\u00b7sr\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eilt' er damals, dem Ged\u00e4chtnis", "tokens": ["Eilt'", "er", "da\u00b7mals", ",", "dem", "Ge\u00b7d\u00e4cht\u00b7nis"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unausl\u00f6schlich einzudr\u00fccken.", "tokens": ["Un\u00b7aus\u00b7l\u00f6schlich", "ein\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und der Genius der Rache", "tokens": ["Und", "der", "Ge\u00b7nius", "der", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00df im Finstern zu ersp\u00e4hen", "tokens": ["Wei\u00df", "im", "Fins\u00b7tern", "zu", "er\u00b7sp\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes Grundst\u00fcck, wo am besten", "tokens": ["Je\u00b7des", "Grund\u00b7st\u00fcck", ",", "wo", "am", "bes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PWAV", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feindesleichen hinzus\u00e4en.", "tokens": ["Fein\u00b7des\u00b7lei\u00b7chen", "hin\u00b7zu\u00b7s\u00e4\u00b7en", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dunkelt auch um Ziskas K\u00f6rper", "tokens": ["Dun\u00b7kelt", "auch", "um", "Zis\u00b7kas", "K\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefe, schimmerlose Nacht,", "tokens": ["Tie\u00b7fe", ",", "schim\u00b7mer\u00b7lo\u00b7se", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4ngelt er doch mit dem Geiste", "tokens": ["G\u00e4n\u00b7gelt", "er", "doch", "mit", "dem", "Geis\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Leicht sein wildes Kind, die Schlacht.", "tokens": ["Leicht", "sein", "wil\u00b7des", "Kind", ",", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "H\u00fcben lenkt die Nacht des Leibes,", "tokens": ["H\u00fc\u00b7ben", "lenkt", "die", "Nacht", "des", "Lei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcben Geistesnacht die Krieger;", "tokens": ["Dr\u00fc\u00b7ben", "Geis\u00b7tes\u00b7nacht", "die", "Krie\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch in keiner Schlacht bezwungen,", "tokens": ["Noch", "in", "kei\u00b7ner", "Schlacht", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt auch heute Ziska Sieger.", "tokens": ["Bleibt", "auch", "heu\u00b7te", "Zis\u00b7ka", "Sie\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ha! wie lauscht dem Kampf der Blinde!", "tokens": ["Ha", "!", "wie", "lauscht", "dem", "Kampf", "der", "Blin\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er erkennt im Sturm der Luft", "tokens": ["Er", "er\u00b7kennt", "im", "Sturm", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jede Waffe an der Stimme,", "tokens": ["Je\u00b7de", "Waf\u00b7fe", "an", "der", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie herbei den Tod sie ruft.", "tokens": ["Wie", "her\u00b7bei", "den", "Tod", "sie", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Wildharmonisch seinem Ohre", "tokens": ["Wild\u00b7har\u00b7mo\u00b7nisch", "sei\u00b7nem", "Oh\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauscht das Ringen zweier Heere,", "tokens": ["Rauscht", "das", "Rin\u00b7gen", "zwei\u00b7er", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Waffen, Schlachtruf, Ziskas Leiblied,", "tokens": ["Waf\u00b7fen", ",", "Schlach\u00b7truf", ",", "Zis\u00b7kas", "Leib\u00b7lied", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Hinsturz Mann und M\u00e4hre.", "tokens": ["Und", "im", "Hins\u00b7turz", "Mann", "und", "M\u00e4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "Freudig h\u00f6rt er, wie die Knechte", "tokens": ["Freu\u00b7dig", "h\u00f6rt", "er", ",", "wie", "die", "Knech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sigismunds hin\u00fcberfahren,", "tokens": ["Si\u00b7gis\u00b7munds", "hin\u00b7\u00fc\u00b7berf\u00b7ah\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "All die s\u00e4chsischen Geschwader", "tokens": ["All", "die", "s\u00e4ch\u00b7si\u00b7schen", "Ge\u00b7schwa\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Samt den ungrischen Husaren.", "tokens": ["Samt", "den", "un\u00b7gri\u00b7schen", "Hu\u00b7sa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.12": {"line.1": {"text": "Und dem wilden blinden Ziska", "tokens": ["Und", "dem", "wil\u00b7den", "blin\u00b7den", "Zis\u00b7ka"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht im Heldenrausch der Ohren", "tokens": ["Geht", "im", "Hel\u00b7den\u00b7rausch", "der", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die klare Feldherrnruhe", "tokens": ["Doch", "die", "kla\u00b7re", "Feld\u00b7herrn\u00b7ru\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Geistes nie verloren.", "tokens": ["Sei\u00b7nes", "Geis\u00b7tes", "nie", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}