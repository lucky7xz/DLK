{"textgrid.poem.46045": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ihr wart bei der Heinefetter]", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr wart bei der Heinefetter,", "tokens": ["Ihr", "wart", "bei", "der", "Hei\u00b7ne\u00b7fet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uns traf hier das Donnerwetter,", "tokens": ["Uns", "traf", "hier", "das", "Don\u00b7ner\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wir schrieben auf die Bretter:", "tokens": ["Und", "wir", "schrie\u00b7ben", "auf", "die", "Bret\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haltet hoch ihr guten G\u00f6tter,", "tokens": ["Hal\u00b7tet", "hoch", "ihr", "gu\u00b7ten", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So wie wir in Herz und Sinn,", "tokens": ["So", "wie", "wir", "in", "Herz", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Willemer und die Willemerin,", "tokens": ["Wil\u00b7le\u00b7mer", "und", "die", "Wil\u00b7le\u00b7me\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Deren Weine hier aus R\u00f6mern", "tokens": ["De\u00b7ren", "Wei\u00b7ne", "hier", "aus", "R\u00f6\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Brentano trank mit B\u00f6hmern.", "tokens": ["Der", "Bren\u00b7ta\u00b7no", "trank", "mit", "B\u00f6h\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Weil hier trank der Herr von Goethe,", "tokens": ["Weil", "hier", "trank", "der", "Herr", "von", "Goe\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Warn wir beide auch nicht bl\u00f6de,", "tokens": ["Warn", "wir", "bei\u00b7de", "auch", "nicht", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADV", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Fragt nur bei der Abendr\u00f6te!", "tokens": ["Fragt", "nur", "bei", "der", "A\u00b7bend\u00b7r\u00f6\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}