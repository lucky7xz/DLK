{"dta.poem.14250": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Entfernte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und so \u00f6ffnet' ich die schw\u00fcle", "tokens": ["Und", "so", "\u00f6ff\u00b7net'", "ich", "die", "schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brust aufathmend in der K\u00fchle!", "tokens": ["Brust", "auf\u00b7ath\u00b7mend", "in", "der", "K\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Locken fort aus Stirn und Wange,", "tokens": ["Lo\u00b7cken", "fort", "aus", "Stirn", "und", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Strom mich ganz umfange,", "tokens": ["Da\u00df", "der", "Strom", "mich", "ganz", "um\u00b7fan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frei das blaue Meer umsp\u00fcle,", "tokens": ["Frei", "das", "blau\u00b7e", "Meer", "um\u00b7sp\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit den Wolken, eilig fliehend,", "tokens": ["Mit", "den", "Wol\u00b7ken", ",", "ei\u00b7lig", "flie\u00b7hend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit der Str\u00f6me lichtem Gr\u00fc\u00dfen", "tokens": ["Mit", "der", "Str\u00f6\u00b7me", "lich\u00b7tem", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die Gedanken fr\u00f6hlich ziehend,", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", "fr\u00f6h\u00b7lich", "zie\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weit voraus vor Wolken, Fl\u00fcssen \u2014", "tokens": ["Weit", "vo\u00b7raus", "vor", "Wol\u00b7ken", ",", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ach! ich f\u00fchlte, da\u00df ich bl\u00fchend!", "tokens": ["Ach", "!", "ich", "f\u00fchl\u00b7te", ",", "da\u00df", "ich", "bl\u00fc\u00b7hend", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}