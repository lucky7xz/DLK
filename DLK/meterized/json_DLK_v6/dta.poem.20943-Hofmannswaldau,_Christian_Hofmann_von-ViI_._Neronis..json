{"dta.poem.20943": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "ViI .  \n Neronis.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Strang/ Feuer/ Spie\u00df und Schwerdt/ trug meine", "tokens": ["Strang", "/", "Feu\u00b7er", "/", "Spie\u00df", "und", "Schwerdt", "/", "trug", "mei\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "rechte Hand/", "tokens": ["rech\u00b7te", "Hand", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Viel tausend hat mein Wort in Plut\u00f6 Reich gesand.", "tokens": ["Viel", "tau\u00b7send", "hat", "mein", "Wort", "in", "Plu\u00b7t\u00f6", "Reich", "ge\u00b7sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPOSAT", "NN", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht tadelt mich darum: Sie solten ihm vermelden/", "tokens": ["Nicht", "ta\u00b7delt", "mich", "da\u00b7rum", ":", "Sie", "sol\u00b7ten", "ihm", "ver\u00b7mel\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "PAV", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df er Bereitschafft macht f\u00fcr einen grossen Hel-", "tokens": ["Da\u00df", "er", "Be\u00b7reit\u00b7schafft", "macht", "f\u00fcr", "ei\u00b7nen", "gros\u00b7sen", "Hel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "den.", "tokens": ["den", "."], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}}}}}