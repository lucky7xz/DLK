{"textgrid.poem.57169": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geschlossenen Auges la\u00df mich gehn,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geschlossenen Auges la\u00df mich gehn,", "tokens": ["Ge\u00b7schlos\u00b7se\u00b7nen", "Au\u00b7ges", "la\u00df", "mich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "mein Schicksal,", "tokens": ["mein", "Schick\u00b7sal", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "bis der Tag vor\u00fcber,", "tokens": ["bis", "der", "Tag", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "der tr\u00fcb und tr\u00fcber", "tokens": ["der", "tr\u00fcb", "und", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "sich umzieht.", "tokens": ["sich", "um\u00b7zieht", "."], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.6": {"text": "Nicht sehn,", "tokens": ["Nicht", "sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "nicht h\u00f6ren!", "tokens": ["nicht", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Wie die Maske sieht", "tokens": ["Wie", "die", "Mas\u00b7ke", "sieht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "aus leeren L\u00f6chern", "tokens": ["aus", "lee\u00b7ren", "L\u00f6\u00b7chern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "und den Wogenschall", "tokens": ["und", "den", "Wo\u00b7gen\u00b7schall"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "die Muschel f\u00e4ngt,", "tokens": ["die", "Mu\u00b7schel", "f\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "nur so noch la\u00df mein Leben sein,", "tokens": ["nur", "so", "noch", "la\u00df", "mein", "Le\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVIMP", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "indes", "tokens": ["in\u00b7des"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "die Seele tief in Schlummer liegen mag,", "tokens": ["die", "See\u00b7le", "tief", "in", "Schlum\u00b7mer", "lie\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "bis sie ein be\u00dfrer Tag", "tokens": ["bis", "sie", "ein", "be\u00df\u00b7rer", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "zu neuem Bl\u00fchen", "tokens": ["zu", "neu\u00b7em", "Bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "dr\u00e4ngt.", "tokens": ["dr\u00e4ngt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}