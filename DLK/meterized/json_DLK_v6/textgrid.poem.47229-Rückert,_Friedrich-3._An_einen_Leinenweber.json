{"textgrid.poem.47229": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3. An einen Leinenweber", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckt bist du, der du mit stetem Flei\u00dfe", "tokens": ["Be\u00b7gl\u00fcckt", "bist", "du", ",", "der", "du", "mit", "ste\u00b7tem", "Flei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von der zufriednen Spule still und eben", "tokens": ["Von", "der", "zu\u00b7fried\u00b7nen", "Spu\u00b7le", "still", "und", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In deine Weberei wie in dein Leben", "tokens": ["In", "dei\u00b7ne", "We\u00b7be\u00b7rei", "wie", "in", "dein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stets gleiche F\u00e4den wirkest, ruhig wei\u00dfe;", "tokens": ["Stets", "glei\u00b7che", "F\u00e4\u00b7den", "wir\u00b7kest", ",", "ru\u00b7hig", "wei\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da t\u00e4glich ich ein bunt Geweb' zerrei\u00dfe,", "tokens": ["Da", "t\u00e4g\u00b7lich", "ich", "ein", "bunt", "Ge\u00b7web'", "zer\u00b7rei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Drang, es morgen bunter noch zu weben;", "tokens": ["Im", "Drang", ",", "es", "mor\u00b7gen", "bun\u00b7ter", "noch", "zu", "we\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "ADV", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dazu mu\u00df Hoffnung gr\u00fcne F\u00e4den geben,", "tokens": ["Da\u00b7zu", "mu\u00df", "Hoff\u00b7nung", "gr\u00fc\u00b7ne", "F\u00e4\u00b7den", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebe gibt das Rot dazu, das hei\u00dfe.", "tokens": ["Die", "Lie\u00b7be", "gibt", "das", "Rot", "da\u00b7zu", ",", "das", "hei\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PAV", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dazwischen schlinget sich ein Schillerstreifen,", "tokens": ["Da\u00b7zwi\u00b7schen", "schlin\u00b7get", "sich", "ein", "Schil\u00b7ler\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Poesie, bald wie ein Sonnenflitter", "tokens": ["Die", "Poe\u00b7sie", ",", "bald", "wie", "ein", "Son\u00b7nen\u00b7flit\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "KOKOM", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Goldgelb, bald wie ein \u00c4therblick, ein blauer.", "tokens": ["Gold\u00b7gelb", ",", "bald", "wie", "ein", "\u00c4\u00b7ther\u00b7blick", ",", "ein", "blau\u00b7er", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "KOKOM", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wie die F\u00e4den ineinander greifen,", "tokens": ["Doch", "wie", "die", "F\u00e4\u00b7den", "in\u00b7ein\u00b7an\u00b7der", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bricht doch, ged\u00e4mpft durchs bunte Farbengitter,", "tokens": ["Bricht", "doch", ",", "ge\u00b7d\u00e4mpft", "durchs", "bun\u00b7te", "Far\u00b7ben\u00b7git\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Hervor der Grund der Wehe, dunkle Trauer.", "tokens": ["Her\u00b7vor", "der", "Grund", "der", "We\u00b7he", ",", "dunk\u00b7le", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}