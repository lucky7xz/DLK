{"dta.poem.14475": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Gest\u00fcrzt sind die gold'nen Br\u00fccken", "tokens": ["Ge\u00b7st\u00fcrzt", "sind", "die", "gold'\u00b7nen", "Br\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und unten und oben so still!", "tokens": ["Und", "un\u00b7ten", "und", "o\u00b7ben", "so", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es will mir nichts mehr gl\u00fccken,", "tokens": ["Es", "will", "mir", "nichts", "mehr", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wei\u00df nicht mehr, was ich will.", "tokens": ["Ich", "wei\u00df", "nicht", "mehr", ",", "was", "ich", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}