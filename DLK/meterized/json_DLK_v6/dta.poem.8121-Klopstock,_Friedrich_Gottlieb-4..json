{"dta.poem.8121": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Gott r\u00e4cht, in Gestirnglanz, Gl\u00fckselige,", "tokens": ["Die", "Gott", "r\u00e4cht", ",", "in", "Ge\u00b7stirn\u00b7glanz", ",", "Gl\u00fck\u00b7se\u00b7li\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "$,", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In des Heils Kleid, ausduldende M\u00e4rtyrer,", "tokens": ["In", "des", "Heils", "Kleid", ",", "aus\u00b7dul\u00b7den\u00b7de", "M\u00e4r\u00b7ty\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--++-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Zu dem Erb\u2019 in dem Lichtreich, kommt freudig ihr,", "tokens": ["Zu", "dem", "Erb'", "in", "dem", "Lich\u00b7treich", ",", "kommt", "freu\u00b7dig", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "ADJD", "PPER", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Die Gott r\u00e4cht, von dem Nachtthal her!", "tokens": ["Die", "Gott", "r\u00e4cht", ",", "von", "dem", "Nachtt\u00b7hal", "her", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}}}}