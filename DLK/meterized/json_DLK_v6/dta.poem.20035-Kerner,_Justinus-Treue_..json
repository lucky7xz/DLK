{"dta.poem.20035": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Treue .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erde ist nur fr\u00f6hlich,", "tokens": ["Die", "Er\u00b7de", "ist", "nur", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wann froh der Himmel blickt,", "tokens": ["Wann", "froh", "der", "Him\u00b7mel", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schnell dann mit bunten Blumen", "tokens": ["Schnell", "dann", "mit", "bun\u00b7ten", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie Haupt und Busen schm\u00fcckt;", "tokens": ["Sie", "Haupt", "und", "Bu\u00b7sen", "schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann t\u00f6nt aus ihrem Munde", "tokens": ["Dann", "t\u00f6nt", "aus", "ih\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So mancher Wonnelaut;", "tokens": ["So", "man\u00b7cher", "Won\u00b7ne\u00b7laut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie fliegt in schnellen T\u00e4nzen", "tokens": ["Sie", "fliegt", "in", "schnel\u00b7len", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie eine junge Braut.", "tokens": ["Wie", "ei\u00b7ne", "jun\u00b7ge", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch blickt, voll d\u00fcstrer Wolken,", "tokens": ["Doch", "blickt", ",", "voll", "d\u00fcst\u00b7rer", "Wol\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Himmel ernst und kalt,", "tokens": ["Der", "Him\u00b7mel", "ernst", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rei\u00dft sie von Haupt und Busen", "tokens": ["Rei\u00dft", "sie", "von", "Haupt", "und", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die bunten Blumen bald,", "tokens": ["Die", "bun\u00b7ten", "Blu\u00b7men", "bald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie zieht den Trauerschleier", "tokens": ["Sie", "zieht", "den", "Trau\u00b7er\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um's Angesicht zur Stund';", "tokens": ["Um's", "An\u00b7ge\u00b7sicht", "zur", "Stund'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es t\u00f6nt kein Laut der Freude", "tokens": ["Es", "t\u00f6nt", "kein", "Laut", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus ihrem bleichen Mund!", "tokens": ["Aus", "ih\u00b7rem", "blei\u00b7chen", "Mund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}