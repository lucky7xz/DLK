{"textgrid.poem.58489": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "527. Ach w\u00e4r', ach w\u00e4r'!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach w\u00e4r', ach w\u00e4r'", "tokens": ["Ach", "w\u00e4r'", ",", "ach", "w\u00e4r'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "VAFIN", "$,", "XY", "XY"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mein Herz von allem leer!", "tokens": ["Mein", "Herz", "von", "al\u00b7lem", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt ist mir zu schlecht,", "tokens": ["Die", "Welt", "ist", "mir", "zu", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nichts, nirgends ist mir's recht,", "tokens": ["Nichts", ",", "nir\u00b7gends", "ist", "mir's", "recht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "VAFIN", "NE", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich kann nicht ruhn bei Gaben,", "tokens": ["Ich", "kann", "nicht", "ruhn", "bei", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich soll den Geber haben;", "tokens": ["Ich", "soll", "den", "Ge\u00b7ber", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach w\u00e4r', ach w\u00e4r'", "tokens": ["Ach", "w\u00e4r'", ",", "ach", "w\u00e4r'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "VAFIN", "$,", "XY", "XY"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ich nur von allem leer!", "tokens": ["Ich", "nur", "von", "al\u00b7lem", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}