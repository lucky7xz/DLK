{"dta.poem.2666": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  Hemerocallis  oder Lilie von Calvarien  \n  Bey Absterben Hn. J. H.   D.   z.   E.   j\u00fcngsten  \n T\u00f6chterlein den 23. Decembr. 1674.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich dich auff die Sch\u00e4del-st\u00e4te/", "tokens": ["Seh'", "ich", "dich", "auff", "die", "Sch\u00e4\u00b7del\u00b7st\u00e4\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du zartes Kind/ schon hingelegt/", "tokens": ["Du", "zar\u00b7tes", "Kind", "/", "schon", "hin\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie dein Tauff- und Leich", "tokens": ["Und", "wie", "dein", "Tauff", "und", "Leich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "TRUNC", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Fast einerley Bedeutung tr\u00e4gt!", "tokens": ["Fast", "ei\u00b7ner\u00b7ley", "Be\u00b7deu\u00b7tung", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Wester-Hembde war ein Zeichen/", "tokens": ["Das", "Wes\u00b7ter\u00b7Hemb\u00b7de", "war", "ein", "Zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df du numehr von S\u00fcnden rein;", "tokens": ["Da\u00df", "du", "nu\u00b7mehr", "von", "S\u00fcn\u00b7den", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die letzte Kleidung deiner Leichen", "tokens": ["Die", "letz\u00b7te", "Klei\u00b7dung", "dei\u00b7ner", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wird auch ein gleicher Zeuge seyn.", "tokens": ["Wird", "auch", "ein", "glei\u00b7cher", "Zeu\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So mu\u00df ich dich vor Lilgen halten/", "tokens": ["So", "mu\u00df", "ich", "dich", "vor", "Lil\u00b7gen", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die auff den Sch\u00e4del-st\u00e4ten stehn/", "tokens": ["Die", "auff", "den", "Sch\u00e4\u00b7del\u00b7st\u00e4\u00b7ten", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einem Tage bl\u00fchn und alten/", "tokens": ["In", "ei\u00b7nem", "Ta\u00b7ge", "bl\u00fchn", "und", "al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "KON", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit der Sonnen untergehn:", "tokens": ["Und", "mit", "der", "Son\u00b7nen", "un\u00b7ter\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr\u2019 Anmuth straalt im h\u00f6chsten prangen/", "tokens": ["Ih\u00b7r'", "An\u00b7muth", "stra\u00b7alt", "im", "h\u00f6chs\u00b7ten", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "VVFIN", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Wenn fr\u00fch\u2019 der g\u00f6ldne Tag erwacht;", "tokens": ["Wenn", "fr\u00fch'", "der", "g\u00f6ld\u00b7ne", "Tag", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Schmuck und Blume sind vergangen/", "tokens": ["Und", "Schmuck", "und", "Blu\u00b7me", "sind", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So b\u00e4ld sich br\u00e4unt die schwartze Nacht.", "tokens": ["So", "b\u00e4ld", "sich", "br\u00e4unt", "die", "schwart\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wie die sch\u00f6nen Farben mahlen", "tokens": ["Doch", "wie", "die", "sch\u00f6\u00b7nen", "Far\u00b7ben", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der edlen Blumen Treffligkeit/", "tokens": ["Der", "ed\u00b7len", "Blu\u00b7men", "Tref\u00b7flig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So f\u00fcnckelt mit nicht mindern Stralen/", "tokens": ["So", "f\u00fcn\u00b7ckelt", "mit", "nicht", "min\u00b7dern", "Stra\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PTKNEG", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verblastes Kind/ dein Sterbe-Kleid;", "tokens": ["Ver\u00b7blas\u00b7tes", "Kind", "/", "dein", "Ster\u00b7be\u00b7Kleid", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Lilgen sind erst Weis zuschauen/", "tokens": ["Die", "Lil\u00b7gen", "sind", "erst", "Weis", "zu\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du auch/ nachdem das heil\u2019ge Bad", "tokens": ["Du", "auch", "/", "nach\u00b7dem", "das", "heil'\u00b7ge", "Bad"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$(", "KOUS", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit dem gesegneten Bethauen", "tokens": ["Mit", "dem", "ge\u00b7se\u00b7gne\u00b7ten", "Be\u00b7thau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Von S\u00fcnden dich gewaschen hat.", "tokens": ["Von", "S\u00fcn\u00b7den", "dich", "ge\u00b7wa\u00b7schen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da bist du schon ein Mit-Glied worden", "tokens": ["Da", "bist", "du", "schon", "ein", "Mit\u00b7Glied", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Heilands/ der dich hertzlich liebt/", "tokens": ["Des", "Hei\u00b7lands", "/", "der", "dich", "hertz\u00b7lich", "liebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und vor des Teuffels List und Morden", "tokens": ["Und", "vor", "des", "Teuf\u00b7fels", "List", "und", "Mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Erbtheil dir den Himmel giebt/", "tokens": ["Sein", "E\u00b7rbtheil", "dir", "den", "Him\u00b7mel", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja dich zu GOttes Tempel machet/", "tokens": ["Ja", "dich", "zu", "Got\u00b7tes", "Tem\u00b7pel", "ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ins Buch des Lebens schreibet ein/", "tokens": ["Ins", "Buch", "des", "Le\u00b7bens", "schrei\u00b7bet", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df wenn gleich Erd\u2019 und Himmel krachet/", "tokens": ["Da\u00df", "wenn", "gleich", "Erd'", "und", "Him\u00b7mel", "kra\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du kanst des Himmels Erbin seyn.", "tokens": ["Du", "kanst", "des", "Him\u00b7mels", "Er\u00b7bin", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es gl\u00e4ntzen auch ber\u00fchmt Lilgen", "tokens": ["Es", "gl\u00e4nt\u00b7zen", "auch", "be\u00b7r\u00fchmt", "Lil\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In einer rothen Purpur-Gluth.", "tokens": ["In", "ei\u00b7ner", "ro\u00b7then", "Pur\u00b7pur\u00b7Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was konte deine Flecken tilgen/", "tokens": ["Was", "kon\u00b7te", "dei\u00b7ne", "Fle\u00b7cken", "til\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als CHristus roth-vergo\u00dfnes Blut?", "tokens": ["Als", "Chris\u00b7tus", "ro\u00b7th\u00b7ver\u00b7go\u00df\u00b7nes", "Blut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Durch di\u00df bist du so rein gewaschen/", "tokens": ["Durch", "di\u00df", "bist", "du", "so", "rein", "ge\u00b7wa\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als wol ein unbetretner Schnee.", "tokens": ["Als", "wol", "ein", "un\u00b7be\u00b7tret\u00b7ner", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Di\u00df f\u00fchrt dich/ nun der Leib ist Aschen/", "tokens": ["Di\u00df", "f\u00fchrt", "dich", "/", "nun", "der", "Leib", "ist", "A\u00b7schen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "ADV", "ART", "NN", "VAFIN", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auch zu der Licht-gestirnten H\u00f6h\u2019.", "tokens": ["Auch", "zu", "der", "Licht\u00b7ge\u00b7stirn\u00b7ten", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zuweilen lassen sich im Golde", "tokens": ["Zu\u00b7wei\u00b7len", "las\u00b7sen", "sich", "im", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Deutungs-volle Lilgen sehn.", "tokens": ["Die", "Deu\u00b7tungs\u00b7vol\u00b7le", "Lil\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "met