{"textgrid.poem.24165": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1. An Frau Malgonia Stern", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr vom Kiebitzhof ist ein bescheidener Grande;", "tokens": ["Der", "Herr", "vom", "Kie\u00b7bitz\u00b7hof", "ist", "ein", "be\u00b7schei\u00b7de\u00b7ner", "Gran\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Man sieht ihn meist im schlichtesten Gewande;", "tokens": ["Man", "sieht", "ihn", "meist", "im", "schlich\u00b7tes\u00b7ten", "Ge\u00b7wan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das macht: er giebt nicht viel auf \u00e4u\u00dferlichen Staat.", "tokens": ["Das", "macht", ":", "er", "giebt", "nicht", "viel", "auf", "\u00e4u\u00b7\u00dfer\u00b7li\u00b7chen", "Staat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch giebt es F\u00e4lle, seltene, mu\u00df ich sagen,", "tokens": ["Doch", "giebt", "es", "F\u00e4l\u00b7le", ",", "sel\u00b7te\u00b7ne", ",", "mu\u00df", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "ADJA", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da wei\u00df sich Krazi wie ein Lord zu tragen,", "tokens": ["Da", "wei\u00df", "sich", "Kra\u00b7zi", "wie", "ein", "Lord", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NE", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Thut an ein M\u00e4ntelchen von goldenem Brokat.", "tokens": ["Thut", "an", "ein", "M\u00e4n\u00b7tel\u00b7chen", "von", "gol\u00b7de\u00b7nem", "Bro\u00b7kat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So hier zu Ehrn", "tokens": ["So", "hier", "zu", "Ehrn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Der Frau Malgonia Stern.", "tokens": ["Der", "Frau", "Mal\u00b7go\u00b7nia", "Stern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}