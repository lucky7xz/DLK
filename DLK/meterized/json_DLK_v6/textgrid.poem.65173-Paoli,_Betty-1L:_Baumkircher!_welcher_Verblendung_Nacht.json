{"textgrid.poem.65173": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Baumkircher! welcher Verblendung Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Baumkircher! welcher Verblendung Nacht", "tokens": ["Baum\u00b7kir\u00b7cher", "!", "wel\u00b7cher", "Ver\u00b7blen\u00b7dung", "Nacht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PWAT", "NN", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Hielt dir die Sinne umwoben,", "tokens": ["Hielt", "dir", "die", "Sin\u00b7ne", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Als du, der Sieger in mancher Schlacht,", "tokens": ["Als", "du", ",", "der", "Sie\u00b7ger", "in", "man\u00b7cher", "Schlacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Aufruhrs Fahne erhoben?", "tokens": ["Des", "Auf\u00b7ruhrs", "Fah\u00b7ne", "er\u00b7ho\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Als du, f\u00fcr kurzer Rache Gewinn,", "tokens": ["Als", "du", ",", "f\u00fcr", "kur\u00b7zer", "Ra\u00b7che", "Ge\u00b7winn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Den Feind gew\u00e4hlt zum Genossen,", "tokens": ["Den", "Feind", "ge\u00b7w\u00e4hlt", "zum", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und mit dem Ungark\u00f6nig Corvin", "tokens": ["Und", "mit", "dem", "Un\u00b7gar\u00b7k\u00f6\u00b7nig", "Cor\u00b7vin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ein frevles B\u00fcndnis geschlossen?!", "tokens": ["Ein", "frev\u00b7les", "B\u00fcnd\u00b7nis", "ge\u00b7schlos\u00b7sen", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Unseliger du! trotz Acht und Bann", "tokens": ["Un\u00b7se\u00b7li\u00b7ger", "du", "!", "trotz", "Acht", "und", "Bann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "$.", "APPR", "CARD", "KON", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Des Rachewerkes beflissen! \u2013", "tokens": ["Des", "Ra\u00b7che\u00b7wer\u00b7kes", "be\u00b7flis\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hans Stubenberg, seinen Tochtermann,", "tokens": ["Hans", "Stu\u00b7ben\u00b7berg", ",", "sei\u00b7nen", "Toch\u00b7ter\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat er mit sich fortgerissen,", "tokens": ["Hat", "er", "mit", "sich", "fort\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die m\u00e4cht'gen Herren von Liechtenstein", "tokens": ["Die", "m\u00e4cht'\u00b7gen", "Her\u00b7ren", "von", "Liech\u00b7ten\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie stehn zu Baumkirchers Fahne, \u2013", "tokens": ["Sie", "stehn", "zu", "Baum\u00b7kir\u00b7chers", "Fah\u00b7ne", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Zum Heer verdichten sich seine Reih'n,", "tokens": ["Zum", "Heer", "ver\u00b7dich\u00b7ten", "sich", "sei\u00b7ne", "Reih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Sturm schwillt an zum Orkane! \u2013", "tokens": ["Der", "Sturm", "schwillt", "an", "zum", "Or\u00b7ka\u00b7ne", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "In Rom, wo er dem Papste sich neigt,", "tokens": ["In", "Rom", ",", "wo", "er", "dem", "Paps\u00b7te", "sich", "neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "PPER", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Erreicht den Kaiser die Kunde,", "tokens": ["Er\u00b7reicht", "den", "Kai\u00b7ser", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und als er sie vernommen, besteigt", "tokens": ["Und", "als", "er", "sie", "ver\u00b7nom\u00b7men", ",", "be\u00b7steigt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "$,", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sein Pferd er zur selben Stunde.", "tokens": ["Sein", "Pferd", "er", "zur", "sel\u00b7ben", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er spricht kein Wort, er hat keinen Blick", "tokens": ["Er", "spricht", "kein", "Wort", ",", "er", "hat", "kei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "F\u00fcr Welschlands Sch\u00f6nheit im Lenze;", "tokens": ["F\u00fcr", "Wel\u00b7schlands", "Sch\u00f6n\u00b7heit", "im", "Len\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Im Fluge geht's nach Deutschland zur\u00fcck,", "tokens": ["Im", "Flu\u00b7ge", "geht's", "nach", "Deutschland", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis \u00fcberschritten die Grenze.", "tokens": ["Bis", "\u00fc\u00b7bersc\u00b7hrit\u00b7ten", "die", "Gren\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nicht l\u00e4nger soll der Emp\u00f6rung Graus", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "soll", "der", "Em\u00b7p\u00f6\u00b7rung", "Graus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Herzen des Reiches walten!", "tokens": ["Im", "Her\u00b7zen", "des", "Rei\u00b7ches", "wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er schreibt in Eil' einen Landtag aus,", "tokens": ["Er", "schreibt", "in", "Eil'", "ei\u00b7nen", "Land\u00b7tag", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu V\u00f6lkermarkt abzuhalten.", "tokens": ["Zu", "V\u00f6l\u00b7ker\u00b7markt", "ab\u00b7zu\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dem Landtag halten sich kl\u00fcglich fern,", "tokens": ["Dem", "Land\u00b7tag", "hal\u00b7ten", "sich", "kl\u00fcg\u00b7lich", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die gegen Friedrich in Waffen,", "tokens": ["Die", "ge\u00b7gen", "Fried\u00b7rich", "in", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch auch die ihm treu geblieb'nen Herr'n,", "tokens": ["Doch", "auch", "die", "ihm", "treu", "ge\u00b7blie\u00b7b'\u00b7nen", "Herr'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Sie k\u00f6nnen nicht Hilfe schaffen.", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nicht", "Hil\u00b7fe", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbdie L\u00e4nder verw\u00fcstet, weit und breit,", "tokens": ["\u00bb", "die", "L\u00e4n\u00b7der", "ver\u00b7w\u00fcs\u00b7tet", ",", "weit", "und", "breit", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Grenzen von Feinden starrend,", "tokens": ["Die", "Gren\u00b7zen", "von", "Fein\u00b7den", "star\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die S\u00f6ldnertruppen seit langer Zeit", "tokens": ["Die", "S\u00f6ld\u00b7ner\u00b7trup\u00b7pen", "seit", "lan\u00b7ger", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vergeblich auf L\u00f6hnung harrend,", "tokens": ["Ver\u00b7geb\u00b7lich", "auf", "L\u00f6h\u00b7nung", "har\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vom Brand ergriffen das eig'ne Haus, \u2013", "tokens": ["Vom", "Brand", "er\u00b7grif\u00b7fen", "das", "eig'\u00b7ne", "Haus", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVPP", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da ist kein Rat zu ersinnen,", "tokens": ["Da", "ist", "kein", "Rat", "zu", "er\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Als: gleicht Euch mit den Rebellen aus,", "tokens": ["Als", ":", "gleicht", "Euch", "mit", "den", "Re\u00b7bel\u00b7len", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und trachtet sie zu gewinnen.\u00ab \u2013", "tokens": ["Und", "trach\u00b7tet", "sie", "zu", "ge\u00b7win\u00b7nen", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Da\u00df nicht ohnm\u00e4chtigen Zornes Qual", "tokens": ["Da\u00df", "nicht", "ohn\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Zor\u00b7nes", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Eis seines Stolzes schmelze,", "tokens": ["Das", "Eis", "sei\u00b7nes", "Stol\u00b7zes", "schmel\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verl\u00e4\u00dft der Kaiser schweigend den Saal", "tokens": ["Ver\u00b7l\u00e4\u00dft", "der", "Kai\u00b7ser", "schwei\u00b7gend", "den", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und wandelt nach dem Geh\u00f6lze.", "tokens": ["Und", "wan\u00b7delt", "nach", "dem", "Ge\u00b7h\u00f6l\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verstohlen folgt ihm auf seinem Pfad", "tokens": ["Ver\u00b7stoh\u00b7len", "folgt", "ihm", "auf", "sei\u00b7nem", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein M\u00e4nnlein mit wei\u00dfen Haaren,", "tokens": ["Ein", "M\u00e4nn\u00b7lein", "mit", "wei\u00b7\u00dfen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Herr Puchau, sein vielvertrauter Rat,", "tokens": ["Herr", "Puc\u00b7hau", ",", "sein", "viel\u00b7ver\u00b7trau\u00b7ter", "Rat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "In allen R\u00e4nken erfahren.", "tokens": ["In", "al\u00b7len", "R\u00e4n\u00b7ken", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Rings Stille, so tief, so fr\u00fchlingsklar!", "tokens": ["Rings", "Stil\u00b7le", ",", "so", "tief", ",", "so", "fr\u00fch\u00b7lings\u00b7klar", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Holze pickt nur der H\u00e4her.", "tokens": ["Im", "Hol\u00b7ze", "pickt", "nur", "der", "H\u00e4\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Kaiser wird den Alten gewahr", "tokens": ["Der", "Kai\u00b7ser", "wird", "den", "Al\u00b7ten", "ge\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und winkt ihm gebietend: N\u00e4her!", "tokens": ["Und", "winkt", "ihm", "ge\u00b7bie\u00b7tend", ":", "N\u00e4\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "$.", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er spricht, \u2013 o wie vom verhaltenen Groll", "tokens": ["Er", "spricht", ",", "\u2013", "o", "wie", "vom", "ver\u00b7hal\u00b7te\u00b7nen", "Groll"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "$(", "FM", "KOKOM", "APPRART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Lippen ihm fahl erbleichen!", "tokens": ["Die", "Lip\u00b7pen", "ihm", "fahl", "er\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00bbvernahmst du den guten Rat? Ich soll", "tokens": ["\u00bb", "ver\u00b7nahmst", "du", "den", "gu\u00b7ten", "Rat", "?", "Ich", "soll"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mich mit Rebellen vergleichen!\u00ab", "tokens": ["Mich", "mit", "Re\u00b7bel\u00b7len", "ver\u00b7glei\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$.", "$("], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Ein schwer unterdr\u00fcckter Ha\u00df erglimmt", "tokens": ["Ein", "schwer", "un\u00b7ter\u00b7dr\u00fcck\u00b7ter", "Ha\u00df", "er\u00b7glimmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Im Aug' des alten Gesellen:", "tokens": ["Im", "Aug'", "des", "al\u00b7ten", "Ge\u00b7sel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbmein gn\u00e4digster Herr! 's ist, wie man's nimmt!", "tokens": ["\u00bb", "mein", "gn\u00e4\u00b7digs\u00b7ter", "Herr", "!", "'s", "ist", ",", "wie", "man's", "nimmt", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$