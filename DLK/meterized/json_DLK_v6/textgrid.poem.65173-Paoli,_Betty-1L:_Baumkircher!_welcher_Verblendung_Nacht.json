text": "Ihn trieb erlitt'ne Kr\u00e4nkung allein", "tokens": ["Ihn", "trieb", "er\u00b7litt'\u00b7ne", "Kr\u00e4n\u00b7kung", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Die Majest\u00e4t zu beleid'gen; \u2013", "tokens": ["Die", "Ma\u00b7jes\u00b7t\u00e4t", "zu", "be\u00b7lei\u00b7d'\u00b7gen", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-++-++-", "measure": "unknown.measure.penta"}, "line.7": {"text": "So ruft ihn an Euern Hof, um sein", "tokens": ["So", "ruft", "ihn", "an", "Eu\u00b7ern", "Hof", ",", "um", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "KOUI", "PPOSAT"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Angeblich Recht zu verteid'gen!", "tokens": ["An\u00b7ge\u00b7blich", "Recht", "zu", "ver\u00b7tei\u00b7d'\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.11": {"line.1": {"text": "Das ist mein Rat, Herr! kurz und schlicht.", "tokens": ["Das", "ist", "mein", "Rat", ",", "Herr", "!", "kurz", "und", "schlicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "NN", "$.", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seht selber zu, ob er tauge.\u00ab", "tokens": ["Seht", "sel\u00b7ber", "zu", ",", "ob", "er", "tau\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als traue er seinen Sinnen nicht,", "tokens": ["Als", "trau\u00b7e", "er", "sei\u00b7nen", "Sin\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hebt rasch der Kaiser das Auge.", "tokens": ["Hebt", "rasch", "der", "Kai\u00b7ser", "das", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Er steht, von dem Gedanken erschreckt,", "tokens": ["Er", "steht", ",", "von", "dem", "Ge\u00b7dan\u00b7ken", "er\u00b7schreckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Der jetzt in ihm aufgegangen,", "tokens": ["Der", "jetzt", "in", "ihm", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und eine dunkle R\u00f6te bedeckt", "tokens": ["Und", "ei\u00b7ne", "dunk\u00b7le", "R\u00f6\u00b7te", "be\u00b7deckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Die erst noch so bleichen Wangen.", "tokens": ["Die", "erst", "noch", "so", "blei\u00b7chen", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbnein!\u00ab murmelt er, \u00bbnichts, o nichts davon!", "tokens": ["\u00bb", "nein", "!", "\u00ab", "mur\u00b7melt", "er", ",", "\u00bb", "nichts", ",", "o", "nichts", "da\u00b7von", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "$(", "VVFIN", "PPER", "$,", "$(", "PIS", "$,", "FM", "PIS", "PAV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Willst Gift in's Herz du mir streuen?", "tokens": ["Willst", "Gift", "in's", "Herz", "du", "mir", "streu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPRART", "NN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Baumkircher hat mir und meinem Thron", "tokens": ["Baum\u00b7kir\u00b7cher", "hat", "mir", "und", "mei\u00b7nem", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "KON", "PPOSAT", "NN"], "meter": "++--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Durch Jahre gedient in Treuen!\u00ab \u2013", "tokens": ["Durch", "Jah\u00b7re", "ge\u00b7dient", "in", "Treu\u00b7en", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "NN", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u00bbl\u00e4ngst hat sein Verrat das wett gemacht,\u00ab", "tokens": ["\u00bb", "l\u00e4ngst", "hat", "sein", "Ver\u00b7rat", "das", "wett", "ge\u00b7macht", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Spricht jener, geb\u00fcckt zur Erde.", "tokens": ["Spricht", "je\u00b7ner", ",", "ge\u00b7b\u00fcckt", "zur", "Er\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "VVPP", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00bbjetzt, gn\u00e4digster Herr, seid nur bedacht,", "tokens": ["\u00bb", "jetzt", ",", "gn\u00e4\u00b7digs\u00b7ter", "Herr", ",", "seid", "nur", "be\u00b7dacht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ADJA", "NN", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Da\u00df wirklich sein Recht ihm werde.\u00ab", "tokens": ["Da\u00df", "wirk\u00b7lich", "sein", "Recht", "ihm", "wer\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "PPER", "VAFIN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Zu schlichten den arg verworr'nen Streit,", "tokens": ["Zu", "schlich\u00b7ten", "den", "arg", "ver\u00b7worr'\u00b7nen", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Soll selbst er zu Graz erscheinen.", "tokens": ["Soll", "selbst", "er", "zu", "Graz", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Entbietet ihn! gebt ihm frei Geleit!", "tokens": ["Ent\u00b7bie\u00b7tet", "ihn", "!", "gebt", "ihm", "frei", "Ge\u00b7leit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So geht es wohl, sollt' ich meinen.\u00ab", "tokens": ["So", "geht", "es", "wohl", ",", "sollt'", "ich", "mei\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VMFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Scheu wendet der Kaiser das Gesicht,", "tokens": ["Scheu", "wen\u00b7det", "der", "Kai\u00b7ser", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Er fl\u00fcstert bang und beklommen:", "tokens": ["Er", "fl\u00fcs\u00b7tert", "bang", "und", "be\u00b7klom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00bbund wenn er nicht k\u00e4me?\u00ab Puchau spricht:", "tokens": ["\u00bb", "und", "wenn", "er", "nicht", "k\u00e4\u00b7me", "?", "\u00ab", "Puc\u00b7hau", "spricht", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "PTKNEG", "VVFIN", "$.", "$(", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "\u00bbseid ruhig, Herr! er wird kommen!\u00ab", "tokens": ["\u00bb", "seid", "ru\u00b7hig", ",", "Herr", "!", "er", "wird", "kom\u00b7men", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "A