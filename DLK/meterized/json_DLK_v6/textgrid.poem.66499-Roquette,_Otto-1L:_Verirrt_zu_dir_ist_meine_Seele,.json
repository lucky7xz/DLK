{"textgrid.poem.66499": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verirrt zu dir ist meine Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verirrt zu dir ist meine Seele,", "tokens": ["Ver\u00b7irrt", "zu", "dir", "ist", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wei\u00df doch, da\u00df sie nur verirrt,", "tokens": ["Und", "wei\u00df", "doch", ",", "da\u00df", "sie", "nur", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie eine fortgewehte Bl\u00fcthe,", "tokens": ["Wie", "ei\u00b7ne", "fort\u00b7ge\u00b7weh\u00b7te", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ewig fremd dir im Gem\u00fcthe,", "tokens": ["Und", "e\u00b7wig", "fremd", "dir", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Umsonst die Heimat suchen wird.", "tokens": ["Um\u00b7sonst", "die", "Hei\u00b7mat", "su\u00b7chen", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vergeblich, da\u00df ich mir verhehle,", "tokens": ["Ver\u00b7geb\u00b7lich", ",", "da\u00df", "ich", "mir", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie du nicht ahnst, was um dich ringt!", "tokens": ["Wie", "du", "nicht", "ahnst", ",", "was", "um", "dich", "ringt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Hoffen t\u00e4uscht das stille Bangen,", "tokens": ["Kein", "Hof\u00b7fen", "t\u00e4uscht", "das", "stil\u00b7le", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch verstummt nicht das Verlangen,", "tokens": ["Und", "doch", "ver\u00b7stummt", "nicht", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das keinen Frieden je mir bringt.", "tokens": ["Das", "kei\u00b7nen", "Frie\u00b7den", "je", "mir", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie fragen qu\u00e4lend, was mir fehle?", "tokens": ["Sie", "fra\u00b7gen", "qu\u00e4\u00b7lend", ",", "was", "mir", "feh\u00b7le", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tag ist blau, die Biene schwirrt:", "tokens": ["Der", "Tag", "ist", "blau", ",", "die", "Bie\u00b7ne", "schwirrt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bin nicht daheim, bin nicht im Leben,", "tokens": ["Bin", "nicht", "da\u00b7heim", ",", "bin", "nicht", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "$,", "VAFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit ich die Seele hingegeben,", "tokens": ["Seit", "ich", "die", "See\u00b7le", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die willenlos zu dir verirrt!", "tokens": ["Die", "wil\u00b7len\u00b7los", "zu", "dir", "ver\u00b7irrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}