{"textgrid.poem.40909": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Abendstern", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De bisch au wieder zitli do", "tokens": ["De", "bisch", "au", "wie\u00b7der", "zit\u00b7li", "do"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "ADV", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und laufsch der Sunne weidli no,", "tokens": ["und", "lauf\u00b7sch", "der", "Sun\u00b7ne", "weid\u00b7li", "no", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "du liebe, sch\u00f6nen Obestern!", "tokens": ["du", "lie\u00b7be", ",", "sch\u00f6\u00b7nen", "O\u00b7bes\u00b7tern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.4": {"text": "Was gilt's, de h\u00e4ttsch di Schm\u00fctzli gern!", "tokens": ["Was", "gilt's", ",", "de", "h\u00e4ttsch", "di", "Schm\u00fctz\u00b7li", "gern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "NE", "ADJD", "NE", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er trippelt ihre Spure no,", "tokens": ["Er", "trip\u00b7pelt", "ih\u00b7re", "Spu\u00b7re", "no", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und cha si doch nit \u00fcbercho.", "tokens": ["und", "cha", "si", "doch", "nit", "\u00fc\u00b7berc\u00b7ho", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von alle Sterne gro\u00df und chlei", "tokens": ["Von", "al\u00b7le", "Ster\u00b7ne", "gro\u00df", "und", "chlei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "isch er der liebst, und er ellei;", "tokens": ["isch", "er", "der", "liebst", ",", "und", "er", "el\u00b7lei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "VVFIN", "$,", "KON", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "si Br\u00fcderli der Morgestern,", "tokens": ["si", "Br\u00fc\u00b7der\u00b7li", "der", "Mor\u00b7ges\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "si het en nit ums halb so gern;", "tokens": ["si", "het", "en", "nit", "ums", "halb", "so", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "und wo sie wandlet us und i,", "tokens": ["und", "wo", "sie", "wand\u00b7let", "us", "und", "i", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "se meint sie, m\u00fce\u00df er um sie si.", "tokens": ["se", "meint", "sie", ",", "m\u00fce\u00df", "er", "um", "sie", "si", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "APPR", "PPER", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Fr\u00fceih, wenn sie hinterm Morgerot", "tokens": ["Fr\u00fc\u00b7eih", ",", "wenn", "sie", "hin\u00b7term", "Mor\u00b7ge\u00b7rot"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "wohl ob em Schwarzwald ufe goht,", "tokens": ["wohl", "ob", "em", "Schwarz\u00b7wald", "u\u00b7fe", "goht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "sie f\u00fchrt ihr B\u00fcbli an der Hand,", "tokens": ["sie", "f\u00fchrt", "ihr", "B\u00fcb\u00b7li", "an", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "sie zeigt em Berg und Strom und Land,", "tokens": ["sie", "zeigt", "em", "Berg", "und", "Strom", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "sie seit: \u00bbTue gmach, 's pressiert nit so!", "tokens": ["sie", "seit", ":", "\u00bb", "Tue", "gmach", ",", "'s", "pres\u00b7siert", "nit", "so", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "$.", "$(", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Di Gumpe wird der bald vergoh.\u00ab", "tokens": ["Di", "Gum\u00b7pe", "wird", "der", "bald", "ver\u00b7goh", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Er schwezt und frogt sie das und deis,", "tokens": ["Er", "schwezt", "und", "frogt", "sie", "das", "und", "deis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PDS", "KON", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "sie git em Bricht, so guet sie 's wei\u00df.", "tokens": ["sie", "git", "em", "Bricht", ",", "so", "guet", "sie", "'s", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Er seit: \u00bbO Mutter, lueg doch au,", "tokens": ["Er", "seit", ":", "\u00bb", "O", "Mut\u00b7ter", ",", "lu\u00b7eg", "doch", "au", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "$.", "$(", "NE", "NN", "$,", "VVFIN", "ADV", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "do unte gl\u00e4nzt's im Morgetau", "tokens": ["do", "un\u00b7te", "gl\u00e4nzt's", "im", "Mor\u00b7ge\u00b7tau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "so sch\u00f6n wie in dim Himmelssaal!\u00ab", "tokens": ["so", "sch\u00f6n", "wie", "in", "dim", "Him\u00b7mels\u00b7saal", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "\u00bbhe\u00ab, seit sie, \u00bbdrum isch's 's Wiesetal.\u00ab", "tokens": ["\u00bb", "he", "\u00ab", ",", "seit", "sie", ",", "\u00bb", "drum", "isch's", "'s", "Wie\u00b7se\u00b7tal", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$(", "$,", "KOUS", "PPER", "$,", "$(", "PAV", "VAFIN", "PPER", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Sie frogt en: \u00bbHesch bald alles gseh?", "tokens": ["Sie", "frogt", "en", ":", "\u00bb", "Hesch", "bald", "al\u00b7les", "gseh", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM", "$.", "$(", "NE", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Jez gangi, und wart n\u00fcmme meh.\u00ab", "tokens": ["Jez", "gan\u00b7gi", ",", "und", "wart", "n\u00fcm\u00b7me", "meh", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.fr", "FM.fr", "$,", "KON", "VVFIN", "NE", "NE", "$.", "$("], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "Druf springt er ihrer Hand dervo,", "tokens": ["Druf", "springt", "er", "ih\u00b7rer", "Hand", "der\u00b7vo", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "und mengem wii\u00dfe W\u00fclkli no;", "tokens": ["und", "men\u00b7gem", "wii\u00b7\u00dfe", "W\u00fclk\u00b7li", "no", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.29": {"text": "do, wenn er meint, jez han i di,", "tokens": ["do", ",", "wenn", "er", "meint", ",", "jez", "han", "i", "di", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.30": {"text": "verschwunden isch's, wei\u00df Gott, wohi.", "tokens": ["ver\u00b7schwun\u00b7den", "isch's", ",", "wei\u00df", "Gott", ",", "wo\u00b7hi", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN", "NN", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Druf, wie si Mutter h\u00f6cher stoht,", "tokens": ["Druf", ",", "wie", "si", "Mut\u00b7ter", "h\u00f6\u00b7cher", "stoht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "NE", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "und alsgmach gegenem Rhistrom goht,", "tokens": ["und", "alsg\u00b7mach", "ge\u00b7ge\u00b7nem", "Rhis\u00b7trom", "goht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "se r\u00fceft sie 'm: \u00bbChumm und fall nit do!\u00ab", "tokens": ["se", "r\u00fceft", "sie", "'m", ":", "\u00bb", "Chumm", "und", "fall", "nit", "do", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$.", "$(", "NE", "KON", "NN", "PTKNEG", "ADV", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Sie f\u00fchrt en fest am H\u00e4ndli no:", "tokens": ["Sie", "f\u00fchrt", "en", "fest", "am", "H\u00e4nd\u00b7li", "no", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "\u00bbde ch\u00f6nntsch verl\u00f6sche, handumcher.", "tokens": ["\u00bb", "de", "ch\u00f6nntsch", "ver\u00b7l\u00f6\u00b7sche", ",", "han\u00b7dum\u00b7cher", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "NE", "ADJD", "ADJA", "$,", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "Nimm, was mer's f\u00fcr e Chummer w\u00e4r!\u00ab", "tokens": ["Nimm", ",", "was", "mer's", "f\u00fcr", "e", "Chum\u00b7mer", "w\u00e4r", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PRELS", "PIS", "APPR", "NE", "NE", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Doch, wo sie \u00fcberm Elsis stoht,", "tokens": ["Doch", ",", "wo", "sie", "\u00fc\u00b7berm", "El\u00b7sis", "stoht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "APPRART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "und alsgmach ehnen abe goht,", "tokens": ["und", "alsg\u00b7mach", "eh\u00b7nen", "a\u00b7be", "goht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "wird nootno 's B\u00fcebli m\u00fced und still,", "tokens": ["wird", "noot\u00b7no", "'s", "B\u00fceb\u00b7li", "m\u00fced", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "FM", "FM", "FM", "FM", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "'s wei\u00df n\u00fcmme, was es mache will;", "tokens": ["'s", "wei\u00df", "n\u00fcm\u00b7me", ",", "was", "es", "ma\u00b7che", "will", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "'s will n\u00fcmme goh, und will nit goh,", "tokens": ["'s", "will", "n\u00fcm\u00b7me", "goh", ",", "und", "will", "nit", "goh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "$,", "KON", "VMFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.42": {"text": "'s frogt hundertmol: \u00bbWie wit isch's no?\u00ab", "tokens": ["'s", "frogt", "hun\u00b7dert\u00b7mol", ":", "\u00bb", "Wie", "wit", "isch's", "no", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "$(", "PWAV", "FM", "FM", "FM", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.43": {"text": "Druf, wie sie ob de Berge stoht,", "tokens": ["Druf", ",", "wie", "sie", "ob", "de", "Ber\u00b7ge", "stoht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PPER", "KOUS", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "und tiefer sinkt ins Oberot,", "tokens": ["und", "tie\u00b7fer", "sinkt", "ins", "O\u00b7be\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "und er afange matt und m\u00fced", "tokens": ["und", "er", "a\u00b7fan\u00b7ge", "matt", "und", "m\u00fced"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "im rote Schimmer d'Heimet sieht,", "tokens": ["im", "ro\u00b7te", "Schim\u00b7mer", "d'\u00b7Hei\u00b7met", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.47": {"text": "se lo\u00dft er sie am F\u00fcrtuch goh,", "tokens": ["se", "lo\u00dft", "er", "sie", "am", "F\u00fcr\u00b7tuch", "goh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "und zottlet alsgmach hinte no.", "tokens": ["und", "zott\u00b7let", "alsg\u00b7mach", "hin\u00b7te", "no", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "In d'Heimet wandle Herd und Hirt,", "tokens": ["In", "d'\u00b7Hei\u00b7met", "wand\u00b7le", "Herd", "und", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.50": {"text": "der Vogel sizt, der Ch\u00e4fer schwirt;", "tokens": ["der", "Vo\u00b7gel", "sizt", ",", "der", "Ch\u00e4\u00b7fer", "schwirt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "und 's Heimli betet d\u00f6rt und do", "tokens