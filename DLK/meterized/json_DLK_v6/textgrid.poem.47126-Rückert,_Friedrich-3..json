{"textgrid.poem.47126": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, o Lippe, von dem Kusse", "tokens": ["Du", ",", "o", "Lip\u00b7pe", ",", "von", "dem", "Kus\u00b7se"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "FM", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Geliebten eingeweiht,", "tokens": ["Der", "Ge\u00b7lieb\u00b7ten", "ein\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun vom Paradiesesflusse", "tokens": ["Nun", "vom", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7flus\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingenetzt auf Ewigkeit!", "tokens": ["Ein\u00b7ge\u00b7netzt", "auf", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von den reinen Himmelsfluten", "tokens": ["Von", "den", "rei\u00b7nen", "Him\u00b7mels\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind die S\u00fcnden weggesp\u00fclt,", "tokens": ["Sind", "die", "S\u00fcn\u00b7den", "weg\u00b7ge\u00b7sp\u00fclt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die tr\u00fcben Sinnesgluten", "tokens": ["Und", "die", "tr\u00fc\u00b7ben", "Sin\u00b7nes\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leicht in \u00c4therhauch gek\u00fchlt.", "tokens": ["Leicht", "in", "\u00c4\u00b7ther\u00b7hauch", "ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht mehr tr\u00e4ge Erdenspeise", "tokens": ["Nicht", "mehr", "tr\u00e4\u00b7ge", "Er\u00b7den\u00b7spei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komme deinem Kelche nah',", "tokens": ["Kom\u00b7me", "dei\u00b7nem", "Kel\u00b7che", "nah'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sondern, nach der G\u00f6tter Weise,", "tokens": ["Son\u00b7dern", ",", "nach", "der", "G\u00f6t\u00b7ter", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nektar und Ambrosia!", "tokens": ["Nek\u00b7tar", "und", "A\u00b7mbro\u00b7sia", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nicht mehr \u00fcber deine Schwelle", "tokens": ["Nicht", "mehr", "\u00fc\u00b7ber", "dei\u00b7ne", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandle dumpfes ird'sches Wort,", "tokens": ["Wand\u00b7le", "dum\u00b7pfes", "ird'\u00b7sches", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sondern des Gesanges helle", "tokens": ["Son\u00b7dern", "des", "Ge\u00b7san\u00b7ges", "hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Opferlohe fort und fort!", "tokens": ["Op\u00b7fer\u00b7lo\u00b7he", "fort", "und", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}