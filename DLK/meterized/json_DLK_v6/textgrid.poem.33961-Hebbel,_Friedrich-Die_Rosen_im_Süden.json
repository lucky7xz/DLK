{"textgrid.poem.33961": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Rosen im S\u00fcden", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus den Knospen, die euch deckten,", "tokens": ["Aus", "den", "Knos\u00b7pen", ",", "die", "euch", "deck\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfe Rosen, mein Entz\u00fccken,", "tokens": ["S\u00fc\u00b7\u00dfe", "Ro\u00b7sen", ",", "mein", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lockte euch der hei\u00dfe S\u00fcd;", "tokens": ["Lock\u00b7te", "euch", "der", "hei\u00b7\u00dfe", "S\u00fcd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch die Gluten, die euch weckten,", "tokens": ["Doch", "die", "Glu\u00b7ten", ",", "die", "euch", "weck\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Drohen jetzt, euch zu ersticken,", "tokens": ["Dro\u00b7hen", "jetzt", ",", "euch", "zu", "er\u00b7sti\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, ihr seid schon halb vergl\u00fcht!", "tokens": ["Ach", ",", "ihr", "seid", "schon", "halb", "ver\u00b7gl\u00fcht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dem Freunde, dem erschreckten,", "tokens": ["Und", "dem", "Freun\u00b7de", ",", "dem", "er\u00b7schreck\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4ucht, er mu\u00df euch eilig pfl\u00fccken,", "tokens": ["D\u00e4ucht", ",", "er", "mu\u00df", "euch", "ei\u00b7lig", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ihr nicht zu schnell verbl\u00fcht!", "tokens": ["Da\u00df", "ihr", "nicht", "zu", "schnell", "ver\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PTKA", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}