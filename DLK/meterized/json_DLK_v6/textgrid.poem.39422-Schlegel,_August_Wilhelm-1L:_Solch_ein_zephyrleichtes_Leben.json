{"textgrid.poem.39422": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Solch ein zephyrleichtes Leben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solch ein zephyrleichtes Leben", "tokens": ["Solch", "ein", "ze\u00b7phyr\u00b7leich\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Solcher Anmuth Gang und Schweben", "tokens": ["Sol\u00b7cher", "An\u00b7muth", "Gang", "und", "Schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah mein trunknes Auge nie.", "tokens": ["Sah", "mein", "trunk\u00b7nes", "Au\u00b7ge", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede Welle der Bewegung", "tokens": ["Je\u00b7de", "Wel\u00b7le", "der", "Be\u00b7we\u00b7gung"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zaubert hin in s\u00fc\u00dfe Regung,", "tokens": ["Zau\u00b7bert", "hin", "in", "s\u00fc\u00b7\u00dfe", "Re\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles athmet Harmonie.", "tokens": ["Al\u00b7les", "ath\u00b7met", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie du spendest deine Kr\u00e4nze", "tokens": ["Wie", "du", "spen\u00b7dest", "dei\u00b7ne", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die Muse froher T\u00e4nze,", "tokens": ["Als", "die", "Mu\u00b7se", "fro\u00b7her", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie dein holdes Fest beginnt:", "tokens": ["Wie", "dein", "hol\u00b7des", "Fest", "be\u00b7ginnt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00e4h' ich nicht der Wonne Z\u00e4hren", "tokens": ["S\u00e4h'", "ich", "nicht", "der", "Won\u00b7ne", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Mutter Blick verkl\u00e4ren,", "tokens": ["Dei\u00b7ner", "Mut\u00b7ter", "Blick", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4hnt' ich dich ein G\u00f6tterkind.", "tokens": ["W\u00e4hnt'", "ich", "dich", "ein", "G\u00f6t\u00b7ter\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In der zarten Bildung Schleier", "tokens": ["In", "der", "zar\u00b7ten", "Bil\u00b7dung", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher Ton von Ph\u00f6bus Leier", "tokens": ["Wel\u00b7cher", "Ton", "von", "Ph\u00f6\u00b7bus", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat den hohen Geist gehaucht?", "tokens": ["Hat", "den", "ho\u00b7hen", "Geist", "ge\u00b7haucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo, auf bl\u00fch'nder Flur geboren,", "tokens": ["Wo", ",", "auf", "bl\u00fch'n\u00b7der", "Flur", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Haben weihend milde Horen", "tokens": ["Ha\u00b7ben", "wei\u00b7hend", "mil\u00b7de", "Ho\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich in Morgenroth getaucht?", "tokens": ["Dich", "in", "Mor\u00b7gen\u00b7roth", "ge\u00b7taucht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was Pygmalion errungen,", "tokens": ["Was", "Pyg\u00b7ma\u00b7li\u00b7on", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Als der sch\u00f6ne Stein, bezwungen,", "tokens": ["Als", "der", "sch\u00f6\u00b7ne", "Stein", ",", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward beseelt auf seinen Ruf,", "tokens": ["Ward", "be\u00b7seelt", "auf", "sei\u00b7nen", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrt sich um: denn festgehalten", "tokens": ["Kehrt", "sich", "um", ":", "denn", "fest\u00b7ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "PTKVZ", "$.", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seh' im Flug ich die Gestalten,", "tokens": ["Seh'", "im", "Flug", "ich", "die", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die der Griechen Mei\u00dfel schuf.", "tokens": ["Die", "der", "Grie\u00b7chen", "Mei\u00b7\u00dfel", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nimm den Bogen und die Pfeile,", "tokens": ["Nimm", "den", "Bo\u00b7gen", "und", "die", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und, Dianen gleich, enteile", "tokens": ["Und", ",", "Di\u00b7a\u00b7nen", "gleich", ",", "ent\u00b7ei\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$,", "NE", "ADV", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolzen Muthes in den Hain.", "tokens": ["Stol\u00b7zen", "Mu\u00b7thes", "in", "den", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Willst du mit der Aegis schrecken,", "tokens": ["Willst", "du", "mit", "der", "A\u00b7e\u00b7gis", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Mit dem Helm die Stirne decken,", "tokens": ["Mit", "dem", "Helm", "die", "Stir\u00b7ne", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirst du Jovis Tochter sein.", "tokens": ["Wirst", "du", "Jo\u00b7vis", "Toch\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Streue Rosen, sei Aurore;", "tokens": ["Streu\u00b7e", "Ro\u00b7sen", ",", "sei", "Au\u00b7ro\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trag das K\u00f6rbchen, Kanephore,", "tokens": ["Trag", "das", "K\u00f6rb\u00b7chen", ",", "Ka\u00b7ne\u00b7pho\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu des Feierzuges Pracht.", "tokens": ["Zu", "des", "Fei\u00b7er\u00b7zu\u00b7ges", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gie\u00df sie aus, die Opferschale;", "tokens": ["Gie\u00df", "sie", "aus", ",", "die", "Op\u00b7fer\u00b7scha\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00fclle nun dich ein, Vestale,", "tokens": ["H\u00fcl\u00b7le", "nun", "dich", "ein", ",", "Ves\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PTKVZ", "$,", "NE", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Die den ew'gen Heerd bewacht.", "tokens": ["Die", "den", "ew'\u00b7gen", "Heerd", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Fliegen la\u00df dein Haar, Bacchante;", "tokens": ["Flie\u00b7gen", "la\u00df", "dein", "Haar", ",", "Bac\u00b7chan\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVIMP", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00fcrte dich, und Atalante", "tokens": ["G\u00fcr\u00b7te", "dich", ",", "und", "A\u00b7tal\u00b7an\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Siegst du im beschwingten Lauf.", "tokens": ["Siegst", "du", "im", "be\u00b7schwing\u00b7ten", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald, allein ein Chor von Musen,", "tokens": ["Bald", ",", "al\u00b7lein", "ein", "Chor", "von", "Mu\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schwellt den jungfr\u00e4ulichen Busen", "tokens": ["Schwellt", "den", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "F\u00fclle der Begeistrung auf.", "tokens": ["F\u00fcl\u00b7le", "der", "Be\u00b7geis\u00b7trung", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dich, Alth\u00e4a, sah mit Schaudern", "tokens": ["Dich", ",", "A\u00b7lth\u00e4a", ",", "sah", "mit", "Schau\u00b7dern"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NE", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich, nach langem Kampf und Zaudern", "tokens": ["Ich", ",", "nach", "lan\u00b7gem", "Kampf", "und", "Zau\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den verh\u00e4ngni\u00dfvollen Brand", "tokens": ["Den", "ver\u00b7h\u00e4ng\u00b7ni\u00df\u00b7vol\u00b7len", "Brand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleudern in die Todesgluten", "tokens": ["Schleu\u00b7dern", "in", "die", "To\u00b7des\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und verzweifelnd dann verbluten,", "tokens": ["Und", "ver\u00b7zwei\u00b7felnd", "dann", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dich selbst den Dolch gewandt.", "tokens": ["Auf", "dich", "selbst", "den", "Dolch", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sag, welch Ahnden hat die Thaten", "tokens": ["Sag", ",", "welch", "Ahn\u00b7den", "hat", "die", "Tha\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wilder Rache dir verrathen,", "tokens": ["Wil\u00b7der", "Ra\u00b7che", "dir", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Seele tiefsten Schmerz?", "tokens": ["Und", "der", "See\u00b7le", "tiefs\u00b7ten", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch an deiner Jugend Schwelle", "tokens": ["Noch", "an", "dei\u00b7ner", "Ju\u00b7gend", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sahst du schon der Leiden Welle", "tokens": ["Sahst", "du", "schon", "der", "Lei\u00b7den", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00fcrmen durch ein menschlich Herz?", "tokens": ["St\u00fcr\u00b7men", "durch", "ein", "menschlich", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Nein, dir tr\u00fcbe nichts die Seele!", "tokens": ["Nein", ",", "dir", "tr\u00fc\u00b7be", "nichts", "die", "See\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei, was Ungl\u00fccksel'ge qu\u00e4le,", "tokens": ["Sei", ",", "was", "Un\u00b7gl\u00fcck\u00b7sel'\u00b7ge", "qu\u00e4\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur im Bilde dir bewu\u00dft!", "tokens": ["Nur", "im", "Bil\u00b7de", "dir", "be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer m\u00f6gst du nur sie spielen", "tokens": ["Im\u00b7mer", "m\u00f6gst", "du", "nur", "sie", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jene Dolche, welche zielen", "tokens": ["Je\u00b7ne", "Dol\u00b7che", ",", "wel\u00b7che", "zie\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDA