{"textgrid.poem.23988": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Mauer entlang,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mauer entlang,", "tokens": ["Die", "Mau\u00b7er", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wo das W\u00e4sserchen rinnt,", "tokens": ["Wo", "das", "W\u00e4s\u00b7ser\u00b7chen", "rinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo die Rosabl\u00fcte des Apfelbaums", "tokens": ["Wo", "die", "Ro\u00b7sa\u00b7bl\u00fc\u00b7te", "des", "Ap\u00b7fel\u00b7baums"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ernste, dunkle Baumgr\u00fcn gr\u00fc\u00dft,", "tokens": ["Das", "erns\u00b7te", ",", "dunk\u00b7le", "Baum\u00b7gr\u00fcn", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da stehen die sch\u00f6nsten Blumen.", "tokens": ["Da", "ste\u00b7hen", "die", "sch\u00f6ns\u00b7ten", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Von jeglicher Art,", "tokens": ["Von", "jeg\u00b7li\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Vielfarbenhell,", "tokens": ["Viel\u00b7far\u00b7ben\u00b7hell", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Leis duftgewiegt und schattengesch\u00fctzt", "tokens": ["Leis", "duft\u00b7ge\u00b7wiegt", "und", "schat\u00b7ten\u00b7ge\u00b7sch\u00fctzt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Lachen sie her aus gr\u00fcnem Gras;", "tokens": ["La\u00b7chen", "sie", "her", "aus", "gr\u00fc\u00b7nem", "Gras", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ach, wollen sie sterben im Fr\u00fchlingsglanz?", "tokens": ["Ach", ",", "wol\u00b7len", "sie", "ster\u00b7ben", "im", "Fr\u00fch\u00b7lings\u00b7glanz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ich breche die flammgl\u00fchrote.", "tokens": ["Ich", "bre\u00b7che", "die", "flamm\u00b7gl\u00fch\u00b7ro\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dir, Liebe, geh ich sie, die du still", "tokens": ["Dir", ",", "Lie\u00b7be", ",", "geh", "ich", "sie", ",", "die", "du", "still"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im schwarzen Kleide traurig gehst", "tokens": ["Im", "schwar\u00b7zen", "Klei\u00b7de", "trau\u00b7rig", "gehst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwischen Lautenschlag und bl\u00fchender Pracht", "tokens": ["Zwi\u00b7schen", "Lau\u00b7ten\u00b7schlag", "und", "bl\u00fc\u00b7hen\u00b7der", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "An deiner Brust aufprange sie hell,", "tokens": ["An", "dei\u00b7ner", "Brust", "auf\u00b7pran\u00b7ge", "sie", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "In dein Herz lohe ihr Lebensrot,", "tokens": ["In", "dein", "Herz", "lo\u00b7he", "ihr", "Le\u00b7bens\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Dir singe ihr Duft aus tiefem Kelch:", "tokens": ["Dir", "sin\u00b7ge", "ihr", "Duft", "aus", "tie\u00b7fem", "Kelch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sieh, dir auch lacht die Au!", "tokens": ["Sieh", ",", "dir", "auch", "lacht", "die", "Au", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}