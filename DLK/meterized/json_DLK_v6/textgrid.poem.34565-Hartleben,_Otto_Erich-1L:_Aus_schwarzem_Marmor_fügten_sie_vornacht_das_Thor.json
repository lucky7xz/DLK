{"textgrid.poem.34565": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus schwarzem Marmor f\u00fcgten sie vornacht das Thor", "genre": "verse", "period": "N.A.", "pub_year": 1902, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus schwarzem Marmor f\u00fcgten sie vornacht das Thor", "tokens": ["Aus", "schwar\u00b7zem", "Mar\u00b7mor", "f\u00fcg\u00b7ten", "sie", "vor\u00b7nacht", "das", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und eines seltnen Stolzes Inschrift gruben sie", "tokens": ["und", "ei\u00b7nes", "selt\u00b7nen", "Stol\u00b7zes", "In\u00b7schrift", "gru\u00b7ben", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auf seine Stirn. Es dr\u00e4ngten die Jahrhunderte", "tokens": ["auf", "sei\u00b7ne", "Stirn", ".", "Es", "dr\u00e4ng\u00b7ten", "die", "Jahr\u00b7hun\u00b7der\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "an diesen Pfosten sich vorbei, das Haus zerfiel,", "tokens": ["an", "die\u00b7sen", "Pfos\u00b7ten", "sich", "vor\u00b7bei", ",", "das", "Haus", "zer\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "das Thor bestand \u2013 heut \u00f6ffnet es die Fl\u00fcgel weit.", "tokens": ["das", "Thor", "be\u00b7stand", "\u2013", "heut", "\u00f6ff\u00b7net", "es", "die", "Fl\u00fc\u00b7gel", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf diesen alten Schwellen lag manch tr\u00e4ger Staub \u2013", "tokens": ["Auf", "die\u00b7sen", "al\u00b7ten", "Schwel\u00b7len", "lag", "manch", "tr\u00e4\u00b7ger", "Staub", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "heut aber sollen braundurchwobne Purpurdecken", "tokens": ["heut", "a\u00b7ber", "sol\u00b7len", "braun\u00b7durch\u00b7wob\u00b7ne", "Pur\u00b7pur\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die grauen Quadern tauchen in den Strom der Seide", "tokens": ["die", "grau\u00b7en", "Qua\u00b7dern", "tau\u00b7chen", "in", "den", "Strom", "der", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und einer Herrin wunderzarte F\u00fcsse sollen", "tokens": ["und", "ei\u00b7ner", "Her\u00b7rin", "wun\u00b7derz\u00b7ar\u00b7te", "F\u00fcs\u00b7se", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "die grossen Wandelsteine scheu auftretend segnen ...", "tokens": ["die", "gros\u00b7sen", "Wan\u00b7del\u00b7stei\u00b7ne", "scheu", "auf\u00b7tre\u00b7tend", "seg\u00b7nen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es tr\u00e4umt der Herbst sein reiches Fest. Es herrscht am See", "tokens": ["Es", "tr\u00e4umt", "der", "Herbst", "sein", "rei\u00b7ches", "Fest", ".", "Es", "herrscht", "am", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der weissen Chrysanthemen sp\u00e4tgeborne Pracht", "tokens": ["der", "weis\u00b7sen", "Chry\u00b7san\u00b7the\u00b7men", "sp\u00e4t\u00b7ge\u00b7bor\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und jener dunkelrothen Bl\u00e4tter \u00dcberfluss,", "tokens": ["und", "je\u00b7ner", "dun\u00b7kel\u00b7ro\u00b7then", "Bl\u00e4t\u00b7ter", "\u00dc\u00b7berf\u00b7luss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der wie ein Mantel auf den morschen Mauern liegt \u2013", "tokens": ["der", "wie", "ein", "Man\u00b7tel", "auf", "den", "mor\u00b7schen", "Mau\u00b7ern", "liegt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo die Cypressen dort im Abendlichte gl\u00fchn,", "tokens": ["Wo", "die", "Cyp\u00b7res\u00b7sen", "dort", "im", "A\u00b7bend\u00b7lich\u00b7te", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "erscheint ihr Gr\u00fcn von F\u00e4den dunklen Golds durchwirkt ...", "tokens": ["er\u00b7scheint", "ihr", "Gr\u00fcn", "von", "F\u00e4\u00b7den", "dunk\u00b7len", "Golds", "durch\u00b7wirkt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die reinen Stufen schreit ich Fragender hinab.", "tokens": ["Die", "rei\u00b7nen", "Stu\u00b7fen", "schreit", "ich", "Fra\u00b7gen\u00b7der", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}