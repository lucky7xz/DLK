{"textgrid.poem.60954": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ver\u00e4nderung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ernsthaft bin ich geworden, ich f\u00fchl's; nicht bin ich derselbe,", "tokens": ["Ernst\u00b7haft", "bin", "ich", "ge\u00b7wor\u00b7den", ",", "ich", "f\u00fchl's", ";", "nicht", "bin", "ich", "der\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VAPP", "$,", "PPER", "VVFIN", "$.", "PTKNEG", "VAFIN", "PPER", "PDAT", "$,"], "meter": "+-+--+--+-++-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der ich als J\u00fcngling schrieb jenes ber\u00fchmte Besteck:", "tokens": ["Der", "ich", "als", "J\u00fcng\u00b7ling", "schrieb", "je\u00b7nes", "be\u00b7r\u00fchm\u00b7te", "Be\u00b7steck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht mehr wohnt im Gem\u00fct der Erfindungen komische F\u00fclle,", "tokens": ["Nicht", "mehr", "wohnt", "im", "Ge\u00b7m\u00fct", "der", "Er\u00b7fin\u00b7dun\u00b7gen", "ko\u00b7mi\u00b7sche", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+---+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Welche verschwenderisch einst freundliche Seelen erg\u00f6tzt;", "tokens": ["Wel\u00b7che", "ver\u00b7schwen\u00b7de\u00b7risch", "einst", "freund\u00b7li\u00b7che", "See\u00b7len", "er\u00b7g\u00f6tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Aber es ward seitdem auch Deutschland bitterlich ernsthaft,", "tokens": ["A\u00b7ber", "es", "ward", "seit\u00b7dem", "auch", "Deutschland", "bit\u00b7ter\u00b7lich", "ernst\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PAV", "ADV", "NE", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+-++-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Fern zwar lebt ich und doch f\u00fchlt ich denselben Beruf.", "tokens": ["Fern", "zwar", "lebt", "ich", "und", "doch", "f\u00fchlt", "ich", "den\u00b7sel\u00b7ben", "Be\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "KON", "ADV", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}