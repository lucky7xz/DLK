{"dta.poem.21375": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Beste w\u00e4re du bliebest zu Haus,", "tokens": ["Das", "Bes\u00b7te", "w\u00e4\u00b7re", "du", "blie\u00b7best", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hier in dem alten Kiffh\u00e4user \u2013", "tokens": ["Hier", "in", "dem", "al\u00b7ten", "Kiff\u00b7h\u00e4u\u00b7ser", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Bedenk\u2019 ich die Sache ganz genau,", "tokens": ["Be\u00b7denk'", "ich", "die", "Sa\u00b7che", "ganz", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So brauchen wir gar keinen Kaiser.", "tokens": ["So", "brau\u00b7chen", "wir", "gar", "kei\u00b7nen", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}