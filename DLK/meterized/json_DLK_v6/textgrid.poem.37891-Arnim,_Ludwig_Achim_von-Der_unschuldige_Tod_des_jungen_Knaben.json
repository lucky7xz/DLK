{"textgrid.poem.37891": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der unschuldige Tod des jungen Knaben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegt ein Schlo\u00df in Oesterreich,", "tokens": ["Es", "liegt", "ein", "Schlo\u00df", "in", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das ist ganz wohl gebauet,", "tokens": ["Das", "ist", "ganz", "wohl", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Silber und von rothem Gold,", "tokens": ["Von", "Sil\u00b7ber", "und", "von", "ro\u00b7them", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Marmorstein gemauert.", "tokens": ["Mit", "Mar\u00b7mor\u00b7stein", "ge\u00b7mau\u00b7ert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Darinnen liegt ein junger Knab,", "tokens": ["Da\u00b7rin\u00b7nen", "liegt", "ein", "jun\u00b7ger", "Knab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinen Hals gefangen,", "tokens": ["Auf", "sei\u00b7nen", "Hals", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl vierzig Klafter unter der Erd,", "tokens": ["Wohl", "vier\u00b7zig", "Klaf\u00b7ter", "un\u00b7ter", "der", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Bei Ottern und bey Schlangen.", "tokens": ["Bei", "Ot\u00b7tern", "und", "bey", "Schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sein Vater kam von Rosenberg,", "tokens": ["Sein", "Va\u00b7ter", "kam", "von", "Ro\u00b7sen\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl vor den Thurm gegangen:", "tokens": ["Wohl", "vor", "den", "Thurm", "ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbach Sohne, liebster Sohne mein,", "tokens": ["\u00bb", "ach", "Soh\u00b7ne", ",", "liebs\u00b7ter", "Soh\u00b7ne", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hart liegst du gefangen!\u00ab", "tokens": ["Wie", "hart", "liegst", "du", "ge\u00b7fan\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbach Vater, liebster Vater mein,", "tokens": ["\u00bb", "ach", "Va\u00b7ter", ",", "liebs\u00b7ter", "Va\u00b7ter", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hart lieg ich gefangen,", "tokens": ["So", "hart", "lieg", "ich", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl vierzig Klafter unter der Erd,", "tokens": ["Wohl", "vier\u00b7zig", "Klaf\u00b7ter", "un\u00b7ter", "der", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Bey Ottern und bey Schlangen.\u00ab", "tokens": ["Bey", "Ot\u00b7tern", "und", "bey", "Schlan\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sein Vater zu dem Herrn hinging,", "tokens": ["Sein", "Va\u00b7ter", "zu", "dem", "Herrn", "hin\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach: \u00bbGebt mir los den Gefangnen,", "tokens": ["Sprach", ":", "\u00bb", "Gebt", "mir", "los", "den", "Ge\u00b7fang\u00b7nen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "VVIMP", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drey hundert Gulden geben wir,", "tokens": ["Drey", "hun\u00b7dert", "Gul\u00b7den", "ge\u00b7ben", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl f\u00fcr des Knaben Leben.\u00ab", "tokens": ["Wohl", "f\u00fcr", "des", "Kna\u00b7ben", "Le\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbdrey hundert Gulden die helfen euch nicht,", "tokens": ["\u00bb", "drey", "hun\u00b7dert", "Gul\u00b7den", "die", "hel\u00b7fen", "euch", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "CARD", "CARD", "NN", "ART", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Der Knabe der mu\u00df sterben,", "tokens": ["Der", "Kna\u00b7be", "der", "mu\u00df", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er tr\u00e4gt von Gold eine Kett' am Hals,", "tokens": ["Er", "tr\u00e4gt", "von", "Gold", "ei\u00b7ne", "Kett'", "am", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die bringt ihn um sein Leben.\u00ab", "tokens": ["Die", "bringt", "ihn", "um", "sein", "Le\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbtr\u00e4gt er von Gold eine Kett' am Hals,", "tokens": ["\u00bb", "tr\u00e4gt", "er", "von", "Gold", "ei\u00b7ne", "Kett'", "am", "Hals", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die hat er nicht gestohlen,", "tokens": ["Die", "hat", "er", "nicht", "ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat ihm ein zart Jungfrau verehrt;", "tokens": ["Hat", "ihm", "ein", "zart", "Jung\u00b7frau", "ver\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dabey sie ihn erzogen.\u00ab", "tokens": ["Da\u00b7bey", "sie", "ihn", "er\u00b7zo\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Man bracht den Knaben aus dem Thurm,", "tokens": ["Man", "bracht", "den", "Kna\u00b7ben", "aus", "dem", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gab ihm die Sakramente:", "tokens": ["Gab", "ihm", "die", "Sak\u00b7ra\u00b7men\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbhilfreicher Christ vom Himmel hoch,", "tokens": ["\u00bb", "hilf\u00b7rei\u00b7cher", "Christ", "vom", "Him\u00b7mel", "hoch", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es geht mit mir am Ende.\u00ab", "tokens": ["Es", "geht", "mit", "mir", "am", "En\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Man bracht ihn zum Gericht hinaus,", "tokens": ["Man", "bracht", "ihn", "zum", "Ge\u00b7richt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Leiter mu\u00df er steigen:", "tokens": ["Die", "Lei\u00b7ter", "mu\u00df", "er", "stei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbach Meister, liebster Meister mein,", "tokens": ["\u00bb", "ach", "Meis\u00b7ter", ",", "liebs\u00b7ter", "Meis\u00b7ter", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df mir eine kleine Weile!\u00ab", "tokens": ["La\u00df", "mir", "ei\u00b7ne", "klei\u00b7ne", "Wei\u00b7le", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbeine kleine Weile la\u00df ich dir nicht,", "tokens": ["\u00bb", "ei\u00b7ne", "klei\u00b7ne", "Wei\u00b7le", "la\u00df", "ich", "dir", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVIMP", "PPER", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Du m\u00f6chtest mir entrinnen,", "tokens": ["Du", "m\u00f6ch\u00b7test", "mir", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Langt mir ein seiden T\u00fcchlein her,", "tokens": ["Langt", "mir", "ein", "sei\u00b7den", "T\u00fcch\u00b7lein", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich seine Augen verbinde.\u00ab", "tokens": ["Da\u00df", "ich", "sei\u00b7ne", "Au\u00b7gen", "ver\u00b7bin\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbach meine Augen verbinde mir nicht,", "tokens": ["\u00bb", "ach", "mei\u00b7ne", "Au\u00b7gen", "ver\u00b7bin\u00b7de", "mir", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Ich mu\u00df die Welt anschauen,", "tokens": ["Ich", "mu\u00df", "die", "Welt", "an\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh sie heut und nimmermehr,", "tokens": ["Ich", "seh", "sie", "heut", "und", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meinen schwarzbraunen Augen.\u00ab", "tokens": ["Mit", "mei\u00b7nen", "schwarz\u00b7brau\u00b7nen", "Au\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Sein Vater beim Gerichte stand,", "tokens": ["Sein", "Va\u00b7ter", "beim", "Ge\u00b7rich\u00b7te", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Herz wollt ihm zerbrechen:", "tokens": ["Sein", "Herz", "wollt", "ihm", "zer\u00b7bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbach Sohne, liebster Sohne mein,", "tokens": ["\u00bb", "ach", "Soh\u00b7ne", ",", "liebs\u00b7ter", "Soh\u00b7ne", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein'n Tod will ich schon r\u00e4chen.\u00ab", "tokens": ["Dein'n", "Tod", "will", "ich", "schon", "r\u00e4\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbach Vater, liebster Vater mein,", "tokens": ["\u00bb", "ach", "Va\u00b7ter", ",", "liebs\u00b7ter", "Va\u00b7ter", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Meinen Tod sollt ihr nicht r\u00e4chen,", "tokens": ["Mei\u00b7nen", "Tod", "sollt", "ihr", "nicht", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Br\u00e4cht meiner Seele schwere Pein,", "tokens": ["Br\u00e4cht", "mei\u00b7ner", "See\u00b7le", "schwe\u00b7re", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um Unschuld will ich sterben.", "tokens": ["Um", "Un\u00b7schuld", "will", "ich", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Es ist nicht um das Leben mein,", "tokens": ["Es", "ist", "nicht", "um", "das", "Le\u00b7ben", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch um meinen stolzen Leibe,", "tokens": ["Noch", "um", "mei\u00b7nen", "stol\u00b7zen", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es ist um meine Frau Mut