{"textgrid.poem.26838": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kein Augustisch Alter bl\u00fchte,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Augustisch Alter bl\u00fchte,", "tokens": ["Kein", "Au\u00b7gus\u00b7tisch", "Al\u00b7ter", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Keines Mediceers G\u00fcte", "tokens": ["Kei\u00b7nes", "Me\u00b7di\u00b7ceers", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "L\u00e4chelte der deutschen Kunst,", "tokens": ["L\u00e4\u00b7chel\u00b7te", "der", "deut\u00b7schen", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie ward nicht gepflegt vom Ruhme,", "tokens": ["Sie", "ward", "nicht", "ge\u00b7pflegt", "vom", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie entfaltete die Blume", "tokens": ["Sie", "ent\u00b7fal\u00b7te\u00b7te", "die", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht am Strahl der F\u00fcrstengunst.", "tokens": ["Nicht", "am", "Strahl", "der", "F\u00fcrs\u00b7ten\u00b7gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von dem gr\u00f6\u00dften deutschen Sohne,", "tokens": ["Von", "dem", "gr\u00f6\u00df\u00b7ten", "deut\u00b7schen", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des gro\u00dfen Friedrichs Throne", "tokens": ["Von", "des", "gro\u00b7\u00dfen", "Fried\u00b7richs", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ging sie schutzlos, ungeehrt.", "tokens": ["Ging", "sie", "schutz\u00b7los", ",", "un\u00b7geehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "R\u00fchmend darfs der Deutsche sagen,", "tokens": ["R\u00fch\u00b7mend", "darfs", "der", "Deut\u00b7sche", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6her darf das Herz ihm schlagen:", "tokens": ["H\u00f6\u00b7her", "darf", "das", "Herz", "ihm", "schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Darum steigt in h\u00f6herm Bogen,", "tokens": ["Da\u00b7rum", "steigt", "in", "h\u00f6\u00b7herm", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darum str\u00f6mt in vollern Wogen", "tokens": ["Da\u00b7rum", "str\u00f6mt", "in", "vol\u00b7lern", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutscher Barden Hochgesang,", "tokens": ["Deut\u00b7scher", "Bar\u00b7den", "Hoch\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in eigner F\u00fclle schwellend", "tokens": ["Und", "in", "eig\u00b7ner", "F\u00fcl\u00b7le", "schwel\u00b7lend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und aus Herzens Tiefen quellend,", "tokens": ["Und", "aus", "Her\u00b7zens", "Tie\u00b7fen", "quel\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spottet er der Regeln Zwang.", "tokens": ["Spot\u00b7tet", "er", "der", "Re\u00b7geln", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}