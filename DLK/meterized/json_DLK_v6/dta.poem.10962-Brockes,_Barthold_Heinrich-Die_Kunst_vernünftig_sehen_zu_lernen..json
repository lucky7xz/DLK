{"dta.poem.10962": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Kunst  \n vern\u00fcnftig sehen zu lernen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich nun, mit allen Kr\u00e4ften, die gro\u00dfe Wahrheit\nvorgetragen,", "tokens": ["Nach\u00b7dem", "ich", "nun", ",", "mit", "al\u00b7len", "Kr\u00e4f\u00b7ten", ",", "die", "gro\u00b7\u00dfe", "Wahr\u00b7heit", "vor\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Da\u00df wir des Sch\u00f6pfers Creatur, wie sie so herrlich und so", "tokens": ["Da\u00df", "wir", "des", "Sch\u00f6p\u00b7fers", "Crea\u00b7tur", ",", "wie", "sie", "so", "herr\u00b7lich", "und", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "PWAV", "PPER", "ADV", "ADJD", "KON", "ADV"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So k\u00fcnst-und wunderlich gebildet (um seine Weisheit zu er-", "tokens": ["So", "k\u00fcnst\u00b7\u00b7und", "wun\u00b7der\u00b7lich", "ge\u00b7bil\u00b7det", "(", "um", "sei\u00b7ne", "Weis\u00b7heit", "zu", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "VVPP", "$(", "APPR", "PPOSAT", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Mit einem aufmerksamen Blick, verpflichtet seyn, oft anzu-", "tokens": ["Mit", "ei\u00b7nem", "auf\u00b7merk\u00b7sa\u00b7men", "Blick", ",", "ver\u00b7pflich\u00b7tet", "seyn", ",", "oft", "an\u00b7zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVPP", "VAINF", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "So h\u00f6r ich viele meiner Leser, hierauf vermuthlich dieses sagen:", "tokens": ["So", "h\u00f6r", "ich", "vie\u00b7le", "mei\u00b7ner", "Le\u00b7ser", ",", "hier\u00b7auf", "ver\u00b7muth\u00b7lich", "die\u00b7ses", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,", "PAV", "ADV", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Wir finden, da\u00df es alles wahr, wir sehen unsre Schul-", "tokens": ["Wir", "fin\u00b7den", ",", "da\u00df", "es", "al\u00b7les", "wahr", ",", "wir", "se\u00b7hen", "uns\u00b7re", "Schul"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PIS", "ADJD", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Wir sind von der Gesch\u00f6pfe Pracht, von ihrer Vollenkom-", "tokens": ["Wir", "sind", "von", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Pracht", ",", "von", "ih\u00b7rer", "Vol\u00b7len\u00b7kom"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Zum Ruhm des Sch\u00f6pfers, \u00fcberzeuget. Wir haben oft auch", "tokens": ["Zum", "Ruhm", "des", "Sch\u00f6p\u00b7fers", ",", "\u00fc\u00b7berz\u00b7eu\u00b7get", ".", "Wir", "ha\u00b7ben", "oft", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Auf sie zu unsers Gottes Ehren, die Augen ernsthaft hinzu-", "tokens": ["Auf", "sie", "zu", "un\u00b7sers", "Got\u00b7tes", "Eh\u00b7ren", ",", "die", "Au\u00b7gen", "ernst\u00b7haft", "hin\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Und, da\u00df sie wirklich sch\u00f6n, vortrefflich, und sehr betr\u00e4chtlich,", "tokens": ["Und", ",", "da\u00df", "sie", "wirk\u00b7lich", "sch\u00f6n", ",", "vor\u00b7treff\u00b7lich", ",", "und", "sehr", "be\u00b7tr\u00e4cht\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "ADJD", "$,", "ADJD", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Allein es ist uns unser\u2019 Absicht nur gar zu schlecht von statten", "tokens": ["Al\u00b7lein", "es", "ist", "uns", "un\u00b7ser'", "Ab\u00b7sicht", "nur", "gar", "zu", "schlecht", "von", "stat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "PTKA", "ADJD", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Wir sachen Gras und Kr\u00e4uter an, auch Feld und Wald: Allein", "tokens": ["Wir", "sa\u00b7chen", "Gras", "und", "Kr\u00e4u\u00b7ter", "an", ",", "auch", "Feld", "und", "Wald", ":", "Al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,", "ADV", "NN", "KON", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Uns alles meistens einerley. Gras, Kraut, und Feld und", "tokens": ["Uns", "al\u00b7les", "meis\u00b7tens", "ei\u00b7ner\u00b7ley", ".", "Gras", ",", "Kraut", ",", "und", "Feld", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PIS", "ADV", "ADV", "$.", "NN", "$,", "NN", "$,", "KON", "NN", "KON"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Die Erde braun, der Himmel blau. Daher wir bald zu Ende", "tokens": ["Die", "Er\u00b7de", "braun", ",", "der", "Him\u00b7mel", "blau", ".", "Da\u00b7her", "wir", "bald", "zu", "En\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$.", "PAV", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "Und kunnten wir, wie gern wir wollten, von aller Creaturen", "tokens": ["Und", "kunn\u00b7ten", "wir", ",", "wie", "gern", "wir", "woll\u00b7ten", ",", "von", "al\u00b7ler", "Crea\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$,", "PWAV", "ADV", "PPER", "VMFIN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Uns doch nicht recht ger\u00fchret f\u00fchlen. Hierauf erwiedr\u2019 ich:", "tokens": ["Uns", "doch", "nicht", "recht", "ge\u00b7r\u00fch\u00b7ret", "f\u00fch\u00b7len", ".", "Hier\u00b7auf", "er\u00b7wiedr'", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADJD", "VVPP", "VVINF", "$.", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Geliebte Leser, euer Klagen ist leider gar zu sehr gegr\u00fcndet,", "tokens": ["Ge\u00b7lieb\u00b7te", "Le\u00b7ser", ",", "eu\u00b7er", "Kla\u00b7gen", "ist", "lei\u00b7der", "gar", "zu", "sehr", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "PTKA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.18": {"text": "Weil diese Unverm\u00f6genheit beym ganzen menschlichen Geschlecht,", "tokens": ["Weil", "die\u00b7se", "Un\u00b7ver\u00b7m\u00f6\u00b7gen\u00b7heit", "beym", "gan\u00b7zen", "menschli\u00b7chen", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.19": {"text": "Durch b\u00f6s Exempel und Gewohnheit, sich leider! eingewurzelt", "tokens": ["Durch", "b\u00f6s", "Ex\u00b7em\u00b7pel", "und", "Ge\u00b7wohn\u00b7heit", ",", "sich", "lei\u00b7der", "!", "ein\u00b7ge\u00b7wur\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,", "PRF", "ADV", "$.", "VVPP"], "meter": "+-+-+--+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "So da\u00df wir, auch mit offnen Augen, nicht sehen, was wir sehn;", "tokens": ["So", "da\u00df", "wir", ",", "auch", "mit", "off\u00b7nen", "Au\u00b7gen", ",", "nicht", "se\u00b7hen", ",", "was", "wir", "sehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$,", "ADV", "APPR", "ADJA", "NN", "$,", "PTKNEG", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.21": {"text": "Was wir unwidersprechlich h\u00f6ren. Woher? Dieweil wir", "tokens": ["Was", "wir", "un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "h\u00f6\u00b7ren", ".", "Wo\u00b7her", "?", "Die\u00b7weil", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "VVINF", "$.", "PWAV", "$.", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "Von unsrer allerersten Pflicht. Wir haben nimmer sehn ge-", "tokens": ["Von", "uns\u00b7rer", "al\u00b7le\u00b7rers\u00b7ten", "Pflicht", ".", "Wir", "ha\u00b7ben", "nim\u00b7mer", "sehn", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "VVINF", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.23": {"text": "Auf eine Weise, die vern\u00fcnftig. Wir sehn: Allein es sieht ein", "tokens": ["Auf", "ei\u00b7ne", "Wei\u00b7se", ",", "die", "ver\u00b7n\u00fcnf\u00b7tig", ".", "Wir", "sehn", ":", "Al\u00b7lein", "es", "sieht", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$.", "PPER", "VVFIN", "$.", "ADV", "PPER", "VVFIN", "ART"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "So gut, als wir. Wo man den Geist, der unsern K\u00f6rpern ein-", "tokens": ["So", "gut", ",", "als", "wir", ".", "Wo", "man", "den", "Geist", ",", "der", "un\u00b7sern", "K\u00f6r\u00b7pern", "ein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "$.", "PWAV", "PIS", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "TRUNC"], "meter": "-+-++--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.25": {"text": "Und welchen uns, vor andern Thieren, der Sch\u00f6pfer uns vor-", "tokens": ["Und", "wel\u00b7chen", "uns", ",", "vor", "an\u00b7dern", "Thie\u00b7ren", ",", "der", "Sch\u00f6p\u00b7fer", "uns", "vor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Nicht mit der Sinnen Kraft verbindet, und wenn man sieht,", "tokens": ["Nicht", "mit", "der", "Sin\u00b7nen", "Kraft", "ver\u00b7bin\u00b7det", ",", "und", "wenn", "man", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "VVFIN", "$,", "KON", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "So sieht und h\u00f6rt der kl\u00fcgste Mensch wahrhaftig anders nicht,", "tokens": ["So", "sieht", "und", "h\u00f6rt", "der", "kl\u00fcgs\u00b7te", "Mensch", "wahr\u00b7haf\u00b7tig", "an\u00b7ders", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.28": {"text": "Um zu der Schule des Gesichts, auf eine Weise, nun zu kom-", "tokens": ["Um", "zu", "der", "Schu\u00b7le", "des", "Ge\u00b7sichts", ",", "auf", "ei\u00b7ne", "Wei\u00b7se", ",", "nun", "zu", "kom"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.29": {"text": "Die n\u00f6thig ist, und doch nicht schwer, hab ich mir jetzo vor-", "tokens": ["Die", "n\u00f6\u00b7thig", "ist", ",", "und", "doch", "nicht", "schwer", ",", "hab", "ich", "mir", "jet\u00b7zo", "vor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KON", "ADV", "PTKNEG", "ADJD", "$,", "VAFIN", "PPER", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.30": {"text": "Euch einen neuen Weg zu weisen. Da alle Werke der Natur,", "tokens": ["Euch", "ei\u00b7nen", "neu\u00b7en", "Weg", "zu", "wei\u00b7sen", ".", "Da", "al\u00b7le", "Wer\u00b7ke", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$.", "ADV", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.31": {"text": "Und alle g\u00f6ttliche Gesch\u00f6pfe, nur blo\u00df in ", "tokens": ["Und", "al\u00b7le", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7sch\u00f6p\u00b7fe", ",", "nur", "blo\u00df", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "ADV", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn man es recht erwegt, bestehn: So will ich eine Kunst", "tokens": ["Wenn", "man", "es", "recht", "er\u00b7wegt", ",", "be\u00b7stehn", ":", "So", "will", "ich", "ei\u00b7ne", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVPP", "$,", "VVINF", "$.", "ADV", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.33": {"text": "Der diese Vorw\u00fcrf alle beyde, so wohl ", "tokens": ["Der", "die\u00b7se", "Vor\u00b7w\u00fcrf", "al\u00b7le", "bey\u00b7de", ",", "so", "wohl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PDAT", "NN", "PIAT", "PIS", "$,", "ADV", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.34": {"text": "Und die daher, auf welche Weise man Gott in seinen Werken", "tokens": ["Und", "die", "da\u00b7her", ",", "auf", "wel\u00b7che", "Wei\u00b7se", "man", "Gott", "in", "sei\u00b7nen", "Wer\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PAV", "$,", "APPR", "PWAT", "NN", "PIS", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-----+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Wenn man sie mit Vernunft gebraucht, zumal vern\u00fcnftig se-", "tokens": ["Wenn", "man", "sie", "mit", "Ver\u00b7nunft", "ge\u00b7braucht", ",", "zu\u00b7mal", "ver\u00b7n\u00fcnf\u00b7tig", "se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVPP", "$,", "KOUS", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.36": {"text": "Die\u00df ist die edle ", "tokens": ["Die\u00df", "ist", "die", "ed\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.37": {"text": "Die kluge Mischung bunter Farben, die ", "tokens": ["Die", "klu\u00b7ge", "Mi