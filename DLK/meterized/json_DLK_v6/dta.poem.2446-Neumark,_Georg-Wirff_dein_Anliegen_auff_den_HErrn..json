{"dta.poem.2446": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Wirff dein Anliegen auff den  \n HErrn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wjrf deine Trangsalslast auff Gottes Gnadenr\u00fckken", "tokens": ["Wjrf", "dei\u00b7ne", "Trangs\u00b7als\u00b7last", "auff", "Got\u00b7tes", "Gna\u00b7den\u00b7r\u00fck\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Thr\u00e4nendem Gebe", "tokens": ["Mit", "Thr\u00e4\u00b7nen\u00b7dem", "Ge\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Dich retten aus der Nohe/ wen\u0303 du am meiste\u0303 weinst", "tokens": ["Dich", "ret\u00b7ten", "aus", "der", "No\u00b7he", "/", "we\u00f1", "du", "am", "meist\u1ebd", "weinst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "APPR", "ART", "NN", "$(", "KOUS", "PPER", "APPRART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So s\u00e4nget er schon an dich hertlich zuerquikken.", "tokens": ["So", "s\u00e4n\u00b7get", "er", "schon", "an", "dich", "hert\u00b7lich", "zu\u00b7er\u00b7quik\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}