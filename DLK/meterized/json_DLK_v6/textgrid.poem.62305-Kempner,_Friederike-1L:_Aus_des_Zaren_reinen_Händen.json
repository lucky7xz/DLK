{"textgrid.poem.62305": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus des Zaren reinen H\u00e4nden", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus des Zaren reinen H\u00e4nden", "tokens": ["Aus", "des", "Za\u00b7ren", "rei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmt die Welt den Frieden an,", "tokens": ["Nimmt", "die", "Welt", "den", "Frie\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die V\u00f6lker alle wie ein Mann", "tokens": ["Und", "die", "V\u00f6l\u00b7ker", "al\u00b7le", "wie", "ein", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihm den reichsten Segen spenden.", "tokens": ["Ihm", "den", "reichs\u00b7ten", "Se\u00b7gen", "spen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wollen all' die Waffen strecken,", "tokens": ["Wol\u00b7len", "all'", "die", "Waf\u00b7fen", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemals sich mit Blut beflecken;", "tokens": ["Nie\u00b7mals", "sich", "mit", "Blut", "be\u00b7fle\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn was niemals vor ihm war,", "tokens": ["Denn", "was", "nie\u00b7mals", "vor", "ihm", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will und schafft der junge Zar.", "tokens": ["Will", "und", "schafft", "der", "jun\u00b7ge", "Zar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es l\u00e4chelt die Geschichte,", "tokens": ["Und", "es", "l\u00e4\u00b7chelt", "die", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonst so ernsthaft im Gerichte.", "tokens": ["Sonst", "so", "ernst\u00b7haft", "im", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Edler Zar, bist Gott gesandt,", "tokens": ["Ed\u00b7ler", "Zar", ",", "bist", "Gott", "ge\u00b7sandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaffst das gr\u00f6\u00dfte Vaterland. \u2013 \u2013", "tokens": ["Schaffst", "das", "gr\u00f6\u00df\u00b7te", "Va\u00b7ter\u00b7land", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}