{"textgrid.poem.43106": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir war, als ob in dumpfem Schmerz", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir war, als ob in dumpfem Schmerz", "tokens": ["Mir", "war", ",", "als", "ob", "in", "dum\u00b7pfem", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seele wollt' erlahmen \u2013", "tokens": ["Die", "See\u00b7le", "wollt'", "er\u00b7lah\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da pl\u00f6tzlich, schier halb unbewu\u00dft,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", ",", "schier", "halb", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nannt' still ich deinen Namen.", "tokens": ["Nannt'", "still", "ich", "dei\u00b7nen", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und nun im selben Augenblick", "tokens": ["Und", "nun", "im", "sel\u00b7ben", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat es mich \u00fcberkommen,", "tokens": ["Hat", "es", "mich", "\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hab' mehr ", "tokens": ["Hab'", "mehr"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Drum ward es mir genommen.", "tokens": ["Drum", "ward", "es", "mir", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}