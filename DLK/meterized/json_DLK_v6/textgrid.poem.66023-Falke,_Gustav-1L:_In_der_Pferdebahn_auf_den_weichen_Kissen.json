{"textgrid.poem.66023": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der Pferdebahn auf den weichen Kissen", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Pferdebahn auf den weichen Kissen", "tokens": ["In", "der", "Pfer\u00b7de\u00b7bahn", "auf", "den", "wei\u00b7chen", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hast du es dir bequem gemacht.", "tokens": ["Hast", "du", "es", "dir", "be\u00b7quem", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschlissen, M\u00e4dchen, ist deine Tracht,", "tokens": ["Ver\u00b7schlis\u00b7sen", ",", "M\u00e4d\u00b7chen", ",", "ist", "dei\u00b7ne", "Tracht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die d\u00fcnnen Schuhe vorne zerrissen.", "tokens": ["Die", "d\u00fcn\u00b7nen", "Schu\u00b7he", "vor\u00b7ne", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und aus der Tasche", "tokens": ["Und", "aus", "der", "Ta\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Reckt die Flasche,", "tokens": ["Reckt", "die", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Die leere, den Hals verwegen", "tokens": ["Die", "lee\u00b7re", ",", "den", "Hals", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nach deinen vornehmen Fahrkollegen.", "tokens": ["Nach", "dei\u00b7nen", "vor\u00b7neh\u00b7men", "Fahr\u00b7kol\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Eine Dame zur Seite dir zieht ein wenig,", "tokens": ["Ei\u00b7ne", "Da\u00b7me", "zur", "Sei\u00b7te", "dir", "zieht", "ein", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "ART", "PIS", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Halbabgewandt, die Nase kraus.", "tokens": ["Hal\u00b7bab\u00b7ge\u00b7wandt", ",", "die", "Na\u00b7se", "kraus", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Du str\u00f6mst den Duft der Armut aus", "tokens": ["Du", "str\u00f6mst", "den", "Duft", "der", "Ar\u00b7mut", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Arbeit, und rekelst m\u00fcd und g\u00e4hnig.", "tokens": ["Und", "der", "Ar\u00b7beit", ",", "und", "re\u00b7kelst", "m\u00fcd", "und", "g\u00e4h\u00b7nig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "--+----+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Es ist dein Betragen,", "tokens": ["Es", "ist", "dein", "Be\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ehrlich zu sagen,", "tokens": ["Ehr\u00b7lich", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Dein ungeniertes nicht passend,", "tokens": ["Dein", "un\u00b7ge\u00b7nier\u00b7tes", "nicht", "pas\u00b7send", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gar zu plebejisch sich gehen lassend.", "tokens": ["Gar", "zu", "ple\u00b7be\u00b7jisch", "sich", "ge\u00b7hen", "las\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "PRF", "VVINF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber was scheren dich die andern,", "tokens": ["A\u00b7ber", "was", "sche\u00b7ren", "dich", "die", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du bist zum Umfallen m\u00fcde ja,", "tokens": ["Du", "bist", "zum", "Um\u00b7fal\u00b7len", "m\u00fc\u00b7de", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dem sanften Entschlummern verzweifelt nah.", "tokens": ["Dem", "sanf\u00b7ten", "Ent\u00b7schlum\u00b7mern", "ver\u00b7zwei\u00b7felt", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zerstreut nur l\u00e4sst du die Blicke wandern.", "tokens": ["Zer\u00b7streut", "nur", "l\u00e4sst", "du", "die", "Bli\u00b7cke", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So liegt ein K\u00e4tzchen", "tokens": ["So", "liegt", "ein", "K\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Auf behaglichem Pl\u00e4tzchen,", "tokens": ["Auf", "be\u00b7hag\u00b7li\u00b7chem", "Pl\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Halb m\u00fcd', halb lauernd blitzen", "tokens": ["Halb", "m\u00fcd'", ",", "halb", "lau\u00b7ernd", "blit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Augen durch die Liderritzen.", "tokens": ["Die", "Au\u00b7gen", "durch", "die", "Li\u00b7der\u00b7rit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du junges Ding mit den weichen Gliedern,", "tokens": ["Du", "jun\u00b7ges", "Ding", "mit", "den", "wei\u00b7chen", "Glie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der wei\u00dfen Haut, fast durchsichtig zart,", "tokens": ["Der", "wei\u00b7\u00dfen", "Haut", ",", "fast", "durch\u00b7sich\u00b7tig", "zart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bist du wohl auch von Katzenart?", "tokens": ["Bist", "du", "wohl", "auch", "von", "Kat\u00b7zen\u00b7art", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das leise Spiel mit den Augenlidern", "tokens": ["Das", "lei\u00b7se", "Spiel", "mit", "den", "Au\u00b7gen\u00b7li\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Macht mich betroffen,", "tokens": ["Macht", "mich", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und die Lippen, halb offen,", "tokens": ["Und", "die", "Lip\u00b7pen", ",", "halb", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Die schwellenden, zeigen die wei\u00dfen", "tokens": ["Die", "schwel\u00b7len\u00b7den", ",", "zei\u00b7gen", "die", "wei\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Spitzz\u00e4hnchen, als wie zum Bei\u00dfen.", "tokens": ["Spitz\u00b7z\u00e4hn\u00b7chen", ",", "als", "wie", "zum", "Bei\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Der Judenj\u00fcngling dir gegen\u00fcber", "tokens": ["Der", "Ju\u00b7den\u00b7j\u00fcng\u00b7ling", "dir", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit dem l\u00fcsternen Blick, h\u00e4tt' gerne gespielt", "tokens": ["Mit", "dem", "l\u00fcs\u00b7ter\u00b7nen", "Blick", ",", "h\u00e4tt'", "ger\u00b7ne", "ge\u00b7spielt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VAFIN", "ADV", "VVPP"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ein wenig mit dir. Schon lange schielt", "tokens": ["Ein", "we\u00b7nig", "mit", "dir", ".", "Schon", "lan\u00b7ge", "schielt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "PPER", "$.", "ADV", "ADV", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er verstohlen, fast sch\u00e4misch zu dir hin\u00fcber,", "tokens": ["Er", "ver\u00b7stoh\u00b7len", ",", "fast", "sch\u00e4\u00b7misch", "zu", "dir", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADV", "ADJD", "APPR", "PPER", "ADV", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Wird was draus?", "tokens": ["Wird", "was", "draus", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Katz oder Maus?", "tokens": ["Katz", "o\u00b7der", "Maus", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "H\u00fcte dich! Manche glaubt Katze", "tokens": ["H\u00fc\u00b7te", "dich", "!", "Man\u00b7che", "glaubt", "Kat\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "PIS", "VVFIN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Zu sein und bleibt als Maus auf dem Platze.", "tokens": ["Zu", "sein", "und", "bleibt", "als", "Maus", "auf", "dem", "Plat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "VVFIN", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}