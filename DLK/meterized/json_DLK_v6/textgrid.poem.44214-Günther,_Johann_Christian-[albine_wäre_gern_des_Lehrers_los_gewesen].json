{"textgrid.poem.44214": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[albine w\u00e4re gern des Lehrers los gewesen]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Albine w\u00e4re gern des Lehrers los gewesen,", "tokens": ["Al\u00b7bi\u00b7ne", "w\u00e4\u00b7re", "gern", "des", "Leh\u00b7rers", "los", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "PTKVZ", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der zehn Jahr Griechsch geh\u00f6rt und noch kein Wort gelesen.", "tokens": ["Der", "zehn", "Jahr", "Griech\u00b7sch", "ge\u00b7h\u00f6rt", "und", "noch", "kein", "Wort", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "NE", "VVFIN", "KON", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Pursche wollten nicht. Was macht er aber hier?", "tokens": ["Die", "Pur\u00b7sche", "woll\u00b7ten", "nicht", ".", "Was", "macht", "er", "a\u00b7ber", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "$.", "PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er hat Verdienst genug, er br\u00e4ut das beste Bier.", "tokens": ["Er", "hat", "Ver\u00b7dienst", "ge\u00b7nug", ",", "er", "br\u00e4ut", "das", "bes\u00b7te", "Bier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}