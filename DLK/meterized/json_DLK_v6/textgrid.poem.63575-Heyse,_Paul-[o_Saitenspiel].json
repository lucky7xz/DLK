{"textgrid.poem.63575": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[o Saitenspiel]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Saitenspiel", "tokens": ["O", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In schweigender Nacht,", "tokens": ["In", "schwei\u00b7gen\u00b7der", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wenn Tagesgew\u00fchl", "tokens": ["Wenn", "Ta\u00b7ges\u00b7ge\u00b7w\u00fchl"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Zur Ruhe gebracht!", "tokens": ["Zur", "Ru\u00b7he", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Worte verschwimmen", "tokens": ["Wor\u00b7te", "ver\u00b7schwim\u00b7men"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im Meer des Seins,", "tokens": ["Im", "Meer", "des", "Seins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Flammen verglimmen,", "tokens": ["Flam\u00b7men", "ver\u00b7glim\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "H\u00fcpfenden Scheins.", "tokens": ["H\u00fcp\u00b7fen\u00b7den", "Scheins", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Nicht Ton und Gestalt,", "tokens": ["Nicht", "Ton", "und", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Nicht Farb' und Sinn;", "tokens": ["Nicht", "Fa\u00b7rb'", "und", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit dunkler Gewalt", "tokens": ["Mit", "dunk\u00b7ler", "Ge\u00b7walt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Nimmt Liebe dich hin.", "tokens": ["Nimmt", "Lie\u00b7be", "dich", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Eins nur f\u00fchlst du:", "tokens": ["Eins", "nur", "f\u00fchlst", "du", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du bist zu Zwein.", "tokens": ["Du", "bist", "zu", "Zwein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auch das verd\u00e4mmert,", "tokens": ["Auch", "das", "ver\u00b7d\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Traum spinnt dich ein.", "tokens": ["Traum", "spinnt", "dich", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "++-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Dich st\u00e4rkt die Welle", "tokens": ["Dich", "st\u00e4rkt", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Ewigkeit", "tokens": ["Der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "F\u00fcr Himmel und H\u00f6lle", "tokens": ["F\u00fcr", "Him\u00b7mel", "und", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der nichtigen Zeit.", "tokens": ["Der", "nich\u00b7ti\u00b7gen", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}