{"dta.poem.1357": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Erh\u00f6re mich/ wenn ich mit Seuffzen vor dich trete/", "tokens": ["Er\u00b7h\u00f6\u00b7re", "mich", "/", "wenn", "ich", "mit", "Seuff\u00b7zen", "vor", "dich", "tre\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPER", "APPR", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "GoTT/ meiner rechten Sach ein GOtt/", "tokens": ["GoTT", "/", "mei\u00b7ner", "rech\u00b7ten", "Sach", "ein", "Gott", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der du mich tr\u00f6st in Angst und Noth/", "tokens": ["Der", "du", "mich", "tr\u00f6st", "in", "Angst", "und", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sey mir gen\u00e4dig und erhore mein Gebete.", "tokens": ["Sey", "mir", "ge\u00b7n\u00e4\u00b7dig", "und", "er\u00b7ho\u00b7re", "mein", "Ge\u00b7be\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Wie lange werdet ihr noch spotten meiner Ehren/", "tokens": ["Wie", "lan\u00b7ge", "wer\u00b7det", "ihr", "noch", "spot\u00b7ten", "mei\u00b7ner", "Eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr stoltzen Herren dieser Welt/", "tokens": ["Ihr", "stolt\u00b7zen", "Her\u00b7ren", "die\u00b7ser", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Eitelkeit gefangen h\u00e4lt/", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "ge\u00b7fan\u00b7gen", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die ihr euch so gern die L\u00fcgen last beth\u00f6ren.", "tokens": ["Und", "die", "ihr", "euch", "so", "gern", "die", "L\u00fc\u00b7gen", "last", "be\u00b7th\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erkennet/ da\u00df der HErr durch wunderliche Wege", "tokens": ["Er\u00b7ken\u00b7net", "/", "da\u00df", "der", "Herr", "durch", "wun\u00b7der\u00b7li\u00b7che", "We\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch allemahl zum Besten f\u00fchrt/", "tokens": ["Doch", "al\u00b7le\u00b7mahl", "zum", "Bes\u00b7ten", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die er von reinem Hertzen sp\u00fcrt/", "tokens": ["Die", "er", "von", "rei\u00b7nem", "Hert\u00b7zen", "sp\u00fcrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der HErr erh\u00f6rt/ wenn ich ihn anzuruffen pflege.", "tokens": ["Der", "Herr", "er\u00b7h\u00f6rt", "/", "wenn", "ich", "ihn", "an\u00b7zu\u00b7ruf\u00b7fen", "pfle\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "KOUS", "PPER", "PPER", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Betr\u00fcbet euren Geist der B\u00f6sen Wohlergehen/", "tokens": ["Be\u00b7tr\u00fc\u00b7bet", "eu\u00b7ren", "Geist", "der", "B\u00f6\u00b7sen", "Woh\u00b7ler\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die offt den Rosen gleiche bl\u00fchn/", "tokens": ["Die", "offt", "den", "Ro\u00b7sen", "glei\u00b7che", "bl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So murret doch nicht wider ihn/", "tokens": ["So", "mur\u00b7ret", "doch", "nicht", "wi\u00b7der", "ihn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lernet seinem Creutz und Pr\u00fcfung stille stehen.", "tokens": ["Und", "ler\u00b7net", "sei\u00b7nem", "Creutz", "und", "Pr\u00fc\u00b7fung", "stil\u00b7le", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ermahnet bey der Nacht auff eurem Thr\u00e4nen-Bette", "tokens": ["Er\u00b7mah\u00b7net", "bey", "der", "Nacht", "auff", "eu\u00b7rem", "Thr\u00e4\u00b7nen\u00b7Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Hertze zur Geduld im Leid/", "tokens": ["Das", "Hert\u00b7ze", "zur", "Ge\u00b7duld", "im", "Leid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beflei\u00dft euch der Gerechtigkeit/", "tokens": ["Be\u00b7flei\u00dft", "euch", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und glaubet/ da\u00df der HErr/ die auff ihn traun/ errette.", "tokens": ["Und", "glau\u00b7bet", "/", "da\u00df", "der", "Herr", "/", "die", "auff", "ihn", "traun", "/", "er\u00b7ret\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "ART", "NN", "$(", "ART", "APPR", "PPER", "VVINF", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Viel sagen/ solt uns der was Guttes k\u00f6nnen weisen/", "tokens": ["Viel", "sa\u00b7gen", "/", "solt", "uns", "der", "was", "Gut\u00b7tes", "k\u00f6n\u00b7nen", "wei\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$(", "VMFIN", "PPER", "ART", "PWS", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem selbsten H\u00fclff und Rath gebricht.", "tokens": ["Dem", "selbs\u00b7ten", "H\u00fclff", "und", "Rath", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "HeRR/ wend auff uns dein Gnaden-Licht/", "tokens": ["HeRR", "/", "wend", "auff", "uns", "dein", "Gna\u00b7den\u00b7Licht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "APPR", "PPER", "PPOSAT", "NN", "$("], "meter": "+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Da\u00df wir und alle Welt dein\u2019 H\u00fclffe m\u00fcssen preisen.", "tokens": ["Da\u00df", "wir", "und", "al\u00b7le", "Welt", "dein'", "H\u00fclf\u00b7fe", "m\u00fcs\u00b7sen", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PIAT", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es mag die falsche Schaar viel Wein und Korn besitzen:", "tokens": ["Es", "mag", "die", "fal\u00b7sche", "Schaar", "viel", "Wein", "und", "Korn", "be\u00b7sit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Schatz und meine Lust bist du/", "tokens": ["Mein", "Schatz", "und", "mei\u00b7ne", "Lust", "bist", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lieg und schlaff\u2019 in stiller Ruh/", "tokens": ["Ich", "lieg", "und", "schlaff'", "in", "stil\u00b7ler", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil mich der H\u00f6chste will vor allem Unfall sch\u00fctzen.", "tokens": ["Weil", "mich", "der", "H\u00f6chs\u00b7te", "will", "vor", "al\u00b7lem", "Un\u00b7fall", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "APPR", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}