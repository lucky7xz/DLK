{"textgrid.poem.50878": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling 1853", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch schauriger Lenz, der Sonne beraubt,", "tokens": ["Welch", "schau\u00b7ri\u00b7ger", "Lenz", ",", "der", "Son\u00b7ne", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um Pfingsten die B\u00e4ume noch nicht belaubt!", "tokens": ["Um", "Pfings\u00b7ten", "die", "B\u00e4u\u00b7me", "noch", "nicht", "be\u00b7laubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der Eisb\u00e4r sperrte den Rachen auf,", "tokens": ["Der", "Eis\u00b7b\u00e4r", "sperr\u00b7te", "den", "Ra\u00b7chen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Propheten hemmten der Erde Lauf;", "tokens": ["Pro\u00b7phe\u00b7ten", "hemm\u00b7ten", "der", "Er\u00b7de", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Hochgebildeten und Geweihten", "tokens": ["Die", "Hoch\u00b7ge\u00b7bil\u00b7de\u00b7ten", "und", "Ge\u00b7weih\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Knieten vor Tischen, die prophezeiten!", "tokens": ["Knie\u00b7ten", "vor", "Ti\u00b7schen", ",", "die", "pro\u00b7phe\u00b7zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Es war eine stechende Maienlust,", "tokens": ["Es", "war", "ei\u00b7ne", "ste\u00b7chen\u00b7de", "Mai\u00b7en\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Das S\u00e4ulein schrie in der Menschenbrust.", "tokens": ["Das", "S\u00e4u\u00b7lein", "schrie", "in", "der", "Men\u00b7schen\u00b7brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}