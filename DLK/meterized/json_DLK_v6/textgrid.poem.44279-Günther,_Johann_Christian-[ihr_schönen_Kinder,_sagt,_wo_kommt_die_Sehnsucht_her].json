{"textgrid.poem.44279": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[ihr sch\u00f6nen Kinder, sagt, wo kommt die Sehnsucht her]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr sch\u00f6nen Kinder, sagt, wo kommt die Sehnsucht her,", "tokens": ["Ihr", "sch\u00f6\u00b7nen", "Kin\u00b7der", ",", "sagt", ",", "wo", "kommt", "die", "Sehn\u00b7sucht", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch, die ihr Jungfern seyd, einander selbst zu k\u00fc\u00dfen?", "tokens": ["Euch", ",", "die", "ihr", "Jung\u00b7fern", "seyd", ",", "ein\u00b7an\u00b7der", "selbst", "zu", "k\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja, wenn halb Frau, halb Mann an euren Gliedern w\u00e4r,", "tokens": ["Ja", ",", "wenn", "halb", "Frau", ",", "halb", "Mann", "an", "eu\u00b7ren", "Glie\u00b7dern", "w\u00e4r", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ADJD", "NN", "$,", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So th\u00e4tet ihr noch recht, die seltne Lust zu b\u00fc\u00dfen.", "tokens": ["So", "th\u00e4\u00b7tet", "ihr", "noch", "recht", ",", "die", "selt\u00b7ne", "Lust", "zu", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein bedenckt es doch, das ist nur Brodt zu Brodt;", "tokens": ["Al\u00b7lein", "be\u00b7denckt", "es", "doch", ",", "das", "ist", "nur", "Brodt", "zu", "Brodt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PDS", "VAFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein Frauenzimmer kan bey seines gleichen f\u00fchlen,", "tokens": ["Kein", "Frau\u00b7en\u00b7zim\u00b7mer", "kan", "bey", "sei\u00b7nes", "glei\u00b7chen", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "(ich bitte, werdet nicht bey dieser Zeile roth!)", "tokens": ["(", "ich", "bit\u00b7te", ",", "wer\u00b7det", "nicht", "bey", "die\u00b7ser", "Zei\u00b7le", "roth", "!", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADV", "$,", "VAFIN", "PTKNEG", "APPR", "PDAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie z\u00e4rtlich und entz\u00fcckt verliebte Seelen spielen.", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "und", "ent\u00b7z\u00fcckt", "ver\u00b7lieb\u00b7te", "See\u00b7len", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVPP", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Begehrt ihr rechten Scherz, so h\u00f6rt den guten Rath", "tokens": ["Be\u00b7gehrt", "ihr", "rech\u00b7ten", "Scherz", ",", "so", "h\u00f6rt", "den", "gu\u00b7ten", "Rath"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und last mich zwischen euch und eure K\u00fc\u00dfe liegen;", "tokens": ["Und", "last", "mich", "zwi\u00b7schen", "euch", "und", "eu\u00b7re", "K\u00fc\u00b7\u00dfe", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wahr ich ehrlich bin, ich will auf frischer That", "tokens": ["So", "wahr", "ich", "ehr\u00b7lich", "bin", ",", "ich", "will", "auf", "fri\u00b7scher", "That"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "VAFIN", "$,", "PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit Nachdruck euren Geist viel z\u00e4rtlicher vergn\u00fcgen.", "tokens": ["Mit", "Nach\u00b7druck", "eu\u00b7ren", "Geist", "viel", "z\u00e4rt\u00b7li\u00b7cher", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nur fraget nicht voraus, wer diesen Reim gesezt;", "tokens": ["Nur", "fra\u00b7get", "nicht", "vo\u00b7raus", ",", "wer", "die\u00b7sen", "Reim", "ge\u00b7sezt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PWS", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es ist zum wenigsten ein Mensch, der euch verehret", "tokens": ["Es", "ist", "zum", "we\u00b7nigs\u00b7ten", "ein", "Mensch", ",", "der", "euch", "ver\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "PIS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.15": {"text": "Und der euch, wo ihr ihn des Umgangs w\u00fcrdig sch\u00e4zt,", "tokens": ["Und", "der", "euch", ",", "wo", "ihr", "ihn", "des", "Um\u00b7gangs", "w\u00fcr\u00b7dig", "sch\u00e4zt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "$,", "PWAV", "PPER", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vielleicht manch sch\u00f6ner Ding als ihr einander lehret.", "tokens": ["Viel\u00b7leicht", "manch", "sch\u00f6\u00b7ner", "Ding", "als", "ihr", "ein\u00b7an\u00b7der", "leh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}