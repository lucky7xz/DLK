{"textgrid.poem.38528": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6nt' ich das Entz\u00fcken Adams malen,", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nt' ich das Entz\u00fcken Adams malen,", "tokens": ["K\u00f6nt'", "ich", "das", "Ent\u00b7z\u00fc\u00b7ken", "A\u00b7dams", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NE", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als in Edens aufgebl\u00fchten Thalen", "tokens": ["Als", "in", "E\u00b7dens", "auf\u00b7ge\u00b7bl\u00fch\u00b7ten", "Tha\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihn des ersten Lenzes Duft umflo\u00df,", "tokens": ["Ihn", "des", "ers\u00b7ten", "Len\u00b7zes", "Duft", "um\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sanft ihn zu begr\u00fcssen, jede Kehle", "tokens": ["Sanft", "ihn", "zu", "be\u00b7gr\u00fcs\u00b7sen", ",", "je\u00b7de", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Seines Hains ihm in die trunkne Seele", "tokens": ["Sei\u00b7nes", "Hains", "ihm", "in", "die", "trunk\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Niegef\u00fchlte Seligkeiten go\u00df!", "tokens": ["Nie\u00b7ge\u00b7f\u00fchl\u00b7te", "Se\u00b7lig\u00b7kei\u00b7ten", "go\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dann, o! dann wollt' ich, auf Adlerschwingen,", "tokens": ["Dann", ",", "o", "!", "dann", "wollt'", "ich", ",", "auf", "Ad\u00b7ler\u00b7schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "$.", "ADV", "VMFIN", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dich, o Taumelquell der Wonne, singen,", "tokens": ["Dich", ",", "o", "Tau\u00b7mel\u00b7quell", "der", "Won\u00b7ne", ",", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dessen Nektar meine Seele trank,", "tokens": ["Des\u00b7sen", "Nek\u00b7tar", "mei\u00b7ne", "See\u00b7le", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als der Lenz, mit morgenrother Wange,", "tokens": ["Als", "der", "Lenz", ",", "mit", "mor\u00b7gen\u00b7ro\u00b7ther", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Unter hellem Nachtigallgesange,", "tokens": ["Un\u00b7ter", "hel\u00b7lem", "Nach\u00b7ti\u00b7gall\u00b7ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Heut' in Laura's Garten niedersank!", "tokens": ["Heut'", "in", "Lau\u00b7ra's", "Gar\u00b7ten", "nie\u00b7der\u00b7sank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}