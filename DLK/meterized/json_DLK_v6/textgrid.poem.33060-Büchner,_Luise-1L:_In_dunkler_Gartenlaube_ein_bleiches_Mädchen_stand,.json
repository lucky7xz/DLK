{"textgrid.poem.33060": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dunkler Gartenlaube ein bleiches M\u00e4dchen stand,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dunkler Gartenlaube ein bleiches M\u00e4dchen stand,", "tokens": ["In", "dunk\u00b7ler", "Gar\u00b7ten\u00b7lau\u00b7be", "ein", "blei\u00b7ches", "M\u00e4d\u00b7chen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie hielt zwei duft'ge Rosen in ihrer wei\u00dfen Hand;", "tokens": ["Sie", "hielt", "zwei", "duft'\u00b7ge", "Ro\u00b7sen", "in", "ih\u00b7rer", "wei\u00b7\u00dfen", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und in den Kelch der rothen schaut sie mit tr\u00fcbem Schmerz:", "tokens": ["Und", "in", "den", "Kelch", "der", "ro\u00b7then", "schaut", "sie", "mit", "tr\u00fc\u00b7bem", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "So gl\u00fchte und so prangte auch einst mein junges Herz;", "tokens": ["So", "gl\u00fch\u00b7te", "und", "so", "prang\u00b7te", "auch", "einst", "mein", "jun\u00b7ges", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADV", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "So trank's in hei\u00dfen Z\u00fcgen des Lebens Morgenluft,", "tokens": ["So", "trank's", "in", "hei\u00b7\u00dfen", "Z\u00fc\u00b7gen", "des", "Le\u00b7bens", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "So quoll aus seinem Innern der Liebe s\u00fc\u00dfer Duft.", "tokens": ["So", "quoll", "aus", "sei\u00b7nem", "In\u00b7nern", "der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfer", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Jetzt gleicht's der wei\u00dfen Rose, so kalt ist es und mild,", "tokens": ["Jetzt", "gleicht's", "der", "wei\u00b7\u00dfen", "Ro\u00b7se", ",", "so", "kalt", "ist", "es", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VAFIN", "PPER", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Wohl ruht darin geborgen manch' liebes, theures Bild,", "tokens": ["Wohl", "ruht", "da\u00b7rin", "ge\u00b7bor\u00b7gen", "man\u00b7ch'", "lie\u00b7bes", ",", "theu\u00b7res", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "VVPP", "PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Doch ist sein Feu'r erloschen, sein Klopfen f\u00fchl' ich kaum,", "tokens": ["Doch", "ist", "sein", "Feu'r", "er\u00b7lo\u00b7schen", ",", "sein", "Klop\u00b7fen", "f\u00fchl'", "ich", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Bald werd' im Grab vergessen ich ganz der Jugend Traum.", "tokens": ["Bald", "werd'", "im", "Grab", "ver\u00b7ges\u00b7sen", "ich", "ganz", "der", "Ju\u00b7gend", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Und wenn auf meinem H\u00fcgel dann wei\u00dfe Rosen bl\u00fch'n,", "tokens": ["Und", "wenn", "auf", "mei\u00b7nem", "H\u00fc\u00b7gel", "dann", "wei\u00b7\u00dfe", "Ro\u00b7sen", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "So la\u00dft an ihrer Seite auch rothe Knospen gl\u00fch'n,", "tokens": ["So", "la\u00dft", "an", "ih\u00b7rer", "Sei\u00b7te", "auch", "ro\u00b7the", "Knos\u00b7pen", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Den Schl\u00e4fer einst zu nennen, des Denksteins braucht es nicht,", "tokens": ["Den", "Schl\u00e4\u00b7fer", "einst", "zu", "nen\u00b7nen", ",", "des", "Denk\u00b7steins", "braucht", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Mein Hoffen und Entsagen aus ihren Kelchen spricht!\u00ab", "tokens": ["Mein", "Hof\u00b7fen", "und", "Ent\u00b7sa\u00b7gen", "aus", "ih\u00b7ren", "Kel\u00b7chen", "spricht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}