{"dta.poem.153": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ein nicht zu junges Frauen-Zim\u0304er  raison-  \n ni ret \u00fcber das Sprich-Wort: Die Alten/ Sind die  \n Kalten; nach vorgeschriebenen End-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr frischen Buhlers sprecht: was n\u00fctzen uns die - - Alten/", "tokens": ["Ihr", "fri\u00b7schen", "Buh\u00b7lers", "sprecht", ":", "was", "n\u00fct\u00b7zen", "uns", "die", "Al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "ART", "$(", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Denn eure Losung heist: die Alten sind die - - kalten/", "tokens": ["Denn", "eu\u00b7re", "Lo\u00b7sung", "heist", ":", "die", "Al\u00b7ten", "sind", "die", "kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "ART", "$(", "$(", "ADJA", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Allein probiret erst der Alten ihre - - Spalten/", "tokens": ["Al\u00b7lein", "pro\u00b7bi\u00b7ret", "erst", "der", "Al\u00b7ten", "ih\u00b7re", "Spal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "$(", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und f\u00fchlet/ da\u00df sie auch die L\u00fcste k\u00f6nnen - - halten/", "tokens": ["Und", "f\u00fch\u00b7let", "/", "da\u00df", "sie", "auch", "die", "L\u00fcs\u00b7te", "k\u00f6n\u00b7nen", "hal\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "VMFIN", "$(", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "So werdt ihr nicht so sehr verachten ihre - - Falten/", "tokens": ["So", "werdt", "ihr", "nicht", "so", "sehr", "ver\u00b7ach\u00b7ten", "ih\u00b7re", "Fal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN", "PPOSAT", "$(", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Und ja so gern in ihr als einer jungen - - walten/", "tokens": ["Und", "ja", "so", "gern", "in", "ihr", "als", "ei\u00b7ner", "jun\u00b7gen", "wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPR", "PPER", "KOUS", "ART", "ADJA", "$(", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Weil ihr mit gleicher Lust daselbsten k\u00f6nnet - - schalten", "tokens": ["Weil", "ihr", "mit", "glei\u00b7cher", "Lust", "da\u00b7selbs\u00b7ten", "k\u00f6n\u00b7net", "schal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVINF", "VMFIN", "$(", "$(", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auch Feur und Hitze sp\u00fchrt bey einer nicht zu - - alten.", "tokens": ["Auch", "Feur", "und", "Hit\u00b7ze", "sp\u00fchrt", "bey", "ei\u00b7ner", "nicht", "zu", "al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "PTKNEG", "PTKZU", "$(", "$(", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Doch sie mu\u00df \u00e4lter nicht als vierzig Jahre - - seyn", "tokens": ["Doch", "sie", "mu\u00df", "\u00e4l\u00b7ter", "nicht", "als", "vier\u00b7zig", "Jah\u00b7re", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "PTKNEG", "KOKOM", "CARD", "NN", "$(", "$(", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Hencker gehe sonst mit ihr die Liebe - - ein/", "tokens": ["Der", "Hen\u00b7cker", "ge\u00b7he", "sonst", "mit", "ihr", "die", "Lie\u00b7be", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$(", "$(", "ART", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "Denn welcher sehnt sich wol nach einen Todten - - Schein/", "tokens": ["Denn", "wel\u00b7cher", "sehnt", "sich", "wol", "nach", "ei\u00b7nen", "Tod\u00b7ten", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$(", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer machet sich vor Lust im Lieben Hertzens - - Pein?", "tokens": ["Wer", "ma\u00b7chet", "sich", "vor", "Lust", "im", "Lie\u00b7ben", "Hert\u00b7zens", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "NN", "APPRART", "ADJA", "NN", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der alte Most geht vor dem jung-und frischen - - Wein/", "tokens": ["Der", "al\u00b7te", "Most", "geht", "vor", "dem", "jung\u00b7\u00b7und", "fri\u00b7schen", "Wein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$(", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ein bewandtes Weib wird recht aptitlich - - seyn.", "tokens": ["Und", "ein", "be\u00b7wand\u00b7tes", "Weib", "wird", "recht", "ap\u00b7tit\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$(", "$(", "VAINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}