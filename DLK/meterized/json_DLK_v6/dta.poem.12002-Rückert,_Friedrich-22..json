{"dta.poem.12002": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Leib, h\u00e4tt' ich den Leib geliebt, mich macht' es grauen,", "tokens": ["Den", "Leib", ",", "h\u00e4tt'", "ich", "den", "Leib", "ge\u00b7liebt", ",", "mich", "macht'", "es", "grau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den von der Seele nun verlassnen Leib zu schauen.", "tokens": ["Den", "von", "der", "See\u00b7le", "nun", "ver\u00b7lass\u00b7nen", "Leib", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Seele liebten wir, doch weil im Leib wir blieben,", "tokens": ["Die", "See\u00b7le", "lieb\u00b7ten", "wir", ",", "doch", "weil", "im", "Leib", "wir", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADV", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So konnten wir auch nur geleibte Seelen lieben.", "tokens": ["So", "konn\u00b7ten", "wir", "auch", "nur", "ge\u00b7leib\u00b7te", "See\u00b7len", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Geliebte Seelen, die ihr eurem Leib entschwebtet,", "tokens": ["Ge\u00b7lieb\u00b7te", "See\u00b7len", ",", "die", "ihr", "eu\u00b7rem", "Leib", "ent\u00b7schweb\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr lebt mir, doch ihr lebt mir anders als ihr lebtet.", "tokens": ["Ihr", "lebt", "mir", ",", "doch", "ihr", "lebt", "mir", "an\u00b7ders", "als", "ihr", "leb\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich euch lieben k\u00f6nn', o kommt mich zu umwalten,", "tokens": ["Da\u00df", "ich", "euch", "lie\u00b7ben", "k\u00f6nn'", ",", "o", "kommt", "mich", "zu", "um\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "FM", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr k\u00f6nnt's, in lieblichen und leiblichen Gestalten.", "tokens": ["Ihr", "k\u00f6nnt's", ",", "in", "lieb\u00b7li\u00b7chen", "und", "leib\u00b7li\u00b7chen", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "La\u00dft mich vergessen, da\u00df ich je sah Todtenz\u00fcge!", "tokens": ["La\u00dft", "mich", "ver\u00b7ges\u00b7sen", ",", "da\u00df", "ich", "je", "sah", "Tod\u00b7ten\u00b7z\u00fc\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Lebens Schein ist wahr, der Tod ist eine L\u00fcge.", "tokens": ["Des", "Le\u00b7bens", "Schein", "ist", "wahr", ",", "der", "Tod", "ist", "ei\u00b7ne", "L\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was anders kann der Tod als gleich der L\u00fcg' erblassen,", "tokens": ["Was", "an\u00b7ders", "kann", "der", "Tod", "als", "gleich", "der", "L\u00fcg'", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "ART", "NN", "KOKOM", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil von der Wahrheit er, vom Leben, ist verlassen!", "tokens": ["Weil", "von", "der", "Wahr\u00b7heit", "er", ",", "vom", "Le\u00b7ben", ",", "ist", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "$,", "APPRART", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}