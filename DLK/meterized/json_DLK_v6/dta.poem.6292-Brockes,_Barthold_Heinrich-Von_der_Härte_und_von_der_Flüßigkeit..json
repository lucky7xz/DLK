{"dta.poem.6292": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus eben diesem Grund entdecket man,", "tokens": ["Aus", "e\u00b7ben", "die\u00b7sem", "Grund", "ent\u00b7de\u00b7cket", "man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warum die C\u00f6rper all theils fl\u00fc\u00dfig und theils hart:", "tokens": ["Wa\u00b7rum", "die", "C\u00f6r\u00b7per", "all", "theils", "fl\u00fc\u00b7\u00dfig", "und", "theils", "hart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir sehn dadurch, wie offt die Fluth erstarrt,", "tokens": ["Wir", "sehn", "da\u00b7durch", ",", "wie", "offt", "die", "Fluth", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PWAV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wie sie sich im Frost verh\u00e4rten kan.", "tokens": ["Und", "wie", "sie", "sich", "im", "Frost", "ver\u00b7h\u00e4r\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch Boreas ergrimmtes Rasen,", "tokens": ["Durch", "Bo\u00b7re\u00b7as", "er\u00b7grimm\u00b7tes", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sein durchdringend kaltes Blasen,", "tokens": ["Und", "sein", "durch\u00b7drin\u00b7gend", "kal\u00b7tes", "Bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sind Lufft und Land gepresset und verschlossen,", "tokens": ["Sind", "Lufft", "und", "Land", "ge\u00b7pres\u00b7set", "und", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Fluth wird fest, und widersteht der Macht", "tokens": ["Die", "Fluth", "wird", "fest", ",", "und", "wi\u00b7der\u00b7steht", "der", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der kleinen C\u00f6rperchen, durch die sie sonst geflossen", "tokens": ["Der", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7chen", ",", "durch", "die", "sie", "sonst", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Und in Bewegung war gebracht.", "tokens": ["Und", "in", "Be\u00b7we\u00b7gung", "war", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es h\u00f6ren auf sich ferner zu bewegen", "tokens": ["Es", "h\u00f6\u00b7ren", "auf", "sich", "fer\u00b7ner", "zu", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die kleine Theil\u2019, und werden starr; sie regen", "tokens": ["Die", "klei\u00b7ne", "Theil'", ",", "und", "wer\u00b7den", "starr", ";", "sie", "re\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VAFIN", "ADJD", "$.", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Sich ferner nicht, und keines rennt,", "tokens": ["Sich", "fer\u00b7ner", "nicht", ",", "und", "kei\u00b7nes", "rennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie vormals, nach dem Druck vom ersten Element.", "tokens": ["Wie", "vor\u00b7mals", ",", "nach", "dem", "Druck", "vom", "ers\u00b7ten", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Theilchen hangen sich zusammen, sie verbinden", "tokens": ["Die", "Theil\u00b7chen", "han\u00b7gen", "sich", "zu\u00b7sam\u00b7men", ",", "sie", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und h\u00e4rten sich, wie wir es finden,", "tokens": ["Und", "h\u00e4r\u00b7ten", "sich", ",", "wie", "wir", "es", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "PWAV", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und da sie gleichsam sich zur Ruhe legen,", "tokens": ["Und", "da", "sie", "gleich\u00b7sam", "sich", "zur", "Ru\u00b7he", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Verliehren sie ihr voriges Bewegen.", "tokens": ["Ver\u00b7lieh\u00b7ren", "sie", "ihr", "vo\u00b7ri\u00b7ges", "Be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}