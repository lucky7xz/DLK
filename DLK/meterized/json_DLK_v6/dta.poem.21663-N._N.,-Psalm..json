{"dta.poem.21663": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe die Tiefen des Elends geschaut,", "tokens": ["Ich", "ha\u00b7be", "die", "Tie\u00b7fen", "des", "E\u00b7lends", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und es hat mir in Tiefen der Seele gegraut,", "tokens": ["Und", "es", "hat", "mir", "in", "Tie\u00b7fen", "der", "See\u00b7le", "ge\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Ich sahe lebendiger Todten Skelett", "tokens": ["Ich", "sa\u00b7he", "le\u00b7ben\u00b7di\u00b7ger", "Tod\u00b7ten", "Ske\u00b7lett"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und stand an der Buhlen entweihtem Bett,", "tokens": ["Und", "stand", "an", "der", "Buh\u00b7len", "ent\u00b7weih\u00b7tem", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich nahte gefallenen Engeln viel,", "tokens": ["Ich", "nah\u00b7te", "ge\u00b7fal\u00b7le\u00b7nen", "En\u00b7geln", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der s\u00fc\u00dfesten S\u00fcnde entsetzlichem Spiel,", "tokens": ["Der", "s\u00fc\u00b7\u00dfes\u00b7ten", "S\u00fcn\u00b7de", "ent\u00b7setz\u00b7li\u00b7chem", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die stolze Vermessenheit sah ich im Schwang", "tokens": ["Die", "stol\u00b7ze", "Ver\u00b7mes\u00b7sen\u00b7heit", "sah", "ich", "im", "Schwang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und lauschte der Reichen beth\u00f6rtem Gesang,", "tokens": ["Und", "lauschte", "der", "Rei\u00b7chen", "be\u00b7th\u00f6r\u00b7tem", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die Seelen sah ich verkauft und feil,", "tokens": ["Die", "See\u00b7len", "sah", "ich", "ver\u00b7kauft", "und", "feil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Nach Gold und Ehre und Wollust geil,", "tokens": ["Nach", "Gold", "und", "Eh\u00b7re", "und", "Wol\u00b7lust", "geil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Der Knechte traf ich ein zahllos Heer", "tokens": ["Der", "Knech\u00b7te", "traf", "ich", "ein", "zahl\u00b7los", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.12": {"text": "Und fand der L\u00fcgner und Heuchler noch mehr,", "tokens": ["Und", "fand", "der", "L\u00fcg\u00b7ner", "und", "Heuch\u00b7ler", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN", "ADV", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Im Bethaus sah ich vor Gott sie knien", "tokens": ["Im", "Be\u00b7thaus", "sah", "ich", "vor", "Gott", "sie", "kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Und sah, wie sie heimlich den Heiland bespien", "tokens": ["Und", "sah", ",", "wie", "sie", "heim\u00b7lich", "den", "Hei\u00b7land", "be\u00b7spi\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.15": {"text": "Und lachten verborgen und trieben Hohn,", "tokens": ["Und", "lach\u00b7ten", "ver\u00b7bor\u00b7gen", "und", "trie\u00b7ben", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "KON", "VVFIN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Und leckten doch s\u00fcndisch an Kreuz und Thron,", "tokens": ["Und", "leck\u00b7ten", "doch", "s\u00fcn\u00b7disch", "an", "Kreuz", "und", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Und ich sah, was mir h\u00f6llisch die Sinne gepackt,", "tokens": ["Und", "ich", "sah", ",", "was", "mir", "h\u00f6l\u00b7lisch", "die", "Sin\u00b7ne", "ge\u00b7packt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWS", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Sie die Wahrheit nothz\u00fccht\u2019gen und peitschten sie nackt \u2014", "tokens": ["Sie", "die", "Wahr\u00b7heit", "noth\u00b7z\u00fccht'\u00b7gen", "und", "peitschten", "sie", "nackt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Und zu Boden sank ich und rang und rang", "tokens": ["Und", "zu", "Bo\u00b7den", "sank", "ich", "und", "rang", "und", "rang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "KON", "VVFIN", "KON", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Und siechte todtm\u00fcde und heillos bang,", "tokens": ["Und", "siech\u00b7te", "todt\u00b7m\u00fc\u00b7de", "und", "heil\u00b7los", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Meine Seele war w\u00fcst, und mein Geist war Nacht,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "war", "w\u00fcst", ",", "und", "mein", "Geist", "war", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Da flammte ein Strahl, nun bin ich erwacht", "tokens": ["Da", "flamm\u00b7te", "ein", "Strahl", ",", "nun", "bin", "ich", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und ich schreie empor voll br\u00fcnstiger Gluth:", "tokens": ["Und", "ich", "schrei\u00b7e", "em\u00b7por", "voll", "br\u00fcns\u00b7ti\u00b7ger", "Gluth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "Du Geist der Welten, verleih\u2019 uns Muth,", "tokens": ["Du", "Geist", "der", "Wel\u00b7ten", ",", "ver\u00b7leih'", "uns", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Da\u00df das Zagen zergeht und der Zweifel zerbricht,", "tokens": ["Da\u00df", "das", "Za\u00b7gen", "zer\u00b7geht", "und", "der", "Zwei\u00b7fel", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Zu sehnen und suchen das ewige Licht,", "tokens": ["Zu", "seh\u00b7nen", "und", "su\u00b7chen", "das", "e\u00b7wi\u00b7ge", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.27": {"text": "In harrender Treu, in Gedanken und That,", "tokens": ["In", "har\u00b7ren\u00b7der", "Treu", ",", "in", "Ge\u00b7dan\u00b7ken", "und", "That", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.28": {"text": "Wann der Abend sinkt, wann der Morgen naht,", "tokens": ["Wann", "der", "A\u00b7bend", "sinkt", ",", "wann", "der", "Mor\u00b7gen", "naht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Mit der Liebe Gewaffen im brennenden Kampf,", "tokens": ["Mit", "der", "Lie\u00b7be", "Ge\u00b7waf\u00b7fen", "im", "bren\u00b7nen\u00b7den", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.30": {"text": "Schildleuchtende Helden im Nebeldampf", "tokens": ["Schild\u00b7leuch\u00b7ten\u00b7de", "Hel\u00b7den", "im", "Ne\u00b7bel\u00b7dampf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "Mit des Mitleids Ruf, mit der Wahrheit Speer,", "tokens": ["Mit", "des", "Mit\u00b7leids", "Ruf", ",", "mit", "der", "Wahr\u00b7heit", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Zahllos sich mehrend ein ", "tokens": ["Zahl\u00b7los", "sich", "meh\u00b7rend", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PRF", "ADJD", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.33": {"text": "Zu l\u00f6sen das Leid und die Welt zu befrei\u2019n \u2014", "tokens": ["Zu", "l\u00f6\u00b7sen", "das", "Leid", "und", "die", "Welt", "zu", "be\u00b7frei'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "KON", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.34": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}}}}