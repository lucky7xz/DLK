{"textgrid.poem.24214": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitternacht. In wei\u00dfen Kutten graben", "tokens": ["Mit\u00b7ter\u00b7nacht", ".", "In", "wei\u00b7\u00dfen", "Kut\u00b7ten", "gra\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich Trappisten ihre letzte Ruhstatt.", "tokens": ["Sich", "Trap\u00b7pis\u00b7ten", "ih\u00b7re", "letz\u00b7te", "Ruh\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbave rosa sine spiris\u00ab singen", "tokens": ["\u00bb", "a\u00b7ve", "ro\u00b7sa", "si\u00b7ne", "spi\u00b7ris", "\u00ab", "sin\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Herzen, aber ihre Lippen", "tokens": ["Ih\u00b7re", "Her\u00b7zen", ",", "a\u00b7ber", "ih\u00b7re", "Lip\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Singen nicht.", "tokens": ["Sin\u00b7gen", "nicht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "\u00bbsei gegr\u00fc\u00dft, du rote dornenlose", "tokens": ["\u00bb", "sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "ro\u00b7te", "dor\u00b7nen\u00b7lo\u00b7se"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VAFIN", "VVPP", "$,", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rose, reinste aller Rosen, gro\u00dfe", "tokens": ["Ro\u00b7se", ",", "reins\u00b7te", "al\u00b7ler", "Ro\u00b7sen", ",", "gro\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "VVFIN", "PIAT", "NN", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weltenrose, Jungfrau, sei gegr\u00fc\u00dft!", "tokens": ["Wel\u00b7ten\u00b7ro\u00b7se", ",", "Jung\u00b7frau", ",", "sei", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dornen haben, ach, uns wundgestochen,", "tokens": ["Dor\u00b7nen", "ha\u00b7ben", ",", "ach", ",", "uns", "wund\u00b7ge\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ITJ", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Doch der Herzenswunde bittres Pochen", "tokens": ["Doch", "der", "Her\u00b7zens\u00b7wun\u00b7de", "bitt\u00b7res", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hat dein Duften selig \u00fcbers\u00fc\u00dft!\u00ab", "tokens": ["Hat", "dein", "Duf\u00b7ten", "se\u00b7lig", "\u00fc\u00b7ber\u00b7s\u00fc\u00dft", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Lassen Schwunges schaufeln sie die Erde,", "tokens": ["Las\u00b7sen", "Schwun\u00b7ges", "schau\u00b7feln", "sie", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bis sie Raum gewonnen ihren Leibern,", "tokens": ["Bis", "sie", "Raum", "ge\u00b7won\u00b7nen", "ih\u00b7ren", "Lei\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und sie legen sich zum Sterben nieder,", "tokens": ["Und", "sie", "le\u00b7gen", "sich", "zum", "Ster\u00b7ben", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einmal noch die schmalen Lippen \u00f6ffnend:", "tokens": ["Ein\u00b7mal", "noch", "die", "schma\u00b7len", "Lip\u00b7pen", "\u00f6ff\u00b7nend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "\u00bbsei gegr\u00fc\u00dft!\u00ab", "tokens": ["\u00bb", "sei", "ge\u00b7gr\u00fc\u00dft", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}}}}