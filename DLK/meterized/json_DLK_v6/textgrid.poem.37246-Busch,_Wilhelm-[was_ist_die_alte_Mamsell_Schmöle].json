{"textgrid.poem.37246": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[was ist die alte Mamsell Schm\u00f6le]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist die alte Mamsell Schm\u00f6le", "tokens": ["Was", "ist", "die", "al\u00b7te", "Mam\u00b7sell", "Schm\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr eine liebe, treue Seele!", "tokens": ["F\u00fcr", "ei\u00b7ne", "lie\u00b7be", ",", "treu\u00b7e", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie spricht zu ihrer Dienerin:", "tokens": ["Sie", "spricht", "zu", "ih\u00b7rer", "Die\u00b7ne\u00b7rin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, Rike, geh Sie da nicht hin!", "tokens": ["Ach", ",", "Ri\u00b7ke", ",", "geh", "Sie", "da", "nicht", "hin", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was will Sie da im goldnen L\u00f6ben", "tokens": ["Was", "will", "Sie", "da", "im", "gold\u00b7nen", "L\u00f6\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heut abend auf und nieder schweben?", "tokens": ["Heut", "a\u00b7bend", "auf", "und", "nie\u00b7der", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wedelt nicht bei Spiel und Tanz", "tokens": ["Denn", "we\u00b7delt", "nicht", "bei", "Spiel", "und", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Teufel fr\u00f6hlich mit dem Schwanz?", "tokens": ["Der", "Teu\u00b7fel", "fr\u00f6h\u00b7lich", "mit", "dem", "Schwanz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und \u00fcberhaupt, was ist es n\u00fctz?", "tokens": ["Und", "\u00fc\u00b7ber\u00b7haupt", ",", "was", "ist", "es", "n\u00fctz", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie qu\u00e4lt sich ab, Sie kommt in Schwitz,", "tokens": ["Sie", "qu\u00e4lt", "sich", "ab", ",", "Sie", "kommt", "in", "Schwitz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie geht hinaus, erk\u00e4ltet sich", "tokens": ["Sie", "geht", "hin\u00b7aus", ",", "er\u00b7k\u00e4l\u00b7tet", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hustet dann ganz f\u00fcrchterlich.", "tokens": ["Und", "hus\u00b7tet", "dann", "ganz", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum bleibe Sie bei mir nur lieber!", "tokens": ["Drum", "blei\u00b7be", "Sie", "bei", "mir", "nur", "lie\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und, Rike, geh Sie mal hin\u00fcber", "tokens": ["Und", ",", "Ri\u00b7ke", ",", "geh", "Sie", "mal", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "NE", "$,", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hole Sie von Kaufmann Fr\u00e4se", "tokens": ["Und", "ho\u00b7le", "Sie", "von", "Kauf\u00b7mann", "Fr\u00e4\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Viertel guten Schweizerk\u00e4se,", "tokens": ["Ein", "Vier\u00b7tel", "gu\u00b7ten", "Schwei\u00b7zer\u00b7k\u00e4\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sei Sie aber ja, ja, ja,", "tokens": ["Und", "sei", "Sie", "a\u00b7ber", "ja", ",", "ja", ",", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "$,", "PTKANT", "$,", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich zur Minute wieder da!", "tokens": ["Gleich", "zur", "Mi\u00b7nu\u00b7te", "wie\u00b7der", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ist die gute Mamsell Schm\u00f6le", "tokens": ["So", "ist", "die", "gu\u00b7te", "Mam\u00b7sell", "Schm\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Besorgt f\u00fcr Rikens Heil der Seele.", "tokens": ["Be\u00b7sorgt", "f\u00fcr", "Ri\u00b7kens", "Heil", "der", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja sp\u00e4ter noch, in stiller Nacht,", "tokens": ["Ja", "sp\u00e4\u00b7ter", "noch", ",", "in", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist sie auf diesen Zweck bedacht", "tokens": ["Ist", "sie", "auf", "die\u00b7sen", "Zweck", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schleicht an Rikens Kammert\u00fcr", "tokens": ["Und", "schleicht", "an", "Ri\u00b7kens", "Kam\u00b7mer\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schaut, ob auch die Rike hier,", "tokens": ["Und", "schaut", ",", "ob", "auch", "die", "Ri\u00b7ke", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und ob sie auch in Frieden ruht", "tokens": ["Und", "ob", "sie", "auch", "in", "Frie\u00b7den", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und da\u00df ihr ja nicht wer was tut,", "tokens": ["Und", "da\u00df", "ihr", "ja", "nicht", "wer", "was", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "PWS", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was sich nun einmal nicht geh\u00f6rt,", "tokens": ["Was", "sich", "nun", "ein\u00b7mal", "nicht", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was gottlos und beneidenswert.", "tokens": ["Was", "gott\u00b7los", "und", "be\u00b7nei\u00b7dens\u00b7wert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}