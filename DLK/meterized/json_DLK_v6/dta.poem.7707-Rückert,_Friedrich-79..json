{"dta.poem.7707": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du gibst dir viele M\u00fch, Unarten abzuthun,", "tokens": ["Du", "gibst", "dir", "vie\u00b7le", "M\u00fch", ",", "Un\u00b7ar\u00b7ten", "ab\u00b7zu\u00b7thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schon zu deiner Art geworden sind sie nun.", "tokens": ["Doch", "schon", "zu", "dei\u00b7ner", "Art", "ge\u00b7wor\u00b7den", "sind", "sie", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VAPP", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Art nun, solcher Art Unarten abzulegen,", "tokens": ["Die", "Art", "nun", ",", "sol\u00b7cher", "Art", "Un\u00b7ar\u00b7ten", "ab\u00b7zu\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PIAT", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erscheint als Unart selbst, drum la\u00df sie unterwegen.", "tokens": ["Er\u00b7scheint", "als", "Un\u00b7art", "selbst", ",", "drum", "la\u00df", "sie", "un\u00b7ter\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "ADV", "$,", "PAV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Geartet bist du so, da\u00df du unartig scheinst,", "tokens": ["Ge\u00b7ar\u00b7tet", "bist", "du", "so", ",", "da\u00df", "du", "un\u00b7ar\u00b7tig", "scheinst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Grad wenn der Unart du dich zu enthalten meinst.", "tokens": ["Grad", "wenn", "der", "Un\u00b7art", "du", "dich", "zu", "ent\u00b7hal\u00b7ten", "meinst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PPER", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}}, "stanza.4": {"line.1": {"text": "Geartet bist du so, da\u00df artig du erscheinest", "tokens": ["Ge\u00b7ar\u00b7tet", "bist", "du", "so", ",", "da\u00df", "ar\u00b7tig", "du", "er\u00b7schei\u00b7nest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "$,", "KOUS", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur durch Unarten, die so artig du vereinest.", "tokens": ["Nur", "durch", "Un\u00b7ar\u00b7ten", ",", "die", "so", "ar\u00b7tig", "du", "ver\u00b7ei\u00b7nest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}