{"textgrid.poem.58597": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Blinden leitet Gott, die keine Wege sehen,", "tokens": ["Die", "Blin\u00b7den", "lei\u00b7tet", "Gott", ",", "die", "kei\u00b7ne", "We\u00b7ge", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie folgen, wie er f\u00fchrt, ob sie es nicht verstehen;", "tokens": ["Sie", "fol\u00b7gen", ",", "wie", "er", "f\u00fchrt", ",", "ob", "sie", "es", "nicht", "ver\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer blinden G\u00f6tzen traut, Vernunft und Bilder ehrt,", "tokens": ["Wer", "blin\u00b7den", "G\u00f6t\u00b7zen", "traut", ",", "Ver\u00b7nunft", "und", "Bil\u00b7der", "ehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mu\u00df alleine gehn und gehet stets verkehrt.", "tokens": ["Der", "mu\u00df", "al\u00b7lei\u00b7ne", "gehn", "und", "ge\u00b7het", "stets", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVINF", "KON", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kann ich dem Herren mich im Glauben ganz ergeben,", "tokens": ["Kann", "ich", "dem", "Her\u00b7ren", "mich", "im", "Glau\u00b7ben", "ganz", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wird die Nacht zum Licht, was h\u00f6ckrig ist, wird eben;", "tokens": ["So", "wird", "die", "Nacht", "zum", "Licht", ",", "was", "h\u00f6ck\u00b7rig", "ist", ",", "wird", "e\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Hirt, verla\u00df mich nicht! O Jesu, w\u00e4r' ich blind", "tokens": ["Mein", "Hirt", ",", "ver\u00b7la\u00df", "mich", "nicht", "!", "O", "Je\u00b7su", ",", "w\u00e4r'", "ich", "blind"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "$.", "NE", "NE", "$,", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und k\u00f6nnt' ich trauen dir gleichwie ein kleines Kind!", "tokens": ["Und", "k\u00f6nnt'", "ich", "trau\u00b7en", "dir", "gleich\u00b7wie", "ein", "klei\u00b7nes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "PPER", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}}}}