{"textgrid.poem.37107": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Maylied", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles liebet! Liebe gleitet", "tokens": ["Al\u00b7les", "lie\u00b7bet", "!", "Lie\u00b7be", "glei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die bl\u00fchende Natur,", "tokens": ["Durch", "die", "bl\u00fc\u00b7hen\u00b7de", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe zeuget Blumen, breitet", "tokens": ["Lie\u00b7be", "zeu\u00b7get", "Blu\u00b7men", ",", "brei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchen Teppich auf die Flur.", "tokens": ["Man\u00b7chen", "Tep\u00b7pich", "auf", "die", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das verliebte Haingefieder,", "tokens": ["Das", "ver\u00b7lieb\u00b7te", "Hain\u00b7ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das sich neue Zellen baut,", "tokens": ["Das", "sich", "neu\u00b7e", "Zel\u00b7len", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "T\u00f6net s\u00fc\u00dfe Liebeslieder,", "tokens": ["T\u00f6\u00b7net", "s\u00fc\u00b7\u00dfe", "Lie\u00b7bes\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn der May vom Himmel thaut.", "tokens": ["Wenn", "der", "May", "vom", "Him\u00b7mel", "thaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebe malt jezt hellre Rosen", "tokens": ["Lie\u00b7be", "malt", "jezt", "hell\u00b7re", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Mund der Sch\u00e4ferin,", "tokens": ["Um", "den", "Mund", "der", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00e4ferin und Sch\u00e4fer kosen", "tokens": ["Sch\u00e4\u00b7fe\u00b7rin", "und", "Sch\u00e4\u00b7fer", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manche goldne Stunde hin.", "tokens": ["Man\u00b7che", "gold\u00b7ne", "Stun\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sizen unter Apfelbl\u00fcthen,", "tokens": ["Si\u00b7zen", "un\u00b7ter", "Ap\u00b7fel\u00b7bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Arm in Arm, und Paar an Paar,", "tokens": ["Arm", "in", "Arm", ",", "und", "Paar", "an", "Paar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kleine Liebesg\u00f6tter bieten", "tokens": ["Klei\u00b7ne", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "bie\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nektar ihren Lippen dar.", "tokens": ["Nek\u00b7tar", "ih\u00b7ren", "Lip\u00b7pen", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unschuld blickt aus ihren Minen,", "tokens": ["Un\u00b7schuld", "blickt", "aus", "ih\u00b7ren", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unschuld ihres Standes Loos,", "tokens": ["Un\u00b7schuld", "ih\u00b7res", "Stan\u00b7des", "Loos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rothe Bl\u00fcthen taumeln ihnen", "tokens": ["Ro\u00b7the", "Bl\u00fc\u00b7then", "tau\u00b7meln", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Wipfel in den Schoos.", "tokens": ["Aus", "dem", "Wip\u00b7fel", "in", "den", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blau und golden schwebt der Aether", "tokens": ["Blau", "und", "gol\u00b7den", "schwebt", "der", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Im beb\u00fcschten Gartenteich,", "tokens": ["Im", "be\u00b7b\u00fcschten", "Gar\u00b7ten\u00b7teich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Alle Bl\u00fcthen werden r\u00f6ther,", "tokens": ["Al\u00b7le", "Bl\u00fc\u00b7then", "wer\u00b7den", "r\u00f6\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Werden Edens Bl\u00fcthen gleich.", "tokens": ["Wer\u00b7den", "E\u00b7dens", "Bl\u00fc\u00b7then", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Durch die Blumen, durch die gr\u00fcnen", "tokens": ["Durch", "die", "Blu\u00b7men", ",", "durch", "die", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kr\u00e4uter, die der Sonnenschein", "tokens": ["Kr\u00e4u\u00b7ter", ",", "die", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcbergoldet, summen Bienen,", "tokens": ["\u00dc\u00b7ber\u00b7gol\u00b7det", ",", "sum\u00b7men", "Bie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sammeln s\u00fc\u00dfen Nektar ein.", "tokens": ["Sam\u00b7meln", "s\u00fc\u00b7\u00dfen", "Nek\u00b7tar", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles hauchet Scherz und Freude,", "tokens": ["Al\u00b7les", "hau\u00b7chet", "Scherz", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo des Fr\u00fchlings Odem bl\u00e4st,", "tokens": ["Wo", "des", "Fr\u00fch\u00b7lings", "O\u00b7dem", "bl\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Natur, im Blumenkleide,", "tokens": ["Die", "Na\u00b7tur", ",", "im", "Blu\u00b7men\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Feirt ein allgemeines Fest.", "tokens": ["Feirt", "ein", "all\u00b7ge\u00b7mei\u00b7nes", "Fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alles k\u00fc\u00dft jetzt! K\u00fc\u00dfe fl\u00fcstern", "tokens": ["Al\u00b7les", "k\u00fc\u00dft", "jetzt", "!", "K\u00fc\u00b7\u00dfe", "fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$.", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In beschatteten Alleen,", "tokens": ["In", "be\u00b7schat\u00b7te\u00b7ten", "Al\u00b7leen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Liebenden in d\u00fcstern", "tokens": ["Wo", "die", "Lie\u00b7ben\u00b7den", "in", "d\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Buchenlabyrinthen gehn.", "tokens": ["Bu\u00b7chen\u00b7la\u00b7by\u00b7rin\u00b7then", "gehn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fc\u00dfe rauschen in den Lauben,", "tokens": ["K\u00fc\u00b7\u00dfe", "rau\u00b7schen", "in", "den", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um die Abendd\u00e4mmerung,", "tokens": ["Um", "die", "A\u00b7bend\u00b7d\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "K\u00fc\u00dfe geben, K\u00fc\u00dfe rauben", "tokens": ["K\u00fc\u00b7\u00dfe", "ge\u00b7ben", ",", "K\u00fc\u00b7\u00dfe", "rau\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist der Welt Besch\u00e4ftigung.", "tokens": ["Ist", "der", "Welt", "Be\u00b7sch\u00e4f\u00b7ti\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}