{"textgrid.poem.31717": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mag nicht r\u00e4sonieren", "tokens": ["Ich", "mag", "nicht", "r\u00e4\u00b7so\u00b7nie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins Dunkelblaue hinein!", "tokens": ["Ins", "Dun\u00b7kel\u00b7blau\u00b7e", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Viel lieber will ich probieren", "tokens": ["Viel", "lie\u00b7ber", "will", "ich", "pro\u00b7bie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einen k\u00fchlenden Abendwein.", "tokens": ["Ei\u00b7nen", "k\u00fch\u00b7len\u00b7den", "Ab\u00b7end\u00b7wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Zwar vor den Herrn Gelahrten,", "tokens": ["Zwar", "vor", "den", "Herrn", "Ge\u00b7lahr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da habe ich gro\u00dfen Respekt,", "tokens": ["Da", "ha\u00b7be", "ich", "gro\u00b7\u00dfen", "Res\u00b7pekt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie haben schon manche Arten", "tokens": ["Sie", "ha\u00b7ben", "schon", "man\u00b7che", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geschichten ausgeheckt.", "tokens": ["Ge\u00b7schich\u00b7ten", "aus\u00b7ge\u00b7heckt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch habe ich stets gefunden:", "tokens": ["Auch", "ha\u00b7be", "ich", "stets", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Schelling, Hegel, Kant,", "tokens": ["Den", "Schel\u00b7ling", ",", "He\u00b7gel", ",", "Kant", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die hat man immer gebunden", "tokens": ["Die", "hat", "man", "im\u00b7mer", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PIS", "ADV", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In einen Schweinslederband.", "tokens": ["In", "ei\u00b7nen", "Schweins\u00b7le\u00b7der\u00b7band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Man sagt, da\u00df dies eine Ehre", "tokens": ["Man", "sagt", ",", "da\u00df", "dies", "ei\u00b7ne", "Eh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PDS", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "F\u00fcr Menschenkinder sei \u2013", "tokens": ["F\u00fcr", "Men\u00b7schen\u00b7kin\u00b7der", "sei", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum, wenn der Wein nicht w\u00e4re,", "tokens": ["Drum", ",", "wenn", "der", "Wein", "nicht", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "ART", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da studiert ich Philosophei!", "tokens": ["Da", "stu\u00b7diert", "ich", "Phi\u00b7lo\u00b7so\u00b7phei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}