{"textgrid.poem.23994": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenaufgang", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rauch \u00fcber Acker und Moor;", "tokens": ["Rauch", "\u00fc\u00b7ber", "A\u00b7cker", "und", "Moor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ueber das ganze Land", "tokens": ["Ue\u00b7ber", "das", "gan\u00b7ze", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ist, aus Nebeln gerafft,", "tokens": ["Ist", ",", "aus", "Ne\u00b7beln", "ge\u00b7rafft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Riesig ein Netz gespannt.", "tokens": ["Rie\u00b7sig", "ein", "Netz", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wird Leviathan gejagt,", "tokens": ["Wird", "Le\u00b7vi\u00b7a\u00b7than", "ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da er entstiegen dem Meer?", "tokens": ["Da", "er", "ent\u00b7stie\u00b7gen", "dem", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hui, wie tobt er im Netz,", "tokens": ["Hui", ",", "wie", "tobt", "er", "im", "Netz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "PWAV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Schleppt es und schleift es umher.", "tokens": ["Schleppt", "es", "und", "schleift", "es", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Sieh! Da blendets im Ost:", "tokens": ["Sieh", "!", "Da", "blen\u00b7dets", "im", "Ost", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Offen der Himmel, es schie\u00dft", "tokens": ["Of\u00b7fen", "der", "Him\u00b7mel", ",", "es", "schie\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Goldene Speere der Tag,", "tokens": ["Gol\u00b7de\u00b7ne", "Spee\u00b7re", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Und der Wurm zerflie\u00dft.", "tokens": ["Und", "der", "Wurm", "zer\u00b7flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Hoch seinen goldenen Schild", "tokens": ["Hoch", "sei\u00b7nen", "gol\u00b7de\u00b7nen", "Schild"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ueber den Wolkenwall", "tokens": ["Ue\u00b7ber", "den", "Wol\u00b7ken\u00b7wall"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hebt der siegende Tag;", "tokens": ["Hebt", "der", "sie\u00b7gen\u00b7de", "Tag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Licht lacht \u00fcber das All.", "tokens": ["Licht", "lacht", "\u00fc\u00b7ber", "das", "All", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}