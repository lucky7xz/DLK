{"dta.poem.12944": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er gelegenheit hatte, sie zu  \n k\u00fcssen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bringt mich dein s\u00fc\u00dfer blick, o Flavia! ums leben,", "tokens": ["Bringt", "mich", "dein", "s\u00fc\u00b7\u00dfer", "blick", ",", "o", "Fla\u00b7via", "!", "ums", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "FM", "NE", "$.", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was w\u00fcrde nicht ein ku\u00df von deinem munde thun?", "tokens": ["Was", "w\u00fcr\u00b7de", "nicht", "ein", "ku\u00df", "von", "dei\u00b7nem", "mun\u00b7de", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch schweig, du tummer mund, und la\u00df den kummer ruhn!", "tokens": ["Doch", "schweig", ",", "du", "tum\u00b7mer", "mund", ",", "und", "la\u00df", "den", "kum\u00b7mer", "ruhn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,", "KON", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie lacht ja deiner bl\u00f6den schl\u00fcsse:", "tokens": ["Sie", "lacht", "ja", "dei\u00b7ner", "bl\u00f6\u00b7den", "schl\u00fcs\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es hat dann keine noth.", "tokens": ["Es", "hat", "dann", "kei\u00b7ne", "noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Drum schweig, und setze k\u00fc\u00df\u2019 auf k\u00fcsse.", "tokens": ["Drum", "schweig", ",", "und", "set\u00b7ze", "k\u00fc\u00df'", "auf", "k\u00fcs\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn giebt dir einer schon den tod;", "tokens": ["Denn", "giebt", "dir", "ei\u00b7ner", "schon", "den", "tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So wird der andre doch das leben", "tokens": ["So", "wird", "der", "and\u00b7re", "doch", "das", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "KON", "PDS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dir also gleich mit wucher wiedergeben.", "tokens": ["Dir", "al\u00b7so", "gleich", "mit", "wu\u00b7cher", "wie\u00b7der\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}