{"textgrid.poem.60240": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Endlich, gutes Kind, gelangst", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich, gutes Kind, gelangst", "tokens": ["End\u00b7lich", ",", "gu\u00b7tes", "Kind", ",", "ge\u00b7langst"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du nach mancher Todesangst", "tokens": ["Du", "nach", "man\u00b7cher", "To\u00b7de\u00b7sangst"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch zum Ziel der Br\u00e4ute;", "tokens": ["Noch", "zum", "Ziel", "der", "Br\u00e4u\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hymen zog den Knoten zu,", "tokens": ["Hy\u00b7men", "zog", "den", "Kno\u00b7ten", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer ist fr\u00f6hlicher als du,", "tokens": ["Wer", "ist", "fr\u00f6h\u00b7li\u00b7cher", "als", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Freund an deiner Seite?", "tokens": ["Und", "der", "Freund", "an", "dei\u00b7ner", "Sei\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! den G\u00f6ttern sey es Dank,", "tokens": ["Ach", "!", "den", "G\u00f6t\u00b7tern", "sey", "es", "Dank", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du nur noch liebeskrank", "tokens": ["Da\u00df", "du", "nur", "noch", "lie\u00b7bes\u00b7krank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich an H... lehnest,", "tokens": ["Dich", "an", "H", "...", "leh\u00b7nest", ","], "token_info": ["word", "word", "abbreviation", "abbreviation", "word", "punct"], "pos": ["PPER", "APPR", "XY", "$(", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und nach jenem Aesculap,", "tokens": ["Und", "nach", "je\u00b7nem", "A\u00b7e\u00b7scu\u00b7lap", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Der dir neues Leben gab,", "tokens": ["Der", "dir", "neu\u00b7es", "Le\u00b7ben", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich nicht mehr so seufzend sehnest.", "tokens": ["Dich", "nicht", "mehr", "so", "seuf\u00b7zend", "seh\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deine Sehnsucht ist der Mann,", "tokens": ["Dei\u00b7ne", "Sehn\u00b7sucht", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der so freundlich l\u00e4cheln kann,", "tokens": ["Der", "so", "freund\u00b7lich", "l\u00e4\u00b7cheln", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der so herzlich weinte,", "tokens": ["Der", "so", "herz\u00b7lich", "wein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und so kl\u00e4glich hat gethan,", "tokens": ["Und", "so", "kl\u00e4g\u00b7lich", "hat", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als dich in den Schattenkahn", "tokens": ["Als", "dich", "in", "den", "Schat\u00b7ten\u00b7kahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Charon mitzunehmen meynte.", "tokens": ["Cha\u00b7ron", "mit\u00b7zu\u00b7neh\u00b7men", "meyn\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Klopfend sprach das Herz in dir:", "tokens": ["Klop\u00b7fend", "sprach", "das", "Herz", "in", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alter F\u00e4hrmann, la\u00df mich hier;", "tokens": ["Al\u00b7ter", "F\u00e4hr\u00b7mann", ",", "la\u00df", "mich", "hier", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVIMP", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bin jung von Jahren:", "tokens": ["Ich", "bin", "jung", "von", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und mein Br\u00e4utigam ist s\u00fc\u00df; \u2013", "tokens": ["Und", "mein", "Br\u00e4u\u00b7ti\u00b7gam", "ist", "s\u00fc\u00df", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NE", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der alte Schiffer lie\u00df", "tokens": ["Und", "der", "al\u00b7te", "Schif\u00b7fer", "lie\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Seinen Kahn vom Ufer fahren.", "tokens": ["Sei\u00b7nen", "Kahn", "vom", "U\u00b7fer", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohl dir, angenehme Braut,", "tokens": ["Wohl", "dir", ",", "an\u00b7ge\u00b7neh\u00b7me", "Braut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Amor hat ein Grab gebaut,", "tokens": ["A\u00b7mor", "hat", "ein", "Grab", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo du nicht verdirbest;", "tokens": ["Wo", "du", "nicht", "ver\u00b7dir\u00b7best", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wo du nur auf Blumen sinkst,", "tokens": ["Wo", "du", "nur", "auf", "Blu\u00b7men", "sinkst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und indem du K\u00fcsse trinkst", "tokens": ["Und", "in\u00b7dem", "du", "K\u00fcs\u00b7se", "trinkst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eines sanften Todes stirbest.", "tokens": ["Ei\u00b7nes", "sanf\u00b7ten", "To\u00b7des", "stir\u00b7best", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schmecke diesen Wonnetodt,", "tokens": ["Schme\u00b7cke", "die\u00b7sen", "Won\u00b7ne\u00b7todt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Wangen rosenroth", "tokens": ["Und", "mit", "Wan\u00b7gen", "ro\u00b7sen\u00b7roth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wache vom Entz\u00fccken", "tokens": ["Wa\u00b7che", "vom", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wieder auf, und sprich: o Mann", "tokens": ["Wie\u00b7der", "auf", ",", "und", "sprich", ":", "o", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "KON", "ADJD", "$.", "FM", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur allein Gott Hymen kann", "tokens": ["Nur", "al\u00b7lein", "Gott", "Hy\u00b7men", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich mit neuem Reize schm\u00fccken.", "tokens": ["Mich", "mit", "neu\u00b7em", "Rei\u00b7ze", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hymen sey dein Lebelang", "tokens": ["Hy\u00b7men", "sey", "dein", "Le\u00b7be\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis ins Alter dein Gesang,", "tokens": ["Bis", "ins", "Al\u00b7ter", "dein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Frucht von Hymen", "tokens": ["Und", "die", "Frucht", "von", "Hy\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Werd' ein junger Apian,", "tokens": ["Werd'", "ein", "jun\u00b7ger", "A\u00b7pian", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Den einst auf der Spielerbahn", "tokens": ["Den", "einst", "auf", "der", "Spie\u00b7ler\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gro\u00dfe Kunstempfinder r\u00fchmen. \u2013", "tokens": ["Gro\u00b7\u00dfe", "Kuns\u00b7temp\u00b7fin\u00b7der", "r\u00fch\u00b7men", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}