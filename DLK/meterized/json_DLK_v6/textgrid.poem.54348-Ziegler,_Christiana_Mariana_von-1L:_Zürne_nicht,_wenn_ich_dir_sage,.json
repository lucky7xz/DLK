{"textgrid.poem.54348": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Z\u00fcrne nicht, wenn ich dir sage,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Z\u00fcrne nicht, wenn ich dir sage,", "tokens": ["Z\u00fcr\u00b7ne", "nicht", ",", "wenn", "ich", "dir", "sa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "Da\u00df ich dich nicht lieben kann.", "tokens": ["Da\u00df", "ich", "dich", "nicht", "lie\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich Aug und Herze frage,", "tokens": ["Wenn", "ich", "Aug", "und", "Her\u00b7ze", "fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Giebt es mir die Ursach an:", "tokens": ["Giebt", "es", "mir", "die", "Ur\u00b7sach", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Amor herrscht gleich den Tyrannen;", "tokens": ["A\u00b7mor", "herrscht", "gleich", "den", "Ty\u00b7ran\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "R\u00e4umt man ihm nur etwas ein,", "tokens": ["R\u00e4umt", "man", "ihm", "nur", "et\u00b7was", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sucht er alles zu verbannen,", "tokens": ["Sucht", "er", "al\u00b7les", "zu", "ver\u00b7ban\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was ihm nicht will dienstbar seyn.", "tokens": ["Was", "ihm", "nicht", "will", "dienst\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ruh und Freyheit gieng verlohren,", "tokens": ["Ruh", "und", "Frey\u00b7heit", "gieng", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Verlust fiel mir zu schwer;", "tokens": ["Der", "Ver\u00b7lust", "fiel", "mir", "zu", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dazu bin nicht gebohren,", "tokens": ["Da\u00b7zu", "bin", "nicht", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nein, mein Ohr giebt kein Geh\u00f6r.", "tokens": ["Nein", ",", "mein", "Ohr", "giebt", "kein", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fchl die Gluth an andern Blicken,", "tokens": ["K\u00fchl", "die", "Gluth", "an", "an\u00b7dern", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was nutzt eine kalte Brust?", "tokens": ["Was", "nutzt", "ei\u00b7ne", "kal\u00b7te", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sich an Schnee und Eis erquicken", "tokens": ["Sich", "an", "Schnee", "und", "Eis", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bringt dem Herzen schlechte Lust.", "tokens": ["Bringt", "dem", "Her\u00b7zen", "schlech\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist edel von Gem\u00fcthe,", "tokens": ["Du", "bist", "e\u00b7del", "von", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bist angenehm und sch\u00f6n,", "tokens": ["Du", "bist", "an\u00b7ge\u00b7nehm", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du bist in der sch\u00f6nsten Bl\u00fcte,", "tokens": ["Du", "bist", "in", "der", "sch\u00f6ns\u00b7ten", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jede mag dich gerne sehn.", "tokens": ["Je\u00b7de", "mag", "dich", "ger\u00b7ne", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein Verstand, dein ganzes Wesen", "tokens": ["Dein", "Ver\u00b7stand", ",", "dein", "gan\u00b7zes", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebt ein Herz das lieben kann;", "tokens": ["Liebt", "ein", "Herz", "das", "lie\u00b7ben", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja dein Werth bleibt auserlesen,", "tokens": ["Ja", "dein", "Werth", "bleibt", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur ich nehm nicht Theil daran.", "tokens": ["Nur", "ich", "nehm", "nicht", "Theil", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja ich will mich selbst verdammen,", "tokens": ["Ja", "ich", "will", "mich", "selbst", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein Herz ist zu felsenfest,", "tokens": ["Mein", "Herz", "ist", "zu", "fel\u00b7sen\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da im Ursprung deiner Flammen", "tokens": ["Da", "im", "Ur\u00b7sprung", "dei\u00b7ner", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es dich gar verschmachten l\u00e4\u00dft.", "tokens": ["Es", "dich", "gar", "ver\u00b7schmach\u00b7ten", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum mu\u00df ich mit dir leiden,", "tokens": ["Da\u00b7rum", "mu\u00df", "ich", "mit", "dir", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich dich nicht tr\u00f6sten kann;", "tokens": ["Da\u00df", "ich", "dich", "nicht", "tr\u00f6s\u00b7ten", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dich und deinen Umgang meiden", "tokens": ["Dich", "und", "dei\u00b7nen", "Um\u00b7gang", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seh ich selbst f\u00fcr strafbar an.", "tokens": ["Seh", "ich", "selbst", "f\u00fcr", "straf\u00b7bar", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hoffe nur und sey zufrieden,", "tokens": ["Hof\u00b7fe", "nur", "und", "sey", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeit und Stunden \u00e4ndern sich.", "tokens": ["Zeit", "und", "Stun\u00b7den", "\u00e4n\u00b7dern", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was der Himmel dir beschieden,", "tokens": ["Was", "der", "Him\u00b7mel", "dir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das erh\u00e4lt er auch vor dich.", "tokens": ["Das", "er\u00b7h\u00e4lt", "er", "auch", "vor", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er kann Geist und Herze lenken;", "tokens": ["Er", "kann", "Geist", "und", "Her\u00b7ze", "len\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht er meine Unschuld an,", "tokens": ["Sieht", "er", "mei\u00b7ne", "Un\u00b7schuld", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird er auch an dich gedenken,", "tokens": ["Wird", "er", "auch", "an", "dich", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ich dich noch lieben kann.", "tokens": ["Da\u00df", "ich", "dich", "noch", "lie\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dennoch will ich mich vergn\u00fcgen,", "tokens": ["Den\u00b7noch", "will", "ich", "mich", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mein Schicksal widerspricht.", "tokens": ["Wenn", "mein", "Schick\u00b7sal", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollte ja die Hoffnung tr\u00fcgen,", "tokens": ["Soll\u00b7te", "ja", "die", "Hoff\u00b7nung", "tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcgt doch deine Liebe nicht.", "tokens": ["Tr\u00fcgt", "doch", "dei\u00b7ne", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich kann dich doch niemals hassen;", "tokens": ["Ich", "kann", "dich", "doch", "nie\u00b7mals", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn der erste Blick und Tag", "tokens": ["Denn", "der", "ers\u00b7te", "Blick", "und", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Lie\u00df mich was ins Herze fassen,", "tokens": ["Lie\u00df", "mich", "was", "ins", "Her\u00b7ze", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das ich nicht gestehen mag.", "tokens": ["Das", "ich", "nicht", "ge\u00b7ste\u00b7hen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PDS", "PPER", "PTKNEG", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}