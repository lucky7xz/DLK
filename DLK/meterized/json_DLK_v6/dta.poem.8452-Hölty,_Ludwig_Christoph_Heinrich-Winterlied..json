{"dta.poem.8452": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winterlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Nur das Wintergr\u00fcn", "tokens": ["Nur", "das", "Win\u00b7ter\u00b7gr\u00fcn"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Blickt durch Silberh\u00fcllen;", "tokens": ["Blickt", "durch", "Sil\u00b7ber\u00b7h\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nur das Fenster f\u00fcllen", "tokens": ["Nur", "das", "Fens\u00b7ter", "f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bl\u00fcmchen, roth und weiss,", "tokens": ["Bl\u00fcm\u00b7chen", ",", "roth", "und", "weiss", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Aufgebl\u00fcht aus Eis.", "tokens": ["Auf\u00b7ge\u00b7bl\u00fcht", "aus", "Eis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ach! kein Vogelsang", "tokens": ["Ach", "!", "kein", "Vo\u00b7gel\u00b7sang"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "T\u00f6nt mit frohem Klang;", "tokens": ["T\u00f6nt", "mit", "fro\u00b7hem", "Klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nur die Winterweise", "tokens": ["Nur", "die", "Win\u00b7ter\u00b7wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jener kleinen Meise,", "tokens": ["Je\u00b7ner", "klei\u00b7nen", "Mei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die am Fenster schwirrt,", "tokens": ["Die", "am", "Fens\u00b7ter", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und um Futter girrt.", "tokens": ["Und", "um", "Fut\u00b7ter", "girrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Minne flieht den Hain,", "tokens": ["Min\u00b7ne", "flieht", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wo die V\u00f6gelein", "tokens": ["Wo", "die", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sonst im gr\u00fcnen Schatten", "tokens": ["Sonst", "im", "gr\u00fc\u00b7nen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihre Nester hatten;", "tokens": ["Ih\u00b7re", "Nes\u00b7ter", "hat\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Minne flieht den Hain,", "tokens": ["Min\u00b7ne", "flieht", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Kehrt ins Zimmer ein.", "tokens": ["Kehrt", "ins", "Zim\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Kalter Januar,", "tokens": ["Kal\u00b7ter", "Ja\u00b7nu\u00b7ar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hier werd' ich f\u00fcrwahr", "tokens": ["Hier", "werd'", "ich", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Unter Minnespielen,", "tokens": ["Un\u00b7ter", "Min\u00b7ne\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Deinen Frost nicht f\u00fchlen!", "tokens": ["Dei\u00b7nen", "Frost", "nicht", "f\u00fch\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Walte immerdar,", "tokens": ["Wal\u00b7te", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Kalter Januar!", "tokens": ["Kal\u00b7ter", "Ja\u00b7nu\u00b7ar", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}