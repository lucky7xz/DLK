{"textgrid.poem.42121": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Fluch", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn es Dich aufrei\u00dft, denk daran.", "tokens": ["Wenn", "es", "Dich", "auf\u00b7rei\u00dft", ",", "denk", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": ". . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}, "line.3": {"text": "Wie so das Herz zum Herzen schwoll", "tokens": ["Wie", "so", "das", "Herz", "zum", "Her\u00b7zen", "schwoll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ohne Ma\u00df, so \u00fcbervoll.", "tokens": ["So", "oh\u00b7ne", "Ma\u00df", ",", "so", "\u00fc\u00b7ber\u00b7voll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo Du auch stehst, gedenk daran.", "tokens": ["Wo", "Du", "auch", "stehst", ",", "ge\u00b7denk", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergi\u00df es nicht, wo Du auch ruhst,", "tokens": ["Ver\u00b7gi\u00df", "es", "nicht", ",", "wo", "Du", "auch", "ruhst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo m\u00fcd Dein Haupt sich hingeneigt", "tokens": ["Wo", "m\u00fcd", "Dein", "Haupt", "sich", "hin\u00b7ge\u00b7neigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Heimat fremd, Verlangen schweigt \u2013", "tokens": ["In", "Hei\u00b7mat", "fremd", ",", "Ver\u00b7lan\u00b7gen", "schweigt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie Stillung einst auf Lider sank,", "tokens": ["Wie", "Stil\u00b7lung", "einst", "auf", "Li\u00b7der", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So blau behaucht, so liebekrank.", "tokens": ["So", "blau", "be\u00b7haucht", ",", "so", "lie\u00b7be\u00b7krank", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wo Du verweilst, vergi\u00df es nicht.", "tokens": ["Wo", "Du", "ver\u00b7weilst", ",", "ver\u00b7gi\u00df", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sei eingedenk, wie Du auch hoffst", "tokens": ["Sei", "ein\u00b7ge\u00b7denk", ",", "wie", "Du", "auch", "hoffst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Irrfahrt gehst, sp\u00e4t oder fr\u00fch,", "tokens": ["Auf", "Irr\u00b7fahrt", "gehst", ",", "sp\u00e4t", "o\u00b7der", "fr\u00fch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "zuviel der M\u00fch", "tokens": ["zu\u00b7viel", "der", "M\u00fch"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wo aller Weg einst einer war", "tokens": ["Wo", "al\u00b7ler", "Weg", "einst", "ei\u00b7ner", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ADV", "PIS", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So voller Ziel, so sonnenklar \u2013", "tokens": ["So", "vol\u00b7ler", "Ziel", ",", "so", "son\u00b7nen\u00b7klar", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sei eingedenk, wohin Du gehst.", "tokens": ["Sei", "ein\u00b7ge\u00b7denk", ",", "wo\u00b7hin", "Du", "gehst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und noch zuletzt gemahn es Dich,", "tokens": ["Und", "noch", "zu\u00b7letzt", "ge\u00b7mahn", "es", "Dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Tod Dich fa\u00dft, die leere Hand", "tokens": ["Wenn", "Tod", "Dich", "fa\u00dft", ",", "die", "lee\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Weg hinweist in leeres Land,", "tokens": ["Den", "Weg", "hin\u00b7weist", "in", "lee\u00b7res", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Hand und Weg schon einmal, leer,", "tokens": ["Wie", "Hand", "und", "Weg", "schon", "ein\u00b7mal", ",", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So fernhinaus, ohn Wiederkehr...", "tokens": ["So", "fern\u00b7hin\u00b7aus", ",", "ohn", "Wie\u00b7der\u00b7kehr", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KOUI", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daran zuletzt gemahn Dichs auch.", "tokens": ["Da\u00b7ran", "zu\u00b7letzt", "ge\u00b7mahn", "Dichs", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}