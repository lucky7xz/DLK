{"dta.poem.11785": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Vorzeit Sprache sei dir heil'ge Hieroglyphe,", "tokens": ["Der", "Vor\u00b7zeit", "Spra\u00b7che", "sei", "dir", "heil'\u00b7ge", "Hie\u00b7ro\u00b7gly\u00b7phe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die du bewahren mu\u00dft stumm in des Busens Tiefe.", "tokens": ["Die", "du", "be\u00b7wah\u00b7ren", "mu\u00dft", "stumm", "in", "des", "Bu\u00b7sens", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie lebet nicht im Ohr, sie schwebet nicht vom Munde;", "tokens": ["Sie", "le\u00b7bet", "nicht", "im", "Ohr", ",", "sie", "schwe\u00b7bet", "nicht", "vom", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie dringt vom Grab hervor, und klingt im Herzensgrunde.", "tokens": ["Sie", "dringt", "vom", "Grab", "her\u00b7vor", ",", "und", "klingt", "im", "Her\u00b7zens\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die J\u00fcnger m\u00fchen sich mit nicht'ger Eitelkeit", "tokens": ["Die", "J\u00fcn\u00b7ger", "m\u00fc\u00b7hen", "sich", "mit", "nicht'\u00b7ger", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu haschen einen Klang, den l\u00e4ngst verweht die Zeit.", "tokens": ["Zu", "ha\u00b7schen", "ei\u00b7nen", "Klang", ",", "den", "l\u00e4ngst", "ver\u00b7weht", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie suchen ihren Mund recht n\u00e4rrisch zu verrenken,", "tokens": ["Sie", "su\u00b7chen", "ih\u00b7ren", "Mund", "recht", "n\u00e4r\u00b7risch", "zu", "ver\u00b7ren\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um mit erzwungnem Laut Buchstaben zu beschenken.", "tokens": ["Um", "mit", "er\u00b7zwung\u00b7nem", "Laut", "Buch\u00b7sta\u00b7ben", "zu", "be\u00b7schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ADJA", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie denken, so den Geist des Lebens einzusenken", "tokens": ["Sie", "den\u00b7ken", ",", "so", "den", "Geist", "des", "Le\u00b7bens", "ein\u00b7zu\u00b7sen\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "ADV", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Buchstab, den sie sich als einen todten denken.", "tokens": ["Dem", "Buch\u00b7stab", ",", "den", "sie", "sich", "als", "ei\u00b7nen", "tod\u00b7ten", "den\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PRF", "KOUS", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was werden sie mit der Beschw\u00f6rungskunst erreichen,", "tokens": ["Was", "wer\u00b7den", "sie", "mit", "der", "Be\u00b7schw\u00f6\u00b7rungs\u00b7kunst", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenn zu Scheinleben sie erwecken W\u00f6rterleichen?", "tokens": ["Wenn", "zu", "Schein\u00b7le\u00b7ben", "sie", "er\u00b7we\u00b7cken", "W\u00f6r\u00b7ter\u00b7lei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "VVINF", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Das geist'ge Bild entsetzt sich vor der K\u00f6rperfratze,", "tokens": ["Das", "geist'\u00b7ge", "Bild", "ent\u00b7setzt", "sich", "vor", "der", "K\u00f6r\u00b7per\u00b7frat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selbst erkennt sich nicht die Sprach' in dem Geschwatze.", "tokens": ["Und", "selbst", "er\u00b7kennt", "sich", "nicht", "die", "Sprach'", "in", "dem", "Ge\u00b7schwat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Du danks dem Geiste, der, weil eben mu\u00dft' entweichen", "tokens": ["Du", "danks", "dem", "Geis\u00b7te", ",", "der", ",", "weil", "e\u00b7ben", "mu\u00dft'", "ent\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "PRELS", "$,", "KOUS", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Stimme Klang, sich selbst befestigt hat im Zeichen.", "tokens": ["Der", "Stim\u00b7me", "Klang", ",", "sich", "selbst", "be\u00b7fes\u00b7tigt", "hat", "im", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRF", "ADV", "VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Den V\u00e4tern dank' es, die vernehmlich ihren S\u00f6hnen", "tokens": ["Den", "V\u00e4\u00b7tern", "dank'", "es", ",", "die", "ver\u00b7nehm\u00b7lich", "ih\u00b7ren", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich \u00fcber Zeit und Raum kund thun, doch nicht in T\u00f6nen.", "tokens": ["Sich", "\u00fc\u00b7ber", "Zeit", "und", "Raum", "kund", "thun", ",", "doch", "nicht", "in", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "PTKVZ", "VVINF", "$,", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wie einst die T\u00f6ne selbst in ihrem Sinn erklungen,", "tokens": ["Wie", "einst", "die", "T\u00f6\u00b7ne", "selbst", "in", "ih\u00b7rem", "Sinn", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das bild' in deinem Sinn, nicht mit dem Spiel der Zungen.", "tokens": ["Das", "bild'", "in", "dei\u00b7nem", "Sinn", ",", "nicht", "mit", "dem", "Spiel", "der", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Den Kindern la\u00df das Spiel, du h\u00f6re mit dem Geist,", "tokens": ["Den", "Kin\u00b7dern", "la\u00df", "das", "Spiel", ",", "du", "h\u00f6\u00b7re", "mit", "dem", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wisse, da\u00df du nur durch Geist den Geist befreist.", "tokens": ["Und", "wis\u00b7se", ",", "da\u00df", "du", "nur", "durch", "Geist", "den", "Geist", "be\u00b7freist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Der Urwelt Sprache thut dir kund mit Geisterhauch", "tokens": ["Der", "Ur\u00b7welt", "Spra\u00b7che", "thut", "dir", "kund", "mit", "Geis\u00b7ter\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht nur den innern Sinn, den innern Wohllaut auch.", "tokens": ["Nicht", "nur", "den", "in\u00b7nern", "Sinn", ",", "den", "in\u00b7nern", "Wohl\u00b7laut", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}