{"textgrid.poem.43128": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sie lodern, wie sie beben,", "tokens": ["Wie", "sie", "lo\u00b7dern", ",", "wie", "sie", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still verglimmen und verweh'n \u2013", "tokens": ["Still", "ver\u00b7glim\u00b7men", "und", "ver\u00b7weh'n", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein St\u00fcck von meinem Leben", "tokens": ["Und", "ein", "St\u00fcck", "von", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh' in Asche ich vergeh'n.", "tokens": ["Seh'", "in", "A\u00b7sche", "ich", "ver\u00b7geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "APPR", "NN", "PPER", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weiche, goldig-blonde Locken,", "tokens": ["Wei\u00b7che", ",", "gol\u00b7dig\u00b7blon\u00b7de", "Lo\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manche Blume, die da schlief,", "tokens": ["Man\u00b7che", "Blu\u00b7me", ",", "die", "da", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es zerstirbt in Aschenflocken", "tokens": ["Es", "zer\u00b7stirbt", "in", "A\u00b7schen\u00b7flo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mancher alte Liebesbrief.", "tokens": ["Man\u00b7cher", "al\u00b7te", "Lie\u00b7bes\u00b7brief", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Welches Gl\u00fcck die Worte brachten,", "tokens": ["Wel\u00b7ches", "Gl\u00fcck", "die", "Wor\u00b7te", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Diese Phrasen, \u2013 Gott erbarm'!", "tokens": ["Die\u00b7se", "Phra\u00b7sen", ",", "\u2013", "Gott", "er\u00b7barm'", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "$(", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie sie ", "tokens": ["Wie", "sie"], "token_info": ["word", "word"], "pos": ["PWAV", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Heute wird die ", "tokens": ["Heu\u00b7te", "wird", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}}}}