{"textgrid.poem.48132": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder weht ein Fr\u00fchlingshauch", "tokens": ["Wie\u00b7der", "weht", "ein", "Fr\u00fch\u00b7lings\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "rings aus Busch und B\u00e4umen,", "tokens": ["rings", "aus", "Busch", "und", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "und die winterm\u00fcde Welt", "tokens": ["und", "die", "win\u00b7ter\u00b7m\u00fc\u00b7de", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "liegt in Ostertr\u00e4umen;", "tokens": ["liegt", "in", "Os\u00b7ter\u00b7tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "doch kein Auferstehungslied", "tokens": ["doch", "kein", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7lied"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "will die M\u00e4r uns deuten \u2013", "tokens": ["will", "die", "M\u00e4r", "uns", "deu\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "durch die dumpfen L\u00fcfte zieht", "tokens": ["durch", "die", "dum\u00b7pfen", "L\u00fcf\u00b7te", "zieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sterbeglockenl\u00e4uten.", "tokens": ["Ster\u00b7be\u00b7glo\u00b7cken\u00b7l\u00e4u\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Lastend wie Karfreitagsweh", "tokens": ["Las\u00b7tend", "wie", "Kar\u00b7frei\u00b7tags\u00b7weh"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KOKOM", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "h\u00e4ngt die Wetterwolke", "tokens": ["h\u00e4ngt", "die", "Wet\u00b7ter\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "tr\u00e4nenreich und blitzeschwer", "tokens": ["tr\u00e4\u00b7nen\u00b7reich", "und", "blit\u00b7ze\u00b7schwer"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00fcber unserm Volke.", "tokens": ["\u00fc\u00b7ber", "un\u00b7serm", "Vol\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Was da spro\u00dft im Sonnenschein,", "tokens": ["Was", "da", "spro\u00dft", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "will ihr Zorn begraben \u2013", "tokens": ["will", "ihr", "Zorn", "be\u00b7gra\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "durch die deutsche Fr\u00fchlingsflur", "tokens": ["durch", "die", "deut\u00b7sche", "Fr\u00fch\u00b7lings\u00b7flur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "flattern schwarze Raben.", "tokens": ["flat\u00b7tern", "schwar\u00b7ze", "Ra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sprengt kein Gott des Grabes Tor,", "tokens": ["Sprengt", "kein", "Gott", "des", "Gra\u00b7bes", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "uns vor Schmach zu retten?", "tokens": ["uns", "vor", "Schmach", "zu", "ret\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deutschen Geistes Herrlichkeit", "tokens": ["Deut\u00b7schen", "Geis\u00b7tes", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "schlagen sie in Ketten;", "tokens": ["schla\u00b7gen", "sie", "in", "Ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Kerkermauern bauen sie", "tokens": ["Ker\u00b7ker\u00b7mau\u00b7ern", "bau\u00b7en", "sie"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "uns zur Fr\u00fchlingsfeier,", "tokens": ["uns", "zur", "Fr\u00fch\u00b7lings\u00b7fei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "und der Sch\u00f6nheit reines Bild", "tokens": ["und", "der", "Sch\u00f6n\u00b7heit", "rei\u00b7nes", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "decken Nonnenschleier.", "tokens": ["de\u00b7cken", "Non\u00b7nen\u00b7schlei\u00b7er", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Pharis\u00e4erscharen,", "tokens": ["Pha\u00b7ri\u00b7s\u00e4er\u00b7scha\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "richten sie den freien Geist,", "tokens": ["rich\u00b7ten", "sie", "den", "frei\u00b7en", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wie vor tausend Jahren.", "tokens": ["wie", "vor", "tau\u00b7send", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wieder soll der Scheite Qualm", "tokens": ["Wie\u00b7der", "soll", "der", "Schei\u00b7te", "Qualm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "lichtumd\u00fcsternd steigen,", "tokens": ["licht\u00b7umd\u00b7\u00fcs\u00b7ternd", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "und das Kreuz von Golgatha", "tokens": ["und", "das", "Kreuz", "von", "Gol\u00b7ga\u00b7tha"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "gr\u00fc\u00dft in ernstem Schweigen.", "tokens": ["gr\u00fc\u00dft", "in", "erns\u00b7tem", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "M\u00f6rder des ", "tokens": ["M\u00f6r\u00b7der", "des"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "wie vor tausend Jahren", "tokens": ["wie", "vor", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "wird es doch aus Grabeskluft", "tokens": ["wird", "es", "doch", "aus", "Gra\u00b7bes\u00b7kluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "siegreich aufw\u00e4rts fahren?", "tokens": ["sieg\u00b7reich", "auf\u00b7w\u00e4rts", "fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nimmer hat Gewalt und Tod", "tokens": ["Nim\u00b7mer", "hat", "Ge\u00b7walt", "und", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "noch das Wort bezwungen,", "tokens": ["noch", "das", "Wort", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "das vom Geist empfangen ist", "tokens": ["das", "vom", "Geist", "emp\u00b7fan\u00b7gen", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und aus Gott entsprungen.", "tokens": ["und", "aus", "Gott", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "La\u00df, mein Volk, die Finsternis", "tokens": ["La\u00df", ",", "mein", "Volk", ",", "die", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "deinen Fu\u00df nicht irren:", "tokens": ["dei\u00b7nen", "Fu\u00df", "nicht", "ir\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "einmal mu\u00df des Lichtes Pfeil", "tokens": ["ein\u00b7mal", "mu\u00df", "des", "Lich\u00b7tes", "Pfeil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "durch die Wolke schwirren \u2013", "tokens": ["durch", "die", "Wol\u00b7ke", "schwir\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "und ein Auferstehungslied", "tokens": ["und", "ein", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7lied"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "sollst du freudig singen,", "tokens": ["sollst", "du", "freu\u00b7dig", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "wenn im ", "tokens": ["wenn", "im"], "token_info": ["word", "word"], "pos": ["KOUS", "APPRART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Osterglocken klingen!", "tokens": ["Os\u00b7ter\u00b7glo\u00b7cken", "klin\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}