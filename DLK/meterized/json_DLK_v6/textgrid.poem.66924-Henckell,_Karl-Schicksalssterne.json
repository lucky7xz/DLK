{"textgrid.poem.66924": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Schicksalssterne", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im silbernen \u00c4ther", "tokens": ["Im", "sil\u00b7ber\u00b7nen", "\u00c4\u00b7ther"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was singen die Leuchten,", "tokens": ["Was", "sin\u00b7gen", "die", "Leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So urfern von zitternder Sehnsucht erschaut?", "tokens": ["So", "ur\u00b7fern", "von", "zit\u00b7tern\u00b7der", "Sehn\u00b7sucht", "er\u00b7schaut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Kein Stern hilft den schwankend", "tokens": ["Kein", "Stern", "hilft", "den", "schwan\u00b7kend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Vom Schicksal Gescheuchten,", "tokens": ["Vom", "Schick\u00b7sal", "Ge\u00b7scheuch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie singen ein Lied nur: \u00bbSteht fest und vertraut!", "tokens": ["Sie", "sin\u00b7gen", "ein", "Lied", "nur", ":", "\u00bb", "Steht", "fest", "und", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$.", "$(", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Steht fest wie die Sterne,", "tokens": ["Steht", "fest", "wie", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Vertraut eurem Kerne,", "tokens": ["Ver\u00b7traut", "eu\u00b7rem", "Ker\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Seid ruhig dem innersten Wesen getreu!", "tokens": ["Seid", "ru\u00b7hig", "dem", "in\u00b7ners\u00b7ten", "We\u00b7sen", "ge\u00b7treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In wirbelnden Flammen", "tokens": ["In", "wir\u00b7beln\u00b7den", "Flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Rafft z\u00e4h euch zusammen,", "tokens": ["Rafft", "z\u00e4h", "euch", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So bannt ihr das Schicksal \u2013 und alles wird neu.", "tokens": ["So", "bannt", "ihr", "das", "Schick\u00b7sal", "\u2013", "und", "al\u00b7les", "wird", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Das Schicksal von oben,", "tokens": ["Das", "Schick\u00b7sal", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In euch will's erproben", "tokens": ["In", "euch", "will's", "er\u00b7pro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die g\u00f6ttliche Kraft, die sich gl\u00fchend bew\u00e4hrt.", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Kraft", ",", "die", "sich", "gl\u00fc\u00b7hend", "be\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wer gibt sie verloren?", "tokens": ["Wer", "gibt", "sie", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zieht k\u00fchn zu den Toren", "tokens": ["Zieht", "k\u00fchn", "zu", "den", "To\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Tat, ob die Zeit auch mit Schrecken sich j\u00e4hrt!", "tokens": ["Der", "Tat", ",", "ob", "die", "Zeit", "auch", "mit", "Schre\u00b7cken", "sich", "j\u00e4hrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "ADV", "APPR", "NN", "PRF", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "In Wunden und Schw\u00e4ren", "tokens": ["In", "Wun\u00b7den", "und", "Schw\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mu\u00df leidend geb\u00e4ren", "tokens": ["Mu\u00df", "lei\u00b7dend", "ge\u00b7b\u00e4\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Mutter, die Erde, den Siegfried des Lichts.", "tokens": ["Die", "Mut\u00b7ter", ",", "die", "Er\u00b7de", ",", "den", "Sieg\u00b7fried", "des", "Lichts", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zu Staub die Verr\u00e4ter!", "tokens": ["Zu", "Staub", "die", "Ver\u00b7r\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Heil hilfreichem T\u00e4ter!\u00ab", "tokens": ["Heil", "hilf\u00b7rei\u00b7chem", "T\u00e4\u00b7ter", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "So singen im \u00c4ther", "tokens": ["So", "sin\u00b7gen", "im", "\u00c4\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die ewigen Sterne des Weltengedichts.", "tokens": ["Die", "e\u00b7wi\u00b7gen", "Ster\u00b7ne", "des", "Wel\u00b7ten\u00b7ge\u00b7dichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}