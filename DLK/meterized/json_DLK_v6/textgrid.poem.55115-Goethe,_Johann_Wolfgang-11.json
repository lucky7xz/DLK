{"textgrid.poem.55115": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn durch das Volk die grimme Seuche w\u00fctet,", "tokens": ["Wenn", "durch", "das", "Volk", "die", "grim\u00b7me", "Seu\u00b7che", "w\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll man vorsichtig die Gesellschaft lassen.", "tokens": ["Soll", "man", "vor\u00b7sich\u00b7tig", "die", "Ge\u00b7sell\u00b7schaft", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch hab ich oft mit Zaudern und Verpassen", "tokens": ["Auch", "hab", "ich", "oft", "mit", "Zau\u00b7dern", "und", "Ver\u00b7pas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor manchen Influenzen mich geh\u00fctet.", "tokens": ["Vor", "man\u00b7chen", "In\u00b7flu\u00b7en\u00b7zen", "mich", "ge\u00b7h\u00fc\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und ob gleich Amor \u00f6fters mich beg\u00fctet,", "tokens": ["Und", "ob", "gleich", "A\u00b7mor", "\u00f6f\u00b7ters", "mich", "be\u00b7g\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NE", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mocht ich zuletzt mich nicht mit ihm befassen.", "tokens": ["Mocht", "ich", "zu\u00b7letzt", "mich", "nicht", "mit", "ihm", "be\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So ging mir's auch mit jenen Lacrimassen,", "tokens": ["So", "ging", "mir's", "auch", "mit", "je\u00b7nen", "La\u00b7cri\u00b7mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als vier \u2013 und dreifach reimend sie gebr\u00fctet.", "tokens": ["Als", "vier", "\u2013", "und", "drei\u00b7fach", "rei\u00b7mend", "sie", "ge\u00b7br\u00fc\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "$(", "KON", "PAV", "VVPP", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun aber folgt die Strafe dem Ver\u00e4chter,", "tokens": ["Nun", "a\u00b7ber", "folgt", "die", "Stra\u00b7fe", "dem", "Ver\u00b7\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wenn die Schlangenfackel der Erinnen", "tokens": ["Als", "wenn", "die", "Schlan\u00b7gen\u00b7fa\u00b7ckel", "der", "E\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Berg zu Tal, von Land zu Meer ihn triebe.", "tokens": ["Von", "Berg", "zu", "Tal", ",", "von", "Land", "zu", "Meer", "ihn", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6re wohl der Genien Gel\u00e4chter;", "tokens": ["Ich", "h\u00f6\u00b7re", "wohl", "der", "Ge\u00b7ni\u00b7en", "Ge\u00b7l\u00e4ch\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch trennet mich von jeglichem Besinnen", "tokens": ["Doch", "tren\u00b7net", "mich", "von", "jeg\u00b7li\u00b7chem", "Be\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sonettenwut und Raserei der Liebe.", "tokens": ["So\u00b7net\u00b7ten\u00b7wut", "und", "Ra\u00b7se\u00b7rei", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}