{"dta.poem.15814": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Dichter ist ein Thor, der das der Welt zu zeigen", "tokens": ["Ein", "Dich\u00b7ter", "ist", "ein", "Thor", ",", "der", "das", "der", "Welt", "zu", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bem\u00fcht ist, was ihr sucht ein Weiser zu verschweigen.", "tokens": ["Be\u00b7m\u00fcht", "ist", ",", "was", "ihr", "sucht", "ein", "Wei\u00b7ser", "zu", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ihm am Herzen liegt, und er gradaus den Leuten", "tokens": ["Was", "ihm", "am", "Her\u00b7zen", "liegt", ",", "und", "er", "gra\u00b7daus", "den", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "KON", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht sagen darf, wei\u00df er verh\u00fcllend anzudeuten.", "tokens": ["Nicht", "sa\u00b7gen", "darf", ",", "wei\u00df", "er", "ver\u00b7h\u00fcl\u00b7lend", "an\u00b7zu\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "VVPP", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er hofft, sie sind nicht fein genug, es zu ergr\u00fcnden,", "tokens": ["Er", "hofft", ",", "sie", "sind", "nicht", "fein", "ge\u00b7nug", ",", "es", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch \u00e4rgern w\u00fcrd' es ihn, wenn sie ihn nicht verst\u00fcnden.", "tokens": ["Doch", "\u00e4r\u00b7gern", "w\u00fcrd'", "es", "ihn", ",", "wenn", "sie", "ihn", "nicht", "ver\u00b7st\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}