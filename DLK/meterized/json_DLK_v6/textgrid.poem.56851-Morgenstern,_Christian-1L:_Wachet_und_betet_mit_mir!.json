{"textgrid.poem.56851": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wachet und betet mit mir!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wachet und betet mit mir!", "tokens": ["Wa\u00b7chet", "und", "be\u00b7tet", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Meine Seele ist traurig", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ist", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "bis an den Tod.", "tokens": ["bis", "an", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wachet und betet!", "tokens": ["Wa\u00b7chet", "und", "be\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Eure Augen", "tokens": ["Eu\u00b7re", "Au\u00b7gen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "sind voll Schlafes, \u2013", "tokens": ["sind", "voll", "Schla\u00b7fes", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "NN", "$,", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "k\u00f6nnt ihr nicht wachen?", "tokens": ["k\u00f6nnt", "ihr", "nicht", "wa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ich gehe,", "tokens": ["Ich", "ge\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "euch mein Letztes zu geben \u2013", "tokens": ["euch", "mein", "Letz\u00b7tes", "zu", "ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "und ihr schlaft ...", "tokens": ["und", "ihr", "schlaft", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Einsam stehe ich", "tokens": ["Ein\u00b7sam", "ste\u00b7he", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "unter Schlafenden,", "tokens": ["un\u00b7ter", "Schla\u00b7fen\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.13": {"text": "einsam vollbring ich", "tokens": ["ein\u00b7sam", "voll\u00b7bring", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "das Werk meiner schwersten Stunde.", "tokens": ["das", "Werk", "mei\u00b7ner", "schwers\u00b7ten", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Wachet und betet mir mir!", "tokens": ["Wa\u00b7chet", "und", "be\u00b7tet", "mir", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.16": {"text": "Ihr alle seid in mir,", "tokens": ["Ihr", "al\u00b7le", "seid", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "aber in wem bin ich?", "tokens": ["a\u00b7ber", "in", "wem", "bin", "ich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PWS", "VAFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.18": {"text": "Was wi\u00dft ihr", "tokens": ["Was", "wi\u00dft", "ihr"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "von meiner Liebe,", "tokens": ["von", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "was wi\u00dft ihr", "tokens": ["was", "wi\u00dft", "ihr"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "vom Schmerz meiner Seele!", "tokens": ["vom", "Schmerz", "mei\u00b7ner", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "O einsam!", "tokens": ["O", "ein\u00b7sam", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "einsam! ", "tokens": ["ein\u00b7sam", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "und ihr schlaft!", "tokens": ["und", "ihr", "schlaft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}