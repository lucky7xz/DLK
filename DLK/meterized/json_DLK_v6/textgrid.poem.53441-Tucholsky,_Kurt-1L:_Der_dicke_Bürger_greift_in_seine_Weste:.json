{"textgrid.poem.53441": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der dicke B\u00fcrger greift in seine Weste:", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der dicke B\u00fcrger greift in seine Weste:", "tokens": ["Der", "di\u00b7cke", "B\u00fcr\u00b7ger", "greift", "in", "sei\u00b7ne", "Wes\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbda nimm! mein Kind!\u00ab \u2013", "tokens": ["\u00bb", "da", "nimm", "!", "mein", "Kind", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADV", "VVIMP", "$.", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Er gibt den Sechser mit ger\u00fchrter Geste \u2013", "tokens": ["Er", "gibt", "den", "Sech\u00b7ser", "mit", "ge\u00b7r\u00fchr\u00b7ter", "Ges\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die Tr\u00e4ne rinnt! \u2013", "tokens": ["die", "Tr\u00e4\u00b7ne", "rinnt", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das Auge tropft. Der dicke Bauch schl\u00e4gt Wellen.", "tokens": ["Das", "Au\u00b7ge", "tropft", ".", "Der", "di\u00b7cke", "Bauch", "schl\u00e4gt", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "In solchen Patriotenrummelf\u00e4llen", "tokens": ["In", "sol\u00b7chen", "Pat\u00b7ri\u00b7o\u00b7ten\u00b7rum\u00b7mel\u00b7f\u00e4l\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da tut er das!", "tokens": ["da", "tut", "er", "das", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Er sorgt f\u00fcr Veteranenpensionierung", "tokens": ["Er", "sorgt", "f\u00fcr", "Ve\u00b7te\u00b7ra\u00b7nen\u00b7pen\u00b7si\u00b7o\u00b7nie\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "von Stolz geschwellt \u2013", "tokens": ["von", "Stolz", "ge\u00b7schwellt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bei uns hat n\u00e4mlich daf\u00fcr die Regierung,", "tokens": ["Bei", "uns", "hat", "n\u00e4m\u00b7lich", "da\u00b7f\u00fcr", "die", "Re\u00b7gie\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wei\u00df Gott! kein Geld.", "tokens": ["wei\u00df", "Gott", "!", "kein", "Geld", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Denn sie mu\u00df eifrig auf die \u2020\u2020\u2020 Roten fahnden \u2013", "tokens": ["Denn", "sie", "mu\u00df", "eif\u00b7rig", "auf", "die", "\u2020\u2020\u2020", "Ro\u00b7ten", "fahn\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "sie darf nicht ruhn.", "tokens": ["sie", "darf", "nicht", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sie mu\u00df politische Verbrechen ahnden \u2013", "tokens": ["Sie", "mu\u00df", "po\u00b7li\u00b7ti\u00b7sche", "Ver\u00b7bre\u00b7chen", "ahn\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "VVINF", "$("], "meter": "+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "sie hat zu tun \u2013!!", "tokens": ["sie", "hat", "zu", "tun", "\u2013", "!!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "F\u00fcrs Kriegerpack", "tokens": ["F\u00fcrs", "Krie\u00b7ger\u00b7pack"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "da betteln sie derweil auf allen Gassen \u2013", "tokens": ["da", "bet\u00b7teln", "sie", "der\u00b7weil", "auf", "al\u00b7len", "Gas\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kornblumentag . . .", "tokens": ["Korn\u00b7blu\u00b7men\u00b7tag", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Der B\u00fcrger denkt bei Tisch, nach s\u00fc\u00dfen Torten", "tokens": ["Der", "B\u00fcr\u00b7ger", "denkt", "bei", "Tisch", ",", "nach", "s\u00fc\u00b7\u00dfen", "Tor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und blauem Aal,", "tokens": ["und", "blau\u00b7em", "Aal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "(hupp! st\u00f6\u00dft's ihm auf \u2013 ): \u00bbWie sind wir allerorten", "tokens": ["(", "hupp", "!", "st\u00f6\u00dft's", "ihm", "auf", "\u2013", ")", ":", "\u00bb", "Wie", "sind", "wir", "al\u00b7le\u00b7ror\u00b7ten"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$.", "VVFIN", "PPER", "APPR", "$(", "$(", "$.", "$(", "PWAV", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "christlich-sozial!\u00ab \u2013", "tokens": ["christ\u00b7lich\u00b7so\u00b7zi\u00b7al", "!", "\u00ab", "\u2013"], "token_info": ["word", "punct", "punct", "punct"], "pos": ["ADJD", "$.", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}