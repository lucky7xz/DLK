{"textgrid.poem.55449": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "29", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Vermittlung einer Teuren", "tokens": ["Durch", "Ver\u00b7mitt\u00b7lung", "ei\u00b7ner", "Teu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht ein T\u00e4schchen bis zur Elbe,", "tokens": ["Geht", "ein", "T\u00e4schchen", "bis", "zur", "El\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPRART", "NE", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Kommt, vom Freunde zu beteuren:", "tokens": ["Kommt", ",", "vom", "Freun\u00b7de", "zu", "be\u00b7teu\u00b7ren", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer bleibet er derselbe.", "tokens": ["Im\u00b7mer", "blei\u00b7bet", "er", "der\u00b7sel\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer wie in Dornburgs Gauen,", "tokens": ["Im\u00b7mer", "wie", "in", "Dorn\u00b7burgs", "Gau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo beim allerbesten Weine", "tokens": ["Wo", "beim", "al\u00b7ler\u00b7bes\u00b7ten", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Waren hell im Sonnenscheine", "tokens": ["Wa\u00b7ren", "hell", "im", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Berg' und T\u00e4ler anzuschauen.", "tokens": ["Ber\u00b7g'", "und", "T\u00e4\u00b7ler", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du nun an der reichen Elbe,", "tokens": ["Du", "nun", "an", "der", "rei\u00b7chen", "El\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem spiegelbreiten Flusse,", "tokens": ["An", "dem", "spie\u00b7gel\u00b7brei\u00b7ten", "Flus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weit entfernt vom trauten Kusse", "tokens": ["Weit", "ent\u00b7fernt", "vom", "trau\u00b7ten", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleib auch immerfort derselbe.", "tokens": ["Bleib", "auch", "im\u00b7mer\u00b7fort", "der\u00b7sel\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PDAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}