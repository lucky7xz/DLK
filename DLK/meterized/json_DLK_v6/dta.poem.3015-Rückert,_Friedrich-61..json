{"dta.poem.3015": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Welt Anschauungen, der Dinge Sinnabdr\u00fccke,", "tokens": ["Der", "Welt", "An\u00b7schau\u00b7un\u00b7gen", ",", "der", "Din\u00b7ge", "Sin\u00b7nab\u00b7dr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind sch\u00f6n da\u00df sich damit das Haus der Seele schm\u00fccke.", "tokens": ["Sind", "sch\u00f6n", "da\u00df", "sich", "da\u00b7mit", "das", "Haus", "der", "See\u00b7le", "schm\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOUS", "PRF", "PAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Je k\u00fcnstlerischer sie anordnet und verkl\u00e4rt", "tokens": ["Je", "k\u00fcnst\u00b7le\u00b7ri\u00b7scher", "sie", "an\u00b7ord\u00b7net", "und", "ver\u00b7kl\u00e4rt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seele, je mehr Wonn' ihr Wohnhaus ihr gew\u00e4hrt.", "tokens": ["Die", "See\u00b7le", ",", "je", "mehr", "Wonn'", "ihr", "Wohn\u00b7haus", "ihr", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIAT", "NN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch keins der Bilder dient zu gr\u00fcndlicher Erbauung", "tokens": ["Doch", "keins", "der", "Bil\u00b7der", "dient", "zu", "gr\u00fcnd\u00b7li\u00b7cher", "Er\u00b7bau\u00b7ung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie das Altarbild nur geweihter Gottanschauung.", "tokens": ["Wie", "das", "Al\u00b7tar\u00b7bild", "nur", "ge\u00b7weih\u00b7ter", "Got\u00b7tan\u00b7schau\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jeweiter seinen Glanz ergie\u00dft dis Mittelbild,", "tokens": ["Je\u00b7wei\u00b7ter", "sei\u00b7nen", "Glanz", "er\u00b7gie\u00dft", "dis", "Mit\u00b7tel\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erf\u00fcllend immermehr das innere Gefild;", "tokens": ["Er\u00b7f\u00fcl\u00b7lend", "im\u00b7mer\u00b7mehr", "das", "in\u00b7ne\u00b7re", "Ge\u00b7fild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Jeweiter tritt zur\u00fcck das zeitliche Gew\u00fchl,", "tokens": ["Je\u00b7wei\u00b7ter", "tritt", "zu\u00b7r\u00fcck", "das", "zeit\u00b7li\u00b7che", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und geht beseligt auf in Ewigkeitsgef\u00fchl.", "tokens": ["Und", "geht", "be\u00b7se\u00b7ligt", "auf", "in", "E\u00b7wig\u00b7keits\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ged\u00e4chtniswissenschaft, Dichtkunsteinbildungskraft,", "tokens": ["Ge\u00b7d\u00e4cht\u00b7nis\u00b7wis\u00b7sen\u00b7schaft", ",", "Dicht\u00b7kuns\u00b7tein\u00b7bil\u00b7dungs\u00b7kraft", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind vor der Seele Gottbewu\u00dftseyn kummerhaft.", "tokens": ["Sind", "vor", "der", "See\u00b7le", "Gott\u00b7be\u00b7wu\u00df\u00b7tseyn", "kum\u00b7mer\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "In ihm wird ihr, die sich gef\u00fchlt nach au\u00dfen endlich,", "tokens": ["In", "ihm", "wird", "ihr", ",", "die", "sich", "ge\u00b7f\u00fchlt", "nach", "au\u00b7\u00dfen", "end\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "$,", "PRELS", "PRF", "VVPP", "APPR", "ADV", "ADV", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihr eigenst-innerstes Unsterbliches verst\u00e4ndlich.", "tokens": ["Ihr", "ei\u00b7genst\u00b7in\u00b7ners\u00b7tes", "U\u00b7nsterb\u00b7li\u00b7ches", "ver\u00b7st\u00e4nd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}