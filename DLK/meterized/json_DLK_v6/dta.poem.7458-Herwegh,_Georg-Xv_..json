{"dta.poem.7458": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Xv .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie blinkend sie von eurem Ruder triefe,", "tokens": ["Wie", "blin\u00b7kend", "sie", "von", "eu\u00b7rem", "Ru\u00b7der", "trie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Perle stammt doch oft aus dunkler Quelle,", "tokens": ["Die", "Per\u00b7le", "stammt", "doch", "oft", "aus", "dunk\u00b7ler", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Klar scheint in flacher Hand so manche Welle,", "tokens": ["Klar", "scheint", "in", "fla\u00b7cher", "Hand", "so", "man\u00b7che", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die doch gesch\u00f6pft aus grauenvoller Tiefe.", "tokens": ["Die", "doch", "ge\u00b7sch\u00f6pft", "aus", "grau\u00b7en\u00b7vol\u00b7ler", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schlie\u00dft, wie's auch einer Welt zuwiderliefe,", "tokens": ["Schlie\u00dft", ",", "wie's", "auch", "ei\u00b7ner", "Welt", "zu\u00b7wi\u00b7der\u00b7lie\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf's Heiligthum nie von der blanken Schwelle,", "tokens": ["Auf's", "Hei\u00b7lig\u00b7thum", "nie", "von", "der", "blan\u00b7ken", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das Einzelwort mag fa\u00dflich sein und helle,", "tokens": ["Das", "Ein\u00b7zel\u00b7wort", "mag", "fa\u00df\u00b7lich", "sein", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VAINF", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ganze Geist bleibt eine Hieroglife.", "tokens": ["Der", "gan\u00b7ze", "Geist", "bleibt", "ei\u00b7ne", "Hie\u00b7rog\u00b7li\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O denket immer bei des Dichters Pracht,", "tokens": ["O", "den\u00b7ket", "im\u00b7mer", "bei", "des", "Dich\u00b7ters", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei allen seinen funkelnden Gesteinen,", "tokens": ["Bei", "al\u00b7len", "sei\u00b7nen", "fun\u00b7keln\u00b7den", "Ge\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ihre Mutter ist die heil'ge Nacht!", "tokens": ["Da\u00df", "ih\u00b7re", "Mut\u00b7ter", "ist", "die", "heil'\u00b7ge", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sein Rauschen m\u00f6gt ihr zu verstehen meinen;", "tokens": ["Sein", "Rau\u00b7schen", "m\u00f6gt", "ihr", "zu", "ver\u00b7ste\u00b7hen", "mei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er selbst birgt sich ein See im Felsenschacht,", "tokens": ["Er", "selbst", "birgt", "sich", "ein", "See", "im", "Fel\u00b7sen\u00b7schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ewig sieht des Himmels Sterne scheinen.", "tokens": ["Der", "e\u00b7wig", "sieht", "des", "Him\u00b7mels", "Ster\u00b7ne", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}