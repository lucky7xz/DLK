{"dta.poem.11534": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Im Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Hier lieg' ich auf dem Fr\u00fchlingsh\u00fcgel;", "tokens": ["Hier", "lieg'", "ich", "auf", "dem", "Fr\u00fch\u00b7lings\u00b7h\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wolke wird mein Fl\u00fcgel,", "tokens": ["Die", "Wol\u00b7ke", "wird", "mein", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Vogel fliegt mir voraus.", "tokens": ["Ein", "Vo\u00b7gel", "fliegt", "mir", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ach, sag mir, all-einzige Liebe,", "tokens": ["Ach", ",", "sag", "mir", ",", "all\u00b7ein\u00b7zi\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Doch du und die L\u00fcfte, sie haben kein Haus.", "tokens": ["Doch", "du", "und", "die", "L\u00fcf\u00b7te", ",", "sie", "ha\u00b7ben", "kein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "ART", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Der Sonnenblume gleich steht mein Gem\u00fcthe offen,", "tokens": ["Der", "Son\u00b7nen\u00b7blu\u00b7me", "gleich", "steht", "mein", "Ge\u00b7m\u00fc\u00b7the", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sehnend,", "tokens": ["Seh\u00b7nend", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sich dehnend,", "tokens": ["Sich", "deh\u00b7nend", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "In Lieben und Hoffen.", "tokens": ["In", "Lie\u00b7ben", "und", "Hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Fr\u00fchling, was bist du gewillt?", "tokens": ["Fr\u00fch\u00b7ling", ",", "was", "bist", "du", "ge\u00b7willt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Wann werd' ich gestillt?", "tokens": ["Wann", "werd'", "ich", "ge\u00b7stillt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Die Wolke seh' ich wandeln und den Flu\u00df,", "tokens": ["Die", "Wol\u00b7ke", "seh'", "ich", "wan\u00b7deln", "und", "den", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es dringt der Sonne goldner Ku\u00df", "tokens": ["Es", "dringt", "der", "Son\u00b7ne", "gold\u00b7ner", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir tief bis in's Gebl\u00fct hinein;", "tokens": ["Mir", "tief", "bis", "in's", "Ge\u00b7bl\u00fct", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Augen, wunderbar berauschet,", "tokens": ["Die", "Au\u00b7gen", ",", "wun\u00b7der\u00b7bar", "be\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Thun, als schliefen sie ein,", "tokens": ["Thun", ",", "als", "schlie\u00b7fen", "sie", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Nur noch das Ohr dem Ton der Biene lauschet.", "tokens": ["Nur", "noch", "das", "Ohr", "dem", "Ton", "der", "Bie\u00b7ne", "lau\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich denke Dies, und denke Das,", "tokens": ["Ich", "den\u00b7ke", "Dies", ",", "und", "den\u00b7ke", "Das", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "KON", "VVFIN", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich sehne mich, und wei\u00df nicht recht, nach was:", "tokens": ["Ich", "seh\u00b7ne", "mich", ",", "und", "wei\u00df", "nicht", "recht", ",", "nach", "was", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKNEG", "ADJD", "$,", "APPR", "PRELS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Halb ist es Lust, halb ist es Klage;", "tokens": ["Halb", "ist", "es", "Lust", ",", "halb", "ist", "es", "Kla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "NN", "$,", "ADJD", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Herz, o sage:", "tokens": ["Mein", "Herz", ",", "o", "sa\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Was webst du f\u00fcr Erinnerung", "tokens": ["Was", "webst", "du", "f\u00fcr", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In golden gr\u00fcner Zweige D\u00e4mmerung?", "tokens": ["In", "gol\u00b7den", "gr\u00fc\u00b7ner", "Zwei\u00b7ge", "D\u00e4m\u00b7me\u00b7rung", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "\u2014 Alte unnennbare Tage!", "tokens": ["Al\u00b7te", "un\u00b7nenn\u00b7ba\u00b7re", "Ta\u00b7ge", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}