{"textgrid.poem.67703": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie nenn' ich es, das \u00fcber Menschenseelen", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie nenn' ich es, das \u00fcber Menschenseelen", "tokens": ["Wie", "nenn'", "ich", "es", ",", "das", "\u00fc\u00b7ber", "Men\u00b7schen\u00b7see\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Siegel Gottes schwebt", "tokens": ["Ein", "Sie\u00b7gel", "Got\u00b7tes", "schwebt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihre Tiefen (Niemand kann sie z\u00e4hlen)", "tokens": ["Und", "ih\u00b7re", "Tie\u00b7fen", "(", "Nie\u00b7mand", "kann", "sie", "z\u00e4h\u00b7len", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "PIS", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Zum Gottgebilde, das, itzt zarte Pflanze,", "tokens": ["Zum", "Gott\u00b7ge\u00b7bil\u00b7de", ",", "das", ",", "itzt", "zar\u00b7te", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich sauget Blut aus Thau,", "tokens": ["Sich", "sau\u00b7get", "Blut", "aus", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird, was nur ", "tokens": ["Wird", ",", "was", "nur"], "token_info": ["word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "PRELS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und feuchter, wilder Au;", "tokens": ["Und", "feuch\u00b7ter", ",", "wil\u00b7der", "Au", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Allm\u00e4hlig Baum, f\u00fchlt Baum\u00e4onenleben,", "tokens": ["All\u00b7m\u00e4h\u00b7lig", "Baum", ",", "f\u00fchlt", "Bau\u00b7m\u00e4\u00b7o\u00b7nen\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NE", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So schlank und fest und gr\u00fcn,", "tokens": ["So", "schlank", "und", "fest", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und strebt mit Wurzeln, Zweigen, fortzuweben", "tokens": ["Und", "strebt", "mit", "Wur\u00b7zeln", ",", "Zwei\u00b7gen", ",", "fort\u00b7zu\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und neue Kraft zu ziehn.", "tokens": ["Und", "neu\u00b7e", "Kraft", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchle Pflanz' und Baum in Frucht, in Samen,", "tokens": ["Ich", "f\u00fch\u00b7le", "Pflanz'", "und", "Baum", "in", "Frucht", ",", "in", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NE", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In D\u00fcften, Laub und Blatt,", "tokens": ["In", "D\u00fcf\u00b7ten", ",", "Laub", "und", "Blatt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Blick' in sie tief, und doch \u2013 wer leiht mir Namen,", "tokens": ["Blick'", "in", "sie", "tief", ",", "und", "doch", "\u2013", "wer", "leiht", "mir", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJD", "$,", "KON", "ADV", "$(", "PWS", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was Jedes ist und hat?", "tokens": ["Was", "Je\u00b7des", "ist", "und", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich schweig' und staune nur, blick' auf und nenne", "tokens": ["Ich", "schweig'", "und", "stau\u00b7ne", "nur", ",", "blick'", "auf", "und", "nen\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonnen jener Nacht,", "tokens": ["Die", "Son\u00b7nen", "je\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welten! \u2013 und blicke tiefer noch und kenne", "tokens": ["Die", "Wel\u00b7ten", "!", "\u2013", "und", "bli\u00b7cke", "tie\u00b7fer", "noch", "und", "ken\u00b7ne"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "KON", "VVFIN", "ADJD", "ADV", "KON", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Abgrund seiner Macht,", "tokens": ["Den", "Ab\u00b7grund", "sei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Seelen! mehr als Welten! Der ins Leben", "tokens": ["Die", "See\u00b7len", "!", "mehr", "als", "Wel\u00b7ten", "!", "Der", "ins", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "KOUS", "NN", "$.", "ART", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie rief, der Gottheit Schein,", "tokens": ["Sie", "rief", ",", "der", "Got\u00b7theit", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unnennbar, unersetzbar fortzuweben,", "tokens": ["Un\u00b7nenn\u00b7bar", ",", "un\u00b7er\u00b7setz\u00b7bar", "fort\u00b7zu\u00b7we\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Licht in die Nacht zu streun:", "tokens": ["Licht", "in", "die", "Nacht", "zu", "streun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Ihm sangen schon die Himmel hohe F\u00fclle", "tokens": ["Ihm", "san\u00b7gen", "schon", "die", "Him\u00b7mel", "ho\u00b7he", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Einklangs der Natur,", "tokens": ["Des", "Ein\u00b7klangs", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und unges\u00e4ttigt stand er, sann, und stille", "tokens": ["Und", "un\u00b7ge\u00b7s\u00e4t\u00b7tigt", "stand", "er", ",", "sann", ",", "und", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "VVFIN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Haucht' er \u2013 in Dich sich nur!", "tokens": ["Haucht'", "er", "\u2013", "in", "Dich", "sich", "nur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "APPR", "PPER", "PRF", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "In Dich, o Seele! Feire, Menschenseele,", "tokens": ["In", "Dich", ",", "o", "See\u00b7le", "!", "Fei\u00b7re", ",", "Men\u00b7schen\u00b7see\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem tiefen Gotteswink,", "tokens": ["Dem", "tie\u00b7fen", "Got\u00b7tes\u00b7wink", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn Dein Wesen, wenn aus Grabesh\u00f6hle", "tokens": ["Und", "wenn", "Dein", "We\u00b7sen", ",", "wenn", "aus", "Gra\u00b7bes\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Schauer Dich umfing", "tokens": ["Mit", "Schau\u00b7er", "Dich", "um\u00b7fing"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein heil'ger Schatte, sahest Bild, wie Z\u00fcge", "tokens": ["Ein", "heil'\u00b7ger", "Schat\u00b7te", ",", "sa\u00b7hest", "Bild", ",", "wie", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Geistesangesicht,", "tokens": ["Von", "Geis\u00b7te\u00b7san\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ging vor\u00fcber, und des Bildes Z\u00fcge,", "tokens": ["Das", "ging", "vor\u00b7\u00fc\u00b7ber", ",", "und", "des", "Bil\u00b7des", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Antlitz sahst Du nicht.", "tokens": ["Sein", "Ant\u00b7litz", "sahst", "Du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und eine Stimme sprach, und tiefes Beben", "tokens": ["Und", "ei\u00b7ne", "Stim\u00b7me", "sprach", ",", "und", "tie\u00b7fes", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ergriff Dich: \u00bbWer bist Du,", "tokens": ["Er\u00b7griff", "Dich", ":", "\u00bb", "Wer", "bist", "Du", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PWS", "VAFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Brunn zu \u00f6ffnen, wo mit ew'gem Streben", "tokens": ["Den", "Brunn", "zu", "\u00f6ff\u00b7nen", ",", "wo", "mit", "ew'\u00b7gem", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Gottheit quillet? Du?", "tokens": ["Die", "Got\u00b7theit", "quil\u00b7let", "?", "Du", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Erzittre dem Gebot! Des Ew'gen Schleier", "tokens": ["Er\u00b7zitt\u00b7re", "dem", "Ge\u00b7bot", "!", "Des", "Ew'\u00b7gen", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umwebt mit Dunkelheit", "tokens": ["Um\u00b7webt", "mit", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sch\u00f6pfung Allerheiligstes, wo Feuer", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7tes", ",", "wo", "Feu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Gottheit Flammen streut,", "tokens": ["Der", "Got\u00b7theit", "Flam\u00b7men", "streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Menschenseele! F\u00fchl's und sinke nieder", "tokens": ["Die", "Men\u00b7schen\u00b7see\u00b7le", "!", "F\u00fchl's", "und", "sin\u00b7ke", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NE", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit frohem Ungest\u00fcm,", "tokens": ["Mit", "fro\u00b7hem", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem alle Stern' und Seelen singen Lieder,", "tokens": ["Dem", "al\u00b7le", "Stern'", "und", "See\u00b7len", "sin\u00b7gen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Seelen Vater, ihm!", "tokens": ["Der", "See\u00b7len", "Va\u00b7ter", ",", "ihm", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Dem ihre Tief' und H\u00f6he singt, ihr Werden", "tokens": ["Dem", "ih\u00b7re", "Tie\u00b7f'", "und", "H\u00f6\u00b7he", "singt", ",", "ihr", "Wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Sein von Licht zu Licht,", "tokens": ["Und", "Sein", "von", "Licht", "zu", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von dumpfer D\u00e4mmrung hier auf dunkler Erden", "tokens": ["Von", "dum\u00b7pfer", "D\u00e4mm\u00b7rung", "hier", "auf", "dunk\u00b7ler", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis einst zu Angesicht.\u00ab", "tokens": ["Bis", "einst", "zu", "An\u00b7ge\u00b7sicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ich zittre! \u2013 Wag' ich's? Seine Schatten winken,", "tokens": ["Ich", "zitt\u00b7re", "!", "\u2013", "Wag'", "ich's", "?", "Sei\u00b7ne", "Schat\u00b7ten", "win\u00b7ken", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "PIS", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Seelen Abglanz winkt", "tokens": ["Der", "See\u00b7len", "Ab\u00b7glanz", "winkt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir Schau'r auf Schauer schon \u2013 und ich? \u2013 soll trinken,", "tokens": ["Mir", "Schau'r", "auf", "Schau\u00b7er", "schon", "\u2013", "und", "ich", "?", "\u2013", "soll", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NE", "ADV", "$(", "KON", "PPER", "$.", "$(", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Seel' aus Seele trinkt,", "tokens": ["Wie", "Seel'", "aus", "See\u00b7le", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wie Bruder hangt am Bruder, trinken Liebe", "tokens": ["Wie", "Bru\u00b7der", "hangt", "am", "Bru\u00b7der", ",", "trin\u00b7ken", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus ihm, der sich f\u00fcr mich", "tokens": ["Aus", "ihm", ",", "der", "sich", "f\u00fcr", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PRELS", "PRF", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur Seele haucht auf dieser Erdentr\u00fcbe", "tokens": ["Zur", "See\u00b7le", "haucht", "auf", "die\u00b7ser", "Er\u00b7den\u00b7tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu meinem Bilde? \u2013 Ich? \u2013", "tokens": ["Zu", "mei\u00b7nem", "Bil\u00b7de", "?", "\u2013", "Ich", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "O, nenn es nicht, was \u00fcber Menschenseelen", "tokens": ["O", ",", "nenn", "es", "nicht", ",", "was", "\u00fc\u00b7ber", "Men\u00b7schen\u00b7see\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Siegel Gottes h\u00e4ngt", "tokens": ["Ein", "Sie\u00b7gel", "Got\u00b7tes", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihre Tiefen (Niemand kann sie z\u00e4hlen)", "tokens": ["Und", "ih\u00b7re", "Tie\u00b7fen", "(", "Nie\u00b7mand", "kann", "sie", "z\u00e4h\u00b7len", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "PIS", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohin? zum Ursprung dr\u00e4ngt.", "tokens": ["Wo\u00b7hin", "?", "zum", "Ur\u00b7sprung", "dr\u00e4ngt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}