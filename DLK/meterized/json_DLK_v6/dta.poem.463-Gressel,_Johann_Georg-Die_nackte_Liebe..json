{"dta.poem.463": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die nackte Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt/ da\u00df die Natur das Feld mit Blumen zieret/", "tokens": ["Man", "sagt", "/", "da\u00df", "die", "Na\u00b7tur", "das", "Feld", "mit", "Blu\u00b7men", "zie\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KOUS", "ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Vogel/ und die Schaaf\u2019 in Woll\u2019 und Federn steckt", "tokens": ["Die", "Vo\u00b7gel", "/", "und", "die", "Schaaf'", "in", "Woll'", "und", "Fe\u00b7dern", "steckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "$(", "KON", "ART", "NN", "APPR", "NE", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wenn ein rauher Nord und wilder Ost regiret", "tokens": ["Und", "wenn", "ein", "rau\u00b7her", "Nord", "und", "wil\u00b7der", "Ost", "re\u00b7gi\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Uns in die Kleider h\u00fclt/ und durch die Kunst bedeckt.", "tokens": ["Uns", "in", "die", "Klei\u00b7der", "h\u00fclt", "/", "und", "durch", "die", "Kunst", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$(", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie hat sie alles denn/ nur ", "tokens": ["Wie", "hat", "sie", "al\u00b7les", "denn", "/", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PIS", "ADV", "$(", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Jemehr der nackend ist/ je wenger er Frost leidet.", "tokens": ["Je\u00b7mehr", "der", "na\u00b7ckend", "ist", "/", "je", "wen\u00b7ger", "er", "Frost", "lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "VAFIN", "$(", "ADV", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}