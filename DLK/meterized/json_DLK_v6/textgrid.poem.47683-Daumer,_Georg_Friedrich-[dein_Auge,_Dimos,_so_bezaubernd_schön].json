{"textgrid.poem.47683": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[dein Auge, Dimos, so bezaubernd sch\u00f6n]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Auge, Dimos, so bezaubernd sch\u00f6n,", "tokens": ["Dein", "Au\u00b7ge", ",", "Di\u00b7mos", ",", "so", "be\u00b7zau\u00b7bernd", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dein Gem\u00fcth, so kalt \u2013", "tokens": ["Und", "dein", "Ge\u00b7m\u00fcth", ",", "so", "kalt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie klag' ich an, beugt Jammer ohne Ma\u00df", "tokens": ["Sie", "klag'", "ich", "an", ",", "beugt", "Jam\u00b7mer", "oh\u00b7ne", "Ma\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Blume meines Seins.", "tokens": ["Die", "Blu\u00b7me", "mei\u00b7nes", "Seins", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Beschleunige mein Ende, sto\u00dfe mir", "tokens": ["Be\u00b7schleu\u00b7ni\u00b7ge", "mein", "En\u00b7de", ",", "sto\u00b7\u00dfe", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dein Eisen in die Brust;", "tokens": ["Dein", "Ei\u00b7sen", "in", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann mit des Herzens fl\u00fcssigem Gebl\u00fct", "tokens": ["Dann", "mit", "des", "Her\u00b7zens", "fl\u00fcs\u00b7si\u00b7gem", "Ge\u00b7bl\u00fct"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Durchn\u00e4sse dir ein Tuch,", "tokens": ["Durch\u00b7n\u00e4s\u00b7se", "dir", "ein", "Tuch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Durch s\u00e4mmtliche Bezirke trag' es um,", "tokens": ["Durch", "s\u00e4mmt\u00b7li\u00b7che", "Be\u00b7zir\u00b7ke", "trag'", "es", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Durch alle D\u00f6rfer rings,", "tokens": ["Durch", "al\u00b7le", "D\u00f6r\u00b7fer", "rings", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und zeig' es im Triumphe jedem Aug',", "tokens": ["Und", "zeig'", "es", "im", "Tri\u00b7um\u00b7phe", "je\u00b7dem", "Aug'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.12": {"text": "Und staunt man an, so sprich:", "tokens": ["Und", "staunt", "man", "an", ",", "so", "sprich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "\u00bbseht das Gebl\u00fcte jener \u00c4rmsten hier,", "tokens": ["\u00bb", "seht", "das", "Ge\u00b7bl\u00fc\u00b7te", "je\u00b7ner", "\u00c4rms\u00b7ten", "hier", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "PDAT", "NN", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Die mir ein \u00dcberma\u00df", "tokens": ["Die", "mir", "ein", "\u00dc\u00b7berm\u00b7a\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Von Huld geweiht und welcher ich daf\u00fcr", "tokens": ["Von", "Huld", "ge\u00b7weiht", "und", "wel\u00b7cher", "ich", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVPP", "KON", "PWS", "PPER", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Den Sto\u00df in's Herze gab!\u00ab ", "tokens": ["Den", "Sto\u00df", "in's", "Her\u00b7ze", "gab", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}