{"dta.poem.2599": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Leichen-Gedicht zum r\u00fchmlichen Andencken/  \n  Fr. M. v. H. g. A. 1662. den 10. Decembr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje kan die Tugend ihr auf dieser Unter-Welt/", "tokens": ["Wie", "kan", "die", "Tu\u00b7gend", "ihr", "auf", "die\u00b7ser", "Un\u00b7ter\u00b7Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PPOSAT", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ewig Wonhaus bau\u2019n/ und schn\u00f6den Augen dienen?", "tokens": ["Ein", "e\u00b7wig", "Won\u00b7haus", "bau'n", "/", "und", "schn\u00f6\u00b7den", "Au\u00b7gen", "die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$(", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn sie in ihrem Licht vollkommen ist erschienen/", "tokens": ["Wenn", "sie", "in", "ih\u00b7rem", "Licht", "voll\u00b7kom\u00b7men", "ist", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So zeucht sie von uns ab/ damit ihr nicht entf\u00e4llt", "tokens": ["So", "zeucht", "sie", "von", "uns", "ab", "/", "da\u00b7mit", "ihr", "nicht", "ent\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$(", "KOUS", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der angebohrne Glantz/ weil unser Fehl\u2019 und Flecken/", "tokens": ["Der", "an\u00b7ge\u00b7bohr\u00b7ne", "Glantz", "/", "weil", "un\u00b7ser", "Fehl'", "und", "Fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die h\u00f6chste Reinligkeit mit Lastergift anstecken.", "tokens": ["Die", "h\u00f6chs\u00b7te", "Rein\u00b7lig\u00b7keit", "mit", "Las\u00b7ter\u00b7gift", "an\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Je heller sich die Flamm\u2019 im ersten Aufgang zeigt/", "tokens": ["Je", "hel\u00b7ler", "sich", "die", "Flamm'", "im", "ers\u00b7ten", "Auf\u00b7gang", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Je schneller f\u00e4hrt der Schein nach den gestirnten H\u00f6hen;", "tokens": ["Je", "schnel\u00b7ler", "f\u00e4hrt", "der", "Schein", "nach", "den", "ge\u00b7stirn\u00b7ten", "H\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So eben ist die Seel\u2019/ sie l\u00e4st di\u00df Wesen stehen/", "tokens": ["So", "e\u00b7ben", "ist", "die", "Seel'", "/", "sie", "l\u00e4st", "di\u00df", "We\u00b7sen", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$(", "PPER", "VVFIN", "PDS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das irrdisch ist und heist/ ihr Tugend-Feuer steigt", "tokens": ["Das", "irr\u00b7disch", "ist", "und", "heist", "/", "ihr", "Tu\u00b7gen\u00b7dFeu\u00b7er", "steigt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJD", "VAFIN", "KON", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weit \u00fcber Wolck\u2019 und Luft/ verl\u00e4st des C\u00f6rpers-Bande/", "tokens": ["Weit", "\u00fc\u00b7ber", "Wolck'", "und", "Luft", "/", "ver\u00b7l\u00e4st", "des", "C\u00f6\u00b7rper\u00b7sBan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "KON", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Geht seinem Ursprung nach zum wahren Vater-Lande.", "tokens": ["Geht", "sei\u00b7nem", "Ur\u00b7sprung", "nach", "zum", "wah\u00b7ren", "Va\u00b7ter\u00b7Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ob zwar ein sterblich Leib der Seelen Wohnhaus bleibt/", "tokens": ["Ob", "zwar", "ein", "sterb\u00b7lich", "Leib", "der", "See\u00b7len", "Wohn\u00b7haus", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und sie als K\u00f6nigin das Leben \u00fcbermeistert/", "tokens": ["Und", "sie", "als", "K\u00f6\u00b7ni\u00b7gin", "das", "Le\u00b7ben", "\u00fc\u00b7ber\u00b7meis\u00b7tert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So ist doch Fleisch und Blut mit der Gewalt begeistert/", "tokens": ["So", "ist", "doch", "Fleisch", "und", "Blut", "mit", "der", "Ge\u00b7walt", "be\u00b7geis\u00b7tert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "KON", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Anfruhr gegen ihr fast alle Stunden treibt.", "tokens": ["Die", "An\u00b7fruhr", "ge\u00b7gen", "ihr", "fast", "al\u00b7le", "Stun\u00b7den", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und soll die Hoheit nicht in tiefsten S\u00fcnden schmachten/", "tokens": ["Und", "soll", "die", "Ho\u00b7heit", "nicht", "in", "tiefs\u00b7ten", "S\u00fcn\u00b7den", "schmach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So mu\u00df sie/ Himmlische/ nur nach dem Himmel trachten.", "tokens": ["So", "mu\u00df", "sie", "/", "Himm\u00b7li\u00b7sche", "/", "nur", "nach", "dem", "Him\u00b7mel", "trach\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$(", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie sieht wol/ wie die Welt sucht Finstern\u00fc\u00df und Nacht/", "tokens": ["Sie", "sieht", "wol", "/", "wie", "die", "Welt", "sucht", "Fins\u00b7ter\u00b7n\u00fc\u00df", "und", "Nacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOKOM", "ART", "NN", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie sie ihr Hertz verstellt den Nechsten zubetriegen/", "tokens": ["Wie", "sie", "ihr", "Hertz", "ver\u00b7stellt", "den", "Nechs\u00b7ten", "zu\u00b7be\u00b7trie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie sich der Mund befleist der Tugend vorzuliegen/", "tokens": ["Wie", "sich", "der", "Mund", "be\u00b7fleist", "der", "Tu\u00b7gend", "vor\u00b7zu\u00b7lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So unter Schimpf und Schand an Bettelstab gebracht/", "tokens": ["So", "un\u00b7ter", "Schimpf", "und", "Schand", "an", "Bet\u00b7tel\u00b7stab", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie in der Augen-Glantz die Freundligkeiten spielen/", "tokens": ["Wie", "in", "der", "Au\u00b7gen\u00b7Glantz", "die", "Freund\u00b7lig\u00b7kei\u00b7ten", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie in dem Busen Ha\u00df/ Zorn/ Rach\u2019 und Meineid w\u00fchlen.", "tokens": ["Wie", "in", "dem", "Bu\u00b7sen", "Ha\u00df", "/", "Zorn", "/", "Rach'", "und", "Mei\u00b7neid", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sie jammerts/ da\u00df der Mensch bey solcher Nichtigkeit", "tokens": ["Sie", "jam\u00b7merts", "/", "da\u00df", "der", "Mensch", "bey", "sol\u00b7cher", "Nich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$(", "KOUS", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sich so vergn\u00fcget sch\u00e4tzt/ sie klagt da\u00df unser Leben/", "tokens": ["Sich", "so", "ver\u00b7gn\u00fc\u00b7get", "sch\u00e4tzt", "/", "sie", "klagt", "da\u00df", "un\u00b7ser", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "VVFIN", "$(", "PPER", "VVFIN", "KOUS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Begierden unterthan/ und Wollust \u00fcbergeben", "tokens": ["Be\u00b7gier\u00b7den", "un\u00b7ter\u00b7than", "/", "und", "Wol\u00b7lust", "\u00fc\u00b7ber\u00b7ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$(", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ja da\u00df wir selbsten seyn ein Spiel und Traum der Zeit/", "tokens": ["Ja", "da\u00df", "wir", "selbs\u00b7ten", "seyn", "ein", "Spiel", "und", "Traum", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "VVFIN", "PPOSAT", "ART", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das Leben ein Magnet/ der uns zur Bahre ziehet/", "tokens": ["Das", "Le\u00b7ben", "ein", "Mag\u00b7net", "/", "der", "uns", "zur", "Bah\u00b7re", "zie\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein blasser Wermuthstrauch/ der auf dem Kirchhof bl\u00fchet.", "tokens": ["Ein", "blas\u00b7ser", "Wer\u00b7muths\u00b7trauch", "/", "der", "auf", "dem", "Kirch\u00b7hof", "bl\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein Alabaster-Grab mit Kohlen au\u00dfgesetzt/", "tokens": ["Ein", "A\u00b7la\u00b7bas\u00b7ter\u00b7Grab", "mit", "Koh\u00b7len", "au\u00df\u00b7ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ein Gold-geflochtner Strick/ der Jahr\u2019 und Freyheit bindet/", "tokens": ["Ein", "Gold\u00b7ge\u00b7flocht\u00b7ner", "Strick", "/", "der", "Jahr'", "und", "Frey\u00b7heit", "bin\u00b7det", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein Molch/ den man verdeckt in Ros\u2019 und Lilgen findet.", "tokens": ["Ein", "Molch", "/", "den", "man", "ver\u00b7deckt", "in", "Ros'", "und", "Lil\u00b7gen", "fin\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIS", "VVFIN", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ein Spiegel/ dessen Schein uns auf den Tod verletzt/", "tokens": ["Ein", "Spie\u00b7gel", "/", "des\u00b7sen", "Schein", "uns", "auf", "den", "Tod", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELAT", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein Feur/ das unser Blut zu kalter Aschen brennet/", "tokens": ["Ein", "Feur", "/", "das", "un\u00b7ser", "Blut", "zu", "kal\u00b7ter", "A\u00b7schen", "bren\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ein Bild/ das man bey Nacht und nicht bey Tage kennet.", "tokens": ["Ein", "Bild", "/", "das", "man", "bey", "Nacht", "und", "nicht", "bey", "Ta\u00b7ge", "ken\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PIS", "APPR", "NN", "KON", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein Mahler/ der nur sich und keinen sonsten trift/", "tokens": ["Ein", "Mah\u00b7ler", "/", "der", "nur", "sich", "und", "kei\u00b7nen", "sons\u00b7ten", "trift", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "PRF", "KON", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein Aethna/ der mit Schnee von aussen \u00fcberdecket/", "tokens": ["Ein", "A\u00b7e\u00b7th\u00b7na", "/", "der", "mit", "Schnee", "von", "aus\u00b7sen", "\u00fc\u00b7berd\u00b7e\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "ART", "APPR", "NN", "APPR", "NE", "VVFIN", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.39": {"text": "Ein brennender Vesuv/ der doch Krystallen hecket/", "tokens": ["Ein", "bren\u00b7nen\u00b7der", "Ve\u00b7suv", "/", "der", "doch", "Krys\u00b7tal\u00b7len", "he\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein Thau/ der seine Perl\u2019 im Essig beitzt und schliefft/", "tokens": ["Ein", "Thau", "/", "der", "sei\u00b7ne", "Perl'", "im", "Es\u00b7sig", "beitzt", "und", "schliefft", "/"], "token_info": ["word", "w