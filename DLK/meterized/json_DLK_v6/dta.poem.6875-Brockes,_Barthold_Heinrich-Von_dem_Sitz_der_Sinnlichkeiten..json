{"dta.poem.6875": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Daher Erscheinungen und Larven denn entstehn,", "tokens": ["Da\u00b7her", "Er\u00b7schei\u00b7nun\u00b7gen", "und", "Lar\u00b7ven", "denn", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die tief bis in die Seele gehn;", "tokens": ["Die", "tief", "bis", "in", "die", "See\u00b7le", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf solche Art mu\u00df sich Orestes immerfort,", "tokens": ["Auf", "sol\u00b7che", "Art", "mu\u00df", "sich", "O\u00b7res\u00b7tes", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PRF", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Furien verfolgt, an jedem Ort,", "tokens": ["Von", "Fu\u00b7ri\u00b7en", "ver\u00b7folgt", ",", "an", "je\u00b7dem", "Ort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Schreck-Gespenstern foltern sehn,", "tokens": ["Von", "Schreck\u00b7Ge\u00b7spens\u00b7tern", "fol\u00b7tern", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie dessen scharffe Plag und Klag auf unsren Scenen,", "tokens": ["Wie", "des\u00b7sen", "scharf\u00b7fe", "Plag", "und", "Klag", "auf", "un\u00b7sren", "Sce\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zuweilen noch erth\u00f6nen,", "tokens": ["Zu\u00b7wei\u00b7len", "noch", "er\u00b7th\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um wahres Schaudern, Furcht und Schrecken,", "tokens": ["Um", "wah\u00b7res", "Schau\u00b7dern", ",", "Furcht", "und", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auch aus verstellter Noht, uns zu erwecken.", "tokens": ["Auch", "aus", "ver\u00b7stell\u00b7ter", "Noht", ",", "uns", "zu", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}