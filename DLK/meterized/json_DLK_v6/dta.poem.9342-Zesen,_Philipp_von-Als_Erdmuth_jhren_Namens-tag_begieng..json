{"dta.poem.9342": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Als Erdmuth jhren Namens-tag  \n begieng.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Onymfe/ der ich di\u00df zu ehren singe/", "tokens": ["O\u00b7nym\u00b7fe", "/", "der", "ich", "di\u00df", "zu", "eh\u00b7ren", "sin\u00b7ge", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "PDS", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Erdmuth/ schicke dich zur fr\u00f6ligkeit/", "tokens": ["O", "Erd\u00b7muth", "/", "schi\u00b7cke", "dich", "zur", "fr\u00f6\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "VVFIN", "PRF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und deinen s\u00fc\u00dfen toon zugleich erschwinge/", "tokens": ["und", "dei\u00b7nen", "s\u00fc\u00b7\u00dfen", "toon", "zu\u00b7gleich", "er\u00b7schwin\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zu loben deinen Gott in dieser zeit.", "tokens": ["zu", "lo\u00b7ben", "dei\u00b7nen", "Gott", "in", "die\u00b7ser", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "La\u00df deine Nadel stehn und faden liegen/", "tokens": ["La\u00df", "dei\u00b7ne", "Na\u00b7del", "stehn", "und", "fa\u00b7den", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "KON", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "weil jtzo bricht herf\u00fcr dein Nahmens-licht/", "tokens": ["weil", "jt\u00b7zo", "bricht", "her\u00b7f\u00fcr", "dein", "Nah\u00b7mens\u00b7licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich seh schon kommen an mit tieffen b\u00fcgen/", "tokens": ["Ich", "seh", "schon", "kom\u00b7men", "an", "mit", "tief\u00b7fen", "b\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die Diener deines Herrn/ der Dier verpflicht.", "tokens": ["die", "Die\u00b7ner", "dei\u00b7nes", "Herrn", "/", "der", "Dier", "ver\u00b7pflicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was tragen sie doch nur vor sch\u00f6ne sachen?", "tokens": ["Was", "tra\u00b7gen", "sie", "doch", "nur", "vor", "sch\u00f6\u00b7ne", "sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ist das f\u00fcr ein wundich in g\u00fcldner schrifft?", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "wun\u00b7dich", "in", "g\u00fcld\u00b7ner", "schrifft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "ADJD", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich der ich dieses seh/ wa\u00df soll ich machen/", "tokens": ["Ich", "der", "ich", "die\u00b7ses", "seh", "/", "wa\u00df", "soll", "ich", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "PDS", "VVFIN", "$(", "PWAV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "weil mich jtzund nun auch die ordnung trifft?", "tokens": ["weil", "mich", "jt\u00b7zund", "nun", "auch", "die", "ord\u00b7nung", "trifft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur einen blo\u00dfen wundsch will ich dier geben/", "tokens": ["Nur", "ei\u00b7nen", "blo\u00b7\u00dfen", "wund\u00b7sch", "will", "ich", "dier", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "PPER", "PDS", "VVINF", "$("], "meter": "-+-+-+++-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "und was es mehr wird seyn das ich verehr/", "tokens": ["und", "was", "es", "mehr", "wird", "seyn", "das", "ich", "ver\u00b7ehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VAFIN", "PPOSAT", "PRELS", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Himmel gebe dier ein langes Leben/", "tokens": ["der", "Him\u00b7mel", "ge\u00b7be", "dier", "ein", "lan\u00b7ges", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "damit ich anderwerts den wundsch vermehr.", "tokens": ["da\u00b7mit", "ich", "an\u00b7der\u00b7werts", "den", "wund\u00b7sch", "ver\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Also bistu nun auch von mier gebunden/", "tokens": ["Al\u00b7so", "bis\u00b7tu", "nun", "auch", "von", "mier", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "du Tugendhafftes Bild zu dieser zeit;", "tokens": ["du", "Tu\u00b7gend\u00b7haff\u00b7tes", "Bild", "zu", "die\u00b7ser", "zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das band das deinen Arm und hand \u00fcmbwunden/", "tokens": ["Das", "band", "das", "dei\u00b7nen", "Arm", "und", "hand", "\u00fcm\u00b7bwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "PPOSAT", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "erfodert traubenbluth/ das dich befreyt.", "tokens": ["er\u00b7fo\u00b7dert", "trau\u00b7ben\u00b7bluth", "/", "das", "dich", "be\u00b7freyt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}