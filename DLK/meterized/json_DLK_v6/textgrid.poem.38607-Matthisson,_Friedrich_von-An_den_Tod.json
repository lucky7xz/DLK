{"textgrid.poem.38607": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Tod", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wonne mir, o Tod! Als Furchtgerippe", "tokens": ["Won\u00b7ne", "mir", ",", "o", "Tod", "!", "Als", "Furcht\u00b7ge\u00b7rip\u00b7pe"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "$,", "FM", "NN", "$.", "KOUS", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schaut' ich selbst im Kindheitstraum dich nie,", "tokens": ["Schaut'", "ich", "selbst", "im", "Kind\u00b7heits\u00b7traum", "dich", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in Palmen barg sich mir die Hippe,", "tokens": ["Und", "in", "Pal\u00b7men", "barg", "sich", "mir", "die", "Hip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PRF", "PPER", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Welche finstrer P\u00f6belwahn dir lieh!", "tokens": ["Wel\u00b7che", "finst\u00b7rer", "P\u00f6\u00b7bel\u00b7wahn", "dir", "lieh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Immer hat mit hohen G\u00f6ttermienen,", "tokens": ["Im\u00b7mer", "hat", "mit", "ho\u00b7hen", "G\u00f6t\u00b7ter\u00b7mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herrlich von der Hoffnung Licht umstralt,", "tokens": ["Herr\u00b7lich", "von", "der", "Hoff\u00b7nung", "Licht", "um\u00b7stralt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie dem Sokrates du einst erschienen,", "tokens": ["Wie", "dem", "Sok\u00b7ra\u00b7tes", "du", "einst", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mir die Phantasie dein Bild gemalt.", "tokens": ["Mir", "die", "Phan\u00b7ta\u00b7sie", "dein", "Bild", "ge\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Immer hat, auf dunkler Lebenswelle,", "tokens": ["Im\u00b7mer", "hat", ",", "auf", "dunk\u00b7ler", "Le\u00b7bens\u00b7wel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch des Mi\u00dfgeschicks entsternte Nacht,", "tokens": ["Durch", "des", "Mi\u00df\u00b7ge\u00b7schicks", "ents\u00b7tern\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Gleich der Tyndariden Silberhelle,", "tokens": ["Gleich", "der", "Tyn\u00b7da\u00b7ri\u00b7den", "Sil\u00b7ber\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Leitend mir dies holde Bild gelacht.", "tokens": ["Lei\u00b7tend", "mir", "dies", "hol\u00b7de", "Bild", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PDS", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Deine Bucht am Abendhorizonte,", "tokens": ["Dei\u00b7ne", "Bucht", "am", "A\u00b7bend\u00b7ho\u00b7ri\u00b7zon\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du, der sich mit Immortellen kr\u00e4nzt,", "tokens": ["Du", ",", "der", "sich", "mit", "Im\u00b7mor\u00b7tel\u00b7len", "kr\u00e4nzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gl\u00e4nzt mir, wie das freundlich \u00fcbersonnte", "tokens": ["Gl\u00e4nzt", "mir", ",", "wie", "das", "freund\u00b7lich", "\u00fc\u00b7ber\u00b7sonn\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PDS", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zufluchtseiland m\u00fcden Schiffern gl\u00e4nzt!", "tokens": ["Zu\u00b7flucht\u00b7sei\u00b7land", "m\u00fc\u00b7den", "Schif\u00b7fern", "gl\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}