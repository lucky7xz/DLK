{"dta.poem.23657": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schiller in Lauchst\u00e4dt 1804.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das hat mir selber erz\u00e4hlt Einer,", "tokens": ["Das", "hat", "mir", "sel\u00b7ber", "er\u00b7z\u00e4hlt", "Ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Damit Schillers werde gedacht;", "tokens": ["Da\u00b7mit", "Schil\u00b7lers", "wer\u00b7de", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "VAFIN", "VVPP", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wir gedenken Schillers und seiner", "tokens": ["Wir", "ge\u00b7den\u00b7ken", "Schil\u00b7lers", "und", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "KON", "PPOSAT"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als h\u00e4tten wir es mitgemacht.", "tokens": ["Als", "h\u00e4t\u00b7ten", "wir", "es", "mit\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}