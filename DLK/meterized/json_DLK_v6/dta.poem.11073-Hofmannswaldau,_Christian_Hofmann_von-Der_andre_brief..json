 "line.93": {"text": "Mein niedrig auge sieht auch nicht nach hohen dingen,", "tokens": ["Mein", "nied\u00b7rig", "au\u00b7ge", "sieht", "auch", "nicht", "nach", "ho\u00b7hen", "din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "Ich buhl\u2019 um kein gepr\u00e4ng, auch f\u00fcrsten-titul nicht.", "tokens": ["Ich", "buhl'", "um", "kein", "ge\u00b7pr\u00e4ng", ",", "auch", "f\u00fcrs\u00b7ten\u00b7ti\u00b7tul", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "ADV", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "Kan ich dir, holder f\u00fcrst! nur stets vergn\u00fcgung bringen,", "tokens": ["Kan", "ich", "dir", ",", "hol\u00b7der", "f\u00fcrst", "!", "nur", "stets", "ver\u00b7gn\u00fc\u00b7gung", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$.", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.96": {"text": "Was acht ichs, wenn der neid mich gleich als magd ansticht.", "tokens": ["Was", "acht", "ichs", ",", "wenn", "der", "neid", "mich", "gleich", "als", "magd", "an\u00b7sticht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "PIS", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.97": {"text": "Doch dieses b\u00fcndnis darf kein ander siegel schliessen,", "tokens": ["Doch", "die\u00b7ses", "b\u00fcnd\u00b7nis", "darf", "kein", "an\u00b7der", "sie\u00b7gel", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PIAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.98": {"text": "Als unverschrencktes recht, und eines priesters band,", "tokens": ["Als", "un\u00b7ver\u00b7schrenck\u00b7tes", "recht", ",", "und", "ei\u00b7nes", "pries\u00b7ters", "band", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.99": {"text": "Die einfalt folget hier, er wird, obs recht sey, wissen,", "tokens": ["Die", "ein\u00b7falt", "fol\u00b7get", "hier", ",", "er", "wird", ",", "obs", "recht", "sey", ",", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "$,", "KOUS", "ADJD", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.100": {"text": "Da\u00df er die andre frau vertraut der lincken hand.", "tokens": ["Da\u00df", "er", "die", "and\u00b7re", "frau", "ver\u00b7traut", "der", "lin\u00b7cken", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.101": {"text": "Ich selbst bin l\u00fcstern nun nach der verm\u00e4hlungs-kette,", "tokens": ["Ich", "selbst", "bin", "l\u00fcs\u00b7tern", "nun", "nach", "der", "ver\u00b7m\u00e4h\u00b7lungs\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.102": {"text": "Und folge, wenn er winckt, ihm zu dem priester nach.", "tokens": ["Und", "fol\u00b7ge", ",", "wenn", "er", "winckt", ",", "ihm", "zu", "dem", "pries\u00b7ter", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.103": {"text": "Denn vom altare gehn nur stuffen in mein bette,", "tokens": ["Denn", "vom", "al\u00b7ta\u00b7re", "gehn", "nur", "stuf\u00b7fen", "in", "mein", "bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "VVFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.104": {"text": "Und durch die kirche kommt man in mein schlaf-gemach.", "tokens": ["Und", "durch", "die", "kir\u00b7che", "kommt", "man", "in", "mein", "schlaf\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIS", "APPR", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}