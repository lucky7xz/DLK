{"dta.poem.11073": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der andre brief.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe seid und brief, durchlauchster! aufgerissen;", "tokens": ["Ich", "ha\u00b7be", "seid", "und", "brief", ",", "durch\u00b7lauchs\u00b7ter", "!", "auf\u00b7ge\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VAFIN", "KON", "VVFIN", "$,", "ADJD", "$.", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bek\u00fcmmert, ob nicht auch den faden meiner ruh?", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", ",", "ob", "nicht", "auch", "den", "fa\u00b7den", "mei\u00b7ner", "ruh", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PTKNEG", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich habe seine schrifft bewillkommt mit viel k\u00fcssen,", "tokens": ["Ich", "ha\u00b7be", "sei\u00b7ne", "schrifft", "be\u00b7will\u00b7kommt", "mit", "viel", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach da\u00df der himmel nicht gall in den zucker thu!", "tokens": ["Ach", "da\u00df", "der", "him\u00b7mel", "nicht", "gall", "in", "den", "zu\u00b7cker", "thu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er und die hoffnung speist mich ja mit himmel-brodte,", "tokens": ["Er", "und", "die", "hoff\u00b7nung", "speist", "mich", "ja", "mit", "him\u00b7mel\u00b7brod\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der zweifel und die furcht mischt aber myrrhen ein.", "tokens": ["Der", "zwei\u00b7fel", "und", "die", "furcht", "mischt", "a\u00b7ber", "myr\u00b7rhen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich wei\u00df, der f\u00fcrsten wort und bitten sind gebote;", "tokens": ["Ich", "wei\u00df", ",", "der", "f\u00fcrs\u00b7ten", "wort", "und", "bit\u00b7ten", "sind", "ge\u00b7bo\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "KON", "VVINF", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch reu und unlust folgt offt aufs gehorsam-seyn.", "tokens": ["Doch", "reu", "und", "un\u00b7lust", "folgt", "offt", "aufs", "ge\u00b7hor\u00b7sam\u00b7seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der eckel ha\u00dft hernach, was er erst angebetet,", "tokens": ["Der", "ec\u00b7kel", "ha\u00dft", "her\u00b7nach", ",", "was", "er", "erst", "an\u00b7ge\u00b7be\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein erst gek\u00fcster mund wird bald gegeifert an,", "tokens": ["Ein", "erst", "ge\u00b7k\u00fcs\u00b7ter", "mund", "wird", "bald", "ge\u00b7gei\u00b7fert", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was rosen gleich gebl\u00fcht, wie unkraut ausgejethet,", "tokens": ["Was", "ro\u00b7sen", "gleich", "ge\u00b7bl\u00fcht", ",", "wie", "un\u00b7kraut", "aus\u00b7ge\u00b7je\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "VVPP", "$,", "PWAV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil doch die liebe nicht den wechsel hassen kan.", "tokens": ["Weil", "doch", "die", "lie\u00b7be", "nicht", "den", "wech\u00b7sel", "has\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "VVFIN", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir frauenzimmer sind granaten-\u00e4pffeln gleiche,", "tokens": ["Wir", "frau\u00b7en\u00b7zim\u00b7mer", "sind", "gra\u00b7na\u00b7ten\u00b7\u00e4pf\u00b7feln", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die man von porzellan wirfft endlich auf den mist;", "tokens": ["Die", "man", "von", "por\u00b7zel\u00b7lan", "wirfft", "end\u00b7lich", "auf", "den", "mist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die heut ein engel war, heist morgen aa\u00df und leiche,", "tokens": ["Die", "heut", "ein", "en\u00b7gel", "war", ",", "heist", "mor\u00b7gen", "aa\u00df", "und", "lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "$,", "VAFIN", "ADV", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Weil sarg und braut-bett offt zwey zoll entfernt kaum ist.", "tokens": ["Weil", "sarg", "und", "braut\u00b7bett", "offt", "zwey", "zoll", "ent\u00b7fernt", "kaum", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "ADV", "CARD", "ADJD", "ADJD", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "H\u00e4tt\u2019 ich auch gleich hiervor nicht, grosser f\u00fcrst, zu sorgen,", "tokens": ["H\u00e4tt'", "ich", "auch", "gleich", "hier\u00b7vor", "nicht", ",", "gros\u00b7ser", "f\u00fcrst", ",", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PAV", "PTKNEG", "$,", "ADJD", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil tugend mir von ihm sagt etwas bessers wahr;", "tokens": ["Weil", "tu\u00b7gend", "mir", "von", "ihm", "sagt", "et\u00b7was", "bes\u00b7sers", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Denn was sie heute liebt, ha\u00dft sie gewi\u00df nicht morgen,", "tokens": ["Denn", "was", "sie", "heu\u00b7te", "liebt", ",", "ha\u00dft", "sie", "ge\u00b7wi\u00df", "nicht", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zumahl wenn liebe wird geanckert ans altar.", "tokens": ["Zu\u00b7mahl", "wenn", "lie\u00b7be", "wird", "ge\u00b7an\u00b7ckert", "ans", "al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJA", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mu\u00df sich doch schlechtes wachs nicht n\u00e4hern solchen sonnen,", "tokens": ["Mu\u00df", "sich", "doch", "schlech\u00b7tes", "wachs", "nicht", "n\u00e4\u00b7hern", "sol\u00b7chen", "son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ADJA", "NN", "PTKNEG", "VVFIN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sonst schmeltzt ihr gunst-strahl auch die k\u00fchnen fl\u00fcgel ab.", "tokens": ["Sonst", "schmeltzt", "ihr", "gunst\u00b7strahl", "auch", "die", "k\u00fch\u00b7nen", "fl\u00fc\u00b7gel", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.23": {"text": "Der liebes-zucker ist in thr\u00e4nen-saltz zerronnen,", "tokens": ["Der", "lie\u00b7bes\u00b7zu\u00b7cker", "ist", "in", "thr\u00e4\u00b7nen\u00b7saltz", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So offt ein irrdisch mund den g\u00f6ttern k\u00fcsse gab.", "tokens": ["So", "offt", "ein", "irr\u00b7disch", "mund", "den", "g\u00f6t\u00b7tern", "k\u00fcs\u00b7se", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJD", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was kan ein zufall nicht, nicht fremde mi\u00dfgunst stifften,", "tokens": ["Was", "kan", "ein", "zu\u00b7fall", "nicht", ",", "nicht", "frem\u00b7de", "mi\u00df\u00b7gunst", "stiff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PTKNEG", "$,", "PTKNEG", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil gantze sonnen ja ein w\u00f6lcklein rauben kan?", "tokens": ["Weil", "gant\u00b7ze", "son\u00b7nen", "ja", "ein", "w\u00f6lck\u00b7lein", "rau\u00b7ben", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was kan der speichel nicht der eyfersucht vergifften?", "tokens": ["Was", "kan", "der", "spei\u00b7chel", "nicht", "der", "ey\u00b7fer\u00b7sucht", "ver\u00b7giff\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NE", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie hengt den tugenden die schlimmsten kletten an,", "tokens": ["Sie", "hengt", "den", "tu\u00b7gen\u00b7den", "die", "schlimms\u00b7ten", "klet\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie schw\u00e4rtzt mit h\u00fctten-rauch die himmel-reinen flammen,", "tokens": ["Sie", "schw\u00e4rtzt", "mit", "h\u00fct\u00b7ten\u00b7rauch", "die", "him\u00b7mel\u00b7rei\u00b7nen", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sie wirfft mit schmach und koth der unschuld ebenbild.", "tokens": ["Sie", "wirfft", "mit", "schmach", "und", "koth", "der", "un\u00b7schuld", "e\u00b7ben\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "KON", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Drey wetter seh ich schon ziehn \u00fcber mir zusammen,", "tokens": ["Drey", "wet\u00b7ter", "seh", "ich", "schon", "ziehn", "\u00fc\u00b7ber", "mir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "VVINF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo liebe mehr bey mir, als kluge vorsicht, gilt.", "tokens": ["Wo", "lie\u00b7be", "mehr", "bey", "mir", ",", "als", "klu\u00b7ge", "vor\u00b7sicht", ",", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "PPER", "$,", "KOUS", "ADJA", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Behertzige, mein f\u00fcrst! wo man mich hin verleitet?", "tokens": ["Be\u00b7hert\u00b7zi\u00b7ge", ",", "mein", "f\u00fcrst", "!", "wo", "man", "mich", "hin", "ver\u00b7lei\u00b7tet", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "VVFIN", "$.", "PWAV", "PIS", "PRF", "APZR", "VVPP", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.34": {"text": "Ob mich versuchung nicht auf h\u00f6chste zinnen stellt?", "tokens": ["Ob", "mich", "ver\u00b7su\u00b7chung", "nicht", "auf", "h\u00f6chs\u00b7te", "zin\u00b7nen", "stellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der anmuth paradie\u00df wird mir ja zubereitet,", "tokens": ["Der", "an\u00b7muth", "pa\u00b7ra\u00b7die\u00df", "wird", "mir", "ja", "zu\u00b7be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Wo nur mein untergang nicht hintern berge h\u00e4lt.", "tokens": ["Wo", "nur", "mein", "un\u00b7ter\u00b7gang", "nicht", "hin\u00b7tern", "ber\u00b7ge", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er reicht den braut-krantz mir vielleicht zum schmuck der bahre,", "tokens": ["Er", "reicht", "den", "braut\u00b7krantz", "mir", "viel\u00b7leicht", "zum", "schmuck", "der", "bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "APPRART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wo noch mein schimpflich sarg wird werth der kr\u00e4ntze seyn:", "tokens": ["Wo", "noch", "mein", "schimpf\u00b7lich", "sarg", "wird", "werth", "der", "kr\u00e4nt\u00b7ze", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJD", "ADJD", "VAFIN", "ADJD", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Rubin und diamant soll bl\u00fchmen meine haare,", "tokens": ["Ru\u00b7bin", "und", "di\u00b7a\u00b7mant", "soll", "bl\u00fch\u00b7men", "mei\u00b7ne", "haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ach! dr\u00fcckten sie mir nur nicht gar den scheitel ein!", "tokens": ["Ach", "!", "dr\u00fcck\u00b7ten", "sie", "mir", "nur", "nicht", "gar", "den", "schei\u00b7tel", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Jedoch ich will mein heil aufs f\u00fcrsten worte gr\u00fcnden,", "tokens": ["Je\u00b7doch", "ich", "will", "mein", "heil", "aufs", "f\u00fcrs\u00b7ten", "wor\u00b7te", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPOSAT", "ADJD", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da wird kein fallbret seyn, wo er mich anckern hei\u00dft,", "tokens": ["Da", "wird", "kein", "fall\u00b7bret", "seyn", ",", "wo", "er", "mich", "an\u00b7ckern", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "VVFIN", "VAINF", "$,", "PWAV", "PPER", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Des f\u00fcrsten blosses ja mu\u00df mehrern glauben finden,", "tokens": ["Des", "f\u00fcrs\u00b7ten", "blos\u00b7ses", "ja", "mu\u00df", "meh\u00b7rern", "glau\u00b7ben", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "ADV", "VMFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Als die betheurung, so mit vielen eyden glei\u00dft.", "tokens": ["Als", "die", "be\u00b7theu\u00b7rung", ",", "so", "mit", "vie\u00b7len", "ey\u00b7den", "glei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die ohnedi\u00df ist magd, und fleisch im busen tr\u00e4get,", "tokens": ["Die", "oh\u00b7ne\u00b7di\u00df", "ist", "magd", ",", "und", "fleisch", "im", "bu\u00b7sen", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "NN", "$,", "KON", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Kan solcher dienstbarkeit sich schwerlich machen frey;", "tokens": ["Kan", "sol\u00b7cher", "dienst\u00b7bar\u00b7keit", "sich", "schwer\u00b7lich", "ma\u00b7chen", "frey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "PRF", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Doch denck\u2019 er, da\u00df das nicht, wenn man ein reh erleget,", "tokens": ["Doch", "denck", "er", ",", "da\u00df", "das", "nicht", ",", "wenn", "man", "ein", "reh", "er\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "KOUS", "PDS", "PTKNEG", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ein m\u00e4gdgen bringt zu fall, ein meister-st\u00fccke sey;", "tokens": ["Ein", "m\u00e4gd\u00b7gen", "bringt", "zu", "fall", ",", "ein", "meis\u00b7ter\u00b7st\u00fc\u00b7cke", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Da\u00df reu und untreu ihn weit mehr als mich beflecken,", "tokens": ["Da\u00df", "reu", "und", "un\u00b7treu", "ihn", "weit", "mehr", "als", "mich", "be\u00b7fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "PPER", "ADJD", "PIAT", "KOKOM", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.50": {"text": "Denn finsterni\u00df verstellt nur sonnen, keinen stern.", "tokens": ["Denn", "fins\u00b7ter\u00b7ni\u00df", "ver\u00b7stellt", "nur", "son\u00b7nen", ",", "kei\u00b7nen", "stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "VVINF", "$,", "PIAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ich warte nun, was er, durchlauchster! wird vollstrecken,", "tokens": ["Ich", "war\u00b7te", "nun", ",", "was", "er", ",", "durch\u00b7lauchs\u00b7ter", "!", "wird", "voll\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "$,", "ADJD", "$.", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Mein hertze nennt ihn schatz, mein auge nennt ihn herrn.", "tokens": ["Mein", "hert\u00b7ze", "nennt", "ihn", "schatz", ",", "mein", "au\u00b7ge", "nennt", "ihn", "herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Mein zimmer stehet ihm mit meiner seelen offen,", "tokens": ["Mein", "zim\u00b7mer", "ste\u00b7het", "ihm", "mit", "mei\u00b7ner", "see\u00b7len", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Worein der liebe hand noch keinen schatz gelegt.", "tokens": ["Wo\u00b7rein", "der", "lie\u00b7be", "hand", "noch", "kei\u00b7nen", "schatz", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Die hei\u00dft itzt deine magd vielmehr vergn\u00fcgung hoffen,", "tokens": ["Die", "hei\u00dft", "itzt", "dei\u00b7ne", "magd", "viel\u00b7mehr", "ver\u00b7gn\u00fc\u00b7gung", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Als die den f\u00fcrsten-hut, doch ohne rosen, tr\u00e4gt.", "tokens": ["Als", "die", "den", "f\u00fcrs\u00b7ten\u00b7hut", ",", "doch", "oh\u00b7ne", "ro\u00b7sen", ",", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "$,", "ADV", "APPR", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Ich wei\u00df nicht, wie ich schon vor freuden schwanger werde,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "ich", "schon", "vor", "freu\u00b7den", "schwan\u00b7ger", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "APPR", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Die br\u00fcste h\u00fcpffen mir vor tausend lust empor,", "tokens": ["Die", "br\u00fcs\u00b7te", "h\u00fcpf\u00b7fen", "mir", "vor", "tau\u00b7send", "lust", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "APPR", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Die furcht verwandelt sich in spielende gebehrde,", "tokens": ["Die", "furcht", "ver\u00b7wan\u00b7delt", "sich", "in", "spie\u00b7len\u00b7de", "ge\u00b7behr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Der liebreitz reitzt mein aug\u2019, und anmuth lockt mein ohr.", "tokens": ["Der", "lieb\u00b7reitz", "reitzt", "mein", "aug'", ",", "und", "an\u00b7muth", "lockt", "mein", "ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Mich d\u00fcnckt, ich f\u00fchle schon, wie er mit tausend k\u00fcssen", "tokens": ["Mich", "d\u00fcnckt", ",", "ich", "f\u00fch\u00b7le", "schon", ",", "wie", "er", "mit", "tau\u00b7send", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "APPR", "CARD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Die scharlach-lippen labt auf meiner lilgen-brust,", "tokens": ["Die", "scha\u00b7rlach\u00b7\u00b7lip\u00b7pen", "labt", "auf", "mei\u00b7ner", "lil\u00b7gen\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Wie sein und meine seel wie wachs zusammen fliessen,", "tokens": ["Wie", "sein", "und", "mei\u00b7ne", "seel", "wie", "wachs", "zu\u00b7sam\u00b7men", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAINF", "KON", "PPOSAT", "NN", "KOKOM", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Wie er mich \u00fcberschwemmt mit einer see voll lust;", "tokens": ["Wie", "er", "mich", "\u00fc\u00b7bersc\u00b7hwemmt", "mit", "ei\u00b7ner", "see", "voll", "lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Wie sein rubinen-mund nach meinen \u00e4pffeln lechset,", "tokens": ["Wie", "sein", "ru\u00b7bi\u00b7nen\u00b7mund", "nach", "mei\u00b7nen", "\u00e4pf\u00b7feln", "lech\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Und als ein saugend kind an den granaten zeucht,", "tokens": ["Und", "als", "ein", "sau\u00b7gend", "kind", "an", "den", "gra\u00b7na\u00b7ten", "zeucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "NN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Weil kein solch honig doch in paradiesen w\u00e4chset,", "tokens": ["Weil", "kein", "solch", "ho\u00b7nig", "doch", "in", "pa\u00b7ra\u00b7die\u00b7sen", "w\u00e4ch\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PIAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Als den ein heisser ku\u00df an seel und lippen streicht.", "tokens": ["Als", "den", "ein", "heis\u00b7ser", "ku\u00df", "an", "seel", "und", "lip\u00b7pen", "streicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "ADJA", "NN", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Urtheile, grosser f\u00fcrst! wie weit ich mich vergangen,", "tokens": ["Ur\u00b7thei\u00b7le", ",", "gros\u00b7ser", "f\u00fcrst", "!", "wie", "weit", "ich", "mich", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PWAV", "ADJD", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Ob mir die liebe nicht bezaubert geist und sinn?", "tokens": ["Ob", "mir", "die", "lie\u00b7be", "nicht", "be\u00b7zau\u00b7bert", "geist", "und", "sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "PTKNEG", "VVFIN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Die furcht dringt auf mein hertz, die schamr\u00f6th auf die wangen,", "tokens": ["Die", "furcht", "dringt", "auf", "mein", "hertz", ",", "die", "scham\u00b7r\u00f6th", "auf", "die", "wan\u00b7g