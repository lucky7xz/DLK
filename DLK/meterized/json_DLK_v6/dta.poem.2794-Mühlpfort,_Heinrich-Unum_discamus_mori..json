{"dta.poem.2794": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unum discamus mori.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir die gantze Welt in unsern Kopff gefast/", "tokens": ["Wenn", "wir", "die", "gant\u00b7ze", "Welt", "in", "un\u00b7sern", "Kopff", "ge\u00b7fast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Himmels Lauff geseh\u2019n/ der Erden Ziel gemess\u1ebd/", "tokens": ["Des", "Him\u00b7mels", "Lauff", "ge\u00b7seh'n", "/", "der", "Er\u00b7den", "Ziel", "ge\u00b7mess\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$(", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey fr\u00fchem Morgen-Licht und auch bey Nacht ge-", "tokens": ["Bey", "fr\u00fc\u00b7hem", "Mor\u00b7gen\u00b7Licht", "und", "auch", "bey", "Nacht", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADV", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alles durchgesucht; so kommt ein fremder Gast/", "tokens": ["Und", "al\u00b7les", "durch\u00b7ge\u00b7sucht", ";", "so", "kommt", "ein", "frem\u00b7der", "Gast", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weist uns das Stunden-Gla\u00df und spricht: Mensch lerne sterb\u0113", "tokens": ["Weist", "uns", "das", "Stun\u00b7den\u00b7Gla\u00df", "und", "spricht", ":", "Mensch", "ler\u00b7ne", "ster\u00b7b\u0113"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "VVFIN", "$.", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Wo du nicht ewig wilst an Leib und Seel verderben.", "tokens": ["Wo", "du", "nicht", "e\u00b7wig", "wilst", "an", "Leib", "und", "Seel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADJD", "VMFIN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach Wunder-volle Kunst und unergr\u00fcndtes Werck!", "tokens": ["Ach", "Wun\u00b7der\u00b7vol\u00b7le", "Kunst", "und", "un\u00b7er\u00b7gr\u00fcnd\u00b7tes", "Werck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Wei\u00dfheit so zuvor ein gantzes Land geehret/", "tokens": ["Die", "Wei\u00df\u00b7heit", "so", "zu\u00b7vor", "ein", "gant\u00b7zes", "Land", "ge\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wird da zum Kinderspiel. Was Plato hat gelehret/", "tokens": ["Wird", "da", "zum", "Kin\u00b7der\u00b7spiel", ".", "Was", "Pla\u00b7to", "hat", "ge\u00b7leh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "$.", "PWS", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was Socrates gesagt/ und was der K\u00fcnste Berg", "tokens": ["Was", "So\u00b7cra\u00b7tes", "ge\u00b7sagt", "/", "und", "was", "der", "K\u00fcns\u00b7te", "Berg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "VVPP", "$(", "KON", "PWS", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Von Klugheit bey sich hat/ das wird allhier zum Thoren/", "tokens": ["Von", "Klug\u00b7heit", "bey", "sich", "hat", "/", "das", "wird", "all\u00b7hier", "zum", "Tho\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PRF", "VAFIN", "$(", "PDS", "VAFIN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer nicht recht sterben lernt/ ist ewiglich verlohren.", "tokens": ["Wer", "nicht", "recht", "ster\u00b7ben", "lernt", "/", "ist", "e\u00b7wig\u00b7lich", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADJD", "VVINF", "VVFIN", "$(", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und weil ich denn gewi\u00df/ da\u00df jede Stunde mich", "tokens": ["Und", "weil", "ich", "denn", "ge\u00b7wi\u00df", "/", "da\u00df", "je\u00b7de", "Stun\u00b7de", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "$(", "KOUS", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus diesem Leben rufft/ das Tag und Nacht bezeuget/", "tokens": ["Aus", "die\u00b7sem", "Le\u00b7ben", "rufft", "/", "das", "Tag", "und", "Nacht", "be\u00b7zeu\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie nah der arme Mensch zu seinem Grabe steiget/", "tokens": ["Wie", "nah", "der", "ar\u00b7me", "Mensch", "zu", "sei\u00b7nem", "Gra\u00b7be", "stei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So mach ich mich bereit/ und trachte br\u00fcnstiglich/", "tokens": ["So", "mach", "ich", "mich", "be\u00b7reit", "/", "und", "trach\u00b7te", "br\u00fcns\u00b7tig\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$(", "KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In dieser h\u00f6chsten Kunst nur dieses zu begreiffen/", "tokens": ["In", "die\u00b7ser", "h\u00f6chs\u00b7ten", "Kunst", "nur", "die\u00b7ses", "zu", "be\u00b7greif\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "PDS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie meine Seele mag in ", "tokens": ["Wie", "mei\u00b7ne", "See\u00b7le", "mag", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VMFIN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}