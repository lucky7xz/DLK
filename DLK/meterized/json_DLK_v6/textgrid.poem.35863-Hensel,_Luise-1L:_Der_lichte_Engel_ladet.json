{"textgrid.poem.35863": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der lichte Engel ladet", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der lichte Engel ladet", "tokens": ["Der", "lich\u00b7te", "En\u00b7gel", "la\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die frommen Hirten ein", "tokens": ["Die", "from\u00b7men", "Hir\u00b7ten", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Kripplein, hoch begnadet,", "tokens": ["Zum", "Krip\u00b7plein", ",", "hoch", "be\u00b7gna\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum s\u00fc\u00dfen Jesulein.", "tokens": ["Zum", "s\u00fc\u00b7\u00dfen", "Je\u00b7su\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich will mit ihnen gr\u00fc\u00dfen", "tokens": ["Ich", "will", "mit", "ih\u00b7nen", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das wunderbare Kind", "tokens": ["Das", "wun\u00b7der\u00b7ba\u00b7re", "Kind"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und legen ihm zu F\u00fc\u00dfen", "tokens": ["Und", "le\u00b7gen", "ihm", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz zum Angebind'.", "tokens": ["Mein", "Herz", "zum", "An\u00b7ge\u00b7bind'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ist es auch von S\u00fcnde", "tokens": ["Und", "ist", "es", "auch", "von", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und manchem Leid gedr\u00fcckt:", "tokens": ["Und", "man\u00b7chem", "Leid", "ge\u00b7dr\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sanct Joseph giebt's dem Kinde", "tokens": ["Sanct", "Jo\u00b7se\u00b7ph", "giebt's", "dem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Gereinigt und geschm\u00fcckt.", "tokens": ["Ge\u00b7rei\u00b7nigt", "und", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Zuflucht kranker Herzen,", "tokens": ["Die", "Zu\u00b7flucht", "kran\u00b7ker", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Maria, schaut so lind;", "tokens": ["Ma\u00b7ria", ",", "schaut", "so", "lind", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die bannt von mir die Schmerzen", "tokens": ["Die", "bannt", "von", "mir", "die", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und s\u00fchnt mich ihrem Kind.", "tokens": ["Und", "s\u00fchnt", "mich", "ih\u00b7rem", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie mag wohl Freude weinen", "tokens": ["Sie", "mag", "wohl", "Freu\u00b7de", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob ihrem Kindlein hold:", "tokens": ["Ob", "ih\u00b7rem", "Kin\u00b7dlein", "hold", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Aeuglein Sterne scheinen,", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7glein", "Ster\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die L\u00f6cklein Sonnengold. \u2013", "tokens": ["Die", "L\u00f6c\u00b7klein", "Son\u00b7nen\u00b7gold", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Magst wohl in Wonne beben,", "tokens": ["Magst", "wohl", "in", "Won\u00b7ne", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Jungfrau-M\u00fctterlein:", "tokens": ["Du", "Jung\u00b7frau\u00b7M\u00fct\u00b7te\u00b7rlein", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Dir zum Kind gegeben,", "tokens": ["Der", "Dir", "zum", "Kind", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist selbst der Sch\u00f6pfer Dein.", "tokens": ["Ist", "selbst", "der", "Sch\u00f6p\u00b7fer", "Dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wohl leuchten Seine Augen", "tokens": ["Wohl", "leuch\u00b7ten", "Sei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und blassen ewig nicht,", "tokens": ["Und", "blas\u00b7sen", "e\u00b7wig", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn Sonn' und Sterne saugen", "tokens": ["Denn", "Sonn'", "und", "Ster\u00b7ne", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus ihnen nur ihr Licht. \u2013", "tokens": ["Aus", "ih\u00b7nen", "nur", "ihr", "Licht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O mutterselig Schweigen!", "tokens": ["O", "mut\u00b7ter\u00b7se\u00b7lig", "Schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Wonne, \u00fcbergro\u00df!", "tokens": ["O", "Won\u00b7ne", ",", "\u00fc\u00b7ber\u00b7gro\u00df", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O still-anbetend Neigen", "tokens": ["O", "still\u00b7an\u00b7be\u00b7tend", "Nei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJD", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Zum Kind in Deinem Schoo\u00df!", "tokens": ["Zum", "Kind", "in", "Dei\u00b7nem", "Schoo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}