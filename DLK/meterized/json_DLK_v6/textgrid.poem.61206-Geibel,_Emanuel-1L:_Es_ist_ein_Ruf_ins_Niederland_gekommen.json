{"textgrid.poem.61206": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist ein Ruf ins Niederland gekommen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Ruf ins Niederland gekommen", "tokens": ["Es", "ist", "ein", "Ruf", "ins", "Nie\u00b7der\u00b7land", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Gau her, wo der Eider Fluten m\u00fcnden,", "tokens": ["Vom", "Gau", "her", ",", "wo", "der", "Ei\u00b7der", "Flu\u00b7ten", "m\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der jede deutsche Seele mu\u00df entz\u00fcnden,", "tokens": ["Der", "je\u00b7de", "deut\u00b7sche", "See\u00b7le", "mu\u00df", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und war sie nie bis heut in Zorn erglommen.", "tokens": ["Und", "war", "sie", "nie", "bis", "heut", "in", "Zorn", "er\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vom Niederlande hat's der Harz vernommen,", "tokens": ["Vom", "Nie\u00b7der\u00b7lan\u00b7de", "hat's", "der", "Harz", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da schrie er auf aus seinen hundert Schl\u00fcnden,", "tokens": ["Da", "schrie", "er", "auf", "aus", "sei\u00b7nen", "hun\u00b7dert", "Schl\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Fichtelberg die Botschaft zu verk\u00fcnden;", "tokens": ["Dem", "Fich\u00b7tel\u00b7berg", "die", "Bot\u00b7schaft", "zu", "ver\u00b7k\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der rief den Alpen sie, vor Grimm beklommen.", "tokens": ["Der", "rief", "den", "Al\u00b7pen", "sie", ",", "vor", "Grimm", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "$,", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Alpen sandten sie nach Ost und Norden", "tokens": ["Die", "Al\u00b7pen", "sand\u00b7ten", "sie", "nach", "Ost", "und", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Rhein und Donau, die im Wogenbrande", "tokens": ["Mit", "Rhein", "und", "Do\u00b7nau", ",", "die", "im", "Wo\u00b7gen\u00b7bran\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Zornesadern schwollen aus den Borden.", "tokens": ["Wie", "Zor\u00b7ne\u00b7sa\u00b7dern", "schwol\u00b7len", "aus", "den", "Bor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun wissen's schon die Kinder weit im Lande,", "tokens": ["Nun", "wis\u00b7sen's", "schon", "die", "Kin\u00b7der", "weit", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und alle Stimmen sind ", "tokens": ["Und", "al\u00b7le", "Stim\u00b7men", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Schrei nach S\u00fchne f\u00fcr so gro\u00dfe Schande.", "tokens": ["Ein", "Schrei", "nach", "S\u00fch\u00b7ne", "f\u00fcr", "so", "gro\u00b7\u00dfe", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}