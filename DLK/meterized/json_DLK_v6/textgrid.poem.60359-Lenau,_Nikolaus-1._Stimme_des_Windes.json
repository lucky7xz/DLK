{"textgrid.poem.60359": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1. Stimme des Windes", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Schlummer ist der dunkle Wald gesunken,", "tokens": ["In", "Schlum\u00b7mer", "ist", "der", "dunk\u00b7le", "Wald", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu tr\u00e4ge ist die Luft, ein Blatt zu neigen,", "tokens": ["Zu", "tr\u00e4\u00b7ge", "ist", "die", "Luft", ",", "ein", "Blatt", "zu", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Bl\u00fctenduft zu tragen, und es schweigen", "tokens": ["Den", "Bl\u00fc\u00b7ten\u00b7duft", "zu", "tra\u00b7gen", ",", "und", "es", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Laub die V\u00f6gel und im Teich die Unken.", "tokens": ["Im", "Laub", "die", "V\u00f6\u00b7gel", "und", "im", "Teich", "die", "Un\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Leuchtk\u00e4fer nur, wie stille Traumesfunken", "tokens": ["Leucht\u00b7k\u00e4\u00b7fer", "nur", ",", "wie", "stil\u00b7le", "Trau\u00b7mes\u00b7fun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schlaf durchgaukelnd, schimmern in den Zweigen,", "tokens": ["Den", "Schlaf", "durch\u00b7gau\u00b7kelnd", ",", "schim\u00b7mern", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und s\u00fc\u00dfer Tr\u00e4ume ungest\u00f6rtem Reigen", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "Tr\u00e4u\u00b7me", "un\u00b7ge\u00b7st\u00f6r\u00b7tem", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ergibt sich meine Seele, schweigenstrunken.", "tokens": ["Er\u00b7gibt", "sich", "mei\u00b7ne", "See\u00b7le", ",", "schwei\u00b7gen\u00b7strun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Horch! \u00fcberraschend saust es in den B\u00e4umen", "tokens": ["Horch", "!", "\u00fc\u00b7berr\u00b7a\u00b7schend", "saust", "es", "in", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVPP", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ruft mich ab von meinen lieben Tr\u00e4umen,", "tokens": ["Und", "ruft", "mich", "ab", "von", "mei\u00b7nen", "lie\u00b7ben", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich h\u00f6re pl\u00f6tzlich ernste Stimme sprechen;", "tokens": ["Ich", "h\u00f6\u00b7re", "pl\u00f6tz\u00b7lich", "erns\u00b7te", "Stim\u00b7me", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die aufgeschreckte Seele lauscht dem Winde", "tokens": ["Die", "auf\u00b7ge\u00b7schreck\u00b7te", "See\u00b7le", "lauscht", "dem", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Worten ihres Vaters, der dem Kinde", "tokens": ["Wie", "Wor\u00b7ten", "ih\u00b7res", "Va\u00b7ters", ",", "der", "dem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zuruft, vom Spiele heimw\u00e4rts aufzubrechen.", "tokens": ["Zu\u00b7ruft", ",", "vom", "Spie\u00b7le", "heim\u00b7w\u00e4rts", "auf\u00b7zu\u00b7bre\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}