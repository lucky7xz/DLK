{"textgrid.poem.62850": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Prinz, bey jedem Zug der Gnade,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Prinz, bey jedem Zug der Gnade,", "tokens": ["Mein", "Prinz", ",", "bey", "je\u00b7dem", "Zug", "der", "Gna\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dir entstrahlt, denk ich entz\u00fcckt:", "tokens": ["Der", "dir", "ent\u00b7strahlt", ",", "denk", "ich", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wallt auf seines Vaters Pfade,", "tokens": ["Er", "wallt", "auf", "sei\u00b7nes", "Va\u00b7ters", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wie ein Gott sein Volk begl\u00fcckt.", "tokens": ["Der", "wie", "ein", "Gott", "sein", "Volk", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Herz ist gro\u00df, aus Herrmanns Lenden", "tokens": ["Dein", "Herz", "ist", "gro\u00df", ",", "aus", "Herr\u00b7manns", "Len\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stammt keines, dem an Huld es weicht.", "tokens": ["Stammt", "kei\u00b7nes", ",", "dem", "an", "Huld", "es", "weicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur werd es nie in deinen H\u00e4nden", "tokens": ["Nur", "werd", "es", "nie", "in", "dei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Dose, die man jedem reicht.", "tokens": ["Zur", "Do\u00b7se", ",", "die", "man", "je\u00b7dem", "reicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zur Zeit des alten Chronos bl\u00fchte", "tokens": ["Zur", "Zeit", "des", "al\u00b7ten", "Chro\u00b7nos", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein K\u00f6nig auf der Thiere Thron,", "tokens": ["Ein", "K\u00f6\u00b7nig", "auf", "der", "Thie\u00b7re", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So zahm, so mild, so voller G\u00fcte,", "tokens": ["So", "zahm", ",", "so", "mild", ",", "so", "vol\u00b7ler", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als je des fr\u00f6mmsten Lammes Sohn.", "tokens": ["Als", "je", "des", "fr\u00f6mms\u00b7ten", "Lam\u00b7mes", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts als belohnen und vergeben", "tokens": ["Nichts", "als", "be\u00b7loh\u00b7nen", "und", "ver\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "VVINF", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "War dieses F\u00fcrsten Leidenschaft;", "tokens": ["War", "die\u00b7ses", "F\u00fcrs\u00b7ten", "Lei\u00b7den\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch ward in seinem ganzen Leben", "tokens": ["Auch", "ward", "in", "sei\u00b7nem", "gan\u00b7zen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kein Frevler mit dem Tod bestraft.", "tokens": ["Kein", "Frev\u00b7ler", "mit", "dem", "Tod", "be\u00b7straft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf einem Zug durch seine Reiche", "tokens": ["Auf", "ei\u00b7nem", "Zug", "durch", "sei\u00b7ne", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Traf einst der thierische Trajan", "tokens": ["Traf", "einst", "der", "thie\u00b7ri\u00b7sche", "Tra\u00b7jan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Schatten einer hohlen Eiche", "tokens": ["Im", "Schat\u00b7ten", "ei\u00b7ner", "hoh\u00b7len", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Nestor aller B\u00e4ren an.", "tokens": ["Den", "Nes\u00b7tor", "al\u00b7ler", "B\u00e4\u00b7ren", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man r\u00fchmte diesen Eremiten;", "tokens": ["Man", "r\u00fchm\u00b7te", "die\u00b7sen", "E\u00b7re\u00b7mi\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es war die Fackel seiner Zeit,", "tokens": ["Es", "war", "die", "Fa\u00b7ckel", "sei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von strengen aber rauhen Sitten,", "tokens": ["Von", "stren\u00b7gen", "a\u00b7ber", "rau\u00b7hen", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und gar kein Freund von H\u00f6flichkeit.", "tokens": ["Und", "gar", "kein", "Freund", "von", "H\u00f6f\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Gro\u00dfherr sprach zum finstern Greise:", "tokens": ["Der", "Gro\u00df\u00b7herr", "sprach", "zum", "fins\u00b7tern", "Grei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe viel von dir geh\u00f6rt;", "tokens": ["Ich", "ha\u00b7be", "viel", "von", "dir", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man sagt du seyst der gr\u00f6\u00dfte Weise,", "tokens": ["Man", "sagt", "du", "seyst", "der", "gr\u00f6\u00df\u00b7te", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich erkenne deinen Werth.", "tokens": ["Und", "ich", "er\u00b7ken\u00b7ne", "dei\u00b7nen", "Werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm, folge mir nach meinem Schlosse;", "tokens": ["Komm", ",", "fol\u00b7ge", "mir", "nach", "mei\u00b7nem", "Schlos\u00b7se", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wechsel wird dich nicht gereun,", "tokens": ["Der", "Wech\u00b7sel", "wird", "dich", "nicht", "ge\u00b7reun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du sollst mein Freund, mein Tischgenosse", "tokens": ["Du", "sollst", "mein", "Freund", ",", "mein", "Tischge\u00b7nos\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Und mein geheimer Staatsrath seyn.", "tokens": ["Und", "mein", "ge\u00b7hei\u00b7mer", "Staats\u00b7rath", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich danke dir f\u00fcr deine Gnade,", "tokens": ["Ich", "dan\u00b7ke", "dir", "f\u00fcr", "dei\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erwiedert ihm der Philosoph;", "tokens": ["Er\u00b7wie\u00b7dert", "ihm", "der", "Phi\u00b7lo\u00b7soph", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn die genie\u00dft in gleichem Grade", "tokens": ["Denn", "die", "ge\u00b7nie\u00dft", "in", "glei\u00b7chem", "Gra\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der arge Fuchs an deinem Hof.", "tokens": ["Der", "ar\u00b7ge", "Fuchs", "an", "dei\u00b7nem", "Hof", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Schlo\u00df, das Buben in sich fasset,", "tokens": ["Ein", "Schlo\u00df", ",", "das", "Bu\u00b7ben", "in", "sich", "fas\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat f\u00fcr den Weisen keinen Glanz,", "tokens": ["Hat", "f\u00fcr", "den", "Wei\u00b7sen", "kei\u00b7nen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wer nicht ganz das Laster hasset,", "tokens": ["Und", "wer", "nicht", "ganz", "das", "Las\u00b7ter", "has\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der liebt die Tugend auch nicht ganz.", "tokens": ["Der", "liebt", "die", "Tu\u00b7gend", "auch", "nicht", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}