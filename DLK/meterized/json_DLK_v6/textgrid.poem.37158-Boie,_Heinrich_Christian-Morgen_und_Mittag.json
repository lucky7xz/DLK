{"textgrid.poem.37158": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Morgen und Mittag", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In erster D\u00e4mmrung aufgegangen", "tokens": ["In", "ers\u00b7ter", "D\u00e4mm\u00b7rung", "auf\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah ich an deinen zarten Wangen", "tokens": ["Sah", "ich", "an", "dei\u00b7nen", "zar\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sch\u00f6nheit Morgenroth;", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Mor\u00b7gen\u00b7roth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und sank allm\u00e4chtig hingeri\u00dfen", "tokens": ["Und", "sank", "all\u00b7m\u00e4ch\u00b7tig", "hin\u00b7ge\u00b7ri\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und zitternd schon zu deinen F\u00fc\u00dfen", "tokens": ["Und", "zit\u00b7ternd", "schon", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ehrte dein Gebot.", "tokens": ["Und", "ehr\u00b7te", "dein", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ganz in deinen Blick verloren", "tokens": ["Und", "ganz", "in", "dei\u00b7nen", "Blick", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah ich dich damals schon erkoren", "tokens": ["Sah", "ich", "dich", "da\u00b7mals", "schon", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Liebe K\u00f6nigin.", "tokens": ["Der", "Lie\u00b7be", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ehe du Verehrer fandest", "tokens": ["Und", "e\u00b7he", "du", "Ver\u00b7eh\u00b7rer", "fan\u00b7dest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und eines Herzen Werth verstandest,", "tokens": ["Und", "ei\u00b7nes", "Her\u00b7zen", "Werth", "ver\u00b7stan\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gab ich mein Herz dir hin.", "tokens": ["Gab", "ich", "mein", "Herz", "dir", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jedweden Reiz sah ich entstehen", "tokens": ["Jed\u00b7we\u00b7den", "Reiz", "sah", "ich", "ent\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und konnte nur dein Auge sehen,", "tokens": ["Und", "konn\u00b7te", "nur", "dein", "Au\u00b7ge", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil sehn noch sicher war;", "tokens": ["Weil", "sehn", "noch", "si\u00b7cher", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und dachte nicht die s\u00fc\u00dfe, frohe", "tokens": ["Und", "dach\u00b7te", "nicht", "die", "s\u00fc\u00b7\u00dfe", ",", "fro\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bescheidne, sanfte Minne drohe", "tokens": ["Be\u00b7scheid\u00b7ne", ",", "sanf\u00b7te", "Min\u00b7ne", "dro\u00b7he"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der halben Welt Gefahr.", "tokens": ["Der", "hal\u00b7ben", "Welt", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Unwiderstehlich aber w\u00fctet", "tokens": ["Un\u00b7wi\u00b7der\u00b7steh\u00b7lich", "a\u00b7ber", "w\u00fc\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Sch\u00f6nheit Mittag nun, gebietet,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Mit\u00b7tag", "nun", ",", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Sklaven beten an.", "tokens": ["Und", "Skla\u00b7ven", "be\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer darf um ihre Blicke werben?", "tokens": ["Wer", "darf", "um", "ih\u00b7re", "Bli\u00b7cke", "wer\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Tod folget ihnen und Verderben,", "tokens": ["Tod", "fol\u00b7get", "ih\u00b7nen", "und", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn man nicht hoffen kann.", "tokens": ["Wenn", "man", "nicht", "hof\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So hebt sich, wenn die ersten Stralen", "tokens": ["So", "hebt", "sich", ",", "wenn", "die", "ers\u00b7ten", "Stra\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonn' in Gold den Osten malen,", "tokens": ["Der", "Sonn'", "in", "Gold", "den", "Os\u00b7ten", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Persers Fr\u00fchgebet,", "tokens": ["Des", "Per\u00b7sers", "Fr\u00fch\u00b7ge\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der, wenn der Mittag ihren Wagen", "tokens": ["Der", ",", "wenn", "der", "Mit\u00b7tag", "ih\u00b7ren", "Wa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In hei\u00dfrer Glut heraufgetragen,", "tokens": ["In", "hei\u00df\u00b7rer", "Glut", "her\u00b7auf\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erblasset, sinkt, vergeht.", "tokens": ["Er\u00b7blas\u00b7set", ",", "sinkt", ",", "ver\u00b7geht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}