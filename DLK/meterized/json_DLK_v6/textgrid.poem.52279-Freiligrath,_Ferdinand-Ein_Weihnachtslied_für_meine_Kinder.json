{"textgrid.poem.52279": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ein Weihnachtslied f\u00fcr meine Kinder", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum sechstenmal der Kerzen Strahl", "tokens": ["Zum", "sechs\u00b7ten\u00b7mal", "der", "Ker\u00b7zen", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Anfach' ich auf der Fichte;", "tokens": ["An\u00b7fach'", "ich", "auf", "der", "Fich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist ein Schein! Herein, herein,", "tokens": ["Das", "ist", "ein", "Schein", "!", "Her\u00b7ein", ",", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und freut euch an dem Lichte!", "tokens": ["Und", "freut", "euch", "an", "dem", "Lich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Genug geharrt, genug gescharrt", "tokens": ["Ge\u00b7nug", "ge\u00b7harrt", ",", "ge\u00b7nug", "ge\u00b7scharrt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Gang und an der T\u00fcre!", "tokens": ["Im", "Gang", "und", "an", "der", "T\u00fc\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Schelle klingt, der Riegel springt:", "tokens": ["Die", "Schel\u00b7le", "klingt", ",", "der", "Rie\u00b7gel", "springt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Herein, mein Kleeblatt-Viere!", "tokens": ["Her\u00b7ein", ",", "mein", "Klee\u00b7blat\u00b7tVie\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herein, ihr Froh'n! Ach, wo nicht schon,", "tokens": ["Her\u00b7ein", ",", "ihr", "Froh'n", "!", "Ach", ",", "wo", "nicht", "schon", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "$.", "ITJ", "$,", "PWAV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr zarten jungen Leben,", "tokens": ["Ihr", "zar\u00b7ten", "jun\u00b7gen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kamt ihr, wie heut, auf mein Gel\u00e4ut' \u2013", "tokens": ["Kamt", "ihr", ",", "wie", "heut", ",", "auf", "mein", "Ge\u00b7l\u00e4ut'", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sind Nomaden eben!", "tokens": ["Wir", "sind", "No\u00b7ma\u00b7den", "e\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Heil eurer Lust! Mir f\u00fcllt die Brust", "tokens": ["Heil", "eu\u00b7rer", "Lust", "!", "Mir", "f\u00fcllt", "die", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ein schmerzlich-s\u00fc\u00dfes Tr\u00e4umen!", "tokens": ["Ein", "schmerz\u00b7lich\u00b7s\u00fc\u00b7\u00dfes", "Tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Anheb' ich weich ein Lied f\u00fcr euch", "tokens": ["An\u00b7heb'", "ich", "weich", "ein", "Lied", "f\u00fcr", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von euren Weihnachtsb\u00e4umen!", "tokens": ["Von", "eu\u00b7ren", "Weih\u00b7nachts\u00b7b\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der erste stund auf Schweizergrund", "tokens": ["Der", "ers\u00b7te", "stund", "auf", "Schwei\u00b7zer\u00b7grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In rauher Felsen Schatten;", "tokens": ["In", "rau\u00b7her", "Fel\u00b7sen", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sah den See, er sah den Schnee,", "tokens": ["Er", "sah", "den", "See", ",", "er", "sah", "den", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ew'gen, ob den Matten;", "tokens": ["Den", "ew'\u00b7gen", ",", "ob", "den", "Mat\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sah Herdenziehn und Alpengl\u00fchn,", "tokens": ["Sah", "Her\u00b7den\u00b7ziehn", "und", "Al\u00b7pen\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Gletscher und die Wiese;", "tokens": ["Den", "Glet\u00b7scher", "und", "die", "Wie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bot mit Gest\u00f6hn die Brust dem F\u00f6hn \u2013", "tokens": ["Bot", "mit", "Ge\u00b7st\u00f6hn", "die", "Brust", "dem", "F\u00f6hn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Dem F\u00f6hn und auch der Bise.", "tokens": ["Dem", "F\u00f6hn", "und", "auch", "der", "Bi\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die zweite dann und dritte Tann'", "tokens": ["Die", "zwei\u00b7te", "dann", "und", "drit\u00b7te", "Tann'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufwuchsen an der Themse;", "tokens": ["Auf\u00b7wuch\u00b7sen", "an", "der", "Them\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Gr\u00fcn entlang zu Berge sprang", "tokens": ["Ihr", "Gr\u00fcn", "ent\u00b7lang", "zu", "Ber\u00b7ge", "sprang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPO", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Steinbock, keine Gemse;", "tokens": ["Kein", "Stein\u00b7bock", ",", "kei\u00b7ne", "Gem\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch stattlich schwamm den niedern Stamm", "tokens": ["Doch", "statt\u00b7lich", "schwamm", "den", "nie\u00b7dern", "Stamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor\u00fcber Bark' um Barke;", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "Bark'", "um", "Bar\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und herbes Wehn, der Nordsee Wehn,", "tokens": ["Und", "her\u00b7bes", "Wehn", ",", "der", "Nord\u00b7see", "Wehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gab Kraft dem jungen Marke.", "tokens": ["Gab", "Kraft", "dem", "jun\u00b7gen", "Mar\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das n\u00e4chste war ein heimisch Paar,", "tokens": ["Das", "n\u00e4chs\u00b7te", "war", "ein", "hei\u00b7misch", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Tannenpaar vpm Rheine,", "tokens": ["Ein", "Tan\u00b7nen\u00b7paar", "vpm", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Wurzeln schlug und Nadeln trug", "tokens": ["Das", "Wur\u00b7zeln", "schlug", "und", "Na\u00b7deln", "trug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf hohem Ufersteine.", "tokens": ["Auf", "ho\u00b7hem", "Uf\u00b7er\u00b7stei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Ri\u00df der Ley entragt' es frei,", "tokens": ["Dem", "Ri\u00df", "der", "Ley", "ent\u00b7ragt'", "es", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Landein die Eifel blaute,", "tokens": ["Lan\u00b7de\u00b7in", "die", "Ei\u00b7fel", "blau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Weingerank umflog den Hang,", "tokens": ["Und", "Wein\u00b7ge\u00b7rank", "um\u00b7flog", "den", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dem es niederschaute.", "tokens": ["Von", "dem", "es", "nie\u00b7der\u00b7schau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und der euch heut sein Astwerk beut,", "tokens": ["Und", "der", "euch", "heut", "sein", "Ast\u00b7werk", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das zackige, das breite,", "tokens": ["Das", "za\u00b7cki\u00b7ge", ",", "das", "brei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der schaute dreist, blank \u00fcbereist", "tokens": ["Der", "schau\u00b7te", "dreist", ",", "blank", "\u00fc\u00b7be\u00b7reist"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Grafenberg ins Weite.", "tokens": ["Vom", "Gra\u00b7fen\u00b7berg", "ins", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Stromniedrung hier, dort Bergrevier \u2013", "tokens": ["Strom\u00b7nie\u00b7drung", "hier", ",", "dort", "Berg\u00b7re\u00b7vier", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "NN", "$("], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein letzter Klippensprenger,", "tokens": ["Ein", "letz\u00b7ter", "Klip\u00b7pen\u00b7spren\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nachrauscht' er hohl ein Lebewohl", "tokens": ["Nachrauscht'", "er", "hohl", "ein", "Le\u00b7be\u00b7wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dem Rhein, dem Hollandsg\u00e4nger.", "tokens": ["Dem", "Rhein", ",", "dem", "Hol\u00b7lands\u00b7g\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ade, ade! Das alte Weh!", "tokens": ["A\u00b7de", ",", "a\u00b7de", "!", "Das", "al\u00b7te", "Weh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wer wei\u00df an was f\u00fcr Wellen", "tokens": ["Wer", "wei\u00df", "an", "was", "f\u00fcr", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir \u00fcbers Jahr, Rauhfrost im Haar,", "tokens": ["Wir", "\u00fc\u00b7bers", "Jahr", ",", "Rauh\u00b7frost", "im", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die Weihnachtstanne f\u00e4llen!", "tokens": ["Die", "Weih\u00b7nachts\u00b7tan\u00b7ne", "f\u00e4l\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vielleicht aufs neu umf\u00e4ngt sie treu", "tokens": ["Viel\u00b7leicht", "aufs", "neu", "um\u00b7f\u00e4ngt", "sie", "treu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJD", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Alt-Englands werter Boden \u2013", "tokens": ["Al\u00b7tEng\u00b7lands", "wer\u00b7ter", "Bo\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch sichrer ist, sie steht zur Frist", "tokens": ["Doch", "sich\u00b7rer", "ist", ",", "sie", "steht", "zur", "Frist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Am Hudson in den Loden.", "tokens": ["Am", "Hud\u00b7son", "in", "den", "Lo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sieht ernst sich an im Michigan,", "tokens": ["Sieht", "ernst", "sich", "an", "im", "Mi\u00b7chi\u00b7gan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "APPRART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Strahlt wieder aus der Bl\u00e4ue", "tokens": ["Strahlt", "wie\u00b7der", "aus", "der", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Erieflut \u2013 eine Rothaut ruht", "tokens": ["Der", "E\u00b7rie\u00b7flut", "\u2013", "ei\u00b7ne", "Rot\u00b7haut", "ruht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf ihrer Nadelstreue.", "tokens": ["Auf", "ih\u00b7rer", "Na\u00b7del\u00b7streu\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zur Hand im Schnee starr liegt ein Reh,", "tokens": ["Zur", "Hand", "im", "Schnee", "starr", "liegt", "ein", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "B\u00f6lutr\u00fcnstig, frisch geschossen;", "tokens": ["B\u00f6\u00b7lu\u00b7tr\u00fcns\u00b7tig", ",", "frisch", "ge\u00b7schos\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ein Feuerlein wirft hellen Schein", "tokens": ["Ein", "Feu\u00b7er\u00b7lein", "wirft", "hel\u00b7len", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf zu den dunklen Sprossen.", "tokens": ["Auf", "zu", "den", "dunk\u00b7len", "Spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die aber spr\u00fchn ihr Harz ins Gl\u00fchn", "tokens": ["Die", "a\u00b7ber", "spr\u00fchn", "ihr", "Harz", "ins", "Gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Reisigs und der Kohlen. \u2013", "tokens": ["Des", "Rei\u00b7sigs", "und", "der", "Koh\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "$("], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Das ist die Tann' \u2013 und horch, beian,", "tokens": ["Das", "ist", "die", "Tann'", "\u2013", "und", "horch", ",", "bei\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "KON", "VVIMP", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was summt im Baum, dem hohlen?", "tokens": ["Was", "summt", "im", "Baum", ",", "dem", "hoh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Eichenstamm, wie wundersam!", "tokens": ["Im", "Ei\u00b7chen\u00b7stamm", ",", "wie", "wun\u00b7der\u00b7sam", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was t\u00f6nen da f\u00fcr Stimmen?", "tokens": ["Was", "t\u00f6\u00b7nen", "da", "f\u00fcr", "Stim\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Roten fragt \u2013 ich wei\u00df, er sagt:", "tokens": ["Den", "Ro\u00b7ten", "fragt", "\u2013", "ich", "wei\u00df", ",", "er", "sagt