matlos", "tokens": ["Die", "Hei\u00b7mat", "blo\u00df", "macht", "hei\u00b7mat\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Kinder ihres Dichters!", "tokens": ["Die", "Kin\u00b7der", "ih\u00b7res", "Dich\u00b7ters", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Da, Glockenton! Halb achte schon!", "tokens": ["Da", ",", "Glo\u00b7cken\u00b7ton", "!", "Halb", "ach\u00b7te", "schon", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$.", "ADJD", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Git' Nacht nun eurem Baume!", "tokens": ["Git'", "Nacht", "nun", "eu\u00b7rem", "Bau\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht, wild Quartett, du gehst zu Bett,", "tokens": ["Nicht", ",", "wild", "Quar\u00b7tett", ",", "du", "gehst", "zu", "Bett", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "ADJD", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du siehst ihn fort im Traume?", "tokens": ["Du", "siehst", "ihn", "fort", "im", "Trau\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon bla\u00dft sein Licht! Verge\u00dft ihn nicht,", "tokens": ["Schon", "bla\u00dft", "sein", "Licht", "!", "Ver\u00b7ge\u00dft", "ihn", 