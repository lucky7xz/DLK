{"dta.poem.2770": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die versagte Liebste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Darff ich nicht meine Freyheit haben", "tokens": ["Darff", "ich", "nicht", "mei\u00b7ne", "Frey\u00b7heit", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In der zuvor mein Geist geprangt?", "tokens": ["In", "der", "zu\u00b7vor", "mein", "Geist", "ge\u00b7prangt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist denn der freye Muth begraben?", "tokens": ["Ist", "denn", "der", "frey\u00b7e", "Muth", "be\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat meine Lieb ihr End erlangt;", "tokens": ["Hat", "mei\u00b7ne", "Lieb", "ihr", "End", "er\u00b7langt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df sie mu\u00df in des Kerckers Nacht", "tokens": ["Da\u00df", "sie", "mu\u00df", "in", "des", "Ker\u00b7ckers", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "ART", "NN", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Verlieren Zierde/ Glantz und Pracht.", "tokens": ["Ver\u00b7lie\u00b7ren", "Zier\u00b7de", "/", "Glantz", "und", "Pracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gefangne hoffen frey zu werden;", "tokens": ["Ge\u00b7fang\u00b7ne", "hof\u00b7fen", "frey", "zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich wei\u00df von keiner Rettung nicht.", "tokens": ["Ich", "wei\u00df", "von", "kei\u00b7ner", "Ret\u00b7tung", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es lebt kein Mensch auf dieser Erden/", "tokens": ["Es", "lebt", "kein", "Mensch", "auf", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dem es so sehr an Trost gebricht.", "tokens": ["Dem", "es", "so", "sehr", "an", "Trost", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Liebe die mich erst ergetzt/", "tokens": ["Die", "Lie\u00b7be", "die", "mich", "erst", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hat nun ihr Schwerdt auf mich gewetzt.", "tokens": ["Hat", "nun", "ihr", "Schwerdt", "auf", "mich", "ge\u00b7wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Jetz mu\u00df ich schwere Kettentragen/", "tokens": ["Jetz", "mu\u00df", "ich", "schwe\u00b7re", "Ket\u00b7ten\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da ich zuvor in Kronen gieng.", "tokens": ["Da", "ich", "zu\u00b7vor", "in", "Kro\u00b7nen", "gieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Was ist noch \u00fcbrig als verzagen;", "tokens": ["Was", "ist", "noch", "\u00fcb\u00b7rig", "als", "ver\u00b7za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Das Leben sch\u00e4tz ich selbst gering/", "tokens": ["Das", "Le\u00b7ben", "sch\u00e4tz", "ich", "selbst", "ge\u00b7ring", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und w\u00fcnsche noch viel Noth und Pein", "tokens": ["Und", "w\u00fcn\u00b7sche", "noch", "viel", "Noth", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Des Grabes lieber Gast zu seyn.", "tokens": ["Des", "Gra\u00b7bes", "lie\u00b7ber", "Gast", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ist mir versagt in deinen Armen/", "tokens": ["Ist", "mir", "ver\u00b7sagt", "in", "dei\u00b7nen", "Ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mein allersch\u00f6nstes Kind zu ruhn?", "tokens": ["Mein", "al\u00b7ler\u00b7sch\u00f6ns\u00b7tes", "Kind", "zu", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So wirst du dich doch noch erbarmen", "tokens": ["So", "wirst", "du", "dich", "doch", "noch", "er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Mir diesen letzten Dienst zu thun/", "tokens": ["Mir", "die\u00b7sen", "letz\u00b7ten", "Dienst", "zu", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Da\u00df du nimmst meinen Schatten an/", "tokens": ["Da\u00df", "du", "nimmst", "mei\u00b7nen", "Schat\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der sonsten nirgends ruhen kan.", "tokens": ["Der", "sons\u00b7ten", "nir\u00b7gends", "ru\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Gedencke/ was ich schon erlitten/", "tokens": ["Ge\u00b7den\u00b7cke", "/", "was", "ich", "schon", "er\u00b7lit\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Weil Himmel/ Gl\u00fcck und Zeit mein Feind:", "tokens": ["Weil", "Him\u00b7mel", "/", "Gl\u00fcck", "und", "Zeit", "mein", "Feind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wie ich umb deine Gunst gestritten/", "tokens": ["Wie", "ich", "umb", "dei\u00b7ne", "Gunst", "ge\u00b7strit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die mir nunmehr nicht hold erscheint.", "tokens": ["Die", "mir", "nun\u00b7mehr", "nicht", "hold", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Du kanst und sollst nicht meine seyn/", "tokens": ["Du", "kanst", "und", "sollst", "nicht", "mei\u00b7ne", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "PPOSAT", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Wie bitter geht mir dieses ein.", "tokens": ["Wie", "bit\u00b7ter", "geht", "mir", "die\u00b7ses", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und weil ich dich nicht soll besitzen/", "tokens": ["Und", "weil", "ich", "dich", "nicht", "soll", "be\u00b7sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "So such ich meinen eignen Tod/", "tokens": ["So", "such", "ich", "mei\u00b7nen", "eig\u00b7nen", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der wird mein gutes Recht besch\u00fctzen/", "tokens": ["Der", "wird", "mein", "gu\u00b7tes", "Recht", "be\u00b7sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Wie du mich hast gebracht in Noth.", "tokens": ["Wie", "du", "mich", "hast", "ge\u00b7bracht", "in", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ich hasse nun das Tage-Licht/", "tokens": ["Ich", "has\u00b7se", "nun", "das", "Ta\u00b7ge\u00b7Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Dieweil ich dich darff sehen nicht.", "tokens": ["Die\u00b7weil", "ich", "dich", "darff", "se\u00b7hen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}