{"dta.poem.217": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er entdeckt ihr seine Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du sanffter Wind dein angenehmes Blasen", "tokens": ["Du", "sanff\u00b7ter", "Wind", "dein", "an\u00b7ge\u00b7neh\u00b7mes", "Bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erregt ein Feur in meiner Brust.", "tokens": ["Er\u00b7regt", "ein", "Feur", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Seele brennt/ die Liebes-Flammen rasen", "tokens": ["Die", "See\u00b7le", "brennt", "/", "die", "Lie\u00b7bes\u00b7Flam\u00b7men", "ra\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jm Hertzen/ dem sonst keine Gluht bewust.", "tokens": ["Jm", "Hert\u00b7zen", "/", "dem", "sonst", "kei\u00b7ne", "Gluht", "be\u00b7wust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "ART", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So bald der Augen-Blitz die Seele angestecket/", "tokens": ["So", "bald", "der", "Au\u00b7gen\u00b7Blitz", "die", "See\u00b7le", "an\u00b7ge\u00b7ste\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So blie\u00df dein sanffter Hauch/", "tokens": ["So", "blie\u00df", "dein", "sanff\u00b7ter", "Hauch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Flamme gr\u00f6sser auf/", "tokens": ["Die", "Flam\u00b7me", "gr\u00f6s\u00b7ser", "auf", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein heller Brand im Hertzen ward erwecket.", "tokens": ["Ein", "hel\u00b7ler", "Brand", "im", "Hert\u00b7zen", "ward", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}