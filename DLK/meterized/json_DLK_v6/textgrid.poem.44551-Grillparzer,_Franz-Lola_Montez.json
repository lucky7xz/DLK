{"textgrid.poem.44551": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Lola Montez", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht nicht zur\u00fcck bis auf des Ursprungs Born,", "tokens": ["Geht", "nicht", "zu\u00b7r\u00fcck", "bis", "auf", "des", "Ur\u00b7sprungs", "Born", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKVZ", "KON", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Wahres obsiegt und wenn Rechtes wird,", "tokens": ["Wenn", "Wah\u00b7res", "ob\u00b7siegt", "und", "wenn", "Rech\u00b7tes", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "KON", "KOUS", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Es ist des Menschen Geist ein Samenkorn,", "tokens": ["Es", "ist", "des", "Men\u00b7schen", "Geist", "ein", "Sa\u00b7men\u00b7korn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das selbst erzeugt, doch selber nicht gebiert.", "tokens": ["Das", "selbst", "er\u00b7zeugt", ",", "doch", "sel\u00b7ber", "nicht", "ge\u00b7biert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "$,", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und Erde andrerseits ein schmutzig Ding,", "tokens": ["Und", "Er\u00b7de", "an\u00b7drer\u00b7seits", "ein", "schmut\u00b7zig", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gemein an sich und besser nicht als Kot,", "tokens": ["Ge\u00b7mein", "an", "sich", "und", "bes\u00b7ser", "nicht", "als", "Kot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "KON", "ADJD", "PTKNEG", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das manchem schon sich an die Sohlen hing,", "tokens": ["Das", "man\u00b7chem", "schon", "sich", "an", "die", "Soh\u00b7len", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Achtlos und taub f\u00fcr Mahnung und Gebot.", "tokens": ["Acht\u00b7los", "und", "taub", "f\u00fcr", "Mah\u00b7nung", "und", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch legst das Samenkorn du in den Grund,", "tokens": ["Doch", "legst", "das", "Sa\u00b7men\u00b7korn", "du", "in", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Eint sich in beiden, was nach Leben sucht,", "tokens": ["Eint", "sich", "in", "bei\u00b7den", ",", "was", "nach", "Le\u00b7ben", "sucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Und aus der Erde aufgeschlo\u00dfnem Mund", "tokens": ["Und", "aus", "der", "Er\u00b7de", "auf\u00b7ge\u00b7schlo\u00df\u00b7nem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hebt sich der Keim, der Bl\u00fcten tr\u00e4gt und Frucht.", "tokens": ["Hebt", "sich", "der", "Keim", ",", "der", "Bl\u00fc\u00b7ten", "tr\u00e4gt", "und", "Frucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "ART", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So eint sich unserm Geist die Leidenschaft,", "tokens": ["So", "eint", "sich", "un\u00b7serm", "Geist", "die", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ihn beirrt, zum Schlimmen oft erregt,", "tokens": ["Die", "ihn", "be\u00b7irrt", ",", "zum", "Schlim\u00b7men", "oft", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch liegt in ihr auch unsers Guten Kraft,", "tokens": ["Doch", "liegt", "in", "ihr", "auch", "un\u00b7sers", "Gu\u00b7ten", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Blinden gleicht sie, der den Lahmen tr\u00e4gt.", "tokens": ["Dem", "Blin\u00b7den", "gleicht", "sie", ",", "der", "den", "Lah\u00b7men", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Denn harrtest du, bis aus Vernunft und Recht", "tokens": ["Denn", "harr\u00b7test", "du", ",", "bis", "aus", "Ver\u00b7nunft", "und", "Recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entst\u00fcnde, was das Recht und die Vernunft gebot,", "tokens": ["Ent\u00b7st\u00fcn\u00b7de", ",", "was", "das", "Recht", "und", "die", "Ver\u00b7nunft", "ge\u00b7bot", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schlimm w\u00e4rs bestellt ums menschliche Geschlecht,", "tokens": ["Schlimm", "w\u00e4rs", "be\u00b7stellt", "ums", "menschli\u00b7che", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Trieb erzeugt die Handlung, die uns not.", "tokens": ["Der", "Trieb", "er\u00b7zeugt", "die", "Hand\u00b7lung", ",", "die", "uns", "not", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Drum kehrt euch nicht verachtend von dem Weib,", "tokens": ["Drum", "kehrt", "euch", "nicht", "ver\u00b7ach\u00b7tend", "von", "dem", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In deren Arm ein K\u00f6nig ward zum Mann,", "tokens": ["In", "de\u00b7ren", "Arm", "ein", "K\u00f6\u00b7nig", "ward", "zum", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie gab dem besseren Gedanken Leib,", "tokens": ["Sie", "gab", "dem", "bes\u00b7se\u00b7ren", "Ge\u00b7dan\u00b7ken", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verlor sich selbst, allein die Welt gewann.", "tokens": ["Ver\u00b7lor", "sich", "selbst", ",", "al\u00b7lein", "die", "Welt", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}