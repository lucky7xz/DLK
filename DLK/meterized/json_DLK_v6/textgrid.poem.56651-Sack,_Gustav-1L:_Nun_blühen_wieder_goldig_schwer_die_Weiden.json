{"textgrid.poem.56651": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun bl\u00fchen wieder goldig schwer die Weiden", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun bl\u00fchen wieder goldig schwer die Weiden", "tokens": ["Nun", "bl\u00fc\u00b7hen", "wie\u00b7der", "gol\u00b7dig", "schwer", "die", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in meinem m\u00e4rzensonndurchgl\u00fchten Moor,", "tokens": ["in", "mei\u00b7nem", "m\u00e4r\u00b7zen\u00b7sonn\u00b7durch\u00b7gl\u00fch\u00b7ten", "Moor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "als ob ich in das windzerschli\u00dfne Rohr", "tokens": ["als", "ob", "ich", "in", "das", "wind\u00b7zer\u00b7schli\u00df\u00b7ne", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "geworfen einen Kn\u00e4uel gelber Seiden.", "tokens": ["ge\u00b7wor\u00b7fen", "ei\u00b7nen", "Kn\u00e4u\u00b7el", "gel\u00b7ber", "Sei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als ob ich meines Winters s\u00fc\u00dfe Leiden", "tokens": ["Als", "ob", "ich", "mei\u00b7nes", "Win\u00b7ters", "s\u00fc\u00b7\u00dfe", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und seiner W\u00fcnsche hadernd lauten Chor", "tokens": ["und", "sei\u00b7ner", "W\u00fcn\u00b7sche", "ha\u00b7dernd", "lau\u00b7ten", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "gepre\u00dft in diesen feinen Seidenflor,", "tokens": ["ge\u00b7pre\u00dft", "in", "die\u00b7sen", "fei\u00b7nen", "Sei\u00b7den\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "um so von ihnen freundlich mich zu scheiden.", "tokens": ["um", "so", "von", "ih\u00b7nen", "freund\u00b7lich", "mich", "zu", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "PPER", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun liege ich mit leidbefreiter Seele", "tokens": ["Nun", "lie\u00b7ge", "ich", "mit", "leid\u00b7be\u00b7frei\u00b7ter", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und schaue ihnen nach aus Rohr und Ried", "tokens": ["und", "schau\u00b7e", "ih\u00b7nen", "nach", "aus", "Rohr", "und", "Ried"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "versenkt in sinnende Melancholie,", "tokens": ["ver\u00b7senkt", "in", "sin\u00b7nen\u00b7de", "Me\u00b7lan\u00b7cho\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "da rauscht's im Schilf, und eh' ich fort mich stehle,", "tokens": ["da", "rauscht's", "im", "Schilf", ",", "und", "eh'", "ich", "fort", "mich", "steh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "KON", "KOUS", "PPER", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da tanzt sie zu mir, die mich Winters mied,", "tokens": ["da", "tanzt", "sie", "zu", "mir", ",", "die", "mich", "Win\u00b7ters", "mied", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und \u2013 hebt kokett ihr R\u00f6ckchen bis zum Knie.", "tokens": ["und", "\u2013", "hebt", "ko\u00b7kett", "ihr", "R\u00f6ck\u00b7chen", "bis", "zum", "Knie", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}