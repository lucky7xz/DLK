{"textgrid.poem.52135": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erndte l\u00e4\u00dfest du schon zwey mahl drei\u00dfig mahl", "tokens": ["Die", "Ernd\u00b7te", "l\u00e4\u00b7\u00dfest", "du", "schon", "zwey", "mahl", "drei\u00b7\u00dfig", "mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "CARD", "ADV", "CARD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In S\u00e4cken eingef\u00fcllt auff deine S\u00f6ller tragen:", "tokens": ["In", "S\u00e4\u00b7cken", "ein\u00b7ge\u00b7f\u00fcllt", "auff", "dei\u00b7ne", "S\u00f6l\u00b7ler", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Alter k\u00f6nnen dir der T\u00f6chter Kinder sagen:", "tokens": ["Dein", "Al\u00b7ter", "k\u00f6n\u00b7nen", "dir", "der", "T\u00f6ch\u00b7ter", "Kin\u00b7der", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jedoch wilt du vermehrn der Jungen Stutzer Zahl.", "tokens": ["Je\u00b7doch", "wilt", "du", "ver\u00b7mehrn", "der", "Jun\u00b7gen", "Stut\u00b7zer", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein Spiel, kein Gastgeboth stellt ie ein Nachbar an,", "tokens": ["Kein", "Spiel", ",", "kein", "Gast\u00b7ge\u00b7both", "stellt", "ie", "ein", "Nach\u00b7bar", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff das dein neues Kleid du auch nicht soltest schicken,", "tokens": ["Auff", "das", "dein", "neu\u00b7es", "Kleid", "du", "auch", "nicht", "sol\u00b7test", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Den auffgesetzten Bart seh' ich von fernen blicken,", "tokens": ["Den", "auff\u00b7ge\u00b7setz\u00b7ten", "Bart", "seh'", "ich", "von", "fer\u00b7nen", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wei\u00df als einen Schleyr ein Klag Weib tragen kan.", "tokens": ["So", "wei\u00df", "als", "ei\u00b7nen", "Schleyr", "ein", "Klag", "Weib", "tra\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wann auff Gesundheit du des Frauen Zimmers trinckst,", "tokens": ["Wann", "auff", "Ge\u00b7sund\u00b7heit", "du", "des", "Frau\u00b7en", "Zim\u00b7mers", "trinckst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bleibt kein Tropffen nicht auff deinem Nagel stehen,", "tokens": ["So", "bleibt", "kein", "Tropf\u00b7fen", "nicht", "auff", "dei\u00b7nem", "Na\u00b7gel", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da bist du sehr bem\u00fcht, wo sch\u00f6ne M\u00e4gdlein springen,", "tokens": ["Da", "bist", "du", "sehr", "be\u00b7m\u00fcht", ",", "wo", "sch\u00f6\u00b7ne", "M\u00e4gd\u00b7lein", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Reh' erhebt sich so, wie du im Tantze springst.", "tokens": ["Kein", "Reh'", "er\u00b7hebt", "sich", "so", ",", "wie", "du", "im", "Tant\u00b7ze", "springst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein Schubsack der ist recht der Buhler Lieder Hau\u00df,", "tokens": ["Dein", "Schub\u00b7sack", "der", "ist", "recht", "der", "Buh\u00b7ler", "Lie\u00b7der", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die in die Seiten du voll Andacht pflegst zu singen:", "tokens": ["Die", "in", "die", "Sei\u00b7ten", "du", "voll", "An\u00b7dacht", "pflegst", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "ADJD", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die B\u00fccher, welche wir aus frembden L\u00e4ndern bringen,", "tokens": ["Die", "B\u00fc\u00b7cher", ",", "wel\u00b7che", "wir", "aus", "fremb\u00b7den", "L\u00e4n\u00b7dern", "brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hast alle du im Kopf und sagst und plauderst draus.", "tokens": ["Hast", "al\u00b7le", "du", "im", "Kopf", "und", "sagst", "und", "plau\u00b7derst", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "APPRART", "NN", "KON", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ja wol, du stellest dich bey uns beh\u00e4glich ein,", "tokens": ["Ja", "wol", ",", "du", "stel\u00b7lest", "dich", "bey", "uns", "be\u00b7h\u00e4g\u00b7lich", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PPER", "VVFIN", "PRF", "APPR", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wann etwan wir vermummt zum Abend Tantze gehen:", "tokens": ["Wann", "et\u00b7wan", "wir", "ver\u00b7mummt", "zum", "A\u00b7bend", "Tant\u00b7ze", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Grei\u00df, la\u00df uns das Spiel, nichts kan ja \u00e4rger stehen,", "tokens": ["Mein", "Grei\u00df", ",", "la\u00df", "uns", "das", "Spiel", ",", "nichts", "kan", "ja", "\u00e4r\u00b7ger", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "$,", "PIS", "VMFIN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wann ein alter Hecht wil Pickelh\u00e4ring seyn.", "tokens": ["Als", "wann", "ein", "al\u00b7ter", "Hecht", "wil", "Pi\u00b7ckel\u00b7h\u00e4\u00b7ring", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "ADJA", "NN", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}