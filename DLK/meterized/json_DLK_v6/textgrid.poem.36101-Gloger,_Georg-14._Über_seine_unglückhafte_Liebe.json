{"textgrid.poem.36101": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "14. \u00dcber seine ungl\u00fcckhafte Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar deiner Liebe Gift verf\u00e4ngt bei mir ganz kr\u00e4ftig,", "tokens": ["Zwar", "dei\u00b7ner", "Lie\u00b7be", "Gift", "ver\u00b7f\u00e4ngt", "bei", "mir", "ganz", "kr\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimmt Blut und Sinnen ein, ent\u00e4dert Leib und Geist.", "tokens": ["Nimmt", "Blut", "und", "Sin\u00b7nen", "ein", ",", "en\u00b7t\u00e4\u00b7dert", "Leib", "und", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$,", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Worte sind gew\u00fcrzt; die Briefe treflich heftig;", "tokens": ["Die", "Wor\u00b7te", "sind", "ge\u00b7w\u00fcrzt", ";", "die", "Brie\u00b7fe", "tref\u00b7lich", "hef\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Feder streicht den Fuchs; die Dinte funkt und glei\u00dft.", "tokens": ["Die", "Fe\u00b7der", "streicht", "den", "Fuchs", ";", "die", "Din\u00b7te", "funkt", "und", "glei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NE", "$.", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja selbst der s\u00fc\u00dfe Mund ber\u00fccket mir das K\u00fcssen;", "tokens": ["Ja", "selbst", "der", "s\u00fc\u00b7\u00dfe", "Mund", "be\u00b7r\u00fc\u00b7cket", "mir", "das", "K\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der \u00c4uglein F\u00e4lknerei, die in die meinen sticht,", "tokens": ["Der", "\u00c4ug\u00b7lein", "F\u00e4lk\u00b7ne\u00b7rei", ",", "die", "in", "die", "mei\u00b7nen", "sticht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "ART", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Hand, darein ich mich pfleg' \u00f6fters ein zu schlie\u00dfen,", "tokens": ["Die", "Hand", ",", "da\u00b7rein", "ich", "mich", "pfleg'", "\u00f6f\u00b7ters", "ein", "zu", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "PRF", "VVFIN", "ADV", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist wol, wenn du nicht z\u00fcrnst, zum Kloben abgericht.", "tokens": ["Ist", "wol", ",", "wenn", "du", "nicht", "z\u00fcrnst", ",", "zum", "Klo\u00b7ben", "ab\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie dr\u00fcckt mich an die Brust, sie l\u00e4sset mich sich lieben,", "tokens": ["Sie", "dr\u00fcckt", "mich", "an", "die", "Brust", ",", "sie", "l\u00e4s\u00b7set", "mich", "sich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie ich nur selber wil. Doch rewt dich bald der Kauf.", "tokens": ["Wie", "ich", "nur", "sel\u00b7ber", "wil", ".", "Doch", "rewt", "dich", "bald", "der", "Kauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VMFIN", "$.", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn meine Trewe sich am besten denkt zu \u00fcben,", "tokens": ["Wenn", "mei\u00b7ne", "Tre\u00b7we", "sich", "am", "bes\u00b7ten", "denkt", "zu", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "PTKA", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So sagestu alsbald mir alle Freundschaft auf.", "tokens": ["So", "sa\u00b7ge\u00b7stu", "als\u00b7bald", "mir", "al\u00b7le", "Freund\u00b7schaft", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O Herze, wo bleibst du, da\u00df ich dein nicht werd innen?", "tokens": ["O", "Her\u00b7ze", ",", "wo", "bleibst", "du", ",", "da\u00df", "ich", "dein", "nicht", "werd", "in\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "PTKNEG", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich gleube, weil du teuschst, und nur die falschen Sinnen", "tokens": ["Ich", "gleu\u00b7be", ",", "weil", "du", "teu\u00b7schst", ",", "und", "nur", "die", "fal\u00b7schen", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Auf Au\u00dfenwerk sind klug, so f\u00e4llet dir nicht ein,", "tokens": ["Auf", "Au\u00b7\u00dfen\u00b7werk", "sind", "klug", ",", "so", "f\u00e4l\u00b7let", "dir", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df auch das Innere darneben m\u00fcsse sein.", "tokens": ["Da\u00df", "auch", "das", "In\u00b7ne\u00b7re", "dar\u00b7ne\u00b7ben", "m\u00fcs\u00b7se", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PAV", "VMFIN", "VAINF", "$."], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}}}}}