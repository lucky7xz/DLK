{"textgrid.poem.66938": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Not", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Deutschland, mein Deutschland,", "tokens": ["O", "Deutschland", ",", "mein", "Deutschland", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was hast du erlebt!", "tokens": ["Was", "hast", "du", "er\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dein Himmel brach nieder,", "tokens": ["Dein", "Him\u00b7mel", "brach", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dein Boden erbebt.", "tokens": ["Dein", "Bo\u00b7den", "er\u00b7bebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Auf t\u00f6nernen F\u00fc\u00dfen", "tokens": ["Auf", "t\u00f6\u00b7ner\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zerschellte dein Thron,", "tokens": ["Zer\u00b7schell\u00b7te", "dein", "Thron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Flammen des Aufruhrs,", "tokens": ["Die", "Flam\u00b7men", "des", "Auf\u00b7ruhrs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie flackern, sie lohn.", "tokens": ["Sie", "fla\u00b7ckern", ",", "sie", "lohn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Dein Volk ist zerfallen", "tokens": ["Dein", "Volk", "ist", "zer\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In feindlichem Zwist,", "tokens": ["In", "feind\u00b7li\u00b7chem", "Zwist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Br\u00fcder sich morden", "tokens": ["Die", "Br\u00fc\u00b7der", "sich", "mor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Voll Wut und voll List.", "tokens": ["Voll", "Wut", "und", "voll", "List", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Feig schleicht die Verleumdung,", "tokens": ["Feig", "schleicht", "die", "Ver\u00b7leum\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Frech z\u00fcngelt Verrat,", "tokens": ["Frech", "z\u00fcn\u00b7gelt", "Ver\u00b7rat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Satan der Zwietracht", "tokens": ["Der", "Sa\u00b7tan", "der", "Zwiet\u00b7racht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "S\u00e4t giftige Saat.", "tokens": ["S\u00e4t", "gif\u00b7ti\u00b7ge", "Saat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Es meuchelt die Kugel", "tokens": ["Es", "meu\u00b7chelt", "die", "Ku\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Aus rasender Hand,", "tokens": ["Aus", "ra\u00b7sen\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die R\u00e4uber und Pl\u00fcndrer", "tokens": ["Die", "R\u00e4u\u00b7ber", "und", "Pl\u00fcnd\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sind Herren im Land.", "tokens": ["Sind", "Her\u00b7ren", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Die heiligen Gr\u00fcfte", "tokens": ["Die", "hei\u00b7li\u00b7gen", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unsterblicher Zeit,", "tokens": ["U\u00b7nsterb\u00b7li\u00b7cher", "Zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "Sie werden gesch\u00e4ndet", "tokens": ["Sie", "wer\u00b7den", "ge\u00b7sch\u00e4n\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und schamlos entweiht.", "tokens": ["Und", "scham\u00b7los", "ent\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "An goldenen Kr\u00e4nzen", "tokens": ["An", "gol\u00b7de\u00b7nen", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Vergreift sich die Gier,", "tokens": ["Ver\u00b7greift", "sich", "die", "Gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Im Schlamme will schwelgen", "tokens": ["Im", "Schlam\u00b7me", "will", "schwel\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das trunkene Tier.", "tokens": ["Das", "trun\u00b7ke\u00b7ne", "Tier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Ergaunerte G\u00fcter", "tokens": ["Er\u00b7gau\u00b7ner\u00b7te", "G\u00fc\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schiebt strotzend die Flut,", "tokens": ["Schiebt", "strot\u00b7zend", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein prassender P\u00f6bel", "tokens": ["Ein", "pras\u00b7sen\u00b7der", "P\u00f6\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Beschmutzt, was er tut.", "tokens": ["Be\u00b7schmutzt", ",", "was", "er", "tut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Was m\u00fchsam erworben,", "tokens": ["Was", "m\u00fch\u00b7sam", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Entwertet der Kauf,", "tokens": ["Ent\u00b7wer\u00b7tet", "der", "Kauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das Brot des Gerechten,", "tokens": ["Das", "Brot", "des", "Ge\u00b7rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Wucher fri\u00dft's auf.", "tokens": ["Der", "Wu\u00b7cher", "fri\u00dft's", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Den schaffenden Geistern", "tokens": ["Den", "schaf\u00b7fen\u00b7den", "Geis\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verscharrt ist der Hort,", "tokens": ["Ver\u00b7scharrt", "ist", "der", "Hort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Not und die Sorgen", "tokens": ["Die", "Not", "und", "die", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ersticken ihr Wort.", "tokens": ["Er\u00b7sti\u00b7cken", "ihr", "Wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Rachs\u00fcchtiger Friede", "tokens": ["Rach\u00b7s\u00fcch\u00b7ti\u00b7ger", "Frie\u00b7de"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zerr\u00fcttet das Reich,", "tokens": ["Zer\u00b7r\u00fct\u00b7tet", "das", "Reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es saugen die Sieger", "tokens": ["Es", "sau\u00b7gen", "die", "Sie\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dem Schemen dich gleich ...", "tokens": ["Dem", "Sche\u00b7men", "dich", "gleich", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Und dennoch, o Deutschland,", "tokens": ["Und", "den\u00b7noch", ",", "o", "Deutschland", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Kein Elend verschlingt,", "tokens": ["Kein", "E\u00b7lend", "ver\u00b7schlingt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kein Frevel ein Lichtvolk,", "tokens": ["Kein", "Fre\u00b7vel", "ein", "Licht\u00b7volk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das frei sich bezwingt.", "tokens": ["Das", "frei", "sich", "be\u00b7zwingt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}