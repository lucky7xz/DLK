{"textgrid.poem.56204": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter, Mutter! unsre Schwalben \u2013", "tokens": ["Mut\u00b7ter", ",", "Mut\u00b7ter", "!", "uns\u00b7re", "Schwal\u00b7ben", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh doch selber, Mutter, sieh!", "tokens": ["Sieh", "doch", "sel\u00b7ber", ",", "Mut\u00b7ter", ",", "sieh", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "NN", "$,", "VVIMP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Junge haben sie bekommen,", "tokens": ["Jun\u00b7ge", "ha\u00b7ben", "sie", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Alten f\u00fcttern sie.", "tokens": ["Und", "die", "Al\u00b7ten", "f\u00fct\u00b7tern", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als die lieben kleinen Schwalben", "tokens": ["Als", "die", "lie\u00b7ben", "klei\u00b7nen", "Schwal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wundervoll ihr Nest gebaut,", "tokens": ["Wun\u00b7der\u00b7voll", "ihr", "Nest", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab ich stundenlang am Fenster", "tokens": ["Hab", "ich", "stun\u00b7den\u00b7lang", "am", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimlich sinnend zugeschaut;", "tokens": ["Heim\u00b7lich", "sin\u00b7nend", "zu\u00b7ge\u00b7schaut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie erst sie eingerichtet", "tokens": ["Und", "wie", "erst", "sie", "ein\u00b7ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "PPER", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und bewohnt das kleine Haus,", "tokens": ["Und", "be\u00b7wohnt", "das", "klei\u00b7ne", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben sie nach mir geschauet", "tokens": ["Ha\u00b7ben", "sie", "nach", "mir", "ge\u00b7schau\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Gar verst\u00e4ndig klug hinaus.", "tokens": ["Gar", "ver\u00b7st\u00e4n\u00b7dig", "klug", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, es schien sie h\u00e4tten gerne", "tokens": ["Ja", ",", "es", "schien", "sie", "h\u00e4t\u00b7ten", "ger\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manches heimlich mir erz\u00e4hlt,", "tokens": ["Man\u00b7ches", "heim\u00b7lich", "mir", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es habe sie betr\u00fcbet,", "tokens": ["Und", "es", "ha\u00b7be", "sie", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was zur Rede noch gefehlt.", "tokens": ["Was", "zur", "Re\u00b7de", "noch", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Also hab ich, liebe Schwalben,", "tokens": ["Al\u00b7so", "hab", "ich", ",", "lie\u00b7be", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unverdrossen euch belauscht,", "tokens": ["Un\u00b7ver\u00b7dros\u00b7sen", "euch", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr habt, mit euren R\u00e4tseln,", "tokens": ["Und", "ihr", "habt", ",", "mit", "eu\u00b7ren", "R\u00e4t\u00b7seln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderseltsam mich berauscht;", "tokens": ["Wun\u00b7der\u00b7selt\u00b7sam", "mich", "be\u00b7rauscht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jetzt erst, jetzt hat das Geheimnis,", "tokens": ["Jetzt", "erst", ",", "jetzt", "hat", "das", "Ge\u00b7heim\u00b7nis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das ihr meintet, sich enth\u00fcllt,", "tokens": ["Das", "ihr", "mein\u00b7tet", ",", "sich", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eure heimlich s\u00fc\u00dfe Hoffnung", "tokens": ["Eu\u00b7re", "heim\u00b7lich", "s\u00fc\u00b7\u00dfe", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat sich freudig euch erf\u00fcllt.", "tokens": ["Hat", "sich", "freu\u00b7dig", "euch", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh doch hin! die beiden Alten", "tokens": ["Sieh", "doch", "hin", "!", "die", "bei\u00b7den", "Al\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ", "$.", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringen ihnen Nahrung dar.", "tokens": ["Brin\u00b7gen", "ih\u00b7nen", "Nah\u00b7rung", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gibt es S\u00fc\u00dferes auf Erden,", "tokens": ["Gibt", "es", "S\u00fc\u00b7\u00dfe\u00b7res", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein solches Schwalbenpaar!", "tokens": ["Als", "ein", "sol\u00b7ches", "Schwal\u00b7ben\u00b7paar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}