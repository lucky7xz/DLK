{"textgrid.poem.37618": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freunde, weihet den Pokal", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freunde, weihet den Pokal", "tokens": ["Freun\u00b7de", ",", "wei\u00b7het", "den", "Po\u00b7kal"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener fremden Menschenwelt,", "tokens": ["Je\u00b7ner", "frem\u00b7den", "Men\u00b7schen\u00b7welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die an gleichem Sonnenstrahl", "tokens": ["Die", "an", "glei\u00b7chem", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich erhellt, gesellt, gef\u00e4llt;", "tokens": ["Sich", "er\u00b7hellt", ",", "ge\u00b7sellt", ",", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00fcck den lieben unbekannten", "tokens": ["Gl\u00fcck", "den", "lie\u00b7ben", "un\u00b7be\u00b7kann\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lichtgesandten Herzverwandten,", "tokens": ["Licht\u00b7ge\u00b7sand\u00b7ten", "Herz\u00b7ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deren Augen \u00fcbergehen,", "tokens": ["De\u00b7ren", "Au\u00b7gen", "\u00fc\u00b7ber\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wen sie in die Sonne sehen. \u2013", "tokens": ["Wen", "sie", "in", "die", "Son\u00b7ne", "se\u00b7hen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}