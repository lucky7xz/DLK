{"dta.poem.23428": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "\u201efort mit Schaden!\u201c", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir lieben die uns hassen \u2014", "tokens": ["Wir", "lie\u00b7ben", "die", "uns", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sonst lie\u00dfe sich's nicht fassen,", "tokens": ["Sonst", "lie\u00b7\u00dfe", "sich's", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df man eine franz\u00f6sische Schauspielerbande", "tokens": ["Da\u00df", "man", "ei\u00b7ne", "fran\u00b7z\u00f6\u00b7si\u00b7sche", "Schau\u00b7spie\u00b7ler\u00b7ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "F\u00fcrstlich besoldet in einem deutschen Lande.", "tokens": ["F\u00fcrst\u00b7lich", "be\u00b7sol\u00b7det", "in", "ei\u00b7nem", "deut\u00b7schen", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Doch ist es eine Blamage \u2014", "tokens": ["Doch", "ist", "es", "ei\u00b7ne", "Bla\u00b7ma\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von dieser Schauspielergage", "tokens": ["Von", "die\u00b7ser", "Schau\u00b7spie\u00b7ler\u00b7ga\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "K\u00f6nnten alle Berliner aus der Grammaire", "tokens": ["K\u00f6nn\u00b7ten", "al\u00b7le", "Ber\u00b7li\u00b7ner", "aus", "der", "Gram\u00b7mai\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Lernen wo ", "tokens": ["Ler\u00b7nen", "wo"], "token_info": ["word", "word"], "pos": ["NN", "PWAV"], "meter": "+-+", "measure": "trochaic.di"}}}}}