{"textgrid.poem.37384": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fritz, der mal wieder schrecklich tr\u00e4ge,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fritz, der mal wieder schrecklich tr\u00e4ge,", "tokens": ["Fritz", ",", "der", "mal", "wie\u00b7der", "schreck\u00b7lich", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vermutet, heute gibt es Schl\u00e4ge,", "tokens": ["Ver\u00b7mu\u00b7tet", ",", "heu\u00b7te", "gibt", "es", "Schl\u00e4\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und kn\u00f6pft zur Abwehr der Attacke", "tokens": ["Und", "kn\u00f6pft", "zur", "Ab\u00b7wehr", "der", "At\u00b7ta\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Buch sich unter seine Jacke,", "tokens": ["Ein", "Buch", "sich", "un\u00b7ter", "sei\u00b7ne", "Ja\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil er sich in dem Glauben wiegt,", "tokens": ["Weil", "er", "sich", "in", "dem", "Glau\u00b7ben", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er was auf den Buckel kriegt.", "tokens": ["Da\u00df", "er", "was", "auf", "den", "Bu\u00b7ckel", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Schl\u00e4ge trafen richtig ein.", "tokens": ["Die", "Schl\u00e4\u00b7ge", "tra\u00b7fen", "rich\u00b7tig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Lehrer meint es gut. Allein", "tokens": ["Der", "Leh\u00b7rer", "meint", "es", "gut", ".", "Al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Gabe wird f\u00fcr heut gespendet", "tokens": ["Die", "Ga\u00b7be", "wird", "f\u00fcr", "heut", "ge\u00b7spen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mehr unten, wo die Jacke endet,", "tokens": ["Mehr", "un\u00b7ten", ",", "wo", "die", "Ja\u00b7cke", "en\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wo Fritz nur \u00e4u\u00dferst leicht bekleidet", "tokens": ["Wo", "Fritz", "nur", "\u00e4u\u00b7\u00dferst", "leicht", "be\u00b7klei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und darum ganz besonders leidet.", "tokens": ["Und", "da\u00b7rum", "ganz", "be\u00b7son\u00b7ders", "lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ach, da\u00df der Mensch so h\u00e4ufig irrt", "tokens": ["Ach", ",", "da\u00df", "der", "Mensch", "so", "h\u00e4u\u00b7fig", "irrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und nie recht wei\u00df, was kommen wird!", "tokens": ["Und", "nie", "recht", "wei\u00df", ",", "was", "kom\u00b7men", "wird", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$,", "PWS", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}