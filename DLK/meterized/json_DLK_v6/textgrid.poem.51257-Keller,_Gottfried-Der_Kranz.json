{"textgrid.poem.51257": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranz", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling ging durchs reiche Schwabenland", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ging", "durchs", "rei\u00b7che", "Schwa\u00b7ben\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und mit ihm Ludwig Uhland, an der Hand", "tokens": ["Und", "mit", "ihm", "Lud\u00b7wig", "Uh\u00b7land", ",", "an", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "NE", "NE", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die treue Gattin; denn es kam zu wandern", "tokens": ["Die", "treu\u00b7e", "Gat\u00b7tin", ";", "denn", "es", "kam", "zu", "wan\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der teure Mann von einem Ort zum andern.", "tokens": ["Der", "teu\u00b7re", "Mann", "von", "ei\u00b7nem", "Ort", "zum", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mag's mit dem Recht in Stuttgart nicht gelingen,", "tokens": ["Mag's", "mit", "dem", "Recht", "in", "Stutt\u00b7gart", "nicht", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will lehrend er ins Herz der Jugend dringen", "tokens": ["Will", "leh\u00b7rend", "er", "ins", "Herz", "der", "Ju\u00b7gend", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPER", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu T\u00fcbingen am alten Musensitz,", "tokens": ["Zu", "T\u00fc\u00b7bin\u00b7gen", "am", "al\u00b7ten", "Mu\u00b7sen\u00b7sitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umleuchtet noch von hellem Geisterblitz.", "tokens": ["Um\u00b7leuch\u00b7tet", "noch", "von", "hel\u00b7lem", "Geis\u00b7ter\u00b7blitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So wallt das Paar still und getrost dahin,", "tokens": ["So", "wallt", "das", "Paar", "still", "und", "ge\u00b7trost", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo T\u00e4ler wei\u00df im Schnee der B\u00e4ume bl\u00fchn;", "tokens": ["Wo", "T\u00e4\u00b7ler", "wei\u00df", "im", "Schnee", "der", "B\u00e4u\u00b7me", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch sieh! beim Steine, der die Markung k\u00fcndet,", "tokens": ["Doch", "sieh", "!", "beim", "Stei\u00b7ne", ",", "der", "die", "Mar\u00b7kung", "k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steht eine Schar von Freunden treu verb\u00fcndet.", "tokens": ["Steht", "ei\u00b7ne", "Schar", "von", "Freun\u00b7den", "treu", "ver\u00b7b\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Kampfgenossen f\u00fcr des Volkes Rechte,", "tokens": ["Die", "Kampf\u00b7ge\u00b7nos\u00b7sen", "f\u00fcr", "des", "Vol\u00b7kes", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie harren sein mit einem Kranzgeflechte", "tokens": ["Sie", "har\u00b7ren", "sein", "mit", "ei\u00b7nem", "Kranz\u00b7ge\u00b7flech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von dichtem Lorbeer, gl\u00e4nzend frisch und gr\u00fcn;", "tokens": ["Von", "dich\u00b7tem", "Lor\u00b7beer", ",", "gl\u00e4n\u00b7zend", "frisch", "und", "gr\u00fcn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den reichen sie dem S\u00e4nger hold und k\u00fchn.", "tokens": ["Den", "rei\u00b7chen", "sie", "dem", "S\u00e4n\u00b7ger", "hold", "und", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein letzter Ku\u00df! Der letzte Becher blinkt,", "tokens": ["Ein", "letz\u00b7ter", "Ku\u00df", "!", "Der", "letz\u00b7te", "Be\u00b7cher", "blinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ferne schon die Hand zum Scheiden winkt;", "tokens": ["Und", "fer\u00b7ne", "schon", "die", "Hand", "zum", "Schei\u00b7den", "winkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Meister gl\u00e4nzt das Aug, das lebenswarme,", "tokens": ["Dem", "Meis\u00b7ter", "gl\u00e4nzt", "das", "Aug", ",", "das", "le\u00b7bens\u00b7war\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Frau und Kranz f\u00fchrt er am rechten Arme.", "tokens": ["Und", "Frau", "und", "Kranz", "f\u00fchrt", "er", "am", "rech\u00b7ten", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sie wandeln bald in einem lichten Walde", "tokens": ["Sie", "wan\u00b7deln", "bald", "in", "ei\u00b7nem", "lich\u00b7ten", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von gro\u00dfen Eichen an der sanften Halde;", "tokens": ["Von", "gro\u00b7\u00dfen", "Ei\u00b7chen", "an", "der", "sanf\u00b7ten", "Hal\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie steht so fest und frei der edle Hain,", "tokens": ["Wie", "steht", "so", "fest", "und", "frei", "der", "ed\u00b7le", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcberall blaut noch der Himmel drein!", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "blaut", "noch", "der", "Him\u00b7mel", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Hoch oben kreist der Falk im Sonnenlicht,", "tokens": ["Hoch", "o\u00b7ben", "kreist", "der", "Falk", "im", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das durch das Gitterwerk der Zweige bricht,", "tokens": ["Das", "durch", "das", "Git\u00b7ter\u00b7werk", "der", "Zwei\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Uhland, schreitend im geweihten Raume,", "tokens": ["Und", "Uh\u00b7land", ",", "schrei\u00b7tend", "im", "ge\u00b7weih\u00b7ten", "Rau\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tritt unversehns zum n\u00e4chsten Eichenbaume.", "tokens": ["Tritt", "un\u00b7ver\u00b7sehns", "zum", "n\u00e4chs\u00b7ten", "Ei\u00b7chen\u00b7bau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Rasch h\u00e4ngt er auf den Kranz, und schweigend wendet", "tokens": ["Rasch", "h\u00e4ngt", "er", "auf", "den", "Kranz", ",", "und", "schwei\u00b7gend", "wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schritt er weiter; nur Frau Emma sendet", "tokens": ["Den", "Schritt", "er", "wei\u00b7ter", ";", "nur", "Frau", "Em\u00b7ma", "sen\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$.", "ADV", "NN", "NE", "VVFIN"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Reuig den Blick zur\u00fcck, doch strahlend licht", "tokens": ["Reu\u00b7ig", "den", "Blick", "zu\u00b7r\u00fcck", ",", "doch", "strah\u00b7lend", "licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird drauf ihr Aug, sieht sie den Mann so schlicht.", "tokens": ["Wird", "drauf", "ihr", "Aug", ",", "sieht", "sie", "den", "Mann", "so", "schlicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Tief schaut sie dieses reinen Goldes Hort", "tokens": ["Tief", "schaut", "sie", "die\u00b7ses", "rei\u00b7nen", "Gol\u00b7des", "Hort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In seinem Herzen \u2013 doch mit keinem Wort", "tokens": ["In", "sei\u00b7nem", "Her\u00b7zen", "\u2013", "doch", "mit", "kei\u00b7nem", "Wort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird sie benennen ihr begl\u00fcckend Wissen", "tokens": ["Wird", "sie", "be\u00b7nen\u00b7nen", "ihr", "be\u00b7gl\u00fc\u00b7ckend", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "PPER", "VVPP", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von einem Schatz, den tausend Frauen missen.", "tokens": ["Von", "ei\u00b7nem", "Schatz", ",", "den", "tau\u00b7send", "Frau\u00b7en", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Im Waldesd\u00e4mmer an dem grauen Stamme", "tokens": ["Im", "Wal\u00b7des\u00b7d\u00e4m\u00b7mer", "an", "dem", "grau\u00b7en", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlassen glimmt des Lorbeers gr\u00fcne Flamme.", "tokens": ["Ver\u00b7las\u00b7sen", "glimmt", "des", "Lor\u00b7beers", "gr\u00fc\u00b7ne", "Flam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor\u00fcber zog das Wanderpaar schon lang,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "zog", "das", "Wan\u00b7der\u00b7paar", "schon", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und laut erschallt im Hain der Vogelsang!", "tokens": ["Und", "laut", "er\u00b7schallt", "im", "Hain", "der", "Vo\u00b7gel\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}