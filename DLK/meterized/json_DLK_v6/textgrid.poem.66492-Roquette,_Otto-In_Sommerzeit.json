{"textgrid.poem.66492": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "In Sommerzeit", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein Knabe gezogen", "tokens": ["Es", "war", "ein", "Kna\u00b7be", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wohl in die Welt hinaus,", "tokens": ["Wohl", "in", "die", "Welt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ob ihm sein Schatz gewogen,", "tokens": ["Und", "ob", "ihm", "sein", "Schatz", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Gl\u00fcck, das Gl\u00fcck war aus.", "tokens": ["Das", "Gl\u00fcck", ",", "das", "Gl\u00fcck", "war", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er wanderte weit", "tokens": ["Er", "wan\u00b7der\u00b7te", "weit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "In Sommerzeit", "tokens": ["In", "Som\u00b7mer\u00b7zeit"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wenn am Walde die Rosen bl\u00fchn.", "tokens": ["Wenn", "am", "Wal\u00b7de", "die", "Ro\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Was bist du so hoch geboren,", "tokens": ["Was", "bist", "du", "so", "hoch", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und ich nur ein armer Knab?", "tokens": ["Und", "ich", "nur", "ein", "ar\u00b7mer", "Knab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Liebe, die Lieb' ist verloren,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "Lieb'", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Welt ist leer wie ein Grab!", "tokens": ["Die", "Welt", "ist", "leer", "wie", "ein", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Was soll mir im Leid", "tokens": ["Was", "soll", "mir", "im", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Sommerzeit,", "tokens": ["Die", "Som\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wenn am Walde die Rosen bl\u00fchn!", "tokens": ["Wenn", "am", "Wal\u00b7de", "die", "Ro\u00b7sen", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Das M\u00e4gdlein barg ihre Klagen", "tokens": ["Das", "M\u00e4gd\u00b7lein", "barg", "ih\u00b7re", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im stillen K\u00e4mmerlein,", "tokens": ["Im", "stil\u00b7len", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und durfte sie's Keinem sagen,", "tokens": ["Und", "durf\u00b7te", "sie's", "Kei\u00b7nem", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PIS", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie hoffte jahraus, jahrein.", "tokens": ["Sie", "hoff\u00b7te", "ja\u00b7hraus", ",", "ja\u00b7hrein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PTKANT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sah \u00fcber die Haid'", "tokens": ["Sah", "\u00fc\u00b7ber", "die", "Haid'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "In Sommerzeit,", "tokens": ["In", "Som\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wenn am Walde die Rosen bl\u00fchn.", "tokens": ["Wenn", "am", "Wal\u00b7de", "die", "Ro\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Es kam ein Reiter geflogen,", "tokens": ["Es", "kam", "ein", "Rei\u00b7ter", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seine Locken wehten im Wind.", "tokens": ["Sei\u00b7ne", "Lo\u00b7cken", "weh\u00b7ten", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und bist du mir noch gewogen,", "tokens": ["Und", "bist", "du", "mir", "noch", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Herzallerliebstes Kind?", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7tes", "Kind", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vergi\u00df das Leid", "tokens": ["Ver\u00b7gi\u00df", "das", "Leid"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "In Sommerzeit,", "tokens": ["In", "Som\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wenn am Walde die Rosen bl\u00fchn!", "tokens": ["Wenn", "am", "Wal\u00b7de", "die", "Ro\u00b7sen", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Er hielt sie in seinen Armen,", "tokens": ["Er", "hielt", "sie", "in", "sei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr Herz vor Wonne schlug,", "tokens": ["Ihr", "Herz", "vor", "Won\u00b7ne", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hatte die Welt nicht Erbarmen,", "tokens": ["Und", "hat\u00b7te", "die", "Welt", "nicht", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Liebe war stark genug.", "tokens": ["Die", "Lie\u00b7be", "war", "stark", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und sie lachten beid", "tokens": ["Und", "sie", "lach\u00b7ten", "beid"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wie die Sommerzeit,", "tokens": ["Wie", "die", "Som\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn am Walde die Rosen bl\u00fchn.", "tokens": ["Wenn", "am", "Wal\u00b7de", "die", "Ro\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}