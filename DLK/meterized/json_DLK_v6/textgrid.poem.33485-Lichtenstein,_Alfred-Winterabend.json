{"textgrid.poem.33485": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Winterabend", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In gelben Fenstern trinken Schatten hei\u00dfen Tee.", "tokens": ["In", "gel\u00b7ben", "Fens\u00b7tern", "trin\u00b7ken", "Schat\u00b7ten", "hei\u00b7\u00dfen", "Tee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sehns\u00fcchtge wiegen sich auf hartem Schimmerteiche.", "tokens": ["Sehn\u00b7s\u00fccht\u00b7ge", "wie\u00b7gen", "sich", "auf", "har\u00b7tem", "Schim\u00b7mer\u00b7tei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Arbeiter finden eine sanfte Damenleiche.", "tokens": ["Ar\u00b7bei\u00b7ter", "fin\u00b7den", "ei\u00b7ne", "sanf\u00b7te", "Da\u00b7men\u00b7lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Johlende Dunkle werfen glimmend blauen Schnee.", "tokens": ["Joh\u00b7len\u00b7de", "Dunk\u00b7le", "wer\u00b7fen", "glim\u00b7mend", "blau\u00b7en", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "An hohen Stangen h\u00e4ngt, verfleht, ein Streichholzmann.", "tokens": ["An", "ho\u00b7hen", "Stan\u00b7gen", "h\u00e4ngt", ",", "ver\u00b7fleht", ",", "ein", "Streich\u00b7holz\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kaufl\u00e4den flackern tr\u00fcb durch frostbeschlagne Scheiben,", "tokens": ["Kauf\u00b7l\u00e4\u00b7den", "fla\u00b7ckern", "tr\u00fcb", "durch", "frost\u00b7be\u00b7schlag\u00b7ne", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor denen Menschenleiber wie Gespenster treiben.", "tokens": ["Vor", "de\u00b7nen", "Men\u00b7schen\u00b7lei\u00b7ber", "wie", "Ge\u00b7spens\u00b7ter", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Studenten schneiden ein erfrornes M\u00e4dchen an.", "tokens": ["Stu\u00b7den\u00b7ten", "schnei\u00b7den", "ein", "er\u00b7fror\u00b7nes", "M\u00e4d\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie lieblich der kristallne Winterabend brennt!", "tokens": ["Wie", "lieb\u00b7lich", "der", "kris\u00b7tall\u00b7ne", "Win\u00b7ter\u00b7a\u00b7bend", "brennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schon str\u00f6mt ein Platinmond durch eine H\u00e4userl\u00fccke.", "tokens": ["Schon", "str\u00f6mt", "ein", "Pla\u00b7tin\u00b7mond", "durch", "ei\u00b7ne", "H\u00e4u\u00b7ser\u00b7l\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bei gr\u00fcnlichen Laternen unter einer Br\u00fccke", "tokens": ["Bei", "gr\u00fcn\u00b7li\u00b7chen", "La\u00b7ter\u00b7nen", "un\u00b7ter", "ei\u00b7ner", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Liegt ein Zigeunerweib. Und spielt ein Instrument.", "tokens": ["Liegt", "ein", "Zi\u00b7geu\u00b7ner\u00b7weib", ".", "Und", "spielt", "ein", "Inst\u00b7ru\u00b7ment", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}