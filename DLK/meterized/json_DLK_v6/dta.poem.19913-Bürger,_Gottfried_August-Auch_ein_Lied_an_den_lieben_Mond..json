{"dta.poem.19913": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Auch ein Lied an den lieben Mond.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Ei! sch\u00f6nen guten Abend dort am Himmel!               ", "tokens": ["Ei", "!", "sch\u00f6\u00b7nen", "gu\u00b7ten", "A\u00b7bend", "dort", "am", "Him\u00b7mel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "ADJA", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man freuet sich, Ihn noch fein wohl zu sehn.", "tokens": ["Man", "freu\u00b7et", "sich", ",", "Ihn", "noch", "fein", "wohl", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,", "PPER", "ADV", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wilkommen mir, vor allem Sterngewimmel!", "tokens": ["Wil\u00b7kom\u00b7men", "mir", ",", "vor", "al\u00b7lem", "Stern\u00b7ge\u00b7wim\u00b7mel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor allem Sterngewimmel lieb und sch\u00f6n! \u2014", "tokens": ["Vor", "al\u00b7lem", "Stern\u00b7ge\u00b7wim\u00b7mel", "lieb", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIS", "NN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was l\u00e4chelst du so bitlich her, mein Theurer?", "tokens": ["Was", "l\u00e4\u00b7chelst", "du", "so", "bit\u00b7lich", "her", ",", "mein", "Theu\u00b7rer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wilst du vielleicht so was von Sing und Sang?", "tokens": ["Wilst", "du", "viel\u00b7leicht", "so", "was", "von", "Sing", "und", "Sang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PWS", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ganz recht! Wof\u00fcr auch w\u00e4r\u2019 ich sonst der Leirer,", "tokens": ["Ganz", "recht", "!", "Wo\u00b7f\u00fcr", "auch", "w\u00e4r'", "ich", "sonst", "der", "Lei\u00b7rer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "NN", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Saitenspiel bisher \u2014 so so! \u2014 noch klang?", "tokens": ["Das", "Sai\u00b7ten\u00b7spiel", "bis\u00b7her", "so", "so", "!", "noch", "klang", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "ADV", "ADV", "$.", "$(", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Es w\u00e4re ja nicht halb mir zu verzeihen,", "tokens": ["Es", "w\u00e4\u00b7re", "ja", "nicht", "halb", "mir", "zu", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das mus ich selbst treuherzig eingestehn,", "tokens": ["Das", "mus", "ich", "selbst", "treu\u00b7her\u00b7zig", "ein\u00b7ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da alle Dichter dir ein Sch\u00e4rflein weihen,", "tokens": ["Da", "al\u00b7le", "Dich\u00b7ter", "dir", "ein", "Sch\u00e4r\u00b7flein", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wolt\u2019 ich allein dich stum vor\u00fcber gehn.", "tokens": ["Wolt'", "ich", "al\u00b7lein", "dich", "stum", "vor\u00b7\u00fc\u00b7ber", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Besonders da ich jezt mit einem Bande,", "tokens": ["Be\u00b7son\u00b7ders", "da", "ich", "jezt", "mit", "ei\u00b7nem", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vol meiner Reimereien, her und hin,", "tokens": ["Vol", "mei\u00b7ner", "Rei\u00b7me\u00b7rei\u00b7en", ",", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im ganzen wehrten teutschen Vaterlande,", "tokens": ["Im", "gan\u00b7zen", "wehr\u00b7ten", "teut\u00b7schen", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hausiren umzugehn entschlossen hin.", "tokens": ["Hau\u00b7si\u00b7ren", "um\u00b7zu\u00b7gehn", "ent\u00b7schlos\u00b7sen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVIZU", "VVFIN", "PTKVZ", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.5": {"line.1": {"text": "Auch bist du\u2019s wehrt, mein sanfter, holder,", "tokens": ["Auch", "bist", "du's", "wehrt", ",", "mein", "sanf\u00b7ter", ",", "hol\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVFIN", "$,", "PPOSAT", "ADJD", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "lieber \u2012 \u2012 \u2012", "tokens": ["lie\u00b7ber", "\u2012", "\u2012", "\u2012"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "$(", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich weis nicht recht, wie ich dich nennen sol?", "tokens": ["Ich", "weis", "nicht", "recht", ",", "wie", "ich", "dich", "nen\u00b7nen", "sol", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PTKNEG", "ADJD", "$,", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Man oder Weib? \u2014 Schon lange war ich \u00fcber", "tokens": ["Man", "o\u00b7der", "Weib", "?", "Schon", "lan\u00b7ge", "war", "ich", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "KON", "NN", "$.", "$(", "ADV", "ADV", "VAFIN", "PPER", "APPR"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Und \u00fcber deines warmen Lobes vol.", "tokens": ["Und", "\u00fc\u00b7ber", "dei\u00b7nes", "war\u00b7men", "Lo\u00b7bes", "vol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So wissen\u2019s dann die Jungen und die Alten,", "tokens": ["So", "wis\u00b7sen's", "dann", "die", "Jun\u00b7gen", "und", "die", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was immerdar auch meine Wenigkeit", "tokens": ["Was", "im\u00b7mer\u00b7dar", "auch", "mei\u00b7ne", "We\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom sch\u00f6nen lieben Monde hat gehalten,", "tokens": ["Vom", "sch\u00f6\u00b7nen", "lie\u00b7ben", "Mon\u00b7de", "hat", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und halten wird in alle Ewigkeit!", "tokens": ["Und", "hal\u00b7ten", "wird", "in", "al\u00b7le", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Sonn\u2019 ist zwar die K\u00f6nigin der Erden.", "tokens": ["Die", "Sonn'", "ist", "zwar", "die", "K\u00f6\u00b7ni\u00b7gin", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das sey hiermit h\u00f6chstfeierlich erkl\u00e4rt!", "tokens": ["Das", "sey", "hier\u00b7mit", "h\u00f6chst\u00b7fei\u00b7er\u00b7lich", "er\u00b7kl\u00e4rt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PAV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich w\u00e4re ja von ihr begl\u00e4nzt zu werden,", "tokens": ["Ich", "w\u00e4\u00b7re", "ja", "von", "ihr", "be\u00b7gl\u00e4nzt", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verneint\u2019 ich dies, nicht eine Stunde wehrt.", "tokens": ["Ver\u00b7neint'", "ich", "dies", ",", "nicht", "ei\u00b7ne", "Stun\u00b7de", "wehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wer aber kan, wann sie im Stralenwagen", "tokens": ["Wer", "a\u00b7ber", "kan", ",", "wann", "sie", "im", "Stra\u00b7len\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "$,", "PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einher an blauer Himmelsstrasse zieht,", "tokens": ["Ein\u00b7her", "an", "blau\u00b7er", "Him\u00b7mels\u00b7stras\u00b7se", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Glorie in seinem Aug\u2019 ertragen,", "tokens": ["Die", "Glo\u00b7rie", "in", "sei\u00b7nem", "Aug'", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die ihre k\u00f6nigliche Stirn umgl\u00fcht?", "tokens": ["Die", "ih\u00b7re", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Stirn", "um\u00b7gl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Du, lieber Mond, bist schw\u00e4cher zwar und", "tokens": ["Du", ",", "lie\u00b7ber", "Mond", ",", "bist", "schw\u00e4\u00b7cher", "zwar", "und"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ADV", "NN", "$,", "VAFIN", "ADJD", "ADV", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "kleiner,", "tokens": ["klei\u00b7ner", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ein Kleid, nur recht und schlecht, bekleidet dich;", "tokens": ["Ein", "Kleid", ",", "nur", "recht", "und", "schlecht", ",", "be\u00b7klei\u00b7det", "dich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Allein du bist so mehr, wie Unsereiner,", "tokens": ["Al\u00b7lein", "du", "bist", "so", "mehr", ",", "wie", "Un\u00b7ser\u00b7ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADV", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dieses ist gerade recht f\u00fcr mich.", "tokens": ["Und", "die\u00b7ses", "ist", "ge\u00b7ra\u00b7de", "recht", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ich w\u00fcrde mir\u2019s f\u00fcrwahr nicht unterstehen,", "tokens": ["Ich", "w\u00fcr\u00b7de", "mir's", "f\u00fcr\u00b7wahr", "nicht", "un\u00b7ter\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihrer hocherhabnen Majest\u00e4t", "tokens": ["Mit", "ih\u00b7rer", "ho\u00b7cher\u00b7hab\u00b7nen", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So br\u00fcderlich und traulich umzugehen,", "tokens": ["So", "br\u00fc\u00b7der\u00b7lich", "und", "trau\u00b7lich", "um\u00b7zu\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie man noch wol mit dir sich untersteht.", "tokens": ["Wie", "man", "noch", "wol", "mit", "dir", "sich", "un\u00b7ter\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Die Sonne mag uns tausend Segen schenken.", "tokens": ["Die", "Son\u00b7ne", "mag", "uns", "tau\u00b7send", "Se\u00b7gen", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das wissen wir und danken\u2019s herzlich ihr.", "tokens": ["Das", "wis\u00b7sen", "wir", "und", "dan\u00b7ken's", "herz\u00b7lich", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "ADV", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch weis sie auch es wieder einzutr\u00e4nken,", "tokens": ["Doch", "weis", "sie", "auch", "es", "wie\u00b7der", "ein\u00b7zu\u00b7tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sengt und brent oft desto bas daf\u00fcr.", "tokens": ["Und", "sengt", "und", "brent", "oft", "des\u00b7to", "bas", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Du aber, aller Kreaturen Freude!", "tokens": ["Du", "a\u00b7ber", ",", "al\u00b7ler", "Kre\u00b7a\u00b7tu\u00b7ren", "Freu\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den jeder Mund so treu und froh begr\u00fcst,", "tokens": ["Den", "je\u00b7der", "Mund", "so", "treu", "und", "froh", "be\u00b7gr\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "pun