{"textgrid.poem.39105": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der zehnte November 1848", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja wahrlich sie war sch\u00f6n, die Nacht der Barrikaden", "tokens": ["Ja", "wahr\u00b7lich", "sie", "war", "sch\u00f6n", ",", "die", "Nacht", "der", "Bar\u00b7ri\u00b7ka\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "PPER", "VAFIN", "ADJD", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in jenem Monat M\u00e4rz, da, auch von Gottes Gnaden,", "tokens": ["in", "je\u00b7nem", "Mo\u00b7nat", "M\u00e4rz", ",", "da", ",", "auch", "von", "Got\u00b7tes", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,", "KOUS", "$,", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Freiheit auf den Thron sich schwang!", "tokens": ["die", "Frei\u00b7heit", "auf", "den", "Thron", "sich", "schwang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da hell im Mondenschein, voran den dunklen Massen,", "tokens": ["Da", "hell", "im", "Mon\u00b7den\u00b7schein", ",", "vo\u00b7ran", "den", "dunk\u00b7len", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "die Trikolore flog! Da durch die stillen Gassen", "tokens": ["die", "Tri\u00b7ko\u00b7lo\u00b7re", "flog", "!", "Da", "durch", "die", "stil\u00b7len", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der Donner der Kart\u00e4tschen klang!", "tokens": ["der", "Don\u00b7ner", "der", "Kar\u00b7t\u00e4t\u00b7schen", "klang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch sch\u00f6ner jene Nacht, da, Arm in Arm geschlossen,", "tokens": ["Doch", "sch\u00f6\u00b7ner", "je\u00b7ne", "Nacht", ",", "da", ",", "Arm", "in", "Arm", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PDAT", "NN", "$,", "KOUS", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "aufrechten Haupts, umragt von starrenden Geschossen", "tokens": ["auf\u00b7rech\u00b7ten", "Haupts", ",", "um\u00b7ragt", "von", "star\u00b7ren\u00b7den", "Ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Volksvertreter wandelten!", "tokens": ["die", "Volks\u00b7ver\u00b7tre\u00b7ter", "wan\u00b7del\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Da selbst ein Wrangel sich vor Groll den Bart zerw\u00fchlte,", "tokens": ["Da", "selbst", "ein", "Wran\u00b7gel", "sich", "vor", "Groll", "den", "Bart", "zer\u00b7w\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "da in der engen Brust der F\u00fcselier es f\u00fchlte,", "tokens": ["da", "in", "der", "en\u00b7gen", "Brust", "der", "F\u00fc\u00b7se\u00b7lier", "es", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df sie wie M\u00e4nner handelten!", "tokens": ["da\u00df", "sie", "wie", "M\u00e4n\u00b7ner", "han\u00b7del\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "In jener Nacht zuerst, da ist, o F\u00fcrst geschehen,", "tokens": ["In", "je\u00b7ner", "Nacht", "zu\u00b7erst", ",", "da", "ist", ",", "o", "F\u00fcrst", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$,", "ADV", "VAFIN", "$,", "FM", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "was deine S\u00f6ldlinge im Traum der Angst gesehen", "tokens": ["was", "dei\u00b7ne", "S\u00f6ld\u00b7lin\u00b7ge", "im", "Traum", "der", "Angst", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VVPP"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "bei Tag, bei Nacht, seit langer Zeit:", "tokens": ["bei", "Tag", ",", "bei", "Nacht", ",", "seit", "lan\u00b7ger", "Zeit", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da wankte, da zuerst der Grund von deinem Throne,", "tokens": ["Da", "wank\u00b7te", ",", "da", "zu\u00b7erst", "der", "Grund", "von", "dei\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "da zitterte, o F\u00fcrst, auf deinem Haupt die Krone,", "tokens": ["da", "zit\u00b7ter\u00b7te", ",", "o", "F\u00fcrst", ",", "auf", "dei\u00b7nem", "Haupt", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die Krone der Gerechtigkeit! \u2013", "tokens": ["die", "Kro\u00b7ne", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr habt von Haus zu Haus sie fl\u00fcchtig jagen k\u00f6nnen,", "tokens": ["Ihr", "habt", "von", "Haus", "zu", "Haus", "sie", "fl\u00fcch\u00b7tig", "ja\u00b7gen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "NN", "PPER", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "d\u00fcrft ihnen selbst daheim die karge Ruh' mi\u00dfg\u00f6nnen \u2013", "tokens": ["d\u00fcrft", "ih\u00b7nen", "selbst", "da\u00b7heim", "die", "kar\u00b7ge", "Ruh'", "mi\u00df\u00b7g\u00f6n\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr d\u00fcrft es \u2013: denn ihr habt die Macht!", "tokens": ["Ihr", "d\u00fcrft", "es", "\u2013", ":", "denn", "ihr", "habt", "die", "Macht", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$(", "$.", "KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch aber, o bedenkt, da\u00df \u00fcber jedes Haupte,", "tokens": ["Doch", "a\u00b7ber", ",", "o", "be\u00b7denkt", ",", "da\u00df", "\u00fc\u00b7ber", "je\u00b7des", "Haup\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "VVFIN", "$,", "KOUS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "das nie verl\u00f6schende, von euch zwar nicht geglaubte,", "tokens": ["das", "nie", "ver\u00b7l\u00f6\u00b7schen\u00b7de", ",", "von", "euch", "zwar", "nicht", "ge\u00b7glaub\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "$,", "APPR", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "der Freiheit heil'ges Auge wacht! \u2013", "tokens": ["der", "Frei\u00b7heit", "heil'\u00b7ges", "Au\u00b7ge", "wacht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "'s ist nicht das erste Mal, wird nicht das letzte bleiben,", "tokens": ["'s", "ist", "nicht", "das", "ers\u00b7te", "Mal", ",", "wird", "nicht", "das", "letz\u00b7te", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,", "VAFIN", "PTKNEG", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "noch \u00f6fter wird, wie heut, Gewalt das Recht vertreiben,", "tokens": ["noch", "\u00f6f\u00b7ter", "wird", ",", "wie", "heut", ",", "Ge\u00b7walt", "das", "Recht", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$,", "PWAV", "ADV", "$,", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und doch berauscht euch nicht im Gl\u00fcck:", "tokens": ["und", "doch", "be\u00b7rauscht", "euch", "nicht", "im", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ging die Freiheit wohl schon \u00f6fters mit Verbannten,", "tokens": ["Es", "ging", "die", "Frei\u00b7heit", "wohl", "schon", "\u00f6f\u00b7ters", "mit", "Ver\u00b7bann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "in fremdem Sand verweht der Staub der kaum Gekannten,", "tokens": ["in", "frem\u00b7dem", "Sand", "ver\u00b7weht", "der", "Staub", "der", "kaum", "Ge\u00b7kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "sie selber aber kehrt zur\u00fcck!", "tokens": ["sie", "sel\u00b7ber", "a\u00b7ber", "kehrt", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}