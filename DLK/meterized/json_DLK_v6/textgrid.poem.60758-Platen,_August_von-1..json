{"textgrid.poem.60758": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Farbenst\u00e4ubchen auf der Schwinge", "tokens": ["Far\u00b7ben\u00b7st\u00e4ub\u00b7chen", "auf", "der", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sommerlicher Schmetterlinge", "tokens": ["Som\u00b7mer\u00b7li\u00b7cher", "Schmet\u00b7ter\u00b7lin\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fl\u00fcchtig sind sie, sind verg\u00e4nglich", "tokens": ["Fl\u00fcch\u00b7tig", "sind", "sie", ",", "sind", "ver\u00b7g\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Gaben, die ich bringe,", "tokens": ["Wie", "die", "Ga\u00b7ben", ",", "die", "ich", "brin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Kr\u00e4nze, die ich flechte,", "tokens": ["Wie", "die", "Kr\u00e4n\u00b7ze", ",", "die", "ich", "flech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie die Lieder, die ich singe:", "tokens": ["Wie", "die", "Lie\u00b7der", ",", "die", "ich", "sin\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schnell vor\u00fcber schweben alle,", "tokens": ["Schnell", "vor\u00b7\u00fc\u00b7ber", "schwe\u00b7ben", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihre Dauer ist geringe,", "tokens": ["Ih\u00b7re", "Dau\u00b7er", "ist", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie ein Schaum auf schwanker Welle,", "tokens": ["Wie", "ein", "Schaum", "auf", "schwan\u00b7ker", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie ein Hauch auf blanker Klinge.", "tokens": ["Wie", "ein", "Hauch", "auf", "blan\u00b7ker", "Klin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht Unsterblichkeit verlang ich,", "tokens": ["Nicht", "U\u00b7nsterb\u00b7lich\u00b7keit", "ver\u00b7lang", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Sterben ist das Los der Dinge:", "tokens": ["Ster\u00b7ben", "ist", "das", "Los", "der", "Din\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Meine T\u00f6ne sind zerbrechlich", "tokens": ["Mei\u00b7ne", "T\u00f6\u00b7ne", "sind", "zer\u00b7brech\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wie das Glas, an das ich klinge.", "tokens": ["Wie", "das", "Glas", ",", "an", "das", "ich", "klin\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}