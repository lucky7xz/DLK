{"textgrid.poem.42401": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um eine tausendj\u00e4hrige Fabel sp\u00fcrt", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um eine tausendj\u00e4hrige Fabel sp\u00fcrt", "tokens": ["Um", "ei\u00b7ne", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7ge", "Fa\u00b7bel", "sp\u00fcrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch Gram mein Herz und geht hoch", "tokens": ["Noch", "Gram", "mein", "Herz", "und", "geht", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "PPOSAT", "NN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wie Springkraut, das man ber\u00fchrt.", "tokens": ["Wie", "Spring\u00b7kraut", ",", "das", "man", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Um eine l\u00e4ngst verschollene Untat tr\u00fcbt", "tokens": ["Um", "ei\u00b7ne", "l\u00e4ngst", "ver\u00b7schol\u00b7le\u00b7ne", "Un\u00b7tat", "tr\u00fcbt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich noch mein Sinn und ergrimmt", "tokens": ["Sich", "noch", "mein", "Sinn", "und", "er\u00b7grimmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ob des Frevlers, der sie ver\u00fcbt.", "tokens": ["Ob", "des", "Frev\u00b7lers", ",", "der", "sie", "ver\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Um die verj\u00e4hrte Schmach von Urahnen spr\u00fcht", "tokens": ["Um", "die", "ver\u00b7j\u00e4hr\u00b7te", "Schmach", "von", "Ur\u00b7ah\u00b7nen", "spr\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Noch Racheglut und glimmt", "tokens": ["Noch", "Ra\u00b7che\u00b7glut", "und", "glimmt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im alt vererbten Gebl\u00fct.", "tokens": ["Im", "alt", "ver\u00b7erb\u00b7ten", "Ge\u00b7bl\u00fct", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Um die Erniedrigung von Geknechteten pocht", "tokens": ["Um", "die", "Er\u00b7nied\u00b7ri\u00b7gung", "von", "Ge\u00b7knech\u00b7te\u00b7ten", "pocht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mein Pulsschlag ungest\u00fcm,", "tokens": ["Mein", "Puls\u00b7schlag", "un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4re ich selbst unterjocht.", "tokens": ["Als", "w\u00e4\u00b7re", "ich", "selbst", "un\u00b7ter\u00b7jocht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}