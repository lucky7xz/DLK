{"textgrid.poem.39661": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1. Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie soll ich Kunde zu ihr bringen,", "tokens": ["O", "wie", "soll", "ich", "Kun\u00b7de", "zu", "ihr", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kunde dieser ruhelosen Pein,", "tokens": ["Kun\u00b7de", "die\u00b7ser", "ru\u00b7he\u00b7lo\u00b7sen", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Von der Holden so getrennt zu sein,", "tokens": ["Von", "der", "Hol\u00b7den", "so", "ge\u00b7trennt", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da Gefahren lauernd mich umringen.", "tokens": ["Da", "Ge\u00b7fah\u00b7ren", "lau\u00b7ernd", "mich", "um\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00fcll' ich, der Entfernten sie zu singen,", "tokens": ["H\u00fcll'", "ich", ",", "der", "Ent\u00b7fern\u00b7ten", "sie", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In den Flor der Heimlichkeit mich ein:", "tokens": ["In", "den", "Flor", "der", "Heim\u00b7lich\u00b7keit", "mich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! so achtet sie wohl schwerlich mein,", "tokens": ["Ach", "!", "so", "ach\u00b7tet", "sie", "wohl", "schwer\u00b7lich", "mein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und vergebens mu\u00df mein Lied erklingen.", "tokens": ["Und", "ver\u00b7ge\u00b7bens", "mu\u00df", "mein", "Lied", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Doch getrost! Zerri\u00df nicht, als sie schied,", "tokens": ["Doch", "ge\u00b7trost", "!", "Zer\u00b7ri\u00df", "nicht", ",", "als", "sie", "schied", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$.", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Laut ihr Schwur die Pause stummer Schmerzen:", "tokens": ["Laut", "ihr", "Schwur", "die", "Pau\u00b7se", "stum\u00b7mer", "Schmer\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbmann, du wohnest ewig mir im Herzen\u00ab?", "tokens": ["\u00bb", "mann", ",", "du", "woh\u00b7nest", "e\u00b7wig", "mir", "im", "Her\u00b7zen", "\u00ab", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "PPER", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Diesem Herzen brauchest du, o Lied,", "tokens": ["Die\u00b7sem", "Her\u00b7zen", "brau\u00b7chest", "du", ",", "o", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Des Verh\u00fcllten Namen nicht zu nennen:", "tokens": ["Des", "Ver\u00b7h\u00fcll\u00b7ten", "Na\u00b7men", "nicht", "zu", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "An der Stimme wird es ihn erkennen.", "tokens": ["An", "der", "Stim\u00b7me", "wird", "es", "ihn", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}