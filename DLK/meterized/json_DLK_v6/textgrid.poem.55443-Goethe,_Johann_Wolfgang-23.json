{"textgrid.poem.55443": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "23", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die abgestutzten, angetauchten,", "tokens": ["Die", "ab\u00b7ge\u00b7stutz\u00b7ten", ",", "an\u00b7ge\u00b7tauch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ungeschickten, vielgebrauchten", "tokens": ["Die", "un\u00b7ge\u00b7schick\u00b7ten", ",", "viel\u00b7ge\u00b7brauch\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hast du, die Freundliche, gewollt.", "tokens": ["Hast", "du", ",", "die", "Freund\u00b7li\u00b7che", ",", "ge\u00b7wollt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "NN", "$,", "VMPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun aber nimm ein frisch Gefieder,", "tokens": ["Nun", "a\u00b7ber", "nimm", "ein", "frisch", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das niederschreiben s\u00fc\u00dfe Lieder", "tokens": ["Das", "nie\u00b7der\u00b7schrei\u00b7ben", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Allsch\u00f6nster Tage dir gesollt.", "tokens": ["All\u00b7sch\u00f6ns\u00b7ter", "Ta\u00b7ge", "dir", "ge\u00b7sollt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}