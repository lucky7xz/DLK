{"textgrid.poem.46275": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Der frauen lob", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit tugend und mit ehr die frau ist wol gezieret,", "tokens": ["Mit", "tu\u00b7gend", "und", "mit", "ehr", "die", "frau", "ist", "wol", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wer darf doch sagen, da\u00df sie wird b\u00f6s und verf\u00fchret?", "tokens": ["wer", "darf", "doch", "sa\u00b7gen", ",", "da\u00df", "sie", "wird", "b\u00f6s", "und", "ver\u00b7f\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "t\u00e4glich sie kommet fromm mit dem gebet f\u00fcr got;", "tokens": ["t\u00e4g\u00b7lich", "sie", "kom\u00b7met", "fromm", "mit", "dem", "ge\u00b7bet", "f\u00fcr", "got", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+--+-+--++-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "selten aus ihrem man sie machet einen spot.", "tokens": ["sel\u00b7ten", "aus", "ih\u00b7rem", "man", "sie", "ma\u00b7chet", "ei\u00b7nen", "spot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "PIS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "allzeit dem man will sie wol dienen und gefallen;", "tokens": ["all\u00b7zeit", "dem", "man", "will", "sie", "wol", "die\u00b7nen", "und", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIS", "VMFIN", "PPER", "ADV", "VVINF", "KON", "VVPP", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "niemals das ganze haus mu\u00df von ihr widerschallen;", "tokens": ["nie\u00b7mals", "das", "gan\u00b7ze", "haus", "mu\u00df", "von", "ihr", "wi\u00b7der\u00b7schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "tag und nacht ihr gem\u00fct sich gleichet ihrem mund.", "tokens": ["tag", "und", "nacht", "ihr", "ge\u00b7m\u00fct", "sich", "glei\u00b7chet", "ih\u00b7rem", "mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "VVFIN", "PRF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "nicht weis ist, der die frau nur liebet eine stund.", "tokens": ["nicht", "weis", "ist", ",", "der", "die", "frau", "nur", "lie\u00b7bet", "ei\u00b7ne", "stund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "VAFIN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}