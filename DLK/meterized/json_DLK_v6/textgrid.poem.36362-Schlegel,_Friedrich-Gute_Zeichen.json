{"textgrid.poem.36362": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gute Zeichen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Krieges grauser Arm umschlo\u00df die Erde,", "tokens": ["Des", "Krie\u00b7ges", "grau\u00b7ser", "Arm", "um\u00b7schlo\u00df", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch seh ich wieder froher Hoffnung Zeichen,", "tokens": ["Doch", "seh", "ich", "wie\u00b7der", "fro\u00b7her", "Hoff\u00b7nung", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie fern in Nacht das Ziel, das wir erreichen,", "tokens": ["Wie", "fern", "in", "Nacht", "das", "Ziel", ",", "das", "wir", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Strom auf Strom auch noch vergossen werde.", "tokens": ["Wie", "Strom", "auf", "Strom", "auch", "noch", "ver\u00b7gos\u00b7sen", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "ADV", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Reuvoll, da\u00df der Verrat belohnt nicht werde,", "tokens": ["Reu\u00b7voll", ",", "da\u00df", "der", "Ver\u00b7rat", "be\u00b7lohnt", "nicht", "wer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "VVFIN", "PTKNEG", "VAFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "H\u00f6r' ich der Habsucht W\u00f6lfe heulend keichen;", "tokens": ["H\u00f6r'", "ich", "der", "Hab\u00b7sucht", "W\u00f6l\u00b7fe", "heu\u00b7lend", "kei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht mehr verborgen will der Tiger schleichen,", "tokens": ["Nicht", "mehr", "ver\u00b7bor\u00b7gen", "will", "der", "Ti\u00b7ger", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zeigt k\u00fchn am Tag die blutige Geb\u00e4rde.", "tokens": ["Zeigt", "k\u00fchn", "am", "Tag", "die", "blu\u00b7ti\u00b7ge", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drob zitternd will die falsche Brut verzagen,", "tokens": ["Drob", "zit\u00b7ternd", "will", "die", "fal\u00b7sche", "Brut", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nacht ist finster, doch bei stillen Flammen", "tokens": ["Die", "Nacht", "ist", "fins\u00b7ter", ",", "doch", "bei", "stil\u00b7len", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Harret der stolze Adler auf den Morgen.", "tokens": ["Har\u00b7ret", "der", "stol\u00b7ze", "Ad\u00b7ler", "auf", "den", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Der junge L\u00f6we schlummert noch verborgen;", "tokens": ["Der", "jun\u00b7ge", "L\u00f6\u00b7we", "schlum\u00b7mert", "noch", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wacht er, so st\u00fcrzt des Tigers Bau zusammen,", "tokens": ["Wacht", "er", ",", "so", "st\u00fcrzt", "des", "Ti\u00b7gers", "Bau", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Drum lodert auf, ihr Flammen, la\u00dft es tagen!", "tokens": ["Drum", "lo\u00b7dert", "auf", ",", "ihr", "Flam\u00b7men", ",", "la\u00dft", "es", "ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}