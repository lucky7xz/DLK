{"textgrid.poem.36284": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Prolog zu Lessings Nathan", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unz\u00e4hlig sind die frohen Kinder meiner Lust,", "tokens": ["Un\u00b7z\u00e4h\u00b7lig", "sind", "die", "fro\u00b7hen", "Kin\u00b7der", "mei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ich aus dunkelm Scho\u00df erzeugend aufgebracht,", "tokens": ["Die", "ich", "aus", "dun\u00b7kelm", "Scho\u00df", "er\u00b7zeu\u00b7gend", "auf\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVPP", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df sie nun Himmel atmend lichte Sterne schaun. \u2013", "tokens": ["Da\u00df", "sie", "nun", "Him\u00b7mel", "at\u00b7mend", "lich\u00b7te", "Ster\u00b7ne", "schaun", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ADJD", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was auf des Fr\u00fchlings gr\u00fcnem Teppich munter spielt,", "tokens": ["Was", "auf", "des", "Fr\u00fch\u00b7lings", "gr\u00fc\u00b7nem", "Tep\u00b7pich", "mun\u00b7ter", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit mutigen Gesellen jugendlich vereint,", "tokens": ["Mit", "mu\u00b7ti\u00b7gen", "Ge\u00b7sel\u00b7len", "ju\u00b7gend\u00b7lich", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und frei sich seines Lebens freut in raschem Kampf,", "tokens": ["Und", "frei", "sich", "sei\u00b7nes", "Le\u00b7bens", "freut", "in", "ra\u00b7schem", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Im Spiel der hei\u00dfen Triebe; oder was auch still", "tokens": ["Im", "Spiel", "der", "hei\u00b7\u00dfen", "Trie\u00b7be", ";", "o\u00b7der", "was", "auch", "still"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$.", "KON", "PWS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verborgen, leise Sch\u00f6nheit duftet in dem Gr\u00fcn,", "tokens": ["Ver\u00b7bor\u00b7gen", ",", "lei\u00b7se", "Sch\u00f6n\u00b7heit", "duf\u00b7tet", "in", "dem", "Gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Aus offnem Kelche dann dem Licht sein eigen Bild,", "tokens": ["Aus", "off\u00b7nem", "Kel\u00b7che", "dann", "dem", "Licht", "sein", "ei\u00b7gen", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein kleiner Farbenhimmel, kindlich wiedergibt,", "tokens": ["Ein", "klei\u00b7ner", "Far\u00b7ben\u00b7him\u00b7mel", ",", "kind\u00b7lich", "wie\u00b7der\u00b7gibt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In einen Blick und Freudenblitz den Geist verhaucht;", "tokens": ["In", "ei\u00b7nen", "Blick", "und", "Freu\u00b7den\u00b7blitz", "den", "Geist", "ver\u00b7haucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ja, was nur atmet, gr\u00fcnet, lebet und sich sehnt;", "tokens": ["Ja", ",", "was", "nur", "at\u00b7met", ",", "gr\u00fc\u00b7net", ",", "le\u00b7bet", "und", "sich", "sehnt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "In allem atmet, schl\u00e4gt und regt, sehnt sich und treibt", "tokens": ["In", "al\u00b7lem", "at\u00b7met", ",", "schl\u00e4gt", "und", "regt", ",", "sehnt", "sich", "und", "treibt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "PRF", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die eine alldurchdringend nie durchdrungne Kraft,", "tokens": ["Die", "ei\u00b7ne", "all\u00b7durch\u00b7drin\u00b7gend", "nie", "durch\u00b7drung\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der treuen Mutter ewig liebeschaffend Herz.", "tokens": ["Der", "treu\u00b7en", "Mut\u00b7ter", "e\u00b7wig", "lie\u00b7be\u00b7schaf\u00b7fend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch wenn die Sommerlust entflohn, die Pracht verbl\u00fcht,", "tokens": ["Doch", "wenn", "die", "Som\u00b7mer\u00b7lust", "ent\u00b7flohn", ",", "die", "Pracht", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von Schmuck entbl\u00f6\u00dft, ganz traurend nur ich schein' und kalt,", "tokens": ["Von", "Schmuck", "ent\u00b7bl\u00f6\u00dft", ",", "ganz", "trau\u00b7rend", "nur", "ich", "schein'", "und", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "ADV", "ADJD", "ADV", "PPER", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dann denkt das Herz in stiller Tiefe andres aus;", "tokens": ["Dann", "denkt", "das", "Herz", "in", "stil\u00b7ler", "Tie\u00b7fe", "and\u00b7res", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein sinnreich k\u00fcnstlich Bilden schafft der k\u00fchne Flei\u00df,", "tokens": ["Ein", "sinn\u00b7reich", "k\u00fcnst\u00b7lich", "Bil\u00b7den", "schafft", "der", "k\u00fch\u00b7ne", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Vielfach verschlungen webend, was er schlau erdacht.", "tokens": ["Viel\u00b7fach", "ver\u00b7schlun\u00b7gen", "we\u00b7bend", ",", "was", "er", "schlau", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVPP", "$,", "PWS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Oft wenn die Laune eigenwillig es befiehlt,", "tokens": ["Oft", "wenn", "die", "Lau\u00b7ne", "ei\u00b7gen\u00b7wil\u00b7lig", "es", "be\u00b7fiehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein scherzend Spiel nur, denn die wildeste Gestalt,", "tokens": ["Ein", "scher\u00b7zend", "Spiel", "nur", ",", "denn", "die", "wil\u00b7des\u00b7te", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die sonderbarste ist's, die sich der Witz erw\u00e4hlt.", "tokens": ["Die", "son\u00b7der\u00b7bars\u00b7te", "ist's", ",", "die", "sich", "der", "Witz", "er\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "PRELS", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Worte r\u00e4tselvoll verworr'n sinnbildend Spiel", "tokens": ["Der", "Wor\u00b7te", "r\u00e4t\u00b7sel\u00b7voll", "ver\u00b7worr'n", "sinn\u00b7bil\u00b7dend", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wird hier erdacht; und was den Menschen wohl bewegt", "tokens": ["Wird", "hier", "er\u00b7dacht", ";", "und", "was", "den", "Men\u00b7schen", "wohl", "be\u00b7wegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "KON", "PWS", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Zu staunen \u00fcber seines Gleichen, da\u00df der Mensch \u2013", "tokens": ["Zu", "stau\u00b7nen", "\u00fc\u00b7ber", "sei\u00b7nes", "Glei\u00b7chen", ",", "da\u00df", "der", "Mensch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dies wundervoll Gew\u00e4chse, gottverwandte Tier,", "tokens": ["Dies", "wun\u00b7der\u00b7voll", "Ge\u00b7w\u00e4ch\u00b7se", ",", "gott\u00b7ver\u00b7wand\u00b7te", "Tier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Des Lebens Blume, helles Aug' und Freudenlicht,", "tokens": ["Des", "Le\u00b7bens", "Blu\u00b7me", ",", "hel\u00b7les", "Aug'", "und", "Freu\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Mutter h\u00f6chste Lust und lieblichstes Gesch\u00f6pf \u2013", "tokens": ["Der", "Mut\u00b7ter", "h\u00f6chs\u00b7te", "Lust", "und", "lieb\u00b7lichs\u00b7tes", "Ge\u00b7sch\u00f6pf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So wundervoll gebildet und gebaut sein kann;", "tokens": ["So", "wun\u00b7der\u00b7voll", "ge\u00b7bil\u00b7det", "und", "ge\u00b7baut", "sein", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So seltsamlich gemischt in seinem Hirn der Ernst", "tokens": ["So", "selt\u00b7sam\u00b7lich", "ge\u00b7mischt", "in", "sei\u00b7nem", "Hirn", "der", "Ernst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und Laun' und Spott, sinnreicher Witz und Lieb' und Zorn,", "tokens": ["Und", "Laun'", "und", "Spott", ",", "sinn\u00b7rei\u00b7cher", "Witz", "und", "Lieb'", "und", "Zorn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "ADJA", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Allm\u00e4chtiger Gedanken schaffendes Gehei\u00df,", "tokens": ["All\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Ge\u00b7dan\u00b7ken", "schaf\u00b7fen\u00b7des", "Ge\u00b7hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und wieder tiefes Sehnen, leiser W\u00fcnsche Hauch", "tokens": ["Und", "wie\u00b7der", "tie\u00b7fes", "Seh\u00b7nen", ",", "lei\u00b7ser", "W\u00fcn\u00b7sche", "Hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "$,", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "In stiller Brust und sanfte Demut, zarte Scheu;", "tokens": ["In", "stil\u00b7ler", "Brust", "und", "sanf\u00b7te", "De\u00b7mut", ",", "zar\u00b7te", "Scheu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So eigen jeder, eine kleine Welt f\u00fcr sich.", "tokens": ["So", "ei\u00b7gen", "je\u00b7der", ",", "ei\u00b7ne", "klei\u00b7ne", "Welt", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "$,", "ART", "ADJA", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Denn mir mi\u00dff\u00e4llt von Herzensgrunde was nur gleich", "tokens": ["Denn", "mir", "mi\u00df\u00b7f\u00e4llt", "von", "Her\u00b7zens\u00b7grun\u00b7de", "was", "nur", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PWS", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Sich selbst einf\u00f6rmig wiederholend immer bleibt.", "tokens": ["Sich", "selbst", "ein\u00b7f\u00f6r\u00b7mig", "wie\u00b7der\u00b7ho\u00b7lend", "im\u00b7mer", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "VVPP", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was ich mit Lieb' und Lust erschaff', ist mannichfalt,", "tokens": ["Was", "ich", "mit", "Lieb'", "und", "Lust", "er\u00b7schaff'", ",", "ist", "man\u00b7nich\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Und frei und k\u00fchn, und mutig bahnt sich's neuen Weg.", "tokens": ["Und", "frei", "und", "k\u00fchn", ",", "und", "mu\u00b7tig", "bahnt", "sich's", "neu\u00b7en", "Weg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,", "KON", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So schuf ich einst in stiller heitrer Winternacht", "tokens": ["So", "schuf", "ich", "einst", "in", "stil\u00b7ler", "hei\u00b7trer", "Win\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da jeder Stern am Himmel freundlich niederschien,", "tokens": ["Da", "je\u00b7der", "Stern", "am", "Him\u00b7mel", "freund\u00b7lich", "nie\u00b7der\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": [