asure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Umfragend, ward des rechten Weges bald gewahr;", "tokens": ["Um\u00b7fra\u00b7gend", ",", "ward", "des", "rech\u00b7ten", "We\u00b7ges", "bald", "ge\u00b7wahr", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Den andern deutend bahnt er selber kr\u00e4ft'ge Spur;", "tokens": ["Den", "an\u00b7dern", "deu\u00b7tend", "bahnt", "er", "sel\u00b7ber", "kr\u00e4ft'\u00b7ge", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Und trat ihm Dummheit, platter P\u00f6bel in den Weg,", "tokens": ["Und", "trat", "ihm", "Dumm\u00b7heit", ",", "plat\u00b7ter", "P\u00f6\u00b7bel", "in", "den", "Weg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Da spr\u00fcht er hellen Witzes Funken weit umher,", "tokens": ["Da", "spr\u00fcht", "er", "hel\u00b7len", "Wit\u00b7zes", "Fun\u00b7ken", "weit", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Und manchen traf der t\u00f6dlich schneidend scharfe Schlag.", "tokens": ["Und", "man\u00b7chen", "traf", "der", "t\u00f6d\u00b7lich", "schnei\u00b7dend", "schar\u00b7fe", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "So blieb der Teure stets mein lieber teurer Sohn,", "tokens": ["So", "blieb", "der", "Teu\u00b7re", "stets", "mein", "lie\u00b7ber", "teu\u00b7rer", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Sein Angedenken mir im Herzen fest und wert;", "tokens": ["Sein", "An\u00b7ge\u00b7den\u00b7ken", "mir", "im", "Her\u00b7zen", "fest", "und", "wert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Und tret' heraufgestiegen, G\u00f6ttin, k\u00fchnlich auf", "tokens": ["Und", "tret'", "her\u00b7auf\u00b7ge\u00b7stie\u00b7gen", ",", "G\u00f6t\u00b7tin", ",", "k\u00fchn\u00b7lich", "auf"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "$,", "NN", "$,", "ADJD", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Zu dieser seiner Namensfeier, das Gedicht,", "tokens": ["Zu", "die\u00b7ser", "sei\u00b7ner", "Na\u00b7mens\u00b7fei\u00b7er", ",", "das", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Wo sich sein Geist am reinsten selber ausgedr\u00fcckt,", "tokens": ["Wo", "sich", "sein", "Geist", "am", "reins\u00b7ten", "sel\u00b7be