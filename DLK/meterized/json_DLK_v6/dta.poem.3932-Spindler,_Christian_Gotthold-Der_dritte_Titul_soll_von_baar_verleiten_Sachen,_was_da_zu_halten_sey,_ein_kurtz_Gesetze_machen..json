{"dta.poem.3932": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Der dritte  Titul  soll von baar verleiten Sachen,  \n was da zu halten sey, ein kurtz Gesetze machen.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Ein alter Medicus, und zwar aus Coppenhagen,", "tokens": ["Ein", "al\u00b7ter", "Me\u00b7di\u00b7cus", ",", "und", "zwar", "aus", "Cop\u00b7pen\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat folgendes Recept f\u00fcr K\u00e4lte vorgeschlagen:", "tokens": ["Hat", "fol\u00b7gen\u00b7des", "Re\u00b7cept", "f\u00fcr", "K\u00e4l\u00b7te", "vor\u00b7ge\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zehn harte Klafftern Holtz gebraucht,", "tokens": ["Zehn", "har\u00b7te", "Klaff\u00b7tern", "Holtz", "ge\u00b7braucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey Wein und Bier Toback geraucht,", "tokens": ["Bey", "Wein", "und", "Bier", "To\u00b7back", "ge\u00b7raucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Abends einen Milch-Caffee,", "tokens": ["Des", "A\u00b7bends", "ei\u00b7nen", "Milch\u00b7Caf\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fr\u00fch Morgens einen guten Thee,", "tokens": ["Fr\u00fch", "Mor\u00b7gens", "ei\u00b7nen", "gu\u00b7ten", "Thee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Unn\u00f6thigen Besuch gespart,", "tokens": ["Un\u00b7n\u00f6\u00b7thi\u00b7gen", "Be\u00b7such", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Die Fenster und das Hau\u00df verwahrt,", "tokens": ["Die", "Fens\u00b7ter", "und", "das", "Hau\u00df", "ver\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zur Nacht ein feurig junges Weib,", "tokens": ["Zur", "Nacht", "ein", "feu\u00b7rig", "jun\u00b7ges", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bewahrt vor Frost des Menschen Leib.", "tokens": ["Be\u00b7wahrt", "vor", "Frost", "des", "Men\u00b7schen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}