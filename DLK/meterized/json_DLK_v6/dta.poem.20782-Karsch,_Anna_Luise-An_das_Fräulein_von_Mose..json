{"dta.poem.20782": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  das Fr\u00e4ulein von Mose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hoffnung schmeichelt mir, Sie werden permittiren,             ", "tokens": ["Die", "Hoff\u00b7nung", "schmei\u00b7chelt", "mir", ",", "Sie", "wer\u00b7den", "per\u00b7mit\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Dero Dienerin sich itzo unterf\u00e4ngt:", "tokens": ["Was", "De\u00b7ro", "Die\u00b7ne\u00b7rin", "sich", "it\u00b7zo", "un\u00b7ter\u00b7f\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwar kann ich meinen Vers mit wenig Anmuth zieren,", "tokens": ["Zwar", "kann", "ich", "mei\u00b7nen", "Vers", "mit", "we\u00b7nig", "An\u00b7muth", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil kein Virgilius mir seine Silben schenkt;", "tokens": ["Weil", "kein", "Vir\u00b7gi\u00b7lius", "mir", "sei\u00b7ne", "Sil\u00b7ben", "schenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NE", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Doch werden Sie darum die Zeilen nicht verachten,", "tokens": ["Doch", "wer\u00b7den", "Sie", "da\u00b7rum", "die", "Zei\u00b7len", "nicht", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die meine Dankbarkeit zu Dero F\u00fc\u00dfen legt.", "tokens": ["Die", "mei\u00b7ne", "Dank\u00b7bar\u00b7keit", "zu", "De\u00b7ro", "F\u00fc\u00b7\u00dfen", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PDS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kann man dieselben nicht als hoch gelehrt betrachten,", "tokens": ["Kann", "man", "die\u00b7sel\u00b7ben", "nicht", "als", "hoch", "ge\u00b7lehrt", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PDAT", "PTKNEG", "KOUS", "ADJD", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Genug, da\u00df jedes Wort vollkommne Treue hegt.", "tokens": ["Ge\u00b7nug", ",", "da\u00df", "je\u00b7des", "Wort", "voll\u00b7komm\u00b7ne", "Treu\u00b7e", "hegt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein angenehmer Tag, so Dero Namen f\u00fchret,", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "Tag", ",", "so", "De\u00b7ro", "Na\u00b7men", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ermuntert mein Gem\u00fcth zu der Ergebenheit,", "tokens": ["Er\u00b7mun\u00b7tert", "mein", "Ge\u00b7m\u00fcth", "zu", "der", "Er\u00b7ge\u00b7ben\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.11": {"text": "Womit ich Ihnen bin Zeit Lebens obligiret:", "tokens": ["Wo\u00b7mit", "ich", "Ih\u00b7nen", "bin", "Zeit", "Le\u00b7bens", "ob\u00b7li\u00b7gi\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drum observire ich itzt meine Schuldigkeit,", "tokens": ["Drum", "ob\u00b7ser\u00b7vi\u00b7re", "ich", "itzt", "mei\u00b7ne", "Schul\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und will durch dieses Blatt gehorsamst gratuliren,", "tokens": ["Und", "will", "durch", "die\u00b7ses", "Blatt", "ge\u00b7hor\u00b7samst", "gra\u00b7tu\u00b7li\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PDAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.14": {"text": "Weil Sie der Herr der \u00fcber Erd und Himmel schwebt,", "tokens": ["Weil", "Sie", "der", "Herr", "der", "\u00fc\u00b7ber", "Erd", "und", "Him\u00b7mel", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Durch seinen Vaterarm so treulich wollen f\u00fchren,", "tokens": ["Durch", "sei\u00b7nen", "Va\u00b7te\u00b7rarm", "so", "treu\u00b7lich", "wol\u00b7len", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df Sie begl\u00fcckt und wohl dies Namensfest erlebt.", "tokens": ["Da\u00df", "Sie", "be\u00b7gl\u00fcckt", "und", "wohl", "dies", "Na\u00b7mens\u00b7fest", "er\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "KON", "ADV", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es bleibe dieser Herr noch ferner Dero F\u00fchrer,", "tokens": ["Es", "blei\u00b7be", "die\u00b7ser", "Herr", "noch", "fer\u00b7ner", "De\u00b7ro", "F\u00fch\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ADV", "ADV", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er unterst\u00fctze Sie mit seiner Allmachtskraft,", "tokens": ["Er", "un\u00b7ter\u00b7st\u00fct\u00b7ze", "Sie", "mit", "sei\u00b7ner", "All\u00b7machts\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er sey Ihr Schild und Lohn, Ihr m\u00e4chtiger Regierer,", "tokens": ["Er", "sey", "Ihr", "Schild", "und", "Lohn", ",", "Ihr", "m\u00e4ch\u00b7ti\u00b7ger", "Re\u00b7gie\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er st\u00e4rke Dero Geist mit s\u00fc\u00dfem Lebenssaft,", "tokens": ["Er", "st\u00e4r\u00b7ke", "De\u00b7ro", "Geist", "mit", "s\u00fc\u00b7\u00dfem", "Le\u00b7bens\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er lasse niemals was so Widriges geschehn,", "tokens": ["Er", "las\u00b7se", "nie\u00b7mals", "was", "so", "Wid\u00b7ri\u00b7ges", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df Ihre Gnaden kr\u00e4nkt und Dero Ruhe st\u00f6rt.", "tokens": ["Da\u00df", "Ih\u00b7re", "Gna\u00b7den", "kr\u00e4nkt", "und", "De\u00b7ro", "Ru\u00b7he", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "KON", "PDS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nein, nein, es m\u00fcsse Sie auf ewig wohl ergehen:", "tokens": ["Nein", ",", "nein", ",", "es", "m\u00fcs\u00b7se", "Sie", "auf", "e\u00b7wig", "wohl", "er\u00b7ge\u00b7hen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PPER", "APPR", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es m\u00fcsse nur geschehn, was Dero Freude mehrt,", "tokens": ["Es", "m\u00fcs\u00b7se", "nur", "ge\u00b7schehn", ",", "was", "De\u00b7ro", "Freu\u00b7de", "mehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVPP", "$,", "PWS", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es m\u00fcsse Ihnen nichts an Gl\u00fcck und Wohlseyn fehlen,", "tokens": ["Es", "m\u00fcs\u00b7se", "Ih\u00b7nen", "nichts", "an", "Gl\u00fcck", "und", "Wohl\u00b7seyn", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der hohe Himmel sey Sie ewig zugethan,", "tokens": ["Der", "ho\u00b7he", "Him\u00b7mel", "sey", "Sie", "e\u00b7wig", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er lasse Sie noch viel begl\u00fcckte Jahre z\u00e4hlen,", "tokens": ["Er", "las\u00b7se", "Sie", "noch", "viel", "be\u00b7gl\u00fcck\u00b7te", "Jah\u00b7re", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Er schenke Ihnen mehr als ich nur w\u00fcnschen kann.", "tokens": ["Er", "schen\u00b7ke", "Ih\u00b7nen", "mehr", "als", "ich", "nur", "w\u00fcn\u00b7schen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}