{"textgrid.poem.56042": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchlst du, wie keiner von allen", "tokens": ["F\u00fchlst", "du", ",", "wie", "kei\u00b7ner", "von", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PIS", "APPR", "PIAT"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wegen steht und stockt;", "tokens": ["We\u00b7gen", "steht", "und", "stockt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "von gelassenen Treppen fallen,", "tokens": ["von", "ge\u00b7las\u00b7se\u00b7nen", "Trep\u00b7pen", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "durch ein Nichts von Neigung", "tokens": ["durch", "ein", "Nichts", "von", "Nei\u00b7gung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "leise weitergelockt,", "tokens": ["lei\u00b7se", "wei\u00b7ter\u00b7ge\u00b7lockt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "\u00fcber alle Terrassen", "tokens": ["\u00fc\u00b7ber", "al\u00b7le", "Ter\u00b7ras\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "die Wege, zwischen den Massen", "tokens": ["die", "We\u00b7ge", ",", "zwi\u00b7schen", "den", "Mas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "verlangsamt und gelenkt,", "tokens": ["ver\u00b7langs\u00b7amt", "und", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "bis zu den weiten Teichen,", "tokens": ["bis", "zu", "den", "wei\u00b7ten", "Tei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "wo sie (wie einem Gleichen)", "tokens": ["wo", "sie", "(", "wie", "ei\u00b7nem", "Glei\u00b7chen", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "der reiche Park verschenkt", "tokens": ["der", "rei\u00b7che", "Park", "ver\u00b7schenkt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "an den reichen Raum: den Einen,", "tokens": ["an", "den", "rei\u00b7chen", "Raum", ":", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der mit Scheinen und Widerscheinen", "tokens": ["der", "mit", "Schei\u00b7nen", "und", "Wi\u00b7der\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "seinen Besitz durchdringt,", "tokens": ["sei\u00b7nen", "Be\u00b7sitz", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "aus dem er von allen Seiten", "tokens": ["aus", "dem", "er", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weiten mit sich bringt,", "tokens": ["Wei\u00b7ten", "mit", "sich", "bringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "wenn er aus schlie\u00dfenden Weihern", "tokens": ["wenn", "er", "aus", "schlie\u00b7\u00dfen\u00b7den", "Wei\u00b7hern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "zu wolkigen Abendfeiern", "tokens": ["zu", "wol\u00b7ki\u00b7gen", "A\u00b7bend\u00b7fei\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "sich in die Himmel schwingt.", "tokens": ["sich", "in", "die", "Him\u00b7mel", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}