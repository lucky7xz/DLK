{"textgrid.poem.43271": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zuweilen tr\u00f6stet mich die Muse wohl,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zuweilen tr\u00f6stet mich die Muse wohl,", "tokens": ["Zu\u00b7wei\u00b7len", "tr\u00f6s\u00b7tet", "mich", "die", "Mu\u00b7se", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie werden langsam doch her\u00fcberschwimmen,", "tokens": ["Sie", "wer\u00b7den", "lang\u00b7sam", "doch", "her\u00b7\u00fc\u00b7bersc\u00b7hwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie werden endlich muth- und m\u00fchevoll", "tokens": ["Sie", "wer\u00b7den", "end\u00b7lich", "muth", "und", "m\u00fc\u00b7he\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "TRUNC", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch dieses steile Ufer noch erklimmen.", "tokens": ["Doch", "die\u00b7ses", "stei\u00b7le", "U\u00b7fer", "noch", "er\u00b7klim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie werden doch auf festen guten Grund", "tokens": ["Sie", "wer\u00b7den", "doch", "auf", "fes\u00b7ten", "gu\u00b7ten", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch ihre armen d\u00fcrftigen Banner stellen,", "tokens": ["Noch", "ih\u00b7re", "ar\u00b7men", "d\u00fcrf\u00b7ti\u00b7gen", "Ban\u00b7ner", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und nimmer k\u00e4mpfen, hungermatt und wund,", "tokens": ["Und", "nim\u00b7mer", "k\u00e4mp\u00b7fen", ",", "hun\u00b7ger\u00b7matt", "und", "wund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr lebelang nur gegen Wind und Wellen.", "tokens": ["Ihr", "le\u00b7be\u00b7lang", "nur", "ge\u00b7gen", "Wind", "und", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erhobnen Hauptes weis't sie auf die Schaar,", "tokens": ["Er\u00b7hob\u00b7nen", "Haup\u00b7tes", "weis't", "sie", "auf", "die", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die durch den schwarzen Flu\u00df der Noth geschritten", "tokens": ["Die", "durch", "den", "schwar\u00b7zen", "Flu\u00df", "der", "Noth", "ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und doch zu Jenen stehet treu und wahr,", "tokens": ["Und", "doch", "zu", "Je\u00b7nen", "ste\u00b7het", "treu", "und", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit denen ehmals dr\u00fcben sie gelitten ...", "tokens": ["Mit", "de\u00b7nen", "eh\u00b7mals", "dr\u00fc\u00b7ben", "sie", "ge\u00b7lit\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}