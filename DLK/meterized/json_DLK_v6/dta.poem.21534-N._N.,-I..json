: "-+-+-+-+-", "measure": "iambic.tetra"}, "line.74": {"text": "Wenn ihn, den Sterbenden,", "tokens": ["Wenn", "ihn", ",", "den", "Ster\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.75": {"text": "Des Todes harter Arm", "tokens": ["Des", "To\u00b7des", "har\u00b7ter", "Arm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.76": {"text": "Auf seinem Lager niederrang,", "tokens": ["Auf", "sei\u00b7nem", "La\u00b7ger", "nie\u00b7der\u00b7rang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.77": {"text": "Und er sich wand \u2014 \u2014", "tokens": ["Und", "er", "sich", "wand"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "PRF", "VVFIN", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.78": {"text": "\u2014 Doch war das S\u00fcnde,", "tokens": ["Doch", "war", "das", "S\u00fcn\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.79": {"text": "Da\u00df mich ein Weib gebar? \u2014", "tokens": ["Da\u00df", "mich", "ein", "Weib", "ge\u00b7bar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.80": {"text": "Nein! \u2014 S\u00fcnde \u2014 wider die Natur \u2014", "tokens": ["Nein", "!", "S\u00fcn\u00b7de", "wi\u00b7der", "die", "Na\u00b7tur"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "$(", "NN", "$(", "APPR", "ART", "NN", "$("], "met