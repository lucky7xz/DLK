{"dta.poem.21534": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.57", "sv:0.42"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kniete am Altar inmitten", "tokens": ["Ich", "knie\u00b7te", "am", "Al\u00b7tar", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der gl\u00e4ubigen Menge, die Gebet lallend", "tokens": ["Der", "gl\u00e4u\u00b7bi\u00b7gen", "Men\u00b7ge", ",", "die", "Ge\u00b7bet", "lal\u00b7lend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVPP"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Auf ihren Knieen lag \u2014", "tokens": ["Auf", "ih\u00b7ren", "Kni\u00b7e\u00b7en", "lag"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und schwellende Orgelt\u00f6ne", "tokens": ["Und", "schwel\u00b7len\u00b7de", "Or\u00b7gel\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie ein entfesselt Meer", "tokens": ["Wie", "ein", "ent\u00b7fes\u00b7selt", "Meer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Umwogten mich, und holde Knabenstimmen", "tokens": ["Um\u00b7wog\u00b7ten", "mich", ",", "und", "hol\u00b7de", "Kna\u00b7bens\u00b7tim\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mir in die Seele drangen \u2014", "tokens": ["Mir", "in", "die", "See\u00b7le", "dran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch meine Lippen hatten einst", "tokens": ["Auch", "mei\u00b7ne", "Lip\u00b7pen", "hat\u00b7ten", "einst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das heil\u2019ge Lied erhoben", "tokens": ["Das", "heil'\u00b7ge", "Lied", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wie eure, die ihr euch", "tokens": ["Wie", "eu\u00b7re", ",", "die", "ihr", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "$,", "PRELS", "PPER", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Mir in die Seele stehlt", "tokens": ["Mir", "in", "die", "See\u00b7le", "stehlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Mit jenen unschuldsvollen", "tokens": ["Mit", "je\u00b7nen", "un\u00b7schulds\u00b7vol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Hinsterbenden Ges\u00e4ngen \u2014", "tokens": ["Hins\u00b7ter\u00b7ben\u00b7den", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Auch meine Seele hatt\u2019 ich einst", "tokens": ["Auch", "mei\u00b7ne", "See\u00b7le", "hatt'", "ich", "einst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Als reines Opfer hin auf den Altar gelegt,", "tokens": ["Als", "rei\u00b7nes", "Op\u00b7fer", "hin", "auf", "den", "Al\u00b7tar", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "So unber\u00fchrt und unbefleckt.", "tokens": ["So", "un\u00b7be\u00b7r\u00fchrt", "und", "un\u00b7be\u00b7fleckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und h\u00f6her stieg der Weihrauchduft empor", "tokens": ["Und", "h\u00f6\u00b7her", "stieg", "der", "Weih\u00b7rauch\u00b7duft", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Zum Schiff die Sinne bannend.", "tokens": ["Zum", "Schiff", "die", "Sin\u00b7ne", "ban\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und von dem s\u00fc\u00dfen Bangen", "tokens": ["Und", "von", "dem", "s\u00fc\u00b7\u00dfen", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Der Kindheit, die zum ersten Mal", "tokens": ["Der", "Kind\u00b7heit", ",", "die", "zum", "ers\u00b7ten", "Mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sich sch\u00fcchtern Gottes Altar naht,", "tokens": ["Sich", "sch\u00fcch\u00b7tern", "Got\u00b7tes", "Al\u00b7tar", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Flog mir ein Hauch", "tokens": ["Flog", "mir", "ein", "Hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Noch einmal durch die Seele,", "tokens": ["Noch", "ein\u00b7mal", "durch", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Ich kostete noch einmal", "tokens": ["Ich", "kos\u00b7te\u00b7te", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Den heil\u2019gen Taumel,", "tokens": ["Den", "heil'\u00b7gen", "Tau\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Gab mich noch einmal", "tokens": ["Gab", "mich", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.27": {"text": "Dem stillen Rausch der Hoffnung", "tokens": ["Dem", "stil\u00b7len", "Rausch", "der", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Mit innig jauchzendem Herzen", "tokens": ["Mit", "in\u00b7nig", "jauch\u00b7zen\u00b7dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.29": {"text": "Ergeben hin.", "tokens": ["Er\u00b7ge\u00b7ben", "hin", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Ich blickte auf \u2014", "tokens": ["Ich", "blick\u00b7te", "auf"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Durch spitze Fenster fielen", "tokens": ["Durch", "spit\u00b7ze", "Fens\u00b7ter", "fie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Die schr\u00e4gen, gelben Sonnenstrahlen", "tokens": ["Die", "schr\u00e4\u00b7gen", ",", "gel\u00b7ben", "Son\u00b7nen\u00b7strah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und woben um das Haupt dir", "tokens": ["Und", "wo\u00b7ben", "um", "das", "Haupt", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Dort an dem Kreuze mit der Dornenkrone", "tokens": ["Dort", "an", "dem", "Kreu\u00b7ze", "mit", "der", "Dor\u00b7nen\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Hell flimmernd einen gold\u2019nen Ring \u2014", "tokens": ["Hell", "flim\u00b7mernd", "ei\u00b7nen", "gold'\u00b7nen", "Ring"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und deine Z\u00fcge lebten noch,", "tokens": ["Und", "dei\u00b7ne", "Z\u00fc\u00b7ge", "leb\u00b7ten", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Ich sah noch einmal dir den Kampf", "tokens": ["Ich", "sah", "noch", "ein\u00b7mal", "dir", "den", "Kampf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Hin durch den Leib, den m\u00fcden, zieh\u2019n,", "tokens": ["Hin", "durch", "den", "Leib", ",", "den", "m\u00fc\u00b7den", ",", "zieh'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und deine Wunden flossen noch einmal", "tokens": ["Und", "dei\u00b7ne", "Wun\u00b7den", "flos\u00b7sen", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Wie blut\u2019ge Z\u00e4hren, die ein Gott", "tokens": ["Wie", "blut'\u00b7ge", "Z\u00e4h\u00b7ren", ",", "die", "ein", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Um sein versunken Eden weint.", "tokens": ["Um", "sein", "ver\u00b7sun\u00b7ken", "E\u00b7den", "weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Der Kranz grub sich in deine Stirn,", "tokens": ["Der", "Kranz", "grub", "sich", "in", "dei\u00b7ne", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Die alabasterwei\u00dfe,", "tokens": ["Die", "a\u00b7la\u00b7bas\u00b7ter\u00b7wei\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Mit purpurrothen Spuren.", "tokens": ["Mit", "pur\u00b7pur\u00b7ro\u00b7then", "Spu\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Da griff es mich mit Geistermacht", "tokens": ["Da", "griff", "es", "mich", "mit", "Geis\u00b7ter\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Und \u00f6ffnete mir das bl\u00f6de Auge,", "tokens": ["Und", "\u00f6ff\u00b7ne\u00b7te", "mir", "das", "bl\u00f6\u00b7de", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.47": {"text": "Das staunend nur an diesen Reigen hing,", "tokens": ["Das", "stau\u00b7nend", "nur", "an", "die\u00b7sen", "Rei\u00b7gen", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Derweil das Herz sich enge mir", "tokens": ["Der\u00b7weil", "das", "Herz", "sich", "en\u00b7ge", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Zusammenpre\u00dfte in der Brust", "tokens": ["Zu\u00b7sam\u00b7men\u00b7pre\u00df\u00b7te", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Mir war\u2019s, als k\u00f6nnt\u2019 ich alles f\u00fchlen,", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "k\u00f6nnt'", "ich", "al\u00b7les", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.51": {"text": "Was du erlebt, da du am Kreuze hingst,", "tokens": ["Was", "du", "er\u00b7lebt", ",", "da", "du", "am", "Kreu\u00b7ze", "hingst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Als dir der Blick auf tausend Gaffer sank", "tokens": ["Als", "dir", "der", "Blick", "auf", "tau\u00b7send", "Gaf\u00b7fer", "sank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.53": {"text": "Und ein\u2019ge nur,", "tokens": ["Und", "ein'\u00b7ge", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.54": {"text": "Die dich beweinten,", "tokens": ["Die", "dich", "be\u00b7wein\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.55": {"text": "Doch nie verstanden.", "tokens": ["Doch", "nie", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.56": {"text": "Du Riesengeist, du f\u00fchltest dich allein!", "tokens": ["Du", "Rie\u00b7sen\u00b7geist", ",", "du", "f\u00fchl\u00b7test", "dich", "al\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.57": {"text": "\u2014 Das schmerzte. \u2014", "tokens": ["Das", "schmerz\u00b7te", "."], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.58": {"text": "Du kanntest wohl das Menschenherz", "tokens": ["Du", "kann\u00b7test", "wohl", "das", "Men\u00b7schen\u00b7herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "In seinem Wollen, seinem Ahnen,", "tokens": ["In", "sei\u00b7nem", "Wol\u00b7len", ",", "sei\u00b7nem", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "In seinem F\u00fchlen, seinem Hasten", "tokens": ["In", "sei\u00b7nem", "F\u00fch\u00b7len", ",", "sei\u00b7nem", "Has\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.61": {"text": "Nach leichtem Gl\u00fcck \u2014", "tokens": ["Nach", "leich\u00b7tem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.62": {"text": "Du wu\u00dftest, was den Armen qu\u00e4lt,", "tokens": ["Du", "wu\u00df\u00b7test", ",", "was", "den", "Ar\u00b7men", "qu\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "Und was dem Ungl\u00fccklichen,", "tokens": ["Und", "was", "dem", "Un\u00b7gl\u00fcck\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "$,"], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.64": {"text": "Der in den Ketten schmachtet, durch die Seele hegt,", "tokens": ["Der", "in", "den", "Ket\u00b7ten", "schmach\u00b7tet", ",", "durch", "die", "See\u00b7le", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Und was den Menschen packt und sch\u00fcttelt,", "tokens": ["Und", "was", "den", "Men\u00b7schen", "packt", "und", "sch\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.66": {"text": "Sieht er des Schicksals ehernen Schritt", "tokens": ["Sieht", "er", "des", "Schick\u00b7sals", "e\u00b7her\u00b7nen", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.67": {"text": "Zu Boden treten unerbittlich,", 