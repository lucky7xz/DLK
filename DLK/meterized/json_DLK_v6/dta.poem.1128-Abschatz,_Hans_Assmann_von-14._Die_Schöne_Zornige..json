{"dta.poem.1128": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "14. Die Sch\u00f6ne Zornige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dein beherztes Blutt/ von Eyfer angefeuret/ ", "tokens": ["Wenn", "dein", "be\u00b7herz\u00b7tes", "Blutt", "/", "von", "Ey\u00b7fer", "an\u00b7ge\u00b7feu\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$(", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff deine Lippen steigt und aus den Wangen schwizt/", "tokens": ["Auff", "dei\u00b7ne", "Lip\u00b7pen", "steigt", "und", "aus", "den", "Wan\u00b7gen", "schwizt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der innerliche Brand aus beyden Augen blizt/", "tokens": ["Der", "in\u00b7ner\u00b7li\u00b7che", "Brand", "aus", "bey\u00b7den", "Au\u00b7gen", "blizt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wirst du der Pallas gleich zu seyn von mir betheuret.", "tokens": ["Wirst", "du", "der", "Pal\u00b7las", "gleich", "zu", "seyn", "von", "mir", "be\u00b7theu\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "PTKZU", "VAINF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Himmel hat mit Zorn die Helden ausgesteuret/", "tokens": ["Der", "Him\u00b7mel", "hat", "mit", "Zorn", "die", "Hel\u00b7den", "aus\u00b7ge\u00b7steu\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Glutt das Sieges-Schwerdt Achillens zugespizt/", "tokens": ["Die", "Glutt", "das", "Sie\u00b7ges\u00b7Schwerdt", "A\u00b7chil\u00b7lens", "zu\u00b7ge\u00b7spizt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wiltu nicht zornig seyn/ so werd\u2019 ich selbst erhizt/", "tokens": ["Wil\u00b7tu", "nicht", "zor\u00b7nig", "seyn", "/", "so", "werd'", "ich", "selbst", "er\u00b7hizt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJD", "VAINF", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Weil der Verliebten Zorn die Liebe nur verneuret.", "tokens": ["Weil", "der", "Ver\u00b7lieb\u00b7ten", "Zorn", "die", "Lie\u00b7be", "nur", "ver\u00b7neu\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was schadet mir/ was doch nicht lange w\u00e4hren kan?", "tokens": ["Was", "scha\u00b7det", "mir", "/", "was", "doch", "nicht", "lan\u00b7ge", "w\u00e4h\u00b7ren", "kan", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "PWS", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "H\u00f6r\u2019 ich ohn Ungedult der Hunde Bellen an/", "tokens": ["H\u00f6r'", "ich", "ohn", "Un\u00b7ge\u00b7dult", "der", "Hun\u00b7de", "Bel\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ART", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie soll ich um ein Wort mich gegen dir erbittern?", "tokens": ["Wie", "soll", "ich", "um", "ein", "Wort", "mich", "ge\u00b7gen", "dir", "er\u00b7bit\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auff Wind und Regen mu\u00df es wieder besser wittern;", "tokens": ["Auff", "Wind", "und", "Re\u00b7gen", "mu\u00df", "es", "wie\u00b7der", "bes\u00b7ser", "wit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auff Hitze folget Durst: Ich wette/ da\u00df ein Ku\u00df", "tokens": ["Auff", "Hit\u00b7ze", "fol\u00b7get", "Durst", ":", "Ich", "wet\u00b7te", "/", "da\u00df", "ein", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "NN", "$.", "PPER", "VVFIN", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dich/ eh der Morgen k\u00f6mmt/ hinwieder k\u00fchlen mu\u00df.", "tokens": ["Dich", "/", "eh", "der", "Mor\u00b7gen", "k\u00f6mmt", "/", "hin\u00b7wie\u00b7der", "k\u00fch\u00b7len", "mu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KOUS", "ART", "NN", "VVFIN", "$(", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}