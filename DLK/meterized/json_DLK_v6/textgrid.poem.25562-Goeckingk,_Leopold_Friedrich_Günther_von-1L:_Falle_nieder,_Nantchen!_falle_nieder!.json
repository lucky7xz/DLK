{"textgrid.poem.25562": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Falle nieder, Nantchen! falle nieder!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Falle nieder, Nantchen! falle nieder!", "tokens": ["Fal\u00b7le", "nie\u00b7der", ",", "Nant\u00b7chen", "!", "fal\u00b7le", "nie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NE", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine Thr\u00e4nen, werden Freudenlieder,", "tokens": ["Dei\u00b7ne", "Thr\u00e4\u00b7nen", ",", "wer\u00b7den", "Freu\u00b7den\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Deine Seufzer, Dankgebete seyn!", "tokens": ["Dei\u00b7ne", "Seuf\u00b7zer", ",", "Dank\u00b7ge\u00b7be\u00b7te", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Siehe, M\u00e4dchen! sieh! du hast mich wieder!", "tokens": ["Sie\u00b7he", ",", "M\u00e4d\u00b7chen", "!", "sieh", "!", "du", "hast", "mich", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$.", "VVIMP", "$.", "PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Denn ich bleibe hier, und bleibe dein!", "tokens": ["Denn", "ich", "blei\u00b7be", "hier", ",", "und", "blei\u00b7be", "dein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Statt, getrennt, des Nachts, in \u00f6der Ferne,", "tokens": ["Statt", ",", "ge\u00b7trennt", ",", "des", "Nachts", ",", "in", "\u00f6\u00b7der", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "ART", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Aus dem Fenster, Himmel, Mond und Sterne", "tokens": ["Aus", "dem", "Fens\u00b7ter", ",", "Him\u00b7mel", ",", "Mond", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mit bethr\u00e4nten Blicken anzusehn,", "tokens": ["Mit", "be\u00b7thr\u00e4n\u00b7ten", "Bli\u00b7cken", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Soll dein Hauch das Licht der Blendlaterne,", "tokens": ["Soll", "dein", "Hauch", "das", "Licht", "der", "Blend\u00b7la\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Wenn ich gehen will, wie sonst verwehn.", "tokens": ["Wenn", "ich", "ge\u00b7hen", "will", ",", "wie", "sonst", "ver\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "PWAV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Sollst sie wieder in das Gras verstecken,", "tokens": ["Sollst", "sie", "wie\u00b7der", "in", "das", "Gras", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und dann fl\u00fcstern: Schlaf'! ich will dich wecken,", "tokens": ["Und", "dann", "fl\u00fcs\u00b7tern", ":", "Schlaf'", "!", "ich", "will", "dich", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$.", "NN", "$.", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Wenn der Hahn zum zweiten male kr\u00e4ht!", "tokens": ["Wenn", "der", "Hahn", "zum", "zwei\u00b7ten", "ma\u00b7le", "kr\u00e4ht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Sollst mich wieder mit dem Halstuch' decken,", "tokens": ["Sollst", "mich", "wie\u00b7der", "mit", "dem", "Hals\u00b7tuch'", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Wenn der Wind mir in die Augen weht.", "tokens": ["Wenn", "der", "Wind", "mir", "in", "die", "Au\u00b7gen", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Sagt' ich nicht beim Abschied': Wenn ich bliebe,", "tokens": ["Sagt'", "ich", "nicht", "beim", "Ab\u00b7schied'", ":", "Wenn", "ich", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$.", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Machte deine Freundschaft alles tr\u00fcbe,", "tokens": ["Mach\u00b7te", "dei\u00b7ne", "Freund\u00b7schaft", "al\u00b7les", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Doch, getrennt, sey sie ein Strahl des Lichts?", "tokens": ["Doch", ",", "ge\u00b7trennt", ",", "sey", "sie", "ein", "Strahl", "des", "Lichts", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "$,", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Und nun bleib' ich. Liebe denn, ach! Liebe!", "tokens": ["Und", "nun", "bleib'", "ich", ".", "Lie\u00b7be", "denn", ",", "ach", "!", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "NN", "ADV", "$,", "ITJ", "$.", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Keine Freundschaft! alles oder nichts!", "tokens": ["Kei\u00b7ne", "Freund\u00b7schaft", "!", "al\u00b7les", "o\u00b7der", "nichts", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PIS", "KON", "PIS", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "O drum eile, liebstes M\u00e4dchen! Schone", "tokens": ["O", "drum", "ei\u00b7le", ",", "liebs\u00b7tes", "M\u00e4d\u00b7chen", "!", "Scho\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "PAV", "VVFIN", "$,", "ADJA", "NN", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Meiner Sehnsucht! Siehe nur, ich wohne", "tokens": ["Mei\u00b7ner", "Sehn\u00b7sucht", "!", "Sie\u00b7he", "nur", ",", "ich", "woh\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "ADV", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Ganze Meilen von dir, so allein!", "tokens": ["Gan\u00b7ze", "Mei\u00b7len", "von", "dir", ",", "so", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "$,", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Winde denn von Myrthen deine Krone,", "tokens": ["Win\u00b7de", "denn", "von", "Myr\u00b7then", "dei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Und auf ewig, ewig bist du mein!", "tokens": ["Und", "auf", "e\u00b7wig", ",", "e\u00b7wig", "bist", "du", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}