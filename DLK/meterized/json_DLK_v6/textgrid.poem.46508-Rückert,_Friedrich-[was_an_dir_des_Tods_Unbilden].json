{"textgrid.poem.46508": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[was an dir des Tods Unbilden]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was an dir des Tods Unbilden", "tokens": ["Was", "an", "dir", "des", "Tods", "Un\u00b7bil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frevelten, hat mit dem milden", "tokens": ["Fre\u00b7vel\u00b7ten", ",", "hat", "mit", "dem", "mil\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "APPR", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zauberstab gerochen", "tokens": ["Zau\u00b7ber\u00b7stab", "ge\u00b7ro\u00b7chen"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Poesie, die soviel Leben", "tokens": ["Poe\u00b7sie", ",", "die", "so\u00b7viel", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Dir bem\u00fcht ist zuzugeben,", "tokens": ["Dir", "be\u00b7m\u00fcht", "ist", "zu\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als das Schicksal dir hat abgebrochen.", "tokens": ["Als", "das", "Schick\u00b7sal", "dir", "hat", "ab\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Armer Stab! ihm, der so wichtig", "tokens": ["Ar\u00b7mer", "Stab", "!", "ihm", ",", "der", "so", "wich\u00b7tig"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich geberdend, ist so nichtig,", "tokens": ["Sich", "ge\u00b7ber\u00b7dend", ",", "ist", "so", "nich\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei der Stab gebrochen!", "tokens": ["Sei", "der", "Stab", "ge\u00b7bro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Frommen dir die Zauberweisen,", "tokens": ["From\u00b7men", "dir", "die", "Zau\u00b7ber\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die dich in den Himmel preisen,", "tokens": ["Die", "dich", "in", "den", "Him\u00b7mel", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da der Tod dich hat ins Grab gesprochen?", "tokens": ["Da", "der", "Tod", "dich", "hat", "ins", "Grab", "ge\u00b7spro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Doch sie sollen dir nicht frommen,", "tokens": ["Doch", "sie", "sol\u00b7len", "dir", "nicht", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sondern uns zu Gute kommen,", "tokens": ["Son\u00b7dern", "uns", "zu", "Gu\u00b7te", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uebers Grab gesprochen;", "tokens": ["Ue\u00b7bers", "Grab", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dir nicht sollen sie dein Leben,", "tokens": ["Dir", "nicht", "sol\u00b7len", "sie", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sondern uns zur\u00fcck es geben,", "tokens": ["Son\u00b7dern", "uns", "zu\u00b7r\u00fcck", "es", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn nur uns, nicht dir ward's abgebrochen.", "tokens": ["Denn", "nur", "uns", ",", "nicht", "dir", "ward's", "ab\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "PTKNEG", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}