{"textgrid.poem.59564": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woher, du s\u00fc\u00dfes Bild aus Licht gewoben,", "tokens": ["Wo\u00b7her", ",", "du", "s\u00fc\u00b7\u00dfes", "Bild", "aus", "Licht", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPER", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um das die Sch\u00f6nheit flie\u00dfen wie die Sterne", "tokens": ["Um", "das", "die", "Sch\u00f6n\u00b7heit", "flie\u00b7\u00dfen", "wie", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ART", "NN", "VVINF", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umflie\u00dfen jene Burg der blauen Ferne,", "tokens": ["Um\u00b7flie\u00b7\u00dfen", "je\u00b7ne", "Burg", "der", "blau\u00b7en", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Gott die Myriaden Geister loben?", "tokens": ["Wo", "Gott", "die", "My\u00b7ria\u00b7den", "Geis\u00b7ter", "lo\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Hast du hieher, mein Engel, dich erhoben,", "tokens": ["Hast", "du", "hie\u00b7her", ",", "mein", "En\u00b7gel", ",", "dich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "$,", "PPOSAT", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich den Himmel schon auf Erden lerne,", "tokens": ["Da\u00df", "ich", "den", "Him\u00b7mel", "schon", "auf", "Er\u00b7den", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem\u00fctig lieb' und hoff' und dulde gerne,", "tokens": ["De\u00b7m\u00fc\u00b7tig", "lieb'", "und", "hoff'", "und", "dul\u00b7de", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das hei\u00dfe Herz sehns\u00fcchtig stets nach oben?", "tokens": ["Das", "hei\u00b7\u00dfe", "Herz", "sehn\u00b7s\u00fcch\u00b7tig", "stets", "nach", "o\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du winkest mild, wie Himmelsliebe winket,", "tokens": ["Du", "win\u00b7kest", "mild", ",", "wie", "Him\u00b7mels\u00b7lie\u00b7be", "win\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weisest auf die ewig hellen Kerzen", "tokens": ["Und", "wei\u00b7sest", "auf", "die", "e\u00b7wig", "hel\u00b7len", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort oben, auf die bunten Blumen unten;", "tokens": ["Dort", "o\u00b7ben", ",", "auf", "die", "bun\u00b7ten", "Blu\u00b7men", "un\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie du, S\u00fc\u00dfe, l\u00e4chelst, sinkt und sinket,", "tokens": ["Und", "wie", "du", ",", "S\u00fc\u00b7\u00dfe", ",", "l\u00e4\u00b7chelst", ",", "sinkt", "und", "sin\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Sterne zu dem Meer, ein Licht zum Herzen,", "tokens": ["Wie", "Ster\u00b7ne", "zu", "dem", "Meer", ",", "ein", "Licht", "zum", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in Entz\u00fcckung ist das Leid verschwunden.", "tokens": ["Und", "in", "Ent\u00b7z\u00fc\u00b7ckung", "ist", "das", "Leid", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}