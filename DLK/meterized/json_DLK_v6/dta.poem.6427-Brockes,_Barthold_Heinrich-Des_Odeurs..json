{"dta.poem.6427": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Odeurs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zuweilen eckelt uns f\u00fcr etwas, so uns doch", "tokens": ["Zu\u00b7wei\u00b7len", "ec\u00b7kelt", "uns", "f\u00fcr", "et\u00b7was", ",", "so", "uns", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIS", "$,", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vorhin recht schmackhafft vorgekommen,", "tokens": ["Vor\u00b7hin", "recht", "schmack\u00b7hafft", "vor\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn uns der Uberflu\u00df den Appetit benommen.", "tokens": ["Wenn", "uns", "der", "U\u00b7ber\u00b7flu\u00df", "den", "Ap\u00b7pe\u00b7tit", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umsonst ist eine Speise", "tokens": ["Um\u00b7sonst", "ist", "ei\u00b7ne", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Uns angenehm und s\u00fc\u00df auf sonderbare Weise.", "tokens": ["Uns", "an\u00b7ge\u00b7nehm", "und", "s\u00fc\u00df", "auf", "son\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir suchen sie annoch", "tokens": ["Wir", "su\u00b7chen", "sie", "an\u00b7noch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu \u00e4ndern: unsre Zung\u2019 ist ferner nicht ger\u00fchrt,", "tokens": ["Zu", "\u00e4n\u00b7dern", ":", "uns\u00b7re", "Zung'", "ist", "fer\u00b7ner", "nicht", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wofern sie den Geschmack auf andre Art nicht sp\u00fcrt.", "tokens": ["Wo\u00b7fern", "sie", "den", "Ge\u00b7schmack", "auf", "and\u00b7re", "Art", "nicht", "sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Durch neue Spitzen mu\u00df ein anders Schmecken,", "tokens": ["Durch", "neu\u00b7e", "Spit\u00b7zen", "mu\u00df", "ein", "an\u00b7ders", "Schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Nerven Mattigkeit, die gleichsam starr, erwecken.", "tokens": ["Der", "Ner\u00b7ven", "Mat\u00b7tig\u00b7keit", ",", "die", "gleich\u00b7sam", "starr", ",", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADJD", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}