{"textgrid.poem.59277": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vorbei", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der alte Baum nicht mehr,", "tokens": ["Das", "ist", "der", "al\u00b7te", "Baum", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der damals hier gestanden,", "tokens": ["Der", "da\u00b7mals", "hier", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf dem ich gesessen im Bl\u00fctenmeer", "tokens": ["Auf", "dem", "ich", "ge\u00b7ses\u00b7sen", "im", "Bl\u00fc\u00b7ten\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "APPRART", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00dcber den sonnigen Landen.", "tokens": ["\u00dc\u00b7ber", "den", "son\u00b7ni\u00b7gen", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist der Wald nicht mehr, der sacht", "tokens": ["Das", "ist", "der", "Wald", "nicht", "mehr", ",", "der", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Berge rauschte nieder,", "tokens": ["Vom", "Ber\u00b7ge", "rauschte", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wenn ich vom Liebchen ritt bei Nacht,", "tokens": ["Wenn", "ich", "vom", "Lieb\u00b7chen", "ritt", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz voll neuer Lieder.", "tokens": ["Das", "Herz", "voll", "neu\u00b7er", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das ist nicht mehr das tiefe Tal", "tokens": ["Das", "ist", "nicht", "mehr", "das", "tie\u00b7fe", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit den grasenden Rehen,", "tokens": ["Mit", "den", "gra\u00b7sen\u00b7den", "Re\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "In das wir nachts vieltausendmal", "tokens": ["In", "das", "wir", "nachts", "viel\u00b7tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zusammen hinausgesehen. \u2013", "tokens": ["Zu\u00b7sam\u00b7men", "hin\u00b7aus\u00b7ge\u00b7se\u00b7hen", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Es ist der Baum noch, Tal und Wald,", "tokens": ["Es", "ist", "der", "Baum", "noch", ",", "Tal", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ist jung geblieben,", "tokens": ["Die", "Welt", "ist", "jung", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du aber wurdest seitdem alt,", "tokens": ["Du", "a\u00b7ber", "wur\u00b7dest", "seit\u00b7dem", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vorbei ist das sch\u00f6ne Lieben.", "tokens": ["Vor\u00b7bei", "ist", "das", "sch\u00f6\u00b7ne", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}