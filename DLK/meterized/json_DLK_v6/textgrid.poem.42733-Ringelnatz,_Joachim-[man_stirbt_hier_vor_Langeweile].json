{"textgrid.poem.42733": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[man stirbt hier vor Langeweile]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man stirbt hier vor Langeweile,", "tokens": ["Man", "stirbt", "hier", "vor", "Lan\u00b7ge\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dachte die Nagelfeile", "tokens": ["Dach\u00b7te", "die", "Na\u00b7gel\u00b7fei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Beim Mittagessen!", "tokens": ["Beim", "Mit\u00b7ta\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und machte sich, wie von ungef\u00e4hr,", "tokens": ["Und", "mach\u00b7te", "sich", ",", "wie", "von", "un\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "PWAV", "APPR", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00dcber den Fingernagel her,", "tokens": ["\u00dc\u00b7ber", "den", "Fin\u00b7ger\u00b7na\u00b7gel", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Beim Mittagessen!", "tokens": ["Beim", "Mit\u00b7ta\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da begann eine silberne Gabel zu schrein:", "tokens": ["Da", "be\u00b7gann", "ei\u00b7ne", "sil\u00b7ber\u00b7ne", "Ga\u00b7bel", "zu", "schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "\u00bbmeine Dame \u2013 \u2013 Sie sind hier nicht allein!\u00ab", "tokens": ["\u00bb", "mei\u00b7ne", "Da\u00b7me", "\u2013", "\u2013", "Sie", "sind", "hier", "nicht", "al\u00b7lein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$(", "$(", "PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}