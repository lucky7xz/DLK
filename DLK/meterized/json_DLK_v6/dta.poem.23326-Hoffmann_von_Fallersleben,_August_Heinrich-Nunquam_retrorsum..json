{"dta.poem.23326": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nunquam retrorsum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht betteln, nicht bitten!", "tokens": ["Nicht", "bet\u00b7teln", ",", "nicht", "bit\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nur muthig gestritten!", "tokens": ["Nur", "mut\u00b7hig", "ge\u00b7strit\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nie k\u00e4mpft es sich schlecht", "tokens": ["Nie", "k\u00e4mpft", "es", "sich", "schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "F\u00fcr Freiheit und Recht.", "tokens": ["F\u00fcr", "Frei\u00b7heit", "und", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Und nimmer verzaget!", "tokens": ["Und", "nim\u00b7mer", "ver\u00b7za\u00b7get", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von Neuem gewaget!", "tokens": ["Von", "Neu\u00b7em", "ge\u00b7wa\u00b7get", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und muthig voran!", "tokens": ["Und", "mut\u00b7hig", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "So zeigt sich der Mann.", "tokens": ["So", "zeigt", "sich", "der", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wir wollen belachen", "tokens": ["Wir", "wol\u00b7len", "be\u00b7la\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Feigen und Schwachen:", "tokens": ["Die", "Fei\u00b7gen", "und", "Schwa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wer steht wie ein Held,", "tokens": ["Wer", "steht", "wie", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dem bleibet das Feld.", "tokens": ["Dem", "blei\u00b7bet", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Einst wird es sich wenden,", "tokens": ["Einst", "wird", "es", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Einst mu\u00df es sich enden", "tokens": ["Einst", "mu\u00df", "es", "sich", "en\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zu unserem Gl\u00fcck:", "tokens": ["Zu", "un\u00b7se\u00b7rem", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Drum nimmer zur\u00fcck!", "tokens": ["Drum", "nim\u00b7mer", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}