{"textgrid.poem.64028": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Waldstimme", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie deine gr\u00fcngoldenen Augen funkeln,", "tokens": ["Wie", "dei\u00b7ne", "gr\u00fcn\u00b7gol\u00b7de\u00b7nen", "Au\u00b7gen", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wald, du moosiger Tr\u00e4umer,", "tokens": ["Wald", ",", "du", "moo\u00b7si\u00b7ger", "Tr\u00e4u\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wie so versonnen deine Gedanken dunkeln,", "tokens": ["Wie", "so", "ver\u00b7son\u00b7nen", "dei\u00b7ne", "Ge\u00b7dan\u00b7ken", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Saftstrotzender Tagesvers\u00e4umer,", "tokens": ["Saft\u00b7strot\u00b7zen\u00b7der", "Ta\u00b7ges\u00b7ver\u00b7s\u00e4u\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Einsiedel, schwer von Leben!", "tokens": ["Ein\u00b7sie\u00b7del", ",", "schwer", "von", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00dcber der Wipfel Hin- und Wiederschweben:", "tokens": ["\u00dc\u00b7ber", "der", "Wip\u00b7fel", "Hin", "und", "Wie\u00b7der\u00b7schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "TRUNC", "KON", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie's Atem holt", "tokens": ["Wie's", "A\u00b7tem", "holt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "und n\u00e4her kommt", "tokens": ["und", "n\u00e4\u00b7her", "kommt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "und braust,", "tokens": ["und", "braust", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Und weiter zieht", "tokens": ["Und", "wei\u00b7ter", "zieht"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "und stille wird", "tokens": ["und", "stil\u00b7le", "wird"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "und saust!", "tokens": ["und", "saust", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "\u00dcber der Wipfel Hin- und Wiederschweben,", "tokens": ["\u00dc\u00b7ber", "der", "Wip\u00b7fel", "Hin", "und", "Wie\u00b7der\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "TRUNC", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Hochoben steht ein ernster Ton,", "tokens": ["Hoch\u00b7o\u00b7ben", "steht", "ein", "erns\u00b7ter", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dem lauschten tausend Jahre schon", "tokens": ["Dem", "lauschten", "tau\u00b7send", "Jah\u00b7re", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "CARD", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und werden tausend Jahre lauschen.", "tokens": ["Und", "wer\u00b7den", "tau\u00b7send", "Jah\u00b7re", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer dieses starke, donnerdunkle Rauschen.", "tokens": ["Und", "im\u00b7mer", "die\u00b7ses", "star\u00b7ke", ",", "don\u00b7ner\u00b7dunk\u00b7le", "Rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}