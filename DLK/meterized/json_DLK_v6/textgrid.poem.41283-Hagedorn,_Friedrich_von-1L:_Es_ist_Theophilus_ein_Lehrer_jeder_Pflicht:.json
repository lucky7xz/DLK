{"textgrid.poem.41283": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist Theophilus ein Lehrer jeder Pflicht:", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist Theophilus ein Lehrer jeder Pflicht:", "tokens": ["Es", "ist", "Theo\u00b7phi\u00b7lus", "ein", "Leh\u00b7rer", "je\u00b7der", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ART", "NN", "PIAT", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "So heilig wie sein Amt, so wahr als sein Gesicht:", "tokens": ["So", "hei\u00b7lig", "wie", "sein", "Amt", ",", "so", "wahr", "als", "sein", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Irrthum billig feind, ohn' Irrende zu hassen:", "tokens": ["Dem", "Irr\u00b7thum", "bil\u00b7lig", "feind", ",", "ohn'", "Ir\u00b7ren\u00b7de", "zu", "has\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Voll Liebe, wie sein Gott, und, als sein Knecht, gelassen:", "tokens": ["Voll", "Lie\u00b7be", ",", "wie", "sein", "Gott", ",", "und", ",", "als", "sein", "Knecht", ",", "ge\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PWAV", "PPOSAT", "NN", "$,", "KON", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur eifrig f\u00fcr das Wort: besorgt f\u00fcr aller Heil,", "tokens": ["Nur", "eif\u00b7rig", "f\u00fcr", "das", "Wort", ":", "be\u00b7sorgt", "f\u00fcr", "al\u00b7ler", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$.", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und keinem Eigennutz und keiner Meinung feil.", "tokens": ["Und", "kei\u00b7nem", "Ei\u00b7gen\u00b7nutz", "und", "kei\u00b7ner", "Mei\u00b7nung", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er sucht die Ehre nicht, noch G\u00fcter dieser Erde;", "tokens": ["Er", "sucht", "die", "Eh\u00b7re", "nicht", ",", "noch", "G\u00fc\u00b7ter", "die\u00b7ser", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "ADV", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Ehre suchet ihn, damit sie edler werde.", "tokens": ["Die", "Eh\u00b7re", "su\u00b7chet", "ihn", ",", "da\u00b7mit", "sie", "ed\u00b7ler", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er unterscheidet sich so sehr vom Geist der Welt,", "tokens": ["Er", "un\u00b7ter\u00b7schei\u00b7det", "sich", "so", "sehr", "vom", "Geist", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df er, im Priesterrock, uns, und nicht sich, gef\u00e4llt.", "tokens": ["Da\u00df", "er", ",", "im", "Pries\u00b7ter\u00b7rock", ",", "uns", ",", "und", "nicht", "sich", ",", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "$,", "PPER", "$,", "KON", "PTKNEG", "PRF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}