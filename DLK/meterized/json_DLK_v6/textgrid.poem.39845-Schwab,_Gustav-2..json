{"textgrid.poem.39845": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Get\u00e4uscht hast du uns oft mit eiteln M\u00e4ren,", "tokens": ["Ge\u00b7t\u00e4uscht", "hast", "du", "uns", "oft", "mit", "ei\u00b7teln", "M\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wunderbarste keck uns vorgedichtet,", "tokens": ["Das", "Wun\u00b7der\u00b7bars\u00b7te", "keck", "uns", "vor\u00b7ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn wir dann uns glaubig aufgerichtet,", "tokens": ["Und", "wenn", "wir", "dann", "uns", "glau\u00b7big", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da freut' es dich, des Trugs uns zu belehren!", "tokens": ["Da", "freut'", "es", "dich", ",", "des", "Trugs", "uns", "zu", "be\u00b7leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun willst du wieder einmal uns beth\u00f6ren!", "tokens": ["Nun", "willst", "du", "wie\u00b7der", "ein\u00b7mal", "uns", "be\u00b7th\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom grausen Kampf, der blutig sich geschlichtet,", "tokens": ["Vom", "grau\u00b7sen", "Kampf", ",", "der", "blu\u00b7tig", "sich", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie der Zufall f\u00fcrchterlich gerichtet,", "tokens": ["Und", "wie", "der", "Zu\u00b7fall", "f\u00fcrch\u00b7ter\u00b7lich", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom eignen Tode l\u00e4ssest gar du h\u00f6ren?", "tokens": ["Vom", "eig\u00b7nen", "To\u00b7de", "l\u00e4s\u00b7sest", "gar", "du", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O tritt hervor, du hast dich gnug verborgen!", "tokens": ["O", "tritt", "her\u00b7vor", ",", "du", "hast", "dich", "gnug", "ver\u00b7bor\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Komm, widersprich dem gr\u00e4\u00dflichen Ger\u00fcchte:", "tokens": ["Komm", ",", "wi\u00b7der\u00b7sprich", "dem", "gr\u00e4\u00df\u00b7li\u00b7chen", "Ge\u00b7r\u00fcch\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es l\u00e4\u00dft der Tod im Wort nicht mit sich scherzen.", "tokens": ["Es", "l\u00e4\u00dft", "der", "Tod", "im", "Wort", "nicht", "mit", "sich", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKNEG", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch still und stumm vergeht der rote Morgen.", "tokens": ["Doch", "still", "und", "stumm", "ver\u00b7geht", "der", "ro\u00b7te", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So ist es denn nicht eitle Truggeschichte,", "tokens": ["So", "ist", "es", "denn", "nicht", "eit\u00b7le", "Trug\u00b7ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und durch die Gassen geht der Ruf der Schmerzen!", "tokens": ["Und", "durch", "die", "Gas\u00b7sen", "geht", "der", "Ruf", "der", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}