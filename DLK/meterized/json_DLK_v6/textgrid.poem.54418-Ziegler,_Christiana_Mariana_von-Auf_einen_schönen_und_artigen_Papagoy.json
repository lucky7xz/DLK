{"textgrid.poem.54418": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen sch\u00f6nen und artigen Papagoy", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat dich die Natur recht herrlich aus geschm\u00fccket;", "tokens": ["Es", "hat", "dich", "die", "Na\u00b7tur", "recht", "herr\u00b7lich", "aus", "ge\u00b7schm\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Glanz ist ungemein, man bleibt dabey entz\u00fccket.", "tokens": ["Dein", "Glanz", "ist", "un\u00b7ge\u00b7mein", ",", "man", "bleibt", "da\u00b7bey", "ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "PIS", "VVFIN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sage was man will von aller Farben Kunst:", "tokens": ["Man", "sa\u00b7ge", "was", "man", "will", "von", "al\u00b7ler", "Far\u00b7ben", "Kunst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PWS", "PIS", "VMFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist und bleibt f\u00fcrwahr nur eitler W\u00f6rter Dunst.", "tokens": ["Es", "ist", "und", "bleibt", "f\u00fcr\u00b7wahr", "nur", "eit\u00b7ler", "W\u00f6r\u00b7ter", "Dunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ADV", "ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich seh das muntre Gr\u00fcn, mit roth und gelb vermenget;", "tokens": ["Ich", "seh", "das", "mun\u00b7tre", "Gr\u00fcn", ",", "mit", "roth", "und", "gelb", "ver\u00b7men\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie sich der weisse Strahl mit in den Schnabel dr\u00e4nget;", "tokens": ["Wie", "sich", "der", "weis\u00b7se", "Strahl", "mit", "in", "den", "Schna\u00b7bel", "dr\u00e4n\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O Anblick, der f\u00fcrwahr mir alle Sinnen r\u00fchrt!", "tokens": ["O", "An\u00b7blick", ",", "der", "f\u00fcr\u00b7wahr", "mir", "al\u00b7le", "Sin\u00b7nen", "r\u00fchrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihr K\u00fcnstler, saget frey! seyd ihr nicht \u00fcberf\u00fchrt,", "tokens": ["Ihr", "K\u00fcnst\u00b7ler", ",", "sa\u00b7get", "frey", "!", "seyd", "ihr", "nicht", "\u00fc\u00b7berf\u00b7\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$.", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Wirkung der Natur hat euch hier \u00fcbertroffen?", "tokens": ["Die", "Wir\u00b7kung", "der", "Na\u00b7tur", "hat", "euch", "hier", "\u00fc\u00b7ber\u00b7trof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Ihr schlechten Redner h\u00f6rt, ihr k\u00f6nnt ein gleiches hoffen.", "tokens": ["Ihr", "schlech\u00b7ten", "Red\u00b7ner", "h\u00f6rt", ",", "ihr", "k\u00f6nnt", "ein", "glei\u00b7ches", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kaum da\u00df sein zartes Ohr sich nach der Stimme richt,", "tokens": ["Kaum", "da\u00df", "sein", "zar\u00b7tes", "Ohr", "sich", "nach", "der", "Stim\u00b7me", "richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die nur von ohngefehr ein W\u00f6rtchen zu ihm spricht,", "tokens": ["Die", "nur", "von", "ohn\u00b7ge\u00b7fehr", "ein", "W\u00f6rt\u00b7chen", "zu", "ihm", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJD", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So sagt er deutlich noch was man von ihm verlanget.", "tokens": ["So", "sagt", "er", "deut\u00b7lich", "noch", "was", "man", "von", "ihm", "ver\u00b7lan\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "PWS", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Unschuld redet hier, die nicht mit Worten pranget.", "tokens": ["Die", "Un\u00b7schuld", "re\u00b7det", "hier", ",", "die", "nicht", "mit", "Wor\u00b7ten", "pran\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er speist sein Zuckerbrodt, steigt in dem Baur herum,", "tokens": ["Er", "speist", "sein", "Zu\u00b7cker\u00b7brodt", ",", "steigt", "in", "dem", "Baur", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sieht sich in keiner Schrift nach Wort und Einfall um.", "tokens": ["Sieht", "sich", "in", "kei\u00b7ner", "Schrift", "nach", "Wort", "und", "Ein\u00b7fall", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ihn plagt kein schwarzer Neid, er will sich nicht verstellen;", "tokens": ["Ihn", "plagt", "kein", "schwar\u00b7zer", "Neid", ",", "er", "will", "sich", "nicht", "ver\u00b7stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,", "PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Kann er gleich als ein Hund mit seinem Stimmchen bellen.", "tokens": ["Kann", "er", "gleich", "als", "ein", "Hund", "mit", "sei\u00b7nem", "Stimm\u00b7chen", "bel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Er lacht, er pfeift, er singt, wenn sich die Zunge regt", "tokens": ["Er", "lacht", ",", "er", "pfeift", ",", "er", "singt", ",", "wenn", "sich", "die", "Zun\u00b7ge", "regt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So wird ein neuer Werth auch an den Tag gelegt.", "tokens": ["So", "wird", "ein", "neu\u00b7er", "Werth", "auch", "an", "den", "Tag", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie sollte nicht mein Freund den klugen Vogel lieben?", "tokens": ["Wie", "soll\u00b7te", "nicht", "mein", "Freund", "den", "klu\u00b7gen", "Vo\u00b7gel", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PTKNEG", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wer ihn nur h\u00f6rt und sieht, wird dazu angetrieben.", "tokens": ["Wer", "ihn", "nur", "h\u00f6rt", "und", "sieht", ",", "wird", "da\u00b7zu", "an\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich sorge wahrlich selbst, da\u00df ihn kein Unfall schreckt,", "tokens": ["Ich", "sor\u00b7ge", "wahr\u00b7lich", "selbst", ",", "da\u00df", "ihn", "kein", "Un\u00b7fall", "schreckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und da\u00df kein Katzenkopf sich nach dem Bauer streckt.", "tokens": ["Und", "da\u00df", "kein", "Kat\u00b7zen\u00b7kopf", "sich", "nach", "dem", "Bau\u00b7er", "streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mein Papchen lebe wohl, belache alle Thoren.", "tokens": ["Mein", "Pap\u00b7chen", "le\u00b7be", "wohl", ",", "be\u00b7la\u00b7che", "al\u00b7le", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "ADJA", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die nicht so edel sind in ihrer Art gebohren.", "tokens": ["Die", "nicht", "so", "e\u00b7del", "sind", "in", "ih\u00b7rer", "Art", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du sprichst dein gutes Deutsch, dein rein gesetzt Latein,", "tokens": ["Du", "sprichst", "dein", "gu\u00b7tes", "Deutsch", ",", "dein", "rein", "ge\u00b7setzt", "La\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Kanst manchem der es lehrt, darinn ein Muster seyn.", "tokens": ["Kanst", "man\u00b7chem", "der", "es", "lehrt", ",", "da\u00b7rinn", "ein", "Mus\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "PRELS", "PPER", "VVFIN", "$,", "PAV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}