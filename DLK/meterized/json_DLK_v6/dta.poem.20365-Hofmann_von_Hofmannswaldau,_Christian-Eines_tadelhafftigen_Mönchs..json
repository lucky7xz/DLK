{"dta.poem.20365": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Eines tadelhafftigen M\u00f6nchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich glaubt'/ und wei\u00df nicht wie/ ich sang/ und wei\u00df nicht\nwas.", "tokens": ["Ich", "glaubt'", "/", "und", "wei\u00df", "nicht", "wie", "/", "ich", "sang", "/", "und", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "PTKNEG", "KOKOM", "$(", "PPER", "VVFIN", "$(", "KON", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein teuffel war ein buch/ mein heiligthum ein gla\u00df.", "tokens": ["Mein", "teuf\u00b7fel", "war", "ein", "buch", "/", "mein", "hei\u00b7lig\u00b7thum", "ein", "gla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein tod die fasten-zeit/ die kirche meine h\u00f6lle.", "tokens": ["Mein", "tod", "die", "fas\u00b7ten\u00b7zeit", "/", "die", "kir\u00b7che", "mei\u00b7ne", "h\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich ruffe hier zu Gott/ wie vor in meiner zelle.", "tokens": ["Ich", "ruf\u00b7fe", "hier", "zu", "Gott", "/", "wie", "vor", "in", "mei\u00b7ner", "zel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$(", "KOKOM", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}