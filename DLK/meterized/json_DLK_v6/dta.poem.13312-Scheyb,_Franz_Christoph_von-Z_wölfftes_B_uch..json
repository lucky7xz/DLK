{"dta.poem.13312": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eallein ich gebe zu, da\u00df meine Meinung fehlt,", "tokens": ["\u201e", "al\u00b7lein", "ich", "ge\u00b7be", "zu", ",", "da\u00df", "mei\u00b7ne", "Mei\u00b7nung", "fehlt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "350\u201dUnd jede diesen Plaz mit Recht f\u00fcr sich erw\u00e4hlt.", "tokens": ["\"", "Und", "je\u00b7de", "die\u00b7sen", "Plaz", "mit", "Recht", "f\u00fcr", "sich", "er\u00b7w\u00e4hlt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIAT", "PDAT", "NN", "APPR", "NN", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201esagt: meine Sorge sey betr\u00fcglich oder nichtig;", "tokens": ["\u201e", "sagt", ":", "mei\u00b7ne", "Sor\u00b7ge", "sey", "be\u00b7tr\u00fcg\u00b7lich", "o\u00b7der", "nich\u00b7tig", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201enennt jede von dem Rath zum Herrschungs-Ruder t\u00fcchtig;", "tokens": ["\u201e", "nennt", "je\u00b7de", "von", "dem", "Rath", "zum", "Herr\u00b7schungs\u00b7Ru\u00b7der", "t\u00fcch\u00b7tig", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIAT", "APPR", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eja wann ihr insgesammt zum Herrschen einig seyt", "tokens": ["\u201e", "ja", "wann", "ihr", "ins\u00b7ge\u00b7sammt", "zum", "Herr\u00b7schen", "ei\u00b7nig", "seyt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PWAV", "PPER", "ADV", "APPRART", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eso lasset Eine nur entfernt: die Danckbarkeit;", "tokens": ["\u201e", "so", "las\u00b7set", "Ei\u00b7ne", "nur", "ent\u00b7fernt", ":", "die", "Dan\u00b7ck\u00b7bar\u00b7keit", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADV", "ADJD", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "355\u201dAlsdann geht hin, regiert und waltet nach Belieben;", "tokens": ["\"", "Als\u00b7dann", "geht", "hin", ",", "re\u00b7giert", "und", "wal\u00b7tet", "nach", "Be\u00b7lie\u00b7ben", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PTKVZ", "$,", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewas folgte, wann ihr nicht die Tugend w\u00fcrdet \u00fcben?", "tokens": ["\u201e", "was", "folg\u00b7te", ",", "wann", "ihr", "nicht", "die", "Tu\u00b7gend", "w\u00fcr\u00b7det", "\u00fc\u00b7ben", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$,", "PWAV", "PPER", "PTKNEG", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eich gr\u00fcnde meinen Schlu\u00df auf den bekannten Spruch:", "tokens": ["\u201e", "ich", "gr\u00fcn\u00b7de", "mei\u00b7nen", "Schlu\u00df", "auf", "den", "be\u00b7kann\u00b7ten", "Spruch", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eder Undanck stiftet nichts, als steten Friedens-Bruch.", "tokens": ["\u201e", "der", "Un\u00b7danck", "stif\u00b7tet", "nichts", ",", "als", "ste\u00b7ten", "Frie\u00b7dens\u00b7Bruch", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PIS", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}