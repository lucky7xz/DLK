{"textgrid.poem.26525": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O da\u00df ich es verm\u00f6chte, wahnbesessen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ich es verm\u00f6chte, wahnbesessen,", "tokens": ["O", "da\u00df", "ich", "es", "ver\u00b7m\u00f6ch\u00b7te", ",", "wahn\u00b7be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie es die Menschen thun, ganz ohne Zagen", "tokens": ["Wie", "es", "die", "Men\u00b7schen", "thun", ",", "ganz", "oh\u00b7ne", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der eig'nen Schuld die Andern anzuklagen", "tokens": ["Der", "eig'\u00b7nen", "Schuld", "die", "An\u00b7dern", "an\u00b7zu\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Fehler eitel zu vergessen.", "tokens": ["Und", "mei\u00b7ne", "Feh\u00b7ler", "ei\u00b7tel", "zu", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "K\u00f6nnt' ich, wie sie, aus jeder Traube pressen \u2013", "tokens": ["K\u00f6nnt'", "ich", ",", "wie", "sie", ",", "aus", "je\u00b7der", "Trau\u00b7be", "pres\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPER", "$,", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begehren, wo es Pflicht ist, zu entsagen:", "tokens": ["Be\u00b7geh\u00b7ren", ",", "wo", "es", "Pflicht", "ist", ",", "zu", "ent\u00b7sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann w\u00fcrd' auch ich mit schmunzelndem Behagen", "tokens": ["Dann", "w\u00fcrd'", "auch", "ich", "mit", "schmun\u00b7zeln\u00b7dem", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom reichen Tische dieses Lebens essen.", "tokens": ["Vom", "rei\u00b7chen", "Ti\u00b7sche", "die\u00b7ses", "Le\u00b7bens", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich w\u00fcrde nimmermehr erfahren haben,", "tokens": ["Ich", "w\u00fcr\u00b7de", "nim\u00b7mer\u00b7mehr", "er\u00b7fah\u00b7ren", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was es bedeutet, still sich \u00fcberwinden,", "tokens": ["Was", "es", "be\u00b7deu\u00b7tet", ",", "still", "sich", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und zu entbehren, statt sich frech zu laben.", "tokens": ["Und", "zu", "ent\u00b7beh\u00b7ren", ",", "statt", "sich", "frech", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,", "KOUI", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den h\u00f6chsten Preis w\u00fcrd' ich zu niedrig finden,", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Preis", "w\u00fcrd'", "ich", "zu", "nied\u00b7rig", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht, wie jetzt, des Schicksals kargste Gaben", "tokens": ["Und", "nicht", ",", "wie", "jetzt", ",", "des", "Schick\u00b7sals", "kargs\u00b7te", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "ADV", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als unverdiente Gnade noch empfinden.", "tokens": ["Als", "un\u00b7ver\u00b7dien\u00b7te", "Gna\u00b7de", "noch", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}