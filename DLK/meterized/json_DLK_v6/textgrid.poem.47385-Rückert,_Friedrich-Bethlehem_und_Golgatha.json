{"textgrid.poem.47385": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Bethlehem und Golgatha", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ist in Bethlehem geboren,", "tokens": ["Er", "ist", "in", "Beth\u00b7le\u00b7hem", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns das Leben hat gebracht,", "tokens": ["Der", "uns", "das", "Le\u00b7ben", "hat", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Golgatha hat er erkoren,", "tokens": ["Und", "Gol\u00b7ga\u00b7tha", "hat", "er", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Durchs Kreuz zu brechen Todes Macht.", "tokens": ["Durchs", "Kreuz", "zu", "bre\u00b7chen", "To\u00b7des", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich fuhr vom abendlichen Strande", "tokens": ["Ich", "fuhr", "vom", "a\u00b7bend\u00b7li\u00b7chen", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hinaus, hindurch die Morgenlande;", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7durch", "die", "Mor\u00b7gen\u00b7lan\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Gr\u00f6\u00dferes ich nirgend sah", "tokens": ["Und", "Gr\u00f6\u00b7\u00dfe\u00b7res", "ich", "nir\u00b7gend", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als Bethlehem und Golgatha.", "tokens": ["Als", "Beth\u00b7le\u00b7hem", "und", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie sind die sieben Wunderwerke", "tokens": ["Wie", "sind", "die", "sie\u00b7ben", "Wun\u00b7der\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der alten Welt dahingerafft,", "tokens": ["Der", "al\u00b7ten", "Welt", "da\u00b7hin\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ist der Trotz der ird'schen St\u00e4rke", "tokens": ["Wie", "ist", "der", "Trotz", "der", "ird'\u00b7schen", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erlegen vor der Himmelskraft!", "tokens": ["Er\u00b7le\u00b7gen", "vor", "der", "Him\u00b7mels\u00b7kraft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah sie, wo ich mochte wallen,", "tokens": ["Ich", "sah", "sie", ",", "wo", "ich", "moch\u00b7te", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In ihre Tr\u00fcmmer hingefallen", "tokens": ["In", "ih\u00b7re", "Tr\u00fcm\u00b7mer", "hin\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und stehn in stiller Gloria", "tokens": ["Und", "stehn", "in", "stil\u00b7ler", "Glo\u00b7ria"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur Bethlehem und Golgatha.", "tokens": ["Nur", "Beth\u00b7le\u00b7hem", "und", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Weg ihr \u00e4gypt'schen Pyramiden!", "tokens": ["Weg", "ihr", "\u00e4\u00b7gypt'\u00b7schen", "Py\u00b7ra\u00b7mi\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In denen nur die Finsternis", "tokens": ["In", "de\u00b7nen", "nur", "die", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Grabes, nicht des Todes Frieden", "tokens": ["Des", "Gra\u00b7bes", ",", "nicht", "des", "To\u00b7des", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu bauen sich der Mensch befli\u00df.", "tokens": ["Zu", "bau\u00b7en", "sich", "der", "Mensch", "be\u00b7fli\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Sphinx' in kolossalen Gr\u00f6\u00dfen,", "tokens": ["Ihr", "Sphinx'", "in", "ko\u00b7los\u00b7sa\u00b7len", "Gr\u00f6\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr konntet nicht der Erde l\u00f6sen", "tokens": ["Ihr", "konn\u00b7tet", "nicht", "der", "Er\u00b7de", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Lebens R\u00e4tsel, wie's geschah", "tokens": ["Des", "Le\u00b7bens", "R\u00e4t\u00b7sel", ",", "wie's", "ge\u00b7schah"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch Bethlehem und Golgatha.", "tokens": ["Durch", "Beth\u00b7le\u00b7hem", "und", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Erdparadies am Roknabade,", "tokens": ["Erd\u00b7pa\u00b7ra\u00b7dies", "am", "Ro\u00b7kna\u00b7ba\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Flur aller Rosen von Schiras!", "tokens": ["Flur", "al\u00b7ler", "Ro\u00b7sen", "von", "Schi\u00b7ras", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und am gew\u00fcrzten Meergestade", "tokens": ["Und", "am", "ge\u00b7w\u00fcrz\u00b7ten", "Meer\u00b7ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du Palmengarten Indias!", "tokens": ["Du", "Pal\u00b7men\u00b7gar\u00b7ten", "In\u00b7di\u00b7as", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich seh' auf euren lichten Fluren", "tokens": ["Ich", "seh'", "auf", "eu\u00b7ren", "lich\u00b7ten", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch gehn den Tod mit dunklen Spuren:", "tokens": ["Noch", "gehn", "den", "Tod", "mit", "dunk\u00b7len", "Spu\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Blickt auf! Euch kommt das Leben da", "tokens": ["Blickt", "auf", "!", "Euch", "kommt", "das", "Le\u00b7ben", "da"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Bethlehem und Golgatha.", "tokens": ["Von", "Beth\u00b7le\u00b7hem", "und", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du Kaaba, schwarzer Stein der W\u00fcste,", "tokens": ["Du", "Ka\u00b7a\u00b7ba", ",", "schwar\u00b7zer", "Stein", "der", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An den der Fu\u00df der halben Welt", "tokens": ["An", "den", "der", "Fu\u00df", "der", "hal\u00b7ben", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich jetzt noch st\u00f6\u00dft, steh' nur und br\u00fcste", "tokens": ["Sich", "jetzt", "noch", "st\u00f6\u00dft", ",", "steh'", "nur", "und", "br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "ADV", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich, matt von deinem Mond erhellt!", "tokens": ["Dich", ",", "matt", "von", "dei\u00b7nem", "Mond", "er\u00b7hellt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Mond wird vor der Sonn' erbleichen,", "tokens": ["Der", "Mond", "wird", "vor", "der", "Sonn'", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dich zerschmettern wird das Zeichen", "tokens": ["Und", "dich", "zer\u00b7schmet\u00b7tern", "wird", "das", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Helden, dem Viktoria", "tokens": ["Des", "Hel\u00b7den", ",", "dem", "Vik\u00b7to\u00b7ria"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ruft Bethlehem und Golgatha.", "tokens": ["Ruft", "Beth\u00b7le\u00b7hem", "und", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O der du in der Hirten Krippe", "tokens": ["O", "der", "du", "in", "der", "Hir\u00b7ten", "Krip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kind geboren wolltest sein,", "tokens": ["Ein", "Kind", "ge\u00b7bo\u00b7ren", "woll\u00b7test", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, leidend Pein am Kreuzgerippe,", "tokens": ["Und", ",", "lei\u00b7dend", "Pein", "am", "Kreuz\u00b7ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von uns genommen hast die Pein!", "tokens": ["Von", "uns", "ge\u00b7nom\u00b7men", "hast", "die", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Krippe d\u00fcnkt dem Stolze niedrig,", "tokens": ["Die", "Krip\u00b7pe", "d\u00fcnkt", "dem", "Stol\u00b7ze", "nied\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist das Kreuz dem Hochmut widrig;", "tokens": ["Es", "ist", "das", "Kreuz", "dem", "Hoch\u00b7mut", "wid\u00b7rig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du aber bist der Demut nah'", "tokens": ["Du", "a\u00b7ber", "bist", "der", "De\u00b7mut", "nah'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Bethlehem und Golgatha.", "tokens": ["In", "Beth\u00b7le\u00b7hem", "und", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die K\u00f6n'ge kamen anzubeten", "tokens": ["Die", "K\u00f6n'\u00b7ge", "ka\u00b7men", "an\u00b7zu\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Hirtenstern, das Opferlamm,", "tokens": ["Den", "Hir\u00b7tens\u00b7tern", ",", "das", "Op\u00b7fer\u00b7lamm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und V\u00f6lker haben angetreten", "tokens": ["Und", "V\u00f6l\u00b7ker", "ha\u00b7ben", "an\u00b7ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Pilgerfahrt zum Kreuzesstamm.", "tokens": ["Die", "Pil\u00b7ger\u00b7fahrt", "zum", "Kreu\u00b7zes\u00b7stamm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ging in Kampfes Ungewitter", "tokens": ["Es", "ging", "in", "Kamp\u00b7fes", "Un\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Welt, doch nicht das Kreuz in Splitter,", "tokens": ["Die", "Welt", ",", "doch", "nicht", "das", "Kreuz", "in", "Split\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PTKNEG", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als Ost und West sich k\u00e4mpfen sah", "tokens": ["Als", "Ost", "und", "West", "sich", "k\u00e4mp\u00b7fen", "sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "VVFIN", "PRF", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um Bethlehem und Golgatha.", "tokens": ["Um", "Beth\u00b7le\u00b7hem", "und", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "O la\u00dft uns nicht mit Lanzenknechten,", "tokens": ["O", "la\u00dft", "uns", "nicht", "mit", "Lan\u00b7zen\u00b7knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"