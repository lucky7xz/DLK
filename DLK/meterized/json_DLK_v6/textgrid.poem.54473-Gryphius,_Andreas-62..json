{"textgrid.poem.54473": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist iemals/ weil der Bau der grossen Welt gestanden/", "tokens": ["Ist", "ie\u00b7mals", "/", "weil", "der", "Bau", "der", "gros\u00b7sen", "Welt", "ge\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$(", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So grimme Tyranney/ vnd Greuel auch erh\u00f6rt?", "tokens": ["So", "grim\u00b7me", "Ty\u00b7ran\u00b7ney", "/", "vnd", "Greu\u00b7el", "auch", "er\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "KON", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist was/ das nicht durch Krieg/ Schwerdt/ Fla i' vnd Spitz zust\u00f6rt!", "tokens": ["Ist", "was", "/", "das", "nicht", "durch", "Krieg", "/", "Schwerdt", "/", "Fla", "i'", "vnd", "Spitz", "zu\u00b7st\u00f6rt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$(", "PDS", "PTKNEG", "APPR", "NN", "$(", "NE", "$(", "NE", "NE", "KON", "NN", "VVPP", "$."], "meter": "+-----+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Ist solche Grausamkeit? sind so viel S\u00fcnd vnd Schanden/", "tokens": ["Ist", "sol\u00b7che", "Grau\u00b7sam\u00b7keit", "?", "sind", "so", "viel", "S\u00fcnd", "vnd", "Schan\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$.", "VAFIN", "ADV", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gantz ohne Furcht ver\u00fcbt? nun Redligkeit in Banden", "tokens": ["Gantz", "oh\u00b7ne", "Furcht", "ver\u00b7\u00fcbt", "?", "nun", "Red\u00b7lig\u00b7keit", "in", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVPP", "$.", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd Heiligkeit verjagt: nun sich die S\u00fcnde mehrt/", "tokens": ["Vnd", "Hei\u00b7lig\u00b7keit", "ver\u00b7jagt", ":", "nun", "sich", "die", "S\u00fcn\u00b7de", "mehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$.", "ADV", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In dem der WiederChrist in Gottes Tempel lehrt/", "tokens": ["In", "dem", "der", "Wie\u00b7der", "Christ", "in", "Got\u00b7tes", "Tem\u00b7pel", "lehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd schwere Ketzerey sich heckt in allen Landen.", "tokens": ["Vnd", "schwe\u00b7re", "Ket\u00b7ze\u00b7rey", "sich", "heckt", "in", "al\u00b7len", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PRF", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach! wie wird deiner Schaar/ Herr Jesu Christ so bang!", "tokens": ["Ach", "!", "wie", "wird", "dei\u00b7ner", "Schaar", "/", "Herr", "Je\u00b7su", "Christ", "so", "bang", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$(", "NN", "NE", "NE", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verk\u00fcrtze doch die Zeit/ vnd bleib nunmehr nicht lang!", "tokens": ["Ver\u00b7k\u00fcrt\u00b7ze", "doch", "die", "Zeit", "/", "vnd", "bleib", "nun\u00b7mehr", "nicht", "lang", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "KON", "VVFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df nicht der Sathan vns in Wahn vnd Irrthumb bringe", "tokens": ["Da\u00df", "nicht", "der", "Sa\u00b7than", "vns", "in", "Wahn", "vnd", "Irr\u00b7thumb", "brin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPER", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In dessen gieb/ da\u00df ich/ O wahre Seelen-Spei\u00df'/", "tokens": ["In", "des\u00b7sen", "gieb", "/", "da\u00df", "ich", "/", "O", "wah\u00b7re", "See\u00b7len\u00b7S\u00b7pei\u00df'", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "$(", "KOUS", "PPER", "$(", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mich von der faulen Welt/ vnd ihrer Lust Abrei\u00df/", "tokens": ["Mich", "von", "der", "fau\u00b7len", "Welt", "/", "vnd", "ih\u00b7rer", "Lust", "Ab\u00b7rei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$(", "KON", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd bald zu dir mein Hort/ mit Adlers Fl\u00fcgeln schwinge.", "tokens": ["Vnd", "bald", "zu", "dir", "mein", "Hort", "/", "mit", "Ad\u00b7lers", "Fl\u00fc\u00b7geln", "schwin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$(", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}