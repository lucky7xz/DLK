{"dta.poem.19539": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Corsin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Als das M\u00fctterlein erkrankt,", "tokens": ["Als", "das", "M\u00fct\u00b7ter\u00b7lein", "er\u00b7krankt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zog es \u00e4chzend aus die Schuh,", "tokens": ["Zog", "es", "\u00e4ch\u00b7zend", "aus", "die", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist dem Bettlein zugewankt,", "tokens": ["Ist", "dem", "Bet\u00b7tlein", "zu\u00b7ge\u00b7wankt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bettet' sich zur ew'gen Ruh,", "tokens": ["Bet\u00b7tet'", "sich", "zur", "ew'\u00b7gen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Seine Haare, wei\u00df wie Flachs,", "tokens": ["Sei\u00b7ne", "Haa\u00b7re", ",", "wei\u00df", "wie", "Flachs", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine F\u00fc\u00dfe, gelb wie Wachs \u2014", "tokens": ["Sei\u00b7ne", "F\u00fc\u00b7\u00dfe", ",", "gelb", "wie", "Wachs"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KOKOM", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Statt wie M\u00fctterlein zu thun,", "tokens": ["Statt", "wie", "M\u00fct\u00b7ter\u00b7lein", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sterb' ich stracks in meinen Schuhn!", "tokens": ["Sterb'", "ich", "stracks", "in", "mei\u00b7nen", "Schuhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heute war ich in der Stadt", "tokens": ["Heu\u00b7te", "war", "ich", "in", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem letzten Silberling,", "tokens": ["Mit", "dem", "letz\u00b7ten", "Sil\u00b7ber\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaute, was der Kr\u00e4mer hat,", "tokens": ["Schau\u00b7te", ",", "was", "der", "Kr\u00e4\u00b7mer", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kramte weder Kreuz noch Ring,", "tokens": ["Kram\u00b7te", "we\u00b7der", "Kreuz", "noch", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kaufte Mehl von Weizenkorn", "tokens": ["Kauf\u00b7te", "Mehl", "von", "Wei\u00b7zen\u00b7korn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein volles Pulverhorn \u2014", "tokens": ["Und", "ein", "vol\u00b7les", "Pul\u00b7ver\u00b7horn"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu dem Liebsten lauf' ich nun,", "tokens": ["Zu", "dem", "Liebs\u00b7ten", "lauf'", "ich", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sterbe stracks in meinen Schuhn!", "tokens": ["Ster\u00b7be", "stracks", "in", "mei\u00b7nen", "Schuhn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ritten just die Blauen ", "tokens": ["Rit\u00b7ten", "just", "die", "Blau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tranken beim Battista Wein,", "tokens": ["Tran\u00b7ken", "beim", "Bat\u00b7tis\u00b7ta", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Luden scharf am Zollerhaus,", "tokens": ["Lu\u00b7den", "scharf", "am", "Zol\u00b7ler\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprengten ins Gebirg hinein.", "tokens": ["Spreng\u00b7ten", "ins", "Ge\u00b7birg", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rasch bin links ich abgeschweift.", "tokens": ["Rasch", "bin", "links", "ich", "ab\u00b7ge\u00b7schweift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Psss ... Die erste Kugel pfeift \u2014", "tokens": ["Psss", "...", "Die", "ers\u00b7te", "Ku\u00b7gel", "pfeift"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "N\u00e4chtens bei dem Liebsten ruhn", "tokens": ["N\u00e4ch\u00b7tens", "bei", "dem", "Liebs\u00b7ten", "ruhn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Werd' ich stracks in meinen Schuhn!", "tokens": ["Werd'", "ich", "stracks", "in", "mei\u00b7nen", "Schuhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}