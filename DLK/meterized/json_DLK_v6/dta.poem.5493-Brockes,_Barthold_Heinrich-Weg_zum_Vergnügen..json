{"dta.poem.5493": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Weg zum Vergn\u00fcgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Uns scheinet unser Bett nie s\u00fcsser, als wenn wir es ver-\nlassen m\u00fcssen;", "tokens": ["Uns", "schei\u00b7net", "un\u00b7ser", "Bett", "nie", "s\u00fcs\u00b7ser", ",", "als", "wenn", "wir", "es", "ver", "las\u00b7sen", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "PPER", "TRUNC", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+--+---+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Der uns sich nahende Verlust des Guten f\u00fcgt uns erst zu", "tokens": ["Der", "uns", "sich", "na\u00b7hen\u00b7de", "Ver\u00b7lust", "des", "Gu\u00b7ten", "f\u00fcgt", "uns", "erst", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+-+---+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Was ungef\u00fchlt genossen worden; die\u00df geht in allen Dingen so:", "tokens": ["Was", "un\u00b7ge\u00b7f\u00fchlt", "ge\u00b7nos\u00b7sen", "wor\u00b7den", ";", "die\u00df", "geht", "in", "al\u00b7len", "Din\u00b7gen", "so", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "VAPP", "$.", "PDS", "VVFIN", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Wir werden, weil wir dran nicht dencken, auch niemahls", "tokens": ["Wir", "wer\u00b7den", ",", "weil", "wir", "dran", "nicht", "den\u00b7cken", ",", "auch", "nie\u00b7mahls"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "PAV", "PTKNEG", "VVINF", "$,", "ADV", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Bi\u00df sie uns, oder wir sie, lassen. Dann allererst wird alles", "tokens": ["Bi\u00df", "sie", "uns", ",", "o\u00b7der", "wir", "sie", ",", "las\u00b7sen", ".", "Dann", "al\u00b7le\u00b7rerst", "wird", "al\u00b7les"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "$,", "KON", "PPER", "PPER", "$,", "VVINF", "$.", "ADV", "ADV", "VAFIN", "PIS"], "meter": "-----+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dann f\u00fchlet allererst der Geist was er gehabt und nicht", "tokens": ["Dann", "f\u00fch\u00b7let", "al\u00b7le\u00b7rerst", "der", "Geist", "was", "er", "ge\u00b7habt", "und", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PWS", "PPER", "VAPP", "KON", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Und die zu sp\u00e4t-gef\u00fchlte Lust macht den Verlust noch desto", "tokens": ["Und", "die", "zu", "sp\u00e4t\u00b7ge\u00b7f\u00fchl\u00b7te", "Lust", "macht", "den", "Ver\u00b7lust", "noch", "des\u00b7to"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PTKZU", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-++--+--+", "measure": "iambelegus"}}, "stanza.2": {"line.1": {"text": "Ach, da\u00df man denn mit mehrerm Ernste nicht hier auf", "tokens": ["Ach", ",", "da\u00df", "man", "denn", "mit", "meh\u00b7rerm", "Erns\u00b7te", "nicht", "hier", "auf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PIS", "ADV", "APPR", "PIAT", "NN", "PTKNEG", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach, da\u00df man stets vom eintzgen Wege der wahren Wollust", "tokens": ["Ach", ",", "da\u00df", "man", "stets", "vom", "eintz\u00b7gen", "We\u00b7ge", "der", "wah\u00b7ren", "Wol\u00b7lust"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PIS", "ADV", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Indem kein Gutes, ohn zu dencken, da\u00df mans besitzt, be-", "tokens": ["In\u00b7dem", "kein", "Gu\u00b7tes", ",", "ohn", "zu", "den\u00b7cken", ",", "da\u00df", "mans", "be\u00b7sitzt", ",", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIAT", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$,", "KOUS", "PIS", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wird man, auch bey dem gr\u00f6sten Gl\u00fcck auf Erden, sich", "tokens": ["Wird", "man", ",", "auch", "bey", "dem", "gr\u00f6s\u00b7ten", "Gl\u00fcck", "auf", "Er\u00b7den", ",", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PIS", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wofern wir unser Gl\u00fcck, nur dann, wann wirs verlieren,", "tokens": ["Wo\u00b7fern", "wir", "un\u00b7ser", "Gl\u00fcck", ",", "nur", "dann", ",", "wann", "wirs", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "ADV", "ADV", "$,", "PWAV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}