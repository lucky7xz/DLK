{"dta.poem.16458": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "119.  Zum Ursprung mustu gehn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch in dem Ursprung ist das Wasser rein und klar/", "tokens": ["Mensch", "in", "dem", "Ur\u00b7sprung", "ist", "das", "Was\u00b7ser", "rein", "und", "klar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Trinkstu nicht au\u00df dem Qual/ so stehstu in Gefahr.", "tokens": ["Trinks\u00b7tu", "nicht", "au\u00df", "dem", "Qual", "/", "so", "steh\u00b7stu", "in", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ART", "NN", "$(", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}