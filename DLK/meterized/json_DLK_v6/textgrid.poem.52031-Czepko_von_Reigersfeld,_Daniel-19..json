{"textgrid.poem.52031": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Mavors einesmals aus s\u00fc\u00dfer Liebes Pflicht", "tokens": ["Als", "Ma\u00b7vors", "ei\u00b7nes\u00b7mals", "aus", "s\u00fc\u00b7\u00dfer", "Lie\u00b7bes", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey seiner Venus lag, nahm ihn Vulcan gefangen,", "tokens": ["Bey", "sei\u00b7ner", "Ve\u00b7nus", "lag", ",", "nahm", "ihn", "Vul\u00b7can", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "NE", "PTKVZ", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein Freund! Du bist dem Garn auff gutes Gl\u00fcck entgangen:", "tokens": ["Mein", "Freund", "!", "Du", "bist", "dem", "Garn", "auff", "gu\u00b7tes", "Gl\u00fcck", "ent\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das halff dir: Dein Vulcan sah' auff ein Auge nicht.", "tokens": ["Das", "halff", "dir", ":", "Dein", "Vul\u00b7can", "sah'", "auff", "ein", "Au\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "PPOSAT", "NE", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}