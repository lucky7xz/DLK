{"textgrid.poem.33789": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Tanzwut", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald nach des schwarzen Todes Zeiten", "tokens": ["Bald", "nach", "des", "schwar\u00b7zen", "To\u00b7des", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geschah's, da\u00df eine wilde Lust", "tokens": ["Ge\u00b7scha\u00b7h's", ",", "da\u00df", "ei\u00b7ne", "wil\u00b7de", "Lust"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zu Tanz und Spiel und \u00dcppigkeiten", "tokens": ["Zu", "Tanz", "und", "Spiel", "und", "\u00dcp\u00b7pig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchzuckte vieler Menschen Brust.", "tokens": ["Durch\u00b7zuck\u00b7te", "vie\u00b7ler", "Men\u00b7schen", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es kam ein Not- und Hungerjahr,", "tokens": ["Es", "kam", "ein", "Not", "und", "Hun\u00b7ger\u00b7jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In L\u00fcften starb der V\u00f6gel Schar.", "tokens": ["In", "L\u00fcf\u00b7ten", "starb", "der", "V\u00f6\u00b7gel", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald sah man Volk, das durch die St\u00e4dte", "tokens": ["Bald", "sah", "man", "Volk", ",", "das", "durch", "die", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am hellen Tag im Jubel zog", "tokens": ["Am", "hel\u00b7len", "Tag", "im", "Ju\u00b7bel", "zog"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fragte, wo man Geiger h\u00e4tte,", "tokens": ["Und", "frag\u00b7te", ",", "wo", "man", "Gei\u00b7ger", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PIS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tanzend durch die Stra\u00dfen flog.", "tokens": ["Und", "tan\u00b7zend", "durch", "die", "Stra\u00b7\u00dfen", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schalmei und Fl\u00f6tenspiel ert\u00f6nten", "tokens": ["Schal\u00b7mei", "und", "Fl\u00f6\u00b7ten\u00b7spiel", "er\u00b7t\u00f6n\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Im Kirchhof und im Kirchengang,", "tokens": ["Im", "Kirch\u00b7hof", "und", "im", "Kir\u00b7chen\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Toten in den Gr\u00fcften st\u00f6hnten:", "tokens": ["Die", "To\u00b7ten", "in", "den", "Gr\u00fcf\u00b7ten", "st\u00f6hn\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erweckt uns schon Posaunenklang?", "tokens": ["Er\u00b7weckt", "uns", "schon", "Po\u00b7sau\u00b7nen\u00b7klang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Bettler lie\u00df sein Lagerstroh,", "tokens": ["Der", "Bett\u00b7ler", "lie\u00df", "sein", "La\u00b7ger\u00b7stroh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vom Kloster kamen M\u00f6nch und Nonne,", "tokens": ["Vom", "Klos\u00b7ter", "ka\u00b7men", "M\u00f6nch", "und", "Non\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Vom Krankenbett der Sieche floh,", "tokens": ["Vom", "Kran\u00b7ken\u00b7bett", "der", "Sie\u00b7che", "floh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der S\u00e4ufer von der vollen Tonne,", "tokens": ["Der", "S\u00e4u\u00b7fer", "von", "der", "vol\u00b7len", "Ton\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und Alle sangen: \u00bbFrisch und froh", "tokens": ["Und", "Al\u00b7le", "san\u00b7gen", ":", "\u00bb", "Frisch", "und", "froh"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "NN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Macht euch an die Sonne!", "tokens": ["Macht", "euch", "an", "die", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Mu\u00dftet lang im Dunkel liegen,", "tokens": ["Mu\u00df\u00b7tet", "lang", "im", "Dun\u00b7kel", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Demut hegen, Wehmut wiegen;", "tokens": ["De\u00b7mut", "he\u00b7gen", ",", "Weh\u00b7mut", "wie\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Aber heute seid ihr Leute!", "tokens": ["A\u00b7ber", "heu\u00b7te", "seid", "ihr", "Leu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Seht ihr wo verlassne Br\u00e4ute,", "tokens": ["Seht", "ihr", "wo", "ver\u00b7lass\u00b7ne", "Br\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.19": {"text": "Seht ihr wo verlorne Kinder,", "tokens": ["Seht", "ihr", "wo", "ver\u00b7lor\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Nehmt sie mit und schwingt sie so,", "tokens": ["Nehmt", "sie", "mit", "und", "schwingt", "sie", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "So und so,", "tokens": ["So", "und", "so", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Immer geschwinder, geschwinder!\u00ab", "tokens": ["Im\u00b7mer", "ge\u00b7schwin\u00b7der", ",", "ge\u00b7schwin\u00b7der", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "So tanzten Arm' in Arme schmiegend", "tokens": ["So", "tanz\u00b7ten", "Arm'", "in", "Ar\u00b7me", "schmie\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In bunten Kleidern Paar an Paar,", "tokens": ["In", "bun\u00b7ten", "Klei\u00b7dern", "Paar", "an", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den kranken Leib in Sehnsucht wiegend,", "tokens": ["Den", "kran\u00b7ken", "Leib", "in", "Sehn\u00b7sucht", "wie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Anmut, sch\u00f6n und wunderbar.", "tokens": ["Voll", "An\u00b7mut", ",", "sch\u00f6n", "und", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Alter schien sich zu verj\u00fcngen,", "tokens": ["Das", "Al\u00b7ter", "schien", "sich", "zu", "ver\u00b7j\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Jugend pl\u00f6tzlich fr\u00fch gereist,", "tokens": ["Die", "Ju\u00b7gend", "pl\u00f6tz\u00b7lich", "fr\u00fch", "ge\u00b7reist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So sprangen sie mit wilden Spr\u00fcngen,", "tokens": ["So", "spran\u00b7gen", "sie", "mit", "wil\u00b7den", "Spr\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis Sock' und Sohle durchgeschleift.", "tokens": ["Bis", "Sock'", "und", "Soh\u00b7le", "durch\u00b7ge\u00b7schleift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die von der Wut ergriffnen Leiber,", "tokens": ["Die", "von", "der", "Wut", "er\u00b7griff\u00b7nen", "Lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ach, wie sie nach dem Wasser schrie'n,", "tokens": ["Ach", ",", "wie", "sie", "nach", "dem", "Was\u00b7ser", "schrie'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die M\u00e4nner und die jungen Weiber", "tokens": ["Die", "M\u00e4n\u00b7ner", "und", "die", "jun\u00b7gen", "Wei\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Man sah sie bitten, weinen, knie'n.", "tokens": ["Man", "sah", "sie", "bit\u00b7ten", ",", "wei\u00b7nen", ",", "knie'", "n."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "abbreviation"], "pos": ["PIS", "VVFIN", "PPER", "VVINF", "$,", "VVINF", "$,", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sie tanzten \u00fcber Flur und Felder,", "tokens": ["Sie", "tanz\u00b7ten", "\u00fc\u00b7ber", "Flur", "und", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sie sprangen \u00fcber Stock und Stein,", "tokens": ["Sie", "spran\u00b7gen", "\u00fc\u00b7ber", "Stock", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sie tanzten in die wilden W\u00e4lder", "tokens": ["Sie", "tanz\u00b7ten", "in", "die", "wil\u00b7den", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und in den tiefen Rhein hinein.", "tokens": ["Und", "in", "den", "tie\u00b7fen", "Rhein", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sie ras'ten fort und fort gezogen", "tokens": ["Sie", "ras'\u00b7ten", "fort", "und", "fort", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und eilten bis ans Meer voll Weh,", "tokens": ["Und", "eil\u00b7ten", "bis", "ans", "Meer", "voll", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und st\u00fcrzten in die wilden Wogen,", "tokens": ["Und", "st\u00fcrz\u00b7ten", "in", "die", "wil\u00b7den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die Fische spritzten in die H\u00f6h'.", "tokens": ["Die", "Fi\u00b7sche", "spritz\u00b7ten", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}