{"dta.poem.18636": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n einen guten  F reund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Bruder/ wehrter Hertzens-Freund!", "tokens": ["Herr", "Bru\u00b7der", "/", "wehr\u00b7ter", "Hert\u00b7zens\u00b7Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Brief ist eingeloffen/", "tokens": ["Dein", "Brief", "ist", "ein\u00b7ge\u00b7lof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hat wie aus den Inhalt scheint/", "tokens": ["Und", "hat", "wie", "aus", "den", "In\u00b7halt", "scheint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch meinen Wunsch getroffen.", "tokens": ["Auch", "mei\u00b7nen", "Wunsch", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dr\u00fcm wei\u00df ich vor zu grosser Lust", "tokens": ["Dr\u00fcm", "wei\u00df", "ich", "vor", "zu", "gros\u00b7ser", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht recht wie ich soll schrei", "tokens": ["Nicht", "recht", "wie", "ich", "soll", "schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "KOKOM", "PPER", "VMFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wie die h\u00f6chst-vergn\u00fcgte Brust", "tokens": ["Und", "wie", "die", "h\u00f6chst\u00b7ver\u00b7gn\u00fcg\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mag ihre Kurtzweil treiben.", "tokens": ["Mag", "ih\u00b7re", "Kurt\u00b7zweil", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Bi\u00df endlich mich der Reimen Geist", "tokens": ["Bi\u00df", "end\u00b7lich", "mich", "der", "Rei\u00b7men", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hans Sachsens Verse schreiben heist", "tokens": ["Hans", "Sach\u00b7sens", "Ver\u00b7se", "schrei\u00b7ben", "heist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "VVINF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Buch ist vor den Ruhm zu klein", "tokens": ["Mein", "Buch", "ist", "vor", "den", "Ruhm", "zu", "klein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den du ihn hast gegeben/", "tokens": ["Den", "du", "ihn", "hast", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und weist/ da\u00df treu und h\u00f6flich seyn", "tokens": ["Und", "weist", "/", "da\u00df", "treu", "und", "h\u00f6f\u00b7lich", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "ADJD", "KON", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl kan beysammen leben.", "tokens": ["Wohl", "kan", "bey\u00b7sam\u00b7men", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch k\u00f6mmt der andre Theil heraus/", "tokens": ["Doch", "k\u00f6mmt", "der", "and\u00b7re", "Theil", "he\u00b7raus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So soll er besser klingen/", "tokens": ["So", "soll", "er", "bes\u00b7ser", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und manchen ehrbarn Liebes-Schmau\u00df", "tokens": ["Und", "man\u00b7chen", "ehr\u00b7barn", "Lie\u00b7bes\u00b7Schmau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In leeren Sch\u00fcsseln bringen.", "tokens": ["In", "lee\u00b7ren", "Sch\u00fcs\u00b7seln", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und wenn es mancher nicht gef\u00e4lt/", "tokens": ["Und", "wenn", "es", "man\u00b7cher", "nicht", "ge\u00b7f\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So denck' ich/ schifter in die Welt.", "tokens": ["So", "denck", "ich", "/", "schif\u00b7ter", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Teuffel reit die M\u00e4dgen gar/", "tokens": ["Der", "Teuf\u00b7fel", "reit", "die", "M\u00e4d\u00b7gen", "gar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie die Liebe plaget.", "tokens": ["Wenn", "sie", "die", "Lie\u00b7be", "pla\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn die versetzet Haut und Haar/", "tokens": ["Denn", "die", "ver\u00b7set\u00b7zet", "Haut", "und", "Haar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mans ihr nicht versaget:", "tokens": ["Da\u00df", "mans", "ihr", "nicht", "ver\u00b7sa\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die g\u00fcldne Ketten wird ein Pfand", "tokens": ["Die", "g\u00fcld\u00b7ne", "Ket\u00b7ten", "wird", "ein", "Pfand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von - - ach! potz Velten!", "tokens": ["Von", "ach", "!", "potz", "Vel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$(", "$(", "ITJ", "$.", "APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sie kaufft dem Courtisane Band/", "tokens": ["Sie", "kaufft", "dem", "Cour\u00b7ti\u00b7sa\u00b7ne", "Band", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df mu\u00df zwey G\u00fclden gelten/", "tokens": ["Da\u00df", "mu\u00df", "zwey", "G\u00fcl\u00b7den", "gel\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wird nun der Hintre nackend stehn/", "tokens": ["Wird", "nun", "der", "Hin\u00b7tre", "na\u00b7ckend", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So mu\u00df die F - - betteln gehn.", "tokens": ["So", "mu\u00df", "die", "F", "bet\u00b7teln", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NE", "$(", "$(", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich n\u00e4hme mir wohl mehr die M\u00fch/", "tokens": ["Ich", "n\u00e4h\u00b7me", "mir", "wohl", "mehr", "die", "M\u00fch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die - - - zu lieben/", "tokens": ["Die", "zu", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "$(", "$(", "PTKZU", "VVINF", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die abgeschabte Courtoisie", "tokens": ["Die", "ab\u00b7ge\u00b7schab\u00b7te", "Cour\u00b7to\u00b7i\u00b7sie"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Kan mich gar schlecht betr\u00fcben/", "tokens": ["Kan", "mich", "gar", "schlecht", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sie h\u00e4nget frembde S\u00e4cke an", "tokens": ["Sie", "h\u00e4n\u00b7get", "fremb\u00b7de", "S\u00e4\u00b7cke", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Frangen wohl versehen/", "tokens": ["Mit", "Fran\u00b7gen", "wohl", "ver\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Weil meiner nicht mehr will und kan/", "tokens": ["Weil", "mei\u00b7ner", "nicht", "mehr", "will", "und", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PTKNEG", "ADV", "VMFIN", "KON", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu ihren Diensten stehen/", "tokens": ["Zu", "ih\u00b7ren", "Diens\u00b7ten", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Vielleicht macht mich was bessers froh/", "tokens": ["Viel\u00b7leicht", "macht", "mich", "was", "bes\u00b7sers", "froh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRELS", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wer schiert sich dr\u00fcm/ ich bin nun so.", "tokens": ["Wer", "schiert", "sich", "dr\u00fcm", "/", "ich", "bin", "nun", "so", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PAV", "$(", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Herr Bruder nun erzehl ich dir", "tokens": ["Herr", "Bru\u00b7der", "nun", "er\u00b7zehl", "ich", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tugenden fein nette/", "tokens": ["Die", "Tu\u00b7gen\u00b7den", "fein", "net\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "$("], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Sie gackt die Nacht offt ins Papier/", "tokens": ["Sie", "gackt", "die", "Nacht", "offt", "ins", "Pa\u00b7pier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wirfft es unters Bette/", "tokens": ["Und", "wirfft", "es", "un\u00b7ters", "Bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Warum? 