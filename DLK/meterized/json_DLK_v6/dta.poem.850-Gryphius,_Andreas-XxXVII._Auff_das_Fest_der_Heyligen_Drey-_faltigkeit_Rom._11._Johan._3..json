{"dta.poem.850": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.  Auff das Fest der Heyligen Drey-  \n faltigkeit Rom. 11. Johan. 3.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Oreiche wissenschafft! wer kan die kunst ergr\u00fcnden ", "tokens": ["O\u00b7rei\u00b7che", "wis\u00b7sen\u00b7schafft", "!", "wer", "kan", "die", "kunst", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PWS", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die man Gott erkennt\u2019/ mag dieser Augen licht", "tokens": ["Durch", "die", "man", "Gott", "er\u00b7kennt'", "/", "mag", "die\u00b7ser", "Au\u00b7gen", "licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "NN", "VVFIN", "$(", "VMFIN", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Begreiffen seine weg? erforschen sein Gericht?", "tokens": ["Be\u00b7greif\u00b7fen", "sei\u00b7ne", "weg", "?", "er\u00b7for\u00b7schen", "sein", "Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "PTKVZ", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird man de\u00df HErren Sinn durch vnser sinnen finden?", "tokens": ["Wird", "man", "de\u00df", "Her\u00b7ren", "Sinn", "durch", "vn\u00b7ser", "sin\u00b7nen", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vn\u00df mu\u00df verstand vnd Geist vor seinen wercken schwinden:", "tokens": ["Vn\u00df", "mu\u00df", "ver\u00b7stand", "vnd", "Geist", "vor", "sei\u00b7nen", "wer\u00b7cken", "schwin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVFIN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir wissen was die Erd vnd was sie einschleu\u00dft/ nicht:", "tokens": ["Wir", "wis\u00b7sen", "was", "die", "Erd", "vnd", "was", "sie", "ein\u00b7schleu\u00dft", "/", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ART", "NN", "KON", "PWS", "PPER", "VVPP", "$(", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer sol verstehn was Er von seinem Himmel spricht.", "tokens": ["Wer", "sol", "ver\u00b7stehn", "was", "Er", "von", "sei\u00b7nem", "Him\u00b7mel", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie wasser/ Glaub vnd Geist vns ledig macht von S\u00fcnd\u1ebd.", "tokens": ["Wie", "was\u00b7ser", "/", "Glaub", "vnd", "Geist", "vns", "le\u00b7dig", "macht", "von", "S\u00fcn\u00b7d\u1ebd", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$(", "NN", "KON", "NN", "PPER", "ADJD", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dem Vater der vns schuff/ dem so am Creutz erh\u00f6ht", "tokens": ["Dem", "Va\u00b7ter", "der", "vns", "schuff", "/", "dem", "so", "am", "Creutz", "er\u00b7h\u00f6ht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$(", "ART", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(al\u00df Mosis Schlang) vns heilt/ dem/ so von beyden geht", "tokens": ["(", "al\u00df", "Mo\u00b7sis", "Schlang", ")", "vns", "heilt", "/", "dem", "/", "so", "von", "bey\u00b7den", "geht"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "NE", "NN", "$(", "PPER", "VVFIN", "$(", "ART", "$(", "ADV", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd durch die New-geburt vns in das Leben f\u00fchret.", "tokens": ["Vnd", "durch", "die", "Ne\u00b7wge\u00b7burt", "vns", "in", "das", "Le\u00b7ben", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den nie kein Hertz erkant/ dem Niemand rath ertheilt", "tokens": ["Den", "nie", "kein", "Hertz", "er\u00b7kant", "/", "dem", "Nie\u00b7mand", "rath", "er\u00b7theilt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP", "$(", "ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der vnser Seel erh\u00e4lt/ vnd vnser Schwachheit heilt", "tokens": ["Der", "vn\u00b7ser", "Seel", "er\u00b7h\u00e4lt", "/", "vnd", "vn\u00b7ser", "Schwach\u00b7heit", "heilt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sey\u2019 ewig Lob vnd Ehr/ die einig Jhm geb\u00fchret.", "tokens": ["Sey'", "e\u00b7wig", "Lob", "vnd", "Ehr", "/", "die", "ei\u00b7nig", "Jhm", "ge\u00b7b\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "NN", "KON", "NN", "$(", "ART", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}