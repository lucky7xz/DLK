{"textgrid.poem.60304": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Mein Herz", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaflose Nacht, der Regen rauscht,", "tokens": ["Schlaf\u00b7lo\u00b7se", "Nacht", ",", "der", "Re\u00b7gen", "rauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sehr wach ist mir das Herz und lauscht", "tokens": ["Sehr", "wach", "ist", "mir", "das", "Herz", "und", "lauscht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur\u00fcck bald nach vergangnen Zeiten,", "tokens": ["Zu\u00b7r\u00fcck", "bald", "nach", "ver\u00b7gang\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald horcht es, wie die k\u00fcnftgen schreiten.", "tokens": ["Bald", "horcht", "es", ",", "wie", "die", "k\u00fcnft\u00b7gen", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Herz, dein Lauschen ist nicht gut;", "tokens": ["O", "Herz", ",", "dein", "Lau\u00b7schen", "ist", "nicht", "gut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei ewig, Herz, und hochgemut!", "tokens": ["Sei", "e\u00b7wig", ",", "Herz", ",", "und", "hoch\u00b7ge\u00b7mut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hinten ruft so manche Klage,", "tokens": ["Da", "hin\u00b7ten", "ruft", "so", "man\u00b7che", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und vorw\u00e4rts zittert manche Frage.", "tokens": ["Und", "vor\u00b7w\u00e4rts", "zit\u00b7tert", "man\u00b7che", "Fra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohlan! was sterblich war, sei tot!", "tokens": ["Wo\u00b7hlan", "!", "was", "sterb\u00b7lich", "war", ",", "sei", "tot", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "ADJD", "VAFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Naht Sturm! wohlan! \u2013 wie einst das Boot", "tokens": ["Naht", "Sturm", "!", "wo\u00b7hlan", "!", "\u2013", "wie", "einst", "das", "Boot"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ADV", "$.", "$(", "KOKOM", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Christus St\u00fcrme nicht zerschellten,", "tokens": ["Mit", "Chris\u00b7tus", "St\u00fcr\u00b7me", "nicht", "zer\u00b7schell\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ruht in dir der Herr der Welten.", "tokens": ["So", "ruht", "in", "dir", "der", "Herr", "der", "Wel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}