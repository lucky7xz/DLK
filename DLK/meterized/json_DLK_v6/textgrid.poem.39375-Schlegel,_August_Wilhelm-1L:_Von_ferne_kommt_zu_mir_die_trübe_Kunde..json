{"textgrid.poem.39375": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von ferne kommt zu mir die tr\u00fcbe Kunde.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von ferne kommt zu mir die tr\u00fcbe Kunde.", "tokens": ["Von", "fer\u00b7ne", "kommt", "zu", "mir", "die", "tr\u00fc\u00b7be", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es trennt mich ein Gebirg mit Wald und Kl\u00fcften,", "tokens": ["Es", "trennt", "mich", "ein", "Ge\u00b7birg", "mit", "Wald", "und", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blau d\u00e4mmernd in des Horizontes D\u00fcften,", "tokens": ["Blau", "d\u00e4m\u00b7mernd", "in", "des", "Ho\u00b7ri\u00b7zon\u00b7tes", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Von dort, wo ich erlitt die Todeswunde.", "tokens": ["Von", "dort", ",", "wo", "ich", "er\u00b7litt", "die", "To\u00b7des\u00b7wun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da mach' ich auf die Wandrung mich zur Stunde:", "tokens": ["Da", "mach'", "ich", "auf", "die", "Wand\u00b7rung", "mich", "zur", "Stun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo B\u00e4che st\u00fcrzend rauschen in den Schl\u00fcften,", "tokens": ["Wo", "B\u00e4\u00b7che", "st\u00fcr\u00b7zend", "rau\u00b7schen", "in", "den", "Schl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Felsen sich gew\u00f6lbt zu dunkeln Gr\u00fcften,", "tokens": ["Wo", "Fel\u00b7sen", "sich", "ge\u00b7w\u00f6lbt", "zu", "dun\u00b7keln", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "VVPP", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da ist der Pfad mit meinem Sinn im Bunde.", "tokens": ["Da", "ist", "der", "Pfad", "mit", "mei\u00b7nem", "Sinn", "im", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier reis'te j\u00fcngst hindurch, die ich betraure,", "tokens": ["Hier", "reis'\u00b7te", "j\u00fcngst", "hin\u00b7durch", ",", "die", "ich", "be\u00b7trau\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PAV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht achtend auf des schroffen Wegs Beschwerde;", "tokens": ["Nicht", "ach\u00b7tend", "auf", "des", "schrof\u00b7fen", "Wegs", "Be\u00b7schwer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zur heitern Landschaft s\u00fcdlich hingezogen.", "tokens": ["Zur", "hei\u00b7tern", "Land\u00b7schaft", "s\u00fcd\u00b7lich", "hin\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mai war's, nun hei\u00dft es Sommer, und ich schaure", "tokens": ["Mai", "wa\u00b7r's", ",", "nun", "hei\u00dft", "es", "Som\u00b7mer", ",", "und", "ich", "schau\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Von kaltem Sturm; ihr ward zum Grab die Erde:", "tokens": ["Von", "kal\u00b7tem", "Sturm", ";", "ihr", "ward", "zum", "Grab", "die", "Er\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Lenz hat Allen, Jugend ", "tokens": ["Der", "Lenz", "hat", "Al\u00b7len", ",", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}