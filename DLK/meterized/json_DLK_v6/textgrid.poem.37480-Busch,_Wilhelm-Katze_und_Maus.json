{"textgrid.poem.37480": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Katze und Maus", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Maus spaziert in die Laterne,", "tokens": ["Die", "Maus", "spa\u00b7ziert", "in", "die", "La\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der b\u00f6se Kater sieht's von ferne.", "tokens": ["Der", "b\u00f6\u00b7se", "Ka\u00b7ter", "sieht's", "von", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schnapp! springt er zu: das Glas zerbricht;", "tokens": ["Schnapp", "!", "springt", "er", "zu", ":", "das", "Glas", "zer\u00b7bricht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Maus, die kriegt er aber nicht.", "tokens": ["Die", "Maus", ",", "die", "kriegt", "er", "a\u00b7ber", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Kleiderstock erklimmt die Maus,", "tokens": ["Den", "Klei\u00b7der\u00b7stock", "er\u00b7klimmt", "die", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kater nach in einem Saus.", "tokens": ["Der", "Ka\u00b7ter", "nach", "in", "ei\u00b7nem", "Saus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier sausen sie durchs Hosenbein,", "tokens": ["Hier", "sau\u00b7sen", "sie", "durchs", "Ho\u00b7sen\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Maus heraus, die Katz hinein.", "tokens": ["Die", "Maus", "he\u00b7raus", ",", "die", "Katz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Klatsch! f\u00e4llt der Kater mit dem Kopf", "tokens": ["Klatsch", "!", "f\u00e4llt", "der", "Ka\u00b7ter", "mit", "dem", "Kopf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einen schwarzen Wichsetopf.", "tokens": ["In", "ei\u00b7nen", "schwar\u00b7zen", "Wich\u00b7se\u00b7topf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Kater ist ein halber Mohr,", "tokens": ["Der", "Ka\u00b7ter", "ist", "ein", "hal\u00b7ber", "Mohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Maus springt in das Stiefelrohr.", "tokens": ["Die", "Maus", "springt", "in", "das", "Stie\u00b7fel\u00b7rohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Kater denkt: \u00bbDich krieg ich noch!\u00ab", "tokens": ["Der", "Ka\u00b7ter", "denkt", ":", "\u00bb", "Dich", "krieg", "ich", "noch", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Stiefel aber hat ein Loch.", "tokens": ["Der", "Stie\u00b7fel", "a\u00b7ber", "hat", "ein", "Loch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Hier springt denn auch die gute Maus", "tokens": ["Hier", "springt", "denn", "auch", "die", "gu\u00b7te", "Maus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bereits zum Stiefelloch heraus.", "tokens": ["Be\u00b7reits", "zum", "Stie\u00b7fel\u00b7loch", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Maus l\u00e4uft schnell ins M\u00e4usenest;", "tokens": ["Die", "Maus", "l\u00e4uft", "schnell", "ins", "M\u00e4u\u00b7se\u00b7nest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kater sitzt im Stiefel fest.", "tokens": ["Der", "Ka\u00b7ter", "sitzt", "im", "Stie\u00b7fel", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ja, stelle dich nur auf den Kopf!", "tokens": ["Ja", ",", "stel\u00b7le", "dich", "nur", "auf", "den", "Kopf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Stiefel bleibt dir doch am Schopf.", "tokens": ["Der", "Stie\u00b7fel", "bleibt", "dir", "doch", "am", "Schopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Die K\u00f6chin und der Hausknecht sehn", "tokens": ["Die", "K\u00f6\u00b7chin", "und", "der", "Haus\u00b7knecht", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Staunen an, was hier geschehn.", "tokens": ["Mit", "Stau\u00b7nen", "an", ",", "was", "hier", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Und beide sieht man mit Bem\u00fchn", "tokens": ["Und", "bei\u00b7de", "sieht", "man", "mit", "Be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PIS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Katzenschwanz und Stiefel ziehn.", "tokens": ["Am", "Kat\u00b7zen\u00b7schwanz", "und", "Stie\u00b7fel", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Pardauz! Da haben wir es ja!", "tokens": ["Par\u00b7dauz", "!", "Da", "ha\u00b7ben", "wir", "es", "ja", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie liegen alle beide da.", "tokens": ["Sie", "lie\u00b7gen", "al\u00b7le", "bei\u00b7de", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Der Kater, der's verdient gehabt,", "tokens": ["Der", "Ka\u00b7ter", ",", "der's", "ver\u00b7dient", "ge\u00b7habt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird eingeklemmt und abgeklappt.", "tokens": ["Wird", "ein\u00b7ge\u00b7klemmt", "und", "ab\u00b7ge\u00b7klappt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Und wenn sich einer schwarz gemacht,", "tokens": ["Und", "wenn", "sich", "ei\u00b7ner", "schwarz", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Wasser wird's herausgebracht.", "tokens": ["Mit", "Was\u00b7ser", "wird's", "her\u00b7aus\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Die M\u00e4use aber springen", "tokens": ["Die", "M\u00e4u\u00b7se", "a\u00b7ber", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Kreis herum und singen:", "tokens": ["Im", "Kreis", "he\u00b7rum", "und", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "\u00bbder brave Stiefel hat ein Loch!", "tokens": ["\u00bb", "der", "bra\u00b7ve", "Stie\u00b7fel", "hat", "ein", "Loch", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Stiefel lebe! Vivat hoch!\u00ab", "tokens": ["Der", "Stie\u00b7fel", "le\u00b7be", "!", "Vi\u00b7vat", "hoch", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}