{"textgrid.poem.25195": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: In \u00fcppigen Sonnenfluten", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In \u00fcppigen Sonnenfluten", "tokens": ["In", "\u00fcp\u00b7pi\u00b7gen", "Son\u00b7nen\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Badet sich der Park,", "tokens": ["Ba\u00b7det", "sich", "der", "Park", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der mit gl\u00e4nzendem Bl\u00e4ttergewoge", "tokens": ["Der", "mit", "gl\u00e4n\u00b7zen\u00b7dem", "Bl\u00e4t\u00b7ter\u00b7ge\u00b7wo\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Grausteinerne H\u00e4userw\u00e4lle besp\u00fclt.", "tokens": ["Graus\u00b7tei\u00b7ner\u00b7ne", "H\u00e4u\u00b7ser\u00b7w\u00e4l\u00b7le", "be\u00b7sp\u00fclt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "++--+-+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Aus schattiger Stra\u00dfenm\u00fcndung str\u00f6mt,", "tokens": ["Aus", "schat\u00b7ti\u00b7ger", "Stra\u00b7\u00dfen\u00b7m\u00fcn\u00b7dung", "str\u00f6mt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Buntblitzenden Wellen gleich,", "tokens": ["Bunt\u00b7blit\u00b7zen\u00b7den", "Wel\u00b7len", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bl\u00fctenfarbig geputztes Volk:", "tokens": ["Bl\u00fc\u00b7ten\u00b7far\u00b7big", "ge\u00b7putz\u00b7tes", "Volk", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "M\u00e4dchen mit buntbeb\u00e4nderten H\u00fcten,", "tokens": ["M\u00e4d\u00b7chen", "mit", "bunt\u00b7be\u00b7b\u00e4n\u00b7der\u00b7ten", "H\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Bl\u00fchenden Augen, schimmernden Z\u00e4hnen.", "tokens": ["Bl\u00fc\u00b7hen\u00b7den", "Au\u00b7gen", ",", "schim\u00b7mern\u00b7den", "Z\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Das Plaudern pl\u00e4tschert", "tokens": ["Das", "Plau\u00b7dern", "pl\u00e4t\u00b7schert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der Ruhebank vorbei", "tokens": ["Der", "Ru\u00b7he\u00b7bank", "vor\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unter lila bl\u00fchendem Flieder,", "tokens": ["Un\u00b7ter", "li\u00b7la", "bl\u00fc\u00b7hen\u00b7dem", "Flie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wo ich sitze bei spielenden Kindern.", "tokens": ["Wo", "ich", "sit\u00b7ze", "bei", "spie\u00b7len\u00b7den", "Kin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Im Strauche fl\u00f6tet die Nachtigall;", "tokens": ["Im", "Strau\u00b7che", "fl\u00f6\u00b7tet", "die", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Fernes Konzert", "tokens": ["Fer\u00b7nes", "Kon\u00b7zert"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Weht mit D\u00fcften s\u00fc\u00df heran", "tokens": ["Weht", "mit", "D\u00fcf\u00b7ten", "s\u00fc\u00df", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und zittert in meiner Seele ...", "tokens": ["Und", "zit\u00b7tert", "in", "mei\u00b7ner", "See\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sanftes M\u00e4dchengesicht", "tokens": ["Sanf\u00b7tes", "M\u00e4d\u00b7chen\u00b7ge\u00b7sicht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Unter sch\u00fcchternem Sommerhut,", "tokens": ["Un\u00b7ter", "sch\u00fcch\u00b7ter\u00b7nem", "Som\u00b7mer\u00b7hut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Blaues Bl\u00fctenauge,", "tokens": ["Blau\u00b7es", "Bl\u00fc\u00b7ten\u00b7au\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ich k\u00f6nnte dich lieben!", "tokens": ["Ich", "k\u00f6nn\u00b7te", "dich", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Doch zages Tr\u00e4umen h\u00e4lt mich fest,", "tokens": ["Doch", "za\u00b7ges", "Tr\u00e4u\u00b7men", "h\u00e4lt", "mich", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dich entf\u00fchrt die Flut ...", "tokens": ["Und", "dich", "ent\u00b7f\u00fchrt", "die", "Flut", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wieder wehen mit Fliederduft", "tokens": ["Und", "wie\u00b7der", "we\u00b7hen", "mit", "Flie\u00b7der\u00b7duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Accorde schmachtend, schwellend;", "tokens": ["Ac\u00b7cor\u00b7de", "schmach\u00b7tend", ",", "schwel\u00b7lend", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Seele zittert", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von s\u00fc\u00dfem Sehnsuchtsschauer.", "tokens": ["Von", "s\u00fc\u00b7\u00dfem", "Sehn\u00b7suchts\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sieh, das Weib", "tokens": ["Sieh", ",", "das", "Weib"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Im dunkeln Kleide,", "tokens": ["Im", "dun\u00b7keln", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Stolz,", "tokens": ["Stolz", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Mit rundem Busen", "tokens": ["Mit", "run\u00b7dem", "Bu\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und schwarzer Augenglut!", "tokens": ["Und", "schwar\u00b7zer", "Au\u00b7gen\u00b7glut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mein Herz entbrennt", "tokens": ["Mein", "Herz", "ent\u00b7brennt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und pocht in wilder Sehnsucht.", "tokens": ["Und", "pocht", "in", "wil\u00b7der", "Sehn\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was brennst du so?", "tokens": ["Was", "brennst", "du", "so", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Die hei\u00df umschlingend", "tokens": ["Die", "hei\u00df", "um\u00b7schlin\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dein zehrend Schmachten stillt?", "tokens": ["Dein", "zeh\u00b7rend", "Schmach\u00b7ten", "stillt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Soll ich ihr folgen, pochendes Herz? \u2013", "tokens": ["Soll", "ich", "ihr", "fol\u00b7gen", ",", "po\u00b7chen\u00b7des", "Herz", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ich wag' es nicht;", "tokens": ["Ich", "wag'", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mir wird so schw\u00fcl und bang.", "tokens": ["Mir", "wird", "so", "schw\u00fcl", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Denn vielleicht \u2013 was wei\u00df ich! \u2013", "tokens": ["Denn", "viel\u00b7leicht", "\u2013", "was", "wei\u00df", "ich", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "PWS", "VVFIN", "PPER", "$.", "$("], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Bl\u00fcht Gift im dunkeln Auge", "tokens": ["Bl\u00fcht", "Gift", "im", "dun\u00b7keln", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und verzehrt mir qualvoll", "tokens": ["Und", "ver\u00b7zehrt", "mir", "qual\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wangen und Seele. \u2013", "tokens": ["Wan\u00b7gen", "und", "See\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "So ist die Liebe!", "tokens": ["So", "ist", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Auf Leben und Tod!", "tokens": ["Auf", "Le\u00b7ben", "und", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Oder ist dies Strahlenauge", "tokens": ["O\u00b7der", "ist", "dies", "Strah\u00b7len\u00b7au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir Quelle ewiger Wonne? \u2013", "tokens": ["Mir", "Quel\u00b7le", "e\u00b7wi\u00b7ger", "Won\u00b7ne", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So geh zur Quelle, schmachtendes Herz!", "tokens": ["So", "geh", "zur", "Quel\u00b7le", ",", "schmach\u00b7ten\u00b7des", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sonst versp\u00fclt die Flut dein Gl\u00fcck! \u2013", "tokens": ["Sonst", "ver\u00b7sp\u00fclt", "die", "Flut", "dein", "Gl\u00fcck", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ja, ich gehe!", "tokens": ["Ja", ",", "ich", "ge\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Noch ", "tokens": ["Noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und hingerissen folg' ich,", "tokens": ["Und", "hin\u00b7ge\u00b7ris\u00b7sen", "fol\u00b7g'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIZU", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu lieben auf Leben und Tod ...", "tokens": ["Zu", "lie\u00b7ben", "auf", "Le\u00b7ben", "und", "Tod", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Doch wehe! Mir schwindelt;", "tokens": ["Doch", "we\u00b7he", "!", "Mir", "schwin\u00b7delt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich wanke, zu st\u00fcrzen", "tokens": ["Ich", "wan\u00b7ke", ",", "zu", "st\u00fcr\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In glitzernde Wellen", "tokens": ["In", "glit\u00b7zern\u00b7de", "Wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Des Menschenstromes.", "tokens": ["Des", "Men\u00b7schen\u00b7stro\u00b7mes", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Halt dich fest, beth\u00f6rte Seele!", "tokens": ["Halt", "dich", "fest", ",", "be\u00b7th\u00f6r\u00b7te", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jedwede liebliche Welle", "tokens": ["Jed\u00b7we\u00b7de", "lieb\u00b7li\u00b7che", "Wel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Ist Liebe auf Leben und Tod! \u2013", "tokens": ["Ist", "Lie\u00b7be", "auf", "Le\u00b7ben", "und", "Tod", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Doch horch, die Nachtigall lockt so hei\u00df,", "tokens": ["Doch", "horch", ",", "die", "Nach\u00b7ti\u00b7gall", "lockt", "so", "hei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Berauschend wehen Musik und Duft,", "tokens": ["Be\u00b7rau\u00b7schend", "we\u00b7hen", "Mu\u00b7sik", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und Menschenaugen bl\u00fchen so sch\u00f6n ...", "tokens": ["Und", "Men\u00b7schen\u00b7au\u00b7gen", "bl\u00fc\u00b7hen", "so", "sch\u00f6n", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wohlauf in den Strom der Seelen,", "tokens": ["Wohl\u00b7auf", "in", "den", "Strom", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu lieben auf Leben und Tod!", "tokens": ["Zu", "lie\u00b7ben", "auf", "Le\u00b7ben", "und", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}