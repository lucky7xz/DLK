{"dta.poem.10499": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LvI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Saphire sind die Augen dein,", "tokens": ["Sa\u00b7phi\u00b7re", "sind", "die", "Au\u00b7gen", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lieblichen, die s\u00fc\u00dfen.", "tokens": ["Die", "lieb\u00b7li\u00b7chen", ",", "die", "s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O, dreimal gl\u00fccklich ist der Mann,", "tokens": ["O", ",", "drei\u00b7mal", "gl\u00fcck\u00b7lich", "ist", "der", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den sie mit Liebe gr\u00fc\u00dfen.", "tokens": ["Den", "sie", "mit", "Lie\u00b7be", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Herz, es ist ein Diamant,", "tokens": ["Dein", "Herz", ",", "es", "ist", "ein", "Di\u00b7a\u00b7mant", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der edle Lichter spr\u00fchet.", "tokens": ["Der", "ed\u00b7le", "Lich\u00b7ter", "spr\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O, dreimal gl\u00fccklich ist der Mann,", "tokens": ["O", ",", "drei\u00b7mal", "gl\u00fcck\u00b7lich", "ist", "der", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr den es liebend gl\u00fchet.", "tokens": ["F\u00fcr", "den", "es", "lie\u00b7bend", "gl\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Rubinen sind die Lippen dein,", "tokens": ["Ru\u00b7bi\u00b7nen", "sind", "die", "Lip\u00b7pen", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man kann nicht sch\u00f6n're sehen.", "tokens": ["Man", "kann", "nicht", "sch\u00f6n'\u00b7re", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O, dreimal gl\u00fccklich ist der Mann,", "tokens": ["O", ",", "drei\u00b7mal", "gl\u00fcck\u00b7lich", "ist", "der", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem sie die Liebe gestehen.", "tokens": ["Dem", "sie", "die", "Lie\u00b7be", "ge\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "O, kennt' ich nur den gl\u00fccklichen Mann,", "tokens": ["O", ",", "kennt'", "ich", "nur", "den", "gl\u00fcck\u00b7li\u00b7chen", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "O, da\u00df ich ihn nur f\u00e4nde,", "tokens": ["O", ",", "da\u00df", "ich", "ihn", "nur", "f\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So recht allein im gr\u00fcnen Wald,", "tokens": ["So", "recht", "al\u00b7lein", "im", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Gl\u00fcck h\u00e4tt' bald ein Ende.", "tokens": ["Sein", "Gl\u00fcck", "h\u00e4tt'", "bald", "ein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}