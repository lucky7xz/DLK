 "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201enichts ist so zweifelhaft, verwirret und zerstreut,", "tokens": ["\u201e", "nichts", "ist", "so", "zwei\u00b7fel\u00b7haft", ",", "ver\u00b7wir\u00b7ret", "und", "zer\u00b7streut", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VAFIN", "ADV", "ADJD", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eso sie nicht von dem Schleyr der Dunckelheit befreyt.", "tokens": ["\u201e", "so", "sie", "nicht", "von", "dem", "Schleyr", "der", "Dun\u00b7ckel\u00b7heit", "be\u00b7freyt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}