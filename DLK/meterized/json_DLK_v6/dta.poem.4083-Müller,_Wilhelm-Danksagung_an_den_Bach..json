{"dta.poem.4083": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Danksagung an den Bach.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein rauschender Freund,", "tokens": ["Mein", "rau\u00b7schen\u00b7der", "Freund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Dein Singen, dein Klingen,", "tokens": ["Dein", "Sin\u00b7gen", ",", "dein", "Klin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "War es also gemeint?", "tokens": ["War", "es", "al\u00b7so", "ge\u00b7meint", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Zur M\u00fcllerin hin!", "tokens": ["Zur", "M\u00fcl\u00b7le\u00b7rin", "hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So lautet der Sinn,", "tokens": ["So", "lau\u00b7tet", "der", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gelt, hab' ich's verstanden?", "tokens": ["Gelt", ",", "hab'", "ich's", "ver\u00b7stan\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PIS", "VVINF", "$."], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Zur M\u00fcllerin hin!", "tokens": ["Zur", "M\u00fcl\u00b7le\u00b7rin", "hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Hat ", "tokens": ["Hat"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Oder hast mich ber\u00fcckt?", "tokens": ["O\u00b7der", "hast", "mich", "be\u00b7r\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das m\u00f6cht' ich noch wissen,", "tokens": ["Das", "m\u00f6cht'", "ich", "noch", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ob ", "tokens": ["Ob"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Nun wie's auch mag sein,", "tokens": ["Nun", "wie's", "auch", "mag", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ich gebe mich drein:", "tokens": ["Ich", "ge\u00b7be", "mich", "drein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was ich such', hab' ich funden,", "tokens": ["Was", "ich", "such'", ",", "hab'", "ich", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie's immer mag sein.", "tokens": ["Wie's", "im\u00b7mer", "mag", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Nach Arbeit ich frug,", "tokens": ["Nach", "Ar\u00b7beit", "ich", "frug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Nun hab' ich genug,", "tokens": ["Nun", "hab'", "ich", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "F\u00fcr die H\u00e4nde, f\u00fcr's Herze", "tokens": ["F\u00fcr", "die", "H\u00e4n\u00b7de", ",", "f\u00fcr's", "Her\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Vollauf genug!", "tokens": ["Vol\u00b7lauf", "ge\u00b7nug", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}