{"textgrid.poem.52421": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "2. Fr\u00fchlingslied", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsre Wiesen gr\u00fcnen wieder,", "tokens": ["Uns\u00b7re", "Wie\u00b7sen", "gr\u00fc\u00b7nen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumen duften \u00fcberall;", "tokens": ["Blu\u00b7men", "duf\u00b7ten", "\u00fc\u00b7be\u00b7rall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00f6hlich t\u00f6nen Finkenlieder,", "tokens": ["Fr\u00f6h\u00b7lich", "t\u00f6\u00b7nen", "Fin\u00b7ken\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00e4rtlich schl\u00e4gt die Nachtigall.", "tokens": ["Z\u00e4rt\u00b7lich", "schl\u00e4gt", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Wipfel d\u00e4mmern gr\u00fcner,", "tokens": ["Al\u00b7le", "Wip\u00b7fel", "d\u00e4m\u00b7mern", "gr\u00fc\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebe girrt und lockt darin;", "tokens": ["Lie\u00b7be", "girrt", "und", "lockt", "da\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jeder Sch\u00e4fer wird nun k\u00fchner,", "tokens": ["Je\u00b7der", "Sch\u00e4\u00b7fer", "wird", "nun", "k\u00fch\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sanfter jede Sch\u00e4ferin.", "tokens": ["Sanf\u00b7ter", "je\u00b7de", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bl\u00fcten, die die Knosp' entwickeln,", "tokens": ["Bl\u00fc\u00b7ten", ",", "die", "die", "Knosp'", "ent\u00b7wi\u00b7ckeln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcllt der Lenz in zartes Laub;", "tokens": ["H\u00fcllt", "der", "Lenz", "in", "zar\u00b7tes", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4rbt den Sammet der Aurikeln,", "tokens": ["F\u00e4rbt", "den", "Sam\u00b7met", "der", "Au\u00b7ri\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pudert sie mit Silberstaub.", "tokens": ["Pu\u00b7dert", "sie", "mit", "Sil\u00b7ber\u00b7staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh! das holde Maienreischen", "tokens": ["Sieh", "!", "das", "hol\u00b7de", "Mai\u00b7en\u00b7rei\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dringt aus breitem Blatt hervor,", "tokens": ["Dringt", "aus", "brei\u00b7tem", "Blatt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Beut sich zum bescheidnen Str\u00e4u\u00dfchen", "tokens": ["Beut", "sich", "zum", "be\u00b7scheid\u00b7nen", "Str\u00e4u\u00df\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An der Unschuld Busenflor.", "tokens": ["An", "der", "Un\u00b7schuld", "Bu\u00b7sen\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf den zarten Stengeln wanken", "tokens": ["Auf", "den", "zar\u00b7ten", "Sten\u00b7geln", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tulpenkelche, rot und gelb,", "tokens": ["Tul\u00b7pen\u00b7kel\u00b7che", ",", "rot", "und", "gelb", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Gei\u00dfblatt flicht aus Ranken", "tokens": ["Und", "das", "Gei\u00df\u00b7blatt", "flicht", "aus", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebenden ein Laubgew\u00f6lb'.", "tokens": ["Lie\u00b7ben\u00b7den", "ein", "Laub\u00b7ge\u00b7w\u00f6lb'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle L\u00fcfte s\u00e4useln lauer", "tokens": ["Al\u00b7le", "L\u00fcf\u00b7te", "s\u00e4u\u00b7seln", "lau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der Liebe Hauch uns an;", "tokens": ["Mit", "der", "Lie\u00b7be", "Hauch", "uns", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fr\u00fchlingslust und Wonneschauer", "tokens": ["Fr\u00fch\u00b7lings\u00b7lust", "und", "Won\u00b7ne\u00b7schau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchlet, was noch f\u00fchlen kann.", "tokens": ["F\u00fch\u00b7let", ",", "was", "noch", "f\u00fch\u00b7len", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}