{"textgrid.poem.36376": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5. Jubel der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie so glanzvoll Trost von oben", "tokens": ["Wie", "so", "glanz\u00b7voll", "Trost", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "NN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich durch die Wolken bricht!", "tokens": ["End\u00b7lich", "durch", "die", "Wol\u00b7ken", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Strahlen je erhoben,", "tokens": ["Kei\u00b7ne", "Strah\u00b7len", "je", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kein Krystall gab solches Licht.", "tokens": ["Kein", "Krys\u00b7tall", "gab", "sol\u00b7ches", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O wie wohl wird meinem Herzen,", "tokens": ["O", "wie", "wohl", "wird", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie so klar mein Angesicht,", "tokens": ["Wie", "so", "klar", "mein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weichet, weichet, Angst und Schmerzen,", "tokens": ["Wei\u00b7chet", ",", "wei\u00b7chet", ",", "Angst", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Euer nun bedarf es nicht.", "tokens": ["Eu\u00b7er", "nun", "be\u00b7darf", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fliehet fort, hinab in Wogen,", "tokens": ["Flie\u00b7het", "fort", ",", "hin\u00b7ab", "in", "Wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Senket euch zur finstern Nacht;", "tokens": ["Sen\u00b7ket", "euch", "zur", "fins\u00b7tern", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freuden kommen hergezogen,", "tokens": ["Freu\u00b7den", "kom\u00b7men", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Luft und Himmel wieder lacht.", "tokens": ["Luft", "und", "Him\u00b7mel", "wie\u00b7der", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kalt' und Winter ist zerbrochen,", "tokens": ["Kalt'", "und", "Win\u00b7ter", "ist", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00fcbsal ist nun ferne hin;", "tokens": ["Tr\u00fcb\u00b7sal", "ist", "nun", "fer\u00b7ne", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht von Trauer sei gesprochen,", "tokens": ["Nicht", "von", "Trau\u00b7er", "sei", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fr\u00f6hlichkeit ist mein Gewinn.", "tokens": ["Fr\u00f6h\u00b7lich\u00b7keit", "ist", "mein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eia lasse uns spazieren", "tokens": ["Ei\u00b7a", "las\u00b7se", "uns", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jesu du Geliebter mein!", "tokens": ["Je\u00b7su", "du", "Ge\u00b7lieb\u00b7ter", "mein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weil die G\u00e4rten nun sich zieren,", "tokens": ["Weil", "die", "G\u00e4r\u00b7ten", "nun", "sich", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil die Bl\u00fcmlein offen sein;", "tokens": ["Weil", "die", "Bl\u00fcm\u00b7lein", "of\u00b7fen", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil die gr\u00fcnen Wiesen lachen,", "tokens": ["Weil", "die", "gr\u00fc\u00b7nen", "Wie\u00b7sen", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil die Pflanzen voller Zweig',", "tokens": ["Weil", "die", "Pflan\u00b7zen", "vol\u00b7ler", "Zweig'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil die V\u00f6glein Nester machen,", "tokens": ["Weil", "die", "V\u00f6\u00b7glein", "Nes\u00b7ter", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kinderbettlein zart und weich.", "tokens": ["Kin\u00b7der\u00b7bet\u00b7tlein", "zart", "und", "weich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schau die reinen Br\u00fcnnlein springen", "tokens": ["Schau", "die", "rei\u00b7nen", "Br\u00fcnn\u00b7lein", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoch in leere Luft hinein,", "tokens": ["Hoch", "in", "lee\u00b7re", "Luft", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Horch die zarten V\u00f6glein singen", "tokens": ["Horch", "die", "zar\u00b7ten", "V\u00f6\u00b7glein", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunder, Wunder, s\u00fc\u00df und rein.", "tokens": ["Wun\u00b7der", ",", "Wun\u00b7der", ",", "s\u00fc\u00df", "und", "rein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Horch die B\u00e4chlein lieblich brausen,", "tokens": ["Horch", "die", "B\u00e4ch\u00b7lein", "lieb\u00b7lich", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Klar wie lauter Silberschein;", "tokens": ["Klar", "wie", "lau\u00b7ter", "Sil\u00b7ber\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Bienlein emsig sausen,", "tokens": ["Und", "die", "Bien\u00b7lein", "em\u00b7sig", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Rauben, klauben Honig ein.", "tokens": ["Rau\u00b7ben", ",", "klau\u00b7ben", "Ho\u00b7nig", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kleine Bienlein, ach ihr fehlet,", "tokens": ["Klei\u00b7ne", "Bien\u00b7lein", ",", "ach", "ihr", "feh\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "XY", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ledig flieget ihr nach Haus;", "tokens": ["Le\u00b7dig", "flie\u00b7get", "ihr", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur von Jesu Lippen stehlet,", "tokens": ["Nur", "von", "Je\u00b7su", "Lip\u00b7pen", "steh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dannen saugt den Honig aus.", "tokens": ["Dan\u00b7nen", "saugt", "den", "Ho\u00b7nig", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jesu Lippen, Mund und Augen,", "tokens": ["Je\u00b7su", "Lip\u00b7pen", ",", "Mund", "und", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00fc\u00dfen Saftes sind sie voll,", "tokens": ["S\u00fc\u00b7\u00dfen", "Saf\u00b7tes", "sind", "sie", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja von diesen m\u00fc\u00dft ihr saugen,", "tokens": ["Ja", "von", "die\u00b7sen", "m\u00fc\u00dft", "ihr", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PDAT", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn Gewinn es bringen soll.", "tokens": ["Wenn", "Ge\u00b7winn", "es", "brin\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Neulich ich in Trauren stunde,", "tokens": ["Neu\u00b7lich", "ich", "in", "Trau\u00b7ren", "stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War voll herber Bitterkeit,", "tokens": ["War", "voll", "her\u00b7ber", "Bit\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Jesum da gekreuzigt funde,", "tokens": ["Je\u00b7sum", "da", "ge\u00b7kreu\u00b7zigt", "fun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Klagend ihm mein Herzenleid.", "tokens": ["Kla\u00b7gend", "ihm", "mein", "Her\u00b7zen\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebend t\u00e4t ich ihn umh\u00e4lsen,", "tokens": ["Lie\u00b7bend", "t\u00e4t", "ich", "ihn", "um\u00b7h\u00e4l\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fc\u00dfte seine Wangen beid',", "tokens": ["K\u00fc\u00df\u00b7te", "sei\u00b7ne", "Wan\u00b7gen", "beid'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir da sprang von diesen Felsen", "tokens": ["Mir", "da", "sprang", "von", "die\u00b7sen", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bronn und Bach der S\u00fc\u00dfigkeit.", "tokens": ["Bronn", "und", "Bach", "der", "S\u00fc\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Meine Kraft war gar zerschlagen,", "tokens": ["Mei\u00b7ne", "Kraft", "war", "gar", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War von tiefen Trauren matt,", "tokens": ["War", "von", "tie\u00b7fen", "Trau\u00b7ren", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin nunmehr in Freudentagen,", "tokens": ["Bin", "nun\u00b7mehr", "in", "Freu\u00b7den\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin von lauter L\u00fcsten satt.", "tokens": ["Bin", "von", "lau\u00b7ter", "L\u00fcs\u00b7ten", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00fcbnis hatte mich umzogen,", "tokens": ["Tr\u00fcb\u00b7nis", "hat\u00b7te", "mich", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon gegeben hin dem Tod;", "tokens": ["Schon", "ge\u00b7ge\u00b7ben", "hin", "dem", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hab' nun Lebens Geist gesogen", "tokens": ["Hab'", "nun", "Le\u00b7bens", "Geist", "ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir aus Jesu Lippen rot.", "tokens": ["Mir", "aus", "Je\u00b7su", "Lip\u00b7pen", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bienlein weidet seine Wangen,", "tokens": ["Bien\u00b7lein", "wei\u00b7det", "sei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch da h\u00e4nget freundlich an;", "tokens": ["Euch", "da", "h\u00e4n\u00b7get", "freund\u00b7lich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sauget, hauchet, bleibet hangen,", "tokens": ["Sau\u00b7get", ",", "hau\u00b7chet", ",", "blei\u00b7bet", "han\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "VVI