{"textgrid.poem.41597": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: All meine Jugend war ein Sturm von Wetterschl\u00e4gen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "All meine Jugend war ein Sturm von Wetterschl\u00e4gen,", "tokens": ["All", "mei\u00b7ne", "Ju\u00b7gend", "war", "ein", "Sturm", "von", "Wet\u00b7ter\u00b7schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur hier und dort durchflammt von hellem Sonnenlicht;", "tokens": ["Nur", "hier", "und", "dort", "durch\u00b7flammt", "von", "hel\u00b7lem", "Son\u00b7nen\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So viel vernichteten der Donner und der Regen,", "tokens": ["So", "viel", "ver\u00b7nich\u00b7te\u00b7ten", "der", "Don\u00b7ner", "und", "der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df wenig Fr\u00fcchte man in meinem Garten bricht.", "tokens": ["Da\u00df", "we\u00b7nig", "Fr\u00fcch\u00b7te", "man", "in", "mei\u00b7nem", "Gar\u00b7ten", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun, da der Herbst mir schon ber\u00fchrt der Seele Schauen,", "tokens": ["Nun", ",", "da", "der", "Herbst", "mir", "schon", "be\u00b7r\u00fchrt", "der", "See\u00b7le", "Schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da Hark und Schaufel ich zu schwerer Arbeit hub,", "tokens": ["Da", "Hark", "und", "Schau\u00b7fel", "ich", "zu", "schwe\u00b7rer", "Ar\u00b7beit", "hub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mu\u00df \u00fcberschwemmt Gefild ich m\u00fchsam neu bebauen,", "tokens": ["Mu\u00df", "\u00fc\u00b7bersc\u00b7hwemmt", "Ge\u00b7fild", "ich", "m\u00fch\u00b7sam", "neu", "be\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "NN", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo L\u00f6cher grabestief der Sturz des Wassers grub.", "tokens": ["Wo", "L\u00f6\u00b7cher", "gra\u00b7bes\u00b7tief", "der", "Sturz", "des", "Was\u00b7sers", "grub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wer mag sagen, ob den Blumen, die ich tr\u00e4ume,", "tokens": ["Und", "wer", "mag", "sa\u00b7gen", ",", "ob", "den", "Blu\u00b7men", ",", "die", "ich", "tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "VVINF", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In diesem Boden, der zersp\u00fclt wie w\u00fcste R\u00e4ume,", "tokens": ["In", "die\u00b7sem", "Bo\u00b7den", ",", "der", "zer\u00b7sp\u00fclt", "wie", "w\u00fcs\u00b7te", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geheimer Saft auch wird, der ihre Kr\u00e4fte n\u00e4hrt?", "tokens": ["Ge\u00b7hei\u00b7mer", "Saft", "auch", "wird", ",", "der", "ih\u00b7re", "Kr\u00e4f\u00b7te", "n\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O Schmerz! O Schmerz! Die Zeit verschlingt all unser Leben,", "tokens": ["O", "Schmerz", "!", "O", "Schmerz", "!", "Die", "Zeit", "ver\u00b7schlingt", "all", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$.", "ART", "NN", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem dunklen Feinde, der uns stumm am Herzen zehrt,", "tokens": ["Dem", "dunk\u00b7len", "Fein\u00b7de", ",", "der", "uns", "stumm", "am", "Her\u00b7zen", "zehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mu\u00df unser eignes Blut stets neue St\u00e4rke geben!", "tokens": ["Mu\u00df", "un\u00b7ser", "eig\u00b7nes", "Blut", "stets", "neu\u00b7e", "St\u00e4r\u00b7ke", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}