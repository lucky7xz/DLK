{"dta.poem.11512": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An einem Wintermorgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "O flaumenleichte Zeit der dunkeln Fr\u00fche!", "tokens": ["O", "flau\u00b7men\u00b7leich\u00b7te", "Zeit", "der", "dun\u00b7keln", "Fr\u00fc\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welch neue Welt bewegest du in mir?", "tokens": ["Welch", "neu\u00b7e", "Welt", "be\u00b7we\u00b7gest", "du", "in", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ist's, da\u00df ich auf einmal nun in dir", "tokens": ["Was", "ist's", ",", "da\u00df", "ich", "auf", "ein\u00b7mal", "nun", "in", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PPER", "APPR", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von sanfter Wollust meines Daseyns gl\u00fche?", "tokens": ["Von", "sanf\u00b7ter", "Wol\u00b7lust", "mei\u00b7nes", "Da\u00b7seyns", "gl\u00fc\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einem Krystall gleicht meine Seele nun,", "tokens": ["Ei\u00b7nem", "Krys\u00b7tall", "gleicht", "mei\u00b7ne", "See\u00b7le", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Den noch kein falscher Strahl des Lichts getroffen;", "tokens": ["Den", "noch", "kein", "fal\u00b7scher", "Strahl", "des", "Lichts", "ge\u00b7trof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu fluthen scheint mein Geist, er scheint zu ruh'n,", "tokens": ["Zu", "flut\u00b7hen", "scheint", "mein", "Geist", ",", "er", "scheint", "zu", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Eindruck naher Wunderkr\u00e4fte offen,", "tokens": ["Dem", "Ein\u00b7druck", "na\u00b7her", "Wun\u00b7der\u00b7kr\u00e4f\u00b7te", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die aus dem klaren G\u00fcrtel blauer Luft", "tokens": ["Die", "aus", "dem", "kla\u00b7ren", "G\u00fcr\u00b7tel", "blau\u00b7er", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zuletzt ein Zauberwort vor meine Sinne ruft.", "tokens": ["Zu\u00b7letzt", "ein", "Zau\u00b7ber\u00b7wort", "vor", "mei\u00b7ne", "Sin\u00b7ne", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bei hellen Augen glaub' ich doch zu schwanken,", "tokens": ["Bei", "hel\u00b7len", "Au\u00b7gen", "glaub'", "ich", "doch", "zu", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich schlie\u00dfe sie, da\u00df nicht der Traum entweiche;", "tokens": ["Ich", "schlie\u00b7\u00dfe", "sie", ",", "da\u00df", "nicht", "der", "Traum", "ent\u00b7wei\u00b7che", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seh' ich hinab in holde Feenreiche?", "tokens": ["Seh'", "ich", "hin\u00b7ab", "in", "hol\u00b7de", "Feen\u00b7rei\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wer hat den bunten Schwarm von Bildern und Gedanken", "tokens": ["Wer", "hat", "den", "bun\u00b7ten", "Schwarm", "von", "Bil\u00b7dern", "und", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zur Pforte meines Herzens hergeladen,", "tokens": ["Zur", "Pfor\u00b7te", "mei\u00b7nes", "Her\u00b7zens", "her\u00b7ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die gl\u00e4nzend sich in diesem Busen baden,", "tokens": ["Die", "gl\u00e4n\u00b7zend", "sich", "in", "die\u00b7sem", "Bu\u00b7sen", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Goldfarb'gen Fischlein gleich im Gartenteiche?", "tokens": ["Gold\u00b7fa\u00b7rb'\u00b7gen", "Fisc\u00b7hlein", "gleich", "im", "Gar\u00b7ten\u00b7tei\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6re bald der Hirtenfl\u00f6ten Kl\u00e4nge,", "tokens": ["Ich", "h\u00f6\u00b7re", "bald", "der", "Hir\u00b7ten\u00b7fl\u00f6\u00b7ten", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie um die Krippe jener Wundernacht,", "tokens": ["Wie", "um", "die", "Krip\u00b7pe", "je\u00b7ner", "Wun\u00b7der\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald weinbekr\u00e4nzter Jugend Lustges\u00e4nge:", "tokens": ["Bald", "wein\u00b7be\u00b7kr\u00e4nz\u00b7ter", "Ju\u00b7gend", "Lust\u00b7ge\u00b7s\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer hat das friedenselige Gedr\u00e4nge", "tokens": ["Wer", "hat", "das", "frie\u00b7den\u00b7se\u00b7li\u00b7ge", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In meine traurigen W\u00e4nde hergebracht?", "tokens": ["In", "mei\u00b7ne", "trau\u00b7ri\u00b7gen", "W\u00e4n\u00b7de", "her\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und welch Gef\u00fchl entz\u00fcckter St\u00e4rke,", "tokens": ["Und", "welch", "Ge\u00b7f\u00fchl", "ent\u00b7z\u00fcck\u00b7ter", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Indem mein Sinn sich frisch zur Ferne lenkt?", "tokens": ["In\u00b7dem", "mein", "Sinn", "sich", "frisch", "zur", "Fer\u00b7ne", "lenkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom ersten Mark des heut'gen Tags getr\u00e4nkt,", "tokens": ["Vom", "ers\u00b7ten", "Mark", "des", "heut'\u00b7gen", "Tags", "ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchl' ich mir Muth zu jedem frommen Werke!", "tokens": ["F\u00fchl'", "ich", "mir", "Muth", "zu", "je\u00b7dem", "from\u00b7men", "Wer\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Seele fliegt, so weit der Himmel reicht,", "tokens": ["Die", "See\u00b7le", "fliegt", ",", "so", "weit", "der", "Him\u00b7mel", "reicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Genius jauchzt in mir; \u2014 doch sage,", "tokens": ["Der", "Ge\u00b7nius", "jauchzt", "in", "mir", ";", "doch", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$.", "$(", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Warum wird jetzt der Blick von Wehmuth feucht?", "tokens": ["Wa\u00b7rum", "wird", "jetzt", "der", "Blick", "von", "Weh\u00b7muth", "feucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Ist's ein verloren Gl\u00fcck, was mich erweicht?", "tokens": ["Ist's", "ein", "ver\u00b7lo\u00b7ren", "Gl\u00fcck", ",", "was", "mich", "er\u00b7weicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ist es ein werdendes, was ich im Herzen trage?", "tokens": ["Ist", "es", "ein", "wer\u00b7den\u00b7des", ",", "was", "ich", "im", "Her\u00b7zen", "tra\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "\u2014 Hinweg, mein Geist! hier gilt kein Stillestehn;", "tokens": ["Hin\u00b7weg", ",", "mein", "Geist", "!", "hier", "gilt", "kein", "Stil\u00b7les\u00b7tehn", ";"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Es ist ein Augenblick, und \u2014 Alles wird verwehn!", "tokens": ["Es", "ist", "ein", "Au\u00b7gen\u00b7blick", ",", "und", "Al\u00b7les", "wird", "ver\u00b7wehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "$(", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dort sieh! am Horizont l\u00fcpft sich der Vorhang schon,", "tokens": ["Dort", "sieh", "!", "am", "Ho\u00b7ri\u00b7zont", "l\u00fcpft", "sich", "der", "Vor\u00b7hang", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es tr\u00e4umt der Tag, nun sey die Nacht entflohn,", "tokens": ["Es", "tr\u00e4umt", "der", "Tag", ",", "nun", "sey", "die", "Nacht", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Purpurlippe, die geschlossen lag,", "tokens": ["Die", "Pur\u00b7pur\u00b7lip\u00b7pe", ",", "die", "ge\u00b7schlos\u00b7sen", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Haucht, halbge\u00f6ffnet, s\u00fc\u00dfe Athemz\u00fcge,", "tokens": ["Haucht", ",", "halb\u00b7ge\u00b7\u00f6ff\u00b7net", ",", "s\u00fc\u00b7\u00dfe", "A\u00b7them\u00b7z\u00fc\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf einmal blitzt das Aug', und, wie ein Gott, der Tag", "tokens": ["Auf", "ein\u00b7mal", "blitzt", "das", "Aug'", ",", "und", ",", "wie", "ein", "Gott", ",", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Beginnt im Sprung die k\u00f6niglichen Fl\u00fcge!", "tokens": ["Be\u00b7ginnt", "im", "Sprung", "die", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Fl\u00fc\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}