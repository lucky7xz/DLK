{"dta.poem.21308": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput IV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In die Nase steigt ein Prickeln so s\u00fc\u00df,", "tokens": ["In", "die", "Na\u00b7se", "steigt", "ein", "Pri\u00b7ckeln", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Man kann sich vor Wonne nicht lassen!", "tokens": ["Man", "kann", "sich", "vor", "Won\u00b7ne", "nicht", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es trieb mich hinaus in die d\u00e4mmernde Nacht,", "tokens": ["Es", "trieb", "mich", "hin\u00b7aus", "in", "die", "d\u00e4m\u00b7mern\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In die wiederhallenden Gassen.", "tokens": ["In", "die", "wie\u00b7der\u00b7hal\u00b7len\u00b7den", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die steinernen H\u00e4user schauten mich an,", "tokens": ["Die", "stei\u00b7ner\u00b7nen", "H\u00e4u\u00b7ser", "schau\u00b7ten", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als wollten sie mir berichten", "tokens": ["Als", "woll\u00b7ten", "sie", "mir", "be\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Legenden aus altverschollener Zeit,", "tokens": ["Le\u00b7gen\u00b7den", "aus", "alt\u00b7ver\u00b7schol\u00b7le\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Der heilgen Stadt C\u00f6llen Geschichten.", "tokens": ["Der", "heil\u00b7gen", "Stadt", "C\u00f6l\u00b7len", "Ge\u00b7schich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Ja, hier hat einst die Clerisey", "tokens": ["Ja", ",", "hier", "hat", "einst", "die", "Cle\u00b7ri\u00b7sey"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr frommes Wesen getrieben,", "tokens": ["Ihr", "from\u00b7mes", "We\u00b7sen", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hier haben die Dunkelm\u00e4nner geherrscht,", "tokens": ["Hier", "ha\u00b7ben", "die", "Dun\u00b7kel\u00b7m\u00e4n\u00b7ner", "ge\u00b7herrscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Ulrich von Hutten beschrieben.", "tokens": ["Die", "Ul\u00b7rich", "von", "Hut\u00b7ten", "be\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Der Cancan des Mittelalters ward hier", "tokens": ["Der", "Can\u00b7can", "des", "Mit\u00b7tel\u00b7al\u00b7ters", "ward", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Getanzt von Nonnen und M\u00f6nchen;", "tokens": ["Ge\u00b7tanzt", "von", "Non\u00b7nen", "und", "M\u00f6n\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hier schrieb Hochstraaten, der Menzel von C\u00f6lln,", "tokens": ["Hier", "schrieb", "Hoch\u00b7straa\u00b7ten", ",", "der", "Men\u00b7zel", "von", "C\u00f6lln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die giftgen Denunziazi\u00f6nchen.", "tokens": ["Die", "gift\u00b7gen", "De\u00b7nun\u00b7zi\u00b7a\u00b7zi\u00b7\u00f6n\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}