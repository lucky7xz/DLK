{"textgrid.poem.66158": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Die gefangenen S\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche, die, im schlauen Garn gefangen,", "tokens": ["Die", "Ler\u00b7che", ",", "die", ",", "im", "schlau\u00b7en", "Garn", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im dunkeln Eisenk\u00e4fig sa\u00df,", "tokens": ["Im", "dun\u00b7keln", "Ei\u00b7sen\u00b7k\u00e4\u00b7fig", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und traurig auf best\u00e4ubten Stangen", "tokens": ["Und", "trau\u00b7rig", "auf", "be\u00b7st\u00e4ub\u00b7ten", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den wirbelnden Gesang verga\u00df;", "tokens": ["Den", "wir\u00b7beln\u00b7den", "Ge\u00b7sang", "ver\u00b7ga\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "F\u00fchlt' einst, vom Morgenstrahl erhoben,", "tokens": ["F\u00fchlt'", "einst", ",", "vom", "Mor\u00b7gen\u00b7strahl", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den m\u00e4chtigen Beruf,", "tokens": ["Den", "m\u00e4ch\u00b7ti\u00b7gen", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In einem Lied den Gott zu loben,", "tokens": ["In", "ei\u00b7nem", "Lied", "den", "Gott", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sie zur Lerche schuf.", "tokens": ["Der", "sie", "zur", "Ler\u00b7che", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schon \u00f6ffnet sich ihr Schn\u00e4belchen zum Singen,", "tokens": ["Schon", "\u00f6ff\u00b7net", "sich", "ihr", "Schn\u00e4\u00b7bel\u00b7chen", "zum", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon kr\u00e4uselt sie die Melodie;", "tokens": ["Schon", "kr\u00e4u\u00b7selt", "sie", "die", "Me\u00b7lo\u00b7die", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Spannt ihre Fl\u00fcgel aus, um sich emporzuschwingen,", "tokens": ["Spannt", "ih\u00b7re", "Fl\u00fc\u00b7gel", "aus", ",", "um", "sich", "em\u00b7por\u00b7zu\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KOUI", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hoch herab aus blauer Luft zu singen", "tokens": ["Und", "hoch", "her\u00b7ab", "aus", "blau\u00b7er", "Luft", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr schmetterndes Tilirili.", "tokens": ["Ihr", "schmet\u00b7tern\u00b7des", "Ti\u00b7li\u00b7ri\u00b7li", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch sie verga\u00df im Jubel ihrer Seele", "tokens": ["Doch", "sie", "ver\u00b7ga\u00df", "im", "Ju\u00b7bel", "ih\u00b7rer", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des engen K\u00e4figs Zwang,", "tokens": ["Des", "en\u00b7gen", "K\u00e4\u00b7figs", "Zwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ach! umsonst kr\u00e4ust ihre Kehle", "tokens": ["Und", "ach", "!", "um\u00b7sonst", "kr\u00e4ust", "ih\u00b7re", "Keh\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den jubelnden Gesang.", "tokens": ["Den", "ju\u00b7beln\u00b7den", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Sie stie\u00df sich an den K\u00e4figboden,", "tokens": ["Sie", "stie\u00df", "sich", "an", "den", "K\u00e4\u00b7fig\u00b7bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "St\u00fcrzt nieder, zuckt im Staub.", "tokens": ["St\u00fcrzt", "nie\u00b7der", ",", "zuckt", "im", "Staub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun liegt sie da, gleich einem Todten,", "tokens": ["Nun", "liegt", "sie", "da", ",", "gleich", "ei\u00b7nem", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr alle T\u00f6ne taub.", "tokens": ["F\u00fcr", "al\u00b7le", "T\u00f6\u00b7ne", "taub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein f\u00fcrchterliches Bild f\u00fcr mich:", "tokens": ["Ein", "f\u00fcrch\u00b7ter\u00b7li\u00b7ches", "Bild", "f\u00fcr", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So flieg' ich auf, und so verstumm' auch ich.", "tokens": ["So", "flieg'", "ich", "auf", ",", "und", "so", "ver\u00b7stumm'", "auch", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADV", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Nachtigall singt auch im Bauer;", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "singt", "auch", "im", "Bau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Doch nicht so s\u00fc\u00df, als w\u00e4r' sie frei.", "tokens": ["Doch", "nicht", "so", "s\u00fc\u00df", ",", "als", "w\u00e4r'", "sie", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "$,", "KOKOM", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Lied gluckt f\u00fcrchterliche Trauer,", "tokens": ["Ihr", "Lied", "gluckt", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht der Freude Melodei.", "tokens": ["Und", "nicht", "der", "Freu\u00b7de", "Me\u00b7lo\u00b7dei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Bild, o Gott! ein Bild f\u00fcr mich:", "tokens": ["Ein", "Bild", ",", "o", "Gott", "!", "ein", "Bild", "f\u00fcr", "mich", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$.", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Lied t\u00f6nt auch so f\u00fcrchterlich!", "tokens": ["Mein", "Lied", "t\u00f6nt", "auch", "so", "f\u00fcrch\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Girrt die gefangne Turteltaube", "tokens": ["Girrt", "die", "ge\u00b7fang\u00b7ne", "Tur\u00b7tel\u00b7tau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch freie Lieb' und Z\u00e4rtlichkeit,", "tokens": ["Auch", "frei\u00b7e", "Lieb'", "und", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie in der sichern Fr\u00fchlingslaube,", "tokens": ["Wie", "in", "der", "si\u00b7chern", "Fr\u00fch\u00b7lings\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die keine Sklaverei entweiht?", "tokens": ["Die", "kei\u00b7ne", "Skla\u00b7ve\u00b7rei", "ent\u00b7weiht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nein, traurig girrt sie, tr\u00fcb' und bang;", "tokens": ["Nein", ",", "trau\u00b7rig", "girrt", "sie", ",", "tr\u00fcb'", "und", "bang", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VVFIN", "PPER", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Lied ist Klag', ist Sterbgesang.", "tokens": ["Ihr", "Lied", "ist", "Klag'", ",", "ist", "Sterb\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Bild, o Gott, ein Bild f\u00fcr mich:", "tokens": ["Ein", "Bild", ",", "o", "Gott", ",", "ein", "Bild", "f\u00fcr", "mich", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$,", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So klag', und wein', und girr' auch ich!", "tokens": ["So", "klag'", ",", "und", "wein'", ",", "und", "girr'", "auch", "ich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}