{"textgrid.poem.37313": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Unberufen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gest\u00fctzt auf seine beiden Kr\u00fccken,", "tokens": ["Ge\u00b7st\u00fctzt", "auf", "sei\u00b7ne", "bei\u00b7den", "Kr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die alte Kiepe auf dem R\u00fccken,", "tokens": ["Die", "al\u00b7te", "Kie\u00b7pe", "auf", "dem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ging durch das Dorf ein Bettelmann", "tokens": ["Ging", "durch", "das", "Dorf", "ein", "Bet\u00b7tel\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und klopfte stets vergeblich an.", "tokens": ["Und", "klopf\u00b7te", "stets", "ver\u00b7geb\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erst aus dem allerletzten Haus", "tokens": ["Erst", "aus", "dem", "al\u00b7ler\u00b7letz\u00b7ten", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam eine gute Frau heraus,", "tokens": ["Kam", "ei\u00b7ne", "gu\u00b7te", "Frau", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die grad den dritten Mann begraben,", "tokens": ["Die", "grad", "den", "drit\u00b7ten", "Mann", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Daher geneigt zu milden Gaben,", "tokens": ["Da\u00b7her", "ge\u00b7neigt", "zu", "mil\u00b7den", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und legt in seines Korbes Grund", "tokens": ["Und", "legt", "in", "sei\u00b7nes", "Kor\u00b7bes", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Brot von mehr als sieben Pfund.", "tokens": ["Ein", "Brot", "von", "mehr", "als", "sie\u00b7ben", "Pfund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein schmaler Steg f\u00fchrt gleich danach", "tokens": ["Ein", "schma\u00b7ler", "Steg", "f\u00fchrt", "gleich", "da\u00b7nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn \u00fcber einen Rauschebach.", "tokens": ["Ihn", "\u00fc\u00b7ber", "ei\u00b7nen", "Rau\u00b7sche\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt hab ich Brot, jetzt bin ich gl\u00fccklich!", "tokens": ["Jetzt", "hab", "ich", "Brot", ",", "jetzt", "bin", "ich", "gl\u00fcck\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So rief er froh, und augenblicklich", "tokens": ["So", "rief", "er", "froh", ",", "und", "au\u00b7gen\u00b7blick\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fiel durch den Korb, der nicht mehr gut,", "tokens": ["Fiel", "durch", "den", "Korb", ",", "der", "nicht", "mehr", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Brot hinunter in die Flut.", "tokens": ["Sein", "Brot", "hin\u00b7un\u00b7ter", "in", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPO", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das kommt von solchem \u00dcbermut.", "tokens": ["Das", "kommt", "von", "sol\u00b7chem", "\u00dc\u00b7ber\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}