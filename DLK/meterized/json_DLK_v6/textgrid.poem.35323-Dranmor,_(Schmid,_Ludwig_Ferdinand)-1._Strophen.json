{"textgrid.poem.35323": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1. Strophen", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O mein einsam \u2013 einsam \u2013 einsam Kissen,", "tokens": ["O", "mein", "ein\u00b7sam", "\u2013", "ein\u00b7sam", "\u2013", "ein\u00b7sam", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "$(", "ADJD", "$(", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo bleibt mein Herzensfreund, der s\u00fc\u00dfe, traute?", "tokens": ["Wo", "bleibt", "mein", "Her\u00b7zens\u00b7freund", ",", "der", "s\u00fc\u00b7\u00dfe", ",", "trau\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist es ", "tokens": ["Ist", "es"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Weit, weit von hier, von St\u00fcrmen fortgerissen?", "tokens": ["Weit", ",", "weit", "von", "hier", ",", "von", "St\u00fcr\u00b7men", "fort\u00b7ge\u00b7ris\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ADV", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O mein einsam \u2013 einsam \u2013 einsam Kissen,", "tokens": ["O", "mein", "ein\u00b7sam", "\u2013", "ein\u00b7sam", "\u2013", "ein\u00b7sam", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "$(", "ADJD", "$(", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die Stelle k\u00fcss' ich, die sein Haupt umfangen;", "tokens": ["Die", "Stel\u00b7le", "k\u00fcss'", "ich", ",", "die", "sein", "Haupt", "um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sind die N\u00e4chte langsam hingegangen,", "tokens": ["Wie", "sind", "die", "N\u00e4ch\u00b7te", "lang\u00b7sam", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seit er mich lie\u00df in diesen K\u00fcmmernissen!", "tokens": ["Seit", "er", "mich", "lie\u00df", "in", "die\u00b7sen", "K\u00fcm\u00b7mer\u00b7nis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O mein einsam, mein betr\u00fcbtes Kissen,", "tokens": ["O", "mein", "ein\u00b7sam", ",", "mein", "be\u00b7tr\u00fcb\u00b7tes", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "La\u00df s\u00fc\u00df mich tr\u00e4umen, la\u00df mein Herz nicht brechen!", "tokens": ["La\u00df", "s\u00fc\u00df", "mich", "tr\u00e4u\u00b7men", ",", "la\u00df", "mein", "Herz", "nicht", "bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "VVINF", "$,", "VVIMP", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Liebster kommt \u2013 ich habe sein Versprechen;", "tokens": ["Mein", "Liebs\u00b7ter", "kommt", "\u2013", "ich", "ha\u00b7be", "sein", "Ver\u00b7spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch ist der Tod zu fr\u00fch \u2013 du mu\u00dft es wissen.", "tokens": ["Noch", "ist", "der", "Tod", "zu", "fr\u00fch", "\u2013", "du", "mu\u00dft", "es", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKA", "ADJD", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und hab' ich ", "tokens": ["Und", "hab'", "ich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "In meine Arme will ich hei\u00df ihn pressen;", "tokens": ["In", "mei\u00b7ne", "Ar\u00b7me", "will", "ich", "hei\u00df", "ihn", "pres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O dann sei aller Kummer rasch vergessen,", "tokens": ["O", "dann", "sei", "al\u00b7ler", "Kum\u00b7mer", "rasch", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann sei sein liebend Herz mein Sterbekissen!", "tokens": ["Dann", "sei", "sein", "lie\u00b7bend", "Herz", "mein", "Ster\u00b7be\u00b7kis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}