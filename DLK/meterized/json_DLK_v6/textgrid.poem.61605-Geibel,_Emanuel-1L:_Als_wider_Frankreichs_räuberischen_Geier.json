{"textgrid.poem.61605": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als wider Frankreichs r\u00e4uberischen Geier", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als wider Frankreichs r\u00e4uberischen Geier", "tokens": ["Als", "wi\u00b7der", "Fran\u00b7kreichs", "r\u00e4u\u00b7be\u00b7ri\u00b7schen", "Gei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Weidwerk anhub durch die deutschen Lande,", "tokens": ["Das", "Weid\u00b7werk", "an\u00b7hub", "durch", "die", "deut\u00b7schen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da scho\u00df, die Seelen zu geweihtem Brande", "tokens": ["Da", "scho\u00df", ",", "die", "See\u00b7len", "zu", "ge\u00b7weih\u00b7tem", "Bran\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entz\u00fcndend, Blitz auf Blitz aus deiner Leier.", "tokens": ["Ent\u00b7z\u00fcn\u00b7dend", ",", "Blitz", "auf", "Blitz", "aus", "dei\u00b7ner", "Lei\u00b7er", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zum Schwerte st\u00fcrmtest du in zorn'ger Feier", "tokens": ["Zum", "Schwer\u00b7te", "st\u00fcrm\u00b7test", "du", "in", "zorn'\u00b7ger", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Volk empor aus tatenloser Schande,", "tokens": ["Dein", "Volk", "em\u00b7por", "aus", "ta\u00b7ten\u00b7lo\u00b7ser", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und selbst voran im schwarzen Jagdgewande", "tokens": ["Und", "selbst", "vo\u00b7ran", "im", "schwar\u00b7zen", "Jagd\u00b7ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Eisenbraut erkorst du dir als Freier.", "tokens": ["Die", "Ei\u00b7sen\u00b7braut", "er\u00b7korst", "du", "dir", "als", "Frei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sangst und rangst du, unsre Not zu s\u00fchnen,", "tokens": ["So", "sangst", "und", "rangst", "du", ",", "uns\u00b7re", "Not", "zu", "s\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wardst in beidem gleich getreu erfunden,", "tokens": ["Und", "wardst", "in", "bei\u00b7dem", "gleich", "ge\u00b7treu", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Lied besiegelnd durch den Tod der K\u00fchnen.", "tokens": ["Dein", "Lied", "be\u00b7sie\u00b7gelnd", "durch", "den", "Tod", "der", "K\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drum, wenn manch edler Kranz im Flug der Stunden", "tokens": ["Drum", ",", "wenn", "manch", "ed\u00b7ler", "Kranz", "im", "Flug", "der", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "PIAT", "ADJA", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dahinwelkt, wird noch frisch der deine gr\u00fcnen,", "tokens": ["Da\u00b7hin\u00b7welkt", ",", "wird", "noch", "frisch", "der", "dei\u00b7ne", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VAFIN", "ADV", "ADJD", "ART", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Betaut mit Opferblut aus heil'gen Wunden.", "tokens": ["Be\u00b7taut", "mit", "Op\u00b7fer\u00b7blut", "aus", "heil'\u00b7gen", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}