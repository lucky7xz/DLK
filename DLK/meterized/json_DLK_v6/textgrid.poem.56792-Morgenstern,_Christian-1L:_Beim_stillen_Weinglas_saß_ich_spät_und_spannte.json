{"textgrid.poem.56792": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Beim stillen Weinglas sa\u00df ich sp\u00e4t und spannte", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beim stillen Weinglas sa\u00df ich sp\u00e4t und spannte", "tokens": ["Beim", "stil\u00b7len", "Wein\u00b7glas", "sa\u00df", "ich", "sp\u00e4t", "und", "spann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zerri\u00dfne Saiten neu der treuen Geige \u2013:", "tokens": ["zer\u00b7ri\u00df\u00b7ne", "Sai\u00b7ten", "neu", "der", "treu\u00b7en", "Gei\u00b7ge", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADJD", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da war's, da\u00df mir das harte Haupt des Dante", "tokens": ["Da", "wa\u00b7r's", ",", "da\u00df", "mir", "das", "har\u00b7te", "Haupt", "des", "Dan\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "erschien in meines R\u00f6mers dunkler Neige:", "tokens": ["er\u00b7schien", "in", "mei\u00b7nes", "R\u00f6\u00b7mers", "dunk\u00b7ler", "Nei\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wollte es die Lieder-Stufen h\u00f6hnen,", "tokens": ["Als", "woll\u00b7te", "es", "die", "Lie\u00b7der\u00b7Stu\u00b7fen", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "auf denen ich zu meinem Ruhme steige.", "tokens": ["auf", "de\u00b7nen", "ich", "zu", "mei\u00b7nem", "Ruh\u00b7me", "stei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und alsobald begann im Zorn zu t\u00f6nen", "tokens": ["Und", "al\u00b7so\u00b7bald", "be\u00b7gann", "im", "Zorn", "zu", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mein Saitenspiel von hochverme\u00dfnen H\u00e4nden", "tokens": ["mein", "Sai\u00b7ten\u00b7spiel", "von", "hoch\u00b7ver\u00b7me\u00df\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und f\u00fcllte mein Gemach mit eh'rnem Dr\u00f6hnen.", "tokens": ["und", "f\u00fcll\u00b7te", "mein", "Ge\u00b7mach", "mit", "eh'r\u00b7nem", "Dr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und zuckend von irrlichterischen Br\u00e4nden", "tokens": ["Und", "zu\u00b7ckend", "von", "irr\u00b7lich\u00b7te\u00b7ri\u00b7schen", "Br\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "zerbarst vor mir die laute Nacht in St\u00fccke,", "tokens": ["zer\u00b7barst", "vor", "mir", "die", "lau\u00b7te", "Nacht", "in", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und von Gespenstern schwoll's aus fahlen W\u00e4nden ...", "tokens": ["und", "von", "Ge\u00b7spens\u00b7tern", "schwoll's", "aus", "fah\u00b7len", "W\u00e4n\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch wie ich rasch des Worts tollk\u00fchne Br\u00fccke", "tokens": ["Doch", "wie", "ich", "rasch", "des", "Worts", "toll\u00b7k\u00fch\u00b7ne", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nach solcher Schattenflucht zu schlagen strebe,", "tokens": ["nach", "sol\u00b7cher", "Schat\u00b7ten\u00b7flucht", "zu", "schla\u00b7gen", "stre\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "entweicht es schon und lockt mit neuer T\u00fccke ...", "tokens": ["ent\u00b7weicht", "es", "schon", "und", "lockt", "mit", "neu\u00b7er", "T\u00fc\u00b7cke", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Bis endlich in die rinnenden Gewebe", "tokens": ["Bis", "end\u00b7lich", "in", "die", "rin\u00b7nen\u00b7den", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "einschl\u00e4gt des Willens grollende Gewalt", "tokens": ["ein\u00b7schl\u00e4gt", "des", "Wil\u00b7lens", "grol\u00b7len\u00b7de", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und eins ergreift inmitten seiner Schwebe \u2013:", "tokens": ["und", "eins", "er\u00b7greift", "in\u00b7mit\u00b7ten", "sei\u00b7ner", "Schwe\u00b7be", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mit finstren St\u00e4mmen dr\u00e4ngt empor ein Wald,", "tokens": ["Mit", "finst\u00b7ren", "St\u00e4m\u00b7men", "dr\u00e4ngt", "em\u00b7por", "ein", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "drin Wiesengrund im Dreieck ausgeweitet,", "tokens": ["drin", "Wie\u00b7sen\u00b7grund", "im", "Drei\u00b7eck", "aus\u00b7ge\u00b7wei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "von Klumpen Mondgew\u00f6lkes \u00fcberballt.", "tokens": ["von", "Klum\u00b7pen", "Mond\u00b7ge\u00b7w\u00f6l\u00b7kes", "\u00fc\u00b7ber\u00b7ballt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch mehr mein Aug dem D\u00e4mmer noch entstreitet:", "tokens": ["Doch", "mehr", "mein", "Aug", "dem", "D\u00e4m\u00b7mer", "noch", "ent\u00b7strei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vier sattelleere Rosse schau ich grasen", "tokens": ["Vier", "sat\u00b7tel\u00b7lee\u00b7re", "Ros\u00b7se", "schau", "ich", "gra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und dunkle K\u00f6rper unweit hingebreitet.", "tokens": ["und", "dunk\u00b7le", "K\u00f6r\u00b7per", "un\u00b7weit", "hin\u00b7ge\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sind's R\u00e4uber, die die Flucht hierher geblasen?", "tokens": ["Sin\u00b7d's", "R\u00e4u\u00b7ber", ",", "die", "die", "Flucht", "hier\u00b7her", "ge\u00b7bla\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN", "PAV", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein Mondstrahl glei\u00dft: Dies Haupt verr\u00e4t ein Weib,", "tokens": ["Ein", "Mond\u00b7strahl", "glei\u00dft", ":", "Dies", "Haupt", "ver\u00b7r\u00e4t", "ein", "Weib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PDS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zwei gr\u00fcne Augen schillern im Verglasen.", "tokens": ["zwei", "gr\u00fc\u00b7ne", "Au\u00b7gen", "schil\u00b7lern", "im", "Ver\u00b7gla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und um dies Haupt welch f\u00fcrchterlicher Leib!", "tokens": ["Und", "um", "dies", "Haupt", "welch", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "NN", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur widerwillig gibt die fahle Nacht", "tokens": ["Nur", "wi\u00b7der\u00b7wil\u00b7lig", "gibt", "die", "fah\u00b7le", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sein Bild, da\u00df keinem es zu treu verbleib'.", "tokens": ["sein", "Bild", ",", "da\u00df", "kei\u00b7nem", "es", "zu", "treu", "ver\u00b7bleib'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PIS", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und j\u00e4h erkenn' ich, wer hier Rast gemacht \u2013:", "tokens": ["Und", "j\u00e4h", "er\u00b7kenn'", "ich", ",", "wer", "hier", "Rast", "ge\u00b7macht", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "PWS", "ADV", "NN", "VVPP", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Tod, der Krieg, der Hunger und die Pest, \u2013", "tokens": ["Der", "Tod", ",", "der", "Krieg", ",", "der", "Hun\u00b7ger", "und", "die", "Pest", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "tiefm\u00fcde Nachtrast! Nur der Hunger wacht ...", "tokens": ["tief\u00b7m\u00fc\u00b7de", "Nach\u00b7trast", "!", "Nur", "der", "Hun\u00b7ger", "wacht", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Die Greisin kauert Kinn an Knie gepre\u00dft ...", "tokens": ["Die", "Grei\u00b7sin", "kau\u00b7ert", "Kinn", "an", "Knie", "ge\u00b7pre\u00dft", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Krieg, die Stirn am Schwertknauf, atmet schwer,", "tokens": ["Der", "Krieg", ",", "die", "Stirn", "am", "Schwert\u00b7knauf", ",", "at\u00b7met", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "blut\u00fcberronnen noch vom letzten Fest ...", "tokens": ["blut\u00b7\u00fc\u00b7ber\u00b7ron\u00b7nen", "noch", "vom", "letz\u00b7ten", "Fest", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "In freudelosen Halbschlaf sank selbst Er ...", "tokens": ["In", "freu\u00b7de\u00b7lo\u00b7sen", "Halb\u00b7schlaf", "sank", "selbst", "Er", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}