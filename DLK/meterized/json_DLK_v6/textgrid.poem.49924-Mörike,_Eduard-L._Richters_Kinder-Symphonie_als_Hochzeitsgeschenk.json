{"textgrid.poem.49924": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "L. Richters Kinder-Symphonie als Hochzeitsgeschenk", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, Liebwerteste, seht ihr einen kleinen", "tokens": ["Hier", ",", "Lieb\u00b7wer\u00b7tes\u00b7te", ",", "seht", "ihr", "ei\u00b7nen", "klei\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dilettantenverein, ungleich an Kr\u00e4ften,", "tokens": ["Di\u00b7let\u00b7tan\u00b7ten\u00b7ver\u00b7ein", ",", "un\u00b7gleich", "an", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und teilweise versehn mit Tonwerkzeugen,", "tokens": ["Und", "teil\u00b7wei\u00b7se", "ver\u00b7sehn", "mit", "Ton\u00b7werk\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die dem H\u00f6renden bange machen k\u00f6nnten.", "tokens": ["Die", "dem", "H\u00f6\u00b7ren\u00b7den", "ban\u00b7ge", "ma\u00b7chen", "k\u00f6nn\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ein symphonisches St\u00fcck mit Singpartieen", "tokens": ["Ein", "sym\u00b7pho\u00b7ni\u00b7sches", "St\u00fcck", "mit", "Sing\u00b7par\u00b7ti\u00b7een"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gilt's, und zwar noch der ersten Proben eine.", "tokens": ["Gilt's", ",", "und", "zwar", "noch", "der", "ers\u00b7ten", "Pro\u00b7ben", "ei\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADV", "ADV", "ART", "ADJA", "NN", "ART", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Vom and\u00e4chtigen Klarinett herunter", "tokens": ["Vom", "an\u00b7d\u00e4ch\u00b7ti\u00b7gen", "Kla\u00b7ri\u00b7nett", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APZR"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bis zum R\u00e4tschchen und Vater Haydns Kuckuck", "tokens": ["Bis", "zum", "R\u00e4t\u00b7schchen", "und", "Va\u00b7ter", "Haydns", "Ku\u00b7ckuck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "KON", "NN", "NE", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.5": {"text": "Tut ein jedes nach seinem Kunstverm\u00f6gen.", "tokens": ["Tut", "ein", "je\u00b7des", "nach", "sei\u00b7nem", "Kunst\u00b7ver\u00b7m\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIAT", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ba\u00dfposaune, Trompete lasten sichtlich", "tokens": ["Ba\u00df\u00b7po\u00b7sau\u00b7ne", ",", "Trom\u00b7pe\u00b7te", "las\u00b7ten", "sicht\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "VVFIN", "PRF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Auf der schmelzenden Bratsche; offenbar auch", "tokens": ["Auf", "der", "schmel\u00b7zen\u00b7den", "Brat\u00b7sche", ";", "of\u00b7fen\u00b7bar", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ADJD", "ADV"], "meter": "+-+--+-+---", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Kommt die S\u00e4ngerin schon nicht mehr zum Worte;", "tokens": ["Kommt", "die", "S\u00e4n\u00b7ge\u00b7rin", "schon", "nicht", "mehr", "zum", "Wor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Doch nichts bringt den Direktor aus der Fassung.", "tokens": ["Doch", "nichts", "bringt", "den", "Di\u00b7rek\u00b7tor", "aus", "der", "Fas\u00b7sung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Sagt, und w\u00e4ren euch denn die guten Kinder", "tokens": ["Sagt", ",", "und", "w\u00e4\u00b7ren", "euch", "denn", "die", "gu\u00b7ten", "Kin\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "V\u00f6llig fremd? es entdeckte wirklich niemand", "tokens": ["V\u00f6l\u00b7lig", "fremd", "?", "es", "ent\u00b7deck\u00b7te", "wirk\u00b7lich", "nie\u00b7mand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$.", "PPER", "VVFIN", "ADJD", "PIS"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ein bekanntes Gesichtchen hier? \u2013 Nun also", "tokens": ["Ein", "be\u00b7kann\u00b7tes", "Ge\u00b7sicht\u00b7chen", "hier", "?", "\u2013", "Nun", "al\u00b7so"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "$(", "ADV", "ADV"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wi\u00dft: Landsleute sind's unsres vielgeehrten", "tokens": ["Wi\u00dft", ":", "Lands\u00b7leu\u00b7te", "sin\u00b7d's", "uns\u00b7res", "viel\u00b7geehr\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "NN", "VAFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Br\u00e4utigams! \u2013 wie ich n\u00e4her gleich erkl\u00e4re.", "tokens": ["Br\u00e4u\u00b7ti\u00b7gams", "!", "\u2013", "wie", "ich", "n\u00e4\u00b7her", "gleich", "er\u00b7kl\u00e4\u00b7re", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "PWAV", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Denn ich selber, mit einem Dresdner Freunde,", "tokens": ["Denn", "ich", "sel\u00b7ber", ",", "mit", "ei\u00b7nem", "Dresd\u00b7ner", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der verwichenen Herbst sich gern, als Maler,", "tokens": ["Der", "ver\u00b7wi\u00b7che\u00b7nen", "Herbst", "sich", "gern", ",", "als", "Ma\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "$,", "KOUS", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Unser Schwaben einmal beschauen wollte,", "tokens": ["Un\u00b7ser", "Schwa\u00b7ben", "ein\u00b7mal", "be\u00b7schau\u00b7en", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "War zuf\u00e4lliger Zeuge dieser Szene,", "tokens": ["War", "zu\u00b7f\u00e4l\u00b7li\u00b7ger", "Zeu\u00b7ge", "die\u00b7ser", "Sze\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Als wir beide, von Friedrichshafen kommend,", "tokens": ["Als", "wir", "bei\u00b7de", ",", "von", "Fried\u00b7richs\u00b7ha\u00b7fen", "kom\u00b7mend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Vor dem St\u00e4dtchen im Ri\u00dftal, das ihr kennet,", "tokens": ["Vor", "dem", "St\u00e4dt\u00b7chen", "im", "Ri\u00df\u00b7tal", ",", "das", "ihr", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "In Erwartung des Vier-Uhr-Zuges m\u00fc\u00dfig", "tokens": ["In", "Er\u00b7war\u00b7tung", "des", "Vier\u00b7Uhr\u00b7Zu\u00b7ges", "m\u00fc\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Hin und her um die alten Mauern strichen.", "tokens": ["Hin", "und", "her", "um", "die", "al\u00b7ten", "Mau\u00b7ern", "stri\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Leider waren des Herrn Dekans Hochw\u00fcrden", "tokens": ["Lei\u00b7der", "wa\u00b7ren", "des", "Herrn", "De\u00b7kans", "Hoch\u00b7w\u00fcr\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NE", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Damals eben verreist, er h\u00e4tte sonst wohl", "tokens": ["Da\u00b7mals", "e\u00b7ben", "ver\u00b7reist", ",", "er", "h\u00e4t\u00b7te", "sonst", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "$,", "PPER", "VAFIN", "ADV", "ADV"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Uns im k\u00fchligen Haus bei sich ein F\u00e4\u00dflein", "tokens": ["Uns", "im", "k\u00fch\u00b7li\u00b7gen", "Haus", "bei", "sich", "ein", "F\u00e4\u00df\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN", "APPR", "PRF", "ART", "NN"], "meter": "+-+--+---+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Angestochen des edlen Kraftgebr\u00e4udes,", "tokens": ["An\u00b7ge\u00b7sto\u00b7chen", "des", "ed\u00b7len", "Kraft\u00b7ge\u00b7br\u00e4u\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Das sein heimatlich Ulm ihm zollt allj\u00e4hrlich.", "tokens": ["Das", "sein", "hei\u00b7mat\u00b7lich", "Ulm", "ihm", "zollt", "all\u00b7j\u00e4hr\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJD", "NE", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nun, beim \u00e4u\u00dfersten H\u00e4uschen an der hintern", "tokens": ["Nun", ",", "beim", "\u00e4u\u00b7\u00dfers\u00b7ten", "H\u00e4usc\u00b7hen", "an", "der", "hin\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Grabenmauer ist gar ein stiller Winkel.", "tokens": ["Gra\u00b7ben\u00b7mau\u00b7er", "ist", "gar", "ein", "stil\u00b7ler", "Win\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Eine Witwe, des Kantors selig, wohnt dort", "tokens": ["Ei\u00b7ne", "Wit\u00b7we", ",", "des", "Kan\u00b7tors", "se\u00b7lig", ",", "wohnt", "dort"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "$,", "VVFIN", "ADV"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Mit drei Kindern. Der eine Sohn ererbte", "tokens": ["Mit", "drei", "Kin\u00b7dern", ".", "Der", "ei\u00b7ne", "Sohn", "er\u00b7erb\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "$.", "ART", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Seines Vaters geliebte Geige, aber", "tokens": ["Sei\u00b7nes", "Va\u00b7ters", "ge\u00b7lieb\u00b7te", "Gei\u00b7ge", ",", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "ADV"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Alle dreie von seinen Gaben etwas.", "tokens": ["Al\u00b7le", "drei\u00b7e", "von", "sei\u00b7nen", "Ga\u00b7ben", "et\u00b7was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Unvollst\u00e4ndig noch, als wir kamen, l\u00e4rmte,", "tokens": ["Un\u00b7voll\u00b7st\u00e4n\u00b7dig", "noch", ",", "als", "wir", "ka\u00b7men", ",", "l\u00e4rm\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Sang und pfiff das Orchester durcheinander:", "tokens": ["Sang", "und", "pfiff", "das", "Or\u00b7ches\u00b7ter", "durch\u00b7ein\u00b7an\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "F\u00fcr die Fehlenden spielte die gesamte", "tokens": ["F\u00fcr", "die", "Feh\u00b7len\u00b7den", "spiel\u00b7te", "die", "ge\u00b7sam\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Junge Nachbarschaft mit, und nicht nach Noten.", "tokens": ["Jun\u00b7ge", "Nach\u00b7bar\u00b7schaft", "mit", ",", "und", "nicht", "nach", "No\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,", "KON", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Doch verstummend auf unsern Wink mit einmal", "tokens": ["Doch", "ver\u00b7stum\u00b7mend", "auf", "un\u00b7sern", "Wink", "mit", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "PPOSAT", "NN", "APPR", "ADV"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Wich das wirre Get\u00f6s dem hellen Goldklang", "tokens": ["Wich", "das", "wir\u00b7re", "Ge\u00b7t\u00f6s", "dem", "hel\u00b7len", "Gold\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Einer himmlischen M\u00e4dchenstimme, wie wenn", "tokens": ["Ei\u00b7ner", "himm\u00b7li\u00b7schen", "M\u00e4d\u00b7chen\u00b7stim\u00b7me", ",", "wie", "wenn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOKOM", "KOUS"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Nachts aus krausem Gew\u00f6lk des Mondes Klarheit", "tokens": ["Nachts", "aus", "krau\u00b7sem", "Ge\u00b7w\u00f6lk", "des", "Mon\u00b7des", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "NN", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Tritt, ein Weilchen die reine Bahn behauptend.", "tokens": ["Tritt", ",", "ein", "Weil\u00b7chen", "die", "rei\u00b7ne", "Bahn", "be\u00b7haup\u00b7tend", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Aber nimmer beschreib ich dieser Kehle", "tokens": ["A\u00b7ber", "nim\u00b7mer", "be\u00b7schreib", "ich", "die\u00b7ser", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PDAT", "NN"], "meter": "+-+--+-+-