{"textgrid.poem.25960": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Heut fragte der blitzgr\u00fcne Wald", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut fragte der blitzgr\u00fcne Wald,", "tokens": ["Heut", "frag\u00b7te", "der", "blitz\u00b7gr\u00fc\u00b7ne", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo Baum bei Baum eng wohnt:", "tokens": ["Wo", "Baum", "bei", "Baum", "eng", "wohnt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbkommt denn Frau Dauthendey nicht bald?", "tokens": ["\u00bb", "kommt", "denn", "Frau", "Dau\u00b7then\u00b7dey", "nicht", "bald", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "KON", "NN", "NE", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Kein Leben sich sonst lohnt!", "tokens": ["Kein", "Le\u00b7ben", "sich", "sonst", "lohnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bl\u00fcmlein und K\u00e4fer sterben mir,", "tokens": ["Bl\u00fcm\u00b7lein", "und", "K\u00e4\u00b7fer", "ster\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Kein Blatt bleibt mehr am Platz,", "tokens": ["Kein", "Blatt", "bleibt", "mehr", "am", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auswandern tun die B\u00e4ume schier,", "tokens": ["Aus\u00b7wan\u00b7dern", "tun", "die", "B\u00e4u\u00b7me", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kommt sie nicht, unser Schatz.", "tokens": ["Kommt", "sie", "nicht", ",", "un\u00b7ser", "Schatz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Denn manchen Kranz Frau Dauthendey", "tokens": ["Denn", "man\u00b7chen", "Kranz", "Frau", "Dau\u00b7then\u00b7dey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Pfl\u00fcckte sie sonst im Wald,", "tokens": ["Pfl\u00fcck\u00b7te", "sie", "sonst", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Den B\u00e4umen war's nicht einerlei,", "tokens": ["Den", "B\u00e4u\u00b7men", "wa\u00b7r's", "nicht", "ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Sie wurden dann steinalt.", "tokens": ["Sie", "wur\u00b7den", "dann", "stei\u00b7nalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.13": {"text": "Das Reh und mancher Damhirsch sprang,", "tokens": ["Das", "Reh", "und", "man\u00b7cher", "Dam\u00b7hirsch", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Trug ihre Schlepp im Maul.", "tokens": ["Trug", "ih\u00b7re", "Schlepp", "im", "Maul", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Jetzt ist es still, kein Kuckuck sang,", "tokens": ["Jetzt", "ist", "es", "still", ",", "kein", "Ku\u00b7ckuck", "sang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der ganze Wald liegt faul.\u00ab", "tokens": ["Der", "gan\u00b7ze", "Wald", "liegt", "faul", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}