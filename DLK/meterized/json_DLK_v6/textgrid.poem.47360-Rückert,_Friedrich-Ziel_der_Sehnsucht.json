{"textgrid.poem.47360": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ziel der Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich durch die Fluren schweife,", "tokens": ["Wenn", "ich", "durch", "die", "Flu\u00b7ren", "schwei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene suchend her und hin,", "tokens": ["Je\u00b7ne", "su\u00b7chend", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mich schlug in goldne Reife,", "tokens": ["Die", "mich", "schlug", "in", "gold\u00b7ne", "Rei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ich ganz zu eigen bin:", "tokens": ["Der", "ich", "ganz", "zu", "ei\u00b7gen", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKA", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welch ein W\u00fcnschen, welch ein W\u00e4hnen", "tokens": ["Welch", "ein", "W\u00fcn\u00b7schen", ",", "welch", "ein", "W\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PWAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hebt die Seele trunken auf;", "tokens": ["Hebt", "die", "See\u00b7le", "trun\u00b7ken", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Wolken tr\u00e4gt das Sehnen,", "tokens": ["In", "die", "Wol\u00b7ken", "tr\u00e4gt", "das", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Himmel mich hinauf.", "tokens": ["In", "die", "Him\u00b7mel", "mich", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit dem Vogel m\u00f6cht' ich fliegen,", "tokens": ["Mit", "dem", "Vo\u00b7gel", "m\u00f6cht'", "ich", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Sternen m\u00f6cht' ich stehn,", "tokens": ["Auf", "den", "Ster\u00b7nen", "m\u00f6cht'", "ich", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich auf Windesfittich wiegen,", "tokens": ["Mich", "auf", "Win\u00b7des\u00b7fit\u00b7tich", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brausend \u00fcber Wipfel gehn!", "tokens": ["Brau\u00b7send", "\u00fc\u00b7ber", "Wip\u00b7fel", "gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bis ich komme zu dem \u00d6rtchen,", "tokens": ["Bis", "ich", "kom\u00b7me", "zu", "dem", "\u00d6rt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo aus B\u00fcschen tief heraus", "tokens": ["Wo", "aus", "B\u00fc\u00b7schen", "tief", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem beigelehnten Pf\u00f6rtchen", "tokens": ["Mit", "dem", "bei\u00b7ge\u00b7lehn\u00b7ten", "Pf\u00f6rt\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Winkt ihr kleines H\u00fcttenhaus.", "tokens": ["Winkt", "ihr", "klei\u00b7nes", "H\u00fct\u00b7ten\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schnell verflogen, schnell zergangen", "tokens": ["Schnell", "ver\u00b7flo\u00b7gen", ",", "schnell", "zer\u00b7gan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind die W\u00fcnsche gro\u00df und klein,", "tokens": ["Sind", "die", "W\u00fcn\u00b7sche", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Sehnsucht kehrt gefangen", "tokens": ["Und", "die", "Sehn\u00b7sucht", "kehrt", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still ins stille H\u00fcttchen ein.", "tokens": ["Still", "ins", "stil\u00b7le", "H\u00fctt\u00b7chen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}