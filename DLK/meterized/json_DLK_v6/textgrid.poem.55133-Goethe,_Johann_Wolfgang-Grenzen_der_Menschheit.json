{"textgrid.poem.55133": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Grenzen der Menschheit", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der uralte", "tokens": ["Wenn", "der", "ur\u00b7al\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Heilige Vater", "tokens": ["Hei\u00b7li\u00b7ge", "Va\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit gelassener Hand", "tokens": ["Mit", "ge\u00b7las\u00b7se\u00b7ner", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Aus rollenden Wolken", "tokens": ["Aus", "rol\u00b7len\u00b7den", "Wol\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Segnende Blitze", "tokens": ["Seg\u00b7nen\u00b7de", "Blit\u00b7ze"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "\u00dcber die Erde s\u00e4t,", "tokens": ["\u00dc\u00b7ber", "die", "Er\u00b7de", "s\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "K\u00fc\u00df ich den letzten", "tokens": ["K\u00fc\u00df", "ich", "den", "letz\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Saum seines Kleides,", "tokens": ["Saum", "sei\u00b7nes", "Klei\u00b7des", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Kindliche Schauer", "tokens": ["Kind\u00b7li\u00b7che", "Schau\u00b7er"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Treu in der Brust.", "tokens": ["Treu", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Denn mit G\u00f6ttern", "tokens": ["Denn", "mit", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Soll sich nicht messen", "tokens": ["Soll", "sich", "nicht", "mes\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "PTKNEG", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Irgendein Mensch", "tokens": ["Ir\u00b7gend\u00b7ein", "Mensch"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Hebt er sich aufw\u00e4rts", "tokens": ["Hebt", "er", "sich", "auf\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und ber\u00fchrt", "tokens": ["Und", "be\u00b7r\u00fchrt"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Mit dem Scheitel die Sterne,", "tokens": ["Mit", "dem", "Schei\u00b7tel", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Nirgends haften dann", "tokens": ["Nir\u00b7gends", "haf\u00b7ten", "dann"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Die unsichern Sohlen,", "tokens": ["Die", "un\u00b7si\u00b7chern", "Soh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Und mit ihm spielen", "tokens": ["Und", "mit", "ihm", "spie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Wolken und Winde.", "tokens": ["Wol\u00b7ken", "und", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Steht er mit festen,", "tokens": ["Steht", "er", "mit", "fes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Markigen Knochen", "tokens": ["Mar\u00b7ki\u00b7gen", "Kno\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Auf der wohlgegr\u00fcndeten,", "tokens": ["Auf", "der", "wohl\u00b7ge\u00b7gr\u00fcn\u00b7de\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dauernden Erde,", "tokens": ["Dau\u00b7ern\u00b7den", "Er\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Reicht er nicht auf,", "tokens": ["Reicht", "er", "nicht", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Nur mit der Eiche", "tokens": ["Nur", "mit", "der", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Oder der Rebe", "tokens": ["O\u00b7der", "der", "Re\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sich zu vergleichen.", "tokens": ["Sich", "zu", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Was unterscheidet", "tokens": ["Was", "un\u00b7ter\u00b7schei\u00b7det"], "token_info": ["word", "word"], "pos": ["PWS", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "G\u00f6tter von Menschen?", "tokens": ["G\u00f6t\u00b7ter", "von", "Men\u00b7schen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df viele Wellen", "tokens": ["Da\u00df", "vie\u00b7le", "Wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Vor jenen wandeln,", "tokens": ["Vor", "je\u00b7nen", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein ewiger Strom:", "tokens": ["Ein", "e\u00b7wi\u00b7ger", "Strom", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Uns hebt die Welle,", "tokens": ["Uns", "hebt", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Verschlingt die Welle,", "tokens": ["Ver\u00b7schlingt", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und wir versinken.", "tokens": ["Und", "wir", "ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Ein kleiner Ring", "tokens": ["Ein", "klei\u00b7ner", "Ring"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Begrenzt unser Leben,", "tokens": ["Be\u00b7grenzt", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und viele Geschlechter", "tokens": ["Und", "vie\u00b7le", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Reihen sich dauernd", "tokens": ["Rei\u00b7hen", "sich", "dau\u00b7ernd"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "An ihres Daseins", "tokens": ["An", "ih\u00b7res", "Da\u00b7seins"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Unendliche Kette.", "tokens": ["Un\u00b7end\u00b7li\u00b7che", "Ket\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}