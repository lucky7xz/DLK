{"dta.poem.16711": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "70.  Die Lauterkeit,", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Vollkomne Lauterkeit ist Bild-Form-Liebe-lo\u00df:", "tokens": ["Voll\u00b7kom\u00b7ne", "Lau\u00b7ter\u00b7keit", "ist", "Bild\u00b7For\u00b7mLie\u00b7be\u00b7lo\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steht aller Eigenschafft/ wie GOttes wesen blo\u00df.", "tokens": ["Steht", "al\u00b7ler", "Ei\u00b7gen\u00b7schafft", "/", "wie", "Got\u00b7tes", "we\u00b7sen", "blo\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$(", "KOKOM", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}