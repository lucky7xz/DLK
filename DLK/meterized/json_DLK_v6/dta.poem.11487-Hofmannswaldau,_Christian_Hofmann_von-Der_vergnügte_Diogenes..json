{"dta.poem.11487": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der vergn\u00fcgte Diogenes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was keine wollust zeigt, kein grosser f\u00fcrst besitzt,", "tokens": ["Was", "kei\u00b7ne", "wol\u00b7lust", "zeigt", ",", "kein", "gros\u00b7ser", "f\u00fcrst", "be\u00b7sitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,", "PIAT", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mancher criticus zwar lang\u2019 auf dem Parnasse,", "tokens": ["Und", "man\u00b7cher", "cri\u00b7ti\u00b7cus", "zwar", "lang'", "auf", "dem", "Par\u00b7nas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NE", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch gantz vergebens sucht: Ja was kein geitz erschwitzt;", "tokens": ["Doch", "gantz", "ver\u00b7ge\u00b7bens", "sucht", ":", "Ja", "was", "kein", "geitz", "er\u00b7schwitzt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$.", "PTKANT", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Genie\u00dft Diogenes in einem engen fasse.", "tokens": ["Ge\u00b7nie\u00dft", "Dio\u00b7ge\u00b7nes", "in", "ei\u00b7nem", "en\u00b7gen", "fas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-