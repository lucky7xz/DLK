{"dta.poem.12859": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria aus einer frantz\u00f6sischen  opera.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Nachtigall! entfleuch doch nicht!", "tokens": ["O", "Nach\u00b7ti\u00b7gall", "!", "ent\u00b7fleuch", "doch", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVIMP", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df deine lieder sicher h\u00f6ren.", "tokens": ["La\u00df", "dei\u00b7ne", "lie\u00b7der", "si\u00b7cher", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich suche nicht die ruh zu st\u00f6ren,", "tokens": ["Ich", "su\u00b7che", "nicht", "die", "ruh", "zu", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die dir der freye wald verspricht:", "tokens": ["Die", "dir", "der", "frey\u00b7e", "wald", "ver\u00b7spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich komme nicht mit pfeil und bogen,", "tokens": ["Ich", "kom\u00b7me", "nicht", "mit", "pfeil", "und", "bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Leb\u2019 immer hin in freyheit, lust und schertz;", "tokens": ["Leb'", "im\u00b7mer", "hin", "in", "frey\u00b7heit", ",", "lust", "und", "schertz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "APPR", "NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich komme nur, und seuffz\u2019 ach! um mein armes", "tokens": ["Ich", "kom\u00b7me", "nur", ",", "und", "seuff\u00b7z'", "ach", "!", "um", "mein", "ar\u00b7mes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "XY", "XY", "$.", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "hertz,", "tokens": ["hertz", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Das mir, o tiefer schmertz!", "tokens": ["Das", "mir", ",", "o", "tie\u00b7fer", "schmertz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "FM", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die falsche lieb\u2019 entzogen.", "tokens": ["Die", "fal\u00b7sche", "lieb'", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}