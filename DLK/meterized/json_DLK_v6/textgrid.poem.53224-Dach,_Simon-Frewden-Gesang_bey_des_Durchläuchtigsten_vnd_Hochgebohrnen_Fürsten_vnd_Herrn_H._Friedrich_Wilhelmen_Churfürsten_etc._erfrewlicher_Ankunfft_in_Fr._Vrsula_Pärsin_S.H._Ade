{"textgrid.poem.53224": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Frewden-Gesang bey des Durchl\u00e4uchtigsten vnd Hochgebohrnen F\u00fcrsten vnd Herrn H. Friedrich Wilhelmen Churf\u00fcrsten etc. erfrewlicher Ankunfft in Fr. Vrsula P\u00e4rsin S.H. Aderbachen hinterlassenen Wittiben Garten", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wa\u00df kan ich noch erwarten?", "tokens": ["Wa\u00df", "kan", "ich", "noch", "er\u00b7war\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wa\u00df fehlet mir wol mehr,", "tokens": ["Wa\u00df", "feh\u00b7let", "mir", "wol", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun, du Held, meinem Garten", "tokens": ["Nun", ",", "du", "Held", ",", "mei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PPER", "NN", "$,", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Erzeigst die Gnad vnd Ehr?", "tokens": ["Er\u00b7zeigst", "die", "Gnad", "vnd", "Ehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd tr\u00e4gstu kein Bedencken", "tokens": ["Vnd", "tr\u00e4gs\u00b7tu", "kein", "Be\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dich, o du Licht der Zeit,", "tokens": ["Dich", ",", "o", "du", "Licht", "der", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So tieff herab zu sencken", "tokens": ["So", "tieff", "her\u00b7ab", "zu", "sen\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu dieser Niedrigheit?", "tokens": ["Zu", "die\u00b7ser", "Nied\u00b7rig\u00b7heit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Komm gn\u00e4digst eingefahren,", "tokens": ["Komm", "gn\u00e4\u00b7digst", "ein\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein thewrer F\u00fcrst vnd Herr!", "tokens": ["Mein", "thew\u00b7rer", "F\u00fcrst", "vnd", "Herr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie soll ich mich gebahren?", "tokens": ["Wie", "soll", "ich", "mich", "ge\u00b7bah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor hielte Jupiter", "tokens": ["Vor", "hiel\u00b7te", "Ju\u00b7pi\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "VVFIN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Es ihm f\u00fcr keine Schande", "tokens": ["Es", "ihm", "f\u00fcr", "kei\u00b7ne", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu gehn zur Baucis hin;", "tokens": ["Zu", "gehn", "zur", "Bau\u00b7cis", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du kompst, O Trost der Lande,", "tokens": ["Du", "kompst", ",", "O", "Trost", "der", "Lan\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu Deiner Dienerin.", "tokens": ["Zu", "Dei\u00b7ner", "Die\u00b7ne\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Lach, o du Glantz der Sonnen,", "tokens": ["Lach", ",", "o", "du", "Glantz", "der", "Son\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lach sch\u00f6ner al\u00df zuvor!", "tokens": ["Lach", "sch\u00f6\u00b7ner", "al\u00df", "zu\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm, Pregel, sanfft geronnen,", "tokens": ["Komm", ",", "Pre\u00b7gel", ",", "sanfft", "ge\u00b7ron\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heb ietzt dein Haupt empor,", "tokens": ["Heb", "ietzt", "dein", "Haupt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ein grosses Haupt der Erden", "tokens": ["Ein", "gros\u00b7ses", "Haupt", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Helden Glantz vnd Zier,", "tokens": ["Der", "Hel\u00b7den", "Glantz", "vnd", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Al\u00df war, ist vnd mag werden,", "tokens": ["Al\u00df", "war", ",", "ist", "vnd", "mag", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "VAFIN", "KON", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zeucht gn\u00e4digst ein bey mir.", "tokens": ["Zeucht", "gn\u00e4\u00b7digst", "ein", "bey", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr B\u00e4ume sampt den Zweigen,", "tokens": ["Ihr", "B\u00e4u\u00b7me", "sampt", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Blumen, Gra\u00df vnd Kraut,", "tokens": ["Ihr", "Blu\u00b7men", ",", "Gra\u00df", "vnd", "Kraut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr m\u00fcsst f\u00fcr ihm euch neigen,", "tokens": ["Ihr", "m\u00fcsst", "f\u00fcr", "ihm", "euch", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sich ietzt euch vertrawt;", "tokens": ["Der", "sich", "ietzt", "euch", "ver\u00b7trawt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "PPER", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Die wilde Linden m\u00fcssen", "tokens": ["Die", "wil\u00b7de", "Lin\u00b7den", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihm hohe Cedern seyn,", "tokens": ["Ihm", "ho\u00b7he", "Ce\u00b7dern", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Pregel sich ergiessen", "tokens": ["Der", "Pre\u00b7gel", "sich", "er\u00b7gies\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Honig, Milch vnd Wein.", "tokens": ["Mit", "Ho\u00b7nig", ",", "Milch", "vnd", "Wein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wa\u00df soll ich nun erweisen,", "tokens": ["Wa\u00df", "soll", "ich", "nun", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df deiner Pracht gefellt?", "tokens": ["Da\u00df", "dei\u00b7ner", "Pracht", "ge\u00b7fellt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit wa\u00df Getr\u00e4nck vnd Speisen", "tokens": ["Mit", "wa\u00df", "Ge\u00b7tr\u00e4nck", "vnd", "Spei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bewirt ich dich, o Held,", "tokens": ["Be\u00b7wirt", "ich", "dich", ",", "o", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "PPER", "PRF", "$,", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wa\u00df find ich dich zu stillen", "tokens": ["Wa\u00df", "find", "ich", "dich", "zu", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ohn Demuht, Pflicht vnd Schuld?", "tokens": ["Ohn", "De\u00b7muht", ",", "Pflicht", "vnd", "Schuld", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nimm auff den reinen Willen", "tokens": ["Nimm", "auff", "den", "rei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd habe nur Gedult!", "tokens": ["Vnd", "ha\u00b7be", "nur", "Ge\u00b7dult", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dich, Churf\u00fcrst, wil ich singen,", "tokens": ["Dich", ",", "Chur\u00b7f\u00fcrst", ",", "wil", "ich", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wil vor dem H\u00f6chsten stehn", "tokens": ["Wil", "vor", "dem", "H\u00f6chs\u00b7ten", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vnd Thr\u00e4nen vor ihn bringen", "tokens": ["Vnd", "Thr\u00e4\u00b7nen", "vor", "ihn", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur vmb dein Wolergehn;", "tokens": ["Nur", "vmb", "dein", "Wo\u00b7ler\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein werthes Hau\u00df wirdt gr\u00fcnen,", "tokens": ["Dein", "wert\u00b7hes", "Hau\u00df", "wirdt", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dir werden Fl\u00fc\u00df vnd St\u00e4dt", "tokens": ["Dir", "wer\u00b7den", "Fl\u00fc\u00df", "vnd", "St\u00e4dt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd L\u00e4nder ewig dienen,", "tokens": ["Vnd", "L\u00e4n\u00b7der", "e\u00b7wig", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Herr, auch durch mein Gebeht.", "tokens": ["Herr", ",", "auch", "durch", "mein", "Ge\u00b7beht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nur la\u00df auch ferner scheinen", "tokens": ["Nur", "la\u00df", "auch", "fer\u00b7ner", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach! einer Witwen nur", "tokens": ["Ach", "!", "ei\u00b7ner", "Wit\u00b7wen", "nur"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd allen lieben meinen", "tokens": ["Vnd", "al\u00b7len", "lie\u00b7ben", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonne f\u00fcr vnd f\u00fcr!", "tokens": ["Die", "Son\u00b7ne", "f\u00fcr", "vnd", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Leid, Herr, nach GnadenSinnen", "tokens": ["Leid", ",", "Herr", ",", "nach", "Gna\u00b7den", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd deiner V\u00e4ter Brauch", "tokens": ["Vnd", "dei\u00b7ner", "V\u00e4\u00b7ter", "Brauch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vmb deine Dienerinnen", "tokens": ["Vmb", "dei\u00b7ne", "Die\u00b7ne\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Adersbachin auch.", "tokens": ["Die", "A\u00b7ders\u00b7ba\u00b7chin", "auch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}