{"textgrid.poem.43044": {"metadata": {"author": {"name": "Jacoby, Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Allee entlang", "genre": "verse", "period": "N.A.", "pub_year": 1867, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Allee entlang", "tokens": ["Die", "Al\u00b7lee", "ent\u00b7lang"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPO"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Scholl herauf heller Trompetenklang", "tokens": ["Scholl", "her\u00b7auf", "hel\u00b7ler", "Trom\u00b7pe\u00b7ten\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und Trommeln und Pfeifen und Pauken und Zinken,", "tokens": ["Und", "Trom\u00b7meln", "und", "Pfei\u00b7fen", "und", "Pau\u00b7ken", "und", "Zin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und schon sah man Schilder und Speere blinken.", "tokens": ["Und", "schon", "sah", "man", "Schil\u00b7der", "und", "Spee\u00b7re", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "NN", "KON", "NN", "VVINF", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber siehe!", "tokens": ["A\u00b7ber", "sie\u00b7he", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVIMP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Da kamen von der Seite heran", "tokens": ["Da", "ka\u00b7men", "von", "der", "Sei\u00b7te", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mann f\u00fcr Mann,", "tokens": ["Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Des Handwerks zugleich und der Waffen Kenner,", "tokens": ["Des", "Hand\u00b7werks", "zu\u00b7gleich", "und", "der", "Waf\u00b7fen", "Ken\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Mit festem Schritt wie Eisenm\u00e4nner,", "tokens": ["Mit", "fes\u00b7tem", "Schritt", "wie", "Ei\u00b7sen\u00b7m\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Eine Schaar Gestalten frisch und k\u00fchn,", "tokens": ["Ei\u00b7ne", "Schaar", "Ge\u00b7stal\u00b7ten", "frisch", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Das war die Schutzwehr der Freistadt Berlin.", "tokens": ["Das", "war", "die", "Schutz\u00b7wehr", "der", "Frei\u00b7stadt", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Eine rothe Fahne rollt ihr F\u00fchrer aus,", "tokens": ["Ei\u00b7ne", "ro\u00b7the", "Fah\u00b7ne", "rollt", "ihr", "F\u00fch\u00b7rer", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Drei goldene Sterne blitzen heraus.", "tokens": ["Drei", "gol\u00b7de\u00b7ne", "Ster\u00b7ne", "blit\u00b7zen", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Zur Linken, wo die Werkm\u00e4nner sa\u00dfen,", "tokens": ["Zur", "Lin\u00b7ken", ",", "wo", "die", "Werk\u00b7m\u00e4n\u00b7ner", "sa\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Stellten sie sich auf l\u00e4ngs der Stra\u00dfen.", "tokens": ["Stell\u00b7ten", "sie", "sich", "auf", "l\u00e4ngs", "der", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPR", "ART", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.12": {"text": "Dort aber, wo der F\u00fchrer stand mit der Fahne,", "tokens": ["Dort", "a\u00b7ber", ",", "wo", "der", "F\u00fch\u00b7rer", "stand", "mit", "der", "Fah\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "War eine Erh\u00f6hung gebaut gleich einem Altane.", "tokens": ["War", "ei\u00b7ne", "Er\u00b7h\u00f6\u00b7hung", "ge\u00b7baut", "gleich", "ei\u00b7nem", "Al\u00b7ta\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und jetzt kam heran der Zug. \u2013", "tokens": ["Und", "jetzt", "kam", "he\u00b7ran", "der", "Zug", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da gab es zu schau'n und zu gaffen genug.", "tokens": ["Da", "gab", "es", "zu", "schau'n", "und", "zu", "gaf\u00b7fen", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Bald wimmelt es hier von Rittern und Knappen", "tokens": ["Bald", "wim\u00b7melt", "es", "hier", "von", "Rit\u00b7tern", "und", "Knap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit allerlei bunten, gestickten Wappen.", "tokens": ["Mit", "al\u00b7ler\u00b7lei", "bun\u00b7ten", ",", "ge\u00b7stick\u00b7ten", "Wap\u00b7pen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Auch die Pr\u00e4laten \u2013 die ritten auf Rappen \u2013", "tokens": ["Auch", "die", "Pr\u00e4\u00b7la\u00b7ten", "\u2013", "die", "rit\u00b7ten", "auf", "Rap\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ART", "VVFIN", "APPR", "NN", "$("], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In gar pr\u00e4chtigem Priestergewand", "tokens": ["In", "gar", "pr\u00e4ch\u00b7ti\u00b7gem", "Pries\u00b7ter\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Stellten sich vor der Trib\u00fcne Rand,", "tokens": ["Stell\u00b7ten", "sich", "vor", "der", "Tri\u00b7b\u00fc\u00b7ne", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Dort, wo sich der Rath und der Bischof befand.", "tokens": ["Dort", ",", "wo", "sich", "der", "Rath", "und", "der", "Bi\u00b7schof", "be\u00b7fand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PRF", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber auf stolzem Ro\u00df alsbald", "tokens": ["A\u00b7ber", "auf", "stol\u00b7zem", "Ro\u00df", "als\u00b7bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Erschien eine ragende Rittergestalt", "tokens": ["Er\u00b7schien", "ei\u00b7ne", "ra\u00b7gen\u00b7de", "Rit\u00b7ter\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mit d\u00fcsterm Antlitz und strengen Mienen,", "tokens": ["Mit", "d\u00fcs\u00b7term", "Ant\u00b7litz", "und", "stren\u00b7gen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ganz gepanzert in Eisenschienen.", "tokens": ["Ganz", "ge\u00b7pan\u00b7zert", "in", "Ei\u00b7sen\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Doch ihm zur Seite ritt eine Frauen,", "tokens": ["Doch", "ihm", "zur", "Sei\u00b7te", "ritt", "ei\u00b7ne", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wunderlieblich anzuschauen,", "tokens": ["Wun\u00b7der\u00b7lieb\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In aller Reize und Zier Vereinung", "tokens": ["In", "al\u00b7ler", "Rei\u00b7ze", "und", "Zier", "Ver\u00b7ei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "So sch\u00f6n wie eine M\u00e4rchenerscheinung.", "tokens": ["So", "sch\u00f6n", "wie", "ei\u00b7ne", "M\u00e4r\u00b7che\u00b7ner\u00b7schei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und wie die Menge sie sah,", "tokens": ["Und", "wie", "die", "Men\u00b7ge", "sie", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Da", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Jauchzten Alle auf ringsum,", "tokens": ["Jauchz\u00b7ten", "Al\u00b7le", "auf", "ring\u00b7sum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPR", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und wu\u00dften eigentlich nicht warum. \u2013", "tokens": ["Und", "wu\u00df\u00b7ten", "ei\u00b7gent\u00b7lich", "nicht", "wa\u00b7rum", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PWAV", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie sie nun allesammt hielten in dem Rondell,", "tokens": ["Wie", "sie", "nun", "al\u00b7le\u00b7sammt", "hiel\u00b7ten", "in", "dem", "Ron\u00b7dell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Da trat in schlichtem Kleid ein Gesell", "tokens": ["Da", "trat", "in", "schlich\u00b7tem", "Kleid", "ein", "Ge\u00b7sell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Aus der Menge vor", "tokens": ["Aus", "der", "Men\u00b7ge", "vor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und stieg auf den Altan empor.", "tokens": ["Und", "stieg", "auf", "den", "Al\u00b7tan", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und mit einmal war", "tokens": ["Und", "mit", "ein\u00b7mal", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Alles still rings so wunderbar,", "tokens": ["Al\u00b7les", "still", "rings", "so", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADV", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Als ob auch alle B\u00e4ume sich neigen", "tokens": ["Als", "ob", "auch", "al\u00b7le", "B\u00e4u\u00b7me", "sich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ADV", "PIAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und lauschen wollten mit ihren Zweigen.", "tokens": ["Und", "lau\u00b7schen", "woll\u00b7ten", "mit", "ih\u00b7ren", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ein Sonnenstrahl aus wolkiger H\u00f6h'", "tokens": ["Ein", "Son\u00b7nen\u00b7strahl", "aus", "wol\u00b7ki\u00b7ger", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Blitzte hernieder auf den Schnee.", "tokens": ["Blitz\u00b7te", "her\u00b7nie\u00b7der", "auf", "den", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da ward von goldigem Glanz umhaucht", "tokens": ["Da", "ward", "von", "gol\u00b7di\u00b7gem", "Glanz", "um\u00b7haucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein bleiches Gesicht in Licht getaucht.", "tokens": ["Sein", "blei\u00b7ches", "Ge\u00b7sicht", "in", "Licht", "ge\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie die Sonne nun das Gew\u00f6lk durchbrach.", "tokens": ["Wie", "die", "Son\u00b7ne", "nun", "das", "Ge\u00b7w\u00f6lk", "durch\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und er sprach,", "tokens": ["Und", "er", "sprach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Seine Stimme die Menschen ganz durchdrang,", "tokens": ["Sei\u00b7ne", "Stim\u00b7me", "die", "Men\u00b7schen", "ganz", "durch\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Und Alles athmete auf tief bang", "tokens": ["Und", "Al\u00b7les", "ath\u00b7me\u00b7te", "auf", "tief", "bang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADJD", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Bei dem noch nie geh\u00f6rten Klang:", "tokens": ["Bei", "dem", "noch", "nie", "ge\u00b7h\u00f6r\u00b7ten", "Klang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein freier Bote steh' ich hier und Herold einer freien Stadt,", "tokens": ["Ein", "frei\u00b7er", "Bo\u00b7te", "steh'", "ich", "hier", "und", "He\u00b7rold", "ei\u00b7ner", "frei\u00b7en", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und eine Botschaft k\u00fcnd' ich dir, die mir ein Gott gegeben hat.", "tokens": ["Und", "ei\u00b7ne", "Bot\u00b7schaft", "k\u00fcnd'", "ich", "dir", ",", "die", "mir", "ein", "Gott", "ge\u00b7ge\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Zum erstenmal geschieht es heut, auf dieser Erd' zum ersten Mal,", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "ge\u00b7schieht", "es", "heut", ",", "auf", "die\u00b7ser", "Erd'", "zum", "ers\u00b7ten", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "PDAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Da\u00df sich der Mensch auf Erden wird bewu\u00dft der ganzen Menschheit Qual,", "tokens": ["Da\u00df", "sich", "der", "Mensch", "auf", "Er\u00b7den", "wird", "be\u00b7wu\u00dft", "der", "gan\u00b7zen", "Menschheit", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "wor