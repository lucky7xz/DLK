."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Betrachte, nebst der Farben Lieblichkeit,", "tokens": ["Be\u00b7trach\u00b7te", ",", "nebst", "der", "Far\u00b7ben", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auch ihrer Bildung Unterscheid,", "tokens": ["Auch", "ih\u00b7rer", "Bil\u00b7dung", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da, wann verschiedene nur klein;", "tokens": ["Da", ",", "wann", "ver\u00b7schie\u00b7de\u00b7ne", "nur", "klein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dort andre fast so gro\u00df, als wie ein Thaler, seyn.", "tokens": ["Dort", "and\u00b7re", "fast", "so", "gro\u00df", ",", "als", "wie", "ein", "Tha\u00b7ler", ",", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "ADV", "ADJD", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Noch andre, sonderlich die gelben, zeigen", "tokens": ["Noch", "and\u00b7re", ",", "son\u00b7der\u00b7lich", "die", "gel\u00b7ben", ",", "zei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PIS", "$,", "ADJD", 