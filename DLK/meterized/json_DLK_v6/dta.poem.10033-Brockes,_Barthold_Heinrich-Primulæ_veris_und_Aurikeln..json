{"dta.poem.10033": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Primul\u00e6 veris  und Aurikeln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOTT, es ist durch Deine G\u00fcte", "tokens": ["Mein", "GoTT", ",", "es", "ist", "durch", "Dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der frohe Fr\u00fchling wieder da.", "tokens": ["Der", "fro\u00b7he", "Fr\u00fch\u00b7ling", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort steht die holde ", "tokens": ["Dort", "steht", "die", "hol\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sammt den Aurikelchen in voller Bl\u00fcthe.", "tokens": ["Sammt", "den", "Au\u00b7ri\u00b7kel\u00b7chen", "in", "vol\u00b7ler", "Bl\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "Ach gieb, da\u00df mein ger\u00fchrt Gem\u00fcthe", "tokens": ["Ach", "gieb", ",", "da\u00df", "mein", "ge\u00b7r\u00fchrt", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$,", "KOUS", "PPOSAT", "VVPP", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht minder fruchtbar, als die Erde,", "tokens": ["Nicht", "min\u00b7der", "frucht\u00b7bar", ",", "als", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu Deines Nahmens Ehren, werde!", "tokens": ["Zu", "Dei\u00b7nes", "Nah\u00b7mens", "Eh\u00b7ren", ",", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gieb, da\u00df mein Geist, wann ich mit Lust", "tokens": ["Gieb", ",", "da\u00df", "mein", "Geist", ",", "wann", "ich", "mit", "Lust"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPOSAT", "NN", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Des Fr\u00fchlings erste Pracht besinge,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "ers\u00b7te", "Pracht", "be\u00b7sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Aus einer Dir geweihten Brust", "tokens": ["Aus", "ei\u00b7ner", "Dir", "ge\u00b7weih\u00b7ten", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Erstlinge der Andacht bringe!", "tokens": ["Die", "Erst\u00b7lin\u00b7ge", "der", "An\u00b7dacht", "brin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Holdseligs Fr\u00fchlings-Kind, dein zart und sanftes", "tokens": ["Hold\u00b7se\u00b7ligs", "Fr\u00fch\u00b7lings\u00b7Kind", ",", "dein", "zart", "und", "sanf\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "ADJD", "KON", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hab\u2019 ich, zu Dessen Ruhm, der dich und alles macht,", "tokens": ["Hab'", "ich", ",", "zu", "Des\u00b7sen", "Ruhm", ",", "der", "dich", "und", "al\u00b7les", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu meiner Lieder Zweck und Vorwurff, ietzt erlesen,", "tokens": ["Zu", "mei\u00b7ner", "Lie\u00b7der", "Zweck", "und", "Vor\u00b7wurff", ",", "ietzt", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein sch\u00f6nes Kleid, und deine bunte Pracht", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Kleid", ",", "und", "dei\u00b7ne", "bun\u00b7te", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kann, nach der langen Winter-Nacht,", "tokens": ["Kann", ",", "nach", "der", "lan\u00b7gen", "Win\u00b7ter\u00b7Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus deinen kleinen gr\u00fcnen B\u00fcschen,", "tokens": ["Aus", "dei\u00b7nen", "klei\u00b7nen", "gr\u00fc\u00b7nen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die dich, wie du sie zierest, zieren;", "tokens": ["Die", "dich", ",", "wie", "du", "sie", "zie\u00b7rest", ",", "zie\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht nur ein achtsam Auge r\u00fchren,", "tokens": ["Nicht", "nur", "ein", "acht\u00b7sam", "Au\u00b7ge", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auch selber unsern Geist erfrischen.", "tokens": ["Auch", "sel\u00b7ber", "un\u00b7sern", "Geist", "er\u00b7fri\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dein f\u00fcnffach Hertzen-f\u00f6rmig Blat,", "tokens": ["Dein", "f\u00fcnf\u00b7fach", "Hert\u00b7zen\u00b7f\u00f6r\u00b7mig", "Blat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das, recht im Mittel-Punct, ein Sonnen-Bildchen hat,", "tokens": ["Das", ",", "recht", "im", "Mit\u00b7tel\u00b7Punct", ",", "ein", "Son\u00b7nen\u00b7Bild\u00b7chen", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "APPRART", "NN", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Macht, da\u00df ich auf mein Hertz auch die Gedancken lencke,", "tokens": ["Macht", ",", "da\u00df", "ich", "auf", "mein", "Hertz", "auch", "die", "Ge\u00b7dan\u00b7cken", "len\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Und w\u00fcnsche, da\u00df auch mir sich stets ins Hertze sencke", "tokens": ["Und", "w\u00fcn\u00b7sche", ",", "da\u00df", "auch", "mir", "sich", "stets", "ins", "Hert\u00b7ze", "sen\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "PPER", "PRF", "ADV", "APPRART", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein Strahl von jener ew\u2019gen Sonne;", "tokens": ["Ein", "Strahl", "von", "je\u00b7ner", "ew'\u00b7gen", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So w\u00fcrd\u2019 es meiner frohen Seelen", "tokens": ["So", "w\u00fcrd'", "es", "mei\u00b7ner", "fro\u00b7hen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "So gar am Vorschmack ew\u2019ger Wonne,", "tokens": ["So", "gar", "am", "Vor\u00b7schmack", "ew'\u00b7ger", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Schon hier auf dieser Welt, nicht fehlen.", "tokens": ["Schon", "hier", "auf", "die\u00b7ser", "Welt", ",", "nicht", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Dein breit und krauses Laub gleicht einer Zungen,", "tokens": ["Dein", "breit", "und", "krau\u00b7ses", "Laub", "gleicht", "ei\u00b7ner", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und spricht, mehr als man meint, zu ihres Sch\u00f6pffers", "tokens": ["Und", "spricht", ",", "mehr", "als", "man", "meint", ",", "zu", "ih\u00b7res", "Sch\u00f6pf\u00b7fers"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "KOUS", "PIS", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Er wird im sanften Thon durch sie besungen.", "tokens": ["Er", "wird", "im", "sanf\u00b7ten", "Thon", "durch", "sie", "be\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Mich deucht, da\u00df es mein Geist, durchs Auge deutlich h\u00f6re,", "tokens": ["Mich", "deucht", ",", "da\u00df", "es", "mein", "Geist", ",", "durchs", "Au\u00b7ge", "deut\u00b7lich", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie ihre Zierlichkeit, Gestalt, Krafft, Farb und Pracht", "tokens": ["Wie", "ih\u00b7re", "Zier\u00b7lich\u00b7keit", ",", "Ge\u00b7stalt", ",", "Krafft", ",", "Farb", "und", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Von Dessen Weisheit, Lieb\u2019 und Macht,", "tokens": ["Von", "Des\u00b7sen", "Weis\u00b7heit", ",", "Lieb'", "und", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der sie aus nichts hervor gebracht,", "tokens": ["Der", "sie", "aus", "nichts", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "In sanfter Harmonie erklinge,", "tokens": ["In", "sanf\u00b7ter", "Har\u00b7mo\u00b7nie", "er\u00b7klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und, Jhm zum wahren Ruhm, ein s\u00fcsses Lob-Lied singe.", "tokens": ["Und", ",", "Jhm", "zum", "wah\u00b7ren", "Ruhm", ",", "ein", "s\u00fcs\u00b7ses", "Lob\u00b7Lied", "sin\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was seh\u2019 ich, liebste Bluhm\u2019, in dir", "tokens": ["Was", "seh'", "ich", ",", "liebs\u00b7te", "Bluhm'", ",", "in", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr eine neue Zier?", "tokens": ["F\u00fcr", "ei\u00b7ne", "neu\u00b7e", "Zier", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die wehrt, da\u00df wir darauf so Geist als Blicke lencken,", "tokens": ["Die", "wehrt", ",", "da\u00df", "wir", "da\u00b7rauf", "so", "Geist", "als", "Bli\u00b7cke", "len\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "PAV", "ADV", "NN", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df wir die Mannigfaltigkeit,", "tokens": ["Da\u00df", "wir", "die", "Man\u00b7nig\u00b7fal\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in den Bildungen die Meng\u2019 und Unterscheid", "tokens": ["Und", "in", "den", "Bil\u00b7dun\u00b7gen", "die", "Meng'", "und", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der spielenden Natur bedencken,", "tokens": ["Der", "spie\u00b7len\u00b7den", "Na\u00b7tur", "be\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als worin ein Geheimni\u00df liegt.", "tokens": ["Als", "wo\u00b7rin", "ein", "Ge\u00b7heim\u00b7ni\u00df", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es scheint die\u00df Bl\u00fchmchen nicht vergn\u00fcgt,", "tokens": ["Es", "scheint", "die\u00df", "Bl\u00fch\u00b7mchen", "nicht", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur einzeln GOttes Macht zu zeigen;", "tokens": ["Nur", "ein\u00b7zeln", "Got\u00b7tes", "Macht", "zu", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie l\u00e4sst, zu ihres Sch\u00f6pffers Preise,", "tokens": ["Sie", "l\u00e4sst", ",", "zu", "ih\u00b7res", "Sch\u00f6pf\u00b7fers", "Prei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf eine wunderbare Weise,", "tokens": ["Auf", "ei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aus einer ieden Bluhm\u2019 ein\u2019 andre Bluhme steigen.", "tokens": ["Aus", "ei\u00b7ner", "ie\u00b7den", "Bluhm'", "ein'", "and\u00b7re", "Bluh\u00b7me", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hierin ist ihr im Bluhmen-Reich", "tokens": ["Hie\u00b7rin", "ist", "ihr", "im", "Bluh\u00b7men\u00b7Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Fast keine Bluhme gleich.", "tokens": ["Fast", "kei\u00b7ne", "Bluh\u00b7me", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die\u00df, deucht mich, zeiget mir nicht nur,", "tokens": ["Die\u00df", ",", "deucht", "mich", ",", "zei\u00b7get", "mir", "nicht", "nur", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie unersch\u00f6pfflich die Natur", "tokens": ["Wie", "un\u00b7er\u00b7sch\u00f6pf\u00b7flich", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "An Bildung und Erfindung sey;", "tokens": ["An", "Bil\u00b7dung", "und", "Er\u00b7fin\u00b7dung", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es stellet mir zugleich dabey", "tokens": ["Es", "stel\u00b7let", "mir", "zu\u00b7gleich", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Bluhm\u2019 ein lehrend Sinn-Bild f\u00fcr", "tokens": ["Die", "Bluhm'", "ein", "leh\u00b7rend", "Sinn\u00b7Bild", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Von einer sch\u00f6nen Seel\u2019 in einem sch\u00f6nen Leibe;", "tokens": ["Von", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Seel'", "in", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Lei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und w\u00fcnsch\u2019 ich, da ich dieses schreibe,", "tokens": ["Und", "w\u00fcn\u00b7sch'", "ich", ",", "da", "ich", "die\u00b7ses", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Da\u00df diese Bluhm\u2019 auch meiner kleinen Heerde", "tokens": ["Da\u00df", "die\u00b7se", "Bluhm'", "auch", "mei\u00b7ner", "klei\u00b7nen", "Heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ein unverwelcklich Vorbild werde,", "tokens": ["Ein", "un\u00b7ver\u00b7welck\u00b7lich", "Vor\u00b7bild", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und stets ihr Lehr- und Sinn-Bild bleibe.", "tokens": ["Und", "stets", "ihr", "Lehr", "und", "Sinn\u00b7Bild", "blei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Allein, was lassen dort", "tokens": ["Al\u00b7lein", ",", "was", "las\u00b7sen", "dort"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir deine Schwersterchen f\u00fcr neue Wunder sehn?", "tokens": ["Mir", "dei\u00b7ne", "Schwers\u00b7ter\u00b7chen", "f\u00fcr", "neu\u00b7e", "Wun\u00b7der", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie angenehm gef\u00e4rbt, wie lieblich und wie sch\u00f6n", "tokens": ["Wie", "an\u00b7ge\u00b7nehm", "ge\u00b7f\u00e4rbt", ",", "wie", "lieb\u00b7lich", "und", "wie", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVPP", "$,", "PWAV", "ADJD", "KON", "PWAV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind die ", "tokens": ["Sind", "die"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Unglaublich ists, wie die Natur in ihnen", "tokens": ["Un\u00b7glaub\u00b7lich", "ists", ",", "wie", "die", "Na\u00b7tur", "in", "ih\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "PWAV", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So mannigfalt die Farben mischt und bricht.", "tokens": ["So", "man\u00b7nig\u00b7falt", "die", "Far\u00b7ben", "mischt", "und", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie tempert bald aus gelb-aus rothem und aus gr\u00fcnen,", "tokens": ["Sie", "tem\u00b7pert", "bald", "aus", "gelb\u00b7aus", "ro\u00b7them", "und", "aus", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "ADJA", "KON", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die sanfte Colorit. Bald l\u00e4sst sie, gr\u00fcn und braun,", "tokens": ["Die", "sanf\u00b7te", "Co\u00b7lo\u00b7rit", ".", "Bald", "l\u00e4sst", "sie", ",", "gr\u00fcn", "und", "braun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bald Purpur, Jsabell, bald r\u00f6thlich, und bald grau,", "tokens": ["Bald", "Pur\u00b7pur", ",", "Jsa\u00b7bell", ",", "bald", "r\u00f6th\u00b7lich", ",", "und", "bald", "grau", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NE", "$,", "ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Bald gelb und gr\u00fcn, bald gr\u00fcn und gelb, bald blau,", "tokens": ["Bald", "gelb", "und", "gr\u00fcn", ",", "bald", "gr\u00fcn", "und", "gelb", ",", "bald", "blau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mit dunckel-roth gebrochen, schaun.", "tokens": ["Mit", "dun\u00b7ckel\u00b7roth", "ge\u00b7bro\u00b7chen", ",", "schaun", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bald schm\u00fcckt die Mitte, bald die Ecken,", "tokens": ["Bald", "schm\u00fcckt", "die", "Mit\u00b7te", ",", "bald", "die", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ein g\u00fcldener, und bald ein Kreis,", "tokens": ["Ein", "g\u00fcl\u00b7de\u00b7ner", ",", "und", "bald", "ein", "Kreis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.14": {"text": "Der mehr als Silber-wei\u00df.", "tokens": ["Der", "mehr", "als", "Sil\u00b7ber\u00b7wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Man sieht (was sonderlich) meist einen weissen Staub", "tokens": ["Man", "sieht", "(", "was", "son\u00b7der\u00b7lich", ")", "meist", "ei\u00b7nen", "weis\u00b7sen", "Staub"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$(", "PWS", "ADJD", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Blumen, ja den Stengel, und da