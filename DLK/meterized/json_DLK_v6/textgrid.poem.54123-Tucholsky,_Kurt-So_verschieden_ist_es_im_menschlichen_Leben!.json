{"textgrid.poem.54123": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "So verschieden ist es im menschlichen Leben!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du eine reizende Damenbekanntschaft machen? Vergi\u00df, dich zu rasieren.", "tokens": ["Willst", "du", "ei\u00b7ne", "rei\u00b7zen\u00b7de", "Da\u00b7men\u00b7be\u00b7kannt\u00b7schaft", "ma\u00b7chen", "?", "Ver\u00b7gi\u00df", ",", "dich", "zu", "ra\u00b7sie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$.", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+--+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Apologetik der katholischen Kirche \u2013: das ist wie ein Luftschiff auf R\u00e4dern.", "tokens": ["Die", "A\u00b7po\u00b7lo\u00b7ge\u00b7tik", "der", "ka\u00b7tho\u00b7li\u00b7schen", "Kir\u00b7che", "\u2013", ":", "das", "ist", "wie", "ein", "Luft\u00b7schiff", "auf", "R\u00e4\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$(", "$.", "PDS", "VAFIN", "KOKOM", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+--+--+--+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Solch ein friedliches Land \u2013! Da tragen die Polizisten noch S\u00e4bel.", "tokens": ["Solch", "ein", "fried\u00b7li\u00b7ches", "Land", "\u2013", "!", "Da", "tra\u00b7gen", "die", "Po\u00b7li\u00b7zis\u00b7ten", "noch", "S\u00e4\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$(", "$.", "ADV", "VVFIN", "ART", "NN", "ADV", "NN", "$."], "meter": "--+--+-+--+-+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Sie sprach so viel, da\u00df ihre Zuh\u00f6rer davon heiser wurden.", "tokens": ["Sie", "sprach", "so", "viel", ",", "da\u00df", "ih\u00b7re", "Zu\u00b7h\u00f6\u00b7rer", "da\u00b7von", "hei\u00b7ser", "wur\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PAV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Nie geraten die Deutschen so au\u00dfer sich, wie wenn sie zu sich kommen wollen.", "tokens": ["Nie", "ge\u00b7ra\u00b7ten", "die", "Deut\u00b7schen", "so", "au\u00b7\u00dfer", "sich", ",", "wie", "wenn", "sie", "zu", "sich", "kom\u00b7men", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PRF", "$,", "KOKOM", "KOUS", "PPER", "APPR", "PRF", "VVINF", "VMINF", "$."], "meter": "+-+--+--+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Er besuchte alle Premieren \u2013 nicht aus Liebe zur Kunst, sondern um als erster Nein sagen zu k\u00f6nnen.", "tokens": ["Er", "be\u00b7such\u00b7te", "al\u00b7le", "Pre\u00b7mie\u00b7ren", "\u2013", "nicht", "aus", "Lie\u00b7be", "zur", "Kunst", ",", "son\u00b7dern", "um", "als", "ers\u00b7ter", "Nein", "sa\u00b7gen", "zu", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PTKNEG", "APPR", "NN", "APPRART", "NN", "$,", "KON", "APPR", "KOUS", "ADJA", "NN", "VVINF", "PTKZU", "VMINF", "$."], "meter": "--+-+--+-+-+--++-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Lungenhaschee . . . das sieht aus wie: \u00bbHaben Sie das gegessen, oder werden Sie das essen?\u00ab", "tokens": ["Lun\u00b7gen\u00b7ha\u00b7schee", ".", ".", ".", "das", "sieht", "aus", "wie", ":", "\u00bb", "Ha\u00b7ben", "Sie", "das", "ge\u00b7ges\u00b7sen", ",", "o\u00b7der", "wer\u00b7den", "Sie", "das", "es\u00b7sen", "?", "\u00ab"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "$.", "$.", "PDS", "VVFIN", "PTKVZ", "KOKOM", "$.", "$(", "VAFIN", "PPER", "PDS", "VVPP", "$,", "KON", "VAFIN", "PPER", "PDS", "VVINF", "$.", "$("], "meter": "+--+-++-+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.5": {"text": "Zwei Kriegsminister: Churchill kann Trotzki nur verh\u00f6hnen. Aber Trotzki kann Churchill mitdenken.", "tokens": ["Zwei", "Kriegs\u00b7mi\u00b7nis\u00b7ter", ":", "Chur\u00b7chill", "kann", "Trotz\u00b7ki", "nur", "ver\u00b7h\u00f6h\u00b7nen", ".", "A\u00b7ber", "Trotz\u00b7ki", "kann", "Chur\u00b7chill", "mit\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$.", "NE", "VMFIN", "NE", "ADV", "VVINF", "$.", "KON", "NE", "VMFIN", "NE", "VVINF", "$."], "meter": "-+-+-+--+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Gott schuf Kluge, Dumme, ganz Dumme und Gesch\u00e4ftsf\u00fchrer der SPD-Presse.", "tokens": ["Gott", "schuf", "Klu\u00b7ge", ",", "Dum\u00b7me", ",", "ganz", "Dum\u00b7me", "und", "Ge\u00b7sch\u00e4fts\u00b7f\u00fch\u00b7rer", "der", "SpD\u00b7Pres\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "NN", "$,", "ADV", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-+--+-+-", "measure": "amphibrach.tri.plus"}}}}}