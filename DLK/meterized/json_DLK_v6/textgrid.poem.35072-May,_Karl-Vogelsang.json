{"textgrid.poem.35072": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Vogelsang", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es klang ein Lied vom Himmelszelt", "tokens": ["Es", "klang", "ein", "Lied", "vom", "Him\u00b7mels\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hell \u00fcber allen Landen,", "tokens": ["Hell", "\u00fc\u00b7ber", "al\u00b7len", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch hat es in der weiten Welt", "tokens": ["Doch", "hat", "es", "in", "der", "wei\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl Niemand recht verstanden.", "tokens": ["Wohl", "Nie\u00b7mand", "recht", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur durch der V\u00f6glein lauschend Ohr", "tokens": ["Nur", "durch", "der", "V\u00f6\u00b7glein", "lau\u00b7schend", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist tiefer es gedrungen", "tokens": ["Ist", "tie\u00b7fer", "es", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wird seitdem als Fr\u00fchlingschor", "tokens": ["Und", "wird", "seit\u00b7dem", "als", "Fr\u00fch\u00b7ling\u00b7schor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PAV", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Feld und Wald gesungen.", "tokens": ["In", "Feld", "und", "Wald", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch wo man einen Menschen sieht", "tokens": ["Doch", "wo", "man", "ei\u00b7nen", "Men\u00b7schen", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Busch und Auen gehen,", "tokens": ["Durch", "Busch", "und", "Au\u00b7en", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So kann er leider dieses Lied", "tokens": ["So", "kann", "er", "lei\u00b7der", "die\u00b7ses", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch immer nicht verstehen.", "tokens": ["Noch", "im\u00b7mer", "nicht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}