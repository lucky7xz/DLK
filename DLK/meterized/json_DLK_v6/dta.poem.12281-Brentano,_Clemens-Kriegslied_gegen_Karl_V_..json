{"dta.poem.12281": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kriegslied gegen Karl   V .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es geht ein Butzemann im Reich herum,               ", "tokens": ["Es", "geht", "ein", "But\u00b7ze\u00b7mann", "im", "Reich", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Didum, Didum,", "tokens": ["Di\u00b7dum", ",", "Di\u00b7dum", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.3": {"text": "Bidi, Bidi, Bum!", "tokens": ["Bi\u00b7di", ",", "Bi\u00b7di", ",", "Bum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Der Kaiser schl\u00e4gt die Trum", "tokens": ["Der", "Kai\u00b7ser", "schl\u00e4gt", "die", "Trum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit H\u00e4nden und mit F\u00fc\u00dfen,", "tokens": ["Mit", "H\u00e4n\u00b7den", "und", "mit", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit S\u00e4beln und mit Spie\u00dfen!", "tokens": ["Mit", "S\u00e4\u00b7beln", "und", "mit", "Spie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Didum, Didum, Didum.", "tokens": ["Di\u00b7dum", ",", "Di\u00b7dum", ",", "Di\u00b7dum", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ach Karle gro\u00dfm\u00e4chtiger Mann,", "tokens": ["Ach", "Kar\u00b7le", "gro\u00df\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wie hast ein Spiel gefangen an,", "tokens": ["Wie", "hast", "ein", "Spiel", "ge\u00b7fan\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ohn Noth in Teutschen Landen?", "tokens": ["Ohn", "Noth", "in", "Teut\u00b7schen", "Lan\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wollt Gott, du h\u00e4ttst es ba\u00df bedacht,", "tokens": ["Wollt", "Gott", ",", "du", "h\u00e4ttst", "es", "ba\u00df", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich solchs nicht unterstanden.", "tokens": ["Dich", "solchs", "nicht", "un\u00b7ter\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es geht ein Butzemann u. s. w.", "tokens": ["Es", "geht", "ein", "But\u00b7ze\u00b7mann", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "VVIMP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ach Karle sieh dich besser vor,", "tokens": ["Ach", "Kar\u00b7le", "sieh", "dich", "bes\u00b7ser", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bedenk den Feind vor deinem Thor,", "tokens": ["Be\u00b7denk", "den", "Feind", "vor", "dei\u00b7nem", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du zu Pabst Gefallen", "tokens": ["Wenn", "du", "zu", "Pabst", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Solch greulich Mord willst richten an,", "tokens": ["Solch", "greu\u00b7lich", "Mord", "willst", "rich\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "NN", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wovon die Land erschallen.", "tokens": ["Wo\u00b7von", "die", "Land", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es geht ein Butzemann u. s. w.", "tokens": ["Es", "geht", "ein", "But\u00b7ze\u00b7mann", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "VVIMP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ach denke an Pabst Hildebrandt,", "tokens": ["Ach", "den\u00b7ke", "an", "Pabst", "Hil\u00b7de\u00b7brandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er regte Krieg im Teutschen Land,", "tokens": ["Er", "reg\u00b7te", "Krieg", "im", "Teut\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kaiser zu vertreiben,", "tokens": ["Den", "Kai\u00b7ser", "zu", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und hetzte an viel F\u00fcrsten stark,", "tokens": ["Und", "hetz\u00b7te", "an", "viel", "F\u00fcrs\u00b7ten", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Bann mu\u00dft er stets bleiben.", "tokens": ["Im", "Bann", "mu\u00dft", "er", "stets", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es geht ein Butzemann u. s. w.", "tokens": ["Es", "geht", "ein", "But\u00b7ze\u00b7mann", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "VVIMP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Pabst zum Kaiser w\u00e4hlen lie\u00df,", "tokens": ["Der", "Pabst", "zum", "Kai\u00b7ser", "w\u00e4h\u00b7len", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein F\u00fcrsten Rudolph Kaiser hie\u00df,", "tokens": ["Ein", "F\u00fcrs\u00b7ten", "Ru\u00b7dolph", "Kai\u00b7ser", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kron th\u00e4t er ihm senden,", "tokens": ["Ein", "Kron", "th\u00e4t", "er", "ihm", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gebot den F\u00fcrsten allzugleich,", "tokens": ["Ge\u00b7bot", "den", "F\u00fcrs\u00b7ten", "all\u00b7zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Heinrich sich zu wenden.", "tokens": ["Von", "Hein\u00b7rich", "sich", "zu", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es geht ein Butzemann u. s. w.", "tokens": ["Es", "geht", "ein", "But\u00b7ze\u00b7mann", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "VVIMP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da ward vergossen gro\u00dfes Blut,", "tokens": ["Da", "ward", "ver\u00b7gos\u00b7sen", "gro\u00b7\u00dfes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als sich besch\u00fctzt der Kaiser gut,", "tokens": ["Als", "sich", "be\u00b7sch\u00fctzt", "der", "Kai\u00b7ser", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Rudolph hat verloren", "tokens": ["Und", "Ru\u00b7dolph", "hat", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Schlacht und seine rechte Hand,", "tokens": ["Die", "Schlacht", "und", "sei\u00b7ne", "rech\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit der er falsch geschworen.", "tokens": ["Mit", "der", "er", "falsch", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es geht ein Butzemann u. s. w.", "tokens": ["Es", "geht", "ein", "But\u00b7ze\u00b7mann", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "VVIMP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ach Hildebrand der feyert nicht,", "tokens": ["Ach", "Hil\u00b7de\u00b7brand", "der", "fe\u00b7yert", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ART", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Kaisers Sohn er auch anricht,", "tokens": ["Des", "Kai\u00b7sers", "Sohn", "er", "auch", "an\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Vater zu verjagen,", "tokens": ["Den", "Va\u00b7ter", "zu", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Reich darob zerrissen ward,", "tokens": ["Das", "Reich", "da\u00b7rob", "zer\u00b7ris\u00b7sen", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel edles Volk erschlagen.", "tokens": ["Viel", "ed\u00b7les", "Volk", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es geht ein Butzemann u. s. w.", "tokens": ["Es", "geht", "ein", "But\u00b7ze\u00b7mann", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "VVIMP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Kaiser mu\u00df vorm Papste stehn,", "tokens": ["Der", "Kai\u00b7ser", "mu\u00df", "vorm", "Paps\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im S\u00fcnderhemd ganz nackt im Schnee,", "tokens": ["Im", "S\u00fcn\u00b7der\u00b7hemd", "ganz", "nackt", "im", "Schnee", ","], "token_i