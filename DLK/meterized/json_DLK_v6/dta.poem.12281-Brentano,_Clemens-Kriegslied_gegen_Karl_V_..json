Teut\u00b7sche", "Macht", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Willst du f\u00fcr ihre B\u00fcberey,", "tokens": ["Willst", "du", "f\u00fcr", "ih\u00b7re", "B\u00fc\u00b7be\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch den Pantoffel k\u00fcssen?", "tokens": ["Noch", "den", "Pan\u00b7tof\u00b7fel", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.10": {"line.1": {"text": "Es geht ein Butzemann u. s. w.", "tokens": ["Es", "geht", "ein", "But\u00b7ze\u00b7mann", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "VVIMP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir haben auch auf unsrer Seit'", "tokens": ["Wir", "ha\u00b7ben", "auch", "auf", "uns\u00b7rer", "Seit'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein starken Held, der f\u00fcr uns streit,", "tokens": ["Ein", "star\u00b7ken", "Held", ",", "der", "f\u00fcr", "uns", "streit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Macht ist nicht seins Gleichen,", "tokens": ["Von", "Macht", "ist", "nicht", "seins", "Glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "w