{"dta.poem.18769": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Dass Suleika von Jussuff entz\u00fcckt war", "tokens": ["Dass", "Sul\u00b7ei\u00b7ka", "von", "Jus\u00b7suff", "ent\u00b7z\u00fcckt", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist keine Kunst,", "tokens": ["Ist", "kei\u00b7ne", "Kunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Er war jung, Jugend hat Gunst,", "tokens": ["Er", "war", "jung", ",", "Ju\u00b7gend", "hat", "Gunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Er war sch\u00f6n, sie sagen zum Entz\u00fccken,", "tokens": ["Er", "war", "sch\u00f6n", ",", "sie", "sa\u00b7gen", "zum", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sch\u00f6n war sie, konnten einander begl\u00fccken.", "tokens": ["Sch\u00f6n", "war", "sie", ",", "konn\u00b7ten", "ein\u00b7an\u00b7der", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "VMFIN", "PRF", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Aber dass du, die so lange mir erharrt war,", "tokens": ["A\u00b7ber", "dass", "du", ",", "die", "so", "lan\u00b7ge", "mir", "er\u00b7harrt", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "PRELS", "ADV", "ADV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Feurige Jugendblicke mir schickst,", "tokens": ["Feu\u00b7ri\u00b7ge", "Ju\u00b7gend\u00b7bli\u00b7cke", "mir", "schickst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Jetzt mich liebst, mich sp\u00e4ter begl\u00fcckst,", "tokens": ["Jetzt", "mich", "liebst", ",", "mich", "sp\u00e4\u00b7ter", "be\u00b7gl\u00fcckst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Das sollen meine Lieder preissen", "tokens": ["Das", "sol\u00b7len", "mei\u00b7ne", "Lie\u00b7der", "preis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sollst mir ewig Suleika heissen.", "tokens": ["Sollst", "mir", "e\u00b7wig", "Sul\u00b7ei\u00b7ka", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "NE", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}