{"textgrid.poem.38518": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Herbstgesang", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe! Rebenh\u00fcgel,", "tokens": ["Sie\u00b7he", "!", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo sich Trauben f\u00e4rben!", "tokens": ["Wo", "sich", "Trau\u00b7ben", "f\u00e4r\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Siehe! Fruchtgel\u00e4nder,", "tokens": ["Sie\u00b7he", "!", "Frucht\u00b7ge\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wo sich Aepfel r\u00f6then!", "tokens": ["Wo", "sich", "A\u00b7e\u00b7pfel", "r\u00f6\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Liber und Vertumnus", "tokens": ["Li\u00b7ber", "und", "Ver\u00b7tum\u00b7nus"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Kr\u00f6nen verbr\u00fcdert die Bundesfeier!", "tokens": ["Kr\u00f6\u00b7nen", "ver\u00b7br\u00fc\u00b7dert", "die", "Bun\u00b7des\u00b7fei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wo der Segensg\u00f6tter", "tokens": ["Wo", "der", "Se\u00b7gens\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mildes Auge l\u00e4chelt,", "tokens": ["Mil\u00b7des", "Au\u00b7ge", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schwellen Purpurbeeren,", "tokens": ["Schwel\u00b7len", "Pur\u00b7pur\u00b7bee\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Malt mit \u00e4therblauem", "tokens": ["Malt", "mit", "\u00e4t\u00b7her\u00b7blau\u00b7em"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dufte sich die Pflaume,", "tokens": ["Duf\u00b7te", "sich", "die", "Pflau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Malt sich der Apfel mit Rosenstreifen!", "tokens": ["Malt", "sich", "der", "Ap\u00b7fel", "mit", "Ro\u00b7sen\u00b7strei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Hoch auf Nektarbergen,", "tokens": ["Hoch", "auf", "Nekt\u00b7ar\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo der Winzerm\u00e4dchen", "tokens": ["Wo", "der", "Win\u00b7zer\u00b7m\u00e4d\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wonnelieder t\u00f6nen,", "tokens": ["Won\u00b7ne\u00b7lie\u00b7der", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Knarren Keltern selber", "tokens": ["Knar\u00b7ren", "Kel\u00b7tern", "sel\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Harmonien, und Jubel", "tokens": ["Har\u00b7mo\u00b7ni\u00b7en", ",", "und", "Ju\u00b7bel"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Brausen, o Rhein, deine Silberwogen!", "tokens": ["Brau\u00b7sen", ",", "o", "Rhein", ",", "dei\u00b7ne", "Sil\u00b7ber\u00b7wo\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Hier am gr\u00fcnumschilften", "tokens": ["Hier", "am", "gr\u00fc\u00b7num\u00b7schilf\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Felsenquell der Wiese", "tokens": ["Fel\u00b7sen\u00b7quell", "der", "Wie\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "D\u00e4mmern die geweihten", "tokens": ["D\u00e4m\u00b7mern", "die", "ge\u00b7weih\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schatten, wo des Jahres", "tokens": ["Schat\u00b7ten", ",", "wo", "des", "Jah\u00b7res"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Erste Blumen Laura", "tokens": ["Ers\u00b7te", "Blu\u00b7men", "Lau\u00b7ra"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Weihte den l\u00e4chelnden Huldg\u00f6ttinnen.", "tokens": ["Weih\u00b7te", "den", "l\u00e4\u00b7cheln\u00b7den", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Fleuch, o s\u00fc\u00dfes M\u00e4dchen,", "tokens": ["Fleuch", ",", "o", "s\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fleuch der Stadt Gepr\u00e4nge!", "tokens": ["Fleuch", "der", "Stadt", "Ge\u00b7pr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Angeblinkt vom Abend,", "tokens": ["An\u00b7ge\u00b7blinkt", "vom", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "La\u00df des Jahres letzte", "tokens": ["La\u00df", "des", "Jah\u00b7res", "letz\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Blumen uns der hohen", "tokens": ["Blu\u00b7men", "uns", "der", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Venus-Urania dankend opfern!", "tokens": ["Ve\u00b7nus\u00b7U\u00b7ra\u00b7nia", "dan\u00b7kend", "op\u00b7fern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}