{"textgrid.poem.26723": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dreifach ist der Schritt der Zeit:", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dreifach ist der Schritt der Zeit:", "tokens": ["Drei\u00b7fach", "ist", "der", "Schritt", "der", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00f6gernd kommt die Zukunft hergezogen,", "tokens": ["Z\u00f6\u00b7gernd", "kommt", "die", "Zu\u00b7kunft", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Pfeilschnell ist das Jetzt entflogen,", "tokens": ["Pfeil\u00b7schnell", "ist", "das", "Jetzt", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PDS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig still steht die Vergangenheit.", "tokens": ["E\u00b7wig", "still", "steht", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Keine Ungeduld befl\u00fcgelt", "tokens": ["Kei\u00b7ne", "Un\u00b7ge\u00b7duld", "be\u00b7fl\u00fc\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren Schritt, wenn sie verweilt.", "tokens": ["Ih\u00b7ren", "Schritt", ",", "wenn", "sie", "ver\u00b7weilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Furcht, kein Zweifeln z\u00fcgelt", "tokens": ["Kei\u00b7ne", "Furcht", ",", "kein", "Zwei\u00b7feln", "z\u00fc\u00b7gelt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren Lauf, wenn sie enteilt.", "tokens": ["Ih\u00b7ren", "Lauf", ",", "wenn", "sie", "ent\u00b7eilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine Reu, kein Zaubersegen", "tokens": ["Kei\u00b7ne", "Reu", ",", "kein", "Zau\u00b7ber\u00b7se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NE", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann die Stehende bewegen.", "tokens": ["Kann", "die", "Ste\u00b7hen\u00b7de", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00f6chtest du begl\u00fcckt und weise", "tokens": ["M\u00f6ch\u00b7test", "du", "be\u00b7gl\u00fcckt", "und", "wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVPP", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endigen des Lebens Reise,", "tokens": ["En\u00b7di\u00b7gen", "des", "Le\u00b7bens", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm die Z\u00f6gernde zum Rat,", "tokens": ["Nimm", "die", "Z\u00f6\u00b7gern\u00b7de", "zum", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nicht zum Werkzeug deiner Tat.", "tokens": ["Nicht", "zum", "Werk\u00b7zeug", "dei\u00b7ner", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4hle nicht die Fliehende zum Freund,", "tokens": ["W\u00e4h\u00b7le", "nicht", "die", "Flie\u00b7hen\u00b7de", "zum", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Nicht die Bleibende zum Feind.", "tokens": ["Nicht", "die", "Blei\u00b7ben\u00b7de", "zum", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}