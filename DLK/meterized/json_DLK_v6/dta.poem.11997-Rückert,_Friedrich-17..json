{"dta.poem.11997": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du f\u00fchlest, da\u00df du hast auf Erden keine Rast,", "tokens": ["Du", "f\u00fch\u00b7lest", ",", "da\u00df", "du", "hast", "auf", "Er\u00b7den", "kei\u00b7ne", "Rast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nichts in Ruhe bleibt, sich alles treibt in Hast;", "tokens": ["Wo", "nichts", "in", "Ru\u00b7he", "bleibt", ",", "sich", "al\u00b7les", "treibt", "in", "Hast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "VVFIN", "$,", "PRF", "PIS", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo nichts in Ruhe bleibt, in Hast sich alles treibt;", "tokens": ["Wo", "nichts", "in", "Ru\u00b7he", "bleibt", ",", "in", "Hast", "sich", "al\u00b7les", "treibt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "VVFIN", "$,", "APPR", "NE", "PRF", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer ist, der hier ein Heil dem kranken Trieb verschreibt?", "tokens": ["Wer", "ist", ",", "der", "hier", "ein", "Heil", "dem", "kran\u00b7ken", "Trieb", "ver\u00b7schreibt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Heil, dem Heilung dankt das Herz, wenn es erkrankt", "tokens": ["Ein", "Heil", ",", "dem", "Hei\u00b7lung", "dankt", "das", "Herz", ",", "wenn", "es", "er\u00b7krankt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Schwanken dieser Welt, und mit ihr schwankt und wankt?", "tokens": ["Vom", "Schwan\u00b7ken", "die\u00b7ser", "Welt", ",", "und", "mit", "ihr", "schwankt", "und", "wankt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,", "KON", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Heil, das Unruh heilt, und das die Ruh ertheilt,", "tokens": ["Ein", "Heil", ",", "das", "Un\u00b7ruh", "heilt", ",", "und", "das", "die", "Ruh", "er\u00b7theilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "KON", "PDS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die in sich selber ruht, wo alles, alles eilt!", "tokens": ["Die", "in", "sich", "sel\u00b7ber", "ruht", ",", "wo", "al\u00b7les", ",", "al\u00b7les", "eilt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "VVFIN", "$,", "PWAV", "PIS", "$,", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Ruhe suchest du! wo findest du die Ruh?", "tokens": ["Die", "Ru\u00b7he", "su\u00b7chest", "du", "!", "wo", "fin\u00b7dest", "du", "die", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du dem Sturm dich ab, dich jenem kehrest zu,", "tokens": ["Wenn", "du", "dem", "Sturm", "dich", "ab", ",", "dich", "je\u00b7nem", "keh\u00b7rest", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "PTKVZ", "$,", "PRF", "PDAT", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Von dessen Hauch bewegt, der Sturm ist angeregt", "tokens": ["Von", "des\u00b7sen", "Hauch", "be\u00b7wegt", ",", "der", "Sturm", "ist", "an\u00b7ge\u00b7regt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Lebensmeeres, das sich nur im Hafen legt;", "tokens": ["Des", "Le\u00b7bens\u00b7mee\u00b7res", ",", "das", "sich", "nur", "im", "Ha\u00b7fen", "legt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Steuer auch und Mast, und Hafen ist und Rast;", "tokens": ["Der", "Steu\u00b7er", "auch", "und", "Mast", ",", "und", "Ha\u00b7fen", "ist", "und", "Rast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "NN", "$,", "KON", "NN", "VAFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Ruhe hast du, wo du ihn gefunden hast.", "tokens": ["Die", "Ru\u00b7he", "hast", "du", ",", "wo", "du", "ihn", "ge\u00b7fun\u00b7den", "hast", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wie dich der Wirbel tr\u00e4gt, wohin er dich verschl\u00e4gt,", "tokens": ["Wie", "dich", "der", "Wir\u00b7bel", "tr\u00e4gt", ",", "wo\u00b7hin", "er", "dich", "ver\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du f\u00fchlest ruhig dich im Gleichgewicht gew\u00e4gt.", "tokens": ["Du", "f\u00fch\u00b7lest", "ru\u00b7hig", "dich", "im", "Gleich\u00b7ge\u00b7wicht", "ge\u00b7w\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Du siehst in jedem Ding, ob wichtig ob gering,", "tokens": ["Du", "siehst", "in", "je\u00b7dem", "Ding", ",", "ob", "wich\u00b7tig", "ob", "ge\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "KOUS", "ADJD", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur das wodurch auch es ist von der Kett' ein Ring.", "tokens": ["Nur", "das", "wo\u00b7durch", "auch", "es", "ist", "von", "der", "Kett'", "ein", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PWAV", "ADV", "PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Dann siehst du kleines gro\u00df, und stolzes nackt und blo\u00df,", "tokens": ["Dann", "siehst", "du", "klei\u00b7nes", "gro\u00df", ",", "und", "stol\u00b7zes", "nackt", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "ADJD", "$,", "KON", "ADJA", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle Kinder gleich im Einen Mutterscho\u00df.", "tokens": ["Und", "al\u00b7le", "Kin\u00b7der", "gleich", "im", "Ei\u00b7nen", "Mut\u00b7ter\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPRART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Willst du im Einen seyn, kehr in dir Einem ein;", "tokens": ["Willst", "du", "im", "Ei\u00b7nen", "seyn", ",", "kehr", "in", "dir", "Ei\u00b7nem", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ART", "VAINF", "$,", "ADV", "APPR", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ein und All ist wo allein du bist allein.", "tokens": ["Das", "Ein", "und", "All", "ist", "wo", "al\u00b7lein", "du", "bist", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "PWAV", "ADV", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Das wirrt nur und zerstreut, was Zeit und Raum dir beut;", "tokens": ["Das", "wirrt", "nur", "und", "zer\u00b7streut", ",", "was", "Zeit", "und", "Raum", "dir", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KON", "VVPP", "$,", "PWS", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur das erfreut, was sich als ew'ges Heut erneut.", "tokens": ["Nur", "das", "er\u00b7freut", ",", "was", "sich", "als", "ew'\u00b7ges", "Heut", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "$,", "PRELS", "PRF", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}