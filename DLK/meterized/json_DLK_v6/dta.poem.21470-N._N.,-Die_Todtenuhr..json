{"dta.poem.21470": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Die Todtenuhr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Bei wintern\u00e4cht\u2019ger Stille", "tokens": ["Bei", "win\u00b7ter\u00b7n\u00e4cht'\u00b7ger", "Stil\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im lampenhellen Zimmer", "tokens": ["Im", "lam\u00b7pen\u00b7hel\u00b7len", "Zim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lag ich in tiefen Tr\u00e4umen", "tokens": ["Lag", "ich", "in", "tie\u00b7fen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf gr\u00fcnem Pf\u00fchl \u2014 wie immer!", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Pf\u00fchl", "wie", "im\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KOKOM", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dazwischen pochte nur", "tokens": ["Da\u00b7zwi\u00b7schen", "poch\u00b7te", "nur"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So geisterhaft die Todtenuhr.", "tokens": ["So", "geis\u00b7ter\u00b7haft", "die", "Tod\u00b7te\u00b7nuhr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und vor mir stand bezaubernd", "tokens": ["Und", "vor", "mir", "stand", "be\u00b7zau\u00b7bernd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus alter Zeit die Sch\u00f6ne;", "tokens": ["Aus", "al\u00b7ter", "Zeit", "die", "Sch\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die stummen Augen sprachen", "tokens": ["Die", "stum\u00b7men", "Au\u00b7gen", "spra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie sel\u2019ge Liebest\u00f6ne \u2026", "tokens": ["Wie", "sel'\u00b7ge", "Lie\u00b7be\u00b7st\u00f6\u00b7ne", "\u2026"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann h\u00f6rt\u2019 ich wieder nur", "tokens": ["Dann", "h\u00f6rt'", "ich", "wie\u00b7der", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So geisterhaft die Todtenuhr.", "tokens": ["So", "geis\u00b7ter\u00b7haft", "die", "Tod\u00b7te\u00b7nuhr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es kam des Freundes Schatten", "tokens": ["Es", "kam", "des", "Freun\u00b7des", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit todesbleichem Munde;", "tokens": ["Mit", "to\u00b7des\u00b7blei\u00b7chem", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er zeigte traurig l\u00e4chelnd", "tokens": ["Er", "zeig\u00b7te", "trau\u00b7rig", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir seines Herzens Wunde \u2026", "tokens": ["Mir", "sei\u00b7nes", "Her\u00b7zens", "Wun\u00b7de", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wieder h\u00f6rt\u2019 ich nur", "tokens": ["Und", "wie\u00b7der", "h\u00f6rt'", "ich", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So geisterhaft die Todtenuhr.", "tokens": ["So", "geis\u00b7ter\u00b7haft", "die", "Tod\u00b7te\u00b7nuhr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}