{"dta.poem.7437": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ufnau! Hier modert ", "tokens": ["Uf\u00b7nau", "!", "Hier", "mo\u00b7dert"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "F\u00fcr's deutsche Volk an's Kreuz geschlagen;", "tokens": ["F\u00fcr's", "deut\u00b7sche", "Volk", "an's", "Kreuz", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein deutsches Mekka w\u00e4r' di\u00df Eiland,", "tokens": ["Ein", "deut\u00b7sches", "Mek\u00b7ka", "w\u00e4r'", "di\u00df", "Ei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4tt' Ihn kein deutsches Weib getragen.", "tokens": ["H\u00e4tt'", "Ihn", "kein", "deut\u00b7sches", "Weib", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Hutten ist's, und Ihn erk\u00fcr' ich", "tokens": ["Der", "Hut\u00b7ten", "ist's", ",", "und", "Ihn", "er\u00b7k\u00fcr'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "KON", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meines Herzens erstem Helden;", "tokens": ["Zu", "mei\u00b7nes", "Her\u00b7zens", "ers\u00b7tem", "Hel\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Weltmeer sei Dein See, o Z\u00fcrich!", "tokens": ["Mein", "Welt\u00b7meer", "sei", "Dein", "See", ",", "o", "Z\u00fc\u00b7rich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von seinen M\u00e4hren la\u00dft mich melden.", "tokens": ["Von", "sei\u00b7nen", "M\u00e4h\u00b7ren", "la\u00dft", "mich", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Hutten ist's, ob den Despoten", "tokens": ["Der", "Hut\u00b7ten", "ist's", ",", "ob", "den", "Des\u00b7po\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verachtet Ihr des Volkes Vesten;", "tokens": ["Ver\u00b7ach\u00b7tet", "Ihr", "des", "Vol\u00b7kes", "Ves\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr buhlet t\u00e4glich mit den Toten,", "tokens": ["Ihr", "buh\u00b7let", "t\u00e4g\u00b7lich", "mit", "den", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! und vergesset Eure Besten.", "tokens": ["Ach", "!", "und", "ver\u00b7ges\u00b7set", "Eu\u00b7re", "Bes\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr weintet jener Hieroglife", "tokens": ["Ihr", "wein\u00b7tet", "je\u00b7ner", "Hie\u00b7rog\u00b7li\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Ocean manch verlorne Thr\u00e4ne,", "tokens": ["Im", "O\u00b7cean", "manch", "ver\u00b7lor\u00b7ne", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ahntet nicht die Wundertiefe", "tokens": ["Und", "ahn\u00b7tet", "nicht", "die", "Wun\u00b7der\u00b7tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der reinen deutschen Hippokrene.", "tokens": ["Der", "rei\u00b7nen", "deut\u00b7schen", "Hip\u00b7po\u00b7kre\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Heran zum H\u00fcgel des Verbannten!", "tokens": ["He\u00b7ran", "zum", "H\u00fc\u00b7gel", "des", "Ver\u00b7bann\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und lernt ihn kennen, den Verbannten!", "tokens": ["Und", "lernt", "ihn", "ken\u00b7nen", ",", "den", "Ver\u00b7bann\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Freiheit schwanket zwischen Klippen", "tokens": ["Die", "Frei\u00b7heit", "schwan\u00b7ket", "zwi\u00b7schen", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umher auf steuerlosem Boote,", "tokens": ["Um\u00b7her", "auf", "steu\u00b7er\u00b7lo\u00b7sem", "Boo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schon nahn sich ihr mit ekeln Lippen", "tokens": ["Schon", "nahn", "sich", "ihr", "mit", "e\u00b7keln", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "PRF", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kusse die Ischariote.", "tokens": ["Zum", "Kus\u00b7se", "die", "I\u00b7scha\u00b7rio\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Wir brauchen einen gro\u00dfen Schatten,", "tokens": ["Wir", "brau\u00b7chen", "ei\u00b7nen", "gro\u00b7\u00dfen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "De\u00df Geist um unsre Waffen schwebe,", "tokens": ["De\u00df", "Geist", "um", "uns\u00b7re", "Waf\u00b7fen", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der, wenn im Kampfe wir ermatten,", "tokens": ["Der", ",", "wenn", "im", "Kamp\u00b7fe", "wir", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns Blut von seinem Blute gebe.", "tokens": ["Uns", "Blut", "von", "sei\u00b7nem", "Blu\u00b7te", "ge\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "O glaubet nicht, da\u00df ihr ihn f\u00e4ndet", "tokens": ["O", "glau\u00b7bet", "nicht", ",", "da\u00df", "ihr", "ihn", "f\u00e4n\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf jenem Fels im fernen Meere;", "tokens": ["Auf", "je\u00b7nem", "Fels", "im", "fer\u00b7nen", "Mee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie zitterte manch stolzer Gibel,", "tokens": ["Wie", "zit\u00b7ter\u00b7te", "manch", "stol\u00b7zer", "Gi\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als donnernd einst in b\u00f6ser Stunde,", "tokens": ["Als", "don\u00b7nernd", "einst", "in", "b\u00f6\u00b7ser", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich Schwerterklang zu Luthers Bibel,", "tokens": ["Gleich", "Schwer\u00b7ter\u00b7klang", "zu", "Lu\u00b7thers", "Bi\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wort erscholl aus ", "tokens": ["Das", "Wort", "er\u00b7scholl", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Das Wort, das, als die Welt geknechtet,", "tokens": ["Das", "Wort", ",", "das", ",", "als", "die", "Welt", "ge\u00b7knech\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als finstrer Wahn sie unterjochte,", "tokens": ["Als", "finst\u00b7rer", "Wahn", "sie", "un\u00b7ter\u00b7joch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00fchn f\u00fcr alle Welt gerechtet,", "tokens": ["So", "k\u00fchn", "f\u00fcr", "al\u00b7le", "Welt", "ge\u00b7rech\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So einsam an den Himmel pochte.", "tokens": ["So", "ein\u00b7sam", "an", "den", "Him\u00b7mel", "poch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Lie\u00df er sich von den Kutten meucheln,", "tokens": ["Lie\u00df", "er", "sich", "von", "den", "Kut\u00b7ten", "meu\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat er darum sterben m\u00fcssen,", "tokens": ["Und", "hat", "er", "da\u00b7rum", "ster\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df nun die Enkel sonder Heucheln", "tokens": ["Da\u00df", "nun", "die", "En\u00b7kel", "son\u00b7der", "Heu\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Mantel von Marengo k\u00fcssen?", "tokens": ["Den", "Man\u00b7tel", "von", "Ma\u00b7ren\u00b7go", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.12": {"line.1": {"text": "Wie lang mit Lorbern \u00fcbersch\u00fctten", "tokens": ["Wie", "lang", "mit", "Lor\u00b7bern", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt ihr die corsische Standarte?", "tokens": ["Wollt", "ihr", "die", "cor\u00b7si\u00b7sche", "Stand\u00b7ar\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}