{"textgrid.poem.26123": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Nacht will sich in laute Wasser einh\u00fcllen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du, wie drau\u00dfen im Regen die Wasser sich necken,", "tokens": ["H\u00f6rst", "du", ",", "wie", "drau\u00b7\u00dfen", "im", "Re\u00b7gen", "die", "Was\u00b7ser", "sich", "ne\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "APPRART", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Wie die Regeng\u00fcsse hinfallen in langen, lauten Strecken", "tokens": ["Wie", "die", "Re\u00b7gen\u00b7g\u00fcs\u00b7se", "hin\u00b7fal\u00b7len", "in", "lan\u00b7gen", ",", "lau\u00b7ten", "Stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+--+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Und \u00fcberlaufen \u00fcber die R\u00e4nder der Wolkenbecken,", "tokens": ["Und", "\u00fc\u00b7berl\u00b7au\u00b7fen", "\u00fc\u00b7ber", "die", "R\u00e4n\u00b7der", "der", "Wol\u00b7ken\u00b7be\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als soll mit Mann und Maus heut Nacht die Erde ersaufen.", "tokens": ["Als", "soll", "mit", "Mann", "und", "Maus", "heut", "Nacht", "die", "Er\u00b7de", "er\u00b7sau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "APPR", "NN", "KON", "NN", "ADV", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Es kann kaum der Regen vor st\u00fcrzender Eile noch schnaufen;", "tokens": ["Es", "kann", "kaum", "der", "Re\u00b7gen", "vor", "st\u00fcr\u00b7zen\u00b7der", "Ei\u00b7le", "noch", "schnau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.6": {"text": "Die Regengeister f\u00fcllen mit ihren Wasserleibern die Traufen.", "tokens": ["Die", "Re\u00b7gen\u00b7geis\u00b7ter", "f\u00fcl\u00b7len", "mit", "ih\u00b7ren", "Was\u00b7ser\u00b7lei\u00b7bern", "die", "Trau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Nacht will sich in laute Wasser einh\u00fcllen,", "tokens": ["Die", "Nacht", "will", "sich", "in", "lau\u00b7te", "Was\u00b7ser", "ein\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus dem Regen sie sich ein eigenes Liebeslied macht,", "tokens": ["Aus", "dem", "Re\u00b7gen", "sie", "sich", "ein", "ei\u00b7ge\u00b7nes", "Lie\u00b7bes\u00b7lied", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+----+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So wie ein Verlassener sein einsames Lachen lacht.", "tokens": ["So", "wie", "ein", "Ver\u00b7las\u00b7se\u00b7ner", "sein", "ein\u00b7sa\u00b7mes", "La\u00b7chen", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}}}}