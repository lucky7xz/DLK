{"textgrid.poem.39356": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O ich wei\u00df, beschr\u00e4nkt und nichtig", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O ich wei\u00df, beschr\u00e4nkt und nichtig", "tokens": ["O", "ich", "wei\u00df", ",", "be\u00b7schr\u00e4nkt", "und", "nich\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "$,", "VVFIN", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist des Menschen Sein und Thun;", "tokens": ["Ist", "des", "Men\u00b7schen", "Sein", "und", "Thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir schweifen in der Irre,", "tokens": ["Und", "wir", "schwei\u00b7fen", "in", "der", "Ir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wir finden im Gewirre", "tokens": ["Und", "wir", "fin\u00b7den", "im", "Ge\u00b7wir\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine St\u00e4tt', um auszuruhn.", "tokens": ["Kei\u00b7ne", "St\u00e4tt'", ",", "um", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUI", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Traum nur bist auch du und Schatten,", "tokens": ["Traum", "nur", "bist", "auch", "du", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ADV", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traum vom Schatten, s\u00fc\u00dfes Weib!", "tokens": ["Traum", "vom", "Schat\u00b7ten", ",", "s\u00fc\u00b7\u00dfes", "Weib", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Leiden, deine Wonnen,", "tokens": ["Dei\u00b7ne", "Lei\u00b7den", ",", "dei\u00b7ne", "Won\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wa\u00dferblasen gleich zerronnen,", "tokens": ["Wa\u00b7\u00dfer\u00b7bla\u00b7sen", "gleich", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind des Schicksals Zeitvertreib.", "tokens": ["Sind", "des", "Schick\u00b7sals", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber sprich: sind unsre Herzen", "tokens": ["A\u00b7ber", "sprich", ":", "sind", "uns\u00b7re", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch der Zeit, des Zufalls Spott?", "tokens": ["Auch", "der", "Zeit", ",", "des", "Zu\u00b7falls", "Spott", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwillt mein Busen nicht mit Beben", "tokens": ["Schwillt", "mein", "Bu\u00b7sen", "nicht", "mit", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir von selbstgeschaffnem Leben?", "tokens": ["Mir", "von", "selbst\u00b7ge\u00b7schaff\u00b7nem", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bin ich mir nicht selbst ein Gott?", "tokens": ["Bin", "ich", "mir", "nicht", "selbst", "ein", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freilich w\u00e4r's ein Spiel den G\u00f6ttern,", "tokens": ["Frei\u00b7lich", "w\u00e4r's", "ein", "Spiel", "den", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die\u00df, was allen Gram mir lohnt,", "tokens": ["Die\u00df", ",", "was", "al\u00b7len", "Gram", "mir", "lohnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mich trotzen hei\u00dft den Wettern,", "tokens": ["Was", "mich", "trot\u00b7zen", "hei\u00dft", "den", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Herzen zu zerschmettern,", "tokens": ["Mit", "dem", "Her\u00b7zen", "zu", "zer\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo es stolz und muthig wohnt.", "tokens": ["Wo", "es", "stolz", "und", "mut\u00b7hig", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch so lang' es pocht, soll ringen", "tokens": ["Doch", "so", "lang'", "es", "pocht", ",", "soll", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "PPER", "VVFIN", "$,", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem H\u00f6chsten jeder Schlag.", "tokens": ["Nach", "dem", "H\u00f6chs\u00b7ten", "je\u00b7der", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinen heil'gen Kranz entbl\u00e4ttern,", "tokens": ["Mei\u00b7nen", "heil'\u00b7gen", "Kranz", "ent\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine G\u00f6ttin mir entg\u00f6ttern,", "tokens": ["Mei\u00b7ne", "G\u00f6t\u00b7tin", "mir", "ent\u00b7g\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche Macht, die das vermag?", "tokens": ["Wel\u00b7che", "Macht", ",", "die", "das", "ver\u00b7mag", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PRELS", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sind die\u00df Wirbel rascher Flammen?", "tokens": ["Sind", "die\u00df", "Wir\u00b7bel", "ra\u00b7scher", "Flam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Taumel wilder Leidenschaft?", "tokens": ["Tau\u00b7mel", "wil\u00b7der", "Lei\u00b7den\u00b7schaft", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, ich f\u00fchl' in diesem Streben", "tokens": ["Nein", ",", "ich", "f\u00fchl'", "in", "die\u00b7sem", "Stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Inniges, geheimes Leben,", "tokens": ["In\u00b7ni\u00b7ges", ",", "ge\u00b7hei\u00b7mes", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seelenw\u00fcrd' und Licht und Kraft.", "tokens": ["See\u00b7len\u00b7w\u00fcrd'", "und", "Licht", "und", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "K\u00f6nnte je die Glut erl\u00f6schen,", "tokens": ["K\u00f6nn\u00b7te", "je", "die", "Glut", "er\u00b7l\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die auf deinem Altar flammt,", "tokens": ["Die", "auf", "dei\u00b7nem", "Al\u00b7tar", "flammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6ttin, o! so la\u00df mich sterben,", "tokens": ["G\u00f6t\u00b7tin", ",", "o", "!", "so", "la\u00df", "mich", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "$.", "ADV", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mich s\u00fc\u00dfen Tod erwerben,", "tokens": ["La\u00df", "mich", "s\u00fc\u00b7\u00dfen", "Tod", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh' das Schicksal mich verdammt;", "tokens": ["Eh'", "das", "Schick\u00b7sal", "mich", "ver\u00b7dammt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mich verdammt zu \u00f6dem Leben,", "tokens": ["Mich", "ver\u00b7dammt", "zu", "\u00f6\u00b7dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das dem Tode langsam weicht,", "tokens": ["Das", "dem", "To\u00b7de", "lang\u00b7sam", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudenleer, in dumpfem Kummer,", "tokens": ["Freu\u00b7den\u00b7leer", ",", "in", "dum\u00b7pfem", "Kum\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4hrend sich des Grabes Schlummer", "tokens": ["W\u00e4h\u00b7rend", "sich", "des", "Gra\u00b7bes", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kalt durch Mark und Nerven schleicht.", "tokens": ["Kalt", "durch", "Mark", "und", "Ner\u00b7ven", "schleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00df vom Dasein mich genesen,", "tokens": ["La\u00df", "vom", "Da\u00b7sein", "mich", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Sanftes Weib, an deiner Brust.", "tokens": ["Sanf\u00b7tes", "Weib", ",", "an", "dei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wuth und Wonne wird mein Wesen", "tokens": ["Wuth", "und", "Won\u00b7ne", "wird", "mein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf im letzten Ku\u00dfe l\u00f6sen.", "tokens": ["Auf", "im", "letz\u00b7ten", "Ku\u00b7\u00dfe", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ha! willkommen, Todeslust!", "tokens": ["Ha", "!", "will\u00b7kom\u00b7men", ",", "To\u00b7des\u00b7lust", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}