{"dta.poem.5643": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es irret unser Geist nicht wenig bey den Sinnen.", "tokens": ["Es", "ir\u00b7ret", "un\u00b7ser", "Geist", "nicht", "we\u00b7nig", "bey", "den", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sollt\u2019 er die C\u00f6rper, die ihn r\u00fchren,", "tokens": ["Sollt'", "er", "die", "C\u00f6r\u00b7per", ",", "die", "ihn", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wol anders kennen und versp\u00fcren,", "tokens": ["Wol", "an\u00b7ders", "ken\u00b7nen", "und", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Als nur dadurch: Er denckt von innen?", "tokens": ["Als", "nur", "da\u00b7durch", ":", "Er", "denckt", "von", "in\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PAV", "$.", "PPER", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Selbst unser Leib wird durch die Seel erkannt.", "tokens": ["Selbst", "un\u00b7ser", "Leib", "wird", "durch", "die", "Seel", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie sieht in seinen Theil und Z\u00fcgen", "tokens": ["Sie", "sieht", "in", "sei\u00b7nen", "Theil", "und", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein wunderbares Werck: Sie sp\u00fcrt, wenn sie erweg\u2019t,", "tokens": ["Ein", "wun\u00b7der\u00b7ba\u00b7res", "Werck", ":", "Sie", "sp\u00fcrt", ",", "wenn", "sie", "er\u00b7weg't", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie mannichfach in ihm sich Elementen f\u00fcgen,", "tokens": ["Wie", "man\u00b7nich\u00b7fach", "in", "ihm", "sich", "E\u00b7le\u00b7men\u00b7ten", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PPER", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Welch zartes Kunst-Werck sich in ihm best\u00e4ndig reg\u2019t,", "tokens": ["Welch", "zar\u00b7tes", "Kunst\u00b7\u00b7Werck", "sich", "in", "ihm", "be\u00b7st\u00e4n\u00b7dig", "reg't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PRF", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wodurch er f\u00fchlet und empfindet.", "tokens": ["Wo\u00b7durch", "er", "f\u00fch\u00b7let", "und", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sie sieht, da\u00df er es sey, der durch so manches Band", "tokens": ["Sie", "sieht", ",", "da\u00df", "er", "es", "sey", ",", "der", "durch", "so", "man\u00b7ches", "Band"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$,", "PRELS", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Uns gantz mit der Natur verbindet.", "tokens": ["Uns", "gantz", "mit", "der", "Na\u00b7tur", "ver\u00b7bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Es dient zur Sinnlichkeit sein wunderbar Geb\u00e4ude.", "tokens": ["Es", "dient", "zur", "Sinn\u00b7lich\u00b7keit", "sein", "wun\u00b7der\u00b7bar", "Ge\u00b7b\u00e4u\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wir mercken die in ihm verborg\u2019nen N\u00e4der wol:", "tokens": ["Wir", "mer\u00b7cken", "die", "in", "ihm", "ver\u00b7bor\u00b7g'\u00b7nen", "N\u00e4\u00b7der", "wol", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "PPER", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.15": {"text": "Wir lieben ihn, er macht uns Freude:", "tokens": ["Wir", "lie\u00b7ben", "ihn", ",", "er", "macht", "uns", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wir wollen, da\u00df er dauren soll;", "tokens": ["Wir", "wol\u00b7len", ",", "da\u00df", "er", "dau\u00b7ren", "soll", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wir nennen eigentlich ihn unsern Leib; man spricht:", "tokens": ["Wir", "nen\u00b7nen", "ei\u00b7gent\u00b7lich", "ihn", "un\u00b7sern", "Leib", ";", "man", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "$.", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Hand, der Arm ist mein, auch das Gesicht.", "tokens": ["Die", "Hand", ",", "der", "Arm", "ist", "mein", ",", "auch", "das", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ich merck\u2019, ich werde selbst ger\u00fchret", "tokens": ["Ich", "merck", ",", "ich", "wer\u00b7de", "selbst", "ge\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Durch Aend\u2019rungen, die er versp\u00fcret.", "tokens": ["Durch", "A\u00b7en\u00b7d'\u00b7run\u00b7gen", ",", "die", "er", "ver\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Offt f\u00fchl\u2019 ich, da\u00df er meinen Willen", "tokens": ["Offt", "f\u00fchl'", "ich", ",", "da\u00df", "er", "mei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Geneigt ist zu erf\u00fcllen.", "tokens": ["Ge\u00b7neigt", "ist", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Offt herrschet er, offt der Verstand,", "tokens": ["Offt", "herr\u00b7schet", "er", ",", "offt", "der", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "Und durch ein recht geheimes Band", "tokens": ["Und", "durch", "ein", "recht", "ge\u00b7hei\u00b7mes", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wird, wann den einen etwas reget,", "tokens": ["Wird", ",", "wann", "den", "ei\u00b7nen", "et\u00b7was", "re\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "ART", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Zugleich der andere stets mit beweget.", "tokens": ["Zu\u00b7gleich", "der", "an\u00b7de\u00b7re", "stets", "mit", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADV", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}