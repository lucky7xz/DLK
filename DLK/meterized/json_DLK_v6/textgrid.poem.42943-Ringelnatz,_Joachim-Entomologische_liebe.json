{"textgrid.poem.42943": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Entomologische liebe", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein K\u00e4fer, den ich kenne,", "tokens": ["Ein", "K\u00e4\u00b7fer", ",", "den", "ich", "ken\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Goldhenne,", "tokens": ["Die", "Gold\u00b7hen\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Spritzt einen \u00fcblen Saft.", "tokens": ["Spritzt", "ei\u00b7nen", "\u00fcb\u00b7len", "Saft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich habe mir eine Betthenne \u2013", "tokens": ["Ich", "ha\u00b7be", "mir", "ei\u00b7ne", "Bet\u00b7then\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nein, Bettpfanne angeschafft.", "tokens": ["Nein", ",", "Bett\u00b7pfan\u00b7ne", "an\u00b7ge\u00b7schafft", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur zur eigenen Ben\u00fctzung,", "tokens": ["Nur", "zur", "ei\u00b7ge\u00b7nen", "Be\u00b7n\u00fct\u00b7zung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht etwa zur Unterst\u00fctzung", "tokens": ["Nicht", "et\u00b7wa", "zur", "Un\u00b7ter\u00b7st\u00fct\u00b7zung"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses K\u00e4fers, der bei Tag und Nacht", "tokens": ["Die\u00b7ses", "K\u00e4\u00b7fers", ",", "der", "bei", "Tag", "und", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Neben meinem Krankenlager steht", "tokens": ["Ne\u00b7ben", "mei\u00b7nem", "Kran\u00b7ken\u00b7la\u00b7ger", "steht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und sich freut, wenn es mir na\u00df ergeht.", "tokens": ["Und", "sich", "freut", ",", "wenn", "es", "mir", "na\u00df", "er\u00b7geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Eingefangen in ein Glasgeb\u00e4ude", "tokens": ["Ein\u00b7ge\u00b7fan\u00b7gen", "in", "ein", "Glas\u00b7ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lebt er. Ich verstehe seine Freude.", "tokens": ["Lebt", "er", ".", "Ich", "ver\u00b7ste\u00b7he", "sei\u00b7ne", "Freu\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn er nie in Freiheit bei mir sitzt,", "tokens": ["Wenn", "er", "nie", "in", "Frei\u00b7heit", "bei", "mir", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "So doch nur, weil er so \u00fcbel spritzt.", "tokens": ["So", "doch", "nur", ",", "weil", "er", "so", "\u00fc\u00b7bel", "spritzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Doch nachdem ich nun seit sieben Wochen", "tokens": ["Doch", "nach\u00b7dem", "ich", "nun", "seit", "sie\u00b7ben", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihm durchs Glas so freundlich zugesprochen,", "tokens": ["Ihm", "durchs", "Glas", "so", "freund\u00b7lich", "zu\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wei\u00df er schon, da\u00df ich ihn Goldfink nenne.", "tokens": ["Wei\u00df", "er", "schon", ",", "da\u00df", "ich", "ihn", "Gold\u00b7fink", "nen\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-++--", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Wir sind Schicksalskameraden.", "tokens": ["Wir", "sind", "Schick\u00b7sals\u00b7ka\u00b7me\u00b7ra\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Demn\u00e4chst will ich meine Goldhenne", "tokens": ["Dem\u00b7n\u00e4chst", "will", "ich", "mei\u00b7ne", "Gold\u00b7hen\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Zu Bettpfannkuchen einladen.", "tokens": ["Zu", "Bett\u00b7pfann\u00b7ku\u00b7chen", "ein\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}