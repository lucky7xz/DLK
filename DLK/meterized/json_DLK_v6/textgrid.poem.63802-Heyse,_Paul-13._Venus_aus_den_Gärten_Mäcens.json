{"textgrid.poem.63802": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Venus aus den G\u00e4rten M\u00e4cens", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Venus nannten sie dich. Nun schelten sie, da\u00df du zur G\u00f6ttin", "tokens": ["Ve\u00b7nus", "nann\u00b7ten", "sie", "dich", ".", "Nun", "schel\u00b7ten", "sie", ",", "da\u00df", "du", "zur", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PRF", "$.", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Doch nicht g\u00f6ttlich genug, irdisch vielmehr und gemein.", "tokens": ["Doch", "nicht", "g\u00f6tt\u00b7lich", "ge\u00b7nug", ",", "ir\u00b7disch", "viel\u00b7mehr", "und", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "ADV", "$,", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Sch\u00f6ne Natur, wie reich im Unvollkommnen begl\u00fcckst du!", "tokens": ["Sch\u00f6\u00b7ne", "Na\u00b7tur", ",", "wie", "reich", "im", "Un\u00b7voll\u00b7komm\u00b7nen", "be\u00b7gl\u00fcckst", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADJD", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Leer ausgehet nur der, der das Vollkommen sucht.", "tokens": ["Leer", "aus\u00b7ge\u00b7het", "nur", "der", ",", "der", "das", "Voll\u00b7kom\u00b7men", "sucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}