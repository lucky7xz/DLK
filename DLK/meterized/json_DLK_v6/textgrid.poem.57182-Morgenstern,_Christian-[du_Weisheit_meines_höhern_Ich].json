{"textgrid.poem.57182": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[du Weisheit meines h\u00f6hern Ich]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Weisheit meines h\u00f6hern Ich,", "tokens": ["Du", "Weis\u00b7heit", "mei\u00b7nes", "h\u00f6\u00b7hern", "Ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "ADJA", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die \u00fcber mir den Fittich spreitet", "tokens": ["die", "\u00fc\u00b7ber", "mir", "den", "Fit\u00b7tich", "sprei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und mich vom Anfang her geleitet,", "tokens": ["und", "mich", "vom", "An\u00b7fang", "her", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wie es am besten war f\u00fcr mich, \u2013", "tokens": ["wie", "es", "am", "bes\u00b7ten", "war", "f\u00fcr", "mich", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PTKA", "ADJD", "VAFIN", "APPR", "PPER", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn Unmut oft mich anfocht: nun \u2013", "tokens": ["Wenn", "Un\u00b7mut", "oft", "mich", "an\u00b7focht", ":", "nun", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPER", "VVFIN", "$.", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es war der Unmut eines Knaben!", "tokens": ["Es", "war", "der", "Un\u00b7mut", "ei\u00b7nes", "Kna\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Mannes reife Blicke haben", "tokens": ["Des", "Man\u00b7nes", "rei\u00b7fe", "Bli\u00b7cke", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Kraft, voll Dank auf Dir zu ruhn.", "tokens": ["die", "Kraft", ",", "voll", "Dank", "auf", "Dir", "zu", "ruhn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}