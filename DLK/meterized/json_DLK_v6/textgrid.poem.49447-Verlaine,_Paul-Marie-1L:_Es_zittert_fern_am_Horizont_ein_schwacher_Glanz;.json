{"textgrid.poem.49447": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es zittert fern am Horizont ein schwacher Glanz;", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es zittert fern am Horizont ein schwacher Glanz;", "tokens": ["Es", "zit\u00b7tert", "fern", "am", "Ho\u00b7ri\u00b7zont", "ein", "schwa\u00b7cher", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wind erhebt sich und erfrischt mit eis'gem Wehen", "tokens": ["Der", "Wind", "er\u00b7hebt", "sich", "und", "er\u00b7frischt", "mit", "eis'\u00b7gem", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Laub der W\u00e4lder und das Bl\u00fchn des Wiesenlands;", "tokens": ["Das", "Laub", "der", "W\u00e4l\u00b7der", "und", "das", "Bl\u00fchn", "des", "Wie\u00b7sen\u00b7lands", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Fr\u00fchrots kalter Gruss l\u00e4sst alles neu erstehen.", "tokens": ["Des", "Fr\u00fch\u00b7rots", "kal\u00b7ter", "Gruss", "l\u00e4sst", "al\u00b7les", "neu", "er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Osten fern ergl\u00fcht, bleich wird die Silberbahn", "tokens": ["Der", "Os\u00b7ten", "fern", "er\u00b7gl\u00fcht", ",", "bleich", "wird", "die", "Sil\u00b7ber\u00b7bahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sterne, die im Gold des \u00c4thers bald verborgen.", "tokens": ["Der", "Ster\u00b7ne", ",", "die", "im", "Gold", "des", "\u00c4\u00b7thers", "bald", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein froher W\u00e4chter ruft zur guten Zeit, der Hahn,", "tokens": ["Ein", "fro\u00b7her", "W\u00e4ch\u00b7ter", "ruft", "zur", "gu\u00b7ten", "Zeit", ",", "der", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hell schmetternd steigt die Lerche auf: das ist der Morgen.", "tokens": ["Hell", "schmet\u00b7ternd", "steigt", "die", "Ler\u00b7che", "auf", ":", "das", "ist", "der", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Sonne bricht im Glanz hervor: das ist der Tag,", "tokens": ["Die", "Son\u00b7ne", "bricht", "im", "Glanz", "her\u00b7vor", ":", "das", "ist", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tag, ihr Freunde, dessen freud'ger Strahl die Schleier", "tokens": ["Der", "Tag", ",", "ihr", "Freun\u00b7de", ",", "des\u00b7sen", "freu\u00b7d'\u00b7ger", "Strahl", "die", "Schlei\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "PRELAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vom dumpfen Schlummer hebt, dem unsre Schar erlag,", "tokens": ["Vom", "dum\u00b7pfen", "Schlum\u00b7mer", "hebt", ",", "dem", "uns\u00b7re", "Schar", "er\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der vom Frasse scheucht die W\u00f6lfe und die Geier.", "tokens": ["Und", "der", "vom", "Fras\u00b7se", "scheucht", "die", "W\u00f6l\u00b7fe", "und", "die", "Gei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wunderbar! Der Strahl, der durch den K\u00fcrass drang,", "tokens": ["Und", "wun\u00b7der\u00b7bar", "!", "Der", "Strahl", ",", "der", "durch", "den", "K\u00fc\u00b7rass", "drang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und durch das Erz ins Herz erneut die Kraft zum Hasse,", "tokens": ["Und", "durch", "das", "Erz", "ins", "Herz", "er\u00b7neut", "die", "Kraft", "zum", "Has\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die brennende Begier zu bessrem Untergang,", "tokens": ["Die", "bren\u00b7nen\u00b7de", "Be\u00b7gier", "zu", "bess\u00b7rem", "Un\u00b7ter\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den alten Zorn und Stolz von jeder edlen Rasse.", "tokens": ["Den", "al\u00b7ten", "Zorn", "und", "Stolz", "von", "je\u00b7der", "ed\u00b7len", "Ras\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Aufrecht voran! Aufrecht voran und vorw\u00e4rts! Mut!", "tokens": ["Auf\u00b7recht", "vo\u00b7ran", "!", "Auf\u00b7recht", "vo\u00b7ran", "und", "vor\u00b7w\u00e4rts", "!", "Mut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "NN", "PTKVZ", "KON", "ADV", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Genug des Zauderns, dem wir schmachvoll unterlegen,", "tokens": ["Ge\u00b7nug", "des", "Zau\u00b7derns", ",", "dem", "wir", "schmach\u00b7voll", "un\u00b7ter\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Kampf! Zum Kampf! Denn unser wildes, heisses Blut", "tokens": ["Zum", "Kampf", "!", "Zum", "Kampf", "!", "Denn", "un\u00b7ser", "wil\u00b7des", ",", "heis\u00b7ses", "Blut"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$.", "APPRART", "NN", "$.", "KON", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Soll rauchen auf der Schneide der gez\u00fcckten Degen!", "tokens": ["Soll", "rau\u00b7chen", "auf", "der", "Schnei\u00b7de", "der", "ge\u00b7z\u00fcck\u00b7ten", "De\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}