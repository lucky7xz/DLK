{"textgrid.poem.52029": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieviel dein d\u00fcrrer Mund der Gl\u00e4ser trucken macht,", "tokens": ["Wie\u00b7viel", "dein", "d\u00fcr\u00b7rer", "Mund", "der", "Gl\u00e4\u00b7ser", "tru\u00b7cken", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch h\u00f6rest du nicht auff, bi\u00df es beginnt zu tagen:", "tokens": ["Doch", "h\u00f6\u00b7rest", "du", "nicht", "auff", ",", "bi\u00df", "es", "be\u00b7ginnt", "zu", "ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Kein Boden knacket so, den Polter Geister schlagen,", "tokens": ["Kein", "Bo\u00b7den", "kna\u00b7cket", "so", ",", "den", "Pol\u00b7ter", "Geis\u00b7ter", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als der gedielte Saal dir untern F\u00fcssen kracht.", "tokens": ["Als", "der", "ge\u00b7diel\u00b7te", "Saal", "dir", "un\u00b7tern", "F\u00fcs\u00b7sen", "kracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bald, wenn ein schneller Zorn dir aus der Gallen f\u00e4hrt,", "tokens": ["Bald", ",", "wenn", "ein", "schnel\u00b7ler", "Zorn", "dir", "aus", "der", "Gal\u00b7len", "f\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So stehstu blanck und wilt den armen Offen st\u00fcrmen,", "tokens": ["So", "steh\u00b7stu", "blanck", "und", "wilt", "den", "ar\u00b7men", "Of\u00b7fen", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald, wann der Stock erwarmt von andern Bacchus W\u00fcrmen:", "tokens": ["Bald", ",", "wann", "der", "Stock", "er\u00b7warmt", "von", "an\u00b7dern", "Bac\u00b7chus", "W\u00fcr\u00b7men", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVPP", "APPR", "ADJA", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lauffstu Kammern auff, und holst was du begehrt.", "tokens": ["So", "lauffs\u00b7tu", "Kam\u00b7mern", "auff", ",", "und", "holst", "was", "du", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann k\u00fchlstu deinen Muth und schl\u00e4gst die Gl\u00e4ser todt,", "tokens": ["Dann", "k\u00fchl\u00b7stu", "dei\u00b7nen", "Muth", "und", "schl\u00e4gst", "die", "Gl\u00e4\u00b7ser", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcbergehst die Wand, da\u00df alle Fenster klingen:", "tokens": ["Und", "\u00fc\u00b7ber\u00b7gehst", "die", "Wand", ",", "da\u00df", "al\u00b7le", "Fens\u00b7ter", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann du die Feind erlegt, so f\u00e4ngstu an zu singen:", "tokens": ["Wann", "du", "die", "Feind", "er\u00b7legt", ",", "so", "f\u00e4ngs\u00b7tu", "an", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ruffest Gl\u00e4ser her: Ly\u00e6us ist mein Gott.", "tokens": ["Und", "ruf\u00b7fest", "Gl\u00e4\u00b7ser", "her", ":", "Ly\u00e6us", "ist", "mein", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$.", "NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nu dis hastu gelernt, als R\u00e6cus und sein Fahn", "tokens": ["Nu", "dis", "has\u00b7tu", "ge\u00b7lernt", ",", "als", "R\u00e6\u00b7cus", "und", "sein", "Fahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VAFIN", "VVPP", "$,", "KOUS", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Sturm im Himmel lieff: Da hastu helffen schlagen,", "tokens": ["Zu", "Sturm", "im", "Him\u00b7mel", "lieff", ":", "Da", "has\u00b7tu", "helf\u00b7fen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$.", "ADV", "VAFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum ist dir Bachus gut: und wie ich h\u00f6re sagen:", "tokens": ["Drum", "ist", "dir", "Ba\u00b7chus", "gut", ":", "und", "wie", "ich", "h\u00f6\u00b7re", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NE", "ADJD", "$.", "KON", "PWAV", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So h\u00e4ngt Silenus dir stets seinen Esel an.", "tokens": ["So", "h\u00e4ngt", "Si\u00b7le\u00b7nus", "dir", "stets", "sei\u00b7nen", "E\u00b7sel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}