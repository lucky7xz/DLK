{"textgrid.poem.52100": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann ich vom Felde komm, und etwan einem Hasen", "tokens": ["Wann", "ich", "vom", "Fel\u00b7de", "komm", ",", "und", "et\u00b7wan", "ei\u00b7nem", "Ha\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das durch beschwitzte Fell durch meinen Wind gezwagt,", "tokens": ["Das", "durch", "be\u00b7schwitz\u00b7te", "Fell", "durch", "mei\u00b7nen", "Wind", "ge\u00b7zwagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann ich dem grimmen Wolff ein Sch\u00e4fflein abgejagt,", "tokens": ["Wann", "ich", "dem", "grim\u00b7men", "Wolff", "ein", "Sch\u00e4f\u00b7flein", "ab\u00b7ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ticht' ich offte Vers, indem ich sol verblasen.", "tokens": ["So", "ticht'", "ich", "off\u00b7te", "Vers", ",", "in\u00b7dem", "ich", "sol", "ver\u00b7bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Offt, (es ist mein Gebrauch) wenn meine Leute dreschen,", "tokens": ["Offt", ",", "(", "es", "ist", "mein", "Ge\u00b7brauch", ")", "wenn", "mei\u00b7ne", "Leu\u00b7te", "dre\u00b7schen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$(", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mu\u00df mit dem Schiferbuch ein Junge bey mir stehn:", "tokens": ["Mu\u00df", "mit", "dem", "Schi\u00b7fer\u00b7buch", "ein", "Jun\u00b7ge", "bey", "mir", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn ich den Sch\u00e4ffer schelt' und hei\u00df ihn von mir gehn,", "tokens": ["Wenn", "ich", "den", "Sch\u00e4f\u00b7fer", "schelt'", "und", "hei\u00df", "ihn", "von", "mir", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "ADJD", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df den Zorn in mir ein Epigramma l\u00f6schen.", "tokens": ["So", "mu\u00df", "den", "Zorn", "in", "mir", "ein", "E\u00b7pig\u00b7ram\u00b7ma", "l\u00f6\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gefall' ich euch ihr Freund, und lobt ihr meine Sachen,", "tokens": ["Ge\u00b7fall'", "ich", "euch", "ihr", "Freund", ",", "und", "lobt", "ihr", "mei\u00b7ne", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So glaub ich, da\u00df ich nicht der letzte Tichter sey,", "tokens": ["So", "glaub", "ich", ",", "da\u00df", "ich", "nicht", "der", "letz\u00b7te", "Tich\u00b7ter", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo nicht, betr\u00fcg' ich mich: Jedoch bedenckt dabey,", "tokens": ["Wo", "nicht", ",", "be\u00b7tr\u00fcg'", "ich", "mich", ":", "Je\u00b7doch", "be\u00b7denckt", "da\u00b7bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "VVFIN", "PPER", "PRF", "$.", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob man es be\u00dfer auch k\u00f6nt' untern Bauern machen.", "tokens": ["Ob", "man", "es", "be\u00b7\u00dfer", "auch", "k\u00f6nt'", "un\u00b7tern", "Bau\u00b7ern", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "ADV", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}