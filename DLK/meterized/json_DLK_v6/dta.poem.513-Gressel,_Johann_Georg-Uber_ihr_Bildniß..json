{"dta.poem.513": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber ihr Bildni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Macht dort der Venus-Bild Apellens Hand erstarrt/", "tokens": ["Macht", "dort", "der", "Ve\u00b7nus\u00b7Bild", "A\u00b7pel\u00b7lens", "Hand", "er\u00b7starrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NE", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da deren Br\u00fcste Zier/ und Angesichtes Pracht/", "tokens": ["Da", "de\u00b7ren", "Br\u00fcs\u00b7te", "Zier", "/", "und", "An\u00b7ge\u00b7sich\u00b7tes", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von seiner leichten Hand zu kostbahr war gemacht/", "tokens": ["Von", "sei\u00b7ner", "leich\u00b7ten", "Hand", "zu", "kost\u00b7bahr", "war", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df auch des Leibes-Theil nicht ausgemahlet ward. ", "tokens": ["Da\u00df", "auch", "des", "Lei\u00b7bes\u00b7T\u00b7heil", "nicht", "aus\u00b7ge\u00b7mah\u00b7let", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So macht eur Wunder-Bild die gantze Stadt verliebt/", "tokens": ["So", "macht", "eur", "Wun\u00b7der\u00b7Bild", "die", "gant\u00b7ze", "Stadt", "ver\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Woran nicht die Natur nur das Gesicht polirt/", "tokens": ["Wo\u00b7ran", "nicht", "die", "Na\u00b7tur", "nur", "das", "Ge\u00b7sicht", "po\u00b7lirt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie hat auch Brust und Geist/ so liebreich ausgef\u00fchrt/", "tokens": ["Sie", "hat", "auch", "Brust", "und", "Geist", "/", "so", "lieb\u00b7reich", "aus\u00b7ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "$(", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df eure Wunder-Pracht was Englisches abgiebt.", "tokens": ["Da\u00df", "eu\u00b7re", "Wun\u00b7der\u00b7Pracht", "was", "Eng\u00b7li\u00b7sches", "ab\u00b7giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PWS", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}