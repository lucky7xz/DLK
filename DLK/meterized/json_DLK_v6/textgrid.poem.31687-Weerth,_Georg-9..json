{"textgrid.poem.31687": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ich dich zu tr\u00f6sten w\u00fc\u00dfte!", "tokens": ["O", "da\u00df", "ich", "dich", "zu", "tr\u00f6s\u00b7ten", "w\u00fc\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will ja alles und alles gestehn.", "tokens": ["Ich", "will", "ja", "al\u00b7les", "und", "al\u00b7les", "ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "KON", "PIS", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sieh, da\u00df ich das kleine Gretchen k\u00fc\u00dfte,", "tokens": ["Sieh", ",", "da\u00df", "ich", "das", "klei\u00b7ne", "Gret\u00b7chen", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ART", "ADJA", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gewi\u00df, es war ein reines Versehn.", "tokens": ["Ge\u00b7wi\u00df", ",", "es", "war", "ein", "rei\u00b7nes", "Ver\u00b7sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Es war so d\u00e4mmrig unter den Linden,", "tokens": ["Es", "war", "so", "d\u00e4mm\u00b7rig", "un\u00b7ter", "den", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich wu\u00dfte selbst nicht, wie mir geschah;", "tokens": ["Ich", "wu\u00df\u00b7te", "selbst", "nicht", ",", "wie", "mir", "ge\u00b7schah", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich hoffte nur dich, nur dich zu finden,", "tokens": ["Ich", "hoff\u00b7te", "nur", "dich", ",", "nur", "dich", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und fand und k\u00fc\u00dfte das Gretchen da.", "tokens": ["Und", "fand", "und", "k\u00fc\u00df\u00b7te", "das", "Gret\u00b7chen", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und ach, es war nur ein kleines K\u00fcssen,", "tokens": ["Und", "ach", ",", "es", "war", "nur", "ein", "klei\u00b7nes", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ich sorge, da\u00df niemand es sonst erf\u00e4hrt;", "tokens": ["Und", "ich", "sor\u00b7ge", ",", "da\u00df", "nie\u00b7mand", "es", "sonst", "er\u00b7f\u00e4hrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und gewi\u00df, du wirst mir vergeben m\u00fcssen \u2013", "tokens": ["Und", "ge\u00b7wi\u00df", ",", "du", "wirst", "mir", "ver\u00b7ge\u00b7ben", "m\u00fcs\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VAFIN", "PPER", "VVINF", "VMINF", "$("], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du hast das K\u00fcssen ja selbst mir gelehrt.", "tokens": ["Du", "hast", "das", "K\u00fcs\u00b7sen", "ja", "selbst", "mir", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}