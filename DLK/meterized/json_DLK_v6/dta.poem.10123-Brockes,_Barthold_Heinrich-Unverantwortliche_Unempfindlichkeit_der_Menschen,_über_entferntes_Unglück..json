{"dta.poem.10123": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unverantwortliche Unempfindlichkeit  \n der Menschen, \u00fcber entferntes  \n Ungl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie schrecklich sind die Krieges-Plagen!", "tokens": ["Wie", "schreck\u00b7lich", "sind", "die", "Krie\u00b7ge\u00b7sPla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie j\u00e4mmerlich ists anzusehn,", "tokens": ["Wie", "j\u00e4m\u00b7mer\u00b7lich", "ists", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann, durch der Bomben schmetternd schlagen,", "tokens": ["Wann", ",", "durch", "der", "Bom\u00b7ben", "schmet\u00b7ternd", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Selbst GOTTES-H\u00e4user untergehn!", "tokens": ["Selbst", "GoT\u00b7TES\u00b7H\u00e4u\u00b7ser", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann, in der Minen rothem Blitze,", "tokens": ["Wann", ",", "in", "der", "Mi\u00b7nen", "ro\u00b7them", "Blit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Stadt-Wall in die L\u00fcffte f\u00e4hrt!", "tokens": ["Der", "Stadt\u00b7Wall", "in", "die", "L\u00fcff\u00b7te", "f\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wann uns die Blut-bespr\u00fctzte Klingen", "tokens": ["Wann", "uns", "die", "Blut\u00b7be\u00b7spr\u00fctz\u00b7te", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durch Adern, Sehnen, Fleisch und Bein,", "tokens": ["Durch", "A\u00b7dern", ",", "Seh\u00b7nen", ",", "Fleisch", "und", "Bein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit zischendem Ger\u00e4usche, dringen:", "tokens": ["Mit", "zi\u00b7schen\u00b7dem", "Ge\u00b7r\u00e4u\u00b7sche", ",", "drin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Erwegt, was die\u00df f\u00fcr Plagen seyn!", "tokens": ["Er\u00b7wegt", ",", "was", "die\u00df", "f\u00fcr", "Pla\u00b7gen", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "PDS", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wann solch ein Jammer uns verletzet,", "tokens": ["Wann", "solch", "ein", "Jam\u00b7mer", "uns", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie hoch wird, zu derselben Zeit,", "tokens": ["Wie", "hoch", "wird", ",", "zu", "der\u00b7sel\u00b7ben", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der edle Friede nicht gesch\u00e4tzet!", "tokens": ["Der", "ed\u00b7le", "Frie\u00b7de", "nicht", "ge\u00b7sch\u00e4t\u00b7zet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00df man sich, im Genu\u00df, nicht freut.", "tokens": ["De\u00df", "man", "sich", ",", "im", "Ge\u00b7nu\u00df", ",", "nicht", "freut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "PRF", "$,", "APPRART", "NN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sollt' ieder, der von solchen Plagen,", "tokens": ["Sollt'", "ie\u00b7der", ",", "der", "von", "sol\u00b7chen", "Pla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PRELS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch GOttes Huld, nichts f\u00fchlt, nichts wei\u00df,", "tokens": ["Durch", "Got\u00b7tes", "Huld", ",", "nichts", "f\u00fchlt", ",", "nichts", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht offt mit froher Seele sagen:", "tokens": ["Nicht", "offt", "mit", "fro\u00b7her", "See\u00b7le", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein GOTT, Dir sey Lob, Ehr und Preis?", "tokens": ["Mein", "GoTT", ",", "Dir", "sey", "Lob", ",", "Ehr", "und", "Preis", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Nicht minder schrecklich ist der Jammer", "tokens": ["Nicht", "min\u00b7der", "schreck\u00b7lich", "ist", "der", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In einer heissen Hungers-Noth:", "tokens": ["In", "ei\u00b7ner", "heis\u00b7sen", "Hun\u00b7ger\u00b7sNoth", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann in der Scheun\u2019 und Speise-Kammer", "tokens": ["Wann", "in", "der", "Scheun'", "und", "Spei\u00b7se\u00b7Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Vorrath von Getraid\u2019 und Brodt.", "tokens": ["Kein", "Vor\u00b7rath", "von", "Ge\u00b7traid'", "und", "Brodt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann in den gantz verschrumpften Magen,", "tokens": ["Wann", "in", "den", "gantz", "ver\u00b7schrumpf\u00b7ten", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Hunger schwartz, verdorrt und wild,", "tokens": ["F\u00fcr", "Hun\u00b7ger", "schwartz", ",", "ver\u00b7dorrt", "und", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Menschen M\u00e4us\u2019 und Ratzen jagen,", "tokens": ["Die", "Men\u00b7schen", "M\u00e4us'", "und", "Rat\u00b7zen", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ja man sich gar mit Unrath f\u00fcllt:", "tokens": ["Ja", "man", "sich", "gar", "mit", "Un\u00b7rath", "f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wann sie, f\u00fcr Hunger, Aeser fressen,", "tokens": ["Wann", "sie", ",", "f\u00fcr", "Hun\u00b7ger", ",", "A\u00b7e\u00b7ser", "fres\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ja gar f\u00fcr Angst, f\u00fcr Pein und Wuht", "tokens": ["Ja", "gar", "f\u00fcr", "Angst", ",", "f\u00fcr", "Pein", "und", "Wuht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Fast selbst der Menschlichkeit vergessen,", "tokens": ["Fast", "selbst", "der", "Menschlich\u00b7keit", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Und w\u00fcten in ihr eigen Blut:", "tokens": ["Und", "w\u00fc\u00b7ten", "in", "ihr", "ei\u00b7gen", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da Weiber eigne Kinder schlachten,", "tokens": ["Da", "Wei\u00b7ber", "eig\u00b7ne", "Kin\u00b7der", "schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und durch ihr eigen Eingeweid", "tokens": ["Und", "durch", "ihr", "ei\u00b7gen", "Ein\u00b7ge\u00b7weid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Jhr Eingeweid zu f\u00fcllen trachten:", "tokens": ["Ihr", "Ein\u00b7ge\u00b7weid", "zu", "f\u00fcl\u00b7len", "trach\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Das heisst wol recht ein Hertzeleid!", "tokens": ["Das", "heisst", "wol", "recht", "ein", "Hert\u00b7ze\u00b7leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ja wie wir aus Geschichten wissen,", "tokens": ["Ja", "wie", "wir", "aus", "Ge\u00b7schich\u00b7ten", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KON", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df sie aus ihrem eignen Arm\u2019", "tokens": ["Da\u00df", "sie", "aus", "ih\u00b7rem", "eig\u00b7nen", "Arm'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Jhr eigen Fleisch heraus gerissen,", "tokens": ["Ihr", "ei\u00b7gen", "Fleisch", "he\u00b7raus", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Zu f\u00fcllen ihren leeren Darm.", "tokens": ["Zu", "f\u00fcl\u00b7len", "ih\u00b7ren", "lee\u00b7ren", "Darm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wie wird das Gl\u00fcck zur selben Zeit,", "tokens": ["Wie", "wird", "das", "Gl\u00fcck", "zur", "sel\u00b7ben", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wann man sein Brodt hat, nicht gesch\u00e4tzet!", "tokens": ["Wann", "man", "sein", "Brodt", "hat", ",", "nicht", "ge\u00b7sch\u00e4t\u00b7zet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VAFIN", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "De\u00df man sich, im Genu\u00df, nicht freut.", "tokens": ["De\u00df", "man",