, "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach lobt denn GOTT, wenn er hienieden,", "tokens": ["Ach", "lobt", "denn", "GoTT", ",", "wenn", "er", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "NE", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stat ", "tokens": ["Stat"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Gesundheit, Nahrung, Freyheit, Frieden,", "tokens": ["Ge\u00b7sund\u00b7heit", ",", "Nah\u00b7rung", ",", "Frey\u00b7heit", ",", "Frie\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Uns schenckt, und uns erleben l\u00e4sst!", "tokens": ["Uns", "schenckt", ",", "und", "uns", "er\u00b7le\u00b7ben", "l\u00e4sst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mein GOTT, gieb mir es zu erkennen,", "tokens": ["Mein", "GoTT", ",", "gieb", "mir", "es", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und la\u00df mich stets zu Dir allein", "tokens": ["Und", "la\u00df", "mich", "stets", "zu", "Dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In froher Danck-Begierde brennen,", "tokens": ["In", "fro\u00b7her", "Dan\u00b7ck\u00b7Be\u00b7gier\u00b7de", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und, fern von Ungl\u00fcck, fr\u00f6hlich seyn!", "tokens": ["Und", ",", "fern", "von", "Un\u00b7gl\u00fcck", ",", "fr\u00f6h\u00b7lich", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "NN", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gieb, da\u00df, wenn etwan Kleinigkeiten,", "tokens": ["Gieb", ",", "da\u00df", ",", "wenn", "et\u00b7wan", "Klei\u00b7nig\u00b7kei\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "$,", "KOUS", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie leider stets bey uns geschicht,", "tokens": ["Wie", "lei\u00b7der", "stets", "bey", "uns", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.15": {"text": "Mich zum Verdru\u00df und murren leiten,", "tokens": ["Mich", "zum", "Ver\u00b7dru\u00df", "und", "mur\u00b7ren", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "KON", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ich selbst mir diesen Unterricht", "tokens": ["Ich", "selbst", "mir", "die\u00b7sen", "Un\u00b7ter\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mit einem frohen Nachdruck gebe:", "tokens": ["Mit", "ei\u00b7nem", "fro\u00b7hen", "Nach\u00b7druck", "ge\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.18": {"text": "Und in erw\u00fcnschtem Friede lebe,", "tokens": ["Und", "in", "er\u00b7w\u00fcnschtem", "Frie\u00b7de", "le\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Trag' ich zu murren billig scheu.", "tokens": ["Trag'", "ich", "zu", "mur\u00b7ren", "bil\u00b7lig", "scheu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wofern man nun an diesen Sch\u00e4tzen", "tokens": ["Wo\u00b7fern", "man", "nun", "an", "die\u00b7sen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Mehr, als man leider sonsten pflegt,", "tokens": ["Mehr", ",", "als", "man", "lei\u00b7der", "sons\u00b7ten", "pflegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PIS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Zu GOTTES Ehren, sich ergetzen,", "tokens": ["Zu", "GoT\u00b7TES", "Eh\u00b7ren", ",", "sich", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und stets zur Danckbarkeit bewegt,", "tokens": ["Und", "stets", "zur", "Dan\u00b7ck\u00b7bar\u00b7keit", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Aus allen Kr\u00e4fften Jhn zu lieben,", "tokens": ["Aus", "al\u00b7len", "Kr\u00e4ff\u00b7ten", "Jhn", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und Seinen Nahmen zu erh\u00f6hn,", "tokens": ["Und", "Sei\u00b7nen", "Nah\u00b7men", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Zeit Lebens wird seyn angetrieben;", "tokens": ["Zeit", "Le\u00b7bens", "wird", "seyn", "an\u00b7ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wird man sich recht begl\u00fccket sehn.", "tokens": ["Wird", "man", "sich", "recht", "be\u00b7gl\u00fc\u00b7cket", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PRF", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}