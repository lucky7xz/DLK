{"textgrid.poem.43032": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Lied einer schlesischen Weberin", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn's in den Bergen rastet,", "tokens": ["Wenn's", "in", "den", "Ber\u00b7gen", "ras\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der M\u00fchlbach st\u00e4rker rauscht,", "tokens": ["Der", "M\u00fchl\u00b7bach", "st\u00e4r\u00b7ker", "rauscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mond in stummer Klage", "tokens": ["Der", "Mond", "in", "stum\u00b7mer", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch's stille Strohdach lauscht;", "tokens": ["Durch's", "stil\u00b7le", "Stroh\u00b7dach", "lauscht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn tr\u00fcb die Lampe flackert", "tokens": ["Wenn", "tr\u00fcb", "die", "Lam\u00b7pe", "fla\u00b7ckert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Im Winkel auf den Schrein:", "tokens": ["Im", "Win\u00b7kel", "auf", "den", "Schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann fallen meine H\u00e4nde", "tokens": ["Dann", "fal\u00b7len", "mei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "M\u00fcd in den Schoo\u00df hinein.", "tokens": ["M\u00fcd", "in", "den", "Schoo\u00df", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "So hab' ich oft gesessen", "tokens": ["So", "hab'", "ich", "oft", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis in die tiefe Nacht,", "tokens": ["Bis", "in", "die", "tie\u00b7fe", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Getr\u00e4umt mit offnen Augen,", "tokens": ["Ge\u00b7tr\u00e4umt", "mit", "off\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wei\u00df nicht, was ich gedacht;", "tokens": ["Wei\u00df", "nicht", ",", "was", "ich", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch immer hei\u00dfer fielen", "tokens": ["Doch", "im\u00b7mer", "hei\u00b7\u00dfer", "fie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Thr\u00e4nen auf die H\u00e4nd' \u2013", "tokens": ["Die", "Thr\u00e4\u00b7nen", "auf", "die", "H\u00e4nd'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gedacht mag ich wohl haben:", "tokens": ["Ge\u00b7dacht", "mag", "ich", "wohl", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hat's Elend gar kein End? \u2013", "tokens": ["Hat's", "E\u00b7lend", "gar", "kein", "End", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "ADV", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gestorben ist mein Vater, \u2013", "tokens": ["Ge\u00b7stor\u00b7ben", "ist", "mein", "Va\u00b7ter", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor Kurzem war's ein Jahr \u2013", "tokens": ["Vor", "Kur\u00b7zem", "wa\u00b7r's", "ein", "Jahr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie sanft und selig schlief er", "tokens": ["Wie", "sanft", "und", "se\u00b7lig", "schlief", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf seiner Todtenbahr'!", "tokens": ["Auf", "sei\u00b7ner", "Tod\u00b7ten\u00b7bahr'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Liebste nahm die B\u00fcchse,", "tokens": ["Der", "Liebs\u00b7te", "nahm", "die", "B\u00fcch\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu helfen in der Noth;", "tokens": ["Zu", "hel\u00b7fen", "in", "der", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nicht wieder ist er kommen,", "tokens": ["Nicht", "wie\u00b7der", "ist", "er", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der F\u00f6rster scho\u00df ihn todt. \u2013", "tokens": ["Der", "F\u00f6rs\u00b7ter", "scho\u00df", "ihn", "todt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es sagen oft die Leute:", "tokens": ["Es", "sa\u00b7gen", "oft", "die", "Leu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbdu bist so jung und sch\u00f6n,", "tokens": ["\u00bb", "du", "bist", "so", "jung", "und", "sch\u00f6n", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und doch so bleich und traurig", "tokens": ["Und", "doch", "so", "bleich", "und", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sollst du in Schmerz vergehn?\u00ab \u2013", "tokens": ["Sollst", "du", "in", "Schmerz", "ver\u00b7gehn", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbnicht bleich und auch nicht traurig!\u00ab", "tokens": ["\u00bb", "nicht", "bleich", "und", "auch", "nicht", "trau\u00b7rig", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKNEG", "ADJD", "KON", "ADV", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie spricht sich das geschwind", "tokens": ["Wie", "spricht", "sich", "das", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo an dem weiten Himmel", "tokens": ["Wo", "an", "dem", "wei\u00b7ten", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Sternlein mehr ich find'!", "tokens": ["Kein", "Stern\u00b7lein", "mehr", "ich", "find'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Fabrikant ist kommen,", "tokens": ["Der", "Fab\u00b7ri\u00b7kant", "ist", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sagt mir: \u00bbmein Herzenskind,", "tokens": ["Sagt", "mir", ":", "\u00bb", "mein", "Her\u00b7zens\u00b7kind", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl wei\u00df ich, wie die Deinen", "tokens": ["Wohl", "wei\u00df", "ich", ",", "wie", "die", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Noth und Kummer sind;", "tokens": ["In", "Noth", "und", "Kum\u00b7mer", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum willst Du bei mir ruhen", "tokens": ["Drum", "willst", "Du", "bei", "mir", "ru\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der N\u00e4chte drei und vier,", "tokens": ["Der", "N\u00e4ch\u00b7te", "drei", "und", "vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "KON", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sieh' dieses blanke Goldst\u00fcck!", "tokens": ["Sieh'", "die\u00b7ses", "blan\u00b7ke", "Gold\u00b7st\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sogleich geh\u00f6rt es Dir!\u00ab", "tokens": ["Sog\u00b7leich", "ge\u00b7h\u00f6rt", "es", "Dir", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich wu\u00dft' nicht, was ich h\u00f6rte \u2013", "tokens": ["Ich", "wu\u00dft'", "nicht", ",", "was", "ich", "h\u00f6r\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei Himmel du gerecht", "tokens": ["Sei", "Him\u00b7mel", "du", "ge\u00b7recht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lasse mir mein Elend,", "tokens": ["Und", "las\u00b7se", "mir", "mein", "E\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur mache mich nicht schlecht!", "tokens": ["Nur", "ma\u00b7che", "mich", "nicht", "schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O lasse mich nicht sinken!", "tokens": ["O", "las\u00b7se", "mich", "nicht", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fast halt' ich's nicht mehr aus,", "tokens": ["Fast", "halt'", "ich's", "nicht", "mehr", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Seh' ich die kranke Mutter", "tokens": ["Seh'", "ich", "die", "kran\u00b7ke", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und's Schwesterlein zu Haus'!", "tokens": ["Un\u00b7d's", "Schwes\u00b7ter\u00b7lein", "zu", "Haus'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jetzt ruh'n so still sie alle,", "tokens": ["Jetzt", "ruh'n", "so", "still", "sie", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verloschen ist das Licht,", "tokens": ["Ver\u00b7lo\u00b7schen", "ist", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur in der Brust das Wehe,", "tokens": ["Nur", "in", "der", "Brust", "das", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Thr\u00e4nen sind es nicht.", "tokens": ["Die", "Thr\u00e4\u00b7nen", "sind", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kannst du, o Gott, nicht helfen,", "tokens": ["Kannst", "du", ",", "o", "Gott", ",", "nicht", "hel\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "NN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So lass' uns lieber gehn,", "tokens": ["So", "lass'", "uns", "lie\u00b7ber", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo drunten tief im Thale", "tokens": ["Wo", "drun\u00b7ten", "tief", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Trauerbirken steh'n! \u2013", "tokens": ["Die", "Trau\u00b7er\u00b7bir\u00b7ken", "steh'n", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}