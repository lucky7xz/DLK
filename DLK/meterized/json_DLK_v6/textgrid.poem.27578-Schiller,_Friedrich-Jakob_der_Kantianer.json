{"textgrid.poem.27578": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Jakob der Kantianer", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollte Kantische Worte der hohle Sch\u00e4del nicht fassen?", "tokens": ["Soll\u00b7te", "Kan\u00b7ti\u00b7sche", "Wor\u00b7te", "der", "hoh\u00b7le", "Sch\u00e4\u00b7del", "nicht", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Hast du in hohler Nu\u00df nicht auch Devisen gesehn?", "tokens": ["Hast", "du", "in", "hoh\u00b7ler", "Nu\u00df", "nicht", "auch", "De\u00b7vi\u00b7sen", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "ADV", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Komm nur von Giebichenstein, von Malepartus! Du bist doch", "tokens": ["Komm", "nur", "von", "Gie\u00b7bi\u00b7chen\u00b7stein", ",", "von", "Ma\u00b7le\u00b7par\u00b7tus", "!", "Du", "bist", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,", "APPR", "NE", "$.", "PPER", "VAFIN", "ADV"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Reineke nicht, du bist doch nur halb B\u00e4r und halb Wolf.", "tokens": ["Rei\u00b7ne\u00b7ke", "nicht", ",", "du", "bist", "doch", "nur", "halb", "B\u00e4r", "und", "halb", "Wolf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "NN", "KON", "ADJD", "NE", "$."], "meter": "+----+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Auch erscheint ein Herr F* rhetorisch, grimmig-ironisch,", "tokens": ["Auch", "er\u00b7scheint", "ein", "Herr", "F", "*", "rhe\u00b7to\u00b7risch", ",", "grim\u00b7mig\u00b7i\u00b7ro\u00b7nisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "XY", "XY", "XY", "$,", "ADJD", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Seltsam geb\u00e4rdet er sich, plattdeutsch, im Zeitungsformat.", "tokens": ["Selt\u00b7sam", "ge\u00b7b\u00e4r\u00b7det", "er", "sich", ",", "platt\u00b7deutsch", ",", "im", "Zei\u00b7tungs\u00b7for\u00b7mat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PRF", "$,", "ADJD", "$,", "APPRART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}