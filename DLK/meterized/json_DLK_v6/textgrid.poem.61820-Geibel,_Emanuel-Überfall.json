{"textgrid.poem.61820": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcberfall", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Monde hin streichen", "tokens": ["Am", "Mon\u00b7de", "hin", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NE", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Wolken im Flug;", "tokens": ["Die", "Wol\u00b7ken", "im", "Flug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auf der Heide, der bleichen,", "tokens": ["Auf", "der", "Hei\u00b7de", ",", "der", "blei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Geht leise der Zug.", "tokens": ["Geht", "lei\u00b7se", "der", "Zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Nur ein heimliches Rufen", "tokens": ["Nur", "ein", "heim\u00b7li\u00b7ches", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "L\u00e4uft fort durch die Reihn,", "tokens": ["L\u00e4uft", "fort", "durch", "die", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und es klirrt wie von Hufen", "tokens": ["Und", "es", "klirrt", "wie", "von", "Hu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und Harnischen drein.", "tokens": ["Und", "Har\u00b7ni\u00b7schen", "drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Schwer zwischen den Reitern", "tokens": ["Schwer", "zwi\u00b7schen", "den", "Rei\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die Kartaune hinf\u00e4hrt;", "tokens": ["Die", "Kar\u00b7tau\u00b7ne", "hin\u00b7f\u00e4hrt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit Pechkranz und Leitern", "tokens": ["Mit", "Pech\u00b7kranz", "und", "Lei\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sind sie bewehrt.", "tokens": ["Sind", "sie", "be\u00b7wehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Sie ziehen zur Feste,", "tokens": ["Sie", "zie\u00b7hen", "zur", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Entgegen der Schanz',", "tokens": ["Ent\u00b7ge\u00b7gen", "der", "Schanz'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ungeladene G\u00e4ste", "tokens": ["Un\u00b7ge\u00b7la\u00b7de\u00b7ne", "G\u00e4s\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Zum blutigen Tanz.", "tokens": ["Zum", "blu\u00b7ti\u00b7gen", "Tanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Hintan reitet einer", "tokens": ["Hin\u00b7tan", "rei\u00b7tet", "ei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf d\u00fcrr, d\u00fcrrem Tier,", "tokens": ["Auf", "d\u00fcrr", ",", "d\u00fcr\u00b7rem", "Tier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sein Antlitz grinst beinern", "tokens": ["Sein", "Ant\u00b7litz", "grinst", "bei\u00b7nern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Aus dem rost'gen Visier.", "tokens": ["Aus", "dem", "rost'\u00b7gen", "Vi\u00b7sier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Um das Panzerhemd schlottern", "tokens": ["Um", "das", "Pan\u00b7zer\u00b7hemd", "schlot\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Grablinnen ihm her;", "tokens": ["Grab\u00b7lin\u00b7nen", "ihm", "her", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Seine Z\u00fcgel sind Ottern,", "tokens": ["Sei\u00b7ne", "Z\u00fc\u00b7gel", "sind", "Ot\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Eine Sens' ist sein Speer.", "tokens": ["Ei\u00b7ne", "Sens'", "ist", "sein", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Jetzt lauscht er vom R\u00f6\u00dflein,", "tokens": ["Jetzt", "lauscht", "er", "vom", "R\u00f6\u00df\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Jetzt spornt er's zum Lauf; \u2013", "tokens": ["Jetzt", "spornt", "er's", "zum", "Lauf", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O da dr\u00fcben im Schl\u00f6\u00dflein,", "tokens": ["O", "da", "dr\u00fc\u00b7ben", "im", "Schl\u00f6\u00df\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ihr Schl\u00e4fer, wacht auf!", "tokens": ["Ihr", "Schl\u00e4\u00b7fer", ",", "wacht", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}