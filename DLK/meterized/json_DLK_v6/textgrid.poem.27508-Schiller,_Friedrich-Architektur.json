{"textgrid.poem.27508": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Architektur", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter dem leichten Geschlecht erscheinst du schwer und bed\u00e4chtig,", "tokens": ["Un\u00b7ter", "dem", "leich\u00b7ten", "Ge\u00b7schlecht", "er\u00b7scheinst", "du", "schwer", "und", "be\u00b7d\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber zu Regel und Zucht winkst du die Schwestern zur\u00fcck.", "tokens": ["A\u00b7ber", "zu", "Re\u00b7gel", "und", "Zucht", "winkst", "du", "die", "Schwes\u00b7tern", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "H\u00fcpfe nur, leichtes Geschlecht, ich Gefesselte kann dir nicht folgen,", "tokens": ["H\u00fcp\u00b7fe", "nur", ",", "leich\u00b7tes", "Ge\u00b7schlecht", ",", "ich", "Ge\u00b7fes\u00b7sel\u00b7te", "kann", "dir", "nicht", "fol\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$,", "PPER", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber ich wei\u00df zu ruhn und auf mir selber zu stehn.", "tokens": ["A\u00b7ber", "ich", "wei\u00df", "zu", "ruhn", "und", "auf", "mir", "sel\u00b7ber", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKZU", "VVINF", "KON", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Freilich kann ich dich nicht in schl\u00e4ngelnden Wellen umspielen,", "tokens": ["Frei\u00b7lich", "kann", "ich", "dich", "nicht", "in", "schl\u00e4n\u00b7geln\u00b7den", "Wel\u00b7len", "um\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber mein Dasein fa\u00dft m\u00e4chtig wie keines dich an.", "tokens": ["A\u00b7ber", "mein", "Da\u00b7sein", "fa\u00dft", "m\u00e4ch\u00b7tig", "wie", "kei\u00b7nes", "dich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "KOKOM", "PIS", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}