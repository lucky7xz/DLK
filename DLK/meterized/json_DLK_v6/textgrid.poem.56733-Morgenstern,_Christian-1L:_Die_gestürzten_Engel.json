{"textgrid.poem.56733": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die gest\u00fcrzten Engel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die gest\u00fcrzten Engel", "tokens": ["Die", "ge\u00b7st\u00fcrz\u00b7ten", "En\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "schweben um den Berg.", "tokens": ["schwe\u00b7ben", "um", "den", "Berg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mit wei\u00dfen, bleiernen Riesenfittichen", "tokens": ["Mit", "wei\u00b7\u00dfen", ",", "blei\u00b7er\u00b7nen", "Rie\u00b7sen\u00b7fit\u00b7ti\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "schleicht ihr Flug aus den Talen,", "tokens": ["schleicht", "ihr", "Flug", "aus", "den", "Ta\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "da\u00df er die H\u00f6hen der Erde auch", "tokens": ["da\u00df", "er", "die", "H\u00f6\u00b7hen", "der", "Er\u00b7de", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "todesk\u00e4ltend \u00fcberfinstere,", "tokens": ["to\u00b7des\u00b7k\u00e4l\u00b7tend", "\u00fc\u00b7berf\u00b7ins\u00b7te\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "da\u00df im Schweigen der Nacht", "tokens": ["da\u00df", "im", "Schwei\u00b7gen", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "endlich das Leben sterbe.", "tokens": ["end\u00b7lich", "das", "Le\u00b7ben", "ster\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Lebendige Flammen", "tokens": ["Le\u00b7ben\u00b7di\u00b7ge", "Flam\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "entrief ich dem Fels", "tokens": ["en\u00b7trief", "ich", "dem", "Fels"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "zum Schutze.", "tokens": ["zum", "Schut\u00b7ze", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "In goldenem Zorn", "tokens": ["In", "gol\u00b7de\u00b7nem", "Zorn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "leuchtet das Berghaupt.", "tokens": ["leuch\u00b7tet", "das", "Berg\u00b7haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Aber die hei\u00dfeste Stirn,", "tokens": ["A\u00b7ber", "die", "hei\u00b7\u00dfes\u00b7te", "Stirn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "das gl\u00fchendste Aug", "tokens": ["das", "gl\u00fc\u00b7hends\u00b7te", "Aug"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "ist nicht lange gefeit,", "tokens": ["ist", "nicht", "lan\u00b7ge", "ge\u00b7feit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "wo solcher Fl\u00fcgel", "tokens": ["wo", "sol\u00b7cher", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "grabkalte Bahrt\u00fccher", "tokens": ["grab\u00b7kal\u00b7te", "Bahr\u00b7t\u00fc\u00b7cher"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.11": {"text": "der Vernichtung eisige Schauer", "tokens": ["der", "Ver\u00b7nich\u00b7tung", "ei\u00b7si\u00b7ge", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "ins Haupt schatten.", "tokens": ["ins", "Haupt", "schat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und fahles Grauen", "tokens": ["Und", "fah\u00b7les", "Grau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "w\u00fcrgt mir die Kehle", "tokens": ["w\u00fcrgt", "mir", "die", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "und rei\u00dft einen Schrei mir", "tokens": ["und", "rei\u00dft", "ei\u00b7nen", "Schrei", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "aus der Brust", "tokens": ["aus", "der", "Brust"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "und wirft ihn hinaus", "tokens": ["und", "wirft", "ihn", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APZR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "in die Finsternisse ...", "tokens": ["in", "die", "Fins\u00b7ter\u00b7nis\u00b7se", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Vom grauen Fittichgew\u00f6lbe", "tokens": ["Vom", "grau\u00b7en", "Fit\u00b7tich\u00b7ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "f\u00e4llt er ohnm\u00e4chtig", "tokens": ["f\u00e4llt", "er", "ohn\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "in mich zur\u00fcck.", "tokens": ["in", "mich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Im Schein der m\u00fchsam", "tokens": ["Im", "Schein", "der", "m\u00fch\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "k\u00e4mpfenden Lohe", "tokens": ["k\u00e4mp\u00b7fen\u00b7den", "Lo\u00b7he"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "trete ich, halb von Sinnen,", "tokens": ["tre\u00b7te", "ich", ",", "halb", "von", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "zum Rande des Abgrunds", "tokens": ["zum", "Ran\u00b7de", "des", "Ab\u00b7grunds"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "und breite, wie pr\u00fcfend,", "tokens": ["und", "brei\u00b7te", ",", "wie", "pr\u00fc\u00b7fend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "PWAV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "die Arme aus.", "tokens": ["die", "Ar\u00b7me", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Da zucken die Nebelgespenster", "tokens": ["Da", "zu\u00b7cken", "die", "Ne\u00b7bel\u00b7ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "grausengepackt zusammen.", "tokens": ["grau\u00b7sen\u00b7ge\u00b7packt", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ihr schn\u00fcrender Reigen", "tokens": ["Ihr", "schn\u00fc\u00b7ren\u00b7der", "Rei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "l\u00f6st sich, zerstreut sich.", "tokens": ["l\u00f6st", "sich", ",", "zer\u00b7streut", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "PRF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In wildem Entsetzen", "tokens": ["In", "wil\u00b7dem", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "rasen heulend die Satane", "tokens": ["ra\u00b7sen", "heu\u00b7lend", "die", "Sa\u00b7ta\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "um den Gipfel.", "tokens": ["um", "den", "Gip\u00b7fel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.8": {"text": "Ich aber erkenne", "tokens": ["Ich", "a\u00b7ber", "er\u00b7ken\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "auf der zitternden Wand", "tokens": ["auf", "der", "zit\u00b7tern\u00b7den", "Wand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "ihrer Fl\u00fcgelflucht", "tokens": ["ih\u00b7rer", "Fl\u00fc\u00b7gel\u00b7flucht"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "ein m\u00e4chtiges, schwarzes Kreuz.", "tokens": ["ein", "m\u00e4ch\u00b7ti\u00b7ges", ",", "schwar\u00b7zes", "Kreuz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Meines K\u00f6rpers", "tokens": ["Mei\u00b7nes", "K\u00f6r\u00b7pers"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "kreuzf\u00f6rmiger Schatte", "tokens": ["kreuz\u00b7f\u00f6r\u00b7mi\u00b7ger", "Schat\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "qu\u00e4lt triumphierend", "tokens": ["qu\u00e4lt", "tri\u00b7um\u00b7phie\u00b7rend"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "die Engel des Todes", "tokens": ["die", "En\u00b7gel", "des", "To\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "hinweg, hinab,", "tokens": ["hin\u00b7weg", ",", "hin\u00b7ab", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "zur\u00fcck in ihr trauriges Reich.", "tokens": ["zu\u00b7r\u00fcck", "in", "ihr", "trau\u00b7ri\u00b7ges", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Ich stehe noch lange,", "tokens": ["Ich", "ste\u00b7he", "noch", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "die Arme gebreitet,", "tokens": ["die", "Ar\u00b7me", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "doch nicht mehr in Angst", "tokens": ["doch", "nicht", "mehr", "in", "Angst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "noch als Wehr,", "tokens": ["noch", "als", "Wehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "nein! jetzt als Gru\u00df", "tokens": ["nein", "!", "jetzt", "als", "Gru\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "KOUS", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "und heilige Ehrung", "tokens": ["und", "hei\u00b7li\u00b7ge", "Eh\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "den tausend l\u00e4chelnden Lichtaugen", "tokens": ["den", "tau\u00b7send", "l\u00e4\u00b7cheln\u00b7den", "Lich\u00b7tau\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "CARD", "ADJA", "NN"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "des unsterblichen Alls.", "tokens": ["des", "uns\u00b7terb\u00b7li\u00b7chen", "Alls", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}