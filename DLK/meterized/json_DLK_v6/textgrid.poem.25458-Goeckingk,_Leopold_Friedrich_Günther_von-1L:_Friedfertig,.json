{"textgrid.poem.25458": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Friedfertig, ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Friedfertig, ", "tokens": ["Fried\u00b7fer\u00b7tig", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "---", "measure": "unknown.measure.zero"}, "line.2": {"text": "Bin ich nur erst seit wenig Jahren.", "tokens": ["Bin", "ich", "nur", "erst", "seit", "we\u00b7nig", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist mein Muth noch nicht dahin,", "tokens": ["Doch", "ist", "mein", "Muth", "noch", "nicht", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die See der Schlachten zu befahren.", "tokens": ["Die", "See", "der", "Schlach\u00b7ten", "zu", "be\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn sieh! mein wackrer Brauder liegt", "tokens": ["Denn", "sieh", "!", "mein", "wack\u00b7rer", "Brau\u00b7der", "liegt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch segelfertig hier im Hafen,", "tokens": ["Noch", "se\u00b7gel\u00b7fer\u00b7tig", "hier", "im", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein purpurfarbner Wimpel fliegt,", "tokens": ["Sein", "pur\u00b7purf\u00b7arb\u00b7ner", "Wim\u00b7pel", "fliegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und droht, den Kecken zu bestrafen,", "tokens": ["Und", "droht", ",", "den", "Ke\u00b7cken", "zu", "be\u00b7stra\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der blo\u00df aus Ehrsuchts-Kitzel kriegt.", "tokens": ["Der", "blo\u00df", "aus", "Ehr\u00b7suchts\u00b7Kit\u00b7zel", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Kaper, der statt reicher Beute,", "tokens": ["Der", "Ka\u00b7per", ",", "der", "statt", "rei\u00b7cher", "Beu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Nur Aufsehn zu erregen sucht,", "tokens": ["Nur", "Auf\u00b7sehn", "zu", "er\u00b7re\u00b7gen", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und laut von dem Verdecke, heute", "tokens": ["Und", "laut", "von", "dem", "Ver\u00b7de\u00b7cke", ",", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Den lobt, auf den er morgen flucht;", "tokens": ["Den", "lobt", ",", "auf", "den", "er", "mor\u00b7gen", "flucht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und jen' undeutsche Landesleute,", "tokens": ["Und", "jen'", "un\u00b7deut\u00b7sche", "Lan\u00b7des\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die stracks auf ihrem plumpen Kahn',", "tokens": ["Die", "stracks", "auf", "ih\u00b7rem", "plum\u00b7pen", "Kahn'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Mit Schn\u00f6rkeln, wie zu ", "tokens": ["Mit", "Schn\u00f6r\u00b7keln", ",", "wie", "zu"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "PTKZU"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.17": {"text": "Staffirt, nachrudern auf der Bahn',", "tokens": ["Staf\u00b7firt", ",", "nach\u00b7ru\u00b7dern", "auf", "der", "Bahn'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Worauf sie ", "tokens": ["Wo\u00b7rauf", "sie"], "token_info": ["word", "word"], "pos": ["PAV", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Mit dem Homerus fliegen sahn;", "tokens": ["Mit", "dem", "Ho\u00b7me\u00b7rus", "flie\u00b7gen", "sahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVINF", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Die Herrchen, welche neues Land", "tokens": ["Die", "Herr\u00b7chen", ",", "wel\u00b7che", "neu\u00b7es", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Entdeckt zu haben, uns verk\u00fcnden,", "tokens": ["Ent\u00b7deckt", "zu", "ha\u00b7ben", ",", "uns", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Sobald auf einer Bank von Sand", "tokens": ["So\u00b7bald", "auf", "ei\u00b7ner", "Bank", "von", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Sie nur zwei neue Bl\u00fcmchen finden;", "tokens": ["Sie", "nur", "zwei", "neu\u00b7e", "Bl\u00fcm\u00b7chen", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "CARD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die M\u00e4nnerchen von Zuckergu\u00df,", "tokens": ["Die", "M\u00e4n\u00b7ner\u00b7chen", "von", "Zu\u00b7cker\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die gleich auf jedem Nautilus", "tokens": ["Die", "gleich", "auf", "je\u00b7dem", "Nau\u00b7ti\u00b7lus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Auch eine Venus schiffen sehen,", "tokens": ["Auch", "ei\u00b7ne", "Ve\u00b7nus", "schif\u00b7fen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "F\u00fcr die der Gott der Liebe stehen", "tokens": ["F\u00fcr", "die", "der", "Gott", "der", "Lie\u00b7be", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und ewig Pfeile wetzen mu\u00df:", "tokens": ["Und", "e\u00b7wig", "Pfei\u00b7le", "wet\u00b7zen", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Kurz, diese Narrn und N\u00e4rrchen alle,", "tokens": ["Kurz", ",", "die\u00b7se", "Narrn", "und", "N\u00e4rr\u00b7chen", "al\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PDAT", "NN", "KON", "NN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Verdienen, da\u00df man auf sie kreuzt;", "tokens": ["Ver\u00b7die\u00b7nen", ",", "da\u00df", "man", "auf", "sie", "kreuzt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Da\u00df aber keiner meine Galle,", "tokens": ["Da\u00df", "a\u00b7ber", "kei\u00b7ner", "mei\u00b7ne", "Gal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wie sonst, zum Ankerlichten reitzt,", "tokens": ["Wie", "sonst", ",", "zum", "An\u00b7ker\u00b7lich\u00b7ten", "reitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Das sey gedankt der guten Seele,", "tokens": ["Das", "sey", "ge\u00b7dankt", "der", "gu\u00b7ten", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Die nun am Steuerruder wacht,", "tokens": ["Die", "nun", "am", "Steu\u00b7er\u00b7ru\u00b7der", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und die Kaj\u00fct' aus einer H\u00f6hle", "tokens": ["Und", "die", "Ka\u00b7j\u00fct'", "aus", "ei\u00b7ner", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Zur Myrtenlaub' auf Paphos macht.", "tokens": ["Zur", "Myr\u00b7ten\u00b7laub'", "auf", "Pa\u00b7phos", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Mit ihr geh' ich am Strand' spatzieren,", "tokens": ["Mit", "ihr", "geh'", "ich", "am", "Strand'", "spat\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Und schau' hinaus aufs hohe Meer,", "tokens": ["Und", "schau'", "hin\u00b7aus", "aufs", "ho\u00b7he", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und sehe Furcht- und Hoffnungsleer", "tokens": ["Und", "se\u00b7he", "Furcht", "und", "Hoff\u00b7nungs\u00b7leer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Den Krieg auf gut korsarisch f\u00fchren.", "tokens": ["Den", "Krieg", "auf", "gut", "kor\u00b7sa\u00b7risch", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.41": {"text": "Neutral zu seyn in jedem Streit',", "tokens": ["Neut\u00b7ral", "zu", "seyn", "in", "je\u00b7dem", "Streit'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ist feine Politik bei allen,", "tokens": ["Ist", "fei\u00b7ne", "Po\u00b7li\u00b7tik", "bei", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Die beiden Theilen gern gefallen;", "tokens": ["Die", "bei\u00b7den", "Thei\u00b7len", "gern", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Bei mir ist's blo\u00df Gem\u00e4chlichkeit.", "tokens": ["Bei", "mir", "ist's", "blo\u00df", "Ge\u00b7m\u00e4ch\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Gelehrt' und gro\u00dfe Herrn vertreiben", "tokens": ["Ge\u00b7lehrt'", "und", "gro\u00b7\u00dfe", "Herrn", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Durch ihre Kriege sich die Zeit,", "tokens": ["Durch", "ih\u00b7re", "Krie\u00b7ge", "sich", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und Ruhm, sogar Unsterblichkeit,", "tokens": ["Und", "Ruhm", ",", "so\u00b7gar", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.48": {"text": "L\u00e4\u00dft sich erfechten und erschreiben,", "tokens": ["L\u00e4\u00dft", "sich", "er\u00b7fech\u00b7ten", "und", "er\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Doch nimmermehr Zufriedenheit.", "tokens": ["Doch", "nim\u00b7mer\u00b7mehr", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Vielleicht k\u00f6nnt' ich auch, durch Satyren,", "tokens": ["Viel\u00b7leicht", "k\u00f6nnt'", "ich", "auch", ",", "durch", "Sa\u00b7ty\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Noch sp\u00e4ten Ruhm, wie Juvenal,", "tokens": ["Noch", "sp\u00e4\u00b7ten", "Ruhm", ",", "wie", "Ju\u00b7ve\u00b7nal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Gewinnen: doch bedenk einmal!", "tokens": ["Ge\u00b7win\u00b7nen", ":", "doch", "be\u00b7denk", "ein\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Was m\u00fc\u00dft' ich ", "tokens": ["Was", "m\u00fc\u00dft'", "ich"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.54": {"text": "Dann k\u00f6nnt' ich nicht so s\u00fc\u00df, wie itzt,", "tokens": ["Dann", "k\u00f6nnt'", "ich", "nicht", "so", "s\u00fc\u00df", ",", "wie", "itzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Am Bach' auf harten Rasen schlafen;", "tokens": ["Am", "Bach'", "auf", "har\u00b7ten", "Ra\u00b7sen", "schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Denn, wen ein Satyr erst besitzt,", "tokens": ["Denn", ",", "wen", "ein", "Sa\u00b7tyr", "erst", "be\u00b7sitzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"