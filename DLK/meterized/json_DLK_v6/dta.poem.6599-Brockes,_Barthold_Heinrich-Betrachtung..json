{"dta.poem.6599": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ists m\u00f6glich da\u00df der Geist sich l\u00e4sst so sehr beth\u00f6ren,", "tokens": ["Ists", "m\u00f6g\u00b7lich", "da\u00df", "der", "Geist", "sich", "l\u00e4sst", "so", "sehr", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ART", "NN", "PRF", "VVFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er sich blo\u00df allein an grobe Schatten h\u00e4lt?", "tokens": ["Da\u00df", "er", "sich", "blo\u00df", "al\u00b7lein", "an", "gro\u00b7be", "Schat\u00b7ten", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An statt zum H\u00f6chsten ", "tokens": ["An", "statt", "zum", "H\u00f6chs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "aller Welt,", "tokens": ["al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "In Seines reinen Lichts Erkenntni\u00df sich zu kehren?", "tokens": ["In", "Sei\u00b7nes", "rei\u00b7nen", "Lichts", "Er\u00b7kennt\u00b7ni\u00df", "sich", "zu", "keh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird nicht in allen dem, was uns umgiebt, versp\u00fcret", "tokens": ["Wird", "nicht", "in", "al\u00b7len", "dem", ",", "was", "uns", "um\u00b7giebt", ",", "ver\u00b7sp\u00fc\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "PIAT", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Hand ", "tokens": ["Die", "Hand"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Bleibt er, da alle Ding\u2019 ihm GOTT entdecken,", "tokens": ["Bleibt", "er", ",", "da", "al\u00b7le", "Ding'", "ihm", "GoTT", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PIAT", "NN", "PPER", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dennoch in seiner Blindheit stecken?", "tokens": ["Den\u00b7noch", "in", "sei\u00b7ner", "Blind\u00b7heit", "ste\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Man sieht das reine Gold und Blau des Himmels an:", "tokens": ["Man", "sieht", "das", "rei\u00b7ne", "Gold", "und", "Blau", "des", "Him\u00b7mels", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "KON", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der sch\u00f6ne Glantz hat nichts, das uns vergn\u00fcgen kan.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Glantz", "hat", "nichts", ",", "das", "uns", "ver\u00b7gn\u00fc\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIS", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man achtet nichts, als Reichthum nur.", "tokens": ["Man", "ach\u00b7tet", "nichts", ",", "als", "Reicht\u00b7hum", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "$,", "KOUS", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Man sucht in Indien unn\u00fctzer Sch\u00e4tze Spur.", "tokens": ["Man", "sucht", "in", "In\u00b7di\u00b7en", "un\u00b7n\u00fct\u00b7zer", "Sch\u00e4t\u00b7ze", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NE", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Man nimmt so manche Noth und Pein,", "tokens": ["Man", "nimmt", "so", "man\u00b7che", "Noth", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So mancherley Gefahr auf sich, um die zu finden,", "tokens": ["So", "man\u00b7cher\u00b7ley", "Ge\u00b7fahr", "auf", "sich", ",", "um", "die", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PRF", "$,", "KOUI", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.16": {"text": "Die in der Erden tieffen Gr\u00fcnden,", "tokens": ["Die", "in", "der", "Er\u00b7den", "tief\u00b7fen", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Verholen und verborgen seyn.", "tokens": ["Ver\u00b7ho\u00b7len", "und", "ver\u00b7bor\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}