{"textgrid.poem.46706": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn ihr redlich es meinet]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr redlich es meinet,", "tokens": ["Wenn", "ihr", "red\u00b7lich", "es", "mei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Alle W\u00e4sserlein, weinet!", "tokens": ["Al\u00b7le", "W\u00e4s\u00b7ser\u00b7lein", ",", "wei\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ihr von jeglicher Seite", "tokens": ["Ihr", "von", "jeg\u00b7li\u00b7cher", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Niederflie\u00dfend im Thale,", "tokens": ["Nie\u00b7der\u00b7flie\u00b7\u00dfend", "im", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ihr so schmale wie breite,", "tokens": ["Ihr", "so", "schma\u00b7le", "wie", "brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "KOKOM", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und so breite wie schmale,", "tokens": ["Und", "so", "brei\u00b7te", "wie", "schma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "KOKOM", "ADV", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Spielend im Sonnenstrahle!", "tokens": ["Spie\u00b7lend", "im", "Son\u00b7nen\u00b7strah\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wenn ihr redlich es meinet,", "tokens": ["Wenn", "ihr", "red\u00b7lich", "es", "mei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Alle W\u00e4sserlein, weinet!", "tokens": ["Al\u00b7le", "W\u00e4s\u00b7ser\u00b7lein", ",", "wei\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wu\u00dfte nicht, was zusammen", "tokens": ["Wu\u00df\u00b7te", "nicht", ",", "was", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PRELS", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ihr erg\u00f6sset den Braus,", "tokens": ["Ihr", "er\u00b7g\u00f6s\u00b7set", "den", "Braus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gleich als st\u00fcnd' es in Flammen,", "tokens": ["Gleich", "als", "st\u00fcnd'", "es", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Um dies einzelne Haus;", "tokens": ["Um", "dies", "ein\u00b7zel\u00b7ne", "Haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzo weist es sich aus,", "tokens": ["Jet\u00b7zo", "weist", "es", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und ich seh, was ihr meinet;", "tokens": ["Und", "ich", "seh", ",", "was", "ihr", "mei\u00b7net", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Alle W\u00e4sserlein weinet!", "tokens": ["Al\u00b7le", "W\u00e4s\u00b7ser\u00b7lein", "wei\u00b7net", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Dachte, da\u00df ihr im Flie\u00dfen", "tokens": ["Dach\u00b7te", ",", "da\u00df", "ihr", "im", "Flie\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wolltet drehen die M\u00fchlen,", "tokens": ["Woll\u00b7tet", "dre\u00b7hen", "die", "M\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und die Bleiche begie\u00dfen,", "tokens": ["Und", "die", "Blei\u00b7che", "be\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Blumen besp\u00fclen;", "tokens": ["Und", "die", "Blu\u00b7men", "be\u00b7sp\u00fc\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Anders la\u00dft ihr mich f\u00fchlen", "tokens": ["An\u00b7ders", "la\u00dft", "ihr", "mich", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nun, wozu ihr erscheinet;", "tokens": ["Nun", ",", "wo\u00b7zu", "ihr", "er\u00b7schei\u00b7net", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Alle W\u00e4sserlein, weinet!", "tokens": ["Al\u00b7le", "W\u00e4s\u00b7ser\u00b7lein", ",", "wei\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Alle, die ihr begossen,", "tokens": ["Al\u00b7le", ",", "die", "ihr", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Alle Blumen ersprie\u00dfen,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "er\u00b7sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zwei nur werden nicht sprossen,", "tokens": ["Zwei", "nur", "wer\u00b7den", "nicht", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wie ihr m\u00f6get begie\u00dfen.", "tokens": ["Wie", "ihr", "m\u00f6\u00b7get", "be\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "La\u00dft's euch doch nicht verdrie\u00dfen,", "tokens": ["La\u00dft's", "euch", "doch", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Meinen Augen vereinet,", "tokens": ["Mei\u00b7nen", "Au\u00b7gen", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Alle W\u00e4sserlein, weinet!", "tokens": ["Al\u00b7le", "W\u00e4s\u00b7ser\u00b7lein", ",", "wei\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Aengstlich bebt' ich vor allen", "tokens": ["A\u00b7engst\u00b7lich", "bebt'", "ich", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PIAT"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Euern Br\u00fccken und Stegen,", "tokens": ["Eu\u00b7ern", "Br\u00fc\u00b7cken", "und", "Ste\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Drein einst m\u00f6chten sie fallen,", "tokens": ["Drein", "einst", "m\u00f6ch\u00b7ten", "sie", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Denn sie sprangen verwegen.", "tokens": ["Denn", "sie", "spran\u00b7gen", "ver\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Sorg' um euretwegen", "tokens": ["Sor\u00b7g'", "um", "eu\u00b7ret\u00b7we\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat umsonst mich gepeinet;", "tokens": ["Hat", "um\u00b7sonst", "mich", "ge\u00b7pei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Alle W\u00e4sserlein, weinet!", "tokens": ["Al\u00b7le", "W\u00e4s\u00b7ser\u00b7lein", ",", "wei\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Ueber Br\u00fccken und Stege", "tokens": ["Ue\u00b7ber", "Br\u00fc\u00b7cken", "und", "Ste\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Kamen stets sie zur\u00fccke,", "tokens": ["Ka\u00b7men", "stets", "sie", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch nun gehn sie die Wege", "tokens": ["Doch", "nun", "gehn", "sie", "die", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Uebern Strom ohne Br\u00fccke,", "tokens": ["Ue\u00b7bern", "Strom", "oh\u00b7ne", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Dort, von wo vom Geschicke", "tokens": ["Dort", ",", "von", "wo", "vom", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wiederkehr ist verneinet;", "tokens": ["Wie\u00b7der\u00b7kehr", "ist", "ver\u00b7nei\u00b7net", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Alle W\u00e4sserlein, weinet!", "tokens": ["Al\u00b7le", "W\u00e4s\u00b7ser\u00b7lein", ",", "wei\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}