{"textgrid.poem.61760": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ein Psalm wider Babel", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist gesch\u00fcrzt vom B\u00f6sen", "tokens": ["Nun", "ist", "ge\u00b7sch\u00fcrzt", "vom", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Knoten also fein,", "tokens": ["Der", "Kno\u00b7ten", "al\u00b7so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Rat mehr kann ihn l\u00f6sen,", "tokens": ["Kein", "Rat", "mehr", "kann", "ihn", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er mu\u00df zerhauen sein.", "tokens": ["Er", "mu\u00df", "zer\u00b7hau\u00b7en", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr habt verworfen den Frieden,", "tokens": ["Ihr", "habt", "ver\u00b7wor\u00b7fen", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den treuer Sinn euch bot,", "tokens": ["Den", "treu\u00b7er", "Sinn", "euch", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So soll euch sein beschieden", "tokens": ["So", "soll", "euch", "sein", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Streit und Jammer und Not.", "tokens": ["Streit", "und", "Jam\u00b7mer", "und", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Den ihr, bekr\u00e4nzt die Schl\u00e4fen,", "tokens": ["Den", "ihr", ",", "be\u00b7kr\u00e4nzt", "die", "Schl\u00e4\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gebraut, den Greueltrank,", "tokens": ["Ge\u00b7braut", ",", "den", "Greu\u00b7el\u00b7trank", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis auf die letzten Hefen", "tokens": ["Bis", "auf", "die", "letz\u00b7ten", "He\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sollt ihr ihn leeren zum Dank.", "tokens": ["Sollt", "ihr", "ihn", "lee\u00b7ren", "zum", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Lobsingt nur eurem G\u00f6tzen", "tokens": ["Lob\u00b7singt", "nur", "eu\u00b7rem", "G\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In frechem Gaukelspiel!", "tokens": ["In", "fre\u00b7chem", "Gau\u00b7kel\u00b7spiel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Herr wird kommen und setzen", "tokens": ["Der", "Herr", "wird", "kom\u00b7men", "und", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem w\u00fcsten Rausch ein Ziel!", "tokens": ["Dem", "w\u00fcs\u00b7ten", "Rausch", "ein", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sein Odem Sturm des Krieges,", "tokens": ["Sein", "O\u00b7dem", "Sturm", "des", "Krie\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der die Heerscharen fegt,", "tokens": ["Der", "die", "Heer\u00b7scha\u00b7ren", "fegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sein Schwert ein Schwert des Sieges,", "tokens": ["Sein", "Schwert", "ein", "Schwert", "des", "Sie\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das allen Frevel schl\u00e4gt!", "tokens": ["Das", "al\u00b7len", "Fre\u00b7vel", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Finster wird sein die Erde", "tokens": ["Fins\u00b7ter", "wird", "sein", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und der Himmel voll Glut,", "tokens": ["Und", "der", "Him\u00b7mel", "voll", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bis an die Z\u00e4ume der Pferde", "tokens": ["Bis", "an", "die", "Z\u00e4u\u00b7me", "der", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Steigen wird das Blut.", "tokens": ["Stei\u00b7gen", "wird", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Die Str\u00f6me werden weichen", "tokens": ["Die", "Str\u00f6\u00b7me", "wer\u00b7den", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus ihren Ufern zur Frist,", "tokens": ["Aus", "ih\u00b7ren", "U\u00b7fern", "zur", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Weil mit Schutt und Leichen", "tokens": ["Weil", "mit", "Schutt", "und", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihr Bett verd\u00e4mmet ist.", "tokens": ["Ihr", "Bett", "ver\u00b7d\u00e4m\u00b7met", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es wird zertreten der R\u00e4cher", "tokens": ["Es", "wird", "zer\u00b7tre\u00b7ten", "der", "R\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die St\u00e4tten, da ihr sitzt,", "tokens": ["Die", "St\u00e4t\u00b7ten", ",", "da", "ihr", "sitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df durch die krachenden D\u00e4cher", "tokens": ["Da\u00df", "durch", "die", "kra\u00b7chen\u00b7den", "D\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hochauf die Lohe spritzt.", "tokens": ["Hoch\u00b7auf", "die", "Lo\u00b7he", "spritzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und Heulen wird sein auf den Gassen", "tokens": ["Und", "Heu\u00b7len", "wird", "sein", "auf", "den", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Hunger Haus bei Haus,", "tokens": ["Und", "Hun\u00b7ger", "Haus", "bei", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Indes die W\u00f6lfe prassen", "tokens": ["In\u00b7des", "die", "W\u00f6l\u00b7fe", "pras\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und die Geier am Schmaus.", "tokens": ["Und", "die", "Gei\u00b7er", "am", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Das aber mag nicht enden,", "tokens": ["Das", "a\u00b7ber", "mag", "nicht", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis ihr dem L\u00fcgengeist", "tokens": ["Bis", "ihr", "dem", "L\u00fc\u00b7gen\u00b7geist"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Abschw\u00f6rt und von den Lenden", "tokens": ["Ab\u00b7schw\u00f6rt", "und", "von", "den", "Len\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Das Kleid der Hoffart rei\u00dft;", "tokens": ["Das", "Kleid", "der", "Hof\u00b7fart", "rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Bis ihr in Reu' vernichtet", "tokens": ["Bis", "ihr", "in", "Reu'", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "NE", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus eurem Herzeleid", "tokens": ["Aus", "eu\u00b7rem", "Her\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Herrn, der euch gerichtet,", "tokens": ["Zum", "Herrn", ",", "der", "euch", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um Gnad' und S\u00fchnung schreit.", "tokens": ["Um", "Gnad'", "und", "S\u00fch\u00b7nung", "schreit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Erst wenn aufs Knie gebogen", "tokens": ["Erst", "wenn", "aufs", "Knie", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr euch bekannt zur Schuld,", "tokens": ["Ihr", "euch", "be\u00b7kannt", "zur", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird Er der Zornflut Wogen", "tokens": ["Wird", "Er", "der", "Zorn\u00b7flut", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerrinnen lassen in Huld.", "tokens": ["Zer\u00b7rin\u00b7nen", "las\u00b7sen", "in", "Huld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Sanftleuchtend auf der Wolke", "tokens": ["Sanft\u00b7leuch\u00b7tend", "auf", "der", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mag dann der Bogen stehn,", "tokens": ["Mag", "dann", "der", "Bo\u00b7gen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und am zerschlagnen Volke", "tokens": ["Und", "am", "zer\u00b7schlag\u00b7nen", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Barmherzigkeit geschehn.", "tokens": ["Barm\u00b7her\u00b7zig\u00b7keit", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Dann mag verwandelt werden", "tokens": ["Dann", "mag", "ver\u00b7wan\u00b7delt", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "VVPP", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Schwert zum Palmenzweig,", "tokens": ["Das", "Schwert", "zum", "Pal\u00b7men\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Friede wird sein auf Erden,", "tokens": ["Und", "Frie\u00b7de", "wird", "sein", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und kommen wird das Reich.", "tokens": ["Und", "kom\u00b7men", "wird", "das", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}