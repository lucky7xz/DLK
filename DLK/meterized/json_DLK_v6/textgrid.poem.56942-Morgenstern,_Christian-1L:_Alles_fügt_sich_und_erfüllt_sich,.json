{"textgrid.poem.56942": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles f\u00fcgt sich und erf\u00fcllt sich,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles f\u00fcgt sich und erf\u00fcllt sich,", "tokens": ["Al\u00b7les", "f\u00fcgt", "sich", "und", "er\u00b7f\u00fcllt", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mu\u00dft es nur erwarten k\u00f6nnen", "tokens": ["mu\u00dft", "es", "nur", "er\u00b7war\u00b7ten", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "VMINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und dem Werden deines Gl\u00fcckes", "tokens": ["und", "dem", "Wer\u00b7den", "dei\u00b7nes", "Gl\u00fc\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jahr' und Felder reichlich g\u00f6nnen.", "tokens": ["Jahr'", "und", "Fel\u00b7der", "reich\u00b7lich", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bis du eines Tages jenen", "tokens": ["Bis", "du", "ei\u00b7nes", "Ta\u00b7ges", "je\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "reifen Duft der K\u00f6rner sp\u00fcrest", "tokens": ["rei\u00b7fen", "Duft", "der", "K\u00f6r\u00b7ner", "sp\u00fc\u00b7rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und dich aufmachst und die Ernte", "tokens": ["und", "dich", "auf\u00b7machst", "und", "die", "Ern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "in die tiefen Speicher f\u00fchrest.", "tokens": ["in", "die", "tie\u00b7fen", "Spei\u00b7cher", "f\u00fch\u00b7rest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}