{"textgrid.poem.36604": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Doris", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beneiden soll man uns, wir wollen unsre Herzen", "tokens": ["Be\u00b7nei\u00b7den", "soll", "man", "uns", ",", "wir", "wol\u00b7len", "uns\u00b7re", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PIS", "PPER", "$,", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vereinigen zu gleichem Ziel!", "tokens": ["Ver\u00b7ei\u00b7ni\u00b7gen", "zu", "glei\u00b7chem", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen gl\u00fccklich sein, wir wollen lachen, scherzen,", "tokens": ["Wir", "wol\u00b7len", "gl\u00fcck\u00b7lich", "sein", ",", "wir", "wol\u00b7len", "la\u00b7chen", ",", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$,", "PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und tanzen, aber nie zu viel!", "tokens": ["Und", "tan\u00b7zen", ",", "a\u00b7ber", "nie", "zu", "viel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ADV", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Annehmen wollen wir die Schmerzen und die Freuden,", "tokens": ["An\u00b7neh\u00b7men", "wol\u00b7len", "wir", "die", "Schmer\u00b7zen", "und", "die", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die uns der Herr des Lebens giebt,", "tokens": ["Die", "uns", "der", "Herr", "des", "Le\u00b7bens", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beneiden soll man mich! ja! mich soll man beneiden,", "tokens": ["Be\u00b7nei\u00b7den", "soll", "man", "mich", "!", "ja", "!", "mich", "soll", "man", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "PPER", "$.", "PTKANT", "$.", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mich Gl\u00fccklichen, den Doris liebt!", "tokens": ["Mich", "Gl\u00fcck\u00b7li\u00b7chen", ",", "den", "Do\u00b7ris", "liebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}