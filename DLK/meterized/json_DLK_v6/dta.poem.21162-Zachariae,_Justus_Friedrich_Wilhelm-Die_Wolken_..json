{"dta.poem.21162": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Wolken .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der bunte Wald verbl\u00fchte;", "tokens": ["Der", "bun\u00b7te", "Wald", "ver\u00b7bl\u00fch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schw\u00fcle Sonne gl\u00fchte:", "tokens": ["Die", "schw\u00fc\u00b7le", "Son\u00b7ne", "gl\u00fch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ich am k\u00fchlen Nachmittag", "tokens": ["Als", "ich", "am", "k\u00fch\u00b7len", "Nach\u00b7mit\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Schatten einer Linde lag.", "tokens": ["Jm", "Schat\u00b7ten", "ei\u00b7ner", "Lin\u00b7de", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sah ich mit Vergn\u00fcgen", "tokens": ["Da", "sah", "ich", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die leichten Wolken fliegen;", "tokens": ["Die", "leich\u00b7ten", "Wol\u00b7ken", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie flogen nach der Gegend hin,", "tokens": ["Sie", "flo\u00b7gen", "nach", "der", "Ge\u00b7gend", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In der ich oft im Geiste bin.", "tokens": ["In", "der", "ich", "oft", "im", "Geis\u00b7te", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nach welchem Himmelstheile", "tokens": ["Nach", "wel\u00b7chem", "Him\u00b7mel\u00b7sthei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fliegt ihr, wie schnelle Pfeile,", "tokens": ["Fliegt", "ihr", ",", "wie", "schnel\u00b7le", "Pfei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Rief ich der einem Wolke nach,", "tokens": ["Rief", "ich", "der", "ei\u00b7nem", "Wol\u00b7ke", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die aus der dunkeln Tiefe sprach:", "tokens": ["Die", "aus", "der", "dun\u00b7keln", "Tie\u00b7fe", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hoch \u00fcber diese H\u00fcgel", "tokens": ["Hoch", "\u00fc\u00b7ber", "die\u00b7se", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tr\u00e4gt uns des Windes Fl\u00fcgel;", "tokens": ["Tr\u00e4gt", "uns", "des", "Win\u00b7des", "Fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir kommen von dem Ocean,", "tokens": ["Wir", "kom\u00b7men", "von", "dem", "O\u00b7cean", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und laufen die bestimmte Bahn.", "tokens": ["Und", "lau\u00b7fen", "die", "be\u00b7stimm\u00b7te", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da sprach ich zu dem Kinde", "tokens": ["Da", "sprach", "ich", "zu", "dem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Meeres und der Winde:", "tokens": ["Des", "Mee\u00b7res", "und", "der", "Win\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie gl\u00fccklich ziehst du an den Ort", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "ziehst", "du", "an", "den", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von allen meinen W\u00fcnschen fort.", "tokens": ["Von", "al\u00b7len", "mei\u00b7nen", "W\u00fcn\u00b7schen", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vielleicht wirst du Selinden", "tokens": ["Viel\u00b7leicht", "wirst", "du", "Se\u00b7lin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jm heitern Garten finden,", "tokens": ["Jm", "hei\u00b7tern", "Gar\u00b7ten", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie sie, von dickem Laub besch\u00fctzt.", "tokens": ["Wie", "sie", ",", "von", "di\u00b7ckem", "Laub", "be\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An hohen Eichen einsam sitzt.", "tokens": ["An", "ho\u00b7hen", "Ei\u00b7chen", "ein\u00b7sam", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Schwebt dort auch in den L\u00fcften", "tokens": ["Schwebt", "dort", "auch", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ein Heer von schw\u00fclen D\u00fcften;", "tokens": ["Ein", "Heer", "von", "schw\u00fc\u00b7len", "D\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So m\u00e4\u00dfige der Sonne Gluth,", "tokens": ["So", "m\u00e4\u00b7\u00dfi\u00b7ge", "der", "Son\u00b7ne", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie in k\u00fchlen Schatten ruht.", "tokens": ["Da\u00df", "sie", "in", "k\u00fch\u00b7len", "Schat\u00b7ten", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}