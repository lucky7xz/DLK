{"textgrid.poem.64694": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "321. Entschuldigung", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Gallier, warum lobst du ihn nicht?", "tokens": ["Den", "Gal\u00b7lier", ",", "wa\u00b7rum", "lobst", "du", "ihn", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er kann doch so viel Gutes zeigen.", "tokens": ["Er", "kann", "doch", "so", "viel", "Gu\u00b7tes", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, doch weil er genug von seinem Lobe spricht,", "tokens": ["Ja", ",", "doch", "weil", "er", "ge\u00b7nug", "von", "sei\u00b7nem", "Lo\u00b7be", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So darf ich wohl dar\u00fcber schweigen.", "tokens": ["So", "darf", "ich", "wohl", "da\u00b7r\u00fc\u00b7ber", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}