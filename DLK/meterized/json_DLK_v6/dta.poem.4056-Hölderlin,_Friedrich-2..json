{"dta.poem.4056": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Ja! es frommet auch nicht, ihr Todesg\u00f6tter! wenn", "tokens": ["Ja", "!", "es", "from\u00b7met", "auch", "nicht", ",", "ihr", "To\u00b7des\u00b7g\u00f6t\u00b7ter", "!", "wenn"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PPOSAT", "NN", "$.", "KOUS"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "einmal", "tokens": ["ein\u00b7mal"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ihr ihn haltet, und fest habt den bezwungenen", "tokens": ["Ihr", "ihn", "hal\u00b7tet", ",", "und", "fest", "habt", "den", "be\u00b7zwun\u00b7ge\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "VVFIN", "$,", "KON", "ADJD", "VAFIN", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mann,", "tokens": ["Mann", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wenn ihr B\u00f6sen hinab in die schaurige Nacht ihn", "tokens": ["Wenn", "ihr", "B\u00f6\u00b7sen", "hin\u00b7ab", "in", "die", "schau\u00b7ri\u00b7ge", "Nacht", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "PPER"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "genommen,", "tokens": ["ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Dann zu suchen, zu flehn, oder zu z\u00fcrnen mit", "tokens": ["Dann", "zu", "su\u00b7chen", ",", "zu", "flehn", ",", "o\u00b7der", "zu", "z\u00fcr\u00b7nen", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "APPR"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "euch,", "tokens": ["euch", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Oder geduldig auch wohl im furchtsamen Banne", "tokens": ["O\u00b7der", "ge\u00b7dul\u00b7dig", "auch", "wohl", "im", "furcht\u00b7sa\u00b7men", "Ban\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "zu wohnen,", "tokens": ["zu", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Und mit L\u00e4cheln von euch h\u00f6ren das n\u00fcchterne", "tokens": ["Und", "mit", "L\u00e4\u00b7cheln", "von", "euch", "h\u00f6\u00b7ren", "das", "n\u00fcch\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "PPER", "VVFIN", "ART", "ADJA"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Lied.", "tokens": ["Lied", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Soll es seyn, so vergi\u00df dein Heil, und schlummere", "tokens": ["Soll", "es", "seyn", ",", "so", "ver\u00b7gi\u00df", "dein", "Heil", ",", "und", "schlum\u00b7me\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "VAINF", "$,", "ADV", "VVIMP", "PPOSAT", "NN", "$,", "KON", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "klanglos!", "tokens": ["klang\u00b7los", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Aber doch quillt ein Laut hoffend im Busen", "tokens": ["A\u00b7ber", "doch", "quillt", "ein", "Laut", "hof\u00b7fend", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "APPR", "ADJD", "APPRART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.16": {"text": "Dir auf,", "tokens": ["Dir", "auf", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Immer kannst Du noch nicht, o meine Seele, noch", "tokens": ["Im\u00b7mer", "kannst", "Du", "noch", "nicht", ",", "o", "mei\u00b7ne", "See\u00b7le", ",", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "$,", "FM", "PPOSAT", "NN", "$,", "ADV"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.18": {"text": "kannst Du's", "tokens": ["kannst", "Du's"], "token_info": ["word", "word"], "pos": ["VMFIN", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Nicht gewohnen, und tr\u00e4umst mitten im eisernen", "tokens": ["Nicht", "ge\u00b7woh\u00b7nen", ",", "und", "tr\u00e4umst", "mit\u00b7ten", "im", "ei\u00b7ser\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "$,", "KON", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Schlaf!", "tokens": ["Schlaf", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Festzeit hab' ich nicht, doch m\u00f6cht' ich die Locke", "tokens": ["Fest\u00b7zeit", "hab'", "ich", "nicht", ",", "doch", "m\u00f6cht'", "ich", "die", "Lo\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "$,", "ADV", "VMFIN", "PPER", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.22": {"text": "bekr\u00e4nzen;", "tokens": ["be\u00b7kr\u00e4n\u00b7zen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Bin ich allein denn nicht? aber ein Freundliches", "tokens": ["Bin", "ich", "al\u00b7lein", "denn", "nicht", "?", "a\u00b7ber", "ein", "Freund\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "$.", "ADV", "ART", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.24": {"text": "mu\u00df", "tokens": ["mu\u00df"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Fernher nahe mir seyn, und l\u00e4cheln mu\u00df ich und", "tokens": ["Fern\u00b7her", "na\u00b7he", "mir", "seyn", ",", "und", "l\u00e4\u00b7cheln", "mu\u00df", "ich", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VAINF", "$,", "KON", "VVFIN", "VMFIN", "PPER", "KON"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "staunen,", "tokens": ["stau\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Wie so selig doch auch mitten im Leide mir ist.", "tokens": ["Wie", "so", "se\u00b7lig", "doch", "auch", "mit\u00b7ten", "im", "Lei\u00b7de", "mir", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ADV", "ADV", "ADV", "APPRART", "NN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}