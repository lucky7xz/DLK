{"textgrid.poem.38194": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich den ganzen Tag", "tokens": ["Wenn", "ich", "den", "gan\u00b7zen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gef\u00fchrt hab meine Klag,", "tokens": ["Ge\u00b7f\u00fchrt", "hab", "mei\u00b7ne", "Klag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So giebts mir noch zu schaffen", "tokens": ["So", "giebts", "mir", "noch", "zu", "schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bey Nacht, wann ich soll schlafen.", "tokens": ["Bey", "Nacht", ",", "wann", "ich", "soll", "schla\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Traum mit gro\u00dfem Schrecken", "tokens": ["Ein", "Traum", "mit", "gro\u00b7\u00dfem", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Thut mich gar oft aufwecken.", "tokens": ["Thut", "mich", "gar", "oft", "auf\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im Schlaf seh ich den Schein", "tokens": ["Im", "Schlaf", "seh", "ich", "den", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Allerliebsten mein,", "tokens": ["Des", "Al\u00b7ler\u00b7liebs\u00b7ten", "mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit einem starken Bogen,", "tokens": ["Mit", "ei\u00b7nem", "star\u00b7ken", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darauf viel Pfeil gezogen,", "tokens": ["Da\u00b7rauf", "viel", "Pfeil", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Damit will er mich heben", "tokens": ["Da\u00b7mit", "will", "er", "mich", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Aus diesem schweren Leben.", "tokens": ["Aus", "die\u00b7sem", "schwe\u00b7ren", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu solchem Schreckgesicht", "tokens": ["Zu", "sol\u00b7chem", "Schreck\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kann ich stillschweigen nicht,", "tokens": ["Kann", "ich", "still\u00b7schwei\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schrei mit lauter Stimmen:", "tokens": ["Ich", "schrei", "mit", "lau\u00b7ter", "Stim\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbo Knabe la\u00df dein Grimmen,", "tokens": ["\u00bb", "o", "Kna\u00b7be", "la\u00df", "dein", "Grim\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht wollst, weil ich thu schlafen,", "tokens": ["Nicht", "wollst", ",", "weil", "ich", "thu", "schla\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "$,", "KOUS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jezt brauchen deine Waffen.\u00ab", "tokens": ["Jezt", "brau\u00b7chen", "dei\u00b7ne", "Waf\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}