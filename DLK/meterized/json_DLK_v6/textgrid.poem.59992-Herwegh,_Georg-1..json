{"textgrid.poem.59992": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1863, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit uns allen geht es ex;", "tokens": ["Mit", "uns", "al\u00b7len", "geht", "es", "ex", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIS", "VVFIN", "PPER", "FM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbtr\u00e4gst du noch so hoch den Scheitel\u00ab,", "tokens": ["\u00bb", "tr\u00e4gst", "du", "noch", "so", "hoch", "den", "Schei\u00b7tel", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "ART", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spricht ein alter Versifex,", "tokens": ["Spricht", "ein", "al\u00b7ter", "Ver\u00b7si\u00b7fex", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbunter der Sonn ist alles eitel.\u00ab", "tokens": ["\u00bb", "un\u00b7ter", "der", "Sonn", "ist", "al\u00b7les", "ei\u00b7tel", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VAFIN", "PIS", "ADJD", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Brutus, Cassius sind ex,", "tokens": ["Bru\u00b7tus", ",", "Cas\u00b7sius", "sind", "ex", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+----+", "measure": "dactylic.init"}, "line.2": {"text": "Die es einst so toll getrieben,", "tokens": ["Die", "es", "einst", "so", "toll", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich hab an meinen Rex", "tokens": ["Und", "ich", "hab", "an", "mei\u00b7nen", "Rex"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Briefe mehr geschrieben.", "tokens": ["Kei\u00b7ne", "Brie\u00b7fe", "mehr", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit dem stolzen Flug ist's ex,", "tokens": ["Mit", "dem", "stol\u00b7zen", "Flug", "ist's", "ex", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "FM", "FM", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus ist's mit den Sturmges\u00e4ngen;", "tokens": ["Aus", "ist's", "mit", "den", "Sturm\u00b7ge\u00b7s\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Leim des goldnen Drecks", "tokens": ["An", "dem", "Leim", "des", "gold\u00b7nen", "Drecks"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleiben jetzt die Spatzen h\u00e4ngen.", "tokens": ["Blei\u00b7ben", "jetzt", "die", "Spat\u00b7zen", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einer nach dem andern schleicht", "tokens": ["Ei\u00b7ner", "nach", "dem", "an\u00b7dern", "schleicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "ADJA", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich vom Tanze \u2013 die Poeten", "tokens": ["Sich", "vom", "Tan\u00b7ze", "\u2013", "die", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "APPRART", "NN", "$(", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden klug \u2013 man kann so leicht", "tokens": ["Wer\u00b7den", "klug", "\u2013", "man", "kann", "so", "leicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$(", "PIS", "VMFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Fu\u00df sich \u00fcbertreten.", "tokens": ["Ei\u00b7nen", "Fu\u00df", "sich", "\u00fc\u00b7bert\u00b7re\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Pauken und Trompetenschall", "tokens": ["Pau\u00b7ken", "und", "Trom\u00b7pe\u00b7ten\u00b7schall"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ist verstummt; nur leise, leise", "tokens": ["Ist", "ver\u00b7stummt", ";", "nur", "lei\u00b7se", ",", "lei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "VVPP", "$.", "ADV", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klingt es noch \u2013 der Karneval", "tokens": ["Klingt", "es", "noch", "\u2013", "der", "Kar\u00b7ne\u00b7val"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht zu Ende \u2013 gl\u00fcckliche Reise!", "tokens": ["Geht", "zu", "En\u00b7de", "\u2013", "gl\u00fcck\u00b7li\u00b7che", "Rei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "W\u00e4r's nur mit der vollen Kraft,", "tokens": ["W\u00e4r's", "nur", "mit", "der", "vol\u00b7len", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4r's nur mit den vollen Gluten,", "tokens": ["W\u00e4r's", "nur", "mit", "den", "vol\u00b7len", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der vollen Leidenschaft,", "tokens": ["Mit", "der", "vol\u00b7len", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df man taucht in Lethes Fluten!", "tokens": ["Da\u00df", "man", "taucht", "in", "Le\u00b7thes", "Flu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch das Leben k\u00fchlt uns ab,", "tokens": ["Doch", "das", "Le\u00b7ben", "k\u00fchlt", "uns", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Langsam, eh wir drunten liegen,", "tokens": ["Lang\u00b7sam", ",", "eh", "wir", "drun\u00b7ten", "lie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir nicht im feuchten Grab", "tokens": ["Da\u00df", "wir", "nicht", "im", "feuch\u00b7ten", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch einmal den Schnupfen kriegen.", "tokens": ["Noch", "ein\u00b7mal", "den", "Schnup\u00b7fen", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}