], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Demantpfeiler stehn \u2014", "tokens": ["Die", "De\u00b7mant\u00b7pfei\u00b7ler", "stehn"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Blitz kann sie zerschmettern,", "tokens": ["Kein", "Blitz", "kann", "sie", "zer\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Sturm sie niederwehn \u2014", "tokens": ["Kein", "Sturm", "sie", "nie\u00b7der\u00b7wehn"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fest, wie des Himmels Axe", "tokens": ["Fest", ",", "wie", "des", "Him\u00b7mels", "A\u00b7xe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Soll meine Treue stehn.", "tokens": ["Soll", "mei\u00b7ne", "Treu\u00b7e", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn jene kracht und splittert,", "tokens": ["Wenn", "je\u00b7ne", "kracht", "und", "split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mag diese untergehn.", "tokens": ["Mag", "die\u00b7se", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Auch weiss ich, meine Traute \u2014", "tokens": ["Auch", "weiss", "ich", ",", "mei\u00b7ne", "Trau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Himmelmelodey", "tokens": ["Und", "Him\u00b7mel\u00b7me\u00b7lo\u00b7dey"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entklingt dem Hochgedanken \u2014", "tokens": ["Ent\u00b7klingt", "dem", "Hoch\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich weiss, du bleibst mir treu.", "tokens": ["Ich", "weiss", ",", "du", "bleibst", "mir", "treu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl koset dir verlockend", "tokens": ["Wohl", "ko\u00b7set", "dir", "ver\u00b7lo\u00b7ckend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Schmeichler schn\u00f6der Mund;", "tokens": ["Der", "Schmeich\u00b7ler", "schn\u00f6\u00b7der", "Mund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du aber wahrst der Liebe", "tokens": ["Du", "a\u00b7ber", "wahrst", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Beschwornen Engelbund.", "tokens": ["Be\u00b7schwor\u00b7nen", "En\u00b7gel\u00b7bund", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Wohl kriechen Lotterbuben", "tokens": ["Wohl", "krie\u00b7chen", "Lot\u00b7ter\u00b7bu\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Staubleckend rund um dich,", "tokens": ["Stau\u00b7ble\u00b7ckend", "rund", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und gleissen zehnmal glatter", "tokens": ["Und", "gleis\u00b7sen", "zehn\u00b7mal", "glat\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und flimmernder, als ich.", "tokens": ["Und", "flim\u00b7mern\u00b7der", ",", "als", "ich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Trotz sey den glatten Gecken!", "tokens": ["Trotz", "sey", "den", "glat\u00b7ten", "Ge\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein J\u00fcngling, stolz und gut,", "tokens": ["Dein", "J\u00fcng\u00b7ling", ",", "stolz", "und", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein M\u00e4dchen, brav und edel,", "tokens": ["Sein", "M\u00e4d\u00b7chen", ",", "brav", "und", "e\u00b7del", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "met