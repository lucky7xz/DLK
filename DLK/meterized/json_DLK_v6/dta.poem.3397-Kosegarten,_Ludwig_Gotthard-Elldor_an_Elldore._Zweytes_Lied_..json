{"dta.poem.3397": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elldor an Elldore.  \n  Zweytes Lied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch vier und zwanzig Stunden!", "tokens": ["Noch", "vier", "und", "zwan\u00b7zig", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "KON", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So flieh' ich fern von dir;", "tokens": ["So", "flieh'", "ich", "fern", "von", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So breitest du die Arme", "tokens": ["So", "brei\u00b7test", "du", "die", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umsonst, umsonst nach mir!", "tokens": ["Um\u00b7sonst", ",", "um\u00b7sonst", "nach", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wend' auf jedem Schritte", "tokens": ["Ich", "wend'", "auf", "je\u00b7dem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den tr\u00fcben Trauerblick.", "tokens": ["Den", "tr\u00fc\u00b7ben", "Trau\u00b7er\u00b7blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es schl\u00e4gt mit jedem Schlage", "tokens": ["Es", "schl\u00e4gt", "mit", "je\u00b7dem", "Schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein sehnend Herz zur\u00fcck.", "tokens": ["Mein", "seh\u00b7nend", "Herz", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch vier und zwanzig Stunden,", "tokens": ["Noch", "vier", "und", "zwan\u00b7zig", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So schmacht' ich fern von dir,", "tokens": ["So", "schmacht'", "ich", "fern", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und breit' in leere L\u00fcfte", "tokens": ["Und", "breit'", "in", "lee\u00b7re", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Arm umsonst nach dir;", "tokens": ["Den", "Arm", "um\u00b7sonst", "nach", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach dir, mein Eins und Alles,", "tokens": ["Nach", "dir", ",", "mein", "Eins", "und", "Al\u00b7les", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "KON", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein s\u00fcsses Eigenthum,", "tokens": ["Mein", "s\u00fcs\u00b7ses", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Gram und mein Entz\u00fccken,", "tokens": ["Mein", "Gram", "und", "mein", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Preis, mein Lied, mein Ruhm!", "tokens": ["Mein", "Preis", ",", "mein", "Lied", ",", "mein", "Ruhm", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verschwunden sind, verschwunden,", "tokens": ["Ver\u00b7schwun\u00b7den", "sind", ",", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich einer Sommernacht,", "tokens": ["Gleich", "ei\u00b7ner", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die goldbes\u00e4umten Tage,", "tokens": ["Die", "gold\u00b7be\u00b7s\u00e4um\u00b7ten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ich mit dir vollbracht.", "tokens": ["Die", "ich", "mit", "dir", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Stunden, ach! des Habens", "tokens": ["Die", "Stun\u00b7den", ",", "ach", "!", "des", "Ha\u00b7bens"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ITJ", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gehn raschen J\u00fcnglingsgang.", "tokens": ["Gehn", "ra\u00b7schen", "J\u00fcng\u00b7lings\u00b7gang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die Stunden des Entbehrens", "tokens": ["Die", "Stun\u00b7den", "des", "Ent\u00b7beh\u00b7rens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verschleichen lahm und krank.", "tokens": ["Ver\u00b7schlei\u00b7chen", "lahm", "und", "krank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Lass, lass die Zeit mich klagen,", "tokens": ["Lass", ",", "lass", "die", "Zeit", "mich", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deren raschen Flug", "tokens": ["In", "de\u00b7ren", "ra\u00b7schen", "Flug"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So innig und so selig", "tokens": ["So", "in\u00b7nig", "und", "so", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz an deinem schlug;", "tokens": ["Mein", "Herz", "an", "dei\u00b7nem", "schlug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo ich so liebem\u00fcde", "tokens": ["Wo", "ich", "so", "lie\u00b7be\u00b7m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An deinen Busen sank,", "tokens": ["An", "dei\u00b7nen", "Bu\u00b7sen", "sank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ewgen Lebens Wonnen", "tokens": ["Und", "ew\u00b7gen", "Le\u00b7bens", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus deinen Lippen trank.", "tokens": ["Aus", "dei\u00b7nen", "Lip\u00b7pen", "trank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Lass, lass mich um sie klagen!", "tokens": ["Lass", ",", "lass", "mich", "um", "sie", "kla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Liebeswein berauscht,", "tokens": ["Von", "Lie\u00b7bes\u00b7wein", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00e4tt' ich um Edens Freuden", "tokens": ["H\u00e4tt'", "ich", "um", "E\u00b7dens", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schnellen nicht vertauscht.", "tokens": ["Die", "Schnel\u00b7len", "nicht", "ver\u00b7tauscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie sind, sie sind verschwunden.", "tokens": ["Sie", "sind", ",", "sie", "sind", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie flogen Adlerflug \u2014", "tokens": ["Sie", "flo\u00b7gen", "Ad\u00b7ler\u00b7flug"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Trau nicht der Erde Schw\u00fcren;", "tokens": ["Trau", "nicht", "der", "Er\u00b7de", "Schw\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ihr Schwur ist Lug und Trug.", "tokens": ["Ihr", "Schwur", "ist", "Lug", "und", "Trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ist alles Trug hienieden?", "tokens": ["Ist", "al\u00b7les", "Trug", "hien\u00b7ie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und alles Tand und Traum?", "tokens": ["Und", "al\u00b7les", "Tand", "und", "Traum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alles luft'ger Schatten,", "tokens": ["Und", "al\u00b7les", "luft'\u00b7ger", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und leichter Wasserschaum?", "tokens": ["Und", "leich\u00b7ter", "Was\u00b7ser\u00b7schaum", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl ist es Wein und Wollust,", "tokens": ["Wohl", "ist", "es", "Wein", "und", "Wol\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohl ist es Gold und Ruhm.", "tokens": ["Wohl", "ist", "es", "Gold", "und", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nur du verbl\u00fchest nimmer,", "tokens": ["Nur", "du", "ver\u00b7bl\u00fc\u00b7hest", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Lieb' Elisium.", "tokens": ["Der", "Lieb'", "E\u00b7li\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Elisium der Liebe,", "tokens": ["E\u00b7li\u00b7si\u00b7um", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Du, du betrogst mich nicht.", "tokens": ["Du", ",", "du", "be\u00b7trogst", "mich", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Elldore l\u00e4chelt. Pl\u00f6tzlich", "tokens": ["Ell\u00b7do\u00b7re", "l\u00e4\u00b7chelt", ".", "Pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "$.", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umstr\u00f6mt mich gl\u00e4nzend Licht.", "tokens": ["Um\u00b7str\u00f6mt", "mich", "gl\u00e4n\u00b7zend", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gelehnt an ihren Busen", "tokens": ["Ge\u00b7lehnt", "an", "ih\u00b7ren", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verlern' ich Grimm und Gram.", "tokens": ["Ver\u00b7lern'", "ich", "Grimm", "und", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es wird in ihren Armen", "tokens": ["Es", "wird", "in", "ih\u00b7ren", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der L\u00f6we l\u00e4mmchenzahm.", "tokens": ["Der", "L\u00f6\u00b7we", "l\u00e4mm\u00b7chen\u00b7zahm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein Nick nur von der Holden,", "tokens": ["Ein", "Nick", "nur", "von", "der", "Hol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Wink nur, der mich meint;", "tokens": ["Ein", "Wink", "nur", ",", "der", "mich", "meint", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und keines Schicksals T\u00fccke", "tokens": ["Und", "kei\u00b7nes", "Schick\u00b7sals", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schreckt, Huldin, deinen Freund.", "tokens": ["Schreckt", ",", "Hul\u00b7din", ",", "dei\u00b7nen", "Freund", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Augenblick nur Ruhens", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", "nur", "Ru\u00b7hens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In deinem sanften Schooss,", "tokens": ["In", "dei\u00b7nem", "sanf\u00b7ten", "Scho\u00b7oss", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und ich werd' alles Rasens", "tokens": ["Und", "ich", "werd'", "al\u00b7les", "Ra\u00b7sens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und alles St\u00fcrmens los.", "tokens": ["Und", "al\u00b7les", "St\u00fcr\u00b7mens", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN