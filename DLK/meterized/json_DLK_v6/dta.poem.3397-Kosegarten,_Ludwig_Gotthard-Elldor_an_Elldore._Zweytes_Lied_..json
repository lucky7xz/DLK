": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was thuts? mein Kleinod bleibet,", "tokens": ["Was", "thuts", "?", "mein", "Klei\u00b7nod", "blei\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Elldore bleibet mir.", "tokens": ["Ell\u00b7do\u00b7re", "blei\u00b7bet", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Triumph! ich kehre wieder,", "tokens": ["Tri\u00b7umph", "!", "ich", "keh\u00b7re", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und treu, und keusch und rein,", "tokens": ["Und", "treu", ",", "und", "keusch", "und", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schliess' ich in heissen Armen", "tokens": ["Schliess'", "ich", "in", "heis\u00b7sen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Elldoren wieder ein.", "tokens": ["Ell\u00b7do\u00b7ren", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Triumph! auf reinen Lippen", "tokens": ["Tri\u00b7umph", "!", "auf", "rei\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Versiegeln wir den Bund.", "tokens": ["Ver\u00b7sie\u00b7geln", "wir", "den", "Bund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kein Bubenkuss entweihte", "tokens": ["Kein", "Bu\u00b7ben\u00b7kuss", "ent\u00b7weih\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den frischen Rosenmund.", "tokens": ["Den", "fri\u00b7schen", "Ro\u00b7sen\u00b7mund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Was drohst du, Abschiedsstunde,", "tokens": ["Was", "drohst", "du", ",", "Ab\u00b7schieds\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So grass? Dein Dolch ist stumpf.", "tokens": ["So", "grass", "?", "Dein", "Dolch", "ist", "stumpf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Treue goldner Panzer", "tokens": ["Der", "Treu\u00b7e", "gold\u00b7ner", "Pan\u00b7zer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beschirmet uns. Triumph!", "tokens": ["Be\u00b7schir\u00b7met", "uns", ".", "Tri\u00b7umph", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Triumph! Ich kehre wieder.", "tokens": ["Tri\u00b7umph", "!", "Ich", "keh\u00b7re", "wie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Treuen, trauert nicht!", "tokens": ["Ihr", "Treu\u00b7en", ",", "trau\u00b7ert", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Triumph! die Treue sieget!", "tokens": ["Tri\u00b7umph", "!", "die", "Treu\u00b7e", "sie\u00b7get", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Elldore, traure nicht!", "tokens": ["Ell\u00b7do\u00b7re", ",", "trau\u00b7re", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}