{"textgrid.poem.61231": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, in diesen blauen Tagen,", "tokens": ["Ach", ",", "in", "die\u00b7sen", "blau\u00b7en", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so licht und sonnig flie\u00dfen,", "tokens": ["Die", "so", "licht", "und", "son\u00b7nig", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch ein inniges Genie\u00dfen,", "tokens": ["Welch", "ein", "in\u00b7ni\u00b7ges", "Ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Welche stillverkl\u00e4rte Ruh'!", "tokens": ["Wel\u00b7che", "still\u00b7ver\u00b7kl\u00e4r\u00b7te", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heiter ist das Blut gez\u00fcgelt,", "tokens": ["Hei\u00b7ter", "ist", "das", "Blut", "ge\u00b7z\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leichter Schlaf und klarer Morgen", "tokens": ["Leich\u00b7ter", "Schlaf", "und", "kla\u00b7rer", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wissen nichts von bangen Sorgen,", "tokens": ["Wis\u00b7sen", "nichts", "von", "ban\u00b7gen", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Seele schweift befl\u00fcgelt", "tokens": ["Und", "die", "See\u00b7le", "schweift", "be\u00b7fl\u00fc\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jeder lieben Stelle zu.", "tokens": ["Je\u00b7der", "lie\u00b7ben", "Stel\u00b7le", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, in diesen blauen Tagen,", "tokens": ["Ach", ",", "in", "die\u00b7sen", "blau\u00b7en", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die wie Wellen so gelinde", "tokens": ["Die", "wie", "Wel\u00b7len", "so", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "NN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich ins Leben weiter tragen,", "tokens": ["Mich", "ins", "Le\u00b7ben", "wei\u00b7ter", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ich hoffen, mu\u00df ich fragen,", "tokens": ["Mu\u00df", "ich", "hof\u00b7fen", ",", "mu\u00df", "ich", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ich nie dich wiederfinde,", "tokens": ["Ob", "ich", "nie", "dich", "wie\u00b7der\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebling meiner Seele du!", "tokens": ["Lieb\u00b7ling", "mei\u00b7ner", "See\u00b7le", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}