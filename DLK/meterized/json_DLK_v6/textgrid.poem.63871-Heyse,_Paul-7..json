{"textgrid.poem.63871": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und so wanderten wir Beide weite Strecken Hand in Hand,", "tokens": ["Und", "so", "wan\u00b7der\u00b7ten", "wir", "Bei\u00b7de", "wei\u00b7te", "Stre\u00b7cken", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hielten treu in Lust und Leide Schritt mitsammen Hand in Hand,", "tokens": ["Hiel\u00b7ten", "treu", "in", "Lust", "und", "Lei\u00b7de", "Schritt", "mit\u00b7sam\u00b7men", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "NN", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Hatten, wie es gl\u00fcckverw\u00f6hnten Sterblichen von je geschehn,", "tokens": ["Hat\u00b7ten", ",", "wie", "es", "gl\u00fcck\u00b7ver\u00b7w\u00f6hn\u00b7ten", "Sterb\u00b7li\u00b7chen", "von", "je", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "ADJA", "NN", "APPR", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Auch ein Lied vom G\u00f6tterneide oft zu singen Hand in Hand.", "tokens": ["Auch", "ein", "Lied", "vom", "G\u00f6t\u00b7ter\u00b7nei\u00b7de", "oft", "zu", "sin\u00b7gen", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "ADV", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Doch ich tat es sonder Klagen, da, so hart die Pr\u00fcfung war,", "tokens": ["Doch", "ich", "tat", "es", "son\u00b7der", "Kla\u00b7gen", ",", "da", ",", "so", "hart", "die", "Pr\u00fc\u00b7fung", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,", "KOUS", "$,", "ADV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Meine Aug- und Seelenweide, du mir nah bliebst Hand in Hand.", "tokens": ["Mei\u00b7ne", "Aug", "und", "See\u00b7len\u00b7wei\u00b7de", ",", "du", "mir", "nah", "bliebst", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "NN", "$,", "PPER", "PPER", "ADJD", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Eins nur fleh' ich vom Geschicke: wenn die strenge Parze naht,", "tokens": ["Eins", "nur", "fleh'", "ich", "vom", "Ge\u00b7schi\u00b7cke", ":", "wenn", "die", "stren\u00b7ge", "Par\u00b7ze", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Die den Faden uns zerschneide, da\u00df wir scheiden Hand in Hand.", "tokens": ["Die", "den", "Fa\u00b7den", "uns", "zer\u00b7schnei\u00b7de", ",", "da\u00df", "wir", "schei\u00b7den", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Denn wie sollt' ich einsam wandeln auf der Erde Dornenpfad,", "tokens": ["Denn", "wie", "sollt'", "ich", "ein\u00b7sam", "wan\u00b7deln", "auf", "der", "Er\u00b7de", "Dor\u00b7nen\u00b7pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Oder du im Witwenkleide, ach, und nicht mehr Hand in Hand?", "tokens": ["O\u00b7der", "du", "im", "Wit\u00b7wen\u00b7klei\u00b7de", ",", "ach", ",", "und", "nicht", "mehr", "Hand", "in", "Hand", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$,", "ITJ", "$,", "KON", "PTKNEG", "ADV", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}