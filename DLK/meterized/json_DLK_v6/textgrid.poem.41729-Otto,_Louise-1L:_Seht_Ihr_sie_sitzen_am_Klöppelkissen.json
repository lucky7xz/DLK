{"textgrid.poem.41729": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht Ihr sie sitzen am Kl\u00f6ppelkissen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht Ihr sie sitzen am Kl\u00f6ppelkissen", "tokens": ["Seht", "Ihr", "sie", "sit\u00b7zen", "am", "Kl\u00f6p\u00b7pel\u00b7kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Wangen bleich und die Augen rot!", "tokens": ["Die", "Wan\u00b7gen", "bleich", "und", "die", "Au\u00b7gen", "rot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie m\u00fchen sich ab f\u00fcr einen Bissen,", "tokens": ["Sie", "m\u00fc\u00b7hen", "sich", "ab", "f\u00fcr", "ei\u00b7nen", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fcr einen Bissen schwarzes Brot!", "tokens": ["F\u00fcr", "ei\u00b7nen", "Bis\u00b7sen", "schwar\u00b7zes", "Brot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gro\u00dfmutter hat sich die Augen erblindet,", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "hat", "sich", "die", "Au\u00b7gen", "er\u00b7blin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie wartet, bis sie der Tod befreit \u2013", "tokens": ["Sie", "war\u00b7tet", ",", "bis", "sie", "der", "Tod", "be\u00b7freit", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im stillen Gebet sie die H\u00e4nde windet:", "tokens": ["Im", "stil\u00b7len", "Ge\u00b7bet", "sie", "die", "H\u00e4n\u00b7de", "win\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gott sch\u00fctz' uns in der schweren Zeit.", "tokens": ["Gott", "sch\u00fctz'", "uns", "in", "der", "schwe\u00b7ren", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Kinder regen die kleinen H\u00e4nde,", "tokens": ["Die", "Kin\u00b7der", "re\u00b7gen", "die", "klei\u00b7nen", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Kl\u00f6ppel fliegen hinab, hinauf,", "tokens": ["Die", "Kl\u00f6p\u00b7pel", "flie\u00b7gen", "hin\u00b7ab", ",", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der M\u00fch' und Sorge kein Ende, keine Ende!", "tokens": ["Der", "M\u00fch'", "und", "Sor\u00b7ge", "kein", "En\u00b7de", ",", "kei\u00b7ne", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das ist ihr k\u00fcnftiger Lebenslauf.", "tokens": ["Das", "ist", "ihr", "k\u00fcnf\u00b7ti\u00b7ger", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Jungfrauen all, da\u00df Gott sich erbarme,", "tokens": ["Die", "Jung\u00b7frau\u00b7en", "all", ",", "da\u00df", "Gott", "sich", "er\u00b7bar\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$,", "KOUS", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie ahnen nimmer der Jugend Lust \u2013", "tokens": ["Sie", "ah\u00b7nen", "nim\u00b7mer", "der", "Ju\u00b7gend", "Lust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Elend schlie\u00dft sie in seine Arme,", "tokens": ["Das", "E\u00b7lend", "schlie\u00dft", "sie", "in", "sei\u00b7ne", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Mangel schmiegt sich an ihre Brust.", "tokens": ["Der", "Man\u00b7gel", "schmiegt", "sich", "an", "ih\u00b7re", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Seht Ihr sie sitzen am Kl\u00f6ppelkissen,", "tokens": ["Seht", "Ihr", "sie", "sit\u00b7zen", "am", "Kl\u00f6p\u00b7pel\u00b7kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seht Ihr die Spitzen, die sie gewebt:", "tokens": ["Seht", "Ihr", "die", "Spit\u00b7zen", ",", "die", "sie", "ge\u00b7webt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr Reichen, Gro\u00dfen \u2013 hat das Gewissen", "tokens": ["Ihr", "Rei\u00b7chen", ",", "Gro\u00b7\u00dfen", "\u2013", "hat", "das", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$(", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Euch nie in der innersten Seele gebebt?", "tokens": ["Euch", "nie", "in", "der", "in\u00b7ners\u00b7ten", "See\u00b7le", "ge\u00b7bebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Ihr schwelgt und prasset, wo sie verderben,", "tokens": ["Ihr", "schwelgt", "und", "pras\u00b7set", ",", "wo", "sie", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Genie\u00dft das Leben in Saus und Braus,", "tokens": ["Ge\u00b7nie\u00dft", "das", "Le\u00b7ben", "in", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Indessen sie vor Hunger sterben,", "tokens": ["In\u00b7des\u00b7sen", "sie", "vor", "Hun\u00b7ger", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott dankend, da\u00df die Qual nun aus!", "tokens": ["Gott", "dan\u00b7kend", ",", "da\u00df", "die", "Qual", "nun", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KOUS", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Seht Ihr sie sitzen am Kl\u00f6ppelkissen", "tokens": ["Seht", "Ihr", "sie", "sit\u00b7zen", "am", "Kl\u00f6p\u00b7pel\u00b7kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und redet noch sch\u00f6n von Gottvertraun?", "tokens": ["Und", "re\u00b7det", "noch", "sch\u00f6n", "von", "Gott\u00b7ver\u00b7traun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr habt es aus ihrer Seele gerissen,", "tokens": ["Ihr", "habt", "es", "aus", "ih\u00b7rer", "See\u00b7le", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weil sie Euch selber gottlos schaun!", "tokens": ["Weil", "sie", "Euch", "sel\u00b7ber", "gott\u00b7los", "schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Seht Ihr sie sitzen am Kl\u00f6ppelkissen", "tokens": ["Seht", "Ihr", "sie", "sit\u00b7zen", "am", "Kl\u00f6p\u00b7pel\u00b7kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und f\u00fchlt kein Erbarmen in solcher Zeit,", "tokens": ["Und", "f\u00fchlt", "kein", "Er\u00b7bar\u00b7men", "in", "sol\u00b7cher", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dann werde Euer Sterbekissen", "tokens": ["Dann", "wer\u00b7de", "Eu\u00b7er", "Ster\u00b7be\u00b7kis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Armut Fluch und all ihr Leid!", "tokens": ["Der", "Ar\u00b7mut", "Fluch", "und", "all", "ihr", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}