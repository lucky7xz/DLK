{"dta.poem.12908": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien, als sie seine klagen  \n nicht anh\u00f6ren wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje wei\u00df dein mund zu klagen,", "tokens": ["Wie", "wei\u00df", "dein", "mund", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn irgend eine bien\u2019 ihm einen stich versetzt;", "tokens": ["Wenn", "ir\u00b7gend", "ei\u00b7ne", "bi\u00b7en'", "ihm", "ei\u00b7nen", "stich", "ver\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "APPR", "PPER", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Allein wenn Sylvia die hertzen selbst verletzt,", "tokens": ["Al\u00b7lein", "wenn", "Syl\u00b7via", "die", "hert\u00b7zen", "selbst", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NE", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da hat kein mensch ein wort zu sagen.", "tokens": ["Da", "hat", "kein", "mensch", "ein", "wort", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}