{"textgrid.poem.41571": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am zweiundzwanzigsten Sonntage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sonnenstrahl, ein goldner Spie\u00df,", "tokens": ["Der", "Son\u00b7nen\u00b7strahl", ",", "ein", "gold\u00b7ner", "Spie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Prallt von des Sees kristallnen Fl\u00e4chen", "tokens": ["Prallt", "von", "des", "Sees", "kris\u00b7tall\u00b7nen", "Fl\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und fahrend gen den Marmorflies", "tokens": ["Und", "fah\u00b7rend", "gen", "den", "Mar\u00b7mor\u00b7flies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Palastes Mauern will durchstechen.", "tokens": ["Pa\u00b7las\u00b7tes", "Mau\u00b7ern", "will", "durch\u00b7ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf seidnen Polstern windet sich,", "tokens": ["Auf", "seid\u00b7nen", "Pols\u00b7tern", "win\u00b7det", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die magern \u00c4rmchen ringt das Kind;", "tokens": ["Die", "ma\u00b7gern", "\u00c4rm\u00b7chen", "ringt", "das", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und eine Tr\u00e4ne bitterlich", "tokens": ["Und", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "bit\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch m\u00f6chte aus dem Auge lind,", "tokens": ["Noch", "m\u00f6ch\u00b7te", "aus", "dem", "Au\u00b7ge", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dem halberstarrten, brechen.", "tokens": ["Dem", "hal\u00b7ber\u00b7starr\u00b7ten", ",", "bre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schon hat der Tod die Hand gelegt", "tokens": ["Schon", "hat", "der", "Tod", "die", "Hand", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seine Beute ohn' Erbarmen;", "tokens": ["Auf", "sei\u00b7ne", "Beu\u00b7te", "ohn'", "Er\u00b7bar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ob er Eis zum Herzen tr\u00e4gt:", "tokens": ["Doch", "ob", "er", "Eis", "zum", "Her\u00b7zen", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch schmilzt im Blutstrom es, dem warmen.", "tokens": ["Noch", "schmilzt", "im", "Bluts\u00b7trom", "es", ",", "dem", "war\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O Jugend! Jugend! wie so fest", "tokens": ["O", "Ju\u00b7gend", "!", "Ju\u00b7gend", "!", "wie", "so", "fest"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "NN", "$.", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hast du verstrickt das Leben dir,", "tokens": ["Hast", "du", "ver\u00b7strickt", "das", "Le\u00b7ben", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie sich das Schlinggew\u00e4chse pre\u00dft", "tokens": ["Wie", "sich", "das", "Schling\u00b7ge\u00b7w\u00e4ch\u00b7se", "pre\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Wurzeln dort und Fasern hier,", "tokens": ["Mit", "Wur\u00b7zeln", "dort", "und", "Fa\u00b7sern", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als vielen tausend Armen.", "tokens": ["Als", "vie\u00b7len", "tau\u00b7send", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Anblick, st\u00e4rker als ein Weib,", "tokens": ["O", "An\u00b7blick", ",", "st\u00e4r\u00b7ker", "als", "ein", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wachen, Angst und Kummer nagen!", "tokens": ["Das", "Wa\u00b7chen", ",", "Angst", "und", "Kum\u00b7mer", "na\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bet\u00e4ubt und schwer, gleich totem Leib,", "tokens": ["Be\u00b7t\u00e4ubt", "und", "schwer", ",", "gleich", "to\u00b7tem", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat man die F\u00fcrstin fortgetragen.", "tokens": ["Hat", "man", "die", "F\u00fcrs\u00b7tin", "fort\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Noch weilt der Vater, wenn ein Sklav'", "tokens": ["Noch", "weilt", "der", "Va\u00b7ter", ",", "wenn", "ein", "Skla\u00b7v'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Bornes frische Labung reicht,", "tokens": ["Des", "Bor\u00b7nes", "fri\u00b7sche", "La\u00b7bung", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit zitternd kalter Hand den Schlaf", "tokens": ["Mit", "zit\u00b7ternd", "kal\u00b7ter", "Hand", "den", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Kindes streicht er sacht und feucht", "tokens": ["Des", "Kin\u00b7des", "streicht", "er", "sacht", "und", "feucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und fl\u00fcstert leise Fragen.", "tokens": ["Und", "fl\u00fcs\u00b7tert", "lei\u00b7se", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer wagt sich an des F\u00fcrsten Ohr?", "tokens": ["Wer", "wagt", "sich", "an", "des", "F\u00fcrs\u00b7ten", "Ohr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Menipp, der J\u00fcngling aus Eub\u00f6a.", "tokens": ["Me\u00b7nipp", ",", "der", "J\u00fcng\u00b7ling", "aus", "Eu\u00b7b\u00f6a", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbherr,\u00ab keucht er, \u00bbhebt den Blick empor!", "tokens": ["\u00bb", "herr", ",", "\u00ab", "keucht", "er", ",", "\u00bb", "hebt", "den", "Blick", "em\u00b7por", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Herr, der Prophete aus Jud\u00e4a,", "tokens": ["Herr", ",", "der", "Pro\u00b7phe\u00b7te", "aus", "Ju\u00b7d\u00e4a", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Von dem das ganze Land erf\u00fcllt,", "tokens": ["Von", "dem", "das", "gan\u00b7ze", "Land", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er k\u00f6mmt, er naht Kapharnaum;", "tokens": ["Er", "k\u00f6mmt", ",", "er", "naht", "Ka\u00b7phar\u00b7naum", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und wie aus hundert Adern quillt", "tokens": ["Und", "wie", "aus", "hun\u00b7dert", "A\u00b7dern", "quillt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Entgegen ihm und nach und um", "tokens": ["Ent\u00b7ge\u00b7gen", "ihm", "und", "nach", "und", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "APPR", "KON", "KOUI"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Glutstrom Galil\u00e4a.\u00ab", "tokens": ["Ein", "Glut\u00b7strom", "Ga\u00b7li\u00b7l\u00e4a", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbsind denn die alten G\u00f6tter tot,", "tokens": ["\u00bb", "sind", "denn", "die", "al\u00b7ten", "G\u00f6t\u00b7ter", "tot", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So m\u00fcssen wir die neuen wahren.", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", "die", "neu\u00b7en", "wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es sei, es sei und meine Not", "tokens": ["Es", "sei", ",", "es", "sei", "und", "mei\u00b7ne", "Not"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag sich dem Volke offenbaren!\u00ab", "tokens": ["Mag", "sich", "dem", "Vol\u00b7ke", "of\u00b7fen\u00b7ba\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Rosse stampfen, einmal schaut", "tokens": ["Die", "Ros\u00b7se", "stamp\u00b7fen", ",", "ein\u00b7mal", "schaut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Vater auf sein sterbend Kind,", "tokens": ["Der", "Va\u00b7ter", "auf", "sein", "ster\u00b7bend", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nun voran! \u00bbWas rauscht so laut,", "tokens": ["Und", "nun", "vo\u00b7ran", "!", "\u00bb", "Was", "rauscht", "so", "laut", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$.", "$(", "PWS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was streicht am Berge wie ein Wind?\u00ab", "tokens": ["Was", "streicht", "am", "Ber\u00b7ge", "wie", "ein", "Wind", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbherr! des Propheten Scharen!\u00ab", "tokens": ["\u00bb", "herr", "!", "des", "Pro\u00b7phe\u00b7ten", "Scha\u00b7ren", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "ART", "NN", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "O wie die Angst den Stolz zerbricht!", "tokens": ["O", "wie", "die", "Angst", "den", "Stolz", "zer\u00b7bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem\u00fctig, zitternd als zur Frone,", "tokens": ["De\u00b7m\u00fc\u00b7tig", ",", "zit\u00b7ternd", "als", "zur", "Fro\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er wei\u00df es nicht, zu wem er spricht,", "tokens": ["Er", "wei\u00df", "es", "nicht", ",", "zu", "wem", "er", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wie der Sklave vor dem Throne", "tokens": ["Doch", "wie", "der", "Skla\u00b7ve", "vor", "dem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gebrochen steht der reiche Mann.", "tokens": ["Ge\u00b7bro\u00b7chen", "steht", "der", "rei\u00b7che", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die bleiche Lippe zuckt vor Schmerz,", "tokens": ["Die", "blei\u00b7che", "Lip\u00b7pe", "zuckt", "vor", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und hei\u00dfer, als das Wort es kann,", "tokens": ["Und", "hei\u00b7\u00dfer", ",", "als", "das", "Wort", "es", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "KOUS", "ART", "NN", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Viel hei\u00dfer fleht das bange Herz:", "tokens": ["Viel", "hei\u00b7\u00dfer", "fleht", "das", "ban\u00b7ge", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbhilf Rabbi meinem Sohne!\u00ab", "tokens": ["\u00bb", "hilf", "Rab\u00b7bi", "mei\u00b7nem", "Soh\u00b7ne", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "NN", "PPOSAT", "NN", "