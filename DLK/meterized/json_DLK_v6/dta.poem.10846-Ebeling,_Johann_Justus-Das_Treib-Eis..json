{"dta.poem.10846": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das Treib-Eis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ein von dichten Eis bebr\u00fckt und auf-\ngeth\u00fcrmter Wasserflus,", "tokens": ["Wenn", "ein", "von", "dich\u00b7ten", "Eis", "be\u00b7br\u00fckt", "und", "auf", "ge\u00b7th\u00fcrm\u00b7ter", "Was\u00b7ser\u00b7flus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "ADJA", "NN", "VVFIN", "KON", "TRUNC", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Von warmen Westenwind behaucht,", "tokens": ["Von", "war\u00b7men", "Wes\u00b7ten\u00b7wind", "be\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Schollen wie zerst\u00fckte Spiegel, in Wirbel-", "tokens": ["Und", "Schol\u00b7len", "wie", "zer\u00b7st\u00fck\u00b7te", "Spie\u00b7gel", ",", "in", "Wir\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "KOKOM", "ADJA", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So kan man nicht ohn innres Grauen die schroffen", "tokens": ["So", "kan", "man", "nicht", "ohn", "inn\u00b7res", "Grau\u00b7en", "die", "schrof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "PTKNEG", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Es scheinet offt als wenn ein Berg sich in der regen", "tokens": ["Es", "schei\u00b7net", "offt", "als", "wenn", "ein", "Berg", "sich", "in", "der", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "KOUS", "ART", "NN", "PRF", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jm Rollen immer kleiner wird, und eh mans meinet", "tokens": ["Jm", "Rol\u00b7len", "im\u00b7mer", "klei\u00b7ner", "wird", ",", "und", "eh", "mans", "mei\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VAFIN", "$,", "KON", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man siehet offt noch eine L\u00e4nge, wo diese Br\u00fckke", "tokens": ["Man", "sie\u00b7het", "offt", "noch", "ei\u00b7ne", "L\u00e4n\u00b7ge", ",", "wo", "die\u00b7se", "Br\u00fck\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ART", "NN", "$,", "PWAV", "PDAT", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Allein wer sich darauf verl\u00e4st und fest zustehen gleich-", "tokens": ["Al\u00b7lein", "wer", "sich", "da\u00b7rauf", "ver\u00b7l\u00e4st", "und", "fest", "zu\u00b7ste\u00b7hen", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "PRF", "PAV", "VVFIN", "KON", "ADJD", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Der glitschet mit zum Abgrund fort; weil ihn die", "tokens": ["Der", "glit\u00b7schet", "mit", "zum", "Ab\u00b7grund", "fort", ";", "weil", "ihn", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "APPRART", "NN", "PTKVZ", "$.", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die wenn sie berstet auch zerflie\u00dft, nicht l\u00e4nger mehr", "tokens": ["Die", "wenn", "sie", "bers\u00b7tet", "auch", "zer\u00b7flie\u00dft", ",", "nicht", "l\u00e4n\u00b7ger", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "KOUS", "PPER", "VVFIN", "ADV", "VVFIN", "$,", "PTKNEG", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das Treib-Eis zeiget uns im Bilde. Wes geht", "tokens": ["Das", "Treib\u00b7Eis", "zei\u00b7get", "uns", "im", "Bil\u00b7de", ".", "Wes", "geht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$.", "NE", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wer sich darauf getrost verl\u00e4sset, der sp\u00fcrt am ersten", "tokens": ["Wer", "sich", "da\u00b7rauf", "ge\u00b7trost", "ver\u00b7l\u00e4s\u00b7set", ",", "der", "sp\u00fcrt", "am", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "PAV", "VVPP", "VVFIN", "$,", "PRELS", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}