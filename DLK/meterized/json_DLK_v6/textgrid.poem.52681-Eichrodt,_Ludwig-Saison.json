{"textgrid.poem.52681": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Saison", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fch dunkelt die Welt. Was l\u00e4\u00dft sich erkl\u00fcgeln?", "tokens": ["Fr\u00fch", "dun\u00b7kelt", "die", "Welt", ".", "Was", "l\u00e4\u00dft", "sich", "er\u00b7kl\u00fc\u00b7geln", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "PWS", "VVFIN", "PRF", "VVINF", "$."], "meter": "++--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Was t\u00f6dtet den Abend, was ist pikant?", "tokens": ["Was", "t\u00f6d\u00b7tet", "den", "A\u00b7bend", ",", "was", "ist", "pi\u00b7kant", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PWS", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Von den Wolkenh\u00f6hn, von den Nebelh\u00fcgeln", "tokens": ["Von", "den", "Wol\u00b7ken\u00b7h\u00f6hn", ",", "von", "den", "Ne\u00b7bel\u00b7h\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wirbelt der Schnee ins offene Land.", "tokens": ["Wir\u00b7belt", "der", "Schnee", "ins", "of\u00b7fe\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ha, gl\u00e4nzende Bahn! Durch Stra\u00dfen und Pl\u00e4tze", "tokens": ["Ha", ",", "gl\u00e4n\u00b7zen\u00b7de", "Bahn", "!", "Durch", "Stra\u00b7\u00dfen", "und", "Pl\u00e4t\u00b7ze"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJA", "NN", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Rauschet die g\u00f6ttliche Schlittenfahrt.", "tokens": ["Rau\u00b7schet", "die", "g\u00f6tt\u00b7li\u00b7che", "Schlit\u00b7ten\u00b7fahrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Ein Jauchzen und Schellen, wildfliegende Hetze,", "tokens": ["Ein", "Jauch\u00b7zen", "und", "Schel\u00b7len", ",", "wild\u00b7flie\u00b7gen\u00b7de", "Het\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Klingen und Knallen barbarisch gepaart!", "tokens": ["Klin\u00b7gen", "und", "Knal\u00b7len", "bar\u00b7ba\u00b7risch", "ge\u00b7paart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Keck wirft sich hinein in die gaffende Menge", "tokens": ["Keck", "wirft", "sich", "hin\u00b7ein", "in", "die", "gaf\u00b7fen\u00b7de", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APZR", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Fackeln rother lodernder Schein,", "tokens": ["Der", "Fa\u00b7ckeln", "ro\u00b7ther", "lo\u00b7dern\u00b7der", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Musik erbraust in das dunkle Gedr\u00e4nge", "tokens": ["Mu\u00b7sik", "er\u00b7braust", "in", "das", "dunk\u00b7le", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Und Rossewiehern hintendrein.", "tokens": ["Und", "Ros\u00b7se\u00b7wie\u00b7hern", "hin\u00b7ten\u00b7drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Halloh! Du f\u00fcrstliche Augenweide,", "tokens": ["Hal\u00b7loh", "!", "Du", "f\u00fcrst\u00b7li\u00b7che", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Du trunkene, du elegante Welt,", "tokens": ["Du", "trun\u00b7ke\u00b7ne", ",", "du", "e\u00b7leg\u00b7an\u00b7te", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vor\u00fcbersaus in Pelz und in Seide,", "tokens": ["Vor\u00b7\u00fc\u00b7bers\u00b7aus", "in", "Pelz", "und", "in", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und spotte der K\u00e4lte, die \u2013 frisch erh\u00e4lt!", "tokens": ["Und", "spot\u00b7te", "der", "K\u00e4l\u00b7te", ",", "die", "\u2013", "frisch", "er\u00b7h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "$(", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Halt! Teppiche her! den Arm den Damen!", "tokens": ["Halt", "!", "Tep\u00b7pi\u00b7che", "her", "!", "den", "Arm", "den", "Da\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "PTKVZ", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sie steigen ab am S\u00e4ulenportal,", "tokens": ["Sie", "stei\u00b7gen", "ab", "am", "S\u00e4u\u00b7len\u00b7por\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo festliche Spr\u00fcche, festliche Namen", "tokens": ["Wo", "fest\u00b7li\u00b7che", "Spr\u00fc\u00b7che", ",", "fest\u00b7li\u00b7che", "Na\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herniederflammern in farbigem Strahl.", "tokens": ["Her\u00b7nie\u00b7der\u00b7flam\u00b7mern", "in", "far\u00b7bi\u00b7gem", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da wallet herein in die duftigen R\u00e4ume", "tokens": ["Da", "wal\u00b7let", "her\u00b7ein", "in", "die", "duf\u00b7ti\u00b7gen", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die bunte, freudelachende Schaar;", "tokens": ["Die", "bun\u00b7te", ",", "freu\u00b7de\u00b7la\u00b7chen\u00b7de", "Schaar", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Schon wiegt sich in stolze, in selige Tr\u00e4ume", "tokens": ["Schon", "wiegt", "sich", "in", "stol\u00b7ze", ",", "in", "se\u00b7li\u00b7ge", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Ein manches liebefl\u00fcsternd Paar.", "tokens": ["Ein", "man\u00b7ches", "lie\u00b7be\u00b7fl\u00fcs\u00b7ternd", "Paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon lockt und ladet die Polonaise", "tokens": ["Schon", "lockt", "und", "la\u00b7det", "die", "Po\u00b7lo\u00b7nai\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum rasenden Reigen, zum tollsten Tanz;", "tokens": ["Zum", "ra\u00b7sen\u00b7den", "Rei\u00b7gen", ",", "zum", "tolls\u00b7ten", "Tanz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Den Hallen entstr\u00f6mt ein klingend Get\u00f6se,", "tokens": ["Den", "Hal\u00b7len", "ent\u00b7str\u00f6mt", "ein", "klin\u00b7gend", "Ge\u00b7t\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hinaus in die Nacht ein Meer von Glanz.", "tokens": ["Hin\u00b7aus", "in", "die", "Nacht", "ein", "Meer", "von", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Es winken die Kelche mit feuriger Labe,", "tokens": ["Es", "win\u00b7ken", "die", "Kel\u00b7che", "mit", "feu\u00b7ri\u00b7ger", "La\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die Geschirre klappern in Lustbarkeit \u2013", "tokens": ["Die", "Ge\u00b7schir\u00b7re", "klap\u00b7pern", "in", "Lust\u00b7bar\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und, o Erbarmen, f\u00fcr \u00bbchristliche\u00ab Gabe", "tokens": ["Und", ",", "o", "Er\u00b7bar\u00b7men", ",", "f\u00fcr", "\u00bb", "christ\u00b7li\u00b7che", "\u00ab", "Ga\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "$,", "FM", "NN", "$,", "APPR", "$(", "ADJA", "$(", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ein bettelnd B\u00fcchslein steht beiseit.", "tokens": ["Ein", "bet\u00b7telnd", "B\u00fcchs\u00b7lein", "steht", "bei\u00b7seit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}