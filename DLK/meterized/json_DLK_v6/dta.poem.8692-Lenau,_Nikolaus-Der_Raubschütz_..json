{"dta.poem.8692": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Raubsch\u00fctz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Der alte M\u00fcller Jakob sizt", "tokens": ["Der", "al\u00b7te", "M\u00fcl\u00b7ler", "Ja\u00b7kob", "sizt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein beim Glase Wein;", "tokens": ["Al\u00b7lein", "beim", "Gla\u00b7se", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwarzmitternacht, nur manchmal blizt", "tokens": ["Schwarz\u00b7mit\u00b7ter\u00b7nacht", ",", "nur", "manch\u00b7mal", "blizt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wetterstrahl herein;", "tokens": ["Ein", "Wet\u00b7ter\u00b7strahl", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das M\u00fchlrad saust, es braust der Wind;", "tokens": ["Das", "M\u00fchl\u00b7rad", "saust", ",", "es", "braust", "der", "Wind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch schlafen ruhig Weib und Kind.", "tokens": ["Doch", "schla\u00b7fen", "ru\u00b7hig", "Weib", "und", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Alte thut manch raschen Zug,", "tokens": ["Der", "Al\u00b7te", "thut", "manch", "ra\u00b7schen", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er denkt an Zeit und Tod.", "tokens": ["Er", "denkt", "an", "Zeit", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie drau\u00dfen jagt des Sturmes Flug,", "tokens": ["Wie", "drau\u00b7\u00dfen", "jagt", "des", "Stur\u00b7mes", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So jagen Lust und Noth,", "tokens": ["So", "ja\u00b7gen", "Lust", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die l\u00e4ngst begrabnen, neuerwacht,", "tokens": ["Die", "l\u00e4ngst", "be\u00b7grab\u00b7nen", ",", "neu\u00b7er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm durch die Brust in dieser Nacht.", "tokens": ["Ihm", "durch", "die", "Brust", "in", "die\u00b7ser", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Th\u00fcr geht auf, er f\u00e4hrt empor:", "tokens": ["Die", "Th\u00fcr", "geht", "auf", ",", "er", "f\u00e4hrt", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer kommt zu solcher Stund?", "tokens": ["Wer", "kommt", "zu", "sol\u00b7cher", "Stund", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Weidmann mit dem Feuerrohr,", "tokens": ["Ein", "Weid\u00b7mann", "mit", "dem", "Feu\u00b7er\u00b7rohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinem St\u00f6berhund,", "tokens": ["Mit", "sei\u00b7nem", "St\u00f6\u00b7ber\u00b7hund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hahnfeder, Gemsbart auf dem Hut,", "tokens": ["Hahn\u00b7fe\u00b7der", ",", "Gems\u00b7bart", "auf", "dem", "Hut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Das gr\u00fcne Wamms befleckt mit Blut.", "tokens": ["Das", "gr\u00fc\u00b7ne", "Wamms", "be\u00b7fleckt", "mit", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der M\u00fcller starrt, zur\u00fcckgebeugt,", "tokens": ["Der", "M\u00fcl\u00b7ler", "starrt", ",", "zu\u00b7r\u00fcck\u00b7ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem J\u00e4ger in's Gesicht,", "tokens": ["Dem", "J\u00e4\u00b7ger", "in's", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Haar entsezt zu Berge fleugt,", "tokens": ["Sein", "Haar", "ent\u00b7sezt", "zu", "Ber\u00b7ge", "fleugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Blut zum Herzen kriecht:", "tokens": ["Sein", "Blut", "zum", "Her\u00b7zen", "kriecht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Raubsch\u00fctz ist's, der wilde Kurd,", "tokens": ["Der", "Raub\u00b7sch\u00fctz", "ist's", ",", "der", "wil\u00b7de", "Kurd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der j\u00fcngst im Wald erschossen wurd.", "tokens": ["Der", "j\u00fcngst", "im", "Wald", "er\u00b7schos\u00b7sen", "wurd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der finstre J\u00e4ger an die Wand,", "tokens": ["Der", "finst\u00b7re", "J\u00e4\u00b7ger", "an", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Jakobs B\u00fcchse winkt;", "tokens": ["Auf", "Ja\u00b7kobs", "B\u00fcch\u00b7se", "winkt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der pre\u00dft sein Glas in zager Hand,", "tokens": ["Der", "pre\u00dft", "sein", "Glas", "in", "za\u00b7ger", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es in Scherben springt;", "tokens": ["Da\u00df", "es", "in", "Scher\u00b7ben", "springt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gehorchend nimmt er sein Gewehr,", "tokens": ["Ge\u00b7hor\u00b7chend", "nimmt", "er", "sein", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schleicht dem Grausen hinterher.", "tokens": ["Und", "schleicht", "dem", "Grau\u00b7sen", "hin\u00b7ter\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie streifen in den Wald hinaus,", "tokens": ["Sie", "strei\u00b7fen", "in", "den", "Wald", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach s\u00fc\u00dfem Wildesraub;", "tokens": ["Nach", "s\u00fc\u00b7\u00dfem", "Wil\u00b7de\u00b7sraub", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stets lauter wird der Winde Braus,", "tokens": ["Stets", "lau\u00b7ter", "wird", "der", "Win\u00b7de", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Pfade d\u00fcrres Laub.", "tokens": ["Der", "Pfa\u00b7de", "d\u00fcr\u00b7res", "Laub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der J\u00e4ger ruft voll hei\u00dfer Gier:", "tokens": ["Der", "J\u00e4\u00b7ger", "ruft", "voll", "hei\u00b7\u00dfer", "Gier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201ekomm, Bruder, jagen, jagen wir!\u201c", "tokens": ["\u201e", "komm", ",", "Bru\u00b7der", ",", "ja\u00b7gen", ",", "ja\u00b7gen", "wir", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie zieh'n fort, fort im finstern Wald", "tokens": ["Sie", "zieh'n", "fort", ",", "fort", "im", "fins\u00b7tern", "Wald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Strupp und Strom gar frisch,", "tokens": ["Durch", "Strupp", "und", "Strom", "gar", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Wild schrickt auf, die B\u00fcchse knallt,", "tokens": ["Das", "Wild", "schrickt", "auf", ",", "die", "B\u00fcch\u00b7se", "knallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der St\u00f6brer im Geb\u00fcsch", "tokens": ["Der", "St\u00f6b\u00b7rer", "im", "Ge\u00b7b\u00fcsch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Rauscht mit arbeitendem Geruch,", "tokens": ["Rauscht", "mit", "ar\u00b7bei\u00b7ten\u00b7dem", "Ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Der J\u00e4ger ruft: such, Hundel, such!", "tokens": ["Der", "J\u00e4\u00b7ger", "ruft", ":", "such", ",", "Hun\u00b7del", ",", "such", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch an des Walds geheimstem Ort\u201a", "tokens": ["Doch", "an", "des", "Walds", "ge\u00b7heims\u00b7tem", "Ort", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinem liebsten Stand\u201a", "tokens": ["Auf", "sei\u00b7nem", "liebs\u00b7ten", "Stand", "\u201a"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo j\u00fcngst die Kugel ihn durchbohrt\u201a", "tokens": ["Wo", "j\u00fcngst", "die", "Ku\u00b7gel", "ihn", "durch\u00b7bohrt", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus meuchlerischer Hand,", "tokens": ["Aus", "meuch\u00b7le\u00b7ri\u00b7scher", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da bleibt er stehn, und donnert: \u201eschau!", "tokens": ["Da", "bleibt", "er", "stehn", ",", "und", "don\u00b7nert", ":", "\u201e", "schau", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "KON", "VVFIN", "$.", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier scho\u00df er mich wie eine Sau!\u201c", "tokens": ["Hier", "scho\u00df", "er", "mich", "wie", "ei\u00b7ne", "Sau", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KOKOM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Es \u00e4chzt der Wald im Sturm, verzagt\u201a", "tokens": ["Es", "\u00e4chzt", "der", "Wald", "im", "Sturm", ",", "ver\u00b7zagt", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Monde jezt erhellt;", "tokens": ["Vom", "Mon\u00b7de", "jezt", "er\u00b7hellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der k\u00fchn gewordne M\u00fcller fragt:", "tokens": ["Der", "k\u00fchn", "ge\u00b7word\u00b7ne", "M\u00fcl\u00b7ler", "fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was ist's in jener Welt?", "tokens": ["Was", "ist's", "in", "je\u00b7ner", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da murmelt tr\u00fcben Angesichts", "tokens": ["Da", "mur\u00b7melt", "tr\u00fc\u00b7ben", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der J\u00e4gersmann: \u201ees ist halt nichts!\u201c", "tokens": ["Der", "J\u00e4\u00b7gers\u00b7mann", ":", "\u201e", "es", "ist", "halt", "nichts", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "PPER", "VAFIN", "VVFIN", "PIS", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}