{"dta.poem.14862": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die stille Gemeine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ees sollen keine Glocken geh'n,", "tokens": ["\u201e", "es", "sol\u00b7len", "kei\u00b7ne", "Glo\u00b7cken", "geh'n", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die N\u00e4chte zu verst\u00f6ren,", "tokens": ["Die", "N\u00e4ch\u00b7te", "zu", "ver\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Kreuz soll mehr auf Erden steh'n,", "tokens": ["Kein", "Kreuz", "soll", "mehr", "auf", "Er\u00b7den", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um Narren zu beth\u00f6ren!\u201c", "tokens": ["Um", "Nar\u00b7ren", "zu", "be\u00b7th\u00f6\u00b7ren", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}