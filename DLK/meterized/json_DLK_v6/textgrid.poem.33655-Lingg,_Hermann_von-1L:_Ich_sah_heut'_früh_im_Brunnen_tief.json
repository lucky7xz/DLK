{"textgrid.poem.33655": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah heut' fr\u00fch im Brunnen tief", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah heut' fr\u00fch im Brunnen tief", "tokens": ["Ich", "sah", "heut'", "fr\u00fch", "im", "Brun\u00b7nen", "tief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Liebende allein:", "tokens": ["Zwei", "Lie\u00b7ben\u00b7de", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6ne Morgenr\u00f6te schlief", "tokens": ["Die", "sch\u00f6\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "schlief"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beim bleichen Mondenschein.", "tokens": ["Beim", "blei\u00b7chen", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie k\u00fc\u00dften sich von Herzen", "tokens": ["Sie", "k\u00fc\u00df\u00b7ten", "sich", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit lichtem Purpurmund,", "tokens": ["Mit", "lich\u00b7tem", "Pur\u00b7pur\u00b7mund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein wellenheimlich Scherzen", "tokens": ["Ein", "wel\u00b7len\u00b7heim\u00b7lich", "Scher\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "War um die Morgenstund!", "tokens": ["War", "um", "die", "Mor\u00b7gen\u00b7stund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbschl\u00fcpf schnell in deine goldnen Schuh',", "tokens": ["\u00bb", "schl\u00fcpf", "schnell", "in", "dei\u00b7ne", "gold\u00b7nen", "Schuh'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O rosenfingrig Kind!", "tokens": ["O", "ro\u00b7sen\u00b7fing\u00b7rig", "Kind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Himmels Tore gehen zu,", "tokens": ["Des", "Him\u00b7mels", "To\u00b7re", "ge\u00b7hen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh heim, geh heim geschwind!\u00ab", "tokens": ["Geh", "heim", ",", "geh", "heim", "ge\u00b7schwind", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PTKVZ", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Voll Angst blickt in die H\u00f6he", "tokens": ["Voll", "Angst", "blickt", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das holde Morgenrot;", "tokens": ["Das", "hol\u00b7de", "Mor\u00b7gen\u00b7rot", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da sieht es oben, wehe!", "tokens": ["Da", "sieht", "es", "o\u00b7ben", ",", "we\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den bleichen Liebling tot!", "tokens": ["Den", "blei\u00b7chen", "Lieb\u00b7ling", "tot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}