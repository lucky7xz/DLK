{"textgrid.poem.61825": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Als Epilog", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allm\u00e4hlich f\u00fchl' ich meine Kraft erlahmen,", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "f\u00fchl'", "ich", "mei\u00b7ne", "Kraft", "er\u00b7lah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und flattern m\u00f6cht' ich nicht auf m\u00fcden Schwingen;", "tokens": ["Und", "flat\u00b7tern", "m\u00f6cht'", "ich", "nicht", "auf", "m\u00fc\u00b7den", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer vierzig Jahr' Gedichte schrieb und Dramen,", "tokens": ["Wer", "vier\u00b7zig", "Jahr'", "Ge\u00b7dich\u00b7te", "schrieb", "und", "Dra\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "NN", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der g\u00f6nnt es J\u00fcngren, um den Preis zu ringen.", "tokens": ["Der", "g\u00f6nnt", "es", "J\u00fcng\u00b7ren", ",", "um", "den", "Preis", "zu", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum eilt' ich, werte Herrn und sch\u00f6ne Damen,", "tokens": ["Drum", "eilt'", "ich", ",", "wer\u00b7te", "Herrn", "und", "sch\u00f6\u00b7ne", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "VVFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein letztes Liederbuch euch darzubringen,", "tokens": ["Mein", "letz\u00b7tes", "Lie\u00b7der\u00b7buch", "euch", "dar\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und will dabei \u2013 zum Abschied l\u00e4\u00dft sich's wagen \u2013", "tokens": ["Und", "will", "da\u00b7bei", "\u2013", "zum", "Ab\u00b7schied", "l\u00e4\u00dft", "sich's", "wa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "$(", "APPRART", "NN", "VVFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit meinem Dank auch meinen Harm euch sagen.", "tokens": ["Mit", "mei\u00b7nem", "Dank", "auch", "mei\u00b7nen", "Harm", "euch", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn eure Gunst zwar lie\u00dfet ihr vor vielen", "tokens": ["Denn", "eu\u00b7re", "Gunst", "zwar", "lie\u00b7\u00dfet", "ihr", "vor", "vie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir angedeihn, doch hat mich eins verdrossen,", "tokens": ["Mir", "an\u00b7ge\u00b7deihn", ",", "doch", "hat", "mich", "eins", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df bei des J\u00fcnglings unvollkommnen Spielen", "tokens": ["Da\u00df", "bei", "des", "J\u00fcng\u00b7lings", "un\u00b7voll\u00b7komm\u00b7nen", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr allzufr\u00fch in Beifall euch ergossen,", "tokens": ["Ihr", "all\u00b7zu\u00b7fr\u00fch", "in", "Bei\u00b7fall", "euch", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch, als er vorw\u00e4rts drang zu w\u00fcrd'gen Zielen,", "tokens": ["Doch", ",", "als", "er", "vor\u00b7w\u00e4rts", "drang", "zu", "w\u00fcrd'\u00b7gen", "Zie\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "VVFIN", "PTKZU", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein halbes Ohr nur seinem Ernst erschlossen,", "tokens": ["Ein", "hal\u00b7bes", "Ohr", "nur", "sei\u00b7nem", "Ernst", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als w\u00e4r' allein der leichte Schmelz der Jugend,", "tokens": ["Als", "w\u00e4r'", "al\u00b7lein", "der", "leich\u00b7te", "Schmelz", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht reife Kunst des Dichters Zier und Tugend.", "tokens": ["Nicht", "rei\u00b7fe", "Kunst", "des", "Dich\u00b7ters", "Zier", "und", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von oben freilich flammt in Feuerzungen", "tokens": ["Von", "o\u00b7ben", "frei\u00b7lich", "flammt", "in", "Feu\u00b7er\u00b7zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kraft herab; doch uns geh\u00f6rt das Streben;", "tokens": ["Die", "Kraft", "her\u00b7ab", ";", "doch", "uns", "ge\u00b7h\u00f6rt", "das", "Stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch keinem ist, was Dauer hat, gelungen,", "tokens": ["Noch", "kei\u00b7nem", "ist", ",", "was", "Dau\u00b7er", "hat", ",", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "$,", "PWS", "NN", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der nicht das Pfund gemehrt, das ihm gegeben.", "tokens": ["Der", "nicht", "das", "Pfund", "ge\u00b7mehrt", ",", "das", "ihm", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So hab' auch ich beharrlich fortgerungen", "tokens": ["So", "hab'", "auch", "ich", "be\u00b7harr\u00b7lich", "fort\u00b7ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schritt, im Lernen wachsend, durch das Leben;", "tokens": ["Und", "schritt", ",", "im", "Ler\u00b7nen", "wach\u00b7send", ",", "durch", "das", "Le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Drum seid mir endlich unbefangne Richter,", "tokens": ["Drum", "seid", "mir", "end\u00b7lich", "un\u00b7be\u00b7fang\u00b7ne", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und w\u00e4gt ihr mich, so w\u00e4gt den ganzen Dichter.", "tokens": ["Und", "w\u00e4gt", "ihr", "mich", ",", "so", "w\u00e4gt", "den", "gan\u00b7zen", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}