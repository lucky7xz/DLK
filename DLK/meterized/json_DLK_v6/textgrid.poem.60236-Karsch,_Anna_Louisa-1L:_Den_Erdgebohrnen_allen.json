{"textgrid.poem.60236": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Erdgebohrnen allen", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Erdgebohrnen allen", "tokens": ["Den", "Erd\u00b7ge\u00b7bohr\u00b7nen", "al\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward ein bestimmtes Loos,", "tokens": ["Ward", "ein", "be\u00b7stimm\u00b7tes", "Loos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nachdem es den G\u00f6ttern gefallen,", "tokens": ["Nach\u00b7dem", "es", "den", "G\u00f6t\u00b7tern", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Klein, mittelm\u00e4\u00dfig, oder gro\u00df,", "tokens": ["Klein", ",", "mit\u00b7tel\u00b7m\u00e4\u00b7\u00dfig", ",", "o\u00b7der", "gro\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verschiedenen wurden Talente gegeben,", "tokens": ["Ver\u00b7schie\u00b7de\u00b7nen", "wur\u00b7den", "Ta\u00b7len\u00b7te", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Hervorzuschimmern in der Welt,", "tokens": ["Her\u00b7vor\u00b7zu\u00b7schim\u00b7mern", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn ihr Bau vom Staube zerf\u00e4llt,", "tokens": ["Und", "wenn", "ihr", "Bau", "vom", "Stau\u00b7be", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Noch in dem Tempel der Ehre zu leben.", "tokens": ["Noch", "in", "dem", "Tem\u00b7pel", "der", "Eh\u00b7re", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Als Br\u00fcckner einst gebohren ward;", "tokens": ["Als", "Br\u00fcck\u00b7ner", "einst", "ge\u00b7boh\u00b7ren", "ward", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da sprach der Musenvater:", "tokens": ["Da", "sprach", "der", "Mu\u00b7sen\u00b7va\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Nehmt T\u00f6chter dies Kn\u00e4bchen von feuriger Art", "tokens": ["Nehmt", "T\u00f6ch\u00b7ter", "dies", "Kn\u00e4b\u00b7chen", "von", "feu\u00b7ri\u00b7ger", "Art"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PDS", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Und bildet es f\u00fcr das Theater.", "tokens": ["Und", "bil\u00b7det", "es", "f\u00fcr", "das", "The\u00b7a\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Da neigete Terpsichore", "tokens": ["Da", "nei\u00b7ge\u00b7te", "Ter\u00b7psi\u00b7cho\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Zuerst ihr Haupt \u2013 Da legte", "tokens": ["Zu\u00b7erst", "ihr", "Haupt", "\u2013", "Da", "leg\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die Hand aufs Herz Melpomene,", "tokens": ["Die", "Hand", "aufs", "Herz", "Mel\u00b7po\u00b7me\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Thaliens Busen regte", "tokens": ["Tha\u00b7li\u00b7ens", "Bu\u00b7sen", "reg\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.17": {"text": "Sich pflegegierig empor.", "tokens": ["Sich", "pfle\u00b7ge\u00b7gie\u00b7rig", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.18": {"text": "Doch mit gefl\u00fcgelten F\u00fc\u00dfen", "tokens": ["Doch", "mit", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Kam einer der G\u00f6tter den Musen zuvor.", "tokens": ["Kam", "ei\u00b7ner", "der", "G\u00f6t\u00b7ter", "den", "Mu\u00b7sen", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.20": {"text": "Sie l\u00e4chelten und lie\u00dfen", "tokens": ["Sie", "l\u00e4\u00b7chel\u00b7ten", "und", "lie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Den k\u00fcnstelehrenden Merkur,", "tokens": ["Den", "k\u00fcns\u00b7te\u00b7leh\u00b7ren\u00b7den", "Mer\u00b7kur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Das Kn\u00e4bchen lehren und bewahren.", "tokens": ["Das", "Kn\u00e4b\u00b7chen", "leh\u00b7ren", "und", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Sie lie\u00dfen ihm aber den Z\u00f6geling nur", "tokens": ["Sie", "lie\u00b7\u00dfen", "ihm", "a\u00b7ber", "den", "Z\u00f6\u00b7ge\u00b7ling", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.24": {"text": "Bis zu den J\u00fcnglingsjahren,", "tokens": ["Bis", "zu", "den", "J\u00fcng\u00b7lings\u00b7jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Da ward er ganz der Ihrige, ganz", "tokens": ["Da", "ward", "er", "ganz", "der", "Ih\u00b7ri\u00b7ge", ",", "ganz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "PPOSS", "$,", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Erf\u00fcllet \u2013 Begeistert von ihnen", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "\u2013", "Be\u00b7geis\u00b7tert", "von", "ih\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVPP", "APPR", "PPER"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.27": {"text": "Erringt er sich einen unsterblichen Kranz,", "tokens": ["Er\u00b7ringt", "er", "sich", "ei\u00b7nen", "uns\u00b7terb\u00b7li\u00b7chen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.28": {"text": "Auf dieser B\u00fchne, die unter den B\u00fchnen", "tokens": ["Auf", "die\u00b7ser", "B\u00fch\u00b7ne", ",", "die", "un\u00b7ter", "den", "B\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Germaniens gl\u00e4nzt,", "tokens": ["Ger\u00b7ma\u00b7ni\u00b7ens", "gl\u00e4nzt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.30": {"text": "Wie Hesperus unter den Sternen.", "tokens": ["Wie", "Hes\u00b7pe\u00b7rus", "un\u00b7ter", "den", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.31": {"text": "Schon lange ward er mit Ehre bekr\u00e4nzt", "tokens": ["Schon", "lan\u00b7ge", "ward", "er", "mit", "Eh\u00b7re", "be\u00b7kr\u00e4nzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Und strebet noch immer zu lernen.", "tokens": ["Und", "stre\u00b7bet", "noch", "im\u00b7mer", "zu", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Melpomene lehret noch immer ihm vor:", "tokens": ["Mel\u00b7po\u00b7me\u00b7ne", "leh\u00b7ret", "noch", "im\u00b7mer", "ihm", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Noch immer trinket sein horchendes Ohr", "tokens": ["Noch", "im\u00b7mer", "trin\u00b7ket", "sein", "hor\u00b7chen\u00b7des", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Der G\u00f6ttinn weise Gesetze,", "tokens": ["Der", "G\u00f6t\u00b7tinn", "wei\u00b7se", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "Und sinnet vom Morgen bis Abend darauf,", "tokens": ["Und", "sin\u00b7net", "vom", "Mor\u00b7gen", "bis", "A\u00b7bend", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "NN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.37": {"text": "Damit er nicht eines verletze,", "tokens": ["Da\u00b7mit", "er", "nicht", "ei\u00b7nes", "ver\u00b7let\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.38": {"text": "Damit er nicht strauchle im r\u00fchmlichen Lauf.", "tokens": ["Da\u00b7mit", "er", "nicht", "strauch\u00b7le", "im", "r\u00fchm\u00b7li\u00b7chen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.39": {"text": "Sagt, Kenner, was will er noch werden?", "tokens": ["Sagt", ",", "Ken\u00b7ner", ",", "was", "will", "er", "noch", "wer\u00b7den", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.40": {"text": "Bewegt er euch alle nicht schon", "tokens": ["Be\u00b7wegt", "er", "euch", "al\u00b7le", "nicht", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PIS", "PTKNEG", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.41": {"text": "Durch Ausdruck seiner Gebehrden,", "tokens": ["Durch", "Aus\u00b7druck", "sei\u00b7ner", "Ge\u00b7behr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Durch seiner Stimme m\u00e4chtigen Ton,", "tokens": ["Durch", "sei\u00b7ner", "Stim\u00b7me", "m\u00e4ch\u00b7ti\u00b7gen", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.43": {"text": "Durch seine gewaltigen Blicke,", "tokens": ["Durch", "sei\u00b7ne", "ge\u00b7wal\u00b7ti\u00b7gen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.44": {"text": "Zum Trauren, oder zur lachenden Lust?", "tokens": ["Zum", "Trau\u00b7ren", ",", "o\u00b7der", "zur", "la\u00b7chen\u00b7den", "Lust", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "Wenn \u00fcber ein grausam Geschicke", "tokens": ["Wenn", "\u00fc\u00b7ber", "ein", "grau\u00b7sam", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJD", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.46": {"text": "Aus Medons best\u00fcrmeter Brust", "tokens": ["Aus", "Me\u00b7dons", "be\u00b7st\u00fcr\u00b7me\u00b7ter", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.47": {"text": "Die Klage bricht; und was er leide", "tokens": ["Die", "Kla\u00b7ge", "bricht", ";", "und", "was", "er", "lei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Aus jeder Miene blickt;", "tokens": ["Aus", "je\u00b7der", "Mie\u00b7ne", "blickt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Dann f\u00fchlen wir alle vom h\u00e4mischen Neide", "tokens": ["Dann", "f\u00fch\u00b7len", "wir", "al\u00b7le", "vom", "h\u00e4\u00b7mi\u00b7schen", "Nei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.50": {"text": "Die Tugend und Unschuld gedr\u00fcckt,", "tokens": ["Die", "Tu\u00b7gend", "und", "Un\u00b7schuld", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.51": {"text": "Und \u00e4chzen voll Kummer und Schmerzen,", "tokens": ["Und", "\u00e4ch\u00b7zen", "voll", "Kum\u00b7mer", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.52": {"text": "Und wenn er den kriechenden Feinde vergiebt,", "tokens": ["Und", "wenn", "er", "den", "krie\u00b7chen\u00b7den", "Fein\u00b7de", "ver\u00b7giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.53": {"text": "Und siegend die Rache des Weisen ver\u00fcbt,", "tokens": ["Und", "sie\u00b7gend", "die", "Ra\u00b7che", "des", "Wei\u00b7sen", "ver\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.54": {"text": "Dann quellen aus fr\u00f6hlichen Herzen", "tokens": ["Dann", "quel\u00b7len", "aus", "fr\u00f6h\u00b7li\u00b7chen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.55": {"text": "Die Thr\u00e4nen zum Auge, wir gl\u00fchn,", "tokens": ["Die", "Thr\u00e4\u00b7nen", "zum", "Au\u00b7ge", ",", "wir", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.56": {"text": "Und lieben die Tugend, den Dichter und ihn.", "tokens": ["Und", "lie\u00b7ben", "die", "Tu\u00b7gend", ",", "den", "Dich\u00b7ter", "und", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}