{"dta.poem.16281": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein W\u00fcrfelspieler, dem schlimm jeder W\u00fcrfel fiel,", "tokens": ["Ein", "W\u00fcr\u00b7fel\u00b7spie\u00b7ler", ",", "dem", "schlimm", "je\u00b7der", "W\u00fcr\u00b7fel", "fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der jedes Spiel verlor, doch nie die Lust am Spiel,", "tokens": ["Der", "je\u00b7des", "Spiel", "ver\u00b7lor", ",", "doch", "nie", "die", "Lust", "am", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hat keine Ruh, bis er aufs Gl\u00fcck des W\u00fcrfelfalles", "tokens": ["Hat", "kei\u00b7ne", "Ruh", ",", "bis", "er", "aufs", "Gl\u00fcck", "des", "W\u00fcr\u00b7fel\u00b7fal\u00b7les"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Setzt alles was er hat, und hat verloren alles.", "tokens": ["Setzt", "al\u00b7les", "was", "er", "hat", ",", "und", "hat", "ver\u00b7lo\u00b7ren", "al\u00b7les", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PWS", "PPER", "VAFIN", "$,", "KON", "VAFIN", "VVPP", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aufs Spiel hat er zuletzt die W\u00fcrfel selbst gesetzt;", "tokens": ["Aufs", "Spiel", "hat", "er", "zu\u00b7letzt", "die", "W\u00fcr\u00b7fel", "selbst", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Villeicht gewinnt er Ruh, wenn er verlieret jetzt.", "tokens": ["Vil\u00b7leicht", "ge\u00b7winnt", "er", "Ruh", ",", "wenn", "er", "ver\u00b7lie\u00b7ret", "jetzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}