{"textgrid.poem.46920": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie der Baum der indianischen Feige]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Baum der indianischen Feige", "tokens": ["Wie", "der", "Baum", "der", "in\u00b7di\u00b7a\u00b7ni\u00b7schen", "Fei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In den Boden senkt die untern Zweige,", "tokens": ["In", "den", "Bo\u00b7den", "senkt", "die", "un\u00b7tern", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und sie schlagen Wurzeln und gedeihn;", "tokens": ["Und", "sie", "schla\u00b7gen", "Wur\u00b7zeln", "und", "ge\u00b7deihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "KON", "VVINF", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Also senk' ich meine j\u00fcngsten Kinder", "tokens": ["Al\u00b7so", "senk'", "ich", "mei\u00b7ne", "j\u00fcng\u00b7sten", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "In die Erd', und hoffe, da\u00df nicht minder", "tokens": ["In", "die", "Erd'", ",", "und", "hof\u00b7fe", ",", "da\u00df", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "$,", "KOUS", "PTKNEG", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sie zum Leben aufbewahret sei'n.", "tokens": ["Sie", "zum", "Le\u00b7ben", "auf\u00b7be\u00b7wah\u00b7ret", "sei'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht den Wipfel will ich traurig senken,", "tokens": ["Nicht", "den", "Wip\u00b7fel", "will", "ich", "trau\u00b7rig", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sondern froh der abgetrennten denken,", "tokens": ["Son\u00b7dern", "froh", "der", "ab\u00b7ge\u00b7trenn\u00b7ten", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und das Vorbild soll mir Trost verleihn,", "tokens": ["Und", "das", "Vor\u00b7bild", "soll", "mir", "Trost", "ver\u00b7leihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie der Baum der indianischen Feige", "tokens": ["Wie", "der", "Baum", "der", "in\u00b7di\u00b7a\u00b7ni\u00b7schen", "Fei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "In den Boden senkt die untern Zweige,", "tokens": ["In", "den", "Bo\u00b7den", "senkt", "die", "un\u00b7tern", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die obern wiegt im Sonnenschein.", "tokens": ["Und", "die", "o\u00b7bern", "wiegt", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}