{"textgrid.poem.58144": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was drau\u00dfen ist, la\u00df drau\u00dfen stehn,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was drau\u00dfen ist, la\u00df drau\u00dfen stehn,", "tokens": ["Was", "drau\u00b7\u00dfen", "ist", ",", "la\u00df", "drau\u00b7\u00dfen", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VVIMP", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kann nur Unruh geben!", "tokens": ["Es", "kann", "nur", "Un\u00b7ruh", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Geist allein mit Gott umgehn,", "tokens": ["Im", "Geist", "al\u00b7lein", "mit", "Gott", "um\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bringt Ruhe, Freud' und Leben.", "tokens": ["Bringt", "Ru\u00b7he", ",", "Freud'", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}