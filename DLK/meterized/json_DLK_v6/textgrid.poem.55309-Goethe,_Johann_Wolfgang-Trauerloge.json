{"textgrid.poem.55309": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Trauerloge", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dem \u00f6den Strand des Lebens,", "tokens": ["An", "dem", "\u00f6\u00b7den", "Strand", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sich D\u00fcn auf D\u00fcne h\u00e4uft,", "tokens": ["Wo", "sich", "D\u00fcn", "auf", "D\u00fc\u00b7ne", "h\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der Sturm im Finstern tr\u00e4uft,", "tokens": ["Wo", "der", "Sturm", "im", "Fins\u00b7tern", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Setze dir ein Ziel des Strebens.", "tokens": ["Set\u00b7ze", "dir", "ein", "Ziel", "des", "Stre\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter schon verloschnen Siegeln", "tokens": ["Un\u00b7ter", "schon", "ver\u00b7lo\u00b7schnen", "Sie\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausend V\u00e4ter hingestreckt,", "tokens": ["Tau\u00b7send", "V\u00e4\u00b7ter", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! von neuen, frischen H\u00fcgeln", "tokens": ["Ach", "!", "von", "neu\u00b7en", ",", "fri\u00b7schen", "H\u00fc\u00b7geln"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freund an Freunden \u00fcberdeckt.", "tokens": ["Freund", "an", "Freun\u00b7den", "\u00fc\u00b7berd\u00b7eckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hast du so dich abgefunden,", "tokens": ["Hast", "du", "so", "dich", "ab\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werde Nacht und \u00c4ther klar,", "tokens": ["Wer\u00b7de", "Nacht", "und", "\u00c4\u00b7ther", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der ew'gen Sterne Schar", "tokens": ["Und", "der", "ew'\u00b7gen", "Ster\u00b7ne", "Schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Deute dir belebte Stunden,", "tokens": ["Deu\u00b7te", "dir", "be\u00b7leb\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo du hier mit Ungetr\u00fcbten,", "tokens": ["Wo", "du", "hier", "mit", "Un\u00b7ge\u00b7tr\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Treulich wirkend, gern verweilst", "tokens": ["Treu\u00b7lich", "wir\u00b7kend", ",", "gern", "ver\u00b7weilst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und auch treulich den geliebten", "tokens": ["Und", "auch", "treu\u00b7lich", "den", "ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewigen entgegeneilst.", "tokens": ["E\u00b7wi\u00b7gen", "ent\u00b7ge\u00b7ge\u00b7neilst", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}