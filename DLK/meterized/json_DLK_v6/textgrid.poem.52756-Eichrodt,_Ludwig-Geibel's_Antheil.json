{"textgrid.poem.52756": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Geibel's Antheil", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn K\u00fcsse fl\u00fcstern durch die Nacht", "tokens": ["Wenn", "K\u00fcs\u00b7se", "fl\u00fcs\u00b7tern", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit heimlich s\u00fc\u00dfem Weh'n,", "tokens": ["Mit", "heim\u00b7lich", "s\u00fc\u00b7\u00dfem", "Weh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Sterne still in Silbertracht", "tokens": ["Und", "Ster\u00b7ne", "still", "in", "Sil\u00b7ber\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ihren Himmel geh'n,", "tokens": ["Durch", "ih\u00b7ren", "Him\u00b7mel", "geh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So ruf' ich, schlafet wohl, jawohl!", "tokens": ["So", "ruf'", "ich", ",", "schla\u00b7fet", "wohl", ",", "ja\u00b7wohl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Ruh'!", "tokens": ["In", "Ruh'", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die lieben Aeuglein zu,", "tokens": ["Die", "lie\u00b7ben", "A\u00b7e\u00b7u\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Engel Gottes h\u00fcten euch,", "tokens": ["Die", "En\u00b7gel", "Got\u00b7tes", "h\u00fc\u00b7ten", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Lulu!", "tokens": ["Lu\u00b7lu", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.2": {"line.1": {"text": "Wer weinen kann, dem ist so wohl,", "tokens": ["Wer", "wei\u00b7nen", "kann", ",", "dem", "ist", "so", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "PDS", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebenedeit ist der,", "tokens": ["Ge\u00b7be\u00b7ne\u00b7deit", "ist", "der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Vom D\u00fcnensand bis nach Tyrol", "tokens": ["Vom", "D\u00fc\u00b7nen\u00b7sand", "bis", "nach", "Ty\u00b7rol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder bis an's Meer.", "tokens": ["Und", "wie\u00b7der", "bis", "an's", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch er schl\u00e4ft wohl, wie ihr, jawohl!", "tokens": ["Auch", "er", "schl\u00e4ft", "wohl", ",", "wie", "ihr", ",", "ja\u00b7wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "$,", "ITJ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "In Ruh',", "tokens": ["In", "Ruh'", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die lieben Aeuglein zu,", "tokens": ["Die", "lie\u00b7ben", "A\u00b7e\u00b7u\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Engel Gottes h\u00fcten euch,", "tokens": ["Die", "En\u00b7gel", "Got\u00b7tes", "h\u00fc\u00b7ten", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Lulu!", "tokens": ["Lu\u00b7lu", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.3": {"line.1": {"text": "Entschlafen ist das blaue Meer", "tokens": ["Ent\u00b7schla\u00b7fen", "ist", "das", "blau\u00b7e", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tr\u00e4umend ruht der Kiel,", "tokens": ["Und", "tr\u00e4u\u00b7mend", "ruht", "der", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sanft erklingt dar\u00fcberher", "tokens": ["Und", "sanft", "er\u00b7klingt", "da\u00b7r\u00fc\u00b7ber\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein gold'nes Saitenspiel.", "tokens": ["Ein", "gold'\u00b7nes", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es singt und klingt, schlaft wohl, jawohl!", "tokens": ["Es", "singt", "und", "klingt", ",", "schlaft", "wohl", ",", "ja\u00b7wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ADJD", "ADV", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Ruh',", "tokens": ["In", "Ruh'", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die lieben Aeuglein zu,", "tokens": ["Die", "lie\u00b7ben", "A\u00b7e\u00b7u\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Engel Gottes h\u00fcten euch,", "tokens": ["Die", "En\u00b7gel", "Got\u00b7tes", "h\u00fc\u00b7ten", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Lulu!", "tokens": ["Lu\u00b7lu", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.4": {"line.1": {"text": "Fahr' wohl, fahr' wohl, fern ferne du!", "tokens": ["Fahr'", "wohl", ",", "fahr'", "wohl", ",", "fern", "fer\u00b7ne", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVFIN", "ADV", "$,", "ADJD", "ADJA", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe zieht und scheucht \u2013", "tokens": ["Die", "Lie\u00b7be", "zieht", "und", "scheucht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber \u2013 finde keine Ruh'", "tokens": ["Ich", "a\u00b7ber", "\u2013", "fin\u00b7de", "kei\u00b7ne", "Ruh'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$(", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die braune Wange feucht ...", "tokens": ["Die", "brau\u00b7ne", "Wan\u00b7ge", "feucht", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch ihr, o schlaft nur wohl, jawohl!", "tokens": ["Doch", "ihr", ",", "o", "schlaft", "nur", "wohl", ",", "ja\u00b7wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "ADJD", "ADV", "ADV", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Ruh'", "tokens": ["In", "Ruh'"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die lieben Aeuglein zu,", "tokens": ["Die", "lie\u00b7ben", "A\u00b7e\u00b7u\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Engel Gottes h\u00fcten euch,", "tokens": ["Die", "En\u00b7gel", "Got\u00b7tes", "h\u00fc\u00b7ten", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Lulu!", "tokens": ["Lu\u00b7lu", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "--", "measure": "unknown.measure.zero"}}}}}