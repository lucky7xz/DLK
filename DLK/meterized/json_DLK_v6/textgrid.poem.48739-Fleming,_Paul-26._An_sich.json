{"textgrid.poem.48739": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "26. An sich", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei dennoch unverzagt, gieb dennoch unverloren,", "tokens": ["Sei", "den\u00b7noch", "un\u00b7ver\u00b7zagt", ",", "gieb", "den\u00b7noch", "un\u00b7ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "VVIMP", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weich keinem Gl\u00fccke nicht, steh' h\u00f6her als der Neid,", "tokens": ["weich", "kei\u00b7nem", "Gl\u00fc\u00b7cke", "nicht", ",", "steh'", "h\u00f6\u00b7her", "als", "der", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "PTKNEG", "$,", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "vergn\u00fcge dich an dir und acht' es f\u00fcr kein Leid,", "tokens": ["ver\u00b7gn\u00fc\u00b7ge", "dich", "an", "dir", "und", "acht'", "es", "f\u00fcr", "kein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "KON", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "hat sich gleich wider dich Gl\u00fcck, Ort und Zeit verschworen.", "tokens": ["hat", "sich", "gleich", "wi\u00b7der", "dich", "Gl\u00fcck", ",", "Ort", "und", "Zeit", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "APPR", "PPER", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was dich betr\u00fcbt und labt, halt Alles f\u00fcr erkoren,", "tokens": ["Was", "dich", "be\u00b7tr\u00fcbt", "und", "labt", ",", "halt", "Al\u00b7les", "f\u00fcr", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVFIN", "$,", "VVFIN", "PIS", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nim dein Verh\u00e4ngn\u00fc\u00df an, lass' Alles unbereut.", "tokens": ["nim", "dein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "an", ",", "lass'", "Al\u00b7les", "un\u00b7be\u00b7reut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Tu, was getan mu\u00df sein, und eh' man dirs gebeut.", "tokens": ["Tu", ",", "was", "ge\u00b7tan", "mu\u00df", "sein", ",", "und", "eh'", "man", "dirs", "ge\u00b7beut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVPP", "VMFIN", "VAINF", "$,", "KON", "KOUS", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was du noch hoffen kanst, das wird noch stets geboren.", "tokens": ["Was", "du", "noch", "hof\u00b7fen", "kanst", ",", "das", "wird", "noch", "stets", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$,", "PDS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was klagt, was lobt man doch? Sein Ungl\u00fcck und sein Gl\u00fccke", "tokens": ["Was", "klagt", ",", "was", "lobt", "man", "doch", "?", "Sein", "Un\u00b7gl\u00fcck", "und", "sein", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "PIS", "ADV", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist ihm ein jeder selbst. Schau alle Sachen an,", "tokens": ["ist", "ihm", "ein", "je\u00b7der", "selbst", ".", "Schau", "al\u00b7le", "Sa\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "PIS", "ADV", "$.", "NN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "di\u00df Alles ist in dir. La\u00df deinen eiteln Wahn,", "tokens": ["di\u00df", "Al\u00b7les", "ist", "in", "dir", ".", "La\u00df", "dei\u00b7nen", "ei\u00b7teln", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "APPR", "PPER", "$.", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "und eh' du f\u00f6rder gehst, so geh' in dich zur\u00fccke.", "tokens": ["und", "eh'", "du", "f\u00f6r\u00b7der", "gehst", ",", "so", "geh'", "in", "dich", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer sein selbst Meister ist und sich beherschen kan,", "tokens": ["Wer", "sein", "selbst", "Meis\u00b7ter", "ist", "und", "sich", "be\u00b7her\u00b7schen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADV", "NN", "VAFIN", "KON", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dem ist die weite Welt und Alles untertan.", "tokens": ["dem", "ist", "die", "wei\u00b7te", "Welt", "und", "Al\u00b7les", "un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "KON", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}