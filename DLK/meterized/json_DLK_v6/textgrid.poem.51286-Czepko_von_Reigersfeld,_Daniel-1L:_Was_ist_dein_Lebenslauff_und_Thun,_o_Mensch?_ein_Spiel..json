{"textgrid.poem.51286": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist dein Lebenslauff und Thun, o Mensch? ein Spiel.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist dein Lebenslauff und Thun, o Mensch? ein Spiel.", "tokens": ["Was", "ist", "dein", "Le\u00b7bens\u00b7lauff", "und", "Thun", ",", "o", "Mensch", "?", "ein", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,", "FM", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Innhalt sage mir? Kinds, Weibs und Tods Beschwerde.", "tokens": ["Den", "Inn\u00b7halt", "sa\u00b7ge", "mir", "?", "Kinds", ",", "Weibs", "und", "Tods", "Be\u00b7schwer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "NN", "$,", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ist es vor ein Platz, darauff wir spieln? Die Erde.", "tokens": ["Was", "ist", "es", "vor", "ein", "Platz", ",", "dar\u00b7auff", "wir", "spieln", "?", "Die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "PAV", "PPER", "VVINF", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer schl\u00e4gt und singt dazu? Die Wollust ohne Ziel.", "tokens": ["Wer", "schl\u00e4gt", "und", "singt", "da\u00b7zu", "?", "Die", "Wol\u00b7lust", "oh\u00b7ne", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "PAV", "$.", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer hei\u00dft auff das Ger\u00fcst' uns treten? Selbst die Zeit.", "tokens": ["Wer", "hei\u00dft", "auff", "das", "Ge\u00b7r\u00fcst'", "uns", "tre\u00b7ten", "?", "Selbst", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$.", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer zeigt die Schauer mir? Mensch, das sind blo\u00df die Weisen,", "tokens": ["Wer", "zeigt", "die", "Schau\u00b7er", "mir", "?", "Mensch", ",", "das", "sind", "blo\u00df", "die", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "$.", "NN", "$,", "PDS", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was ist vor Stellung hier? Stehn, schlaffen, wachen, reisen,", "tokens": ["Was", "ist", "vor", "Stel\u00b7lung", "hier", "?", "Stehn", ",", "schlaf\u00b7fen", ",", "wa\u00b7chen", ",", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "ADV", "$.", "NN", "$,", "VVFIN", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer theilt Gesichter aus? Allein die Eitelkeit.", "tokens": ["Wer", "theilt", "Ge\u00b7sich\u00b7ter", "aus", "?", "Al\u00b7lein", "die", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "PTKVZ", "$.", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer macht den Schau Platz auff? Der wunderbare Gott.", "tokens": ["Wer", "macht", "den", "Schau", "Platz", "auff", "?", "Der", "wun\u00b7der\u00b7ba\u00b7re", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was vor ein Vorhang deckts? Das ewige Versehen.", "tokens": ["Was", "vor", "ein", "Vor\u00b7hang", "deckts", "?", "Das", "e\u00b7wi\u00b7ge", "Ver\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie wird es abgetheilt? Durch leben, sterben, flehen.", "tokens": ["Wie", "wird", "es", "ab\u00b7ge\u00b7theilt", "?", "Durch", "le\u00b7ben", ",", "ster\u00b7ben", ",", "fle\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$.", "APPR", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer f\u00fchrt uns ab, wer zeucht uns Kleider aus? Der Tod.", "tokens": ["Wer", "f\u00fchrt", "uns", "ab", ",", "wer", "zeucht", "uns", "Klei\u00b7der", "aus", "?", "Der", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "NN", "PTKVZ", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo wird der Schlu\u00df erwartt des Spieles? in der Grufft.", "tokens": ["Wo", "wird", "der", "Schlu\u00df", "er\u00b7wartt", "des", "Spie\u00b7les", "?", "in", "der", "Grufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVFIN", "ART", "NN", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer spielt am besten mit? Der wol sein Ammt kan f\u00fchren.", "tokens": ["Wer", "spielt", "am", "bes\u00b7ten", "mit", "?", "Der", "wol", "sein", "Ammt", "kan", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKA", "ADJD", "PTKVZ", "$.", "ART", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist das Spiel vor sich gut? Das Ende mu\u00df es zieren.", "tokens": ["Ist", "das", "Spiel", "vor", "sich", "gut", "?", "Das", "En\u00b7de", "mu\u00df", "es", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PRF", "ADJD", "$.", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wenn ist es aus? o Mensch! wenn dir dein JESUS rufft.", "tokens": ["Wenn", "ist", "es", "aus", "?", "o", "Mensch", "!", "wenn", "dir", "dein", "Je\u00b7SUS", "rufft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PTKVZ", "$.", "FM", "NN", "$.", "KOUS", "PPER", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}