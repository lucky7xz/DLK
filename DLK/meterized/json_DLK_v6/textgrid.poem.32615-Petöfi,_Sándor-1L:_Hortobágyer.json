{"textgrid.poem.32615": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hortob\u00e1gyer", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hortob\u00e1gyer", "tokens": ["Hor\u00b7tob\u00e1gyer"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Trinken will ich, bring' doch eine Flasche Wein!", "tokens": ["Trin\u00b7ken", "will", "ich", ",", "bring'", "doch", "ei\u00b7ne", "Fla\u00b7sche", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Hei, von Debreczin bis Hortob\u00e1gy ist's weit,", "tokens": ["Hei", ",", "von", "De\u00b7bre\u00b7czin", "bis", "Hor\u00b7tob\u00e1gy", "ist's", "weit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "APPR", "NE", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und den ganzen Weg her bin ich durstig heut'!", "tokens": ["Und", "den", "gan\u00b7zen", "Weg", "her", "bin", "ich", "durs\u00b7tig", "heut'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APZR", "VAFIN", "PPER", "ADJD", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Grimme Melodie ist's, die der Wind sich pfeift,", "tokens": ["Grim\u00b7me", "Me\u00b7lo\u00b7die", "ist's", ",", "die", "der", "Wind", "sich", "pfeift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PRELS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und vor K\u00e4lte sind die Glieder mir ersteift;", "tokens": ["Und", "vor", "K\u00e4l\u00b7te", "sind", "die", "Glie\u00b7der", "mir", "er\u00b7steift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Goldne Wirtin, schau mich nur ein bi\u00dfchen an,", "tokens": ["Gold\u00b7ne", "Wir\u00b7tin", ",", "schau", "mich", "nur", "ein", "bi\u00df\u00b7chen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Da\u00df an deinem Blick ich mich erw\u00e4rmen kann!", "tokens": ["Da\u00df", "an", "dei\u00b7nem", "Blick", "ich", "mich", "er\u00b7w\u00e4r\u00b7men", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Goldne Wirtin, ei, woher ist dieser Wein?", "tokens": ["Gold\u00b7ne", "Wir\u00b7tin", ",", "ei", ",", "wo\u00b7her", "ist", "die\u00b7ser", "Wein", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ITJ", "$,", "PWAV", "VAFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Nur ein wilder Apfel kann so sauer sein!", "tokens": ["Nur", "ein", "wil\u00b7der", "Ap\u00b7fel", "kann", "so", "sau\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Auf der Stelle schenk' mir einen s\u00fc\u00dfen Ku\u00df,", "tokens": ["Auf", "der", "Stel\u00b7le", "schenk'", "mir", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Der den herben Wein mir nun vers\u00fc\u00dfen mu\u00df!", "tokens": ["Der", "den", "her\u00b7ben", "Wein", "mir", "nun", "ver\u00b7s\u00fc\u00b7\u00dfen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6nes Weibchen ... saurer Wein ... und ... s\u00fc\u00dfer Ku\u00df ...", "tokens": ["Sch\u00f6\u00b7nes", "Weib\u00b7chen", "...", "sau\u00b7rer", "Wein", "...", "und", "...", "s\u00fc\u00b7\u00dfer", "Ku\u00df", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$(", "KON", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ei, wie schwank' ich hin und her ... wie wankt mein Fu\u00df ...", "tokens": ["Ei", ",", "wie", "schwank'", "ich", "hin", "und", "her", "...", "wie", "wankt", "mein", "Fu\u00df", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "$(", "PWAV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "S\u00fc\u00dfe Wirtin, \u2013 halte fest mich doch im Arm,", "tokens": ["S\u00fc\u00b7\u00dfe", "Wir\u00b7tin", ",", "\u2013", "hal\u00b7te", "fest", "mich", "doch", "im", "Arm", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "$(", "VVFIN", "ADJD", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Weil ich sonst zu Boden fall', \u2013 da\u00df Gott erbarm'!", "tokens": ["Weil", "ich", "sonst", "zu", "Bo\u00b7den", "fall'", ",", "\u2013", "da\u00df", "Gott", "er\u00b7barm'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$,", "$(", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "O, mein T\u00e4ubchen ... was ist doch dein Busen weich!", "tokens": ["O", ",", "mein", "T\u00e4ub\u00b7chen", "...", "was", "ist", "doch", "dein", "Bu\u00b7sen", "weich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$(", "PWS", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "La\u00df mich nur ein wenig ruhn da, wonnereich!", "tokens": ["La\u00df", "mich", "nur", "ein", "we\u00b7nig", "ruhn", "da", ",", "won\u00b7ne\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "PIS", "VVFIN", "ADV", "$,", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Hab' die Nacht ein hartes Bett, und wohn' gar weit,", "tokens": ["Hab'", "die", "Nacht", "ein", "har\u00b7tes", "Bett", ",", "und", "wohn'", "gar", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,", "KON", "PWAV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und gelange nimmermehr nach Hause heut! ...", "tokens": ["Und", "ge\u00b7lan\u00b7ge", "nim\u00b7mer\u00b7mehr", "nach", "Hau\u00b7se", "heut", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}