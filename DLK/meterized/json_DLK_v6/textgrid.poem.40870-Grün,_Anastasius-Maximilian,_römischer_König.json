{"textgrid.poem.40870": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Maximilian, r\u00f6mischer K\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwer auf dem morschen Haupte liegt mir die goldne Kron';", "tokens": ["Schwer", "auf", "dem", "mor\u00b7schen", "Haup\u00b7te", "liegt", "mir", "die", "gold\u00b7ne", "Kron'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du wirst sie leichter tragen, mein Max, mein starker Sohn!", "tokens": ["Du", "wirst", "sie", "leich\u00b7ter", "tra\u00b7gen", ",", "mein", "Max", ",", "mein", "star\u00b7ker", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,", "PPOSAT", "NE", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das Zepter, zitternd in meiner, ruht fest in deiner Hand.", "tokens": ["Das", "Zep\u00b7ter", ",", "zit\u00b7ternd", "in", "mei\u00b7ner", ",", "ruht", "fest", "in", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "PPOSAT", "$,", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So dachte der alte Kaiser, \u2013 so dachte das ganze Land.", "tokens": ["So", "dach\u00b7te", "der", "al\u00b7te", "Kai\u00b7ser", ",", "\u2013", "so", "dach\u00b7te", "das", "gan\u00b7ze", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Zu Aachen in dem Dome, da wogt's in Seid' und Sammt,", "tokens": ["Zu", "Aa\u00b7chen", "in", "dem", "Do\u00b7me", ",", "da", "wogt's", "in", "Seid'", "und", "Sammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,", "KOUS", "NE", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In Infuln und in Helmen, beim ersten Kr\u00f6nungsamt,", "tokens": ["In", "In\u00b7fuln", "und", "in", "Hel\u00b7men", ",", "beim", "ers\u00b7ten", "Kr\u00f6\u00b7nungs\u00b7amt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Da troff vom heil'gen Oele die Stirne Maxens verkl\u00e4rt,", "tokens": ["Da", "troff", "vom", "heil'\u00b7gen", "O\u00b7e\u00b7le", "die", "Stir\u00b7ne", "Ma\u00b7xens", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da trug er in w\u00fcrd'gen H\u00e4nden des gro\u00dfen Carols Schwert.", "tokens": ["Da", "trug", "er", "in", "w\u00fcrd'\u00b7gen", "H\u00e4n\u00b7den", "des", "gro\u00b7\u00dfen", "Ca\u00b7rols", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Von K\u00f6ln der greise Bischof vor dem Altare stand;", "tokens": ["Von", "K\u00f6ln", "der", "grei\u00b7se", "Bi\u00b7schof", "vor", "dem", "Al\u00b7ta\u00b7re", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie 'n treuer Freund, so sch\u00fcttelt ihm sonst das Alter die Hand", "tokens": ["Wie", "'n", "treu\u00b7er", "Freund", ",", "so", "sch\u00fct\u00b7telt", "ihm", "sonst", "das", "Al\u00b7ter", "die", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+--+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch fest und ohne Zittern fa\u00dft er die Krone jetzt,", "tokens": ["Doch", "fest", "und", "oh\u00b7ne", "Zit\u00b7tern", "fa\u00dft", "er", "die", "Kro\u00b7ne", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er wei\u00df ja, da\u00df noch Niemand auf bessern Ort sie gesetzt.", "tokens": ["Er", "wei\u00df", "ja", ",", "da\u00df", "noch", "Nie\u00b7mand", "auf", "bes\u00b7sern", "Ort", "sie", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV", "PIS", "APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Orgel ist verklungen. \u2013 Im hohen Kaisersaal,", "tokens": ["Die", "Or\u00b7gel", "ist", "ver\u00b7klun\u00b7gen", ".", "\u2013", "Im", "ho\u00b7hen", "Kai\u00b7ser\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "$(", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da sitzen die Herren und F\u00fcrsten beim heitern Kr\u00f6nungsmahl;", "tokens": ["Da", "sit\u00b7zen", "die", "Her\u00b7ren", "und", "F\u00fcrs\u00b7ten", "beim", "hei\u00b7tern", "Kr\u00f6\u00b7nungs\u00b7mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Aus Silberurnen rieselt der k\u00fchle Wein hervor,", "tokens": ["Aus", "Sil\u00b7be\u00b7rur\u00b7nen", "rie\u00b7selt", "der", "k\u00fch\u00b7le", "Wein", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und blaue W\u00f6lkchen kr\u00e4useln aus goldnen Sch\u00fcsseln empor.", "tokens": ["Und", "blau\u00b7e", "W\u00f6lk\u00b7chen", "kr\u00e4u\u00b7seln", "aus", "gold\u00b7nen", "Sch\u00fcs\u00b7seln", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Pf\u00e4lzer schwang den Becher und scherzend hub er an:", "tokens": ["Der", "Pf\u00e4l\u00b7zer", "schwang", "den", "Be\u00b7cher", "und", "scher\u00b7zend", "hub", "er", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbhoch, Vater Rhein! Ihr Herren, wer ist's, der's r\u00fchmen kann,", "tokens": ["\u00bb", "hoch", ",", "Va\u00b7ter", "Rhein", "!", "Ihr", "Her\u00b7ren", ",", "wer", "ist's", ",", "der's", "r\u00fch\u00b7men", "kann", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "NE", "$.", "PPOSAT", "NN", "$,", "PWS", "VAFIN", "$,", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er seh' solch edles Kleinod in seinem Lande stammen,", "tokens": ["Er", "seh'", "solch", "ed\u00b7les", "Klei\u00b7nod", "in", "sei\u00b7nem", "Lan\u00b7de", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das so, wie meine Reben, die Herzen mag entflammen?\u00ab", "tokens": ["Das", "so", ",", "wie", "mei\u00b7ne", "Re\u00b7ben", ",", "die", "Her\u00b7zen", "mag", "ent\u00b7flam\u00b7men", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ADV", "$,", "PWAV", "PPOSAT", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Da priesen in der Runde die F\u00fcrsten Thron und Reich,", "tokens": ["Da", "prie\u00b7sen", "in", "der", "Run\u00b7de", "die", "F\u00fcrs\u00b7ten", "Thron", "und", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der alte Kaiser Friedrich pries hoch sein Oesterreich,", "tokens": ["Der", "al\u00b7te", "Kai\u00b7ser", "Fried\u00b7rich", "pries", "hoch", "sein", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Von K\u00f6ln der greise Bischof r\u00fchmt seinen Riesendom,", "tokens": ["Von", "K\u00f6ln", "der", "grei\u00b7se", "Bi\u00b7schof", "r\u00fchmt", "sei\u00b7nen", "Rie\u00b7sen\u00b7dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Der Baier seine Fluren und seinen blauen Strom.", "tokens": ["Der", "Bai\u00b7er", "sei\u00b7ne", "Flu\u00b7ren", "und", "sei\u00b7nen", "blau\u00b7en", "Strom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Aus Sachsenland Herr Albert,", "tokens": ["Aus", "Sach\u00b7sen\u00b7land", "Herr", "Al\u00b7bert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbes blitzt als Gold und Eisen in dunklem Schacht mein Hort,", "tokens": ["\u00bb", "es", "blitzt", "als", "Gold", "und", "Ei\u00b7sen", "in", "dunk\u00b7lem", "Schacht", "mein", "Hort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KOKOM", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das Gold lehrt unsre Weiber, so lauter und so rein,", "tokens": ["Das", "Gold", "lehrt", "uns\u00b7re", "Wei\u00b7ber", ",", "so", "lau\u00b7ter", "und", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das Eisen unsre M\u00e4nner, so stark und treu zu sein.\u00ab", "tokens": ["Das", "Ei\u00b7sen", "uns\u00b7re", "M\u00e4n\u00b7ner", ",", "so", "stark", "und", "treu", "zu", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Drauf sprach der W\u00fcrtemberger, Graf Eberhard im Bart:", "tokens": ["Drauf", "sprach", "der", "W\u00fcr\u00b7tem\u00b7ber\u00b7ger", ",", "Graf", "E\u00b7ber\u00b7hard", "im", "Bart", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "NE", "NE", "APPRART", "NN", "$."], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbzwar sprie\u00dft in meinen Gauen kein Kleinod solcher Art;", "tokens": ["\u00bb", "zwar", "sprie\u00dft", "in", "mei\u00b7nen", "Gau\u00b7en", "kein", "Klei\u00b7nod", "sol\u00b7cher", "Art", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch k\u00e4m' im tiefsten Walde mir Lust zu schlummern an,", "tokens": ["Doch", "k\u00e4m'", "im", "tiefs\u00b7ten", "Wal\u00b7de", "mir", "Lust", "zu", "schlum\u00b7mern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PPER", "NN", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "W\u00e4r' jeder Schoo\u00df mein Kissen, drauf sanft ich schlafen kann.\u00ab", "tokens": ["W\u00e4r'", "je\u00b7der", "Schoo\u00df", "mein", "Kis\u00b7sen", ",", "drauf", "sanft", "ich", "schla\u00b7fen", "kann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PPOSAT", "NN", "$,", "PAV", "VVFIN", "PPER", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Einst h\u00e4tt' in solchem Wettstreit Max auch ein Wort gebracht,", "tokens": ["Einst", "h\u00e4tt'", "in", "sol\u00b7chem", "Wett\u00b7streit", "Max", "auch", "ein", "Wort", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN", "NE", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jetzt h\u00fcllt die schwarze Erde, ach, all sein Gl\u00fcck in Nacht;", "tokens": ["Jetzt", "h\u00fcllt", "die", "schwar\u00b7ze", "Er\u00b7de", ",", "ach", ",", "all", "sein", "Gl\u00fcck", "in", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ITJ", "$,", "PIAT", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Drum blieb auf seine Lippen ein d\u00fcstres Schweigen gebannt,", "tokens": ["Drum", "blieb", "auf", "sei\u00b7ne", "Lip\u00b7pen", "ein", "d\u00fcst\u00b7res", "Schwei\u00b7gen", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch leis und innig dr\u00fcckt' er des W\u00fcrtembergers Hand.", "tokens": ["Doch", "leis", "und", "in\u00b7nig", "dr\u00fcckt'", "er", "des", "W\u00fcr\u00b7tem\u00b7ber\u00b7gers", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}}}}}