{"textgrid.poem.48760": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "47. \u00dcber der Salven vor Astrachan, den 15. September 1636", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Nymfen, la\u00dft euch nicht verst\u00f6ren euer Bad.", "tokens": ["Ihr", "Nym\u00b7fen", ",", "la\u00dft", "euch", "nicht", "ver\u00b7st\u00f6\u00b7ren", "eu\u00b7er", "Bad", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Empanda, komm' herbei, legt nicht ab eure Schleier,", "tokens": ["Em\u00b7pan\u00b7da", ",", "komm'", "her\u00b7bei", ",", "legt", "nicht", "ab", "eu\u00b7re", "Schlei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ihr blumichten Napeen. Gib deinen Winzern Feier,", "tokens": ["ihr", "blu\u00b7mich\u00b7ten", "Na\u00b7peen", ".", "Gib", "dei\u00b7nen", "Win\u00b7zern", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "VVIMP", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lycnita, und schau zu. Und du auch, edle Stadt,", "tokens": ["Lyc\u00b7ni\u00b7ta", ",", "und", "schau", "zu", ".", "Und", "du", "auch", ",", "ed\u00b7le", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADJD", "PTKVZ", "$.", "KON", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "die eh' des T\u00fcrken Grimm vor sich verlachet, hat,", "tokens": ["die", "eh'", "des", "T\u00fcr\u00b7ken", "Grimm", "vor", "sich", "ver\u00b7la\u00b7chet", ",", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "NE", "APPR", "PRF", "VVFIN", "$,", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "trit um den sch\u00f6nen Strand. Denn dieser Preis ist euer,", "tokens": ["trit", "um", "den", "sch\u00f6\u00b7nen", "Strand", ".", "Denn", "die\u00b7ser", "Preis", "ist", "eu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "KON", "PDAT", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der Preis den ", "tokens": ["der", "Preis", "den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Kommt sicher, h\u00f6rt uns recht; kommt frei, beseht uns satt.", "tokens": ["Kommt", "si\u00b7cher", ",", "h\u00f6rt", "uns", "recht", ";", "kommt", "frei", ",", "be\u00b7seht", "uns", "satt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "$.", "VVFIN", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Spiele lauter L\u00e4rm, der Flaggen rotes Blut,", "tokens": ["Der", "Spie\u00b7le", "lau\u00b7ter", "L\u00e4rm", ",", "der", "Flag\u00b7gen", "ro\u00b7tes", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Haken dunkler Plitz, der St\u00fccken tr\u00fcbe Glut", "tokens": ["der", "Ha\u00b7ken", "dunk\u00b7ler", "Plitz", ",", "der", "St\u00fc\u00b7cken", "tr\u00fc\u00b7be", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und was ihr Schrecklichs seht und was ihr Furchtsams h\u00f6ret,", "tokens": ["und", "was", "ihr", "Schreck\u00b7lichs", "seht", "und", "was", "ihr", "Furcht\u00b7sams", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVFIN", "KON", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "das mach euch keine Furcht. Der feindgestalt'te Freund,", "tokens": ["das", "mach", "euch", "kei\u00b7ne", "Furcht", ".", "Der", "feind\u00b7ge\u00b7stalt'\u00b7te", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "di\u00df Schiff l\u00e4\u00dft euch auch sehn, worauf es ist gelehret.", "tokens": ["di\u00df", "Schiff", "l\u00e4\u00dft", "euch", "auch", "sehn", ",", "wo\u00b7rauf", "es", "ist", "ge\u00b7leh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Scherz der ist f\u00fcr euch, sein Ernst f\u00fcr seinen Feind.", "tokens": ["Sein", "Scherz", "der", "ist", "f\u00fcr", "euch", ",", "sein", "Ernst", "f\u00fcr", "sei\u00b7nen", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}