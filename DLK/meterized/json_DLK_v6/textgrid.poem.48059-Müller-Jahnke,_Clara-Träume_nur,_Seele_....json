{"textgrid.poem.48059": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00e4ume nur, Seele ...", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den verd\u00e4mmernden Herbsttag hinein", "tokens": ["In", "den", "ver\u00b7d\u00e4m\u00b7mern\u00b7den", "Herbst\u00b7tag", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APZR"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "zauberst du lachenden Sonnenschein,", "tokens": ["zau\u00b7berst", "du", "la\u00b7chen\u00b7den", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "und aus der Bl\u00e4tter vergilbendem Flor", "tokens": ["und", "aus", "der", "Bl\u00e4t\u00b7ter", "ver\u00b7gil\u00b7ben\u00b7dem", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "bl\u00fchen dir duftige Veilchen empor,", "tokens": ["bl\u00fc\u00b7hen", "dir", "duf\u00b7ti\u00b7ge", "Veil\u00b7chen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "tr\u00e4umende Seele \u2013", "tokens": ["tr\u00e4u\u00b7men\u00b7de", "See\u00b7le", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "T\u00f6nt denn der Glocken dumpfhallender Klang", "tokens": ["T\u00f6nt", "denn", "der", "Glo\u00b7cken", "dumpf\u00b7hal\u00b7len\u00b7der", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "dir wie ein schmetternder Lerchengesang?", "tokens": ["dir", "wie", "ein", "schmet\u00b7tern\u00b7der", "Ler\u00b7chen\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Siehst du der Erde verweintes Gesicht,", "tokens": ["Siehst", "du", "der", "Er\u00b7de", "ver\u00b7wein\u00b7tes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "f\u00fchlst du die eisigen Nebel denn nicht,", "tokens": ["f\u00fchlst", "du", "die", "ei\u00b7si\u00b7gen", "Ne\u00b7bel", "denn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "PTKNEG", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "tr\u00e4umende Seele? \u2013", "tokens": ["tr\u00e4u\u00b7men\u00b7de", "See\u00b7le", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Tr\u00e4ume nur, tr\u00e4ume . . . der Fr\u00fchling ist weit;", "tokens": ["Tr\u00e4u\u00b7me", "nur", ",", "tr\u00e4u\u00b7me", ".", ".", ".", "der", "Fr\u00fch\u00b7ling", "ist", "weit", ";"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "$.", "$.", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Rosen hat's nimmer im Winter geschneit \u2013", "tokens": ["Ro\u00b7sen", "hat's", "nim\u00b7mer", "im", "Win\u00b7ter", "ge\u00b7schneit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "dumpf nur und klagend, verweht vom Nordwest,", "tokens": ["dumpf", "nur", "und", "kla\u00b7gend", ",", "ver\u00b7weht", "vom", "Nord\u00b7west", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "VVPP", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "l\u00e4uten die Glocken zum Totenfest.", "tokens": ["l\u00e4u\u00b7ten", "die", "Glo\u00b7cken", "zum", "To\u00b7ten\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Tr\u00e4ume nur, Seele . . .", "tokens": ["Tr\u00e4u\u00b7me", "nur", ",", "See\u00b7le", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$.", "$.", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}