{"textgrid.poem.24870": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Barcarole", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um der fallenden Ruder Spitzen", "tokens": ["Um", "der", "fal\u00b7len\u00b7den", "Ru\u00b7der", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zittert und leuchtet ein schimmernder Glanz,", "tokens": ["Zit\u00b7tert", "und", "leuch\u00b7tet", "ein", "schim\u00b7mern\u00b7der", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Flieht bei jedem Schlage mit Blitzen", "tokens": ["Flieht", "bei", "je\u00b7dem", "Schla\u00b7ge", "mit", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "APPR", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hin von Wellen zu Wellen im Tanz.", "tokens": ["Hin", "von", "Wel\u00b7len", "zu", "Wel\u00b7len", "im", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mir im Busen von Liebeswonnen", "tokens": ["Mir", "im", "Bu\u00b7sen", "von", "Lie\u00b7bes\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "APPR", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zittert und leuchtet das Herz wie die Flut,", "tokens": ["Zit\u00b7tert", "und", "leuch\u00b7tet", "das", "Herz", "wie", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Jubelt hinauf zu den Sternen und Sonnen,", "tokens": ["Ju\u00b7belt", "hin\u00b7auf", "zu", "den", "Ster\u00b7nen", "und", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Bebt zu vergehn in der wogenden Glut.", "tokens": ["Bebt", "zu", "ver\u00b7gehn", "in", "der", "wo\u00b7gen\u00b7den", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon auf dem Felsen durchs Gr\u00fcn der Platane", "tokens": ["Schon", "auf", "dem", "Fel\u00b7sen", "durchs", "Gr\u00fcn", "der", "Pla\u00b7ta\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Seh' ich das s\u00e4ulengetragene Dach,", "tokens": ["Seh'", "ich", "das", "s\u00e4u\u00b7len\u00b7ge\u00b7tra\u00b7ge\u00b7ne", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und das flimmernde Licht am Altane", "tokens": ["Und", "das", "flim\u00b7mern\u00b7de", "Licht", "am", "Al\u00b7ta\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "K\u00fcndet mir, da\u00df die Geliebte noch wach.", "tokens": ["K\u00fcn\u00b7det", "mir", ",", "da\u00df", "die", "Ge\u00b7lieb\u00b7te", "noch", "wach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Fliege, mein Kahn! und birg uns verschwiegen,", "tokens": ["Flie\u00b7ge", ",", "mein", "Kahn", "!", "und", "birg", "uns", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "KON", "VVIMP", "PPER", "VVPP", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Birg uns, selige Nacht des August!", "tokens": ["Birg", "uns", ",", "se\u00b7li\u00b7ge", "Nacht", "des", "Au\u00b7gust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "S\u00fc\u00df wohl ist's, auf den Wellen sich wiegen,", "tokens": ["S\u00fc\u00df", "wohl", "ist's", ",", "auf", "den", "Wel\u00b7len", "sich", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "$,", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aber s\u00fc\u00dfer an ihrer Brust.", "tokens": ["A\u00b7ber", "s\u00fc\u00b7\u00dfer", "an", "ih\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}