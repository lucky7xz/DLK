{"textgrid.poem.62555": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Heiland ist erstanden", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Heiland ist erstanden", "tokens": ["Mein", "Hei\u00b7land", "ist", "er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus finstrer Grabesnacht;", "tokens": ["Aus", "finst\u00b7rer", "Gra\u00b7bes\u00b7nacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tod, wo sind deine Banden?", "tokens": ["Tod", ",", "wo", "sind", "dei\u00b7ne", "Ban\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wo, H\u00f6lle, deine Macht?", "tokens": ["Wo", ",", "H\u00f6l\u00b7le", ",", "dei\u00b7ne", "Macht", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du kannst mich nicht mehr schrecken,", "tokens": ["Du", "kannst", "mich", "nicht", "mehr", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du modervolle Gruft;", "tokens": ["Du", "mo\u00b7der\u00b7vol\u00b7le", "Gruft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sollst mich nur bedecken,", "tokens": ["Du", "sollst", "mich", "nur", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis mich mein Heiland ruft.", "tokens": ["Bis", "mich", "mein", "Hei\u00b7land", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er wird am Tag der Garben,", "tokens": ["Er", "wird", "am", "Tag", "der", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Leichenfeldern gehn;", "tokens": ["Auf", "Lei\u00b7chen\u00b7fel\u00b7dern", "gehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird wecken, die ihm starben,", "tokens": ["Wird", "we\u00b7cken", ",", "die", "ihm", "star\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und seine Saaten m\u00e4hn.", "tokens": ["Und", "sei\u00b7ne", "Saa\u00b7ten", "m\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir leben im Vertrauen,", "tokens": ["Wir", "le\u00b7ben", "im", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir sterben unsrer Pflicht;", "tokens": ["Wir", "ster\u00b7ben", "uns\u00b7rer", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Glauben f\u00fchrt zum Schauen,", "tokens": ["Das", "Glau\u00b7ben", "f\u00fchrt", "zum", "Schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Kreuzes Bahn zum Licht.", "tokens": ["Des", "Kreu\u00b7zes", "Bahn", "zum", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}