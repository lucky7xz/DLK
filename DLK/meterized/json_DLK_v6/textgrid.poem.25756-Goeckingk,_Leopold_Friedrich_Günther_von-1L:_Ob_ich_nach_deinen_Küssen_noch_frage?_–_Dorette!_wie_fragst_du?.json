{"textgrid.poem.25756": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob ich nach deinen K\u00fcssen noch frage? \u2013 Dorette! wie fragst du?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ich nach deinen K\u00fcssen noch frage? \u2013 Dorette! wie fragst du?", "tokens": ["Ob", "ich", "nach", "dei\u00b7nen", "K\u00fcs\u00b7sen", "noch", "fra\u00b7ge", "?", "\u2013", "Do\u00b7ret\u00b7te", "!", "wie", "fragst", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$.", "$(", "NN", "$.", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Was ist unter dem Mond' mehr wohl der Frage noch werth,", "tokens": ["Was", "ist", "un\u00b7ter", "dem", "Mond'", "mehr", "wohl", "der", "Fra\u00b7ge", "noch", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "ADV", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Als ein herzlicher Ku\u00df vom Rosenmunde der Unschuld?", "tokens": ["Als", "ein", "herz\u00b7li\u00b7cher", "Ku\u00df", "vom", "Ro\u00b7sen\u00b7mun\u00b7de", "der", "Un\u00b7schuld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mindestens d\u00fcnket er doch Dichtern das liebste der Welt.", "tokens": ["Min\u00b7des\u00b7tens", "d\u00fcn\u00b7ket", "er", "doch", "Dich\u00b7tern", "das", "liebs\u00b7te", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "NN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Gold kann jeder im Schwei\u00df' des Angesichtes erwerben,", "tokens": ["Gold", "kann", "je\u00b7der", "im", "Schwei\u00df'", "des", "An\u00b7ge\u00b7sich\u00b7tes", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Man kauft vieles daf\u00fcr, K\u00fcsse wie deine nur nicht.", "tokens": ["Man", "kauft", "vie\u00b7les", "da\u00b7f\u00fcr", ",", "K\u00fcs\u00b7se", "wie", "dei\u00b7ne", "nur", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "PAV", "$,", "NN", "KOKOM", "PPOSAT", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Was man mit Golde nicht kauft, das k\u00f6nnten freilich die F\u00fcrsten", "tokens": ["Was", "man", "mit", "Gol\u00b7de", "nicht", "kauft", ",", "das", "k\u00f6nn\u00b7ten", "frei\u00b7lich", "die", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "APPR", "NN", "PTKNEG", "VVFIN", "$,", "PDS", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Einem Dichter verleihn: W\u00fcrden und Orden und Macht.", "tokens": ["Ei\u00b7nem", "Dich\u00b7ter", "ver\u00b7leihn", ":", "W\u00fcr\u00b7den", "und", "Or\u00b7den", "und", "Macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Aber sie k\u00f6nnten doch nicht Dorettens Kusse gebieten,", "tokens": ["A\u00b7ber", "sie", "k\u00f6nn\u00b7ten", "doch", "nicht", "Do\u00b7ret\u00b7tens", "Kus\u00b7se", "ge\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "NN", "VVINF", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Da\u00df er auf den Mund h\u00fcpf' aus dem Herzen empor,", "tokens": ["Da\u00df", "er", "auf", "den", "Mund", "h\u00fcpf'", "aus", "dem", "Her\u00b7zen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.11": {"text": "Da\u00df er bemerken nicht soll die silberfarbenen Haare,", "tokens": ["Da\u00df", "er", "be\u00b7mer\u00b7ken", "nicht", "soll", "die", "sil\u00b7ber\u00b7far\u00b7be\u00b7nen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKNEG", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Nicht die gefaltete Stirn, nicht den gebogenen Hals.", "tokens": ["Nicht", "die", "ge\u00b7fal\u00b7te\u00b7te", "Stirn", ",", "nicht", "den", "ge\u00b7bo\u00b7ge\u00b7nen", "Hals", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Wenn sich je der Wunsch nach diesem Kusse verl\u00f6re:", "tokens": ["Wenn", "sich", "je", "der", "Wunsch", "nach", "die\u00b7sem", "Kus\u00b7se", "ver\u00b7l\u00f6\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Dann so hoble du mir, Tischler! die Bretter zum Sarg'.", "tokens": ["Dann", "so", "hob\u00b7le", "du", "mir", ",", "Tischler", "!", "die", "Bret\u00b7ter", "zum", "Sar\u00b7g'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "$.", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+--++", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Todtengr\u00e4ber! bereite mir dann nur immer im voraus", "tokens": ["Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "!", "be\u00b7rei\u00b7te", "mir", "dann", "nur", "im\u00b7mer", "im", "vo\u00b7raus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPRART", "PTKVZ"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Eine Grube, die mich, Hoffnung und W\u00fcnsche verschlingt.", "tokens": ["Ei\u00b7ne", "Gru\u00b7be", ",", "die", "mich", ",", "Hoff\u00b7nung", "und", "W\u00fcn\u00b7sche", "ver\u00b7schlingt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Aber nehmet euch beide noch Zeit; denn wisset, im Fr\u00fchling'", "tokens": ["A\u00b7ber", "neh\u00b7met", "euch", "bei\u00b7de", "noch", "Zeit", ";", "denn", "wis\u00b7set", ",", "im", "Fr\u00fch\u00b7ling'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV", "NN", "$.", "KON", "VVFIN", "$,", "APPRART", "NN"], "meter": "--+--+--+-++-+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Kommt Dorette zu mir. Wartet so lange vorerst!", "tokens": ["Kommt", "Do\u00b7ret\u00b7te", "zu", "mir", ".", "War\u00b7tet", "so", "lan\u00b7ge", "vor\u00b7erst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPER", "$.", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}