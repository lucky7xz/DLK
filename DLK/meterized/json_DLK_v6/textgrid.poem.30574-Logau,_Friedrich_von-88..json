{"textgrid.poem.30574": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Br\u00fcck ist auffgebauet, dr\u00fcber bringt man in die Stadt", "tokens": ["Ei\u00b7ne", "Br\u00fcck", "ist", "auff\u00b7ge\u00b7bau\u00b7et", ",", "dr\u00fc\u00b7ber", "bringt", "man", "in", "die", "Stadt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PAV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Thiere, Fische, V\u00f6gel, Fr\u00fcchte, was man kaum zu nennen hat;", "tokens": ["Thie\u00b7re", ",", "Fi\u00b7sche", ",", "V\u00f6\u00b7gel", ",", "Fr\u00fcch\u00b7te", ",", "was", "man", "kaum", "zu", "nen\u00b7nen", "hat", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "PRELS", "PIS", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dieses nicht, sonst aber manches, k\u00fcmt zu r\u00fccke durch das Thor,", "tokens": ["Die\u00b7ses", "nicht", ",", "sonst", "a\u00b7ber", "man\u00b7ches", ",", "k\u00fcmt", "zu", "r\u00fc\u00b7cke", "durch", "das", "Thor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "$,", "ADV", "ADV", "PIS", "$,", "VVFIN", "PTKZU", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch nicht, was das Auge sihet, sondern nur vernimt das Ohr.", "tokens": ["Doch", "nicht", ",", "was", "das", "Au\u00b7ge", "si\u00b7het", ",", "son\u00b7dern", "nur", "ver\u00b7nimt", "das", "Ohr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "F\u00fcr die Br\u00fccke, f\u00fcr die Wahren wil der Bauherr keinen Zoll,", "tokens": ["F\u00fcr", "die", "Br\u00fc\u00b7cke", ",", "f\u00fcr", "die", "Wah\u00b7ren", "wil", "der", "Bau\u00b7herr", "kei\u00b7nen", "Zoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Ausser da\u00df man seiner G\u00fcte hertzlich immer dancken soll;", "tokens": ["Aus\u00b7ser", "da\u00df", "man", "sei\u00b7ner", "G\u00fc\u00b7te", "hertz\u00b7lich", "im\u00b7mer", "dan\u00b7cken", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "PPOSAT", "NN", "ADJD", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Einer thut es kaum von Zehnen, fluchen mehr und lestern eh;", "tokens": ["Ei\u00b7ner", "thut", "es", "kaum", "von", "Zeh\u00b7nen", ",", "flu\u00b7chen", "mehr", "und", "les\u00b7tern", "eh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "VVFIN", "ADV", "KON", "VVFIN", "KOUS", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Er ist g\u00fctig, strafft nicht balde; endlich doch folgt ewig Weh.", "tokens": ["Er", "ist", "g\u00fc\u00b7tig", ",", "strafft", "nicht", "bal\u00b7de", ";", "end\u00b7lich", "doch", "folgt", "e\u00b7wig", "Weh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VVFIN", "PTKNEG", "ADV", "$.", "ADV", "ADV", "VVFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}