{"textgrid.poem.24211": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: R\u00fcste dich, mein Herz, und bebe", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fcste dich, mein Herz, und bebe", "tokens": ["R\u00fcs\u00b7te", "dich", ",", "mein", "Herz", ",", "und", "be\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so sehr: es wird geschehen,", "tokens": ["Nicht", "so", "sehr", ":", "es", "wird", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich ihre N\u00e4he f\u00fchle,", "tokens": ["Da\u00df", "ich", "ih\u00b7re", "N\u00e4\u00b7he", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn mein Auge wird sie sehen.", "tokens": ["Denn", "mein", "Au\u00b7ge", "wird", "sie", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wird sie sein, wie ich sie tr\u00e4ume?", "tokens": ["Wird", "sie", "sein", ",", "wie", "ich", "sie", "tr\u00e4u\u00b7me", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VAINF", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird mein Hoffen j\u00e4h vergehen?", "tokens": ["Wird", "mein", "Hof\u00b7fen", "j\u00e4h", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fcste dich, mein Herz, mit St\u00e4rke,", "tokens": ["R\u00fcs\u00b7te", "dich", ",", "mein", "Herz", ",", "mit", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn mein Auge wird sie sehen.", "tokens": ["Denn", "mein", "Au\u00b7ge", "wird", "sie", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}