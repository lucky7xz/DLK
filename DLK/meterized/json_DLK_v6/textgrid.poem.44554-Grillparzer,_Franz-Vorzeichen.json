{"textgrid.poem.44554": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Vorzeichen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich der Untergang auf Staat und Haus ger\u00fcstet,", "tokens": ["Wenn", "sich", "der", "Un\u00b7ter\u00b7gang", "auf", "Staat", "und", "Haus", "ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schickt er seinen Herold erst voran,", "tokens": ["So", "schickt", "er", "sei\u00b7nen", "He\u00b7rold", "erst", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dems nach der Umkehr des Gewordnen l\u00fcstet,", "tokens": ["Dems", "nach", "der", "Um\u00b7kehr", "des", "Ge\u00b7word\u00b7nen", "l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "++--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Den Wahnsinn, der den Sinn verkehrt in Wahn.", "tokens": ["Den", "Wahn\u00b7sinn", ",", "der", "den", "Sinn", "ver\u00b7kehrt", "in", "Wahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der schl\u00e4gt den M\u00f6rtel ab und l\u00f6st die Fugen,", "tokens": ["Der", "schl\u00e4gt", "den", "M\u00f6r\u00b7tel", "ab", "und", "l\u00f6st", "die", "Fu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Damit des Meisters Arbeit leicht und kurz,", "tokens": ["Da\u00b7mit", "des", "Meis\u00b7ters", "Ar\u00b7beit", "leicht", "und", "kurz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die St\u00fctzen wanken, die den Giebel trugen,", "tokens": ["Die", "St\u00fct\u00b7zen", "wan\u00b7ken", ",", "die", "den", "Gie\u00b7bel", "tru\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und weithin donnere der j\u00e4he Sturz.", "tokens": ["Und", "weit\u00b7hin", "don\u00b7ne\u00b7re", "der", "j\u00e4\u00b7he", "Sturz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da ist ein zwecklos Rennen, t\u00f6richt Schaffen,", "tokens": ["Da", "ist", "ein", "zweck\u00b7los", "Ren\u00b7nen", ",", "t\u00f6\u00b7richt", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Fliehen und ein Suchen auch der Not,", "tokens": ["Ein", "Flie\u00b7hen", "und", "ein", "Su\u00b7chen", "auch", "der", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man zahlt mit Gold und sch\u00e4rft die schneidgen Waffen,", "tokens": ["Man", "zahlt", "mit", "Gold", "und", "sch\u00e4rft", "die", "schneid\u00b7gen", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die f\u00e4rben soll des Eigners eigner Tod.", "tokens": ["Die", "f\u00e4r\u00b7ben", "soll", "des", "Eig\u00b7ners", "eig\u00b7ner", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie Roboam, als, die beim Volk in Ehren,", "tokens": ["Wie", "Ro\u00b7boam", ",", "als", ",", "die", "beim", "Volk", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "KOUS", "$,", "PRELS", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Steuerdruck ihm klagten als verha\u00dft,", "tokens": ["Den", "Steu\u00b7er\u00b7druck", "ihm", "klag\u00b7ten", "als", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ausrief, den Zoll ums Doppelte zu mehren,", "tokens": ["Aus\u00b7rief", ",", "den", "Zoll", "ums", "Dop\u00b7pel\u00b7te", "zu", "meh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Finger wiege gleich der sonstgen Last.", "tokens": ["Sein", "Fin\u00b7ger", "wie\u00b7ge", "gleich", "der", "sonst\u00b7gen", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als vor Byzanz die Moslim schon zu schauen,", "tokens": ["Als", "vor", "By\u00b7zanz", "die", "Mos\u00b7lim", "schon", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "ART", "NE", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Einigkeit zu retten nur vermag,", "tokens": ["Und", "Ei\u00b7nig\u00b7keit", "zu", "ret\u00b7ten", "nur", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da stritten sich die Gr\u00fcnen und die Blauen;", "tokens": ["Da", "strit\u00b7ten", "sich", "die", "Gr\u00fc\u00b7nen", "und", "die", "Blau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schwarzen ohnehin bis diesen Tag.", "tokens": ["Die", "Schwar\u00b7zen", "oh\u00b7ne\u00b7hin", "bis", "die\u00b7sen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wenn nun ein Letztes hinweist auf die Fr\u00fchern,", "tokens": ["Wenn", "nun", "ein", "Letz\u00b7tes", "hin\u00b7weist", "auf", "die", "Fr\u00fc\u00b7hern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist auch ein Fr\u00fchres nur, weil eins zuletzt,", "tokens": ["Ist", "auch", "ein", "Fr\u00fch\u00b7res", "nur", ",", "weil", "eins", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,", "KOUS", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6rst du erst des Wahnsinns Lache wiehern,", "tokens": ["Und", "h\u00f6rst", "du", "erst", "des", "Wahn\u00b7sinns", "La\u00b7che", "wie\u00b7hern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Klingts mit des Unheils Weinen schon versetzt.", "tokens": ["Klingts", "mit", "des", "Un\u00b7heils", "Wei\u00b7nen", "schon", "ver\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich wei\u00df ein Land, das lag so unbeweglich,", "tokens": ["Ich", "wei\u00df", "ein", "Land", ",", "das", "lag", "so", "un\u00b7be\u00b7weg\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es regte kaum die Glieder wie ein Wurm,", "tokens": ["Es", "reg\u00b7te", "kaum", "die", "Glie\u00b7der", "wie", "ein", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Ringen schob sichs nach der Nahrung t\u00e4glich,", "tokens": ["In", "Rin\u00b7gen", "schob", "sichs", "nach", "der", "Nah\u00b7rung", "t\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Zeit war nur im Glockenschlag vom Turm;", "tokens": ["Die", "Zeit", "war", "nur", "im", "Glo\u00b7cken\u00b7schlag", "vom", "Turm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die n\u00e4chste N\u00e4he lag auf hundert Meilen,", "tokens": ["Die", "n\u00e4chs\u00b7te", "N\u00e4\u00b7he", "lag", "auf", "hun\u00b7dert", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die D\u00e4mmerung gab noch zu helles Licht,", "tokens": ["Die", "D\u00e4m\u00b7me\u00b7rung", "gab", "noch", "zu", "hel\u00b7les", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das H\u00f6chste schien des Niedern Schmach zu teilen,", "tokens": ["Das", "H\u00f6chs\u00b7te", "schien", "des", "Nie\u00b7dern", "Schmach", "zu", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Ruhe war nicht blo\u00df der B\u00fcrger Pflicht.", "tokens": ["Und", "Ru\u00b7he", "war", "nicht", "blo\u00df", "der", "B\u00fcr\u00b7ger", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Da b\u00e4umt sichs pl\u00f6tzlich auf wie b\u00f6se Fieber,", "tokens": ["Da", "b\u00e4umt", "sichs", "pl\u00f6tz\u00b7lich", "auf", "wie", "b\u00f6\u00b7se", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein schaurig Wehen geht durchs ganze Land,", "tokens": ["Ein", "schau\u00b7rig", "We\u00b7hen", "geht", "durchs", "gan\u00b7ze", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Wellen steigts und st\u00fcrzt sich brandend \u00fcber,", "tokens": ["In", "Wel\u00b7len", "steigts", "und", "st\u00fcrzt", "sich", "bran\u00b7dend", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "PRF", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gel\u00f6st ist des Gewohnten altes Band.", "tokens": ["Ge\u00b7l\u00f6st", "ist", "des", "Ge\u00b7wohn\u00b7ten", "al\u00b7tes", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Das matte Aug strengt an die bl\u00f6den Sterne", "tokens": ["Das", "mat\u00b7te", "Aug", "strengt", "an", "die", "bl\u00f6\u00b7den", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sucht des \u00dcbels Keim, der gar zu nah,", "tokens": ["Und", "sucht", "des", "\u00dc\u00b7bels", "Keim", ",", "der", "gar", "zu", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,", "PRELS", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit leerem Gr\u00fcbeln in der weiten Ferne,", "tokens": ["Mit", "lee\u00b7rem", "Gr\u00fc\u00b7beln", "in", "der", "wei\u00b7ten", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erforscht, was wird und nicht, was l\u00e4ngst geschah.", "tokens": ["Er\u00b7forscht", ",", "was", "wird", "und", "nicht", ",", "was", "l\u00e4ngst", "ge\u00b7schah", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "KON", "PTKNEG", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Die b\u00f6sen Fugen, die die Zeit gelichtet", "tokens": ["Die", "b\u00f6\u00b7sen", "Fu\u00b7gen", ",", "die", "die", "Zeit", "ge\u00b7lich\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die die Tr\u00e4gheit kaum noch hielt in Haft,", "tokens": ["Und", "die", "die", "Tr\u00e4g\u00b7heit", "kaum", "noch", "hielt", "in", "Haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADV", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 La\u00dft sehen, ob ein Ansto\u00df sie verdichtet,", "tokens": ["\u2013", "La\u00dft", "se\u00b7hen", ",", "ob", "ein", "An\u00b7sto\u00df", "sie", "ver\u00b7dich\u00b7tet", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "VVINF", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Widerstand verdoppelt ja die Kraft!", "tokens": ["Der", "Wi\u00b7der\u00b7stand", "ver\u00b7dop\u00b7pelt", "ja", "die", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "St\u00f6rt sie im Schlaf der Feile dumpfes Nagen,", "tokens": ["St\u00f6rt", "sie", "im", "Schlaf", "der", "Fei\u00b7le", "dum\u00b7pfes", "Na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Teilt andern mit des eignen Volkes Druck!", "tokens": ["\u2013", "