{"textgrid.poem.37298": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[w\u00e4rst du wirklich so ein rechter]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4rst du wirklich so ein rechter", "tokens": ["W\u00e4rst", "du", "wirk\u00b7lich", "so", "ein", "rech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wahrhaftiger Asket,", "tokens": ["Und", "wahr\u00b7haf\u00b7ti\u00b7ger", "As\u00b7ket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "So ein Welt- und Kostver\u00e4chter,", "tokens": ["So", "ein", "Welt", "und", "Kost\u00b7ver\u00b7\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der bis an die Wurzel geht,", "tokens": ["Der", "bis", "an", "die", "Wur\u00b7zel", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dem des Goldes freundlich Blinken,", "tokens": ["Dem", "des", "Gol\u00b7des", "freund\u00b7lich", "Blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem die Liebe eine Last,", "tokens": ["Dem", "die", "Lie\u00b7be", "ei\u00b7ne", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der das Essen und das Trinken,", "tokens": ["Der", "das", "Es\u00b7sen", "und", "das", "Trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der des Ruhmes Kr\u00e4nze ha\u00dft \u2013", "tokens": ["Der", "des", "Ruh\u00b7mes", "Kr\u00e4n\u00b7ze", "ha\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Gekratze und Gejucke,", "tokens": ["Das", "Ge\u00b7krat\u00b7ze", "und", "Ge\u00b7ju\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Jammer h\u00f6rte auf;", "tokens": ["Al\u00b7ler", "Jam\u00b7mer", "h\u00f6r\u00b7te", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kracks! mit einem einz'gen Rucke", "tokens": ["Kracks", "!", "mit", "ei\u00b7nem", "einz'\u00b7gen", "Ru\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hemmtest du den Weltenlauf.", "tokens": ["Hemm\u00b7test", "du", "den", "Wel\u00b7ten\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}