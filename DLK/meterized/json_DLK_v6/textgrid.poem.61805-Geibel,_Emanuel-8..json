{"textgrid.poem.61805": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rauscht das Meer gelinde,", "tokens": ["Es", "rauscht", "das", "Meer", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gew\u00f6lkumschleiert sinkt der Tag,", "tokens": ["Ge\u00b7w\u00f6l\u00b7kum\u00b7schlei\u00b7ert", "sinkt", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lockend ziehn im Winde", "tokens": ["Und", "lo\u00b7ckend", "ziehn", "im", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVINF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gesang und Harfenschlag.", "tokens": ["Ge\u00b7sang", "und", "Har\u00b7fen\u00b7schlag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O la\u00df dich nicht bezwingen,", "tokens": ["O", "la\u00df", "dich", "nicht", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie sehnsuchtsvoll dein Herz erbebt!", "tokens": ["Wie", "sehn\u00b7suchts\u00b7voll", "dein", "Herz", "er\u00b7bebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist der Meerfrau Singen,", "tokens": ["Das", "ist", "der", "Meer\u00b7frau", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das \u00fcberm Wasser schwebt.", "tokens": ["Das", "\u00fc\u00b7berm", "Was\u00b7ser", "schwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie sang dieselbe Weise,", "tokens": ["Sie", "sang", "die\u00b7sel\u00b7be", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sie hernieder ins Gewog", "tokens": ["Da", "sie", "her\u00b7nie\u00b7der", "ins", "Ge\u00b7wog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Liebesarmen leise", "tokens": ["Mit", "Lie\u00b7be\u00b7sar\u00b7men", "lei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den K\u00f6nig Harald zog.", "tokens": ["Den", "K\u00f6\u00b7nig", "Ha\u00b7rald", "zog", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}