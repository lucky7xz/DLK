ambic.tetra"}, "line.84": {"text": "Was ist's nun, das die Ruh' mir raubt?", "tokens": ["Was", "ist's", "nun", ",", "das", "die", "Ruh'", "mir", "raubt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.85": {"text": "Zehn Kutschen fahren sp\u00e4t vom Schmause,", "tokens": ["Zehn", "Kut\u00b7schen", "fah\u00b7ren", "sp\u00e4t", "vom", "Schmau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.86": {"text": "Vor meinem Fenster durch, nach Hause.", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", "durch", ",", "nach", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.87": {"text": "O wohl mir! da\u00df mit Weib und Kind", "tokens": ["O", "wohl", "mir", "!", "da\u00df", "mit", "Weib", "und", "Kind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PPER", "$.", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.88": {"text": "Ich Tisch und Bett kann wieder theilen!", "tokens": ["Ich", "Tisch", "und", "Bett", "kann", "wie\u00b7der", "thei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.89": {"text": "Denn unsre Schildwach' sind die Eulen", "tokens": ["Denn", "uns\u00b7re", "Schild\u00b7wach'", "sind", "die", "Eu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.90": {"text": "Und unser Kutschgerassel \u2013 Wind!", "tokens": ["Und", "un\u00b7ser", "Kutschge\u00b7ras\u00b7sel", "\u2013", "Wind", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.91": {"text": "Mit beiden bin ich schon vertraut;", "tokens": ["Mit", "bei\u00b7den", "bin", "ich", "schon", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.92": {"text": "Drum komm, o Schlaf! wie eine Braut", "tokens": ["Drum", "komm", ",", "o", "Schlaf", "!", "wie", "ei\u00b7ne", "Braut"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "FM", "NN", "$.", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.93": {"text": "In k\u00fcnftger Nacht mich zu umfangen;", "tokens": ["In", "k\u00fcnft\u00b7ger", "Nacht", "mich", "zu", "um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.94": {"text": "Denn glaube, ", "tokens": ["Denn", "glau\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.95": {"text": "Nicht mehr nach Phaon, als nach dir", "tokens": ["Nicht", "mehr", "nach", "Phaon", ",", "als", "nach", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "$,", "KOUS", "APPR", "PPER"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.96": {"text": "Mein 