{"textgrid.poem.25473": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wohl mir, da\u00df mein Weib und Sohn", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wohl mir, da\u00df mein Weib und Sohn", "tokens": ["O", "wohl", "mir", ",", "da\u00df", "mein", "Weib", "und", "Sohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PPER", "$,", "KOUS", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich nur lebendig wieder haben!", "tokens": ["Mich", "nur", "le\u00b7ben\u00b7dig", "wie\u00b7der", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vier Wochen sp\u00e4ter, war ich schon", "tokens": ["Vier", "Wo\u00b7chen", "sp\u00e4\u00b7ter", ",", "war", "ich", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deiner K\u00f6nigsstadt begraben.", "tokens": ["In", "dei\u00b7ner", "K\u00f6\u00b7nigs\u00b7stadt", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zwar, s\u00e4h' die G\u00f6ttin Sparsamkeit,", "tokens": ["Zwar", ",", "s\u00e4h'", "die", "G\u00f6t\u00b7tin", "Spar\u00b7sam\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich mir, wie ihr so gastfrei seyd:", "tokens": ["Gleich", "mir", ",", "wie", "ihr", "so", "gast\u00b7frei", "seyd", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie m\u00fc\u00dfte schier f\u00fcr sich err\u00f6then!", "tokens": ["Sie", "m\u00fc\u00df\u00b7te", "schier", "f\u00fcr", "sich", "er\u00b7r\u00f6\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch grade diese Gastfreiheit", "tokens": ["Doch", "gra\u00b7de", "die\u00b7se", "Gast\u00b7frei\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PDAT", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.9": {"text": "W\u00fcrd' einen Fremden endlich t\u00f6dten.", "tokens": ["W\u00fcrd'", "ei\u00b7nen", "Frem\u00b7den", "end\u00b7lich", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nie, nie vergi\u00dft der K\u00f6nigsstadt", "tokens": ["Nie", ",", "nie", "ver\u00b7gi\u00dft", "der", "K\u00f6\u00b7nigs\u00b7stadt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und ihrer Gro\u00dfen, ihrer Weisen,", "tokens": ["Und", "ih\u00b7rer", "Gro\u00b7\u00dfen", ",", "ih\u00b7rer", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dein Freund. So lang er Athem hat,", "tokens": ["Dein", "Freund", ".", "So", "lang", "er", "A\u00b7them", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ADV", "ADJD", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wird er, der nichts fast lobt, sie preisen.", "tokens": ["Wird", "er", ",", "der", "nichts", "fast", "lobt", ",", "sie", "prei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und dennoch: st\u00e4nde gleich die Wahl", "tokens": ["Und", "den\u00b7noch", ":", "st\u00e4n\u00b7de", "gleich", "die", "Wahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In meiner Macht; zum zweitenmal", "tokens": ["In", "mei\u00b7ner", "Macht", ";", "zum", "zwei\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "APPRART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "W\u00fcrd' ich wohl nach ", "tokens": ["W\u00fcrd'", "ich", "wohl", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Wer wei\u00df, wer wei\u00df! wann, eh' ich nur", "tokens": ["Wer", "wei\u00df", ",", "wer", "wei\u00df", "!", "wann", ",", "eh'", "ich", "nur"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "$.", "PWAV", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die erste Fahrt dahin, verwinde!", "tokens": ["Die", "ers\u00b7te", "Fahrt", "da\u00b7hin", ",", "ver\u00b7win\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Denn, au\u00dfer da\u00df ich meine Flur,", "tokens": ["Denn", ",", "au\u00b7\u00dfer", "da\u00df", "ich", "mei\u00b7ne", "Flur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Trotz eurem Park'", "tokens": ["Trotz", "eu\u00b7rem", "Park'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Als eh' ich neulich sie verlie\u00df,", "tokens": ["Als", "eh'", "ich", "neu\u00b7lich", "sie", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Erfriert dein Freund fast bei dem Winde,", "tokens": ["Er\u00b7friert", "dein", "Freund", "fast", "bei", "dem", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Vor dem, so rauh er immer blie\u00df,", "tokens": ["Vor", "dem", ",", "so", "rauh", "er", "im\u00b7mer", "blie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Sonst kaum von seinem Blut' ein Tr\u00f6pfchen", "tokens": ["Sonst", "kaum", "von", "sei\u00b7nem", "Blut'", "ein", "Tr\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Erstarrt'; und ach! aus seinem N\u00e4pfchen", "tokens": ["Er\u00b7starrt'", ";", "und", "ach", "!", "aus", "sei\u00b7nem", "N\u00e4pf\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "KON", "XY", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Schmeckt s\u00fc\u00dfer Rohm ihm nicht mehr s\u00fc\u00df.", "tokens": ["Schmeckt", "s\u00fc\u00b7\u00dfer", "Rohm", "ihm", "nicht", "mehr", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Nicht, weil mein Gaumen, den Poeten", "tokens": ["Nicht", ",", "weil", "mein", "Gau\u00b7men", ",", "den", "Po\u00b7et\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Der Bouillon", "tokens": ["Der", "Bou\u00b7il\u00b7lon"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Und euren Zandern l\u00fcstern ist;", "tokens": ["Und", "eu\u00b7ren", "Zan\u00b7dern", "l\u00fcs\u00b7tern", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Nein! doch mein Magen lebt, zur Strafe", "tokens": ["Nein", "!", "doch", "mein", "Ma\u00b7gen", "lebt", ",", "zur", "Stra\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Des sp\u00e4tern Schw\u00e4rmens, mit dem Schlafe", "tokens": ["Des", "sp\u00e4\u00b7tern", "Schw\u00e4r\u00b7mens", ",", "mit", "dem", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Noch immerfort in argem Zwist'.", "tokens": ["Noch", "im\u00b7mer\u00b7fort", "in", "ar\u00b7gem", "Zwist'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Gottlob! da\u00df ich nicht mehr, wie dort,", "tokens": ["Gott\u00b7lob", "!", "da\u00df", "ich", "nicht", "mehr", ",", "wie", "dort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Auf Federn von gewundnem Stahle,", "tokens": ["Auf", "Fe\u00b7dern", "von", "ge\u00b7wund\u00b7nem", "Stah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "In Kutschen sitze; mit dem Nord'", "tokens": ["In", "Kut\u00b7schen", "sit\u00b7ze", ";", "mit", "dem", "Nord'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Die Zorga", "tokens": ["Die", "Zor\u00b7ga"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Wie sonst, vom Berg' ein Abendroth", "tokens": ["Wie", "sonst", ",", "vom", "Ber\u00b7g'", "ein", "A\u00b7ben\u00b7droth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Auf schwarzen Wald kann brennen sehen;", "tokens": ["Auf", "schwar\u00b7zen", "Wald", "kann", "bren\u00b7nen", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Wie sonst, sobald mein Butterbrod", "tokens": ["Wie", "sonst", ",", "so\u00b7bald", "mein", "But\u00b7ter\u00b7brod"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Verdaut nur ist, zu Bette gehen,", "tokens": ["Ver\u00b7daut", "nur", "ist", ",", "zu", "Bet\u00b7te", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAFIN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Wenn eure K\u00f6che noch den Koth,", "tokens": ["Wenn", "eu\u00b7re", "K\u00f6\u00b7che", "noch", "den", "Koth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Am Feuer, aus der Schnepfe drehen.", "tokens": ["Am", "Feu\u00b7er", ",", "aus", "der", "Schnep\u00b7fe", "dre\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Nun, hoff' ich, soll mein Magen wohl", "tokens": ["Nun", ",", "hoff'", "ich", ",", "soll", "mein", "Ma\u00b7gen", "wohl"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Bald mit dem Schlafe sich vers\u00f6hnen,", "tokens": ["Bald", "mit", "dem", "Schla\u00b7fe", "sich", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Der eine wieder sich an Kohl,", "tokens": ["Der", "ei\u00b7ne", "wie\u00b7der", "sich", "an", "Kohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Der andr' an Glocke zehn gew\u00f6hnen.", "tokens": ["Der", "andr'", "an", "Glo\u00b7cke", "zehn", "ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "CARD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Zwar haben eure Leckerbissen,", "tokens": ["Zwar", "ha\u00b7ben", "eu\u00b7re", "Le\u00b7cker\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Und eure Weine wei\u00df und roth,", "tokens": ["Und", "eu\u00b7re", "Wei\u00b7ne", "wei\u00df", "und", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Mir nicht den s\u00fc\u00dfen Schlaf entrissen,", "tokens": ["Mir", "nicht", "den", "s\u00fc\u00b7\u00dfen", "Schlaf", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Weil dort kein Wirth so lange droht,", "tokens": ["Weil", "dort", "kein", "Wirth", "so", "lan\u00b7ge", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Da\u00df wohl die G\u00e4ste trinken m\u00fcssen;", "tokens": ["Da\u00df", "wohl", "die", "G\u00e4s\u00b7te", "trin\u00b7ken", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Wer aber denkt noch an den Leib,", "tokens": ["Wer", "a\u00b7ber", "denkt", "noch", "an", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Da, wo er seltne Weisheit h\u00f6ret?", "tokens": ["Da", ",", "wo", "er", "selt\u00b7ne", "Weis\u00b7heit", "h\u00f6\u00b7ret", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Und wer vergi\u00dft nicht gern den Becher,", "tokens": ["Und", "wer", "ver\u00b7gi\u00dft", "nicht", "gern", "den", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Da, wo der Witz zum Zeitvertreib'", "tokens": ["Da", ",", "wo", "der", "Witz", "zum", "Zeit\u00b7ver\u00b7treib'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Mit leichter Hand den vollen K\u00f6cher,", "tokens": ["Mit", "leich\u00b7ter", "Hand", "den", "vol\u00b7len", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Als wenn es Pfeile schneite, leeret?", "tokens": ["Als", "wenn", "es", "Pfei\u00b7le", "schnei\u00b7te", ",", "lee\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Doch, Wochen lang, um Mitternacht", "tokens": ["Doch", ",", "Wo\u00b7chen", "lang", ",", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "ADJD", "$,", "KOUI", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Gerad' ins Bett vom Schmause fahren:", "tokens": ["Ge\u00b7rad'", "ins", "Bett", "vom", "Schmau\u00b7se", "fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Daf\u00fcr mag mich das Gl\u00fcck bewahren!", "tokens": ["Da\u00b7f\u00fcr", "mag", "mich", "das", "Gl\u00fcck", "be\