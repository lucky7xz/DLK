{"textgrid.poem.43069": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es rauscht und schwirrt das Saitenspiel;", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rauscht und schwirrt das Saitenspiel;", "tokens": ["Es", "rauscht", "und", "schwirrt", "das", "Sai\u00b7ten\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fa\u00dften mich an zum Tanz.", "tokens": ["Sie", "fa\u00df\u00b7ten", "mich", "an", "zum", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hei, wie der bachantische Kreis sich schwang", "tokens": ["Hei", ",", "wie", "der", "ba\u00b7ch\u00b7an\u00b7ti\u00b7sche", "Kreis", "sich", "schwang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Im blendenden Lichterglanz!", "tokens": ["Im", "blen\u00b7den\u00b7den", "Lich\u00b7ter\u00b7glanz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie pre\u00dften mir in die Hand ein Glas,", "tokens": ["Sie", "pre\u00df\u00b7ten", "mir", "in", "die", "Hand", "ein", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bekr\u00e4nzten mit Rosen mein Kleid;", "tokens": ["Be\u00b7kr\u00e4nz\u00b7ten", "mit", "Ro\u00b7sen", "mein", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich ward in Bachus Namen getauft", "tokens": ["Ich", "ward", "in", "Ba\u00b7chus", "Na\u00b7men", "ge\u00b7tauft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NE", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und der Frau Venus geweiht.", "tokens": ["Und", "der", "Frau", "Ve\u00b7nus", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie ich in dumpfer Bet\u00e4ubung", "tokens": ["Und", "wie", "ich", "in", "dum\u00b7pfer", "Be\u00b7t\u00e4u\u00b7bung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im Wagen bin gesessen,", "tokens": ["Im", "Wa\u00b7gen", "bin", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sagte man mir l\u00e4chelnd:", "tokens": ["Da", "sag\u00b7te", "man", "mir", "l\u00e4\u00b7chelnd", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wirst du ihn vergessen.", "tokens": ["So", "wirst", "du", "ihn", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}