{"textgrid.poem.43777": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Freud' und Leid", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stolz die Blumen heut ihr Haupt erheben,", "tokens": ["Stolz", "die", "Blu\u00b7men", "heut", "ihr", "Haupt", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch es kommt ein Reif wohl \u00fcber Nacht,", "tokens": ["Doch", "es", "kommt", "ein", "Reif", "wohl", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und zerknickt ist alles frohe Leben,", "tokens": ["Und", "zer\u00b7knickt", "ist", "al\u00b7les", "fro\u00b7he", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dahin der Blumen sch\u00f6ne Pracht.", "tokens": ["Und", "da\u00b7hin", "der", "Blu\u00b7men", "sch\u00f6\u00b7ne", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und die Lust, die wir am Sommer hatten,", "tokens": ["Und", "die", "Lust", ",", "die", "wir", "am", "Som\u00b7mer", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist verwandelt dann in lauter Leid,", "tokens": ["Ist", "ver\u00b7wan\u00b7delt", "dann", "in", "lau\u00b7ter", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit Schnee bedeckt hat Feld und Matten,", "tokens": ["Und", "mit", "Schnee", "be\u00b7deckt", "hat", "Feld", "und", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Berg und Thal die kalte Winterzeit.", "tokens": ["Berg", "und", "Thal", "die", "kal\u00b7te", "Win\u00b7ter\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wie Leid und Freude stets hienieden,", "tokens": ["Doch", "wie", "Leid", "und", "Freu\u00b7de", "stets", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "KON", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und wie Nacht und Tag stets wechseln mag,", "tokens": ["Und", "wie", "Nacht", "und", "Tag", "stets", "wech\u00b7seln", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "KON", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jedem Winter ein Lenz beschieden,", "tokens": ["Je\u00b7dem", "Win\u00b7ter", "ein", "Lenz", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Immer kommt ein Auferstehungstag.", "tokens": ["Im\u00b7mer", "kommt", "ein", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}