{"dta.poem.8601": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die schlimme Jagd .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Das edle Wild der Freiheit scharf zu hetzen,", "tokens": ["Das", "ed\u00b7le", "Wild", "der", "Frei\u00b7heit", "scharf", "zu", "het\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchst\u00f6bert eine finstre J\u00e4gerbande", "tokens": ["Durch\u00b7st\u00f6\u00b7bert", "ei\u00b7ne", "finst\u00b7re", "J\u00e4\u00b7ger\u00b7ban\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Blutgewehren, stillen Meuchelnetzen", "tokens": ["Mit", "Blut\u00b7ge\u00b7weh\u00b7ren", ",", "stil\u00b7len", "Meu\u00b7chel\u00b7net\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der W\u00e4lder Heiligthum im deutschen Lande.", "tokens": ["Der", "W\u00e4l\u00b7der", "Hei\u00b7lig\u00b7thum", "im", "deut\u00b7schen", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Wild mag \u00fcber Str\u00f6m' und Kl\u00fcfte setzen,", "tokens": ["Das", "Wild", "mag", "\u00fc\u00b7ber", "Str\u00f6m'", "und", "Kl\u00fcf\u00b7te", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und klettern mag's am steilen Klippenrande:", "tokens": ["Und", "klet\u00b7tern", "mag's", "am", "stei\u00b7len", "Klip\u00b7pen\u00b7ran\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Waidruf schallt durch Felsen, Str\u00f6m' und Kl\u00fcfte,", "tokens": ["Der", "Wai\u00b7druf", "schallt", "durch", "Fel\u00b7sen", ",", "Str\u00f6m'", "und", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Emp\u00f6rt verschleudern ihn die deutschen L\u00fcfte.", "tokens": ["Em\u00b7p\u00f6rt", "ver\u00b7schleu\u00b7dern", "ihn", "die", "deut\u00b7schen", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}