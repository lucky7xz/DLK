{"textgrid.poem.24678": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Lebensquell", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fr\u00fchlingsfluten ziehn durch meinen Geist:", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7flu\u00b7ten", "ziehn", "durch", "mei\u00b7nen", "Geist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verwandte G\u00e4rung f\u00fchl ich sich ergie\u00dfen", "tokens": ["Ver\u00b7wand\u00b7te", "G\u00e4\u00b7rung", "f\u00fchl", "ich", "sich", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch tausend Knospen, die sich heut erschlie\u00dfen,", "tokens": ["Durch", "tau\u00b7send", "Knos\u00b7pen", ",", "die", "sich", "heut", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und neues Leben dampft und quillt und kreist.", "tokens": ["Und", "neu\u00b7es", "Le\u00b7ben", "dampft", "und", "quillt", "und", "kreist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist des ewgen Jugendbrunnens Flie\u00dfen,", "tokens": ["Das", "ist", "des", "ew\u00b7gen", "Ju\u00b7gend\u00b7brun\u00b7nens", "Flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der jedem Jahr die gleiche F\u00fclle weist:", "tokens": ["Der", "je\u00b7dem", "Jahr", "die", "glei\u00b7che", "F\u00fcl\u00b7le", "weist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In neuer, feuchtverkl\u00e4rter Sch\u00f6nheit glei\u00dft", "tokens": ["In", "neu\u00b7er", ",", "feucht\u00b7ver\u00b7kl\u00e4r\u00b7ter", "Sch\u00f6n\u00b7heit", "glei\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was er benetzt, und locket zum Genie\u00dfen:", "tokens": ["Was", "er", "be\u00b7netzt", ",", "und", "lo\u00b7cket", "zum", "Ge\u00b7nie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gedanken, kommt und trinkt euch neues Leben:", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "kommt", "und", "trinkt", "euch", "neu\u00b7es", "Le\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du scheue Hoffnung, fastverklungnes F\u00fchlen,", "tokens": ["Du", "scheu\u00b7e", "Hoff\u00b7nung", ",", "fast\u00b7ver\u00b7klung\u00b7nes", "F\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du halbverzagtes, wegem\u00fcdes Streben,", "tokens": ["Du", "halb\u00b7ver\u00b7zag\u00b7tes", ",", "we\u00b7ge\u00b7m\u00fc\u00b7des", "Stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "La\u00dft euch von lichter Lebensflut umsp\u00fclen,", "tokens": ["La\u00dft", "euch", "von", "lich\u00b7ter", "Le\u00b7bens\u00b7flut", "um\u00b7sp\u00fc\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Tr\u00e4ume, Bilder, die ich t\u00e4glich schaue,", "tokens": ["Ihr", "Tr\u00e4u\u00b7me", ",", "Bil\u00b7der", ",", "die", "ich", "t\u00e4g\u00b7lich", "schau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df euch auf immer dieser Glanz betaue.", "tokens": ["Da\u00df", "euch", "auf", "im\u00b7mer", "die\u00b7ser", "Glanz", "be\u00b7tau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}