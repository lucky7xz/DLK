{"dta.poem.12912": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Sylvien, welche allezeit, wenn  \n sie die violine strich, dabey zu l\u00e4cheln  \n pflegte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, lose Sylvia! ich habe l\u00e4ngst gedacht:", "tokens": ["Ach", ",", "lo\u00b7se", "Syl\u00b7via", "!", "ich", "ha\u00b7be", "l\u00e4ngst", "ge\u00b7dacht", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "NE", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "We\u00dfwegen lacht sie doch, wenn sie ein artig st\u00fccke", "tokens": ["We\u00df\u00b7we\u00b7gen", "lacht", "sie", "doch", ",", "wenn", "sie", "ein", "ar\u00b7tig", "st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf ihrer violine macht?", "tokens": ["Auf", "ih\u00b7rer", "vi\u00b7o\u00b7li\u00b7ne", "macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch hab\u2019 ich es zu meinem ungel\u00fccke", "tokens": ["Doch", "hab'", "ich", "es", "zu", "mei\u00b7nem", "un\u00b7ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht eh\u2019, als itzt, heraus gebracht.", "tokens": ["Nicht", "eh'", ",", "als", "itzt", ",", "he\u00b7raus", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$,", "KOUS", "ADV", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du lachst, ach ja! du lachst, wenn du den bogen f\u00fchrest,", "tokens": ["Du", "lachst", ",", "ach", "ja", "!", "du", "lachst", ",", "wenn", "du", "den", "bo\u00b7gen", "f\u00fch\u00b7rest", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "ITJ", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dem Apollo gleichst,", "tokens": ["Und", "dem", "A\u00b7pol\u00b7lo", "gleichst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Weil du dem scheine nach nur auf die s\u00e4iten streichst,", "tokens": ["Weil", "du", "dem", "schei\u00b7ne", "nach", "nur", "auf", "die", "s\u00e4i\u00b7ten", "streichst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "APPR", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und in der that mein hertz, als eine Venus, r\u00fchrest.", "tokens": ["Und", "in", "der", "that", "mein", "hertz", ",", "als", "ei\u00b7ne", "Ve\u00b7nus", ",", "r\u00fch\u00b7rest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}