{"dta.poem.13422": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "An das Gl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Komm du einmahl mit lachendem Gesichte,", "tokens": ["Komm", "du", "ein\u00b7mahl", "mit", "la\u00b7chen\u00b7dem", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Frau! G\u00f6ttin! wer du bist, die auf der Kugel steht,", "tokens": ["Frau", "!", "G\u00f6t\u00b7tin", "!", "wer", "du", "bist", ",", "die", "auf", "der", "Ku\u00b7gel", "steht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PWS", "PPER", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Uud h\u00f6re doch mein k\u00fchnes Lobgedichte,", "tokens": ["U\u00b7ud", "h\u00f6\u00b7re", "doch", "mein", "k\u00fch\u00b7nes", "Lob\u00b7ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wo aber nicht, so werde selbst Poet,", "tokens": ["Wo", "a\u00b7ber", "nicht", ",", "so", "wer\u00b7de", "selbst", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKNEG", "$,", "ADV", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verdamme mich davor zu deinem Rade,", "tokens": ["Ver\u00b7dam\u00b7me", "mich", "da\u00b7vor", "zu", "dei\u00b7nem", "Ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vertraue mir sogar den Segel an,", "tokens": ["Ver\u00b7trau\u00b7e", "mir", "so\u00b7gar", "den", "Se\u00b7gel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "La\u00df sehen, ob ich auf dem krummen Pfade", "tokens": ["La\u00df", "se\u00b7hen", ",", "ob", "ich", "auf", "dem", "krum\u00b7men", "Pfa\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Lauf zu dir befrachtet richten kann.", "tokens": ["Den", "Lauf", "zu", "dir", "be\u00b7frach\u00b7tet", "rich\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwar bleibe nur, wie du bisher gewesen,", "tokens": ["Zwar", "blei\u00b7be", "nur", ",", "wie", "du", "bis\u00b7her", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWAV", "PPER", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df mir mein Saytenspiel, u. sey du Jungfer Gl\u00fcck,", "tokens": ["La\u00df", "mir", "mein", "Say\u00b7ten\u00b7spiel", ",", "u.", "sey", "du", "Jung\u00b7fer", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "KON", "VAFIN", "PPER", "NE", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Jedoch du kannst nicht meine Lieder lesen,", "tokens": ["Je\u00b7doch", "du", "kannst", "nicht", "mei\u00b7ne", "Lie\u00b7der", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich bitt ich auch um keinen Gnadenblick,", "tokens": ["Dich", "bitt", "ich", "auch", "um", "kei\u00b7nen", "Gna\u00b7den\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein Aug hat noch kein Sterblicher gesehen,", "tokens": ["Dein", "Aug", "hat", "noch", "kein", "Sterb\u00b7li\u00b7cher", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sind beyde schwarz? blau? oder wie man will?", "tokens": ["Sind", "bey\u00b7de", "schwarz", "?", "blau", "?", "o\u00b7der", "wie", "man", "will", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$.", "ADJD", "$.", "KON", "PWAV", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So halte denn bey dem F\u00fcr\u00fcbergehen,", "tokens": ["So", "hal\u00b7te", "denn", "bey", "dem", "F\u00fc\u00b7r\u00fc\u00b7ber\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ich singe dir zu Ehren, halte still!", "tokens": ["Ich", "sin\u00b7ge", "dir", "zu", "Eh\u00b7ren", ",", "hal\u00b7te", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich bin verliebt! du sollst mir G\u00f6ttin heissen,", "tokens": ["Ich", "bin", "ver\u00b7liebt", "!", "du", "sollst", "mir", "G\u00f6t\u00b7tin", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Ehrfurcht betet dich nunmehr poetisch an,", "tokens": ["Die", "Ehr\u00b7furcht", "be\u00b7tet", "dich", "nun\u00b7mehr", "po\u00b7e\u00b7tisch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Ohr will ich begeistert zu mir reissen,", "tokens": ["Dein", "Ohr", "will", "ich", "be\u00b7geis\u00b7tert", "zu", "mir", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie es Horaz ", "tokens": ["Wie", "es", "Ho\u00b7raz"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wer wei\u00df, vielleicht l\u00e4\u00dft sich dein Herz ber\u00fccken,", "tokens": ["Wer", "wei\u00df", ",", "viel\u00b7leicht", "l\u00e4\u00dft", "sich", "dein", "Herz", "be\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df es f\u00fcr mich ein wenig Regung sp\u00fchrt,", "tokens": ["Da\u00df", "es", "f\u00fcr", "mich", "ein", "we\u00b7nig", "Re\u00b7gung", "sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn diesesmahl ein hoffnungsvoll Entz\u00fccken", "tokens": ["Wenn", "die\u00b7ses\u00b7mahl", "ein", "hoff\u00b7nungs\u00b7voll", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Noch eins so schnell so stark die Sayten r\u00fchrt.", "tokens": ["Noch", "eins", "so", "schnell", "so", "stark", "die", "Say\u00b7ten", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "ADJD", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jhr, deren Zorn und Wuth das Gl\u00fcck gescholten,", "tokens": ["Ihr", ",", "de\u00b7ren", "Zorn", "und", "Wuth", "das", "Gl\u00fcck", "ge\u00b7schol\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn es nicht alsobald nach eurem Wunsch gewollt,", "tokens": ["Wenn", "es", "nicht", "al\u00b7so\u00b7bald", "nach", "eu\u00b7rem", "Wunsch", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Euch wird mit Recht auf euren Kopf vergolten,", "tokens": ["Euch", "wird", "mit", "Recht", "auf", "eu\u00b7ren", "Kopf", "ver\u00b7gol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indem ihr nie erh\u00f6ret werden sollt,", "tokens": ["In\u00b7dem", "ihr", "nie", "er\u00b7h\u00f6\u00b7ret", "wer\u00b7den", "sollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O K\u00f6nigin! die ihren Wagen lenket,", "tokens": ["O", "K\u00f6\u00b7ni\u00b7gin", "!", "die", "ih\u00b7ren", "Wa\u00b7gen", "len\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zugleich belohnt, erh\u00f6het, st\u00fcrzt und straft,", "tokens": ["Zu\u00b7gleich", "be\u00b7lohnt", ",", "er\u00b7h\u00f6\u00b7het", ",", "st\u00fcrzt", "und", "straft", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jedoch nicht so, wie es der Wille lenket,", "tokens": ["Je\u00b7doch", "nicht", "so", ",", "wie", "es", "der", "Wil\u00b7le", "len\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.8": {"text": "Und nach dem Wunsch der blinden Leidenschaft.", "tokens": ["Und", "nach", "dem", "Wunsch", "der", "blin\u00b7den", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich nenne dich den Ausbund aller Dinge,", "tokens": ["Ich", "nen\u00b7ne", "dich", "den", "Aus\u00b7bund", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die nur ein Dichtergeist zur Wirklichkeit gebracht,", "tokens": ["Die", "nur", "ein", "Dich\u00b7ter\u00b7geist", "zur", "Wirk\u00b7lich\u00b7keit", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gl\u00fcck! das ich noch, wer wei\u00df, entfernt besinge,", "tokens": ["Gl\u00fcck", "!", "das", "ich", "noch", ",", "wer", "wei\u00df", ",", "ent\u00b7fernt", "be\u00b7sin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PRELS", "PPER", "ADV", "$,", "PWS", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht, indem es sich herzu gemacht,", "tokens": ["Viel\u00b7leicht", ",", "in\u00b7dem", "es", "sich", "her\u00b7zu", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verwandle dich, wie diese meine T\u00f6ne,", "tokens": ["Ver\u00b7wand\u00b7le", "dich", ",", "wie", "die\u00b7se", "mei\u00b7ne", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PDS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nimm einen Leib, so, wie es dir gef\u00e4llt,", "tokens": ["Nimm", "ei\u00b7nen", "Leib", ",", "so", ",", "wie", "es", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADV", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sieh br\u00e4unlich aus, sey eine blonde Sch\u00f6ne,", "tokens": ["Sieh", "br\u00e4un\u00b7lich", "aus", ",", "sey", "ei\u00b7ne", "blon\u00b7de", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie du dich sonst Poeten dargestellt.", "tokens": ["Wie", "du", "dich", "sonst", "Po\u00b7et\u00b7en", "dar\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo nicht? so bleib ein Etwas im Gehirne,", "tokens": ["Wo", "nicht", "?", "so", "bleib", "ein", "Et\u00b7was", "im", "Ge\u00b7hir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$.", "ADV", "VVFIN", "ART", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie du bishero dich so mannigfalt gezeigt,", "tokens": ["Wie", "du", "bis\u00b7he\u00b7ro", "dich", "so", "man\u00b7nig\u00b7falt", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur werde mir mit einer holden Stirne", "tokens": ["Nur", "wer\u00b7de", "mir", "mit", "ei\u00b7ner", "hol\u00b7den", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ins k\u00fcnftige auch unsichtbar geneigt;", "tokens": ["Ins", "k\u00fcnf\u00b7ti\u00b7ge", "auch", "un\u00b7sicht\u00b7bar", "ge\u00b7neigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gebiet einmahl dem ohngefehren Gange,", "tokens": ["Ge\u00b7biet", "ein\u00b7mahl", "dem", "ohn\u00b7ge\u00b7feh\u00b7ren", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und kehre doch in meiner H\u00fctten ein,", "tokens": ["Und", "keh\u00b7re", "doch", "in", "mei\u00b7ner", "H\u00fct\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du kannst nicht fehlen, folge dem Gesange,", "tokens": ["Du", "kannst", "nicht", "feh\u00b7len", ",", "fol\u00b7ge", "dem", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sollt er auch im Traum erhoben seyn.", "tokens": ["Und", "sollt", "er", "auch", "im", "Traum", "er\u00b7ho\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Gesetzt, dir sind die Augen zugebunden,", "tokens": ["Ge\u00b7setzt", ",", "dir", "sind", "die", "Au\u00b7gen", "zu\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gnug, es sto\u00dft dein Geschirr bey keine\u0303 Hausrath an,", "tokens": ["Gnug", ",", "es", "sto\u00dft", "dein", "Ge\u00b7schirr", "bey", "kein\u1ebd", "Haus\u00b7rath", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da findest du itzt und zu allen Stunden", "tokens": ["Da", "fin\u00b7dest", "du", "itzt", "und", "zu", "al\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noth \u00fcbrig gnug auf einer leeren Bahn,", "tokens": ["Noth", "\u00fcb\u00b7rig", "gnug", "auf", "ei\u00b7ner", "lee\u00b7ren", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und einen Rock, der hat wohl zwanzig L\u00f6cher,", "tokens": ["Und", "ei\u00b7nen", "Rock", ",", "der", "hat", "wohl", "zwan\u00b7zig", "L\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zwey Dichter noch, die sind Horaz, Virgil,", "tokens": ["Zwey", "Dich\u00b7ter", "noch", ",", "die", "sind", "Ho\u00b7raz", ",", "Vir\u00b7gil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "PRELS", "VAFIN", "NE", "$,", "NE", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "An jeder Wand h\u00e4ngt auch ein alter K\u00f6cher,", "tokens": ["An", "je\u00b7der", "Wand", "h\u00e4ngt", "auch", "ein", "al\u00b7ter", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Darinne steckt manch stumpfer Federkiel.", "tokens": ["Da\u00b7rin\u00b7ne", "steckt", "manch", "stump\u00b7fer", "Fe\u00b7der\u00b7kiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch, G\u00f6ttin, oder wie soll ich dich nennen,", "tokens": ["Doch", ",", "G\u00f6t\u00b7tin", ",", "o\u00b7der", "wie", "soll", "ich", "dich", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "KON", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein blosser Zuspruch bringt mir alle G\u00fcter mit,", "tokens": ["Dein", "blos\u00b7ser", "Zu\u00b7spruch", "bringt", "mir", "al\u00b7le", "G\u00fc\u00b7ter", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich werde dich doch wohl am Gru\u00df erkennen,", "tokens": ["Ich", "wer\u00b7de", "dich", "doch", "wohl", "am", "Gru\u00df", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Obgleich dein Fu\u00df die Kugel leise tritt;", "tokens": ["Ob\u00b7gleich", "dein", "Fu\u00df", "die", "Ku\u00b7gel", "lei\u00b7se", "tritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Beym Helikon! da will ich erstlich prangen,", "tokens": ["Beym", "He\u00b7li\u00b7kon", "!", "da", "will", "ich", "erst\u00b7lich", "pran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil sich mein Nichts bey dir so wohl befind,", "tokens": ["Weil", "sich", "mein", "Nichts", "bey", "dir", "so", "wohl", "be\u00b7find", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "PIS", "APPR", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn will ich jauchzend an zu schreyen fangen,", "tokens": ["Denn", "will", "ich", "jauch\u00b7zend", "an", "zu", "schre\u00b7yen", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df manchmahl auch Poeten gl\u00fccklich sind.", "tokens": ["Da\u00df", "manch\u00b7mahl", "auch", "Po\u00b7et\u00b7en", "gl\u00fcck\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sie sind es, ja! nur aber in Gedanken,", "tokens": ["Sie", "sind", "es", ",", "ja", "!", "nur", "a\u00b7ber", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADV", "$.", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als denen nicht allein die Welt zu Dienste steht,", "tokens": ["Als", "de\u00b7nen", "nicht", "al\u00b7lein", "die", "Welt", "zu", "Diens\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PTKNEG", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie schwingen sich auch ausser ihren Schranken,", "tokens": ["Sie", "schwin\u00b7gen", "sich", "auch", "aus\u00b7ser", "ih\u00b7ren", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Gl\u00fccke selbst ist sich schon ein Poet!", "tokens": ["Sein", "Gl\u00fc\u00b7cke", "selbst", "ist", "sich", "schon", "ein", "Po\u00b7et", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er kann sich selbst sein Eden auferbauen,", "tokens": ["Er", "kann", "sich", "selbst", "sein", "E\u00b7den", "au\u00b7fer\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er ist es, der mehr Welten m\u00f6glich macht,", "tokens": ["Er", "ist", "es", ",", "der", "mehr", "Wel\u00b7ten", "m\u00f6g\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sein Auge kann so manch Geheimni\u00df schauen,", "tokens": ["Sein", "Au\u00b7ge", "kann", "so", "manch", "Ge\u00b7heim\u00b7ni\u00df", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wor\u00fcber oft die Tugend selbst gelacht.", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "oft", "die", "Tu\u00b7gend", "selbst", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "O Gl\u00fcck! willst du nun nicht den Werth verliehren,", "tokens": ["O", "Gl\u00fcck", "!", "willst", "du", "nun", "nicht", "den", "Werth", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn meine Wenigkeit dein Ohngefehr verlacht,", "tokens": ["Wenn", "mei\u00b7ne", "We\u00b7nig\u00b7keit", "dein", "Ohn\u00b7ge\u00b7fehr", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie so gar schlecht es steht um dein Regieren,", "tokens": ["Wie", "so", "gar", "schlecht", "es", "steht", "um", "dein", "Re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da deine Gunst sich Narren dienstbar macht,", "tokens": ["Da", "dei\u00b7ne", "Gunst", "sich", "Nar\u00b7ren", "dienst\u00b7bar", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie bald erh\u00f6ht, mit G\u00fctern \u00fcbersch\u00fcttet,", "tokens": ["Sie", "bald", "er\u00b7h\u00f6ht", ",", "mit", "G\u00fc\u00b7tern", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ihnen giebt, was nur ihr Herz begehrt,", "tokens": ["Und", "ih\u00b7nen", "giebt", ",", "was", "nur", "ihr", "Herz", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O! so verleih, was ein Poet dich bittet,", "tokens": ["O", "!", "so", "ver\u00b7leih", ",", "was", "ein", "Po\u00b7et", "dich", "bit\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJD", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mir werde nur ein fr\u00f6hlich Herz beschert.", "tokens": ["Mir", "wer\u00b7de", "nur", "ein", "fr\u00f6h\u00b7lich", "Herz", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}