{"textgrid.poem.47014": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, setz' dich, la\u00df dir 'mal ins Antlitz schauen,", "tokens": ["Komm", ",", "setz'", "dich", ",", "la\u00df", "dir", "'mal", "ins", "Ant\u00b7litz", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df deine Hand 'mal friedlich ruhn in meiner;", "tokens": ["La\u00df", "dei\u00b7ne", "Hand", "'mal", "fried\u00b7lich", "ruhn", "in", "mei\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich will einmal als Zimmerer und Schreiner,", "tokens": ["Ich", "will", "ein\u00b7mal", "als", "Zim\u00b7me\u00b7rer", "und", "Schrei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So gut ich kann, im Geist ein H\u00fcttchen bauen.", "tokens": ["So", "gut", "ich", "kann", ",", "im", "Geist", "ein", "H\u00fctt\u00b7chen", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VMFIN", "$,", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ganz schlecht und recht soll's sein, nicht viel behauen,", "tokens": ["Ganz", "schlecht", "und", "recht", "soll's", "sein", ",", "nicht", "viel", "be\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "PIS", "VAINF", "$,", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz klein von au\u00dfen, innen doch viel kleiner,", "tokens": ["Ganz", "klein", "von", "au\u00b7\u00dfen", ",", "in\u00b7nen", "doch", "viel", "klei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADV", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur gro\u00df genug mir einem und noch einer,", "tokens": ["Nur", "gro\u00df", "ge\u00b7nug", "mir", "ei\u00b7nem", "und", "noch", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPER", "ART", "KON", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die eine ist \u2013 was furchst du denn die Brauen?", "tokens": ["Die", "ei\u00b7ne", "ist", "\u2013", "was", "furchst", "du", "denn", "die", "Brau\u00b7en", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "$(", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So klein soll's H\u00fcttchen sein, da\u00df all vor\u00fcber", "tokens": ["So", "klein", "soll's", "H\u00fctt\u00b7chen", "sein", ",", "da\u00df", "all", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PIAT", "NN", "VAINF", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein jeder Wind geht, ohn' ans Dach zu hauchen,", "tokens": ["Ein", "je\u00b7der", "Wind", "geht", ",", "ohn'", "ans", "Dach", "zu", "hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "KOUI", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder L\u00e4rm zieht, ohn' ans Thor zu pochen.", "tokens": ["Ein", "je\u00b7der", "L\u00e4rm", "zieht", ",", "ohn'", "ans", "Thor", "zu", "po\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "KOUI", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Durchaus kein Platz, kein Raum im H\u00fcttchen \u00fcber,", "tokens": ["Durc\u00b7haus", "kein", "Platz", ",", "kein", "Raum", "im", "H\u00fctt\u00b7chen", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PIAT", "NN", "APPRART", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als nur so viel zwei jetzt zum Bette brauchen,", "tokens": ["Als", "nur", "so", "viel", "zwei", "jetzt", "zum", "Bet\u00b7te", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADV", "CARD", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein drittes dann zur Wieg' in Jahr und Wochen.", "tokens": ["Ein", "drit\u00b7tes", "dann", "zur", "Wieg'", "in", "Jahr", "und", "Wo\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}