{"dta.poem.9515": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n Straffe des f\u00fcrwitzes.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich die Lesbie nechst in der kammer fand/", "tokens": ["Als", "ich", "die", "Les\u00b7bie", "nechst", "in", "der", "kam\u00b7mer", "fand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da sie sich \u00fcberhin und schl\u00e4ffrig angeleget;", "tokens": ["Da", "sie", "sich", "\u00fc\u00b7berh\u00b7in", "und", "schl\u00e4f\u00b7frig", "an\u00b7ge\u00b7le\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So schaut ich eine brust/ die sch\u00f6ner \u00e4pffel tr\u00e4get/", "tokens": ["So", "schaut", "ich", "ei\u00b7ne", "brust", "/", "die", "sch\u00f6\u00b7ner", "\u00e4pf\u00b7fel", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als iemals vorgebracht das reiche morgen-land.", "tokens": ["Als", "ie\u00b7mals", "vor\u00b7ge\u00b7bracht", "das", "rei\u00b7che", "mor\u00b7gen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die brunst zog meinen geist/ der f\u00fcrwitz trieb die hand", "tokens": ["Die", "brunst", "zog", "mei\u00b7nen", "geist", "/", "der", "f\u00fcr\u00b7witz", "trieb", "die", "hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu suchen/ was sich hier in diesem zirck beweget.", "tokens": ["Zu", "su\u00b7chen", "/", "was", "sich", "hier", "in", "die\u00b7sem", "zirck", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "PWS", "PRF", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df hat der Lesbie so grossen zorn erreget/", "tokens": ["Di\u00df", "hat", "der", "Les\u00b7bie", "so", "gros\u00b7sen", "zorn", "er\u00b7re\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df sie in h\u00f6chstem grimm ist gegen mich entbrand;", "tokens": ["Da\u00df", "sie", "in", "h\u00f6chs\u00b7tem", "grimm", "ist", "ge\u00b7gen", "mich", "ent\u00b7brand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "ADJD", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie trieb mich von sich weg/ sie stie\u00df mich zu der seiten/", "tokens": ["Sie", "trieb", "mich", "von", "sich", "weg", "/", "sie", "stie\u00df", "mich", "zu", "der", "sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PRF", "PTKVZ", "$(", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie hie\u00df mich unverweilt aus ihren augen schreiten.", "tokens": ["Sie", "hie\u00df", "mich", "un\u00b7ver\u00b7weilt", "aus", "ih\u00b7ren", "au\u00b7gen", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich sprach/ indem sie mich aus ihrer kammer stie\u00df/", "tokens": ["Ich", "sprach", "/", "in\u00b7dem", "sie", "mich", "aus", "ih\u00b7rer", "kam\u00b7mer", "stie\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dieweil ich allzuk\u00fchn und mehr als sichs geb\u00fchret/", "tokens": ["Die\u00b7weil", "ich", "all\u00b7zu\u00b7k\u00fchn", "und", "mehr", "als", "sichs", "ge\u00b7b\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADV", "KOUS", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mir verbotne frucht der \u00e4pffel anger\u00fchret/", "tokens": ["Die", "mir", "ver\u00b7bot\u00b7ne", "frucht", "der", "\u00e4pf\u00b7fel", "an\u00b7ge\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So st\u00f6st ein engel mich ietzt aus dem paradie\u00df.", "tokens": ["So", "st\u00f6st", "ein", "en\u00b7gel", "mich", "ietzt", "aus", "dem", "pa\u00b7ra\u00b7die\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}