{"textgrid.poem.25844": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[nun wieder Nacht]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wieder Nacht.", "tokens": ["Nun", "wie\u00b7der", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Sterne nicken im schwarzen Blau.", "tokens": ["Die", "Ster\u00b7ne", "ni\u00b7cken", "im", "schwar\u00b7zen", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich f\u00fchle dein Auge von ferne.", "tokens": ["Ich", "f\u00fch\u00b7le", "dein", "Au\u00b7ge", "von", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dein Auge wacht einsam,", "tokens": ["Dein", "Au\u00b7ge", "wacht", "ein\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Und deine Lippen wachen,", "tokens": ["Und", "dei\u00b7ne", "Lip\u00b7pen", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Deine sehns\u00fcchtigen Lippen.", "tokens": ["Dei\u00b7ne", "sehn\u00b7s\u00fcch\u00b7ti\u00b7gen", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schleiche zur Ferne, von Stern zu Stern,", "tokens": ["Ich", "schlei\u00b7che", "zur", "Fer\u00b7ne", ",", "von", "Stern", "zu", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bis ich deine Lippen erreiche,", "tokens": ["Bis", "ich", "dei\u00b7ne", "Lip\u00b7pen", "er\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Deine sehnenden Lippen.", "tokens": ["Dei\u00b7ne", "seh\u00b7nen\u00b7den", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}