{"textgrid.poem.35773": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, h\u00e4tt' ich Engelzungen,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, h\u00e4tt' ich Engelzungen,", "tokens": ["Ach", ",", "h\u00e4tt'", "ich", "En\u00b7gel\u00b7zun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich h\u00e4tt' euch wohl gesungen", "tokens": ["Ich", "h\u00e4tt'", "euch", "wohl", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das s\u00fc\u00dfe, liebe Lied,", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", ",", "lie\u00b7be", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das mir so still und selig", "tokens": ["Das", "mir", "so", "still", "und", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im jungen Herzen gl\u00fcht.", "tokens": ["Im", "jun\u00b7gen", "Her\u00b7zen", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df ja keine Weisen,", "tokens": ["Ich", "wei\u00df", "ja", "kei\u00b7ne", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Herren so zu preisen,", "tokens": ["Den", "Her\u00b7ren", "so", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Vater, treu und mild,", "tokens": ["Den", "Va\u00b7ter", ",", "treu", "und", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie meine ganze Seele", "tokens": ["Wie", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihm singt und jauchzt und spielt.", "tokens": ["Ihm", "singt", "und", "jauchzt", "und", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df mein Haupt Ihm neigen,", "tokens": ["Ich", "mu\u00df", "mein", "Haupt", "Ihm", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kann weinen nur und schweigen", "tokens": ["Kann", "wei\u00b7nen", "nur", "und", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "ADV", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Seligkeit und Schmerz;", "tokens": ["In", "Se\u00b7lig\u00b7keit", "und", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ach Kind, Er wei\u00df dein Lieben,", "tokens": ["Ach", "Kind", ",", "Er", "wei\u00df", "dein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sieht dir ja ins Herz.", "tokens": ["Er", "sieht", "dir", "ja", "ins", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}