{"textgrid.poem.49313": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Hirtenlied", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spielet auf und la\u00dft uns singen,", "tokens": ["Spie\u00b7let", "auf", "und", "la\u00dft", "uns", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die kalte Zeit einbricht,", "tokens": ["Wie", "die", "kal\u00b7te", "Zeit", "ein\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die zwar alles kan bezwingen,", "tokens": ["Die", "zwar", "al\u00b7les", "kan", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur verliebte Seelen nicht;", "tokens": ["Nur", "ver\u00b7lieb\u00b7te", "See\u00b7len", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Luft und Wasser, See und Erden", "tokens": ["Luft", "und", "Was\u00b7ser", ",", "See", "und", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stehen gleich dem harten Stahl,", "tokens": ["Ste\u00b7hen", "gleich", "dem", "har\u00b7ten", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch das Feld mu\u00df Eisen werden", "tokens": ["Auch", "das", "Feld", "mu\u00df", "Ei\u00b7sen", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "NN", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Wiesen allzumal,", "tokens": ["Und", "die", "Wie\u00b7sen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Nur bei treuer Lieb' allein", "tokens": ["Nur", "bei", "treu\u00b7er", "Lieb'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wil das Feur erhalten sein.", "tokens": ["Wil", "das", "Feur", "er\u00b7hal\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Kan der Winter alles z\u00e4hmen,", "tokens": ["Kan", "der", "Win\u00b7ter", "al\u00b7les", "z\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Kan er t\u00f6ten Laub und Gras,", "tokens": ["Kan", "er", "t\u00f6\u00b7ten", "Laub", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Kan er schon die Fr\u00fccht uns nehmen,", "tokens": ["Kan", "er", "schon", "die", "Fr\u00fccht", "uns", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Kan er zwingen alles Na\u00df:", "tokens": ["Kan", "er", "zwin\u00b7gen", "al\u00b7les", "Na\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Ei, so mu\u00df er doch mit Schanden", "tokens": ["Ei", ",", "so", "mu\u00df", "er", "doch", "mit", "Schan\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Von den Herzen ziehen ab,", "tokens": ["Von", "den", "Her\u00b7zen", "zie\u00b7hen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Die mit festen Liebesbanden", "tokens": ["Die", "mit", "fes\u00b7ten", "Lie\u00b7bes\u00b7ban\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sind verkn\u00fcpfet bis ins Grab:", "tokens": ["Sind", "ver\u00b7kn\u00fcp\u00b7fet", "bis", "ins", "Grab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Es wil treue Lieb' allein", "tokens": ["Es", "wil", "treu\u00b7e", "Lieb'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Bis ans Ende standhaft sein.", "tokens": ["Bis", "ans", "En\u00b7de", "stand\u00b7haft", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Ist ein Sch\u00e4fer, der nicht liebet?", "tokens": ["Ist", "ein", "Sch\u00e4\u00b7fer", ",", "der", "nicht", "lie\u00b7bet", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Solcher ist von schlechter Art,", "tokens": ["Sol\u00b7cher", "ist", "von", "schlech\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Wo er nicht die Sinnen \u00fcbet", "tokens": ["Wo", "er", "nicht", "die", "Sin\u00b7nen", "\u00fc\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und im Herzen die bewahrt,", "tokens": ["Und", "im", "Her\u00b7zen", "die", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Die er ihm allein erkoren", "tokens": ["Die", "er", "ihm", "al\u00b7lein", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Vor sein auserw\u00e4hltes Gut:", "tokens": ["Vor", "sein", "au\u00b7ser\u00b7w\u00e4hl\u00b7tes", "Gut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Ei, so hat er gar verloren", "tokens": ["Ei", ",", "so", "hat", "er", "gar", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Witz und K\u00fchnheit, Herz und Mut;", "tokens": ["Witz", "und", "K\u00fchn\u00b7heit", ",", "Herz", "und", "Mut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Doch wil treue Lieb' allein", "tokens": ["Doch", "wil", "treu\u00b7e", "Lieb'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJA", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Steif und fest erhalten sein.", "tokens": ["Steif", "und", "fest", "er\u00b7hal\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Wil man hohe Geister kennen,", "tokens": ["Wil", "man", "ho\u00b7he", "Geis\u00b7ter", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Klug von Worten, gro\u00df von That,", "tokens": ["Klug", "von", "Wor\u00b7ten", ",", "gro\u00df", "von", "That", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Lieber, la\u00df dir einen nennen,", "tokens": ["Lie\u00b7ber", ",", "la\u00df", "dir", "ei\u00b7nen", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVIMP", "PPER", "ART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Der mit Ernst geliebet hat.", "tokens": ["Der", "mit", "Ernst", "ge\u00b7lie\u00b7bet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Das sind ja geringe Seelen,", "tokens": ["Das", "sind", "ja", "ge\u00b7rin\u00b7ge", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Bl\u00f6de Sch\u00e4fer, sehr verzagt,", "tokens": ["Bl\u00f6\u00b7de", "Sch\u00e4\u00b7fer", ",", "sehr", "ver\u00b7zagt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Die kein eignes Herz erw\u00e4hlen", "tokens": ["Die", "kein", "eig\u00b7nes", "Herz", "er\u00b7w\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Und das Lieben nie gewagt.", "tokens": ["Und", "das", "Lie\u00b7ben", "nie", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Doch wil treue Lieb' allein", "tokens": ["Doch", "wil", "treu\u00b7e", "Lieb'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJA", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Bis ins Grab erhalten sein.", "tokens": ["Bis", "ins", "Grab", "er\u00b7hal\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Daphnis hat sich recht besonnen,", "tokens": ["Daph\u00b7nis", "hat", "sich", "recht", "be\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Da\u00df er seine Galathe", "tokens": ["Da\u00df", "er", "sei\u00b7ne", "Ga\u00b7la\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "In den Feldern lieb gewonnen,", "tokens": ["In", "den", "Fel\u00b7dern", "lieb", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Eh' der Sonnentrank, der Schnee,", "tokens": ["Eh'", "der", "Son\u00b7nen\u00b7trank", ",", "der", "Schnee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Aus den Wiesen uns getrieben;", "tokens": ["Aus", "den", "Wie\u00b7sen", "uns", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Ach, was ist es wolgethan,", "tokens": ["Ach", ",", "was", "ist", "es", "wol\u00b7ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "In dem Sommer so zu lieben,", "tokens": ["In", "dem", "Som\u00b7mer", "so", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Da\u00df man es genie\u00dfen kan,", "tokens": ["Da\u00df", "man", "es", "ge\u00b7nie\u00b7\u00dfen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Wenn der Winter l\u00e4\u00dft allein", "tokens": ["Wenn", "der", "Win\u00b7ter", "l\u00e4\u00dft", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Zwei verliebte Herzen sein.", "tokens": ["Zwei", "ver\u00b7lieb\u00b7te", "Her\u00b7zen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Spielet auf, ihr Hirtenknaben,", "tokens": ["Spie\u00b7let", "auf", ",", "ihr", "Hir\u00b7ten\u00b7kna\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Eh' der Tag wird hingerafft;", "tokens": ["Eh'", "der", "Tag", "wird", "hin\u00b7ge\u00b7rafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Daphnis wil nun bald vergraben", "tokens": ["Daph\u00b7nis", "wil", "nun", "bald", "ver\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Galatheen Jungfrauschaft;", "tokens": ["Ga\u00b7la\u00b7theen", "Jung\u00b7frausc\u00b7haft", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.55": {"text": "Lasset die Schalmey erschallen,", "tokens": ["Las\u00b7set", "die", "Schal\u00b7mey", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.56": {"text": "Eh' die Sonn' ins Wasser geht,", "tokens": ["Eh'", "die", "Sonn'", "ins", "Was\u00b7ser", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Galatheen zu gefallen,", "tokens": ["Ga\u00b7la\u00b7theen", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Welch' in Daphnis' Armen steht.", "tokens": ["Welch'", "in", "Daph\u00b7nis'", "Ar\u00b7men", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Galathee sol allein", "tokens": ["Ga\u00b7la\u00b7thee", "sol", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["NE", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.60": {"text": "Daphnis' Allerliebste sein.", "tokens": ["Daph\u00b7nis'", "Al\u00b7ler\u00b7liebs\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}