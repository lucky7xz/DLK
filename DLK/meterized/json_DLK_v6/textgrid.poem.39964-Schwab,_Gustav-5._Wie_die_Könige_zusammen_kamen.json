{"textgrid.poem.39964": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "5. Wie die K\u00f6nige zusammen kamen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als nun die zw\u00f6lfte Nacht vergangen,", "tokens": ["Als", "nun", "die", "zw\u00f6lf\u00b7te", "Nacht", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die doch war keine Nacht zu nennen;", "tokens": ["Die", "doch", "war", "kei\u00b7ne", "Nacht", "zu", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da ward ein Nebel umgehangen,", "tokens": ["Da", "ward", "ein", "Ne\u00b7bel", "um\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df auch erlosch des Sternes Brennen.", "tokens": ["Da\u00df", "auch", "er\u00b7losch", "des", "Ster\u00b7nes", "Bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da blieb der Sonne Licht verborgen,", "tokens": ["Da", "blieb", "der", "Son\u00b7ne", "Licht", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da mu\u00dfte, von der Nacht befallen,", "tokens": ["Da", "mu\u00df\u00b7te", ",", "von", "der", "Nacht", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das erste Mal seit dreizehn Morgen", "tokens": ["Das", "ers\u00b7te", "Mal", "seit", "drei\u00b7zehn", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Zug der F\u00fcrsten m\u00fchsam wallen.", "tokens": ["Der", "Zug", "der", "F\u00fcrs\u00b7ten", "m\u00fch\u00b7sam", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf eines H\u00fcgels Felsgesteinen", "tokens": ["Auf", "ei\u00b7nes", "H\u00fc\u00b7gels", "Fels\u00b7ge\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hielt an der Eine mit dem Traben;", "tokens": ["Hielt", "an", "der", "Ei\u00b7ne", "mit", "dem", "Tra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er lagerte sich mit den Seinen,", "tokens": ["Er", "la\u00b7ger\u00b7te", "sich", "mit", "den", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "PPOSS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das war der K\u00f6nig der Araben.", "tokens": ["Das", "war", "der", "K\u00f6\u00b7nig", "der", "A\u00b7ra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sahn sich in den Finsternissen", "tokens": ["Sie", "sahn", "sich", "in", "den", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vergebens um nach Stern und Sonnen,", "tokens": ["Ver\u00b7ge\u00b7bens", "um", "nach", "Stern", "und", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie lagen ohne nur zu wissen,", "tokens": ["Sie", "la\u00b7gen", "oh\u00b7ne", "nur", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was sie f\u00fcr St\u00e4tte sich gewonnen.", "tokens": ["Was", "sie", "f\u00fcr", "St\u00e4t\u00b7te", "sich", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da t\u00f6nte nebenan Getose,", "tokens": ["Da", "t\u00f6n\u00b7te", "ne\u00b7be\u00b7nan", "Ge\u00b7to\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als ob vom Ro\u00df auch Andre stiegen,", "tokens": ["Als", "ob", "vom", "Ro\u00df", "auch", "And\u00b7re", "stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPRART", "NN", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da raschelt es im Bergesmoose,", "tokens": ["Da", "ra\u00b7schelt", "es", "im", "Ber\u00b7ges\u00b7moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Als th\u00e4ten Andre neben liegen.", "tokens": ["Als", "th\u00e4\u00b7ten", "And\u00b7re", "ne\u00b7ben", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIS", "APPR", "VVFIN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Und Antwort ward auf das Gebr\u00fclle", "tokens": ["Und", "Ant\u00b7wort", "ward", "auf", "das", "Ge\u00b7br\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Stier' in des Araben Heerde: \u2013", "tokens": ["Der", "Stier'", "in", "des", "A\u00b7ra\u00b7ben", "Heer\u00b7de", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Jetzt stieg empor des Nebels H\u00fclle,", "tokens": ["Jetzt", "stieg", "em\u00b7por", "des", "Ne\u00b7bels", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Tag ward wieder auf der Erde.", "tokens": ["Und", "Tag", "ward", "wie\u00b7der", "auf", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er stand auf eines Kreuzwegs Mitten,", "tokens": ["Er", "stand", "auf", "ei\u00b7nes", "Kreuz\u00b7wegs", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm gegen\u00fcber hielt ein Andrer,", "tokens": ["Ihm", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "hielt", "ein", "A\u00b7ndrer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom zweiten Pfade hergeschritten,", "tokens": ["Vom", "zwei\u00b7ten", "Pfa\u00b7de", "her\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein m\u00e4nnlich wohlgethaner Wandrer;", "tokens": ["Ein", "m\u00e4nn\u00b7lich", "wohl\u00b7ge\u00b7tha\u00b7ner", "Wand\u00b7rer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und hinter ihm die Schar der Reiter,", "tokens": ["Und", "hin\u00b7ter", "ihm", "die", "Schar", "der", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schafe Heerden, der Kameele;", "tokens": ["Der", "Scha\u00b7fe", "Heer\u00b7den", ",", "der", "Ka\u00b7mee\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da zieht von beiden keiner weiter,", "tokens": ["Da", "zieht", "von", "bei\u00b7den", "kei\u00b7ner", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Staunen fliegt durch beider Seele.", "tokens": ["Ein", "Stau\u00b7nen", "fliegt", "durch", "bei\u00b7der", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und noch sind sie im Schau'n verloren,", "tokens": ["Und", "noch", "sind", "sie", "im", "Schau'n", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da k\u00f6mmt auf drittem Weg ein Dritter,", "tokens": ["Da", "k\u00f6mmt", "auf", "drit\u00b7tem", "Weg", "ein", "Drit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In einer stolzen Schar von Mohren,", "tokens": ["In", "ei\u00b7ner", "stol\u00b7zen", "Schar", "von", "Moh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er selbst ein junger, schwarzer Ritter.", "tokens": ["Er", "selbst", "ein", "jun\u00b7ger", ",", "schwar\u00b7zer", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbwas bringt euch,\u00ab rief er, \u00bbaus der Ferne,", "tokens": ["\u00bb", "was", "bringt", "euch", ",", "\u00ab", "rief", "er", ",", "\u00bb", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So edlen Mann, so w\u00fcrd'gen Greisen?", "tokens": ["So", "ed\u00b7len", "Mann", ",", "so", "w\u00fcrd'\u00b7gen", "Grei\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "W\u00e4rt ihr gef\u00fchrt von einem Sterne,", "tokens": ["W\u00e4rt", "ihr", "ge\u00b7f\u00fchrt", "von", "ei\u00b7nem", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie ich, ihr w\u00fcrdet lust'ger reisen!\u00ab", "tokens": ["Wie", "ich", ",", "ihr", "w\u00fcr\u00b7det", "lust'\u00b7ger", "rei\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "$,", "PPER", "VAFIN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der zweite sprach: \u00bbWohl einem Sterne", "tokens": ["Der", "zwei\u00b7te", "sprach", ":", "\u00bb", "Wohl", "ei\u00b7nem", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "$.", "$(", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertraut' ich meine festen Tritte!", "tokens": ["Ver\u00b7traut'", "ich", "mei\u00b7ne", "fes\u00b7ten", "Trit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht blindlings zieht ", "tokens": ["Nicht", "blind\u00b7lings", "zieht"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Nach sichrem Gut lenkt er die Schritte!\u00ab", "tokens": ["Nach", "sich\u00b7rem", "Gut", "lenkt", "er", "die", "Schrit\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der erste sprach: \u00bbEs mu\u00df den Greisen", "tokens": ["Der", "ers\u00b7te", "sprach", ":", "\u00bb", "Es", "mu\u00df", "den", "Grei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "$.", "$(", "PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein helles Licht zum Wandern laden,", "tokens": ["Ein", "hel\u00b7les", "Licht", "zum", "Wan\u00b7dern", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mich hie\u00df das Licht der Seele reisen,", "tokens": ["Mich", "hie\u00df", "das", "Licht", "der", "See\u00b7le", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Himmelslicht schien meinen Pfaden.\u00ab", "tokens": ["Ein", "Him\u00b7mels\u00b7licht", "schien", "mei\u00b7nen", "Pfa\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Jeder sprach's in seiner Zungen,", "tokens": ["Ein", "Je\u00b7der", "sprach's", "in", "sei\u00b7ner", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4r's die eigne, d\u00e4ucht's dem andern;", "tokens": ["Als", "w\u00e4r's", "die", "eig\u00b7ne", ",", "d\u00e4ucht's", "dem", "an\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "$,", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Jeder wei\u00df, vom Geist durchdrungen,", "tokens": ["Ein", "Je\u00b7der", "wei\u00df", ",", "vom", "Geist", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Woher, wohin, zu wem sie wandern.", "tokens": ["Wo\u00b7her", ",", "wo\u00b7hin", ",", "zu", "wem", "sie", "wan\u00b7dern", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "APPR", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie reichen sich die Hand zum Bunde,", "tokens": ["Sie", "rei\u00b7chen", "sich", "die", "Hand", "zum", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sind Ein Herz und Eine Seele;", "tokens": ["Sie", "sind", "Ein", "Herz", "und", "Ei\u00b7ne", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie k\u00fcssen sich mit Brudermunde", "tokens": ["Sie", "k\u00fcs\u00b7sen", "sich", "mit", "Bru\u00b7der\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und loben Gott mit ein'ger Kehle.", "tokens": ["Und", "lo\u00b7ben", "Gott", "mit", "ein'\u00b7ger", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Nebel zwar, der aufgestiegen,", "tokens": ["Der", "Ne\u00b7bel", "zwar", ",", "der", "auf\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat ihrem Blick den Stern verborgen,", "tokens": ["Hat", "ih\u00b7rem", "Blick", "den", "Stern", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sic