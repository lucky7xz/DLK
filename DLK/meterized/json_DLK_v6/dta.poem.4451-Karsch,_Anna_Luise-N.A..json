{"dta.poem.4451": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Und ward von viel hinzugest\u00fcrmtem Volke", "tokens": ["Und", "ward", "von", "viel", "hin\u00b7zu\u00b7ge\u00b7st\u00fcrm\u00b7tem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewundert und gelobt; ich aber streife schon", "tokens": ["Be\u00b7wun\u00b7dert", "und", "ge\u00b7lobt", ";", "ich", "a\u00b7ber", "strei\u00b7fe", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$.", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit stolzem Nacken an die lufterf\u00fcllte Wolke,", "tokens": ["Mit", "stol\u00b7zem", "Na\u00b7cken", "an", "die", "luf\u00b7ter\u00b7f\u00fcll\u00b7te", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Getheilt von meiner Leyer Thon.", "tokens": ["Ge\u00b7theilt", "von", "mei\u00b7ner", "Le\u00b7yer", "Thon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}