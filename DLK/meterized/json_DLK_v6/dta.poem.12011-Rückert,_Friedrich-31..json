{"dta.poem.12011": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist das h\u00f6chste Gut. Das sagt der Sprache Wort,", "tokens": ["Gott", "ist", "das", "h\u00f6chs\u00b7te", "Gut", ".", "Das", "sagt", "der", "Spra\u00b7che", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$.", "PDS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das sagt auch die Vernunft sich selber fort und fort.", "tokens": ["Das", "sagt", "auch", "die", "Ver\u00b7nunft", "sich", "sel\u00b7ber", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "PRF", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gott ist das h\u00f6chste Gut. Wenn Ursprung nun genommen", "tokens": ["Gott", "ist", "das", "h\u00f6chs\u00b7te", "Gut", ".", "Wenn", "Ur\u00b7sprung", "nun", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$.", "KOUS", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Gott die Welt, wo ist ihr B\u00f6ses hergekommen?", "tokens": ["Von", "Gott", "die", "Welt", ",", "wo", "ist", "ihr", "B\u00f6\u00b7ses", "her\u00b7ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist B\u00f6ses nur ein Schein, und alles gut allein?", "tokens": ["Ist", "B\u00f6\u00b7ses", "nur", "ein", "Schein", ",", "und", "al\u00b7les", "gut", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ART", "NN", "$,", "KON", "PIS", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das innerste Gef\u00fchl im Busen sagt dir Nein.", "tokens": ["Das", "in\u00b7ners\u00b7te", "Ge\u00b7f\u00fchl", "im", "Bu\u00b7sen", "sagt", "dir", "Nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Was ist das B\u00f6se denn? Es ist der innre Streit,", "tokens": ["Was", "ist", "das", "B\u00f6\u00b7se", "denn", "?", "Es", "ist", "der", "inn\u00b7re", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "KON", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Doppelheit der Welt, die sie mit Gott entzweit.", "tokens": ["Die", "Dop\u00b7pel\u00b7heit", "der", "Welt", ",", "die", "sie", "mit", "Gott", "ent\u00b7zweit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wol ist, was ist, in Gott, sonst w\u00e4r' es nicht vorhanden;", "tokens": ["Wol", "ist", ",", "was", "ist", ",", "in", "Gott", ",", "sonst", "w\u00e4r'", "es", "nicht", "vor\u00b7han\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWS", "VAFIN", "$,", "APPR", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Doch ists auch au\u00dfer ihm, sonst w\u00e4r' es nicht entstanden.", "tokens": ["Doch", "ists", "auch", "au\u00b7\u00dfer", "ihm", ",", "sonst", "w\u00e4r'", "es", "nicht", "ent\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sofern in Gott es ruht, ist alles Leben gut,", "tokens": ["So\u00b7fern", "in", "Gott", "es", "ruht", ",", "ist", "al\u00b7les", "Le\u00b7ben", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "VVFIN", "$,", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und b\u00f6s' ist alles, was es f\u00fcr sich selber thut.", "tokens": ["Und", "b\u00f6s'", "ist", "al\u00b7les", ",", "was", "es", "f\u00fcr", "sich", "sel\u00b7ber", "thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIS", "$,", "PWS", "PPER", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "O komm, uns und die Welt zu machen frei vom B\u00f6sen,", "tokens": ["O", "komm", ",", "uns", "und", "die", "Welt", "zu", "ma\u00b7chen", "frei", "vom", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "KON", "ART", "NN", "PTKZU", "VVINF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df uns in Gottgef\u00fchl den Sinn der Welt aufl\u00f6sen!", "tokens": ["La\u00df", "uns", "in", "Gott\u00b7ge\u00b7f\u00fchl", "den", "Sinn", "der", "Welt", "auf\u00b7l\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}