{"textgrid.poem.37828": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tannh\u00e4user", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun will ich aber heben an,", "tokens": ["Nun", "will", "ich", "a\u00b7ber", "he\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Tannh\u00e4user wollen wir singen,", "tokens": ["Vom", "Tann\u00b7h\u00e4u\u00b7ser", "wol\u00b7len", "wir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und was er wunders hat gethan,", "tokens": ["Und", "was", "er", "wun\u00b7ders", "hat", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Frau Venussinnen.", "tokens": ["Mit", "Frau", "Ve\u00b7nus\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Tannh\u00e4user war ein Ritter gut,", "tokens": ["Der", "Tann\u00b7h\u00e4u\u00b7ser", "war", "ein", "Rit\u00b7ter", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Er wollt gro\u00df Wunder schauen,", "tokens": ["Er", "wollt", "gro\u00df", "Wun\u00b7der", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da zog er in Frau Venus Berg,", "tokens": ["Da", "zog", "er", "in", "Frau", "Ve\u00b7nus", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "NE", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Zu andern sch\u00f6nen Frauen.", "tokens": ["Zu", "an\u00b7dern", "sch\u00f6\u00b7nen", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbherr Tannh\u00e4user, Ihr seyd mir lieb,", "tokens": ["\u00bb", "herr", "Tann\u00b7h\u00e4u\u00b7ser", ",", "Ihr", "seyd", "mir", "lieb", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Daran sollt Ihr gedenken,", "tokens": ["Da\u00b7ran", "sollt", "Ihr", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr habt mir einen Eid geschworen,", "tokens": ["Ihr", "habt", "mir", "ei\u00b7nen", "Eid", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr wollt nicht von mir wanken.\u00ab", "tokens": ["Ihr", "wollt", "nicht", "von", "mir", "wan\u00b7ken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbfrau Venus, ich hab' es nicht gethan,", "tokens": ["\u00bb", "frau", "Ve\u00b7nus", ",", "ich", "hab'", "es", "nicht", "ge\u00b7than", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich will dem widersprechen,", "tokens": ["Ich", "will", "dem", "wi\u00b7der\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn niemand spricht das mehr, als Ihr,", "tokens": ["Denn", "nie\u00b7mand", "spricht", "das", "mehr", ",", "als", "Ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADV", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott helf mir zu den Rechten.\u00ab", "tokens": ["Gott", "helf", "mir", "zu", "den", "Rech\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbherr Tannh\u00e4user, wie saget ihr mir!", "tokens": ["\u00bb", "herr", "Tann\u00b7h\u00e4u\u00b7ser", ",", "wie", "sa\u00b7get", "ihr", "mir", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ihr sollet bey uns bleiben,", "tokens": ["Ihr", "sol\u00b7let", "bey", "uns", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich geb Euch meiner Gespielen ein,", "tokens": ["Ich", "geb", "Euch", "mei\u00b7ner", "Ge\u00b7spie\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu einem eh'lichen Weibe.\u00ab", "tokens": ["Zu", "ei\u00b7nem", "eh'\u00b7li\u00b7chen", "Wei\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbnehme ich dann ein ander Weib,", "tokens": ["\u00bb", "neh\u00b7me", "ich", "dann", "ein", "an\u00b7der", "Weib", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Als ich hab in meinem Sinne,", "tokens": ["Als", "ich", "hab", "in", "mei\u00b7nem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So mu\u00df ich in der H\u00f6llen-Gluth,", "tokens": ["So", "mu\u00df", "ich", "in", "der", "H\u00f6l\u00b7len\u00b7Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ewiglich verbrennen.\u00ab", "tokens": ["Da", "e\u00b7wig\u00b7lich", "ver\u00b7bren\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbdu sagst mir viel von der H\u00f6llengluth,", "tokens": ["\u00bb", "du", "sagst", "mir", "viel", "von", "der", "H\u00f6l\u00b7len\u00b7gluth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du hast es doch nicht befunden,", "tokens": ["Du", "hast", "es", "doch", "nicht", "be\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Gedenk an meinen rothen Mund,", "tokens": ["Ge\u00b7denk", "an", "mei\u00b7nen", "ro\u00b7then", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der lacht zu allen Stunden.\u00ab", "tokens": ["Der", "lacht", "zu", "al\u00b7len", "Stun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbwas hilft mich Euer rother Mund,", "tokens": ["\u00bb", "was", "hilft", "mich", "Eu\u00b7er", "ro\u00b7ther", "Mund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist mir gar unmehre,", "tokens": ["Er", "ist", "mir", "gar", "un\u00b7meh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun gib mir Urlaub Frau Venus zart,", "tokens": ["Nun", "gib", "mir", "Ur\u00b7laub", "Frau", "Ve\u00b7nus", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Durch aller Frauen Ehre.\u00ab", "tokens": ["Durch", "al\u00b7ler", "Frau\u00b7en", "Eh\u00b7re", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbherr Tannh\u00e4user, wollt Ihr Urlaub han,", "tokens": ["\u00bb", "herr", "Tann\u00b7h\u00e4u\u00b7ser", ",", "wollt", "Ihr", "Ur\u00b7laub", "han", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "VMFIN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich will Euch keinen geben,", "tokens": ["Ich", "will", "Euch", "kei\u00b7nen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun bleibet edler Tannh\u00e4user zart,", "tokens": ["Nun", "blei\u00b7bet", "ed\u00b7ler", "Tann\u00b7h\u00e4u\u00b7ser", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und frischet Euer Leben.\u00ab", "tokens": ["Und", "fri\u00b7schet", "Eu\u00b7er", "Le\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbmein Leben ist schon worden krank,", "tokens": ["\u00bb", "mein", "Le\u00b7ben", "ist", "schon", "wor\u00b7den", "krank", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADV", "VAPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kann nicht l\u00e4nger bleiben,", "tokens": ["Ich", "kann", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gebt mir Urlaub Fraue zart,", "tokens": ["Gebt", "mir", "Ur\u00b7laub", "Frau\u00b7e", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Eurem stolzen Leibe.\u00ab", "tokens": ["Von", "Eu\u00b7rem", "stol\u00b7zen", "Lei\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbherr Tannh\u00e4user nicht sprecht also,", "tokens": ["\u00bb", "herr", "Tann\u00b7h\u00e4u\u00b7ser", "nicht", "sprecht", "al\u00b7so", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ihr seyd nicht wohl bey Sinnen,", "tokens": ["Ihr", "seyd", "nicht", "wohl", "bey", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "m