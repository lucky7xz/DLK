{"textgrid.poem.53425": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[bey dieser Sterbens-Sucht]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey dieser Sterbens-Sucht", "tokens": ["Bey", "die\u00b7ser", "Ster\u00b7bens\u00b7Sucht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Giebt alles schier die Flucht,", "tokens": ["Giebt", "al\u00b7les", "schier", "die", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir meinen gar mit Hauffen", "tokens": ["Wir", "mei\u00b7nen", "gar", "mit", "Hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Tode zu entlauffen.", "tokens": ["Dem", "To\u00b7de", "zu", "ent\u00b7lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wohin sol ich doch eilen", "tokens": ["Wo\u00b7hin", "sol", "ich", "doch", "ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr seinen leichten Pfeilen?", "tokens": ["F\u00fcr", "sei\u00b7nen", "leich\u00b7ten", "Pfei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er ist des H\u00f6chsten Ruht", "tokens": ["Er", "ist", "des", "H\u00f6chs\u00b7ten", "Ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auff unsern Ubermuht,", "tokens": ["Auff", "un\u00b7sern", "U\u00b7ber\u00b7muht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sol uns alle straffen,", "tokens": ["Die", "sol", "uns", "al\u00b7le", "straf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Hirten mit den Schaffen,", "tokens": ["Die", "Hir\u00b7ten", "mit", "den", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie hat uns leicht gedr\u00e4uet,", "tokens": ["Sie", "hat", "uns", "leicht", "ge\u00b7dr\u00e4u\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer hat sich gro\u00df gescheuet?", "tokens": ["Wer", "hat", "sich", "gro\u00df", "ge\u00b7scheu\u00b7et", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun sie mit allem Recht,", "tokens": ["Nun", "sie", "mit", "al\u00b7lem", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Trifft uns, die b\u00f6sen Knecht,", "tokens": ["Trifft", "uns", ",", "die", "b\u00f6\u00b7sen", "Knecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wolte Knechtisch fliehen,", "tokens": ["Ich", "wol\u00b7te", "Knech\u00b7tisch", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich meiner Straff entziehen?", "tokens": ["Mich", "mei\u00b7ner", "Straff", "ent\u00b7zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es m\u00f6chten Gottes Plagen", "tokens": ["Es", "m\u00f6ch\u00b7ten", "Got\u00b7tes", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich sonst viel h\u00e4rter schlagen.", "tokens": ["Mich", "sonst", "viel", "h\u00e4r\u00b7ter", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Kind, das seine Schuld", "tokens": ["Ein", "Kind", ",", "das", "sei\u00b7ne", "Schuld"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "L\u00e4\u00dft straffen mit Gedult", "tokens": ["L\u00e4\u00dft", "straf\u00b7fen", "mit", "Ge\u00b7dult"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00f6mmt der Ruht entgegen,", "tokens": ["Und", "k\u00f6mmt", "der", "Ruht", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kan Eltern noch bewegen,", "tokens": ["Kan", "El\u00b7tern", "noch", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das sich der Zucht entrissen,", "tokens": ["Das", "sich", "der", "Zucht", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird doppelt mehr geschmissen.", "tokens": ["Wird", "dop\u00b7pelt", "mehr", "ge\u00b7schmis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wo verberg ich mich?", "tokens": ["Und", "wo", "ver\u00b7berg", "ich", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "L\u00e4\u00dft Gottes Eiffer sich", "tokens": ["L\u00e4\u00dft", "Got\u00b7tes", "Eif\u00b7fer", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "NN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In aller Welt nicht finden?", "tokens": ["In", "al\u00b7ler", "Welt", "nicht", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er k\u00f6mmt zuvor den Winden", "tokens": ["Er", "k\u00f6mmt", "zu\u00b7vor", "den", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und kan viel schneller wancken,", "tokens": ["Und", "kan", "viel", "schnel\u00b7ler", "wan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als Menschen mit Gedancken.", "tokens": ["Als", "Men\u00b7schen", "mit", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "F\u00fchr ich gleich Himmel-ein,", "tokens": ["F\u00fchr", "ich", "gleich", "Him\u00b7mel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gott wird zugegen seyn,", "tokens": ["Gott", "wird", "zu\u00b7ge\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "K\u00f6nnt ich, mich zu erretten,", "tokens": ["K\u00f6nnt", "ich", ",", "mich", "zu", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch in die H\u00f6lle betten,", "tokens": ["Auch", "in", "die", "H\u00f6l\u00b7le", "bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das letzte Meer erreichen,", "tokens": ["Das", "letz\u00b7te", "Meer", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihm werd ich nicht entweichen.", "tokens": ["Ihm", "werd", "ich", "nicht", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich wei\u00df in dieser Noht", "tokens": ["Ich", "wei\u00df", "in", "die\u00b7ser", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Mittel f\u00fcr den Todt:", "tokens": ["Ein", "Mit\u00b7tel", "f\u00fcr", "den", "Todt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df wir uns Gott ergeben,", "tokens": ["Da\u00df", "wir", "uns", "Gott", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir sterben oder leben,", "tokens": ["Wir", "ster\u00b7ben", "o\u00b7der", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch wahre Bu\u00df ihn stillen", "tokens": ["Durch", "wah\u00b7re", "Bu\u00df", "ihn", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und leyden seinen Willen.", "tokens": ["Und", "ley\u00b7den", "sei\u00b7nen", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und hieran halt ich mich,", "tokens": ["Und", "hie\u00b7ran", "halt", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erbarm, Gott, meiner dich,", "tokens": ["Er\u00b7barm", ",", "Gott", ",", "mei\u00b7ner", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm meinen Missethaten", "tokens": ["Komm", "mei\u00b7nen", "Mis\u00b7se\u00b7tha\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit deiner Gunst zu statten,", "tokens": ["Mit", "dei\u00b7ner", "Gunst", "zu", "stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin nicht werth der Erden", "tokens": ["Ich", "bin", "nicht", "werth", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und dein genennt zu werden.", "tokens": ["Und", "dein", "ge\u00b7nennt", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mach mein Verbrechen gut", "tokens": ["Mach", "mein", "Ver\u00b7bre\u00b7chen", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch Jesu Christi Blut,", "tokens": ["Durch", "Je\u00b7su", "Chris\u00b7ti", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der unsrer S\u00fcnden wegen", "tokens": ["Der", "uns\u00b7rer", "S\u00fcn\u00b7den", "we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Grab ist todt gelegen,", "tokens": ["Im", "Grab", "ist", "todt", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und uns von ihren Banden", "tokens": ["Und", "uns", "von", "ih\u00b7ren", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Befreyt, da er erstanden.", "tokens": ["Be\u00b7freyt", ",", "da", "er", "er\u00b7stan\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und weil ich dieser Zeit", "tokens": ["Und", "weil", "ich", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00df nirgends Sicherheit,", "tokens": ["Wei\u00df", "nir\u00b7gends", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wollst du sie mir verstatten,", "tokens": ["Wollst", "du", "sie", "mir", "ver\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herr unter deinem Schatten,", "tokens": ["Herr", "un\u00b7ter", "dei\u00b7nem", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sey meine Burg und St\u00e4rcke,", "tokens": ["Sey", "mei\u00b7ne", "Burg", "und", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann ich ein Unheil mercke.", "tokens": ["Wann", "ich", "ein", "Un\u00b7heil", "mer\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dein Fittich breite sich,", "tokens": ["Dein", "Fit\u00b7tich", "brei\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O Vater, \u00fcber mich,", "tokens": ["O", "Va\u00b7ter", ",", "\u00fc\u00b7ber", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich das Gifft der Seuchen", "tokens": ["La\u00df", "mich", "das", "Gifft", "der", "Seu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An keinem Ort erschleichen,", "tokens": ["An", "kei\u00b7nem", "Ort", "er\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch wider Todt und Grauen", "tokens": ["Auch", "wi\u00b7der", "Todt", "und", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gew\u00fcnschte H\u00fclffe schauen.", "tokens": ["Ge\u00b7w\u00fcnschte", "H\u00fclf\u00b7fe", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Nimmt doch wol in Gefahr", "tokens": ["Nimmt", "doch", "wol", "in", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ein Mensch des andern wahr,", "tokens": ["Ein", "Mensch", "des", "an\u00b7dern", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kan er ohn seinen Schaden", "tokens": ["Kan", "er", "ohn", "sei\u00b7nen", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn nur der Noht entladen,", "tokens": ["Ihn", "nur", "der", "Noht", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und dieser, der beschweret,", "tokens": ["Und", "die\u00b7ser", ",", "der", "be\u00b7schwe\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Guthertzig fein begehret.", "tokens": ["Gu\u00b7thert\u00b7zig", "fein", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Wie soltest du dann nicht,", "tokens": ["Wie", "sol\u00b7test", "du", "dann", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O meine Zuversicht,", "tokens": ["O", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geneigt seyn mich zu sch\u00fctzen,", "tokens": ["Ge\u00b7neigt", "seyn", "mich", "zu", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann mir es nur sol n\u00fctzen,", "tokens": ["Wann", "mir", "es", "nur", "sol", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und nicht mein Todt f\u00fcr allen", "tokens": ["Und", "nicht", "mein", "Todt", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "PPOSAT", "NN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir heilsam m\u00f6chte fallen.", "tokens": ["Mir", "heil\u00b7sam", "m\u00f6ch\u00b7te", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Di\u00df trau ich, Herr, dir zu,", "tokens": ["Di\u00df", "trau", "ich", ",", "Herr", ",", "dir", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Drumb sey jetzt meine Ruh,", "tokens": ["Drumb", "sey", "jetzt", "mei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df andrer Leute Schrecken", "tokens": ["La\u00df", "an\u00b7drer", "Leu\u00b7te", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht Furcht bey mir erwecken,", "tokens": ["Nicht", "Furcht", "bey", "mir", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann was hat der zu sorgen,", "tokens": ["Dann", "was", "hat", "der", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "ART", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Den du bey dir verborgen?", "tokens": ["Den", "du", "bey", "dir", "ver\u00b7bor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.15": {"line.1": {"text": "W\u00fcrd ich dann heimgesucht", "tokens": ["W\u00fcrd", "ich", "dann", "heim\u00b7ge\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit dieser deiner Zucht,", "tokens": ["Mit", "die\u00b7ser", "dei\u00b7ner", "Zucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gib mitten in den Schmertzen", "tokens": ["Gib", "mit\u00b7ten", "in", "den", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gedult und Krafft dem Hertzen,", "tokens": ["Ge\u00b7dult", "und", "Krafft", "dem", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wann alles tritt von weiten,", "tokens": ["Wann", "al\u00b7les", "tritt", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So steh du mir zur Seiten.", "tokens": ["So", "steh", "du", "mir", "zur", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Sprich in der letzten Pein", "tokens": ["Sprich", "in", "der", "letz\u00b7ten", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Trost meiner Seelen ein,", "tokens": ["Trost", "mei\u00b7ner", "See\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Schrey mir ihn in die Ohren,", "tokens": ["Schrey", "mir", "ihn", "in", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wann ich die Sprach verlohren,", "tokens": ["Wann", "ich", "die", "Sprach", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und la\u00df auff Christi Leyden", "tokens": ["Und", "la\u00df", "auff", "Chris\u00b7ti", "Ley\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich sanfft von hinnen scheiden.", "tokens": ["Mich", "sanfft", "von", "hin\u00b7nen", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Wer wei\u00df was harter Stand", "tokens": ["Wer", "wei\u00df", "was", "har\u00b7ter", "Stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PWS", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Trifft dieses arme Land,", "tokens": ["Trifft", "die\u00b7ses", "ar\u00b7me", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob nicht die Krieges-Flammen", "tokens": ["Ob", "nicht", "die", "Krie\u00b7ge\u00b7sFlam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verzehren uns zusammen,", "tokens": ["Ver\u00b7zeh\u00b7ren", "uns", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Vorbott ist von fernen", "tokens": ["Der", "Vor\u00b7bott", "ist", "von", "fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erschienen au\u00df den Sternen.", "tokens": ["Er\u00b7schie\u00b7nen", "au\u00df", "den", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Wie selig werd ich seyn,", "tokens": ["Wie", "se\u00b7lig", "werd", "ich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "H\u00fclt mich das Grab dann ein,", "tokens": ["H\u00fclt", "mich", "das", "Grab", "dann", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ich f\u00fcr allem Jammer", "tokens": ["Und", "ich", "f\u00fcr", "al\u00b7lem", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fein schlaff in meiner Kammer", "tokens": ["Fein", "schlaff", "in", "mei\u00b7ner", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und h\u00f6re kein Get\u00fcmmel,", "tokens": ["Und", "h\u00f6\u00b7re", "kein", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fiel auch gleich ein der Himmel.", "tokens": ["Fiel", "auch", "gleich", "ein", "der", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Thu, was dein weiser Raht,", "tokens": ["Thu", ",", "was", "dein", "wei\u00b7ser", "Raht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O Gott beschlossen hat", "tokens": ["O", "Gott", "be\u00b7schlos\u00b7sen", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit mir in meinen Sachen,", "tokens": ["Mit", "mir", "in", "mei\u00b7nen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur la\u00df mich flei\u00dfig wachen,", "tokens": ["Nur", "la\u00df", "mich", "flei\u00b7\u00dfig", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und allzeit seyn befunden", "tokens": ["Und", "all\u00b7zeit", "seyn", "be\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In einer selign Stunden!", "tokens": ["In", "ei\u00b7ner", "se\u00b7lign", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}