{"textgrid.poem.37911": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Judentochter", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war eine sch\u00f6ne J\u00fcdin,", "tokens": ["Es", "war", "ei\u00b7ne", "sch\u00f6\u00b7ne", "J\u00fc\u00b7din", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein wundersch\u00f6nes Weib,", "tokens": ["Ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Weib", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hatt' ein sch\u00f6ne Tochter,", "tokens": ["Sie", "hatt'", "ein", "sch\u00f6\u00b7ne", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Haar war sch\u00f6n geflochten,", "tokens": ["Ihr", "Haar", "war", "sch\u00f6n", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zum Tanz war sie bereit.", "tokens": ["Zum", "Tanz", "war", "sie", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbach, liebste, liebste Mutter!", "tokens": ["\u00bb", "ach", ",", "liebs\u00b7te", ",", "liebs\u00b7te", "Mut\u00b7ter", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was thut mir mein Herz so weh!", "tokens": ["Was", "thut", "mir", "mein", "Herz", "so", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach, la\u00dft mich eine Weile", "tokens": ["Ach", ",", "la\u00dft", "mich", "ei\u00b7ne", "Wei\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spazieren auf gr\u00fcner Heide,", "tokens": ["Spa\u00b7zie\u00b7ren", "auf", "gr\u00fc\u00b7ner", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bis da\u00df mir's besser wird.\u00ab", "tokens": ["Bis", "da\u00df", "mir's", "bes\u00b7ser", "wird", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "KOUS", "NE", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Mutter wandt den R\u00fccken,", "tokens": ["Die", "Mut\u00b7ter", "wandt", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Tochter sprang in die Ga\u00df,", "tokens": ["Die", "Toch\u00b7ter", "sprang", "in", "die", "Ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo alle Schreiber sa\u00dfen:", "tokens": ["Wo", "al\u00b7le", "Schrei\u00b7ber", "sa\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbach liebster, liebster Schreiber!", "tokens": ["\u00bb", "ach", "liebs\u00b7ter", ",", "liebs\u00b7ter", "Schrei\u00b7ber", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was thut mir mein Herz so weh.\u00ab", "tokens": ["Was", "thut", "mir", "mein", "Herz", "so", "weh", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbwenn du dich l\u00e4ssest taufen,", "tokens": ["\u00bb", "wenn", "du", "dich", "l\u00e4s\u00b7sest", "tau\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Luisa sollst du heissen,", "tokens": ["Lu\u00b7i\u00b7sa", "sollst", "du", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Weibchen sollst du seyn.\u00ab", "tokens": ["Mein", "Weib\u00b7chen", "sollst", "du", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbeh ich mich lasse taufen,", "tokens": ["\u00bb", "eh", "ich", "mich", "las\u00b7se", "tau\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lieber will ich mich versaufen", "tokens": ["Lie\u00b7ber", "will", "ich", "mich", "ver\u00b7sau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ins tiefe, tiefe Meer.", "tokens": ["Ins", "tie\u00b7fe", ",", "tie\u00b7fe", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gut Nacht, mein Vater und Mutter,", "tokens": ["Gut", "Nacht", ",", "mein", "Va\u00b7ter", "und", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie auch mein stolzer Bruder,", "tokens": ["Wie", "auch", "mein", "stol\u00b7zer", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr seht mich nimmermehr!", "tokens": ["Ihr", "seht", "mich", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonne ist untergegangen", "tokens": ["Die", "Son\u00b7ne", "ist", "un\u00b7ter\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Im tiefen, tiefen Meer.\u00ab", "tokens": ["Im", "tie\u00b7fen", ",", "tie\u00b7fen", "Meer", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}