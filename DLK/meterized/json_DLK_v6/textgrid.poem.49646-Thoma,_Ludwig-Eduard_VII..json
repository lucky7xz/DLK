{"textgrid.poem.49646": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Eduard VII.", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wi\u00dft ihr noch, wie man vor Jahren", "tokens": ["Wi\u00dft", "ihr", "noch", ",", "wie", "man", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PIS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allgemein auf ihn geschimpft?", "tokens": ["All\u00b7ge\u00b7mein", "auf", "ihn", "ge\u00b7schimpft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suff und Spiel, die beiden waren", "tokens": ["Suff", "und", "Spiel", ",", "die", "bei\u00b7den", "wa\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "NN", "$,", "PRELS", "PIS", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm als Laster eingeimpft.", "tokens": ["Ihm", "als", "Las\u00b7ter", "ein\u00b7ge\u00b7impft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwischen ihm und kleinen M\u00e4dchen", "tokens": ["Zwi\u00b7schen", "ihm", "und", "klei\u00b7nen", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 Sagte man, ich wei\u00df es nicht \u2013", "tokens": ["\u2013", "Sag\u00b7te", "man", ",", "ich", "wei\u00df", "es", "nicht", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spann sich ab und zu ein F\u00e4dchen,", "tokens": ["Spann", "sich", "ab", "und", "zu", "ein", "F\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "PTKVZ", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er hatte fr\u00fch die Gicht.", "tokens": ["Und", "er", "hat\u00b7te", "fr\u00fch", "die", "Gicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt, in seinen alten Tagen,", "tokens": ["Jetzt", ",", "in", "sei\u00b7nen", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt er wirklich mit Geschick", "tokens": ["Treibt", "er", "wirk\u00b7lich", "mit", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u2013 Auch sein Neider mu\u00df es sagen \u2013", "tokens": ["\u2013", "Auch", "sein", "Nei\u00b7der", "mu\u00df", "es", "sa\u00b7gen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine kluge Politik.", "tokens": ["Ei\u00b7ne", "klu\u00b7ge", "Po\u00b7li\u00b7tik", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dieser Mann der sch\u00f6nen Hosen,", "tokens": ["Die\u00b7ser", "Mann", "der", "sch\u00f6\u00b7nen", "Ho\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den man oft betrunken sah,", "tokens": ["Den", "man", "oft", "be\u00b7trun\u00b7ken", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6dert erst die Herrn Franzosen,", "tokens": ["K\u00f6\u00b7dert", "erst", "die", "Herrn", "Fran\u00b7zo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sodann Hispania.", "tokens": ["Und", "so\u00b7dann", "His\u00b7pa\u00b7nia", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ferne sei mir's, zu vergleichen!", "tokens": ["Fer\u00b7ne", "sei", "mir's", ",", "zu", "ver\u00b7glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immerhin, trotz Suff und Jeu,", "tokens": ["Im\u00b7mer\u00b7hin", ",", "trotz", "Suff", "und", "Jeu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheint er dennoch zu erreichen", "tokens": ["Scheint", "er", "den\u00b7noch", "zu", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehr viel mehr als sein Neveu.", "tokens": ["Sehr", "viel", "mehr", "als", "sein", "Ne\u00b7veu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}}}}