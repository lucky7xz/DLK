{"dta.poem.6150": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Subtilen Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er zeugt auf diese Art die Federn in den Stahl,", "tokens": ["Er", "zeugt", "auf", "die\u00b7se", "Art", "die", "Fe\u00b7dern", "in", "den", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zeigt die Spannungs-Krafft an allen Orten.", "tokens": ["Und", "zeigt", "die", "Span\u00b7nungs\u00b7Krafft", "an", "al\u00b7len", "Or\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom alleruntersten, verborgnen Abgrunds-Thal,", "tokens": ["Vom", "al\u00b7ler\u00b7un\u00b7ters\u00b7ten", ",", "ver\u00b7borg\u00b7nen", "Ab\u00b7grunds\u00b7Thal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis an des h\u00f6chsten Himmels Pforten,", "tokens": ["Bis", "an", "des", "h\u00f6chs\u00b7ten", "Him\u00b7mels", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durchl\u00e4ufft er alle Welt. Ein Stoff, der rein,", "tokens": ["Durch\u00b7l\u00e4ufft", "er", "al\u00b7le", "Welt", ".", "Ein", "Stoff", ",", "der", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$.", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "War schon von Zeno auch erkannt,", "tokens": ["War", "schon", "von", "Ze\u00b7no", "auch", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als wenn er n\u00e4hrete der hellen Sonnen Brand,", "tokens": ["Als", "wenn", "er", "n\u00e4h\u00b7re\u00b7te", "der", "hel\u00b7len", "Son\u00b7nen", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ersetzt\u2019 und mehrete des Lichtes Schein.", "tokens": ["Er\u00b7setzt'", "und", "meh\u00b7re\u00b7te", "des", "Lich\u00b7tes", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}