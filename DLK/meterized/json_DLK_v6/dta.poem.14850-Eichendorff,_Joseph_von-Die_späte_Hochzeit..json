{"dta.poem.14850": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die sp\u00e4te Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mond ging unter &#8212; jetzt ist's Zeit. &#8212;", "tokens": ["Der", "Mond", "ging", "un\u00b7ter", "&#8212;", "jetzt", "ist's", "Zeit", ".", "&#8212;"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "punct", "XML_entity"], "pos": ["ART", "NN", "VVFIN", "APPR", "$(", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Br\u00e4ut'gam steigt vom Ro\u00df,", "tokens": ["Der", "Br\u00e4ut'\u00b7gam", "steigt", "vom", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat so lange schon gefreit \u2014", "tokens": ["Er", "hat", "so", "lan\u00b7ge", "schon", "ge\u00b7freit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da thut sich auf das Schlo\u00df,", "tokens": ["Da", "thut", "sich", "auf", "das", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und in der Halle sitzt die Braut", "tokens": ["Und", "in", "der", "Hal\u00b7le", "sitzt", "die", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf diamant'nem Sitz,", "tokens": ["Auf", "di\u00b7a\u00b7mant'\u00b7nem", "Sitz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Von ihrem Schmuck thut's durch den Bau", "tokens": ["Von", "ih\u00b7rem", "Schmuck", "thut's", "durch", "den", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein'n langen rothen Blitz. \u2014", "tokens": ["Ein'n", "lan\u00b7gen", "ro\u00b7then", "Blitz", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}