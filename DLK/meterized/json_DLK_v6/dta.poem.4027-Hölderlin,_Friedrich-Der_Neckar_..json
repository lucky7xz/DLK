{"dta.poem.4027": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Neckar .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "In deinen Th\u00e4lern wachte mein Herz mir auf", "tokens": ["In", "dei\u00b7nen", "Th\u00e4\u00b7lern", "wach\u00b7te", "mein", "Herz", "mir", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zum Leben, deine Wellen umspielten mich,", "tokens": ["Zum", "Le\u00b7ben", ",", "dei\u00b7ne", "Wel\u00b7len", "um\u00b7spiel\u00b7ten", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und all' der holden H\u00fcgel, die Dich,", "tokens": ["Und", "all'", "der", "hol\u00b7den", "H\u00fc\u00b7gel", ",", "die", "Dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wanderer! kennen, ist keiner fremd mir.", "tokens": ["Wan\u00b7de\u00b7rer", "!", "ken\u00b7nen", ",", "ist", "kei\u00b7ner", "fremd", "mir", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVINF", "$,", "VAFIN", "PIS", "ADJD", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Auf ihren Gipfeln l\u00f6ste des Himmels Luft", "tokens": ["Auf", "ih\u00b7ren", "Gip\u00b7feln", "l\u00f6s\u00b7te", "des", "Him\u00b7mels", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir oft der Knechtschaft Schmerzen; und aus dem", "tokens": ["Mir", "oft", "der", "Knecht\u00b7schaft", "Schmer\u00b7zen", ";", "und", "aus", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN", "NN", "$.", "KON", "APPR", "ART"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Thal,", "tokens": ["Thal", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wie Leben aus dem Freudebecher,", "tokens": ["Wie", "Le\u00b7ben", "aus", "dem", "Freu\u00b7de\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gl\u00e4nzte die bl\u00e4uliche Silberwelle.", "tokens": ["Gl\u00e4nz\u00b7te", "die", "bl\u00e4u\u00b7li\u00b7che", "Sil\u00b7ber\u00b7wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Der Berge Quellen eilten hinab zu dir,", "tokens": ["Der", "Ber\u00b7ge", "Quel\u00b7len", "eil\u00b7ten", "hin\u00b7ab", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit ihnen auch mein Herz, und Du nahmst uns mit", "tokens": ["Mit", "ih\u00b7nen", "auch", "mein", "Herz", ",", "und", "Du", "nahmst", "uns", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zum still erhabnen Rhein, zu seinen", "tokens": ["Zum", "still", "er\u00b7hab\u00b7nen", "Rhein", ",", "zu", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJD", "ADJA", "NE", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00e4dten hinunter und lust'gen Inseln. \u2014", "tokens": ["St\u00e4d\u00b7ten", "hin\u00b7un\u00b7ter", "und", "lust'\u00b7gen", "In\u00b7seln", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Noch d\u00fcnkt die Welt mir sch\u00f6n, und das Aug'", "tokens": ["Noch", "d\u00fcnkt", "die", "Welt", "mir", "sch\u00f6n", ",", "und", "das", "Aug'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,", "KON", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "entflieht,", "tokens": ["ent\u00b7flieht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Verlangend nach den Reizen der Erde, mir", "tokens": ["Ver\u00b7lan\u00b7gend", "nach", "den", "Rei\u00b7zen", "der", "Er\u00b7de", ",", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "$,", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zum goldnen Pactol, zu Smyrna's", "tokens": ["Zum", "gold\u00b7nen", "Pac\u00b7tol", ",", "zu", "Smyr\u00b7na's"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ufer, zu Ilions Wald. Auch m\u00f6cht' ich", "tokens": ["U\u00b7fer", ",", "zu", "I\u00b7li\u00b7ons", "Wald", ".", "Auch", "m\u00f6cht'", "ich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NE", "NN", "$.", "ADV", "VMFIN", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Bei Sunium oft landen, den stummen Pfad", "tokens": ["Bei", "Su\u00b7ni\u00b7um", "oft", "lan\u00b7den", ",", "den", "stum\u00b7men", "Pfad"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "ADV", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Nach deinen S\u00e4ulen fragen, Olympion!", "tokens": ["Nach", "dei\u00b7nen", "S\u00e4u\u00b7len", "fra\u00b7gen", ",", "O\u00b7lym\u00b7pi\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch eh' der Sturmwind und das Alter", "tokens": ["Noch", "eh'", "der", "Sturm\u00b7wind", "und", "das", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hin in den Schutt der Athenertempel", "tokens": ["Hin", "in", "den", "Schutt", "der", "A\u00b7the\u00b7ner\u00b7tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Und ihrer Gottesbilder auch dich begr\u00e4bt;", "tokens": ["Und", "ih\u00b7rer", "Got\u00b7tes\u00b7bil\u00b7der", "auch", "dich", "be\u00b7gr\u00e4bt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn lang schon einsam stehst du, o Stolz der Welt,", "tokens": ["Denn", "lang", "schon", "ein\u00b7sam", "stehst", "du", ",", "o", "Stolz", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADJD", "VVFIN", "PPER", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die nicht mehr ist. Und o ihr sch\u00f6nen", "tokens": ["Die", "nicht", "mehr", "ist", ".", "Und", "o", "ihr", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV", "VAFIN", "$.", "KON", "FM", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Inseln Ioniens! wo die Meerluft", "tokens": ["In\u00b7seln", "I\u00b7o\u00b7ni\u00b7ens", "!", "wo", "die", "Meer\u00b7luft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$.", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Die hei\u00dfen Ufer k\u00fchlt und den Lorbeerwald", "tokens": ["Die", "hei\u00b7\u00dfen", "U\u00b7fer", "k\u00fchlt", "und", "den", "Lor\u00b7beer\u00b7wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durchs\u00e4uselt, wenn die Sonne den Weinstock w\u00e4rmt.", "tokens": ["Durch\u00b7s\u00e4u\u00b7selt", ",", "wenn", "die", "Son\u00b7ne", "den", "Wein\u00b7stock", "w\u00e4rmt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ach! wo ein goldner Herbst dem armen", "tokens": ["Ach", "!", "wo", "ein", "gold\u00b7ner", "Herbst", "dem", "ar\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Volk' in Ges\u00e4nge die Seufzer wandelt,", "tokens": ["Vol\u00b7k'", "in", "Ge\u00b7s\u00e4n\u00b7ge", "die", "Seuf\u00b7zer", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Wenn sein Granatbaum reift, wenn aus gr\u00fcner", "tokens": ["Wenn", "sein", "Gra\u00b7nat\u00b7baum", "reift", ",", "wenn", "aus", "gr\u00fc\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "APPR", "ADJA"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nacht", "tokens": ["Nacht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die Pomeranze blinkt, und der Mastirbaum", "tokens": ["Die", "Po\u00b7me\u00b7ran\u00b7ze", "blinkt", ",", "und", "der", "Mas\u00b7tir\u00b7baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von Harze tr\u00e4uft, und Pauck' und Cymbel", "tokens": ["Von", "Har\u00b7ze", "tr\u00e4uft", ",", "und", "Pauck'", "und", "Cym\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "$,", "KON", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum labyrinthischen Tanze klingen.", "tokens": ["Zum", "la\u00b7by\u00b7rin\u00b7thi\u00b7schen", "Tan\u00b7ze", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Zu euch, ihr Inseln! bringt mich vielleicht, zu euch,", "tokens": ["Zu", "euch", ",", "ihr", "In\u00b7seln", "!", "bringt", "mich", "viel\u00b7leicht", ",", "zu", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Schutzgott einst; doch weicht mir aus treuem", "tokens": ["Mein", "Schutz\u00b7gott", "einst", ";", "doch", "weicht", "mir", "aus", "treu\u00b7em"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sinn", "tokens": ["Sinn"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Auch da mein Neckar nicht mit seinen", "tokens": ["Auch", "da", "mein", "Ne\u00b7ckar", "nicht", "mit", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Lieblichen Wiesen und Uferweiden.", "tokens": ["Lieb\u00b7li\u00b7chen", "Wie\u00b7sen", "und", "U\u00b7fer\u00b7wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}