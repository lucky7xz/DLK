{"textgrid.poem.64818": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob ich mit finsterm Urtheilsspruche", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ich mit finsterm Urtheilsspruche", "tokens": ["Ob", "ich", "mit", "fins\u00b7term", "Ur\u00b7theils\u00b7spru\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dich beschw\u00f6r' der Schmerzen Heer?", "tokens": ["Auf", "dich", "be\u00b7schw\u00f6r'", "der", "Schmer\u00b7zen", "Heer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob ich dich hasse und dir fluche? \u2013", "tokens": ["Ob", "ich", "dich", "has\u00b7se", "und", "dir", "flu\u00b7che", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "KON", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich liebe dich, wie nie vorher!", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "wie", "nie", "vor\u00b7her", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und h\u00f6rst du auch manch' Wort mich sprechen,", "tokens": ["Und", "h\u00f6rst", "du", "auch", "man\u00b7ch'", "Wort", "mich", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bei dem es Dir im Innern scheint,", "tokens": ["Bei", "dem", "es", "Dir", "im", "In\u00b7nern", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wollt' ich meine Qualen r\u00e4chen", "tokens": ["Als", "wollt'", "ich", "mei\u00b7ne", "Qua\u00b7len", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An dir, du vielgeliebter Feind: \u2013", "tokens": ["An", "dir", ",", "du", "viel\u00b7ge\u00b7lieb\u00b7ter", "Feind", ":", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So w\u00e4hlt, auf ewig dir verfallen,", "tokens": ["So", "w\u00e4hlt", ",", "auf", "e\u00b7wig", "dir", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz des Hasses Maske nur,", "tokens": ["Mein", "Herz", "des", "Has\u00b7ses", "Mas\u00b7ke", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um unverh\u00f6hnt dahinzuwallen", "tokens": ["Um", "un\u00b7ver\u00b7h\u00f6hnt", "da\u00b7hin\u00b7zu\u00b7wal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seiner heil'gen Liebesspur!", "tokens": ["Auf", "sei\u00b7ner", "heil'\u00b7gen", "Lie\u00b7bes\u00b7spur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}