{"dta.poem.2965": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir erw\u00e4gen Zeit und Ort, wo jeder steht,", "tokens": ["Wenn", "wir", "er\u00b7w\u00e4\u00b7gen", "Zeit", "und", "Ort", ",", "wo", "je\u00b7der", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "KON", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So darf uns gelten auch Mohammed als Profet.", "tokens": ["So", "darf", "uns", "gel\u00b7ten", "auch", "Mo\u00b7ham\u00b7med", "als", "Pro\u00b7fet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "NE", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "F\u00fcr andre Statt und Zeit w\u00e4r' er vielleicht ein schlechter,", "tokens": ["F\u00fcr", "and\u00b7re", "Statt", "und", "Zeit", "w\u00e4r'", "er", "viel\u00b7leicht", "ein", "schlech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Doch f\u00fcr die eigene war er gerad' ein rechter.", "tokens": ["Doch", "f\u00fcr", "die", "ei\u00b7ge\u00b7ne", "war", "er", "ge\u00b7rad'", "ein", "rech\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VAFIN", "PPER", "ADV", "ART", "ADJA", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Du aber danke Gott, da\u00df er an be\u00dfrem Ort", "tokens": ["Du", "a\u00b7ber", "dan\u00b7ke", "Gott", ",", "da\u00df", "er", "an", "be\u00df\u00b7rem", "Ort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(ehr's und verdirb es nicht!) dir gab ein be\u00dfres Wort.", "tokens": ["(", "ehr's", "und", "ver\u00b7dirb", "es", "nicht", "!", ")", "dir", "gab", "ein", "be\u00df\u00b7res", "Wort", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "KON", "VVIMP", "PPER", "PTKNEG", "$.", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}