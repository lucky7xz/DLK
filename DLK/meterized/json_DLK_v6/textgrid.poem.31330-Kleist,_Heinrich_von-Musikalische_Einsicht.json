{"textgrid.poem.31330": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Musikalische Einsicht", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeno, beschirmt, und Diogen, mich, ihr Weisen! Wie soll ich", "tokens": ["Ze\u00b7no", ",", "be\u00b7schirmt", ",", "und", "Dio\u00b7gen", ",", "mich", ",", "ihr", "Wei\u00b7sen", "!", "Wie", "soll", "ich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVPP", "$,", "KON", "NN", "$,", "PPER", "$,", "PPOSAT", "NN", "$.", "PWAV", "VMFIN", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Heute tugendhaft sein, da ich die Stimme geh\u00f6rt.", "tokens": ["Heu\u00b7te", "tu\u00b7gend\u00b7haft", "sein", ",", "da", "ich", "die", "Stim\u00b7me", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Eine Stimme, der Brust so schlank, wie die Zeder, entwachsen,", "tokens": ["Ei\u00b7ne", "Stim\u00b7me", ",", "der", "Brust", "so", "schlank", ",", "wie", "die", "Ze\u00b7der", ",", "ent\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sch\u00f6ner gewipfelt entbl\u00fcht keine, Parthenope, dir.", "tokens": ["Sch\u00f6\u00b7ner", "ge\u00b7wip\u00b7felt", "ent\u00b7bl\u00fcht", "kei\u00b7ne", ",", "Par\u00b7the\u00b7no\u00b7pe", ",", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "PIAT", "$,", "NN", "$,", "PPER", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nun versteh ich den Platon erst, ihr \u00efonischen Lieder,", "tokens": ["Nun", "ver\u00b7steh", "ich", "den", "Pla\u00b7ton", "erst", ",", "ihr", "\u00efo\u00b7ni\u00b7schen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Eure Gewalt, und warum Hellas in Fesseln jetzt liegt.", "tokens": ["Eu\u00b7re", "Ge\u00b7walt", ",", "und", "wa\u00b7rum", "Hel\u00b7las", "in", "Fes\u00b7seln", "jetzt", "liegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PWAV", "NE", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}