{"textgrid.poem.58446": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "484. Das sorgenlose Kind", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Kind, ich kann nicht sorgen,", "tokens": ["Ich", "bin", "ein", "Kind", ",", "ich", "kann", "nicht", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum geb' ich mich der Mutter hin", "tokens": ["Drum", "geb'", "ich", "mich", "der", "Mut\u00b7ter", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bleib in ihrem Scho\u00df verborgen", "tokens": ["Und", "bleib", "in", "ih\u00b7rem", "Scho\u00df", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In still-gelass'nem Kindersinn;", "tokens": ["In", "still\u00b7ge\u00b7lass'\u00b7nem", "Kin\u00b7der\u00b7sinn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zur\u00fcckzusehn, vorauszudenken,", "tokens": ["Zu\u00b7r\u00fcck\u00b7zu\u00b7sehn", ",", "vor\u00b7aus\u00b7zu\u00b7den\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00fcrd' nur mein Herz vergeblich kr\u00e4nken.", "tokens": ["W\u00fcrd'", "nur", "mein", "Herz", "ver\u00b7geb\u00b7lich", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}