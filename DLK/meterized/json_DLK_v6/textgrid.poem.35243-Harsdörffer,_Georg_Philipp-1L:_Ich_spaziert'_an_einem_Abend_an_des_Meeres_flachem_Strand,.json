{"textgrid.poem.35243": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich spaziert' an einem Abend an des Meeres flachem Strand,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich spaziert' an einem Abend an des Meeres flachem Strand,", "tokens": ["Ich", "spa\u00b7ziert'", "an", "ei\u00b7nem", "A\u00b7bend", "an", "des", "Mee\u00b7res", "fla\u00b7chem", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Da viel rauhgefaltne Muscheln lagen auf dem schroffen Sand.", "tokens": ["Da", "viel", "rauh\u00b7ge\u00b7falt\u00b7ne", "Mu\u00b7scheln", "la\u00b7gen", "auf", "dem", "schrof\u00b7fen", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Als der Sonnen Purpurglanz fast war in die Fluth verschossen,", "tokens": ["Als", "der", "Son\u00b7nen", "Pur\u00b7pur\u00b7glanz", "fast", "war", "in", "die", "Fluth", "ver\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Haben diese Muschels\u00f6hn' ihre H\u00e4user zugeschlossen.", "tokens": ["Ha\u00b7ben", "die\u00b7se", "Mu\u00b7schels\u00f6hn'", "ih\u00b7re", "H\u00e4u\u00b7ser", "zu\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Es ging aus dem g\u00fcldnen Bette auf die Sonn', als Br\u00e4utigam;", "tokens": ["Es", "ging", "aus", "dem", "g\u00fcld\u00b7nen", "Bet\u00b7te", "auf", "die", "Sonn'", ",", "als", "Br\u00e4u\u00b7ti\u00b7gam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,", "KOUS", "NE", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Bald von ihrer Strahlenhitze neue Kraft der Saft gewann", "tokens": ["Bald", "von", "ih\u00b7rer", "Strah\u00b7len\u00b7hit\u00b7ze", "neu\u00b7e", "Kraft", "der", "Saft", "ge\u00b7wann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "In dem offnen Muschelschlo\u00df; und den Strahlen zugekehret,", "tokens": ["In", "dem", "off\u00b7nen", "Mu\u00b7schel\u00b7schlo\u00df", ";", "und", "den", "Strah\u00b7len", "zu\u00b7ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+++--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "Ward gebuntet ihre Farbe und ihr innrer Kern gen\u00e4hret.", "tokens": ["Ward", "ge\u00b7bun\u00b7tet", "ih\u00b7re", "Far\u00b7be", "und", "ihr", "inn\u00b7rer", "Kern", "ge\u00b7n\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ob uns wohl die S\u00fcndenn\u00e4chte schlie\u00dfen eine kurze Zeit,", "tokens": ["Ob", "uns", "wohl", "die", "S\u00fcn\u00b7den\u00b7n\u00e4ch\u00b7te", "schlie\u00b7\u00dfen", "ei\u00b7ne", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Wird doch unser Durst und Hunger nach der Sonnen Lieblichkeit", "tokens": ["Wird", "doch", "un\u00b7ser", "Durst", "und", "Hun\u00b7ger", "nach", "der", "Son\u00b7nen", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Nie geschlossen und bet\u00e4ubt. Unsre Herzen kluften offen;", "tokens": ["Nie", "ge\u00b7schlos\u00b7sen", "und", "be\u00b7t\u00e4ubt", ".", "Uns\u00b7re", "Her\u00b7zen", "kluf\u00b7ten", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Du f\u00fcllst, o getreuer Gott, die auf deine Gnade hoffen.", "tokens": ["Du", "f\u00fcllst", ",", "o", "ge\u00b7treu\u00b7er", "Gott", ",", "die", "auf", "dei\u00b7ne", "Gna\u00b7de", "hof\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}