{"textgrid.poem.31157": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder nun l\u00e4sst aus dem Dunkeln", "tokens": ["Und", "wie\u00b7der", "nun", "l\u00e4sst", "aus", "dem", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Weihnacht ihre Sterne funkeln!", "tokens": ["Die", "Weih\u00b7nacht", "ih\u00b7re", "Ster\u00b7ne", "fun\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Engel im Himmel h\u00f6rt man sich k\u00fcssen", "tokens": ["Die", "En\u00b7gel", "im", "Him\u00b7mel", "h\u00f6rt", "man", "sich", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PIS", "PRF", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die ganze Welt riecht nach Pfeffern\u00fcssen ...", "tokens": ["Und", "die", "gan\u00b7ze", "Welt", "riecht", "nach", "Pfef\u00b7fer\u00b7n\u00fcs\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So heimlich war es die letzten Wochen,", "tokens": ["So", "heim\u00b7lich", "war", "es", "die", "letz\u00b7ten", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die H\u00e4user nach Mehl und Honig rochen,", "tokens": ["Die", "H\u00e4u\u00b7ser", "nach", "Mehl", "und", "Ho\u00b7nig", "ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die D\u00e4cher lagen dick verschneit", "tokens": ["Die", "D\u00e4\u00b7cher", "la\u00b7gen", "dick", "ver\u00b7schneit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fern, noch fern schien die sch\u00f6ne Zeit.", "tokens": ["Und", "fern", ",", "noch", "fern", "schien", "die", "sch\u00f6\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Man dachte an sie kaum dann und wann.", "tokens": ["Man", "dach\u00b7te", "an", "sie", "kaum", "dann", "und", "wann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "ADV", "ADV", "KON", "PWAV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mutter teigte die Kuchen an", "tokens": ["Mut\u00b7ter", "teig\u00b7te", "die", "Ku\u00b7chen", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Und Vater, dem mehr der Lehnstuhl taugte,", "tokens": ["Und", "Va\u00b7ter", ",", "dem", "mehr", "der", "Lehn\u00b7stuhl", "taug\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sass daneben und las und rauchte.", "tokens": ["Sass", "da\u00b7ne\u00b7ben", "und", "las", "und", "rauch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da pl\u00f6tzlich, eh man sich's versah,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", ",", "eh", "man", "sich's", "ver\u00b7sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit einem Mal war sie wieder da.", "tokens": ["Mit", "ei\u00b7nem", "Mal", "war", "sie", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Mitten im Zimmer steht nun der Baum!", "tokens": ["Mit\u00b7ten", "im", "Zim\u00b7mer", "steht", "nun", "der", "Baum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Man reibt sich die Augen und glaubt es kaum ...", "tokens": ["Man", "reibt", "sich", "die", "Au\u00b7gen", "und", "glaubt", "es", "kaum", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Ketten schaukeln, die Lichter wehn,", "tokens": ["Die", "Ket\u00b7ten", "schau\u00b7keln", ",", "die", "Lich\u00b7ter", "wehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Herrgott, was giebt's da nicht alles zu sehn!", "tokens": ["Herr\u00b7gott", ",", "was", "giebt's", "da", "nicht", "al\u00b7les", "zu", "sehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "ADV", "PTKNEG", "PIS", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Die kleinen K\u00fcgelchen und hier", "tokens": ["Die", "klei\u00b7nen", "K\u00fc\u00b7gel\u00b7chen", "und", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADV"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Die niedlichen Kr\u00f6nchen aus Goldpapier!", "tokens": ["Die", "nied\u00b7li\u00b7chen", "Kr\u00f6n\u00b7chen", "aus", "Gold\u00b7pa\u00b7pier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und an all den gr\u00fcnen, glitzernden Schn\u00fcrchen", "tokens": ["Und", "an", "all", "den", "gr\u00fc\u00b7nen", ",", "glit\u00b7zern\u00b7den", "Schn\u00fcr\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "All die unz\u00e4hligen, kleinen Fig\u00fcrchen:", "tokens": ["All", "die", "un\u00b7z\u00e4h\u00b7li\u00b7gen", ",", "klei\u00b7nen", "Fi\u00b7g\u00fcr\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Mohren, Schlittschuhl\u00e4ufer und Schw\u00e4lbchen,", "tokens": ["Moh\u00b7ren", ",", "Schlitt\u00b7schuh\u00b7l\u00e4u\u00b7fer", "und", "Schw\u00e4lb\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Elephanten und kleine K\u00e4lbchen,", "tokens": ["E\u00b7le\u00b7phan\u00b7ten", "und", "klei\u00b7ne", "K\u00e4lb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Schornsteinfeger und trommelnde Hasen,", "tokens": ["Schorn\u00b7stein\u00b7fe\u00b7ger", "und", "trom\u00b7meln\u00b7de", "Ha\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+----+--+-", "measure": "trochaic.tri.relaxed"}, "line.11": {"text": "Dicke Kerle mit rothen Nasen,", "tokens": ["Di\u00b7cke", "Ker\u00b7le", "mit", "ro\u00b7then", "Na\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Reiche Hunde und arme Schlucker", "tokens": ["Rei\u00b7che", "Hun\u00b7de", "und", "ar\u00b7me", "Schlu\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Und Alles, Alles aus purem Zucker!", "tokens": ["Und", "Al\u00b7les", ",", "Al\u00b7les", "aus", "pu\u00b7rem", "Zu\u00b7cker", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein alter Herr mit weissen B\u00e4ffchen", "tokens": ["Ein", "al\u00b7ter", "Herr", "mit", "weis\u00b7sen", "B\u00e4ff\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4ngt grade unter einem Aeffchen.", "tokens": ["H\u00e4ngt", "gra\u00b7de", "un\u00b7ter", "ei\u00b7nem", "A\u00b7ef\u00b7fchen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hier gar sch\u00e4lt sich aus seinem Ei", "tokens": ["Und", "hier", "gar", "sch\u00e4lt", "sich", "aus", "sei\u00b7nem", "Ei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein kleiner, gefl\u00fcgelter Nackedei.", "tokens": ["Ein", "klei\u00b7ner", ",", "ge\u00b7fl\u00fc\u00b7gel\u00b7ter", "Na\u00b7cke\u00b7dei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und oben, oben erst in der Krone!!", "tokens": ["Und", "o\u00b7ben", ",", "o\u00b7ben", "erst", "in", "der", "Kro\u00b7ne", "!!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Da h\u00e4ngt eine wirkliche, gelbe Kanone", "tokens": ["Da", "h\u00e4ngt", "ei\u00b7ne", "wirk\u00b7li\u00b7che", ",", "gel\u00b7be", "Ka\u00b7no\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und ein Husarenleutnant mit silbernen Tressen \u2013", "tokens": ["Und", "ein", "Hu\u00b7sa\u00b7ren\u00b7leut\u00b7nant", "mit", "sil\u00b7ber\u00b7nen", "Tres\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "--+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ich glaube wahrhaftig, man kann ihn essen!", "tokens": ["Ich", "glau\u00b7be", "wahr\u00b7haf\u00b7tig", ",", "man", "kann", "ihn", "es\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In den offenen M\u00e4ulerchen ihre Finger,", "tokens": ["In", "den", "of\u00b7fe\u00b7nen", "M\u00e4u\u00b7ler\u00b7chen", "ih\u00b7re", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Stehn um den Tisch die kleinen Dinger,", "tokens": ["Stehn", "um", "den", "Tisch", "die", "klei\u00b7nen", "Din\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und um die Wette mit den Kerzen", "tokens": ["Und", "um", "die", "Wet\u00b7te", "mit", "den", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Puppern vor Freuden ihre Herzen.", "tokens": ["Pup\u00b7pern", "vor", "Freu\u00b7den", "ih\u00b7re", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ihre grossen, blauen Augen leuchten,", "tokens": ["Ih\u00b7re", "gros\u00b7sen", ",", "blau\u00b7en", "Au\u00b7gen", "leuch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Indess die unsern sich leise feuchten.", "tokens": ["In\u00b7dess", "die", "un\u00b7sern", "sich", "lei\u00b7se", "feuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "PRF", "ADJD", "ADJA", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wir sind ja leider schon l\u00e4ngst \u00bberwachsen\u00ab,", "tokens": ["Wir", "sind", "ja", "lei\u00b7der", "schon", "l\u00e4ngst", "\u00bb", "er\u00b7wach\u00b7sen", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADV", "$(", "VVPP", "$(", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Uns dreht sich die Welt um ", "tokens": ["Uns", "dreht", "sich", "die", "Welt", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Und zwar zumeist um unser B\u00fcreau.", "tokens": ["Und", "zwar", "zu\u00b7meist", "um", "un\u00b7ser", "B\u00fc\u00b7re\u00b7au", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ach, nicht wie fr\u00fcher mehr macht uns froh", "tokens": ["Ach", ",", "nicht", "wie", "fr\u00fc\u00b7her", "mehr", "macht", "uns", "froh"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PTKNEG", "KOKOM", "ADJD", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aus Zinkblech eine Eisenbahn,", "tokens": ["Aus", "Zink\u00b7blech", "ei\u00b7ne", "Ei\u00b7sen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein kleines Schweinchen aus Marzipan.", "tokens": ["Ein", "klei\u00b7nes", "Schwein\u00b7chen", "aus", "Mar\u00b7zi\u00b7pan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Eine Blechtrompete gefiel uns einst sehr,", "tokens": ["Ei\u00b7ne", "Blech\u00b7trom\u00b7pe\u00b7te", "ge\u00b7fiel", "uns", "einst", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Der Reichstag interessirt uns heut mehr;", "tokens": ["Der", "Reichs\u00b7tag", "in\u00b7ter\u00b7es\u00b7sirt", "uns", "heut", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Auch sind wir verliebt in die Regeldetri", "tokens": ["Auch", "sind", "wir", "ver\u00b7liebt", "in", "die"