{"dta.poem.23820": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "G\u00e4rtnerin", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Die dieses G\u00e4rtner-Weib in ihrer Einfalt schauen/", "tokens": ["Die", "die\u00b7ses", "G\u00e4rt\u00b7ner\u00b7Weib", "in", "ih\u00b7rer", "Ein\u00b7falt", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die glauben nicht zu sehr dem frommen Angesicht.", "tokens": ["Die", "glau\u00b7ben", "nicht", "zu", "sehr", "dem", "from\u00b7men", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "PTKA", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den stillen Wassern ist am wenigsten zu trauen/", "tokens": ["Den", "stil\u00b7len", "Was\u00b7sern", "ist", "am", "we\u00b7nigs\u00b7ten", "zu", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wi\u00dft da\u00df man viel von ihr und dem ", "tokens": ["Wi\u00dft", "da\u00df", "man", "viel", "von", "ihr", "und", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PIS", "ADV", "APPR", "PPER", "KON", "ART"], "meter": "+--