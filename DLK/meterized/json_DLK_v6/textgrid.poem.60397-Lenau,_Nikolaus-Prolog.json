{"textgrid.poem.60397": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Prolog", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Winter stand ein eiserner Tyrann,", "tokens": ["Der", "Win\u00b7ter", "stand", "ein", "ei\u00b7ser\u00b7ner", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Nie l\u00f6send seine Faust, die festgeballte,", "tokens": ["Nie", "l\u00f6\u00b7send", "sei\u00b7ne", "Faust", ",", "die", "fest\u00b7ge\u00b7ball\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die eisig sich um Berg' und T\u00e4ler krallte;", "tokens": ["Die", "ei\u00b7sig", "sich", "um", "Ber\u00b7g'", "und", "T\u00e4\u00b7ler", "krall\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihr Leben lag erstarrt in seinem Bann.", "tokens": ["Ihr", "Le\u00b7ben", "lag", "er\u00b7starrt", "in", "sei\u00b7nem", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als frostbedeckt die Berg' und Tale ruhten,", "tokens": ["Als", "frost\u00b7be\u00b7deckt", "die", "Ber\u00b7g'", "und", "Ta\u00b7le", "ruh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gesellig dr\u00e4ngte doch das Menschenleben", "tokens": ["Ge\u00b7sel\u00b7lig", "dr\u00e4ng\u00b7te", "doch", "das", "Men\u00b7schen\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Lust und Spiel zusammen seine Gluten,", "tokens": ["In", "Lust", "und", "Spiel", "zu\u00b7sam\u00b7men", "sei\u00b7ne", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Lie\u00df Freudenfeste \u00fcberm Tode schweben.", "tokens": ["Lie\u00df", "Freu\u00b7den\u00b7fes\u00b7te", "\u00fc\u00b7berm", "To\u00b7de", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zum Tanz berauschend sangen helle Geigen,", "tokens": ["Zum", "Tanz", "be\u00b7rau\u00b7schend", "san\u00b7gen", "hel\u00b7le", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die sch\u00f6ne Jugend drehte sich im Reigen,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ju\u00b7gend", "dreh\u00b7te", "sich", "im", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nicht denkend an ein Scheiden und Vergehen,", "tokens": ["Nicht", "den\u00b7kend", "an", "ein", "Schei\u00b7den", "und", "Ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sorglos, wie sich die Stern am Himmel drehen.", "tokens": ["Sorg\u00b7los", ",", "wie", "sich", "die", "Stern", "am", "Him\u00b7mel", "dre\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PRF", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und \u00fcbers blanke Feld des Eises glitten", "tokens": ["Und", "\u00fc\u00b7bers", "blan\u00b7ke", "Feld", "des", "Ei\u00b7ses", "glit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Mit Gei\u00dfelknall und Schellenklang die Schlitten.", "tokens": ["Mit", "Gei\u00b7\u00df\u00b7el\u00b7knall", "und", "Schel\u00b7len\u00b7klang", "die", "Schlit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "So war es j\u00fcngst noch im Magyarenlande,", "tokens": ["So", "war", "es", "j\u00fcngst", "noch", "im", "Ma\u00b7gy\u00b7ar\u00b7en\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Am segen\u00fcberh\u00e4uften Donaustrande.", "tokens": ["Am", "se\u00b7gen\u00b7\u00fc\u00b7berh\u00b7\u00e4uf\u00b7ten", "Do\u00b7naus\u00b7tran\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wer h\u00e4tte wohl in so begl\u00fcckten Stunden", "tokens": ["Wer", "h\u00e4t\u00b7te", "wohl", "in", "so", "be\u00b7gl\u00fcck\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Den Donnerschlag des Ungl\u00fccks vorempfunden?", "tokens": ["Den", "Don\u00b7ner\u00b7schlag", "des", "Un\u00b7gl\u00fccks", "vor\u00b7emp\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wer h\u00f6rte damals in den Schlittenschellen", "tokens": ["Wer", "h\u00f6r\u00b7te", "da\u00b7mals", "in", "den", "Schlit\u00b7ten\u00b7schel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Prophetisch grause Totengl\u00f6cklein gellen?", "tokens": ["Pro\u00b7phe\u00b7tisch", "grau\u00b7se", "To\u00b7ten\u00b7gl\u00f6c\u00b7klein", "gel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Kein T\u00e4nzer ahnte dort beim Taumelfeste", "tokens": ["Kein", "T\u00e4n\u00b7zer", "ahn\u00b7te", "dort", "beim", "Tau\u00b7mel\u00b7fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Im Wassersturme tanzende Pal\u00e4ste.", "tokens": ["Im", "Was\u00b7ser\u00b7stur\u00b7me", "tan\u00b7zen\u00b7de", "Pa\u00b7l\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Die Jubeltage waren bald verflogen,", "tokens": ["Die", "Ju\u00b7bel\u00b7ta\u00b7ge", "wa\u00b7ren", "bald", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die Freude senkte die erregten Wogen,", "tokens": ["Die", "Freu\u00b7de", "senk\u00b7te", "die", "er\u00b7reg\u00b7ten", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die Zeit des holden Fr\u00fchlings war gekommen,", "tokens": ["Die", "Zeit", "des", "hol\u00b7den", "Fr\u00fch\u00b7lings", "war", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die alle Herzen sp\u00fcren s\u00fc\u00df beklommen,", "tokens": ["Die", "al\u00b7le", "Her\u00b7zen", "sp\u00fc\u00b7ren", "s\u00fc\u00df", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die Zeit, wo aus dem Eis die Knospen springen", "tokens": ["Die", "Zeit", ",", "wo", "aus", "dem", "Eis", "die", "Knos\u00b7pen", "sprin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und hell vom Liebesfest die W\u00e4lder klingen.", "tokens": ["Und", "hell", "vom", "Lie\u00b7bes\u00b7fest", "die", "W\u00e4l\u00b7der", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "O Fr\u00fchling, alle Herzen harrten dein,", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "al\u00b7le", "Her\u00b7zen", "harr\u00b7ten", "dein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PIAT", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Auf deine Lieder, deinen Sonnenschein;", "tokens": ["Auf", "dei\u00b7ne", "Lie\u00b7der", ",", "dei\u00b7nen", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Wie schrecklich aber t\u00e4uschtest du ihr Hoffen,", "tokens": ["Wie", "schreck\u00b7lich", "a\u00b7ber", "t\u00e4uschtest", "du", "ihr", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.33": {"text": "Sturml\u00e4uten, Jammerruf und H\u00fclfeschreien,", "tokens": ["Sturm\u00b7l\u00e4u\u00b7ten", ",", "Jam\u00b7mer\u00b7ruf", "und", "H\u00fcl\u00b7fe\u00b7schrei\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.34": {"text": "Und Flutendonner, schlagend an die W\u00e4nde,", "tokens": ["Und", "Flu\u00b7ten\u00b7don\u00b7ner", ",", "schla\u00b7gend", "an", "die", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Sind diesmal, Fr\u00fchling, deine Melodeien;", "tokens": ["Sind", "dies\u00b7mal", ",", "Fr\u00fch\u00b7ling", ",", "dei\u00b7ne", "Me\u00b7lo\u00b7dei\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Und deine Blumen sind gerungne H\u00e4nde,", "tokens": ["Und", "dei\u00b7ne", "Blu\u00b7men", "sind", "ge\u00b7rung\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und rings verzweiflungsblasse Angesichter;", "tokens": ["Und", "rings", "ver\u00b7zwei\u00b7flungs\u00b7blas\u00b7se", "An\u00b7ge\u00b7sich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Diesmal bist du gekommen als Vernichter!", "tokens": ["Dies\u00b7mal", "bist", "du", "ge\u00b7kom\u00b7men", "als", "Ver\u00b7nich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Danubius, der starke Riese, hat", "tokens": ["Da\u00b7nu\u00b7bi\u00b7us", ",", "der", "star\u00b7ke", "Rie\u00b7se", ",", "hat"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Schon l\u00e4ngst gebuhlt um diese sch\u00f6ne Stadt;", "tokens": ["Schon", "l\u00e4ngst", "ge\u00b7buhlt", "um", "die\u00b7se", "sch\u00f6\u00b7ne", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Der Riese hat an hellen Sommertagen", "tokens": ["Der", "Rie\u00b7se", "hat", "an", "hel\u00b7len", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Auf seiner breiten Brust ihr Bild getragen,", "tokens": ["Auf", "sei\u00b7ner", "brei\u00b7ten", "Brust", "ihr", "Bild", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Er trug ihr Bild gefa\u00dft in Strahlenflimmer;", "tokens": ["Er", "trug", "ihr", "Bild", "ge\u00b7fa\u00dft", "in", "Strah\u00b7len\u00b7flim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Wie hat es doch so bang gezittert immer!", "tokens": ["Wie", "hat", "es", "doch", "so", "bang", "ge\u00b7zit\u00b7tert", "im\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.45": {"text": "Zu Winter hielt er einen festen Schlaf,", "tokens": ["Zu", "Win\u00b7ter", "hielt", "er", "ei\u00b7nen", "fes\u00b7ten", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Bis weckend ihn der Hauch des Fr\u00fchlings traf.", "tokens": ["Bis", "we\u00b7ckend", "ihn", "der", "Hauch", "des", "Fr\u00fch\u00b7lings", "traf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Urpl\u00f6tzlich ward vom Schlaf Danubius munter,", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "ward", "vom", "Schlaf", "Da\u00b7nu\u00b7bi\u00b7us", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "NE", "ADJD", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Er springt nach seiner Braut mit offnen Armen,", "tokens": ["Er", "springt", "nach", "sei\u00b7ner", "Braut", "mit", "off\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Sie jammert auf, er fa\u00dft sie ohn Erbarmen", "tokens": ["Sie", "jam\u00b7mert", "auf", ",", "er", "fa\u00dft", "sie", "ohn", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+