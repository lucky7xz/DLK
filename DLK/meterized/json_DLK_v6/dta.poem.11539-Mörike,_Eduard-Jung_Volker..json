{"dta.poem.11539": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Jung Volker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Fidel und mit Flinte,", "tokens": ["Mit", "Fi\u00b7del", "und", "mit", "Flin\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Damit er geigen und schie\u00dfen kann,", "tokens": ["Da\u00b7mit", "er", "gei\u00b7gen", "und", "schie\u00b7\u00dfen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nachdem just Wetter und Winde.", "tokens": ["Nach\u00b7dem", "just", "Wet\u00b7ter", "und", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Fidel und die Flint,", "tokens": ["Fi\u00b7del", "und", "die", "Flint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Fidel und die Flint!", "tokens": ["Fi\u00b7del", "und", "die", "Flint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Volker spielt auf.", "tokens": ["Vol\u00b7ker", "spielt", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ich sah ihn hoch im Sonnenschein", "tokens": ["Ich", "sah", "ihn", "hoch", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einem H\u00fcgel sitzen:", "tokens": ["Auf", "ei\u00b7nem", "H\u00fc\u00b7gel", "sit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da spielt er die Geig' und schluckt rothen Wein,", "tokens": ["Da", "spielt", "er", "die", "Geig'", "und", "schluckt", "ro\u00b7then", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seine blauen Augen ihm blitzen.", "tokens": ["Sei\u00b7ne", "blau\u00b7en", "Au\u00b7gen", "ihm", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Fidel und die Flint,", "tokens": ["Fi\u00b7del", "und", "die", "Flint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Fidel und die Flint!", "tokens": ["Fi\u00b7del", "und", "die", "Flint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Volker spielt auf.", "tokens": ["Vol\u00b7ker", "spielt", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Auf einmal, er schleudert die Geig' in die Luft,", "tokens": ["Auf", "ein\u00b7mal", ",", "er", "schleu\u00b7dert", "die", "Geig'", "in", "die", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf einmal, er wirft sich zu Pferde;", "tokens": ["Auf", "ein\u00b7mal", ",", "er", "wirft", "sich", "zu", "Pfer\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Feind kommt! Da st\u00f6\u00dft er in's Pfeifchen und ruft:", "tokens": ["Der", "Feind", "kommt", "!", "Da", "st\u00f6\u00dft", "er", "in's", "Pfei\u00b7fchen", "und", "ruft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Brecht los, wie der Wolf in die Herde!", "tokens": ["Brecht", "los", ",", "wie", "der", "Wolf", "in", "die", "Her\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "ART", "NE", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Fidel und die Flint,", "tokens": ["Fi\u00b7del", "und", "die", "Flint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Fidel und die Flint!", "tokens": ["Fi\u00b7del", "und", "die", "Flint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Volker spielt auf.", "tokens": ["Vol\u00b7ker", "spielt", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}