{"textgrid.poem.50074": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Manch ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manch ", "tokens": ["Manch"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Indem manch theurer Mann aus aller H\u00f6ffling' acht',", "tokens": ["In\u00b7dem", "manch", "theu\u00b7rer", "Mann", "aus", "al\u00b7ler", "H\u00f6ff\u00b7ling'", "acht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sich sonst bey keiner ", "tokens": ["Sich", "sonst", "bey", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Viel hebt das Gl\u00fcck empor, viel h\u00e4lt es auch zur\u00fcck:", "tokens": ["Viel", "hebt", "das", "Gl\u00fcck", "em\u00b7por", ",", "viel", "h\u00e4lt", "es", "auch", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wer die ", "tokens": ["Doch", "wer", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}