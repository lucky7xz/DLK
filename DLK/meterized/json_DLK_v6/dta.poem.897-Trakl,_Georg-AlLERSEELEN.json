{"dta.poem.897": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "AlLERSEELEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Die M\u00e4nnlein, Weiblein, traurige Gesellen,", "tokens": ["Die", "M\u00e4nn\u00b7lein", ",", "Weib\u00b7lein", ",", "trau\u00b7ri\u00b7ge", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie streuen heute Blumen blau und rot", "tokens": ["Sie", "streu\u00b7en", "heu\u00b7te", "Blu\u00b7men", "blau", "und", "rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf ihre Gr\u00fcfte, die sich zag erhellen.", "tokens": ["Auf", "ih\u00b7re", "Gr\u00fcf\u00b7te", ",", "die", "sich", "zag", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie tun wie arme Puppen vor dem Tod.", "tokens": ["Sie", "tun", "wie", "ar\u00b7me", "Pup\u00b7pen", "vor", "dem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KOKOM", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O! wie sie hier voll Angst und Demut scheinen,", "tokens": ["O", "!", "wie", "sie", "hier", "voll", "Angst", "und", "De\u00b7mut", "schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "ADV", "ADJD", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Schatten hinter schwarzen B\u00fcschen stehn.", "tokens": ["Wie", "Schat\u00b7ten", "hin\u00b7ter", "schwar\u00b7zen", "B\u00fc\u00b7schen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Herbstwind klagt der Ungebornen Weinen,", "tokens": ["Im", "Herbst\u00b7wind", "klagt", "der", "Un\u00b7ge\u00b7bor\u00b7nen", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch sieht man Lichter in der Irre gehn.", "tokens": ["Auch", "sieht", "man", "Lich\u00b7ter", "in", "der", "Ir\u00b7re", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Seufzen Liebender haucht in Gezweigen", "tokens": ["Das", "Seuf\u00b7zen", "Lie\u00b7ben\u00b7der", "haucht", "in", "Ge\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und dort verwest die Mutter mit dem Kind.", "tokens": ["Und", "dort", "ver\u00b7west", "die", "Mut\u00b7ter", "mit", "dem", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unwirklich scheinet der Lebendigen Reigen", "tokens": ["Un\u00b7wirk\u00b7lich", "schei\u00b7net", "der", "Le\u00b7ben\u00b7di\u00b7gen", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wunderlich zerstreut im Abendwind.", "tokens": ["Und", "wun\u00b7der\u00b7lich", "zer\u00b7streut", "im", "Ab\u00b7end\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr Leben ist so wirr, voll tr\u00fcber Plagen.", "tokens": ["Ihr", "Le\u00b7ben", "ist", "so", "wirr", ",", "voll", "tr\u00fc\u00b7ber", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erbarm\u2019 dich Gott der Frauen H\u00f6ll\u2019 und Qual,", "tokens": ["Er\u00b7barm'", "dich", "Gott", "der", "Frau\u00b7en", "H\u00f6ll'", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dieser hoffnungslosen Todesklagen.", "tokens": ["Und", "die\u00b7ser", "hoff\u00b7nungs\u00b7lo\u00b7sen", "To\u00b7des\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einsame wandeln still im Sternensaal.", "tokens": ["Ein\u00b7sa\u00b7me", "wan\u00b7deln", "still", "im", "Ster\u00b7nen\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}