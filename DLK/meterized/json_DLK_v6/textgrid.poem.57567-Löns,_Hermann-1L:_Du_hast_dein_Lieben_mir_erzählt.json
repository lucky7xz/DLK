{"textgrid.poem.57567": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast dein Lieben mir erz\u00e4hlt", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast dein Lieben mir erz\u00e4hlt", "tokens": ["Du", "hast", "dein", "Lie\u00b7ben", "mir", "er\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsrer letzten Nacht \u2013", "tokens": ["In", "uns\u00b7rer", "letz\u00b7ten", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun h\u00f6re, wie mir das Herz berauscht", "tokens": ["Nun", "h\u00f6\u00b7re", ",", "wie", "mir", "das", "Herz", "be\u00b7rauscht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wer es krank gemacht:", "tokens": ["Und", "wer", "es", "krank", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbder erste war ein Handwerksmann", "tokens": ["\u00bb", "der", "ers\u00b7te", "war", "ein", "Hand\u00b7werks\u00b7mann"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Wort und H\u00e4nden rauh,", "tokens": ["An", "Wort", "und", "H\u00e4n\u00b7den", "rauh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er bat mit plumpen Worten mich", "tokens": ["Er", "bat", "mit", "plum\u00b7pen", "Wor\u00b7ten", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu werden seine Frau.", "tokens": ["Zu", "wer\u00b7den", "sei\u00b7ne", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich wollt' ihn nicht \u2013 ein andrer kam", "tokens": ["Ich", "wollt'", "ihn", "nicht", "\u2013", "ein", "an\u00b7drer", "kam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$(", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit H\u00e4nden wei\u00df und sch\u00f6n.", "tokens": ["Mit", "H\u00e4n\u00b7den", "wei\u00df", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit s\u00fc\u00dfem Wort \u2013 mein junges Herz", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Wort", "\u2013", "mein", "jun\u00b7ges", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Konnt' ihm nicht widerstehn.", "tokens": ["Konnt'", "ihm", "nicht", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sechs hei\u00dfe Monate und dann \u2013", "tokens": ["Sechs", "hei\u00b7\u00dfe", "Mo\u00b7na\u00b7te", "und", "dann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "KON", "ADV", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Dann ward er meiner m\u00fcd' ...", "tokens": ["Dann", "ward", "er", "mei\u00b7ner", "m\u00fcd'", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Vater warf mich aus dem Haus \u2013", "tokens": ["Mein", "Va\u00b7ter", "warf", "mich", "aus", "dem", "Haus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist das alte Lied.", "tokens": ["Es", "ist", "das", "al\u00b7te", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u2013 Rauh war sein Wort und rauh seine Hand,", "tokens": ["\u2013", "Rauh", "war", "sein", "Wort", "und", "rauh", "sei\u00b7ne", "Hand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Treu hat er es gemeint;", "tokens": ["Treu", "hat", "er", "es", "ge\u00b7meint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als er mein trauriges Los erfuhr,", "tokens": ["Als", "er", "mein", "trau\u00b7ri\u00b7ges", "Los", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Hat er um mich geweint. \u2013", "tokens": ["Hat", "er", "um", "mich", "ge\u00b7weint", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun la\u00df die Falten von der Stirn", "tokens": ["Nun", "la\u00df", "die", "Fal\u00b7ten", "von", "der", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und komm in meinen Arm,", "tokens": ["Und", "komm", "in", "mei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit dein kaltes Herz nicht friert \u2013", "tokens": ["Da\u00b7mit", "dein", "kal\u00b7tes", "Herz", "nicht", "friert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Bett ist weich und warm.\u00ab", "tokens": ["Mein", "Bett", "ist", "weich", "und", "warm", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}