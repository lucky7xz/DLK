{"dta.poem.12674": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einem landsmann.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm, bruder! la\u00df uns dahin gehn,", "tokens": ["Komm", ",", "bru\u00b7der", "!", "la\u00df", "uns", "da\u00b7hin", "gehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVIMP", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo hundert tausend mann im freyen felde stehn,", "tokens": ["Wo", "hun\u00b7dert", "tau\u00b7send", "mann", "im", "frey\u00b7en", "fel\u00b7de", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "CARD", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir wollen uns dazu verdingen,", "tokens": ["Wir", "wol\u00b7len", "uns", "da\u00b7zu", "ver\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir die krancken wiederum zu rechte wollen bringen.", "tokens": ["Da\u00df", "wir", "die", "kran\u00b7cken", "wie\u00b7de\u00b7rum", "zu", "rech\u00b7te", "wol\u00b7len", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Wir taugen ohnedem in keine gro\u00dfe stadt,", "tokens": ["Wir", "tau\u00b7gen", "oh\u00b7ne\u00b7dem", "in", "kei\u00b7ne", "gro\u00b7\u00dfe", "stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil keiner sich von uns dar auf geleget hat,", "tokens": ["Weil", "kei\u00b7ner", "sich", "von", "uns", "dar", "auf", "ge\u00b7le\u00b7get", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "PPER", "PTKVZ", "APPR", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie man aus dem urin,", "tokens": ["Wie", "man", "aus", "dem", "u\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Man seh und riech und kost auch ihn,", "tokens": ["Man", "seh", "und", "riech", "und", "kost", "auch", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den leuten vor soll l\u00fcgen,", "tokens": ["Den", "leu\u00b7ten", "vor", "soll", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wer wei\u00df, was wir noch da vor beut und ehre kriegen?", "tokens": ["Wer", "wei\u00df", ",", "was", "wir", "noch", "da", "vor", "beut", "und", "eh\u00b7re", "krie\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "PPER", "ADV", "ADV", "APPR", "ADJD", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}