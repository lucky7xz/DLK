{"dta.poem.8685": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Wurmlinger Kapelle.   In W\u00fcrtemberg bei T\u00fcbingen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Luftig, wie ein leichter Kahn,", "tokens": ["Luf\u00b7tig", ",", "wie", "ein", "leich\u00b7ter", "Kahn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf des H\u00fcgels gr\u00fcner Welle,", "tokens": ["Auf", "des", "H\u00fc\u00b7gels", "gr\u00fc\u00b7ner", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwebt sie l\u00e4chelnd himmelan,", "tokens": ["Schwebt", "sie", "l\u00e4\u00b7chelnd", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort die friedliche Kapelle.", "tokens": ["Dort", "die", "fried\u00b7li\u00b7che", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst bei Sonnenuntergang", "tokens": ["Einst", "bei", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schritt ich durch die \u00f6den R\u00e4ume,", "tokens": ["Schritt", "ich", "durch", "die", "\u00f6\u00b7den", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Priesterwort und Festgesang", "tokens": ["Pries\u00b7ter\u00b7wort", "und", "Fest\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00e4uselten um mich wie Tr\u00e4ume.", "tokens": ["S\u00e4u\u00b7sel\u00b7ten", "um", "mich", "wie", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KOKOM", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und Maria's sch\u00f6nes Bild", "tokens": ["Und", "Ma\u00b7ria's", "sch\u00f6\u00b7nes", "Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schien vom Altar sich zu senken,", "tokens": ["Schien", "vom", "Al\u00b7tar", "sich", "zu", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schien in Trauer, heilig mild,", "tokens": ["Schien", "in", "Trau\u00b7er", ",", "hei\u00b7lig", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alter Tage zu gedenken.", "tokens": ["Al\u00b7ter", "Ta\u00b7ge", "zu", "ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "R\u00f6thlich kommt der Morgenschein,", "tokens": ["R\u00f6th\u00b7lich", "kommt", "der", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es kehrt der Abendschimmer", "tokens": ["Und", "es", "kehrt", "der", "A\u00b7bend\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treulich bei dem Bilde ein;", "tokens": ["Treu\u00b7lich", "bei", "dem", "Bil\u00b7de", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch die Menschen kommen nimmer.", "tokens": ["Doch", "die", "Men\u00b7schen", "kom\u00b7men", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Leise werd' ich hier umweht", "tokens": ["Lei\u00b7se", "werd'", "ich", "hier", "um\u00b7weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von geheimen, frohen Schauern,", "tokens": ["Von", "ge\u00b7hei\u00b7men", ",", "fro\u00b7hen", "Schau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich als h\u00e4tt' ein fromm Gebet", "tokens": ["Gleich", "als", "h\u00e4tt'", "ein", "fromm", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich versp\u00e4tet in den Mauern.", "tokens": ["Sich", "ver\u00b7sp\u00e4\u00b7tet", "in", "den", "Mau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Scheidend gr\u00fc\u00dfet hell und klar", "tokens": ["Schei\u00b7dend", "gr\u00fc\u00b7\u00dfet", "hell", "und", "klar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch die Sonn' in die Kapelle,", "tokens": ["Noch", "die", "Sonn'", "in", "die", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Gr\u00e4ber stille Schaar", "tokens": ["Und", "der", "Gr\u00e4\u00b7ber", "stil\u00b7le", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt so traulich vor der Schwelle.", "tokens": ["Liegt", "so", "trau\u00b7lich", "vor", "der", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Freundlich schmiegt des Herbstes Ruh", "tokens": ["Freund\u00b7lich", "schmiegt", "des", "Herbs\u00b7tes", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich an die verlassnen Gr\u00fcfte;", "tokens": ["Sich", "an", "die", "ver\u00b7lass\u00b7nen", "Gr\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort, dem fernen S\u00fcden zu,", "tokens": ["Dort", ",", "dem", "fer\u00b7nen", "S\u00fc\u00b7den", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandern V\u00f6gel durch die L\u00fcfte.", "tokens": ["Wan\u00b7dern", "V\u00f6\u00b7gel", "durch", "die", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Alles schlummert, Alles schweigt,", "tokens": ["Al\u00b7les", "schlum\u00b7mert", ",", "Al\u00b7les", "schweigt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancher H\u00fcgel ist versunken,", "tokens": ["Man\u00b7cher", "H\u00fc\u00b7gel", "ist", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Kreuze stehn geneigt", "tokens": ["Und", "die", "Kreu\u00b7ze", "stehn", "ge\u00b7neigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Gr\u00e4bern \u2014 schlafestrunken.", "tokens": ["Auf", "den", "Gr\u00e4\u00b7bern", "schla\u00b7fe\u00b7strun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Und der Baum im Abendwind", "tokens": ["Und", "der", "Baum", "im", "Ab\u00b7end\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "L\u00e4\u00dft sein Laub zu Boden wallen,", "tokens": ["L\u00e4\u00dft", "sein", "Laub", "zu", "Bo\u00b7den", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein schlafergriffnes Kind", "tokens": ["Wie", "ein", "schla\u00b7fer\u00b7griff\u00b7nes", "Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft sein buntes Spielzeug fallen. \u2014", "tokens": ["L\u00e4\u00dft", "sein", "bun\u00b7tes", "Spiel\u00b7zeug", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Hier ist all' mein Erdenleid", "tokens": ["Hier", "ist", "all'", "mein", "Er\u00b7den\u00b7leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie ein tr\u00fcber Duft zerflossen;", "tokens": ["Wie", "ein", "tr\u00fc\u00b7ber", "Duft", "zer\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfe Todesm\u00fcdigkeit", "tokens": ["S\u00fc\u00b7\u00dfe", "To\u00b7des\u00b7m\u00fc\u00b7dig\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt die Seele hier umschlossen.", "tokens": ["H\u00e4lt", "die", "See\u00b7le", "hier", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}