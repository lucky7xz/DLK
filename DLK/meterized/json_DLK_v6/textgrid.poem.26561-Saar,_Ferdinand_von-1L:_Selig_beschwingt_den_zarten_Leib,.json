{"textgrid.poem.26561": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Selig beschwingt den zarten Leib,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selig beschwingt den zarten Leib,", "tokens": ["Se\u00b7lig", "be\u00b7schwingt", "den", "zar\u00b7ten", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Leis' sich aufbl\u00e4tternde Rosen im Haar,", "tokens": ["Leis'", "sich", "auf\u00b7bl\u00e4t\u00b7tern\u00b7de", "Ro\u00b7sen", "im", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Schwebst du im Reigen dahin,", "tokens": ["Schwebst", "du", "im", "Rei\u00b7gen", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PAV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Der den funkelnden Saal durchwogt.", "tokens": ["Der", "den", "fun\u00b7keln\u00b7den", "Saal", "durch\u00b7wogt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Und wie du mit halbgeschlossenen Lidern,", "tokens": ["Und", "wie", "du", "mit", "halb\u00b7ge\u00b7schlos\u00b7se\u00b7nen", "Li\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Weltentr\u00fcckt,", "tokens": ["Wel\u00b7ten\u00b7tr\u00fcckt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Das sch\u00f6ne Haupt zur Schulter des T\u00e4nzers neigst:", "tokens": ["Das", "sch\u00f6\u00b7ne", "Haupt", "zur", "Schul\u00b7ter", "des", "T\u00e4n\u00b7zers", "neigst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ist es mir pl\u00f6tzlich,", "tokens": ["Ist", "es", "mir", "pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Als s\u00e4h' ich bleich deines Vaters Antlitz,", "tokens": ["Als", "s\u00e4h'", "ich", "bleich", "dei\u00b7nes", "Va\u00b7ters", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Das dem deinen glich,", "tokens": ["Das", "dem", "dei\u00b7nen", "glich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Beim Lampenlicht hinabgebeugt", "tokens": ["Beim", "Lam\u00b7pen\u00b7licht", "hin\u00b7ab\u00b7ge\u00b7beugt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zum fordernden Bogen,", "tokens": ["Zum", "for\u00b7dern\u00b7den", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Den er f\u00fcllte in schwerem Gedankenkampf.", "tokens": ["Den", "er", "f\u00fcll\u00b7te", "in", "schwe\u00b7rem", "Ge\u00b7dan\u00b7ken\u00b7kampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Todt schrieb sich der Arme,", "tokens": ["Todt", "schrieb", "sich", "der", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.15": {"text": "Ein \u00e4chter Germane,", "tokens": ["Ein", "\u00e4ch\u00b7ter", "Ger\u00b7ma\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Um lumpigen Bettlersold,", "tokens": ["Um", "lum\u00b7pi\u00b7gen", "Bett\u00b7ler\u00b7sold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Den Verleger und Zeitung", "tokens": ["Den", "Ver\u00b7le\u00b7ger", "und", "Zei\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.18": {"text": "Achselzuckend bewilligt.", "tokens": ["Ach\u00b7sel\u00b7zu\u00b7ckend", "be\u00b7wil\u00b7ligt", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Denn von den \u00bbLeichten\u00ab war er nicht Einer:", "tokens": ["Denn", "von", "den", "\u00bb", "Leich\u00b7ten", "\u00ab", "war", "er", "nicht", "Ei\u00b7ner", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "$(", "NN", "$(", "VAFIN", "PPER", "PTKNEG", "PIS", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Tiefen wollt' er erhellen mit qualmender Fackel", "tokens": ["Tie\u00b7fen", "wollt'", "er", "er\u00b7hel\u00b7len", "mit", "qual\u00b7men\u00b7der", "Fa\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Und \u2013 wieder als \u00e4chter Germane \u2013", "tokens": ["Und", "\u2013", "wie\u00b7der", "als", "\u00e4ch\u00b7ter", "Ger\u00b7ma\u00b7ne", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "KOUS", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "Wu\u00dft' er zu sch\u00f6pfen,", "tokens": ["Wu\u00dft'", "er", "zu", "sch\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Doch nicht zu gestalten.", "tokens": ["Doch", "nicht", "zu", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Ballast hielt er f\u00fcr Fracht \u2013", "tokens": ["Bal\u00b7last", "hielt", "er", "f\u00fcr", "Fracht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "Und so sank, was er schrieb.", "tokens": ["Und", "so", "sank", ",", "was", "er", "schrieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Nun er todt, hat die Welt", "tokens": ["Nun", "er", "todt", ",", "hat", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "ADJD", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das L\u00e4cheln mitleidigen Spottes", "tokens": ["Das", "L\u00e4\u00b7cheln", "mit\u00b7lei\u00b7di\u00b7gen", "Spot\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "In Wohlwollen f\u00fcr ", "tokens": ["In", "Wohl\u00b7wol\u00b7len", "f\u00fcr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und nicht entgelten l\u00e4\u00dft sie die Tochter", "tokens": ["Und", "nicht", "ent\u00b7gel\u00b7ten", "l\u00e4\u00dft", "sie", "die", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVINF", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Des Vaters pedantische Schrullen.", "tokens": ["Des", "Va\u00b7ters", "pe\u00b7dan\u00b7ti\u00b7sche", "Schrul\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Sie preis't deine Anmuth,", "tokens": ["Sie", "preis't", "dei\u00b7ne", "An\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Deinen witzigen Geist,", "tokens": ["Dei\u00b7nen", "wit\u00b7zi\u00b7gen", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Zieht dich zu ihren Festen", "tokens": ["Zieht", "dich", "zu", "ih\u00b7ren", "Fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und schm\u00fcckt dich mit Rosen und hellen Gew\u00e4ndern.", "tokens": ["Und", "schm\u00fcckt", "dich", "mit", "Ro\u00b7sen", "und", "hel\u00b7len", "Ge\u00b7w\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "D'rum schwebe nur hin, mein Kind,", "tokens": ["D'\u00b7rum", "schwe\u00b7be", "nur", "hin", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Bei den rauschenden Kl\u00e4ngen!", "tokens": ["Bei", "den", "rau\u00b7schen\u00b7den", "Kl\u00e4n\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Was k\u00f6nnen Lebendige", "tokens": ["Was", "k\u00f6n\u00b7nen", "Le\u00b7ben\u00b7di\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Auch Anderes thun,", "tokens": ["Auch", "An\u00b7de\u00b7res", "thun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Als auf Gr\u00e4bern tanzen ...", "tokens": ["Als", "auf", "Gr\u00e4\u00b7bern", "tan\u00b7zen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}