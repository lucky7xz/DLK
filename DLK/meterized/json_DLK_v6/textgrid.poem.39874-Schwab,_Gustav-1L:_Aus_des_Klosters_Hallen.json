{"textgrid.poem.39874": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus des Klosters Hallen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus des Klosters Hallen", "tokens": ["Aus", "des", "Klos\u00b7ters", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schallt der Jungfrau'n Sang,", "tokens": ["Schallt", "der", "Jung\u00b7frau'n", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die zur Kirche wallen", "tokens": ["Die", "zur", "Kir\u00b7che", "wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bei der Glocken Klang;", "tokens": ["Bei", "der", "Glo\u00b7cken", "Klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Alle Gott geweiht,", "tokens": ["Al\u00b7le", "Gott", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Haben sie der Zeit", "tokens": ["Ha\u00b7ben", "sie", "der", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Abgesagt und ihrer Wonne,", "tokens": ["Ab\u00b7ge\u00b7sagt", "und", "ih\u00b7rer", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kehren sich zur ew'gen Sonne.", "tokens": ["Keh\u00b7ren", "sich", "zur", "ew'\u00b7gen", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was an ihnen bl\u00fchet,", "tokens": ["Was", "an", "ih\u00b7nen", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bl\u00fcht zu seinem Ruhm,", "tokens": ["Bl\u00fcht", "zu", "sei\u00b7nem", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was in ihnen gl\u00fchet,", "tokens": ["Was", "in", "ih\u00b7nen", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist sein Heiligtum.", "tokens": ["Ist", "sein", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ihrer Jugend Stern", "tokens": ["Ih\u00b7rer", "Ju\u00b7gend", "Stern"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Leuchtet vor dem Herrn,", "tokens": ["Leuch\u00b7tet", "vor", "dem", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Was ein Weib auf Erden schm\u00fccket,", "tokens": ["Was", "ein", "Weib", "auf", "Er\u00b7den", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Opfern sie, der Welt entr\u00fccket.", "tokens": ["Op\u00b7fern", "sie", ",", "der", "Welt", "ent\u00b7r\u00fc\u00b7cket", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hoher Stirne Bogen,", "tokens": ["Ho\u00b7her", "Stir\u00b7ne", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Langes goldnes Haar,", "tokens": ["Lan\u00b7ges", "gold\u00b7nes", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jungen Busens Wogen", "tokens": ["Jun\u00b7gen", "Bu\u00b7sens", "Wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bringen sie ihm dar;", "tokens": ["Brin\u00b7gen", "sie", "ihm", "dar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Farb'ger Wangen Blut,", "tokens": ["Fa\u00b7rb'\u00b7ger", "Wan\u00b7gen", "Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Roter Lippen Glut,", "tokens": ["Ro\u00b7ter", "Lip\u00b7pen", "Glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Was da freut und treibt das Leben,", "tokens": ["Was", "da", "freut", "und", "treibt", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Haben sie ihm hingegeben.", "tokens": ["Ha\u00b7ben", "sie", "ihm", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch, die J\u00fcngste schauet", "tokens": ["Doch", ",", "die", "J\u00fcngs\u00b7te", "schau\u00b7et"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dort am letzten Platz,", "tokens": ["Dort", "am", "letz\u00b7ten", "Platz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die erst heut vertrauet", "tokens": ["Die", "erst", "heut", "ver\u00b7trau\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihm den reichen Schatz!", "tokens": ["Ihm", "den", "rei\u00b7chen", "Schatz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Welcher Brauen Kranz!", "tokens": ["Wel\u00b7cher", "Brau\u00b7en", "Kranz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Welcher Augen Glanz!", "tokens": ["Wel\u00b7cher", "Au\u00b7gen", "Glanz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Welchen Stral von Sehnsuchtsblicken", "tokens": ["Wel\u00b7chen", "Stral", "von", "Sehn\u00b7suchts\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht man sie gen Himmel schicken!", "tokens": ["Sieht", "man", "sie", "gen", "Him\u00b7mel", "schi\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Rosse h\u00f6rt man scharren", "tokens": ["Ros\u00b7se", "h\u00f6rt", "man", "schar\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vor dem Klosterthor,", "tokens": ["Vor", "dem", "Klos\u00b7ter\u00b7thor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Einen J\u00fcngling harren", "tokens": ["Ei\u00b7nen", "J\u00fcng\u00b7ling", "har\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Siehet man davor:", "tokens": ["Sie\u00b7het", "man", "da\u00b7vor", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sein entz\u00fcndet Hirn", "tokens": ["Sein", "ent\u00b7z\u00fcn\u00b7det", "Hirn"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "VVPP", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "F\u00e4rbet Aug' und Stirn", "tokens": ["F\u00e4r\u00b7bet", "Aug'", "und", "Stirn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mit der ird'schen Flamme Gluten", "tokens": ["Mit", "der", "ird'\u00b7schen", "Flam\u00b7me", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die aus dunkler Tiefe fluten.", "tokens": ["Die", "aus", "dunk\u00b7ler", "Tie\u00b7fe", "flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "An des Thores Gitter", "tokens": ["An", "des", "Tho\u00b7res", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fr\u00e4gt die Schaffnerin:", "tokens": ["Fr\u00e4gt", "die", "Schaff\u00b7ne\u00b7rin", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was begehrt der Ritter", "tokens": ["Was", "be\u00b7gehrt", "der", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Im emp\u00f6rten Sinn? \u2013", "tokens": ["Im", "em\u00b7p\u00f6r\u00b7ten", "Sinn", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "\u00bbaus dem schwarzen Haus", "tokens": ["\u00bb", "aus", "dem", "schwar\u00b7zen", "Haus"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Sendet sie heraus!", "tokens": ["Sen\u00b7det", "sie", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Drinnen gl\u00fchn zwei Sonnenaugen,", "tokens": ["Drin\u00b7nen", "gl\u00fchn", "zwei", "Son\u00b7nen\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die f\u00fcr eure Nacht nicht taugen!\u00ab", "tokens": ["Die", "f\u00fcr", "eu\u00b7re", "Nacht", "nicht", "tau\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Seine Waffen t\u00f6nen", "tokens": ["Sei\u00b7ne", "Waf\u00b7fen", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Durch der Hallen Gang,", "tokens": ["Durch", "der", "Hal\u00b7len", "Gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df man's h\u00f6ret dr\u00f6hnen", "tokens": ["Da\u00df", "man's", "h\u00f6\u00b7ret", "dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zu der Jungfrau'n Sang.", "tokens": ["Zu", "der", "Jung\u00b7frau'n", "Sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Alle beten laut,", "tokens": ["Al\u00b7le", "be\u00b7ten", "laut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Doch die fromme Braut,", "tokens": ["Doch", "die", "from\u00b7me", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wie sie h\u00f6rt die frechen Worte,", "tokens": ["Wie", "sie", "h\u00f6rt", "die", "fre\u00b7chen", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wandelt schweigend durch die Pforte.", "tokens": ["Wan\u00b7delt", "schwei\u00b7gend", "durch", "die", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "In der stillen Zelle", "tokens": ["In", "der", "stil\u00b7len", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Durch das Fensterlein,", "tokens": ["Durch", "das", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nach des Himmels Helle,", "tokens": ["Nach", "des", "Him\u00b7mels", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nach der Sonne Schein", "tokens": ["Nach", "der", "Son\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Kehrt sie noch einmal", "tokens": ["Kehrt", "sie", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ihrer Augen Stral,", "tokens": ["Ih\u00b7rer", "Au\u00b7gen", "Stral", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "L\u00f6set mit dem Stahl sich leise", "tokens": ["L\u00f6\u00b7set", "mit", "dem", "Stahl", "sich", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "ADJD"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Dann der Augen goldne Kreise.", "tokens": ["Dann", "der", "Au\u00b7gen", "gold\u00b7ne", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Schlie\u00dft die Perlen beide", "tokens": ["Schlie\u00dft", "die", "Per\u00b7len", "bei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Von erloschnem Schein,", "tokens": ["Von", "er\u00b7lo\u00b7schnem", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Blutiges Geschmeide,", "tokens": ["Blu\u00b7ti\u00b7ges", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In die Kapsel ein,", "tokens": ["In", "die", "Kap\u00b7sel", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Zieht den Schleier vor,", "tokens": ["Zieht", "den", "Schlei\u00b7er", "vor", ","