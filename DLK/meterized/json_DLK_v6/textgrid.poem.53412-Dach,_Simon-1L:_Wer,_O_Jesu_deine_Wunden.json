{"textgrid.poem.53412": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer, O Jesu deine Wunden", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer, O Jesu deine Wunden", "tokens": ["Wer", ",", "O", "Je\u00b7su", "dei\u00b7ne", "Wun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "$,", "NE", "NE", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stets f\u00fcr seine Ruhstatt helt,", "tokens": ["Stets", "f\u00fcr", "sei\u00b7ne", "Ruh\u00b7statt", "helt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat den gr\u00f6sten Schatz gefunden,", "tokens": ["Hat", "den", "gr\u00f6s\u00b7ten", "Schatz", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er verachtet diese Welt,", "tokens": ["Er", "ver\u00b7ach\u00b7tet", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihm ist sterben eine Lust,", "tokens": ["Ihm", "ist", "ster\u00b7ben", "ei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil ihm Himmels-Freud bewust.", "tokens": ["Weil", "ihm", "Him\u00b7mels\u00b7Freud", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht des Satans w\u00fcstes Schrecken", "tokens": ["Nicht", "des", "Sa\u00b7tans", "w\u00fcs\u00b7tes", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch die gro\u00dfe St\u00e4rck und List", "tokens": ["Noch", "die", "gro\u00b7\u00dfe", "St\u00e4rck", "und", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan ihm eine Furcht erwecken,", "tokens": ["Kan", "ihm", "ei\u00b7ne", "Furcht", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob sie noch so grausam ist.", "tokens": ["Ob", "sie", "noch", "so", "grau\u00b7sam", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Christus Leiden ist sein Schutz,", "tokens": ["Chris\u00b7tus", "Lei\u00b7den", "ist", "sein", "Schutz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bietet allen Feinden Trutz.", "tokens": ["Bie\u00b7tet", "al\u00b7len", "Fein\u00b7den", "Trutz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht des frechen Todes Drewen", "tokens": ["Nicht", "des", "fre\u00b7chen", "To\u00b7des", "Dre\u00b7wen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kan ihn bringen in Gefahr,", "tokens": ["Kan", "ihn", "brin\u00b7gen", "in", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er darf seinen Grimm nicht schewen,", "tokens": ["Er", "darf", "sei\u00b7nen", "Grimm", "nicht", "sche\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Darf getrost seyn immerdar,", "tokens": ["Darf", "ge\u00b7trost", "seyn", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "VAINF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wa\u00df den b\u00f6sen Furcht einjagt,", "tokens": ["Wa\u00df", "den", "b\u00f6\u00b7sen", "Furcht", "ein\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Di\u00df erwart' er unverzagt.", "tokens": ["Di\u00df", "er\u00b7wart'", "er", "un\u00b7ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nimmer kombt ihm aus dem Hertzen", "tokens": ["Nim\u00b7mer", "kombt", "ihm", "aus", "dem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein Erl\u00f6ser, sein Gesicht", "tokens": ["Sein", "Er\u00b7l\u00f6\u00b7ser", ",", "sein", "Ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist auf seine schwere Schmertzen", "tokens": ["Ist", "auf", "sei\u00b7ne", "schwe\u00b7re", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das bittre Kreutz gericht,", "tokens": ["Und", "das", "bitt\u00b7re", "Kreutz", "ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jesu Wunden und sein Blut", "tokens": ["Je\u00b7su", "Wun\u00b7den", "und", "sein", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht ihm einen Helden Muth.", "tokens": ["Macht", "ihm", "ei\u00b7nen", "Hel\u00b7den", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hirin wil ich ewig bleiben,", "tokens": ["Hi\u00b7rin", "wil", "ich", "e\u00b7wig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht Er: es sol keine Noth", "tokens": ["Spricht", "Er", ":", "es", "sol", "kei\u00b7ne", "Noth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VMFIN", "PIAT", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Mich aus dieser Wohnung treiben,", "tokens": ["Mich", "aus", "die\u00b7ser", "Woh\u00b7nung", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hie kan nichts der bla\u00dfe Todt,", "tokens": ["Hie", "kan", "nichts", "der", "bla\u00b7\u00dfe", "Todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hie ist keine Sorg und Qual,", "tokens": ["Hie", "ist", "kei\u00b7ne", "Sorg", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sondern Wollust ohne Zahl.", "tokens": ["Son\u00b7dern", "Wol\u00b7lust", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Christe, la\u00df auch deine Wunden", "tokens": ["Chris\u00b7te", ",", "la\u00df", "auch", "dei\u00b7ne", "Wun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir Trost, H\u00fclff und Rettung seyn", "tokens": ["Mir", "Trost", ",", "H\u00fclff", "und", "Ret\u00b7tung", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "VAINF"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "In den letzten Todes-Stunden", "tokens": ["In", "den", "letz\u00b7ten", "To\u00b7des\u00b7Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder allen Schmertz und Pein,", "tokens": ["Wie\u00b7der", "al\u00b7len", "Schmertz", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer dein tewres Blut aufffast,", "tokens": ["Wer", "dein", "tew\u00b7res", "Blut", "auff\u00b7fast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem ist Sterben keine Last.", "tokens": ["Dem", "ist", "Ster\u00b7ben", "kei\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}