{"textgrid.poem.32438": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "117. Der Furchtsame", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum seh' ich den Donner die Himmel umziehen,", "tokens": ["Kaum", "seh'", "ich", "den", "Don\u00b7ner", "die", "Him\u00b7mel", "um\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So flieh' ich zum Keller hinein.", "tokens": ["So", "flieh'", "ich", "zum", "Kel\u00b7ler", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Was meint ihr? ich suchte den Donner zu fliehen?", "tokens": ["Was", "meint", "ihr", "?", "ich", "such\u00b7te", "den", "Don\u00b7ner", "zu", "flie\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr irrt euch; ich suche den Wein.", "tokens": ["Ihr", "irrt", "euch", ";", "ich", "su\u00b7che", "den", "Wein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}