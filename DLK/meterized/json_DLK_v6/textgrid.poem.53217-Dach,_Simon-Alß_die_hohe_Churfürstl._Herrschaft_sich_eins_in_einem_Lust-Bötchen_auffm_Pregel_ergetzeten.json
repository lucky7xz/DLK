{"textgrid.poem.53217": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Al\u00df die hohe Churf\u00fcrstl. Herrschaft sich eins in einem Lust-B\u00f6tchen auffm Pregel ergetzeten", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Edler Pregel, dessen Flu\u00df", "tokens": ["Ed\u00b7ler", "Pre\u00b7gel", ",", "des\u00b7sen", "Flu\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie sich seeliger kan sch\u00e4tzen,", "tokens": ["Nie", "sich", "see\u00b7li\u00b7ger", "kan", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auff, entbeut jetzt deinen Gru\u00df", "tokens": ["Auff", ",", "ent\u00b7beut", "jetzt", "dei\u00b7nen", "Gru\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesem thewren Welt-Ergetzen,", "tokens": ["Die\u00b7sem", "thew\u00b7ren", "Welt\u00b7Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den Prince\u00dfinnen, so heut", "tokens": ["Den", "Prin\u00b7ce\u00b7\u00dfin\u00b7nen", ",", "so", "heut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Dich und alles Feld erfrewt.", "tokens": ["Dich", "und", "al\u00b7les", "Feld", "er\u00b7frewt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Richt anjetzt dein H\u00e4upt empor,", "tokens": ["Richt", "an\u00b7jetzt", "dein", "H\u00e4upt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00fcste dich mit deinen Fl\u00fcssen", "tokens": ["Br\u00fcs\u00b7te", "dich", "mit", "dei\u00b7nen", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pr\u00e4chtiger als nie, hervor;", "tokens": ["Pr\u00e4ch\u00b7ti\u00b7ger", "als", "nie", ",", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Werde stoltz dich zu ergiessen!", "tokens": ["Wer\u00b7de", "stoltz", "dich", "zu", "er\u00b7gies\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was dich rinnen l\u00e4sst in Ruh", "tokens": ["Was", "dich", "rin\u00b7nen", "l\u00e4sst", "in", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Spricht dir selbst in Gnaden zu.", "tokens": ["Spricht", "dir", "selbst", "in", "Gna\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lacht, ihr Wiesen, lachet jetzt", "tokens": ["Lacht", ",", "ihr", "Wie\u00b7sen", ",", "la\u00b7chet", "jetzt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewrer Herrschaft lieb-zukosen,", "tokens": ["Ew\u00b7rer", "Herr\u00b7schaft", "lie\u00b7bzu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehet, da\u00df ihr reichlich schwitzt,", "tokens": ["Se\u00b7het", ",", "da\u00df", "ihr", "reich\u00b7lich", "schwitzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nelcken, Lilien und Rosen,", "tokens": ["Nel\u00b7cken", ",", "Li\u00b7li\u00b7en", "und", "Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schm\u00fcckt mit Blumen mancherley", "tokens": ["Schm\u00fcckt", "mit", "Blu\u00b7men", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ewre Feld-Tapecerey.", "tokens": ["Ew\u00b7re", "Feld\u00b7Ta\u00b7pe\u00b7ce\u00b7rey", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber Ihr, O Pracht der Welt,", "tokens": ["A\u00b7ber", "Ihr", ",", "O", "Pracht", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NE", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr, des Himmels Wollgefallen,", "tokens": ["Ihr", ",", "des", "Him\u00b7mels", "Woll\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mercket, wie die\u00df bunte Feld", "tokens": ["Mer\u00b7cket", ",", "wie", "die\u00df", "bun\u00b7te", "Feld"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PDS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Pranget Euch zu gut f\u00fcr allen,", "tokens": ["Pran\u00b7get", "Euch", "zu", "gut", "f\u00fcr", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der Pregel sich ergeusst,", "tokens": ["Wie", "der", "Pre\u00b7gel", "sich", "er\u00b7geusst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und Euch als willkommen heist!", "tokens": ["Und", "Euch", "als", "will\u00b7kom\u00b7men", "heist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was hie ewrer Gottheit Licht", "tokens": ["Was", "hie", "ew\u00b7rer", "Got\u00b7theit", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin und wieder kan bestrahlen,", "tokens": ["Hin", "und", "wie\u00b7der", "kan", "be\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt, die Sonne k\u00f6nne nicht", "tokens": ["Sagt", ",", "die", "Son\u00b7ne", "k\u00f6n\u00b7ne", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "VMFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiesen, Feld und Awen mahlen", "tokens": ["Wie\u00b7sen", ",", "Feld", "und", "A\u00b7wen", "mah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit so einer edlen Zier,", "tokens": ["Mit", "so", "ei\u00b7ner", "ed\u00b7len", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "O Princessinnen, als Ihr.", "tokens": ["O", "Prin\u00b7ces\u00b7sin\u00b7nen", ",", "als", "Ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}