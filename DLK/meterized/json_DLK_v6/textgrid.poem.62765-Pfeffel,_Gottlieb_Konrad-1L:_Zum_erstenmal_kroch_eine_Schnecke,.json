{"textgrid.poem.62765": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum erstenmal kroch eine Schnecke,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum erstenmal kroch eine Schnecke,", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "kroch", "ei\u00b7ne", "Schne\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das sch\u00f6nste Kunstst\u00fcck der Natur,", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Kunst\u00b7st\u00fcck", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus der verborgnen Fliederhecke,", "tokens": ["Aus", "der", "ver\u00b7borg\u00b7nen", "Flie\u00b7der\u00b7he\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sie gebahr auf Tempes Flur.", "tokens": ["Die", "sie", "ge\u00b7bahr", "auf", "Tem\u00b7pes", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier sa\u00df auf weichen Lotusbl\u00e4ttern", "tokens": ["Hier", "sa\u00df", "auf", "wei\u00b7chen", "Lo\u00b7tus\u00b7bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Ph\u00f6nix ihrer jungen Vettern.", "tokens": ["Der", "Ph\u00f6\u00b7nix", "ih\u00b7rer", "jun\u00b7gen", "Vet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie stutzt, sie gafft ihn staunend an", "tokens": ["Sie", "stutzt", ",", "sie", "gafft", "ihn", "stau\u00b7nend", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und nickt ihm Dank, als er sie gr\u00fc\u00dfet,", "tokens": ["Und", "nickt", "ihm", "Dank", ",", "als", "er", "sie", "gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch der versuchtere Galan", "tokens": ["Doch", "der", "ver\u00b7such\u00b7te\u00b7re", "Ga\u00b7lan"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "R\u00fcckt n\u00e4her, k\u00f6mmt und sieht und k\u00fcsset.", "tokens": ["R\u00fcckt", "n\u00e4\u00b7her", ",", "k\u00f6mmt", "und", "sieht", "und", "k\u00fcs\u00b7set", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Das B\u00e4schen schaudert und verschlie\u00dfet", "tokens": ["Das", "B\u00e4\u00b7schen", "schau\u00b7dert", "und", "ver\u00b7schlie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sich schnell in ihr verschanztes Haus.", "tokens": ["Sich", "schnell", "in", "ihr", "ver\u00b7schanz\u00b7tes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Allein itzt schien es ihr zu enge,", "tokens": ["Al\u00b7lein", "itzt", "schien", "es", "ihr", "zu", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Es war als z\u00f6gen hundert Str\u00e4nge", "tokens": ["Es", "war", "als", "z\u00f6\u00b7gen", "hun\u00b7dert", "Str\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sie aus der finstern Gruft heraus.", "tokens": ["Sie", "aus", "der", "fins\u00b7tern", "Gruft", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Kaum schl\u00fcpft sie aus der bunten Schale,", "tokens": ["Kaum", "schl\u00fcpft", "sie", "aus", "der", "bun\u00b7ten", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So k\u00fc\u00dft er sie zum andernmale.", "tokens": ["So", "k\u00fc\u00dft", "er", "sie", "zum", "an\u00b7dern\u00b7ma\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sie str\u00e4ubt sich und mit scheuem Blick", "tokens": ["Sie", "str\u00e4ubt", "sich", "und", "mit", "scheu\u00b7em", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Glitscht sie in ihr Castell zur\u00fcck;", "tokens": ["Glitscht", "sie", "in", "ihr", "Cas\u00b7tell", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Doch dasmal nur mit dem Gesichte.", "tokens": ["Doch", "das\u00b7mal", "nur", "mit", "dem", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ihr Busen winkt dem losen Wichte", "tokens": ["Ihr", "Bu\u00b7sen", "winkt", "dem", "lo\u00b7sen", "Wich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Noch k\u00fchner als zuvor zu seyn.", "tokens": ["Noch", "k\u00fch\u00b7ner", "als", "zu\u00b7vor", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Er wars. \u2013 Sie bi\u00df ihn doch? \u2013 Ach nein!", "tokens": ["Er", "wars", ".", "\u2013", "Sie", "bi\u00df", "ihn", "doch", "?", "\u2013", "Ach", "nein", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "$(", "PPER", "APPR", "PPER", "ADV", "$.", "$(", "NN", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Sie bebte nur durch alle Glieder,", "tokens": ["Sie", "beb\u00b7te", "nur", "durch", "al\u00b7le", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und sch\u00e4umte Zorn, doch blos zum Schein.", "tokens": ["Und", "sch\u00e4um\u00b7te", "Zorn", ",", "doch", "blos", "zum", "Schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Nach zwo Minuten kam sie wieder.", "tokens": ["Nach", "zwo", "Mi\u00b7nu\u00b7ten", "kam", "sie", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Zwar grollt noch ihr Gesicht; allein", "tokens": ["Zwar", "grollt", "noch", "ihr", "Ge\u00b7sicht", ";", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der Lecker k\u00fc\u00dfte seine Falten,", "tokens": ["Der", "Le\u00b7cker", "k\u00fc\u00df\u00b7te", "sei\u00b7ne", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und sie zog blos die Augen ein,", "tokens": ["Und", "sie", "zog", "blos", "die", "Au\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.30": {"text": "Die wir get\u00e4uscht f\u00fcr H\u00f6rner halten.", "tokens": ["Die", "wir", "ge\u00b7t\u00e4uscht", "f\u00fcr", "H\u00f6r\u00b7ner", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Bald aber zuckt sie gar nicht mehr,", "tokens": ["Bald", "a\u00b7ber", "zuckt", "sie", "gar", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und k\u00fcsset lieber noch als er.", "tokens": ["Und", "k\u00fcs\u00b7set", "lie\u00b7ber", "noch", "als", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "W\u00e4r ich ein Schalk, ich w\u00fcrde schw\u00f6ren", "tokens": ["W\u00e4r", "ich", "ein", "Schalk", ",", "ich", "w\u00fcr\u00b7de", "schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df junge M\u00e4dchen Schnecken w\u00e4ren.", "tokens": ["Da\u00df", "jun\u00b7ge", "M\u00e4d\u00b7chen", "Schne\u00b7cken", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}