{"textgrid.poem.25484": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gerad ist's, Freundin, heut ein Jahr,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerad ist's, Freundin, heut ein Jahr,", "tokens": ["Ge\u00b7rad", "ist's", ",", "Freun\u00b7din", ",", "heut", "ein", "Jahr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich in ", "tokens": ["Da\u00df", "ich", "in"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Frei vom gew\u00f6hnlichen Geleite", "tokens": ["Frei", "vom", "ge\u00b7w\u00f6hn\u00b7li\u00b7chen", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Des Grillenschwarms, so fr\u00f6hlich war!", "tokens": ["Des", "Gril\u00b7len\u00b7schwarms", ",", "so", "fr\u00f6h\u00b7lich", "war", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er blieb, wie dem Geschmei\u00df' es ziemet,", "tokens": ["Er", "blieb", ",", "wie", "dem", "Ge\u00b7schmei\u00df'", "es", "zie\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Furchtsam an deiner Th\u00fcr' zur\u00fcck,", "tokens": ["Furcht\u00b7sam", "an", "dei\u00b7ner", "Th\u00fcr'", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Von deren Schwelle Fama r\u00fchmet,", "tokens": ["Von", "de\u00b7ren", "Schwel\u00b7le", "Fa\u00b7ma", "r\u00fch\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df eine gute Fee, zum Gl\u00fcck'", "tokens": ["Da\u00df", "ei\u00b7ne", "gu\u00b7te", "Fee", ",", "zum", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "F\u00fcr dich und deine Freund', am Morgen", "tokens": ["F\u00fcr", "dich", "und", "dei\u00b7ne", "Freund'", ",", "am", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der dich gebahr, das Meisterst\u00fcck", "tokens": ["Der", "dich", "ge\u00b7bahr", ",", "das", "Meis\u00b7ter\u00b7st\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von ihrer Kunst darin verborgen:", "tokens": ["Von", "ih\u00b7rer", "Kunst", "da\u00b7rin", "ver\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den Talisman, der keine Sorgen,", "tokens": ["Den", "Ta\u00b7lis\u00b7man", ",", "der", "kei\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und hingen sie sich noch so fest", "tokens": ["Und", "hin\u00b7gen", "sie", "sich", "noch", "so", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "An ihren Mann, hin\u00fcber l\u00e4\u00dft.", "tokens": ["An", "ih\u00b7ren", "Mann", ",", "hin\u00b7\u00fc\u00b7ber", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ach! k\u00f6nnt' ich diesen Talisman", "tokens": ["Ach", "!", "k\u00f6nnt'", "ich", "die\u00b7sen", "Ta\u00b7lis\u00b7man"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Mir doch auf einen Monat borgen!", "tokens": ["Mir", "doch", "auf", "ei\u00b7nen", "Mo\u00b7nat", "bor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Dann fing' ich lieber heut als morgen", "tokens": ["Dann", "fing'", "ich", "lie\u00b7ber", "heut", "als", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die lang verschobne Kur noch an,", "tokens": ["Die", "lang", "ver\u00b7schob\u00b7ne", "Kur", "noch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und alle Launen, alle Sorgen,", "tokens": ["Und", "al\u00b7le", "Lau\u00b7nen", ",", "al\u00b7le", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ertr\u00e4nkt' ich in Pyrmonter dann.", "tokens": ["Er\u00b7tr\u00e4nkt'", "ich", "in", "Pyr\u00b7mon\u00b7ter", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ADV", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Du selbst, du k\u00f6nntest leicht ihn missen,", "tokens": ["Du", "selbst", ",", "du", "k\u00f6nn\u00b7test", "leicht", "ihn", "mis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VMFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Denn zweifeln mu\u00df ich, ob du schon,", "tokens": ["Denn", "zwei\u00b7feln", "mu\u00df", "ich", ",", "ob", "du", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "(so viel du sonst auch wei\u00dft,) magst wissen,", "tokens": ["(", "so", "viel", "du", "sonst", "auch", "wei\u00dft", ",", ")", "magst", "wis\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPER", "ADV", "ADV", "VVFIN", "$,", "$(", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Da\u00df Sorgen von dem Scorpion,", "tokens": ["Da\u00df", "Sor\u00b7gen", "von", "dem", "Scor\u00b7pi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und b\u00f6se Laune von den Wanzen", "tokens": ["Und", "b\u00f6\u00b7se", "Lau\u00b7ne", "von", "den", "Wan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Nur darin unterschieden sind,", "tokens": ["Nur", "da\u00b7rin", "un\u00b7ter\u00b7schie\u00b7den", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Da\u00df erstre, wenn der Tag beginnt,", "tokens": ["Da\u00df", "er\u00b7stre", ",", "wenn", "der", "Tag", "be\u00b7ginnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "Und letztre uns bei Nacht kuranzen,", "tokens": ["Und", "letz\u00b7tre", "uns", "bei", "Nacht", "ku\u00b7ran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und man durch Wurzeln oder Pflanzen,", "tokens": ["Und", "man", "durch", "Wur\u00b7zeln", "o\u00b7der", "Pflan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Den Sieg nicht \u00fcber sie gewinnt.", "tokens": ["Den", "Sieg", "nicht", "\u00fc\u00b7ber", "sie", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Zwar wirst du sagen: Ich soll kommen,", "tokens": ["Zwar", "wirst", "du", "sa\u00b7gen", ":", "Ich", "soll", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Selbst suchen unter deinem Schwell',", "tokens": ["Selbst", "su\u00b7chen", "un\u00b7ter", "dei\u00b7nem", "Schwell'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und Alles, auch das goldne Fell", "tokens": ["Und", "Al\u00b7les", ",", "auch", "das", "gold\u00b7ne", "Fell"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Von Kolchis selbst nicht ausgenommen,", "tokens": ["Von", "Kol\u00b7chis", "selbst", "nicht", "aus\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Soll ich, der Finder, zum Geschenk'", "tokens": ["Soll", "ich", ",", "der", "Fin\u00b7der", ",", "zum", "Ge\u00b7schen\u00b7k'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "F\u00fcr meinen weiten Weg empfangen.", "tokens": ["F\u00fcr", "mei\u00b7nen", "wei\u00b7ten", "Weg", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Dank f\u00fcr den Vorschlag! Doch bedenk!", "tokens": ["Dank", "f\u00fcr", "den", "Vor\u00b7schlag", "!", "Doch", "be\u00b7denk", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$.", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Wie schwer es mir schon eingegangen,", "tokens": ["Wie", "schwer", "es", "mir", "schon", "ein\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Als ich, so bald! das erstemal", "tokens": ["Als", "ich", ",", "so", "bald", "!", "das", "ers\u00b7te\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADV", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Zur Trennung mich gezwungen sahe.", "tokens": ["Zur", "Tren\u00b7nung", "mich", "ge\u00b7zwun\u00b7gen", "sa\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Ein Herz von Eisen und von Stahl", "tokens": ["Ein", "Herz", "von", "Ei\u00b7sen", "und", "von", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Mu\u00df schmelzen, kommt es dir zu nahe,", "tokens": ["Mu\u00df", "schmel\u00b7zen", ",", "kommt", "es", "dir", "zu", "na\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Und macht gleich dieses keine Qual,", "tokens": ["Und", "macht", "gleich", "die\u00b7ses", "kei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "So kam das Trennen doch noch allen,", "tokens": ["So", "kam", "das", "Tren\u00b7nen", "doch", "noch", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Die je dich sahn, so leicht nicht an,", "tokens": ["Die", "je", "dich", "sahn", ",", "so", "leicht", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Als wie dein Vater", "tokens": ["Als", "wie", "dein", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.47": {"text": "Das reine Silber scheiden kann.", "tokens": ["Das", "rei\u00b7ne", "Sil\u00b7ber", "schei\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Lieg' wo du liegst, o Talisman!", "tokens": ["Lieg'", "wo", "du", "liegst", ",", "o", "Ta\u00b7lis\u00b7man", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPER", "VVFIN", "$,", "FM", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.49": {"text": "Kannst du nicht sch\u00fctzen vor dem Trennen,", "tokens": ["Kannst", "du", "nicht", "sch\u00fct\u00b7zen", "vor", "dem", "Tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Wie w\u00fcrdest du nachher mich dann", "tokens": ["Wie", "w\u00fcr\u00b7dest", "du", "nach\u00b7her", "mich", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Vor neuen Grillen sch\u00fctzen k\u00f6nnen?", "tokens": ["Vor", "neu\u00b7en", "Gril\u00b7len", "sch\u00fct\u00b7zen", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}