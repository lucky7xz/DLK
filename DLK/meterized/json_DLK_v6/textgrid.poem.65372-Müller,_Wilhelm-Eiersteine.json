{"textgrid.poem.65372": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Eiersteine", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh die glatten Kieselb\u00e4lle", "tokens": ["Sieh", "die", "glat\u00b7ten", "Kie\u00b7sel\u00b7b\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegen in dem weichen Sand!", "tokens": ["Lie\u00b7gen", "in", "dem", "wei\u00b7chen", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frage sie, wie oft die Welle", "tokens": ["Fra\u00b7ge", "sie", ",", "wie", "oft", "die", "Wel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie geworfen auf den Strand,", "tokens": ["Sie", "ge\u00b7wor\u00b7fen", "auf", "den", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh' an Klippen und an Riffen", "tokens": ["Eh'", "an", "Klip\u00b7pen", "und", "an", "Rif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihre Ecken sind zerschliffen.", "tokens": ["Ih\u00b7re", "E\u00b7cken", "sind", "zer\u00b7schlif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00f6nntest du mein Herze sehen,", "tokens": ["K\u00f6nn\u00b7test", "du", "mein", "Her\u00b7ze", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie es jetzt im Busen ruht!", "tokens": ["Wie", "es", "jetzt", "im", "Bu\u00b7sen", "ruht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh die Stein', um zu verstehen,", "tokens": ["Sieh", "die", "Stein'", ",", "um", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der m\u00fcde Fried' ihm thut.", "tokens": ["Wie", "der", "m\u00fc\u00b7de", "Fried'", "ihm", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Steine, k\u00f6nnt ihr nicht zerspringen,", "tokens": ["Stei\u00b7ne", ",", "k\u00f6nnt", "ihr", "nicht", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Statt euch also glatt zu ringen?", "tokens": ["Statt", "euch", "al\u00b7so", "glatt", "zu", "rin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}