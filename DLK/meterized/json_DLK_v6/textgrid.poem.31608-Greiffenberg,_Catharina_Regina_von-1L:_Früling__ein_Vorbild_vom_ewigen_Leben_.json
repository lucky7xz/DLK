{"textgrid.poem.31608": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fcling / ein Vorbild vom ewigen Leben /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fcling / ein Vorbild vom ewigen Leben /", "tokens": ["Fr\u00fc\u00b7ling", "/", "ein", "Vor\u00b7bild", "vom", "e\u00b7wi\u00b7gen", "Le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Spiegel der Jugend / der Freuden Gezelt /", "tokens": ["Spie\u00b7gel", "der", "Ju\u00b7gend", "/", "der", "Freu\u00b7den", "Ge\u00b7zelt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "J\u00e4hrlich-verj\u00fcngeter F\u00f6nix der Welt /", "tokens": ["J\u00e4hr\u00b7lich\u00b7ver\u00b7j\u00fcn\u00b7ge\u00b7ter", "F\u00f6\u00b7nix", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Athem der Musen / der Huldinnen Weben /", "tokens": ["A\u00b7them", "der", "Mu\u00b7sen", "/", "der", "Hul\u00b7din\u00b7nen", "We\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Wonne so alle Ergetzung kan geben /", "tokens": ["Won\u00b7ne", "so", "al\u00b7le", "Er\u00b7get\u00b7zung", "kan", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Goldschmid der Wiesen / und Mahler im Feld /", "tokens": ["Gold\u00b7schmid", "der", "Wie\u00b7sen", "/", "und", "Mah\u00b7ler", "im", "Feld", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "KON", "NN", "APPRART", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Kleinod / das niemand erkauffet mit Gelt /", "tokens": ["Klei\u00b7nod", "/", "das", "nie\u00b7mand", "er\u00b7kauf\u00b7fet", "mit", "Gelt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "PIS", "VVFIN", "APPR", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "frischer der vieler Herz-frischenden Reben!", "tokens": ["fri\u00b7scher", "der", "vie\u00b7ler", "Her\u00b7zfri\u00b7schen\u00b7den", "Re\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "PIAT", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "sey mir willkommen / ausl\u00e4ndischer Gast /", "tokens": ["sey", "mir", "will\u00b7kom\u00b7men", "/", "aus\u00b7l\u00e4n\u00b7di\u00b7scher", "Gast", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$(", "ADJA", "NN", "$("], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Freuden-Freund / Gl\u00fcckes-Wirt / Diener der Liebe!", "tokens": ["Freu\u00b7den\u00b7Freund", "/", "Gl\u00fc\u00b7ckes\u00b7Wirt", "/", "Die\u00b7ner", "der", "Lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "sey nur mit Blumen und Bl\u00e4ttern gefast /", "tokens": ["sey", "nur", "mit", "Blu\u00b7men", "und", "Bl\u00e4t\u00b7tern", "ge\u00b7fast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "deine hieherkunfft nicht l\u00e4nger verschiebe!", "tokens": ["dei\u00b7ne", "hie\u00b7her\u00b7kunfft", "nicht", "l\u00e4n\u00b7ger", "ver\u00b7schie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "alle verlangbare Sch\u00e4tze du hast.", "tokens": ["al\u00b7le", "ver\u00b7lang\u00b7ba\u00b7re", "Sch\u00e4t\u00b7ze", "du", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VAFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.14": {"text": "Dir ich die Krone der Lieblichkeit giebe.", "tokens": ["Dir", "ich", "die", "Kro\u00b7ne", "der", "Lieb\u00b7lich\u00b7keit", "gie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}