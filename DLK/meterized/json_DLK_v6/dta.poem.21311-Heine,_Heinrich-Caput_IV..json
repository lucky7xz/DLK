{"dta.poem.21311": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput IV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vergebens wird der gro\u00dfe Franz Li\u00dft", "tokens": ["Ver\u00b7ge\u00b7bens", "wird", "der", "gro\u00b7\u00dfe", "Franz", "Li\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NE", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zum Besten des Doms musiziren,", "tokens": ["Zum", "Bes\u00b7ten", "des", "Doms", "mu\u00b7si\u00b7zi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ein talentvoller K\u00f6nig wird", "tokens": ["Und", "ein", "ta\u00b7lent\u00b7vol\u00b7ler", "K\u00f6\u00b7nig", "wird"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Vergebens deklamiren!", "tokens": ["Ver\u00b7ge\u00b7bens", "de\u00b7kla\u00b7mi\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Er wird nicht vollendet, der K\u00f6llner Dom,", "tokens": ["Er", "wird", "nicht", "voll\u00b7en\u00b7det", ",", "der", "K\u00f6ll\u00b7ner", "Dom", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$,", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Obgleich die Narren in Schwaben", "tokens": ["Ob\u00b7gleich", "die", "Nar\u00b7ren", "in", "Schwa\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zu seinem Fortbau ein ganzes Schiff", "tokens": ["Zu", "sei\u00b7nem", "Fort\u00b7bau", "ein", "gan\u00b7zes", "Schiff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Voll Steine gesendet haben.", "tokens": ["Voll", "Stei\u00b7ne", "ge\u00b7sen\u00b7det", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVPP", "VAINF", "$."], "meter": "-+----+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Er wird nicht vollendet, trotz allem Geschrey", "tokens": ["Er", "wird", "nicht", "voll\u00b7en\u00b7det", ",", "trotz", "al\u00b7lem", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$,", "APPR", "PIS", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Raben und der Eulen,", "tokens": ["Der", "Ra\u00b7ben", "und", "der", "Eu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die, alterth\u00fcmlich gesinnt, so gern", "tokens": ["Die", ",", "al\u00b7tert\u00b7h\u00fcm\u00b7lich", "ge\u00b7sinnt", ",", "so", "gern"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "ADJD", "VVPP", "$,", "ADV", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In hohen Kirchth\u00fcrmen weilen.", "tokens": ["In", "ho\u00b7hen", "Kircht\u00b7h\u00fcr\u00b7men", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ja, kommen wird die Zeit sogar", "tokens": ["Ja", ",", "kom\u00b7men", "wird", "die", "Zeit", "so\u00b7gar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVINF", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo man, statt ihn zu vollenden,", "tokens": ["Wo", "man", ",", "statt", "ihn", "zu", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die inneren R\u00e4ume zu einem Stall", "tokens": ["Die", "in\u00b7ne\u00b7ren", "R\u00e4u\u00b7me", "zu", "ei\u00b7nem", "Stall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "F\u00fcr Pferde wird verwenden.", "tokens": ["F\u00fcr", "Pfer\u00b7de", "wird", "ver\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}