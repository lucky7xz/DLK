{"dta.poem.14418": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Stille sa\u00df' ich, sah die Jahre fliegen,", "tokens": ["Stil\u00b7le", "sa\u00df'", "ich", ",", "sah", "die", "Jah\u00b7re", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bin im Innersten Dein treues Kind geblieben.", "tokens": ["Bin", "im", "In\u00b7ners\u00b7ten", "Dein", "treu\u00b7es", "Kind", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aus dem duft'gen Kelche aufgestiegen,", "tokens": ["Aus", "dem", "duft'\u00b7gen", "Kel\u00b7che", "auf\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach! wann lohnst Du endlich auch mein treues Lieben!", "tokens": ["Ach", "!", "wann", "lohnst", "Du", "end\u00b7lich", "auch", "mein", "treu\u00b7es", "Lie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}