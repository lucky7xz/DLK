{"textgrid.poem.41366": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Bauer und die Schlange", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ackersmann fand eine Schlange,", "tokens": ["Ein", "A\u00b7ckers\u00b7mann", "fand", "ei\u00b7ne", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die fast erstarrt vor K\u00e4lte war.", "tokens": ["Die", "fast", "er\u00b7starrt", "vor", "K\u00e4l\u00b7te", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Arm entri\u00df sie der Gefahr,", "tokens": ["Sein", "Arm", "ent\u00b7ri\u00df", "sie", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihrem nahen Untergange.", "tokens": ["Und", "ih\u00b7rem", "na\u00b7hen", "Un\u00b7ter\u00b7gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er nahm sie mit sich in sein Haus,", "tokens": ["Er", "nahm", "sie", "mit", "sich", "in", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sucht' ihr einen Winkel aus,", "tokens": ["Und", "sucht'", "ihr", "ei\u00b7nen", "Win\u00b7kel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo noch ein Rest von Reisern gl\u00fchte.", "tokens": ["Wo", "noch", "ein", "Rest", "von", "Rei\u00b7sern", "gl\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch als ihr Frost und Noth entwich,", "tokens": ["Doch", "als", "ihr", "Frost", "und", "Noth", "ent\u00b7wich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Erholte, regt', und hub sie sich,", "tokens": ["Er\u00b7hol\u00b7te", ",", "regt'", ",", "und", "hub", "sie", "sich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und lohnte dem mit Bi\u00df und Stich,", "tokens": ["Und", "lohn\u00b7te", "dem", "mit", "Bi\u00df", "und", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Den ihre Rettung so bem\u00fchte.", "tokens": ["Den", "ih\u00b7re", "Ret\u00b7tung", "so", "be\u00b7m\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Betrogne Huld und Z\u00e4rtlichkeit,", "tokens": ["Be\u00b7trog\u00b7ne", "Huld", "und", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Frevlern blindlings Hilfe beut!", "tokens": ["Die", "Frev\u00b7lern", "blind\u00b7lings", "Hil\u00b7fe", "beut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier folgt der Schade stets der G\u00fcte.", "tokens": ["Hier", "folgt", "der", "Scha\u00b7de", "stets", "der", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}