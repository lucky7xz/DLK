{"textgrid.poem.31223": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der vor bereiffte Wald", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der vor bereiffte Wald", "tokens": ["Der", "vor", "be\u00b7re\u00b7iff\u00b7te", "Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "steht wihder wohl gestalt/", "tokens": ["steht", "wih\u00b7der", "wohl", "ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der gantze gr\u00fcne Grund", "tokens": ["der", "gant\u00b7ze", "gr\u00fc\u00b7ne", "Grund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "lacht wihder Bluhmen-bundt.", "tokens": ["lacht", "wih\u00b7der", "Bluh\u00b7men\u00b7bundt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Schon p\u00e4rlt au\u00df ihrer Kehle", "tokens": ["Schon", "p\u00e4rlt", "au\u00df", "ih\u00b7rer", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die k\u00f6stlichsten Jubele", "tokens": ["die", "k\u00f6st\u00b7lichs\u00b7ten", "Ju\u00b7be\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "mit gleichsahm siegerischem Schall", "tokens": ["mit", "gleich\u00b7sahm", "sie\u00b7ge\u00b7ri\u00b7schem", "Schall"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "die durchau\u00df kleine Nachtigall.", "tokens": ["die", "durch\u00b7au\u00df", "klei\u00b7ne", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der holden ", "tokens": ["Der", "hol\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "das Klee-gepaarte Paar/", "tokens": ["das", "Klee\u00b7ge\u00b7paar\u00b7te", "Paar", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "zeigt itzt so r\u00e4cht mit Lust", "tokens": ["zeigt", "itzt", "so", "r\u00e4cht", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "VVFIN", "APPR", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "sich seine nakkte Brust.", "tokens": ["sich", "sei\u00b7ne", "nakk\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon pakkt man sich am B\u00e4ndel/", "tokens": ["Schon", "pakkt", "man", "sich", "am", "B\u00e4n\u00b7del", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "kom mit mir in den Qw\u00e4ndel!", "tokens": ["kom", "mit", "mir", "in", "den", "Qw\u00e4n\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Vergraben gantz in Helffen-Bein/", "tokens": ["Ver\u00b7gra\u00b7ben", "gantz", "in", "Helf\u00b7fen\u00b7Bein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "will alles itzt gestorben seyn!", "tokens": ["will", "al\u00b7les", "itzt", "ge\u00b7stor\u00b7ben", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dorillgen/ stoltzes Thier/", "tokens": ["Do\u00b7rill\u00b7gen", "/", "stolt\u00b7zes", "Thier", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "wa\u00df hastu gegen mir?", "tokens": ["wa\u00df", "has\u00b7tu", "ge\u00b7gen", "mir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du m\u00fchst dich fast au\u00df Stein", "tokens": ["Du", "m\u00fchst", "dich", "fast", "au\u00df", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wie ", "tokens": ["wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Rubinen und Korallen", "tokens": ["Ru\u00b7bi\u00b7nen", "und", "Ko\u00b7ral\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "bezihren dir die Ballen/", "tokens": ["be\u00b7zih\u00b7ren", "dir", "die", "Bal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "ach/ k\u00fcnt ich doch/ du s\u00fcsses Huhn/", "tokens": ["ach", "/", "k\u00fcnt", "ich", "doch", "/", "du", "s\u00fcs\u00b7ses", "Huhn", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["XY", "$(", "VVFIN", "PPER", "ADV", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "vergn\u00fcgt in deinen Armen ruhn!", "tokens": ["ver\u00b7gn\u00fcgt", "in", "dei\u00b7nen", "Ar\u00b7men", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du g\u00f6ldener Magnet", "tokens": ["Du", "g\u00f6l\u00b7de\u00b7ner", "Mag\u00b7net"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "au\u00df Julep und Zibeth/", "tokens": ["au\u00df", "Ju\u00b7lep", "und", "Zi\u00b7beth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$("], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "du Sarg for meine Pein/", "tokens": ["du", "Sarg", "for", "mei\u00b7ne", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "kom/ la\u00df mich bey dir seyn!", "tokens": ["kom", "/", "la\u00df", "mich", "bey", "dir", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVIMP", "PPER", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein H\u00e4uptgen kr\u00e4ntzt ein Kr\u00f6ngen", "tokens": ["Dein", "H\u00e4upt\u00b7gen", "kr\u00e4ntzt", "ein", "Kr\u00f6n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "au\u00df lautter Tausend-Sch\u00f6ngen/", "tokens": ["au\u00df", "laut\u00b7ter", "Tau\u00b7sen\u00b7dSch\u00f6n\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "die lihbe Frau au\u00df ", "tokens": ["die", "lih\u00b7be", "Frau", "au\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "ist Hindten-r\u00fcmb nicht halb so rund!", "tokens": ["ist", "Hin\u00b7dten\u00b7r\u00fcmb", "nicht", "halb", "so", "rund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gl\u00e4ubs mir/ du junges Bluht/", "tokens": ["Gl\u00e4ubs", "mir", "/", "du", "jun\u00b7ges", "Bluht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "ich b\u00fcn kein F\u00fcnffzehn-Hut.", "tokens": ["ich", "b\u00fcn", "kein", "F\u00fcnf\u00b7fzehn\u00b7Hut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich b\u00fcn zu jeder Zeit", "tokens": ["Ich", "b\u00fcn", "zu", "je\u00b7der", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "for Zucht und Ehrbarkeit.", "tokens": ["for", "Zucht", "und", "Ehr\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den klugen ", "tokens": ["Den", "klu\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "gab ich mich gantz von innen/", "tokens": ["gab", "ich", "mich", "gantz", "von", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "ADV", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "ein solches Hertz schmihrt insgemein", "tokens": ["ein", "sol\u00b7ches", "Hertz", "schmihrt", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "die Tugend r\u00e4cht mit Balsam ein!", "tokens": ["die", "Tu\u00b7gend", "r\u00e4cht", "mit", "Bal\u00b7sam", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ey/ ey/ wa\u00df ist denn da\u00df?", "tokens": ["Ey", "/", "ey", "/", "wa\u00df", "ist", "denn", "da\u00df", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "XY", "$(", "PWAV", "VAFIN", "ADV", "KOUS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich gl\u00e4ub/ da hastu wa\u00df?", "tokens": ["Ich", "gl\u00e4ub", "/", "da", "has\u00b7tu", "wa\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von dihsem wehrten Ort", "tokens": ["Von", "dih\u00b7sem", "wehr\u00b7ten", "Ort"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "zih ich die Hand nicht fort.", "tokens": ["zih", "ich", "die", "Hand", "nicht", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vor solchem Paradiese", "tokens": ["Vor", "sol\u00b7chem", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "wird ", "tokens": ["wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "verstatte/ da\u00df mein Kihl sich spizzt", "tokens": ["ver\u00b7stat\u00b7te", "/", "da\u00df", "mein", "Kihl", "sich", "spizzt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und ihm nur Musc und Amber schwizzt!", "tokens": ["und", "ihm", "nur", "Musc", "und", "Am\u00b7ber", "schwizzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Zei\u00dfgen dirilirt/", "tokens": ["Ein", "Zei\u00df\u00b7gen", "di\u00b7ri\u00b7lirt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "ins gr\u00fcne Gra\u00df bostirt/", "tokens": ["ins", "gr\u00fc\u00b7ne", "Gra\u00df", "bos\u00b7tirt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "darzu so k\u00fckkt es her/", "tokens": ["dar\u00b7zu", "so", "k\u00fckkt", "es", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wa\u00df hat es denn so sehr?", "tokens": ["wa\u00df", "hat", "es", "denn", "so", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Bach h\u00f6rt auff zu rauschen/", "tokens": ["Der", "Bach", "h\u00f6rt", "auff", "zu", "rau\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die ", "tokens": ["die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "all meinen schwartzen Kummer stopff", "tokens": ["all", "mei\u00b7nen", "schwart\u00b7zen", "Kum\u00b7mer", "stopff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ich itzt in dich al\u00df Threnen-Topff!", "tokens": ["ich", "itzt", "in", "dich", "al\u00df", "Thre\u00b7nen\u00b7Topff", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PRF", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "sie lihbten alle Schertz/", "tokens": ["sie", "lihb\u00b7ten", "al\u00b7le", "Schertz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "s\u00fc\u00df droff ihr Fehder-Kihl.", "tokens": ["s\u00fc\u00df", "droff", "ihr", "Feh\u00b7der\u00b7Kihl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dr\u00fcmb lehrt auch dich mein Fl\u00f6hten", "tokens": ["Dr\u00fcmb", "lehrt", "auch", "dich", "mein", "Fl\u00f6h\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "empfindlich itzt err\u00f6hten;", "tokens": ["emp\u00b7find\u00b7lich", "itzt", "er\u00b7r\u00f6h\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "der Saffran-gelbe L\u00f6wen-Zahn", "tokens": ["der", "Saf\u00b7fran\u00b7gel\u00b7be", "L\u00f6\u00b7wen\u00b7Zahn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "beschehmbt offt s\u00e4lbst den Dulipan!", "tokens": ["be\u00b7schehmbt", "offt", "s\u00e4lbst", "den", "Du\u00b7li\u00b7pan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}