{"textgrid.poem.46626": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ist's besser, nicht besessen haben]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's besser, nicht besessen haben,", "tokens": ["Ist's", "bes\u00b7ser", ",", "nicht", "be\u00b7ses\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als zu verlieren das Bese\u00dfne?", "tokens": ["Als", "zu", "ver\u00b7lie\u00b7ren", "das", "Be\u00b7se\u00df\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Grunde gleich sind alle Gaben,", "tokens": ["Im", "Grun\u00b7de", "gleich", "sind", "al\u00b7le", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Himmel Menschen zugeme\u00dfne.", "tokens": ["Vom", "Him\u00b7mel", "Men\u00b7schen", "zu\u00b7ge\u00b7me\u00df\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es fehlt uns doch, was wir nicht wissen;", "tokens": ["Es", "fehlt", "uns", "doch", ",", "was", "wir", "nicht", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir haben noch, was wir vermissen!", "tokens": ["Wir", "ha\u00b7ben", "noch", ",", "was", "wir", "ver\u00b7mis\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und endlich ruht in Finsternissen,", "tokens": ["Und", "end\u00b7lich", "ruht", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ob nie gehabt und ob entrissen,", "tokens": ["Ob", "nie", "ge\u00b7habt", "und", "ob", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAPP", "KON", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gleich Ungekanntem das Verge\u00dfne.", "tokens": ["Gleich", "Un\u00b7ge\u00b7kann\u00b7tem", "das", "Ver\u00b7ge\u00df\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}