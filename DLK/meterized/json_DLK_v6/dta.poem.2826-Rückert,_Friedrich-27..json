{"dta.poem.2826": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deiner Bed\u00fcrfnisse Befriedigung gereicht", "tokens": ["Dei\u00b7ner", "Be\u00b7d\u00fcrf\u00b7nis\u00b7se", "Be\u00b7frie\u00b7di\u00b7gung", "ge\u00b7reicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVPP"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dir zum Genusse wol, doch zur Beschwer auch leicht.", "tokens": ["Dir", "zum", "Ge\u00b7nus\u00b7se", "wol", ",", "doch", "zur", "Be\u00b7schwer", "auch", "leicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADV", "$,", "ADV", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gebietrisch fordern sie einmal-Gewohntes immer:", "tokens": ["Ge\u00b7bie\u00b7trisch", "for\u00b7dern", "sie", "ein\u00b7ma\u00b7lGe\u00b7wohn\u00b7tes", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADV", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gib oder weigre nun! was ist von beidem schlimmer?", "tokens": ["Gib", "o\u00b7der", "weig\u00b7re", "nun", "!", "was", "ist", "von", "bei\u00b7dem", "schlim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "ADV", "$.", "PWS", "VAFIN", "APPR", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}