{"textgrid.poem.39129": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rt auf zu mir zu sprechen,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt auf zu mir zu sprechen,", "tokens": ["H\u00f6rt", "auf", "zu", "mir", "zu", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr sprecht zu Stein und Holz,", "tokens": ["Ihr", "sprecht", "zu", "Stein", "und", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr sollt mir ihn nicht brechen,", "tokens": ["Ihr", "sollt", "mir", "ihn", "nicht", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den freud'gen Jugendstolz.", "tokens": ["Den", "freu\u00b7d'\u00b7gen", "Ju\u00b7gend\u00b7stolz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr sollt mich nicht bereden,", "tokens": ["Ihr", "sollt", "mich", "nicht", "be\u00b7re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df alle Menschen schlecht,", "tokens": ["Da\u00df", "al\u00b7le", "Men\u00b7schen", "schlecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ganz in einem Jeden", "tokens": ["Da\u00df", "ganz", "in", "ei\u00b7nem", "Je\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erstorben sei das Recht.", "tokens": ["E\u00b7rstor\u00b7ben", "sei", "das", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Licht, es ist so bla\u00df nicht,", "tokens": ["Das", "Licht", ",", "es", "ist", "so", "bla\u00df", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ihr es immer meint,", "tokens": ["Als", "ihr", "es", "im\u00b7mer", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Nebel ist so gra\u00df nicht,", "tokens": ["Der", "Ne\u00b7bel", "ist", "so", "gra\u00df", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ihr es stets beweint.", "tokens": ["Als", "ihr", "es", "stets", "be\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Welt ist nicht so sch\u00e4ndlich,", "tokens": ["Die", "Welt", "ist", "nicht", "so", "sch\u00e4nd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ihr es immer sagt,", "tokens": ["Als", "ihr", "es", "im\u00b7mer", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Not nicht so unendlich,", "tokens": ["Die", "Not", "nicht", "so", "un\u00b7end\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ihr es stets beklagt.", "tokens": ["Als", "ihr", "es", "stets", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Himmel hat von Sonnen", "tokens": ["Der", "Him\u00b7mel", "hat", "von", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch eine gro\u00dfe Schar,", "tokens": ["Noch", "ei\u00b7ne", "gro\u00b7\u00dfe", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist von allen Wonnen", "tokens": ["Es", "ist", "von", "al\u00b7len", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Erde noch nicht bar.", "tokens": ["Die", "Er\u00b7de", "noch", "nicht", "bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Noch gibt es Helden bieder", "tokens": ["Noch", "gibt", "es", "Hel\u00b7den", "bie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Feder und mit Schwert,", "tokens": ["Mit", "Fe\u00b7der", "und", "mit", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch gibt es Heldenlieder", "tokens": ["Noch", "gibt", "es", "Hel\u00b7den\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von freier Helden Wert.", "tokens": ["Von", "frei\u00b7er", "Hel\u00b7den", "Wert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Noch gibt es zarte Dichter", "tokens": ["Noch", "gibt", "es", "zar\u00b7te", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Dichter wilder Art,", "tokens": ["Und", "Dich\u00b7ter", "wil\u00b7der", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es gl\u00fchn als Sangeslichter", "tokens": ["Es", "gl\u00fchn", "als", "San\u00b7ges\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch Frauen wunderzart.", "tokens": ["Noch", "Frau\u00b7en", "wun\u00b7derz\u00b7art", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es ist das Gold der Rebe", "tokens": ["Es", "ist", "das", "Gold", "der", "Re\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch lange nicht vergl\u00fcht,", "tokens": ["Noch", "lan\u00b7ge", "nicht", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Lenzes Duftgewebe", "tokens": ["Des", "Len\u00b7zes", "Duft\u00b7ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat Jahr f\u00fcr Jahr gebl\u00fcht.", "tokens": ["Hat", "Jahr", "f\u00fcr", "Jahr", "ge\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wo Herzen, stolze, starke,", "tokens": ["Wo", "Her\u00b7zen", ",", "stol\u00b7ze", ",", "star\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch f\u00fcr das Rechte stehn,", "tokens": ["Noch", "f\u00fcr", "das", "Rech\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da darf der Hoffnung Barke", "tokens": ["Da", "darf", "der", "Hoff\u00b7nung", "Bar\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht v\u00f6llig untergehn.", "tokens": ["Nicht", "v\u00f6l\u00b7lig", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und wo der Hoffnung Flammen", "tokens": ["Und", "wo", "der", "Hoff\u00b7nung", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch spr\u00fchn in einer Brust,", "tokens": ["Noch", "spr\u00fchn", "in", "ei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da soll man nicht verdammen", "tokens": ["Da", "soll", "man", "nicht", "ver\u00b7dam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die frische Liedeslust.", "tokens": ["Die", "fri\u00b7sche", "Lie\u00b7des\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und h\u00e4uft sich noch so tr\u00fcbe", "tokens": ["Und", "h\u00e4uft", "sich", "noch", "so", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ums Herz der Nebeldunst,", "tokens": ["Ums", "Herz", "der", "Ne\u00b7bel\u00b7dunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Herz sei voll von Liebe,", "tokens": ["Das", "Herz", "sei", "voll", "von", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und fr\u00f6hlich sei die Kunst.", "tokens": ["Und", "fr\u00f6h\u00b7lich", "sei", "die", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}