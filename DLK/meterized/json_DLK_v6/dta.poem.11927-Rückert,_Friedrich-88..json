{"dta.poem.11927": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Tempel Gottes hat sich die Natur gebaut,", "tokens": ["Ein", "Tem\u00b7pel", "Got\u00b7tes", "hat", "sich", "die", "Na\u00b7tur", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worin er tausendfach geahnt wird und geschaut.", "tokens": ["Wo\u00b7rin", "er", "tau\u00b7send\u00b7fach", "ge\u00b7ahnt", "wird", "und", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als Tempeldiener gehn hindurch die Jahreszeiten,", "tokens": ["Als", "Tem\u00b7pel\u00b7die\u00b7ner", "gehn", "hin\u00b7durch", "die", "Jah\u00b7res\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die bunten Teppiche am Boden hinzubreiten.", "tokens": ["Die", "bun\u00b7ten", "Tep\u00b7pi\u00b7che", "am", "Bo\u00b7den", "hin\u00b7zu\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Stralend im h\u00f6chsten Chor lobsingen Sonn' und Sterne,", "tokens": ["Stra\u00b7lend", "im", "h\u00f6chs\u00b7ten", "Chor", "lob\u00b7sin\u00b7gen", "Sonn'", "und", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der Abgrund und das Meer antworten aus der Ferne.", "tokens": ["Der", "Ab\u00b7grund", "und", "das", "Meer", "ant\u00b7wor\u00b7ten", "aus", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das Mittelfeuer gl\u00fcht am ew'gen Opferherde,", "tokens": ["Das", "Mit\u00b7tel\u00b7feu\u00b7er", "gl\u00fcht", "am", "ew'\u00b7gen", "Op\u00b7fer\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles Leben naht, da\u00df es das Opfer werde.", "tokens": ["Und", "al\u00b7les", "Le\u00b7ben", "naht", ",", "da\u00df", "es", "das", "Op\u00b7fer", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Als Opferpriester kniet der Geist an viel Alt\u00e4ren,", "tokens": ["Als", "Op\u00b7fer\u00b7pries\u00b7ter", "kniet", "der", "Geist", "an", "viel", "Al\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die er mit Bildern schm\u00fcckt, und sucht sie zu erkl\u00e4ren.", "tokens": ["Die", "er", "mit", "Bil\u00b7dern", "schm\u00fcckt", ",", "und", "sucht", "sie", "zu", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In viele H\u00fcllen hat die F\u00fclle sich verh\u00fcllt,", "tokens": ["In", "vie\u00b7le", "H\u00fcl\u00b7len", "hat", "die", "F\u00fcl\u00b7le", "sich", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch von der F\u00fclle nur ist jede H\u00fcll' erf\u00fcllt.", "tokens": ["Doch", "von", "der", "F\u00fcl\u00b7le", "nur", "ist", "je\u00b7de", "H\u00fcll'", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und wo der Geist vermag hinweg der Selbsucht Schleier", "tokens": ["Und", "wo", "der", "Geist", "ver\u00b7mag", "hin\u00b7weg", "der", "Selb\u00b7sucht", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu heben, sieht er hell darunter Gottes Feier.", "tokens": ["Zu", "he\u00b7ben", ",", "sieht", "er", "hell", "da\u00b7run\u00b7ter", "Got\u00b7tes", "Fei\u00b7er", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ADJD", "PAV", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und Gottes Athem geht ein Morgenhauch durchs Schiff,", "tokens": ["Und", "Got\u00b7tes", "A\u00b7them", "geht", "ein", "Mor\u00b7gen\u00b7hauch", "durchs", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einsammelnd jeglicher Verehrung Inbegriff.", "tokens": ["Ein\u00b7sam\u00b7melnd", "jeg\u00b7li\u00b7cher", "Ver\u00b7eh\u00b7rung", "In\u00b7be\u00b7griff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "NN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.9": {"line.1": {"text": "Sein L\u00e4cheln streuet Duft in tr\u00fcber Inbrunst Glimmen,", "tokens": ["Sein", "L\u00e4\u00b7cheln", "streu\u00b7et", "Duft", "in", "tr\u00fc\u00b7ber", "In\u00b7brunst", "Glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein S\u00e4useln Einigung in widerstreit'ge Stimmen,", "tokens": ["Sein", "S\u00e4u\u00b7seln", "Ei\u00b7ni\u00b7gung", "in", "wi\u00b7der\u00b7streit'\u00b7ge", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Aus jedem Opferrauch nimmt er das feinste Korn,", "tokens": ["Aus", "je\u00b7dem", "Op\u00b7fer\u00b7rauch", "nimmt", "er", "das", "feins\u00b7te", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Den reinsten Tropfen auch aus jedem Andachtsborn;", "tokens": ["Den", "reins\u00b7ten", "Trop\u00b7fen", "auch", "aus", "je\u00b7dem", "An\u00b7dachts\u00b7born", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Aus jedem Wortgebet den ihm bewu\u00dften Sinn;", "tokens": ["Aus", "je\u00b7dem", "Wort\u00b7ge\u00b7bet", "den", "ihm", "be\u00b7wu\u00df\u00b7ten", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er selbst legt ihn hinein, und findet ihn darinn.", "tokens": ["Er", "selbst", "legt", "ihn", "hin\u00b7ein", ",", "und", "fin\u00b7det", "ihn", "da\u00b7rinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Dann will er auch den Sinn der Sinnenden entfalten,", "tokens": ["Dann", "will", "er", "auch", "den", "Sinn", "der", "Sin\u00b7nen\u00b7den", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df immer w\u00fcrdiger sie ihm die Feier halten;", "tokens": ["Da\u00df", "im\u00b7mer", "w\u00fcr\u00b7di\u00b7ger", "sie", "ihm", "die", "Fei\u00b7er", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Da\u00df die gebundnen frei zu h\u00f6hrer Wonn' aufgehn;", "tokens": ["Da\u00df", "die", "ge\u00b7bund\u00b7nen", "frei", "zu", "h\u00f6h\u00b7rer", "Wonn'", "auf\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn das ist seine Lust, der Sch\u00f6pfung Lust zu sehn.", "tokens": ["Denn", "das", "ist", "sei\u00b7ne", "Lust", ",", "der", "Sch\u00f6p\u00b7fung", "Lust", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}