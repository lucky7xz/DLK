{"textgrid.poem.33580": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "10. Fernsprache", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie du mir fehlst,", "tokens": ["O", "wie", "du", "mir", "fehlst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wie ich dich vermisse!", "tokens": ["Wie", "ich", "dich", "ver\u00b7mis\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch du hellst, beseelst", "tokens": ["Doch", "du", "hellst", ",", "be\u00b7seelst"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mir auch Finsternisse.", "tokens": ["Mir", "auch", "Fins\u00b7ter\u00b7nis\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn du mir erscheinst,", "tokens": ["Wenn", "du", "mir", "er\u00b7scheinst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wir im Traum uns finden,", "tokens": ["Wir", "im", "Traum", "uns", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6r' ich's, wie du weinst,", "tokens": ["H\u00f6r'", "ich's", ",", "wie", "du", "weinst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "F\u00fchl' ich dein Empfinden.", "tokens": ["F\u00fchl'", "ich", "dein", "Emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, ein geisterhaft", "tokens": ["Ja", ",", "ein", "geis\u00b7ter\u00b7haft"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Magisch Liebeswalten,", "tokens": ["Ma\u00b7gisch", "Lie\u00b7bes\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eine h\u00f6h're Kraft", "tokens": ["Ei\u00b7ne", "h\u00f6h'\u00b7re", "Kraft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wird dich mir erhalten!", "tokens": ["Wird", "dich", "mir", "er\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Auch durch Mauern bricht", "tokens": ["Auch", "durch", "Mau\u00b7ern", "bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sie mit Seelenschwingen,", "tokens": ["Sie", "mit", "See\u00b7len\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und es wird ihr Licht", "tokens": ["Und", "es", "wird", "ihr", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Durch die Ferne dringen.", "tokens": ["Durch", "die", "Fer\u00b7ne", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}