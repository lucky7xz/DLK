{"textgrid.poem.53980": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der Meineid", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn denn Jeorjen seine Fauste", "tokens": ["Wenn", "denn", "Jeor\u00b7jen", "sei\u00b7ne", "Faus\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NE", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in Lottchen ihre Augen sauste,", "tokens": ["in", "Lott\u00b7chen", "ih\u00b7re", "Au\u00b7gen", "saus\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "denn freute sich det janze Haus.", "tokens": ["denn", "freu\u00b7te", "sich", "det", "jan\u00b7ze", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Indem da\u00df alle einich waren:", "tokens": ["In\u00b7dem", "da\u00df", "al\u00b7le", "ei\u00b7nich", "wa\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ne Frau von vierunddrei\u00dfig Jahren,", "tokens": ["ne", "Frau", "von", "vie\u00b7rund\u00b7drei\u00b7\u00dfig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die sieht jef\u00e4lligst anders aus.", "tokens": ["die", "sieht", "je\u00b7f\u00e4l\u00b7ligst", "an\u00b7ders", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Na, det will ick mein \u2013!", "tokens": ["Na", ",", "det", "will", "ick", "mein", "\u2013", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PDS", "VMFIN", "PPER", "PPOSAT", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Von wejen: sich die Backen pudern", "tokens": ["Von", "we\u00b7jen", ":", "sich", "die", "Ba\u00b7cken", "pu\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$.", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "un nachts mit fremde Kerle ludern \u2013", "tokens": ["un", "nachts", "mit", "frem\u00b7de", "Ker\u00b7le", "lu\u00b7dern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "man wee\u00df doch, wat det hei\u00dfen soll!", "tokens": ["man", "wee\u00df", "doch", ",", "wat", "det", "hei\u00b7\u00dfen", "soll", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "PWS", "PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer Ohren hat, kann manches h\u00f6ren . . .", "tokens": ["Wer", "Oh\u00b7ren", "hat", ",", "kann", "man\u00b7ches", "h\u00f6\u00b7ren", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "VMFIN", "PIS", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbdet k\u00f6nn wa allesamt beschw\u00f6rn \u2013", "tokens": ["\u00bb", "det", "k\u00f6nn", "wa", "al\u00b7le\u00b7samt", "be\u00b7schw\u00f6rn", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "er haut ihr nachts den Buckel voll!", "tokens": ["er", "haut", "ihr", "nachts", "den", "Bu\u00b7ckel", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Frau Grimkasch sacht auch.\u00ab", "tokens": ["Frau", "Grim\u00b7kasch", "sacht", "auch", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Frau Grimkasch hats von Frollein Kl\u00fcber,", "tokens": ["Frau", "Grim\u00b7kasch", "hats", "von", "Frol\u00b7lein", "Kl\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die wohnt Jeorjen jejen\u00fcber,", "tokens": ["die", "wohnt", "Jeor\u00b7jen", "je\u00b7je\u00b7n\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NE", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wer richtich kieken kann, der sieht.", "tokens": ["wer", "rich\u00b7tich", "kie\u00b7ken", "kann", ",", "der", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frau Grimkasch sacht noch uffn Flure:", "tokens": ["Frau", "Grim\u00b7kasch", "sacht", "noch", "uffn", "Flu\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u00bbna, wissen Se, die olle Hure . . . !\u00ab", "tokens": ["\u00bb", "na", ",", "wis\u00b7sen", "Se", ",", "die", "ol\u00b7le", "Hu\u00b7re", ".", ".", ".", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "NE", "$,", "ART", "ADJA", "NN", "$.", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "denn jehn se alle nach Moabit.", "tokens": ["denn", "jehn", "se", "al\u00b7le", "nach", "Mo\u00b7a\u00b7bit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "APPR", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Morjens halb zehn, zweiter Stock.", "tokens": ["Mor\u00b7jens", "halb", "zehn", ",", "zwei\u00b7ter", "Stock", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "CARD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da stehn se nu wie Orjelpfeifen;", "tokens": ["Da", "stehn", "se", "nu", "wie", "Or\u00b7jel\u00b7pfei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Weiba fangen an zu keifen,", "tokens": ["die", "Wei\u00b7ba", "fan\u00b7gen", "an", "zu", "kei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der Richter ruft: \u00bbImmer eine nur!\u00ab", "tokens": ["der", "Rich\u00b7ter", "ruft", ":", "\u00bb", "Im\u00b7mer", "ei\u00b7ne", "nur", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADV", "ART", "ADV", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Det sind nu Fraun von Kommenisten,", "tokens": ["Det", "sind", "nu", "Fraun", "von", "Kom\u00b7me\u00b7nis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "von Jelben un von Sozialisten . . .", "tokens": ["von", "Jel\u00b7ben", "un", "von", "So\u00b7zi\u00b7a\u00b7lis\u00b7ten", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PDAT", "FM", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "hier is det allens eine Tour.", "tokens": ["hier", "is", "det", "al\u00b7lens", "ei\u00b7ne", "Tour", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "PDS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn nischt jreift so det Herze an", "tokens": ["Denn", "nischt", "jreift", "so", "det", "Her\u00b7ze", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "PDS", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "wie die Sorje um den Nebenmann.", "tokens": ["wie", "die", "Sor\u00b7je", "um", "den", "Ne\u00b7ben\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Nu wird man die Pochtjehsche h\u00f6ren.", "tokens": ["Nu", "wird", "man", "die", "Pocht\u00b7jeh\u00b7sche", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbjawoll! Det kann ick jlatt beschw\u00f6ren!", "tokens": ["\u00bb", "ja\u00b7woll", "!", "Det", "kann", "ick", "jlatt", "be\u00b7schw\u00f6\u00b7ren", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "NE", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der kleene Horst stand ooch dabei!", "tokens": ["Der", "klee\u00b7ne", "Horst", "stand", "o\u00b7och", "da\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Frau Grimkasch sacht, die Kl\u00fcbern h\u00e4tte", "tokens": ["Frau", "Grim\u00b7kasch", "sacht", ",", "die", "Kl\u00fc\u00b7bern", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "die beiden \u00fcberrascht int Bette \u2013", "tokens": ["die", "bei\u00b7den", "\u00fc\u00b7berr\u00b7ascht", "int", "Bet\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und det Klosett wah auch nich frei!", "tokens": ["und", "det", "Klo\u00b7sett", "wah", "auch", "nich", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VVFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "So wahr mir Gott helfe!\u00ab", "tokens": ["So", "wahr", "mir", "Gott", "hel\u00b7fe", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PPER", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Richter schreibt det in die Biecher.", "tokens": ["Der", "Rich\u00b7ter", "schreibt", "det", "in", "die", "Bie\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Staatsanwalt mit seinen Riecher . . .", "tokens": ["Der", "Staats\u00b7an\u00b7walt", "mit", "sei\u00b7nen", "Rie\u00b7cher", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Meineidsverfahren! Alle Mann.", "tokens": ["Mein\u00b7eids\u00b7ver\u00b7fah\u00b7ren", "!", "Al\u00b7le", "Mann", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frau Grimkasch. Lottchen mit de Pr\u00fcjel,", "tokens": ["Frau", "Grim\u00b7kasch", ".", "Lott\u00b7chen", "mit", "de", "Pr\u00fc\u00b7jel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "der janze linke Seitenfl\u00fcjel \u2013", "tokens": ["der", "jan\u00b7ze", "lin\u00b7ke", "Sei\u00b7ten\u00b7fl\u00fc\u00b7jel", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die treten alle nochmah an.", "tokens": ["die", "tre\u00b7ten", "al\u00b7le", "noch\u00b7mah", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Acht Jahre Zuchthaus.", "tokens": ["Acht", "Jah\u00b7re", "Zucht\u00b7haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Wat nehmlich unsa Staat ist heute \u2013:", "tokens": ["Wat", "nehm\u00b7lich", "un\u00b7sa", "Staat", "ist", "heu\u00b7te", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VAFIN", "ADV", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "pisaken sone kleinen Leute,", "tokens": ["pi\u00b7sa\u00b7ken", "so\u00b7ne", "klei\u00b7nen", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVINF", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "det kann er n\u00e4mich meisterlich.", "tokens": ["det", "kann", "er", "n\u00e4\u00b7mich", "meis\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "A seine Deutschen Arbeit jehm", "tokens": ["A", "sei\u00b7ne", "Deut\u00b7schen", "Ar\u00b7beit", "jehm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "un Licht un Luft un jutet Lehm . . .", "tokens": ["un", "Licht", "un", "Luft", "un", "ju\u00b7tet", "Lehm", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "det kann er nich.", "tokens": ["det", "kann", "er", "nich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Det kann er nich.", "tokens": ["Det", "kann", "er", "nich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}