{"textgrid.poem.63170": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Die Ballade vom Schlaf der Kindheit", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scheuche nicht den Schlaf des Kindes", "tokens": ["Scheu\u00b7che", "nicht", "den", "Schlaf", "des", "Kin\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der schwarzen Bucht.", "tokens": ["In", "der", "schwar\u00b7zen", "Bucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In den Zweigen des erwachten Windes", "tokens": ["In", "den", "Zwei\u00b7gen", "des", "er\u00b7wach\u00b7ten", "Win\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00e4ngt er hell wie eine runde Frucht.", "tokens": ["H\u00e4ngt", "er", "hell", "wie", "ei\u00b7ne", "run\u00b7de", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sonne w\u00e4rmt sich an des Nackens Spiegel,", "tokens": ["Son\u00b7ne", "w\u00e4rmt", "sich", "an", "des", "Na\u00b7ckens", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Echo strahlt in der erf\u00fcllten Flut,", "tokens": ["E\u00b7cho", "strahlt", "in", "der", "er\u00b7f\u00fcll\u00b7ten", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Venus w\u00fcnscht sich leichte Fl\u00fcgel,", "tokens": ["Ve\u00b7nus", "w\u00fcnscht", "sich", "leich\u00b7te", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo er in des Spieles Barke ruht.", "tokens": ["Wo", "er", "in", "des", "Spie\u00b7les", "Bar\u00b7ke", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Jage nicht den Knaben in die Schule", "tokens": ["Ja\u00b7ge", "nicht", "den", "Kna\u00b7ben", "in", "die", "Schu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fr\u00fch um sieben, wenn der Ofen kalt.", "tokens": ["Fr\u00fch", "um", "sie\u00b7ben", ",", "wenn", "der", "O\u00b7fen", "kalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "CARD", "$,", "KOUS", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00e4sslich hockt er an der Arbeit Spule", "tokens": ["H\u00e4ss\u00b7lich", "hockt", "er", "an", "der", "Ar\u00b7beit", "Spu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und zerschmettert von des Lehrers Gramgewalt.", "tokens": ["Und", "zer\u00b7schmet\u00b7tert", "von", "des", "Leh\u00b7rers", "Gram\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Sieh: an seinen langen schwarzen Wimpern", "tokens": ["Sieh", ":", "an", "sei\u00b7nen", "lan\u00b7gen", "schwar\u00b7zen", "Wim\u00b7pern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00e4ngt ein schmaler Schatten noch das Bild.", "tokens": ["H\u00e4ngt", "ein", "schma\u00b7ler", "Schat\u00b7ten", "noch", "das", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in seine wachen Qualen klimpern", "tokens": ["Und", "in", "sei\u00b7ne", "wa\u00b7chen", "Qua\u00b7len", "klim\u00b7pern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mondgesang und Schwert und Harfenschild.", "tokens": ["Mond\u00b7ge\u00b7sang", "und", "Schwert", "und", "Har\u00b7fen\u00b7schild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}