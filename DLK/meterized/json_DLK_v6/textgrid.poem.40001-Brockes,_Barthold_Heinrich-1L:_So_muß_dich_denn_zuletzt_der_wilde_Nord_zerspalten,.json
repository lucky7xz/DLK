{"textgrid.poem.40001": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: So mu\u00df dich denn zuletzt der wilde Nord zerspalten,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So mu\u00df dich denn zuletzt der wilde Nord zerspalten,", "tokens": ["So", "mu\u00df", "dich", "denn", "zu\u00b7letzt", "der", "wil\u00b7de", "Nord", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da dein Verdienst, wodurch du dich erhalten,", "tokens": ["Da", "dein", "Ver\u00b7dienst", ",", "wo\u00b7durch", "du", "dich", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Beil oft von dir abgekehrt,", "tokens": ["Das", "Beil", "oft", "von", "dir", "ab\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sonst dein Stand die Durchsicht mir verwehrt?", "tokens": ["Weil", "sonst", "dein", "Stand", "die", "Durch\u00b7sicht", "mir", "ver\u00b7wehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ob ich nun gleich dadurch, bey deinem Scheiden,", "tokens": ["Ob", "ich", "nun", "gleich", "da\u00b7durch", ",", "bey", "dei\u00b7nem", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PAV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fast mehr gewonnen, als verlohren;", "tokens": ["Fast", "mehr", "ge\u00b7won\u00b7nen", ",", "als", "ver\u00b7loh\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So seh ich dich doch, mit betr\u00fcbten Freuden,", "tokens": ["So", "seh", "ich", "dich", "doch", ",", "mit", "be\u00b7tr\u00fcb\u00b7ten", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In deinem Lager an.", "tokens": ["In", "dei\u00b7nem", "La\u00b7ger", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Es hat dich dein Verdienst besch\u00fctzet:", "tokens": ["Es", "hat", "dich", "dein", "Ver\u00b7dienst", "be\u00b7sch\u00fct\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die\u00df dein Verdienst begleitet dich", "tokens": ["Die\u00df", "dein", "Ver\u00b7dienst", "be\u00b7glei\u00b7tet", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zu der Zeit auch, da grimmiglich", "tokens": ["Zu", "der", "Zeit", "auch", ",", "da", "grim\u00b7mig\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "KOUS", "ADJD"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Ein Wetter auf dich st\u00fcrmt und blitzet.", "tokens": ["Ein", "Wet\u00b7ter", "auf", "dich", "st\u00fcrmt", "und", "blit\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Dein l\u00e4ngstgeborst'ner Stamm hat eh nicht brennen wollen,", "tokens": ["Dein", "l\u00e4ngst\u00b7ge\u00b7bor\u00b7st'\u00b7ner", "Stamm", "hat", "eh", "nicht", "bren\u00b7nen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "KOUS", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Als bis du mir zu guter letzt", "tokens": ["Als", "bis", "du", "mir", "zu", "gu\u00b7ter", "letzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "PPER", "APPR", "ADJA", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das, was ich an dir hoch gesch\u00e4tzt,", "tokens": ["Das", ",", "was", "ich", "an", "dir", "hoch", "ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "APPR", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die grossen Kirschen reif hast k\u00f6nnen zollen.", "tokens": ["Die", "gros\u00b7sen", "Kir\u00b7schen", "reif", "hast", "k\u00f6n\u00b7nen", "zol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Kinder, die sich bis daher,", "tokens": ["Die", "Kin\u00b7der", ",", "die", "sich", "bis", "da\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit aufgeschlag'nem Aug', an deiner Frucht ergetzet,", "tokens": ["Mit", "auf\u00b7ge\u00b7schlag'\u00b7nem", "Aug'", ",", "an", "dei\u00b7ner", "Frucht", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Betr\u00fcben sich; doch freuen sie sich mehr,", "tokens": ["Be\u00b7tr\u00fc\u00b7ben", "sich", ";", "doch", "freu\u00b7en", "sie", "sich", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Indem sie ihren Wunsch, die reifen Kirschen nun,", "tokens": ["In\u00b7dem", "sie", "ih\u00b7ren", "Wunsch", ",", "die", "rei\u00b7fen", "Kir\u00b7schen", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wodurch dein Haupt bisher sich pflag zu schm\u00fccken,", "tokens": ["Wo\u00b7durch", "dein", "Haupt", "bis\u00b7her", "sich", "pflag", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "PRF", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Itzund, wie sie mit Jauchzen thun,", "tokens": ["It\u00b7zund", ",", "wie", "sie", "mit", "Jauch\u00b7zen", "thun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In deinen Zweigen selber pfl\u00fccken.", "tokens": ["In", "dei\u00b7nen", "Zwei\u00b7gen", "sel\u00b7ber", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie k\u00f6nnen nunmehr, ohn' Gefahr,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nun\u00b7mehr", ",", "ohn'", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auf deinen ehedem erhab'nen Gipfel steigen.", "tokens": ["Auf", "dei\u00b7nen", "e\u00b7he\u00b7dem", "er\u00b7hab'\u00b7nen", "Gip\u00b7fel", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bald halb verdeckt, bald gantz und gar", "tokens": ["Bald", "halb", "ver\u00b7deckt", ",", "bald", "gantz", "und", "gar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht man sie in den gr\u00fcnen Zweigen,", "tokens": ["Sieht", "man", "sie", "in", "den", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit kindischem Gew\u00fchl und frohem Lermen,", "tokens": ["Mit", "kin\u00b7di\u00b7schem", "Ge\u00b7w\u00fchl", "und", "fro\u00b7hem", "Ler\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gesch\u00e4fftig schlupfen, h\u00fcpfen, schw\u00e4rmen.", "tokens": ["Ge\u00b7sch\u00e4ff\u00b7tig", "schlup\u00b7fen", ",", "h\u00fcp\u00b7fen", ",", "schw\u00e4r\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kein einziger von ihnen denckt daran,", "tokens": ["Kein", "ein\u00b7zi\u00b7ger", "von", "ih\u00b7nen", "denckt", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "APPR", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie es nun auch das letzte mahl,", "tokens": ["Wie", "es", "nun", "auch", "das", "letz\u00b7te", "mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er der s\u00fcssen Kirschen Zahl", "tokens": ["Da\u00df", "er", "der", "s\u00fcs\u00b7sen", "Kir\u00b7schen", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von diesem Baume pfl\u00fccken kann.", "tokens": ["Von", "die\u00b7sem", "Bau\u00b7me", "pfl\u00fc\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie wissen nicht, da\u00df oft Verdru\u00df,", "tokens": ["Sie", "wis\u00b7sen", "nicht", ",", "da\u00df", "oft", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auch aus der Lust so gar, entspringet,", "tokens": ["Auch", "aus", "der", "Lust", "so", "gar", ",", "ent\u00b7sprin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und da\u00df ein kurtzer Ueberflu\u00df", "tokens": ["Und", "da\u00df", "ein", "kurt\u00b7zer", "Ue\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Oft einen langen Mangel bringet.", "tokens": ["Oft", "ei\u00b7nen", "lan\u00b7gen", "Man\u00b7gel", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}