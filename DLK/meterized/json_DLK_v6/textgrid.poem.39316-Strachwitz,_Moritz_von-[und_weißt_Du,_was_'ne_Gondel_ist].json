{"textgrid.poem.39316": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[und wei\u00dft Du, was 'ne Gondel ist]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wei\u00dft Du, was 'ne Gondel ist,", "tokens": ["Und", "wei\u00dft", "Du", ",", "was", "'ne", "Gon\u00b7del", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie sich's drinnen wiegt?", "tokens": ["Und", "wie", "sich's", "drin\u00b7nen", "wiegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ding, das kaum die Woge k\u00fc\u00dft,", "tokens": ["Ein", "Ding", ",", "das", "kaum", "die", "Wo\u00b7ge", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn's zierlich dr\u00fcber fliegt!", "tokens": ["Wenn's", "zier\u00b7lich", "dr\u00fc\u00b7ber", "fliegt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie schwebt so gleich, Du ruhst so weich,", "tokens": ["Sie", "schwebt", "so", "gleich", ",", "Du", "ruhst", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der \u00c4ther liegt im Meer,", "tokens": ["Der", "\u00c4\u00b7ther", "liegt", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du denkst, Du schwimmst im Himmelreich,", "tokens": ["Du", "denkst", ",", "Du", "schwimmst", "im", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Sterne um Dich her!", "tokens": ["Die", "Ster\u00b7ne", "um", "Dich", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sei mir gesegnet, schwarzer Schwan,", "tokens": ["Sei", "mir", "ge\u00b7seg\u00b7net", ",", "schwar\u00b7zer", "Schwan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie hast du mich verw\u00f6hnt \u2013", "tokens": ["Wie", "hast", "du", "mich", "ver\u00b7w\u00f6hnt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O weh! das ist die Eisenbahn,", "tokens": ["O", "weh", "!", "das", "ist", "die", "Ei\u00b7sen\u00b7bahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ha, wie das keucht und st\u00f6hnt!", "tokens": ["Ha", ",", "wie", "das", "keucht", "und", "st\u00f6hnt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Das Ohr ist taub, das Herz ist matt,", "tokens": ["Das", "Ohr", "ist", "taub", ",", "das", "Herz", "ist", "matt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun rumpelt es von dann' \u2013", "tokens": ["Nun", "rum\u00b7pelt", "es", "von", "dann'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wer das Ding erfunden hat,", "tokens": ["Und", "wer", "das", "Ding", "er\u00b7fun\u00b7den", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der war \u2013 ein gro\u00dfer Mann!", "tokens": ["Der", "war", "\u2013", "ein", "gro\u00b7\u00dfer", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}