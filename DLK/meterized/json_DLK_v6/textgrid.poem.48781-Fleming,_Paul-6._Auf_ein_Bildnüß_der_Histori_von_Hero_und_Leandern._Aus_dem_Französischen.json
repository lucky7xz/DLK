{"textgrid.poem.48781": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Auf ein Bildn\u00fc\u00df der Histori von Hero und Leandern. Aus dem Franz\u00f6sischen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leandern bin ich ganz in meiner Liebe gleich,", "tokens": ["Le\u00b7an\u00b7dern", "bin", "ich", "ganz", "in", "mei\u00b7ner", "Lie\u00b7be", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "empfindend, wie auch er, viel tausent harter Plagen.", "tokens": ["emp\u00b7fin\u00b7dend", ",", "wie", "auch", "er", ",", "viel", "tau\u00b7sent", "har\u00b7ter", "Pla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ADV", "PPER", "$,", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wellen und der Wind, die hie\u00dfen ihn verzagen.", "tokens": ["Die", "Wel\u00b7len", "und", "der", "Wind", ",", "die", "hie\u00b7\u00dfen", "ihn", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An so viel Herzensangst bin auch ich, Armer, reich.", "tokens": ["An", "so", "viel", "Her\u00b7zen\u00b7sangst", "bin", "auch", "ich", ",", "Ar\u00b7mer", ",", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "VAFIN", "ADV", "PPER", "$,", "NN", "$,", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Kein Wetter, keine Furcht, kein Tod, der macht' ihn bleich.", "tokens": ["Kein", "Wet\u00b7ter", ",", "kei\u00b7ne", "Furcht", ",", "kein", "Tod", ",", "der", "macht'", "ihn", "bleich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er mu\u00dfte dennoch sich nach seiner Liebe wagen.", "tokens": ["Er", "mu\u00df\u00b7te", "den\u00b7noch", "sich", "nach", "sei\u00b7ner", "Lie\u00b7be", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So will ich unverwandt auch auf mein Liebstes jagen.", "tokens": ["So", "will", "ich", "un\u00b7ver\u00b7wandt", "auch", "auf", "mein", "Liebs\u00b7tes", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tut, Feinde, was ihr wollt, was frag' ich wol nach euch?", "tokens": ["Tut", ",", "Fein\u00b7de", ",", "was", "ihr", "wollt", ",", "was", "frag'", "ich", "wol", "nach", "euch", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWS", "PPER", "VMFIN", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In Einem scheinen wir nur nicht sein zu vergleichen;", "tokens": ["In", "Ei\u00b7nem", "schei\u00b7nen", "wir", "nur", "nicht", "sein", "zu", "ver\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ADV", "PTKNEG", "VAINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sein liebliches Gestirn', als fester Liebe Zeichen,", "tokens": ["sein", "lieb\u00b7li\u00b7ches", "Ge\u00b7stirn'", ",", "als", "fes\u00b7ter", "Lie\u00b7be", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "kunt' ihm f\u00fcr Wind und Tod sein kein Beh\u00e4ltn\u00fc\u00df nicht;", "tokens": ["kunt'", "ihm", "f\u00fcr", "Wind", "und", "Tod", "sein", "kein", "Be\u00b7h\u00e4lt\u00b7n\u00fc\u00df", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "PPOSAT", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "du mehr als g\u00f6tlichs Licht zwei himlischer Laternen,", "tokens": ["du", "mehr", "als", "g\u00f6t\u00b7lichs", "Licht", "zwei", "him\u00b7li\u00b7scher", "La\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "KOKOM", "ADJA", "NN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "machst, da\u00df der Sturm und Strom an mir verspielen lernen", "tokens": ["machst", ",", "da\u00df", "der", "Sturm", "und", "Strom", "an", "mir", "ver\u00b7spie\u00b7len", "ler\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zuwider aller Macht, die sich an mir zerbricht.", "tokens": ["zu\u00b7wi\u00b7der", "al\u00b7ler", "Macht", ",", "die", "sich", "an", "mir", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}