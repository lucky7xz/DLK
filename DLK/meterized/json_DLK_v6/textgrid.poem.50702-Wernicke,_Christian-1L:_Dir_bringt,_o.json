{"textgrid.poem.50702": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir bringt, o ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir bringt, o ", "tokens": ["Dir", "bringt", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "FM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Als eines Dichterlings verwelktes Lorbeerreis;", "tokens": ["Als", "ei\u00b7nes", "Dich\u00b7ter\u00b7lings", "ver\u00b7welk\u00b7tes", "Lor\u00b7beer\u00b7reis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Selbst dein Verleger giebt dir nichts,", "tokens": ["Selbst", "dein", "Ver\u00b7le\u00b7ger", "giebt", "dir", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Adam's Fluch ist dir gedoppelt zugemessen:", "tokens": ["Und", "A\u00b7dam's", "Fluch", "ist", "dir", "ge\u00b7dop\u00b7pelt", "zu\u00b7ge\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du mu\u00dft im ", "tokens": ["Du", "mu\u00dft", "im"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}}}}}