{"textgrid.poem.67242": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Pindar, den mit der F\u00fclle der Gaben die G\u00f6tter, der Muse", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pindar, den mit der F\u00fclle der Gaben die G\u00f6tter, der Muse", "tokens": ["Pin\u00b7dar", ",", "den", "mit", "der", "F\u00fcl\u00b7le", "der", "Ga\u00b7ben", "die", "G\u00f6t\u00b7ter", ",", "der", "Mu\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hohen Gaben, und wen, reicher gesegnet als ihn?", "tokens": ["Ho\u00b7hen", "Ga\u00b7ben", ",", "und", "wen", ",", "rei\u00b7cher", "ge\u00b7seg\u00b7net", "als", "ihn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PWS", "$,", "ADJD", "VVPP", "KOKOM", "PPER", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Pindar hub die H\u00e4nde, der Greis, gen Himmel und flehte:", "tokens": ["Pin\u00b7dar", "hub", "die", "H\u00e4n\u00b7de", ",", "der", "Greis", ",", "gen", "Him\u00b7mel", "und", "fleh\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "APPR", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "\u00bbwas am besten mir frommt, gebet, o G\u00f6tter, mir das!\u00ab", "tokens": ["\u00bb", "was", "am", "bes\u00b7ten", "mir", "frommt", ",", "ge\u00b7bet", ",", "o", "G\u00f6t\u00b7ter", ",", "mir", "das", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PTKA", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "$,", "FM", "NN", "$,", "PPER", "PDS", "$.", "$("], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Fleht's und nicht lange so sank ihm das Haupt an den Busen des Freundes,", "tokens": ["Fleht's", "und", "nicht", "lan\u00b7ge", "so", "sank", "ihm", "das", "Haupt", "an", "den", "Bu\u00b7sen", "des", "Freun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PTKNEG", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Leiser athmete der, da\u00df nicht entschwebe der Schlaf.", "tokens": ["Lei\u00b7ser", "ath\u00b7me\u00b7te", "der", ",", "da\u00df", "nicht", "ent\u00b7schwe\u00b7be", "der", "Schlaf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "$,", "KOUS", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Ach, es war des Schlummers Bruder! \u2013 Seliger! hier nicht,", "tokens": ["Ach", ",", "es", "war", "des", "Schlum\u00b7mers", "Bru\u00b7der", "!", "\u2013", "Se\u00b7li\u00b7ger", "!", "hier", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$.", "$(", "NN", "$.", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Dort im Elysium ward sch\u00f6ner gekr\u00f6net dein Wunsch!", "tokens": ["Dort", "im", "E\u00b7ly\u00b7si\u00b7um", "ward", "sch\u00f6\u00b7ner", "ge\u00b7kr\u00f6\u00b7net", "dein", "Wunsch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ADJD", "VVPP", "PPOSAT", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}