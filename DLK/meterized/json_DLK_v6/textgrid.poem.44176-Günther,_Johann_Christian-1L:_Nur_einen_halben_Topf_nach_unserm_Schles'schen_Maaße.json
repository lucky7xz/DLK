{"textgrid.poem.44176": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur einen halben Topf nach unserm Schles'schen Maa\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur einen halben Topf nach unserm Schles'schen Maa\u00dfe", "tokens": ["Nur", "ei\u00b7nen", "hal\u00b7ben", "Topf", "nach", "un\u00b7serm", "Schles'\u00b7schen", "Maa\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von gestrigem Confect, das in die Kammer fiel,", "tokens": ["Von", "ge\u00b7stri\u00b7gem", "Con\u00b7fect", ",", "das", "in", "die", "Kam\u00b7mer", "fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Erbittet jezt ein Knecht/Freund auf dieser Leiter-Stra\u00dfe,", "tokens": ["Er\u00b7bit\u00b7tet", "jezt", "ein", "Knecht", "/", "Freund", "auf", "die\u00b7ser", "Lei\u00b7ter\u00b7Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der mehr ein Diener ist, als er es hei\u00dfen will.", "tokens": ["Der", "mehr", "ein", "Die\u00b7ner", "ist", ",", "als", "er", "es", "hei\u00b7\u00dfen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gewi\u00dfe Gr\u00fcnde zwingen mich,", "tokens": ["Ge\u00b7wi\u00b7\u00dfe", "Gr\u00fcn\u00b7de", "zwin\u00b7gen", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4r dieses nicht, so holt ich dich.", "tokens": ["W\u00e4r", "die\u00b7ses", "nicht", ",", "so", "holt", "ich", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}