{"dta.poem.18807": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Suleika .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! um deine feuchten Schwingen,", "tokens": ["Ach", "!", "um", "dei\u00b7ne", "feuch\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "West, wie sehr ich dich beneide:", "tokens": ["West", ",", "wie", "sehr", "ich", "dich", "be\u00b7nei\u00b7de", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du kannst ihm Kunde bringen", "tokens": ["Denn", "du", "kannst", "ihm", "Kun\u00b7de", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich in der Trennung leide.", "tokens": ["Was", "ich", "in", "der", "Tren\u00b7nung", "lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Bewegung deiner Fl\u00fcgel", "tokens": ["Die", "Be\u00b7we\u00b7gung", "dei\u00b7ner", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weckt im Busen stilles Sehnen,", "tokens": ["Weckt", "im", "Bu\u00b7sen", "stil\u00b7les", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blumen, Augen, Wald und H\u00fcgel", "tokens": ["Blu\u00b7men", ",", "Au\u00b7gen", ",", "Wald", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehn bey deinem Hauch in Thr\u00e4nen.", "tokens": ["Stehn", "bey", "dei\u00b7nem", "Hauch", "in", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch dein mildes sanftes Wehen", "tokens": ["Doch", "dein", "mil\u00b7des", "sanf\u00b7tes", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchlt die wunden Augenlieder;", "tokens": ["K\u00fchlt", "die", "wun\u00b7den", "Au\u00b7gen\u00b7lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach f\u00fcr Leid m\u00fcsst\u2019 ich vergehen,", "tokens": ["Ach", "f\u00fcr", "Leid", "m\u00fcsst'", "ich", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hofft\u2019 ich nicht zu sehn ihn wieder.", "tokens": ["Hofft'", "ich", "nicht", "zu", "sehn", "ihn", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eile denn zu meinem Lieben,", "tokens": ["Ei\u00b7le", "denn", "zu", "mei\u00b7nem", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spreche sanft zu seinem Herzen;", "tokens": ["Spre\u00b7che", "sanft", "zu", "sei\u00b7nem", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch vermeid\u2019 ihn zu betr\u00fcben", "tokens": ["Doch", "ver\u00b7meid'", "ihn", "zu", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verbirg ihm meine Schmerzen.", "tokens": ["Und", "ver\u00b7birg", "ihm", "mei\u00b7ne", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sag ihm, aber sag\u2019s bescheiden:", "tokens": ["Sag", "ihm", ",", "a\u00b7ber", "sag's", "be\u00b7schei\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "NE", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Liebe sey mein Leben,", "tokens": ["Sei\u00b7ne", "Lie\u00b7be", "sey", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudiges Gef\u00fchl von beyden", "tokens": ["Freu\u00b7di\u00b7ges", "Ge\u00b7f\u00fchl", "von", "bey\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird mir seine N\u00e4he geben.", "tokens": ["Wird", "mir", "sei\u00b7ne", "N\u00e4\u00b7he", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}