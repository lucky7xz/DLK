{"textgrid.poem.53050": {"metadata": {"author": {"name": "Lohenstein, Daniel Casper von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr d\u00fcrres Volk, leblose Leute, todte Stumme,", "genre": "verse", "period": "N.A.", "pub_year": 1659, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr d\u00fcrres Volk, leblose Leute, todte Stumme,", "tokens": ["Ihr", "d\u00fcr\u00b7res", "Volk", ",", "leb\u00b7lo\u00b7se", "Leu\u00b7te", ",", "tod\u00b7te", "Stum\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Sterblichen, die ihr euch w\u00fcnscht zu leben,", "tokens": ["Ihr", "Sterb\u00b7li\u00b7chen", ",", "die", "ihr", "euch", "w\u00fcnscht", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ihr den hellen Tag f\u00fcr Nacht,", "tokens": ["Die", "ihr", "den", "hel\u00b7len", "Tag", "f\u00fcr", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Krone f\u00fcr Gef\u00e4ngni\u00df, Freiheit f\u00fcr die Ketten,", "tokens": ["Die", "Kro\u00b7ne", "f\u00fcr", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", ",", "Frei\u00b7heit", "f\u00fcr", "die", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "F\u00fcr Kerker Ruhm, f\u00fcr Wenig Alles alle,", "tokens": ["F\u00fcr", "Ker\u00b7ker", "Ruhm", ",", "f\u00fcr", "We\u00b7nig", "Al\u00b7les", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "PIAT", "PIS", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ihr f\u00fcr Nebel Glanz, f\u00fcr D\u00fcnste Sonnenschein,", "tokens": ["Die", "ihr", "f\u00fcr", "Ne\u00b7bel", "Glanz", ",", "f\u00fcr", "D\u00fcns\u00b7te", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "F\u00fcr's Grab den Thron, den Zepter f\u00fcr das Grabescheit,", "tokens": ["F\u00fcr's", "Grab", "den", "Thron", ",", "den", "Zep\u00b7ter", "f\u00fcr", "das", "Gra\u00b7be\u00b7scheit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "F\u00fcr Nichts nicht Viel, den Himmel f\u00fcr die Erden,", "tokens": ["F\u00fcr", "Nichts", "nicht", "Viel", ",", "den", "Him\u00b7mel", "f\u00fcr", "die", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKNEG", "ADV", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "F\u00fcr Asche Gold,", "tokens": ["F\u00fcr", "A\u00b7sche", "Gold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Das Leben f\u00fcr den Tod,", "tokens": ["Das", "Le\u00b7ben", "f\u00fcr", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die Seide f\u00fcr den Koth,", "tokens": ["Die", "Sei\u00b7de", "f\u00fcr", "den", "Koth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Verwechseln wollt!", "tokens": ["Ver\u00b7wech\u00b7seln", "wollt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Ihr Menschen, die ihr G\u00f6tter wollet werden,", "tokens": ["Ihr", "Men\u00b7schen", ",", "die", "ihr", "G\u00f6t\u00b7ter", "wol\u00b7let", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die ihr den Kitzel schn\u00f6der Eitelkeit,", "tokens": ["Die", "ihr", "den", "Kit\u00b7zel", "schn\u00f6\u00b7der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Tr\u00e4ume Nichts, der Ehrsucht s\u00fc\u00dfe Pein,", "tokens": ["Der", "Tr\u00e4u\u00b7me", "Nichts", ",", "der", "Ehr\u00b7sucht", "s\u00fc\u00b7\u00dfe", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Wehmuth Wermuth, der Woll\u00fcste Galle", "tokens": ["Der", "Weh\u00b7muth", "Wer\u00b7muth", ",", "der", "Wol\u00b7l\u00fcs\u00b7te", "Gal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Verschm\u00e4ht und euch vom Dorn' auf Rosen wollet betten!", "tokens": ["Ver\u00b7schm\u00e4ht", "und", "euch", "vom", "Dorn'", "auf", "Ro\u00b7sen", "wol\u00b7let", "bet\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PPER", "APPRART", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Kommt, kommt! hier segelt her und macht", "tokens": ["Kommt", ",", "kommt", "!", "hier", "se\u00b7gelt", "her", "und", "macht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "ADV", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Den Lebensnachen an, wollt ihr erheben", "tokens": ["Den", "Le\u00b7bens\u00b7na\u00b7chen", "an", ",", "wollt", "ihr", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Den Preis der Ewigkeit, das Wahre f\u00fcr das Dumme!", "tokens": ["Den", "Preis", "der", "E\u00b7wig\u00b7keit", ",", "das", "Wah\u00b7re", "f\u00fcr", "das", "Dum\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}