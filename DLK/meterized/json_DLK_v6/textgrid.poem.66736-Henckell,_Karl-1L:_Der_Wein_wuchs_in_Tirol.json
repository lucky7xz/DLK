{"textgrid.poem.66736": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Wein wuchs in Tirol", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wein wuchs in Tirol", "tokens": ["Der", "Wein", "wuchs", "in", "Ti\u00b7rol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und nicht zu meinem Leide \u2013", "tokens": ["Und", "nicht", "zu", "mei\u00b7nem", "Lei\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihn trank mein Ahne wohl,", "tokens": ["Ihn", "trank", "mein", "Ah\u00b7ne", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der von der Vogelweide.", "tokens": ["Der", "von", "der", "Vo\u00b7gel\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr Walter war daheim", "tokens": ["Herr", "Wal\u00b7ter", "war", "da\u00b7heim"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bei Brixen oder Bozen,", "tokens": ["Bei", "Bri\u00b7xen", "o\u00b7der", "Bo\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo von dem starken Seim", "tokens": ["Wo", "von", "dem", "star\u00b7ken", "Seim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die blauen Beeren strotzen.", "tokens": ["Die", "blau\u00b7en", "Bee\u00b7ren", "strot\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "So will ich als Geno\u00df", "tokens": ["So", "will", "ich", "als", "Ge\u00b7no\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ihn freundlich aufbeschw\u00f6ren,", "tokens": ["Ihn", "freund\u00b7lich", "auf\u00b7be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "In k\u00fchlem Weingescho\u00df", "tokens": ["In", "k\u00fch\u00b7lem", "Wein\u00b7ge\u00b7scho\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Soll er mein Prosit h\u00f6ren.", "tokens": ["Soll", "er", "mein", "Pro\u00b7sit", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.13": {"text": "Willkommen, edler Gast!", "tokens": ["Will\u00b7kom\u00b7men", ",", "ed\u00b7ler", "Gast", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "An diesem Tisch verweile!", "tokens": ["An", "die\u00b7sem", "Tisch", "ver\u00b7wei\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Der du ", "tokens": ["Der", "du"], "token_info": ["word", "word"], "pos": ["ART", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Erw\u00e4hlt zu deinem Heile.", "tokens": ["Er\u00b7w\u00e4hlt", "zu", "dei\u00b7nem", "Hei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Tief aus dem Weinpokal", "tokens": ["Tief", "aus", "dem", "Wein\u00b7po\u00b7kal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.18": {"text": "Der Welt hast du getrunken,", "tokens": ["Der", "Welt", "hast", "du", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Nie ward der Trank dir schal,", "tokens": ["Nie", "ward", "der", "Trank", "dir", "schal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Zu Asche nie dein Funken.", "tokens": ["Zu", "A\u00b7sche", "nie", "dein", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Der Minne Heidebett,", "tokens": ["Der", "Min\u00b7ne", "Hei\u00b7de\u00b7bett", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Die Lust der s\u00fc\u00dfen Frauen,", "tokens": ["Die", "Lust", "der", "s\u00fc\u00b7\u00dfen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Des Kampfes Ruhest\u00e4tt' \u2013", "tokens": ["Des", "Kamp\u00b7fes", "Ru\u00b7he\u00b7st\u00e4tt'", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "La\u00df dir ins Auge schauen!", "tokens": ["La\u00df", "dir", "ins", "Au\u00b7ge", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Du warst in wirrer Zeit", "tokens": ["Du", "warst", "in", "wir\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Ein lebensweiser Singer,", "tokens": ["Ein", "le\u00b7bens\u00b7wei\u00b7ser", "Sin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Mit frommer Innigkeit", "tokens": ["Mit", "from\u00b7mer", "In\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Ein tapfrer Freudenbringer.", "tokens": ["Ein", "tapf\u00b7rer", "Freu\u00b7den\u00b7brin\u00b7ger", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Du schenkst gewi\u00df auch heut", "tokens": ["Du", "schenkst", "ge\u00b7wi\u00df", "auch", "heut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Bei diesem Trunk, dem stillen,", "tokens": ["Bei", "die\u00b7sem", "Trunk", ",", "dem", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Mir, was das Herz erfreut,", "tokens": ["Mir", ",", "was", "das", "Herz", "er\u00b7freut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Und scheuchst die feigen Grillen.", "tokens": ["Und", "scheuchst", "die", "fei\u00b7gen", "Gril\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Was uns das Leben bringt", "tokens": ["Was", "uns", "das", "Le\u00b7ben", "bringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "An schmerzlichem Erfahren,", "tokens": ["An", "schmerz\u00b7li\u00b7chem", "Er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Ein kleiner Vogel singt", "tokens": ["Ein", "klei\u00b7ner", "Vo\u00b7gel", "singt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Seit vielen hundert Jahren:", "tokens": ["Seit", "vie\u00b7len", "hun\u00b7dert", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "\u00bbkommst du in d\u00fcstern Wald,", "tokens": ["\u00bb", "kommst", "du", "in", "d\u00fcs\u00b7tern", "Wald", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.38": {"text": "Und regnet's rings von P\u00fcffen", "tokens": ["Und", "reg\u00b7net's", "rings", "von", "P\u00fcf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Aus hohlem Hinterhalt \u2013", "tokens": ["Aus", "hoh\u00b7lem", "Hin\u00b7ter\u00b7halt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Freund, la\u00df dich nicht verbl\u00fcffen!", "tokens": ["Freund", ",", "la\u00df", "dich", "nicht", "ver\u00b7bl\u00fcf\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Lach in die dickste Nacht", "tokens": ["Lach", "in", "die", "dicks\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Und schreite unerschrocken", "tokens": ["Und", "schrei\u00b7te", "un\u00b7er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Zum Tort der Niedertracht!", "tokens": ["Zum", "Tort", "der", "Nie\u00b7der\u00b7tracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Sie bleibt im Finstern hocken.", "tokens": ["Sie", "bleibt", "im", "Fins\u00b7tern", "ho\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Du aber gehst getrost", "tokens": ["Du", "a\u00b7ber", "gehst", "ge\u00b7trost"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "Mit deines Liedes Segen,", "tokens": ["Mit", "dei\u00b7nes", "Lie\u00b7des", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Ob dich der Spuk umtost,", "tokens": ["Ob", "dich", "der", "Spuk", "um\u00b7tost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "Dem Morgenlicht entgegen.\u00ab", "tokens": ["Dem", "Mor\u00b7gen\u00b7licht", "ent\u00b7ge\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "In diesem Sinn, sto\u00df an,", "tokens": ["In", "die\u00b7sem", "Sinn", ",", "sto\u00df", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.50": {"text": "Herr Walter von Tirole:", "tokens": ["Herr", "Wal\u00b7ter", "von", "Ti\u00b7ro\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Wenn man drauf pfeifen kann \u2013", "tokens": ["Wenn", "man", "drauf", "pfei\u00b7fen", "kann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.52": {"text": "Der argen Welt zum Wohle!", "tokens": ["Der", "ar\u00b7gen", "Welt", "zum", "Woh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}