{"textgrid.poem.33078": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: So tief verwundet ist dies Herz \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So tief verwundet ist dies Herz \u2013", "tokens": ["So", "tief", "ver\u00b7wun\u00b7det", "ist", "dies", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "PDS", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es m\u00f6chte sich in Nacht versenken,", "tokens": ["Es", "m\u00f6ch\u00b7te", "sich", "in", "Nacht", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht sehen, h\u00f6ren und nicht denken,", "tokens": ["Nicht", "se\u00b7hen", ",", "h\u00f6\u00b7ren", "und", "nicht", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Nur f\u00fchlen seinen bitt'ren Schmerz!", "tokens": ["Nur", "f\u00fch\u00b7len", "sei\u00b7nen", "bitt'\u00b7ren", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So kostet' es ihn bis zum Grund,", "tokens": ["So", "kos\u00b7tet'", "es", "ihn", "bis", "zum", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Es m\u00fc\u00dfte langsam sich verbluten,", "tokens": ["Es", "m\u00fc\u00df\u00b7te", "lang\u00b7sam", "sich", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und aus den ausgebrannten Gluthen", "tokens": ["Und", "aus", "den", "aus\u00b7ge\u00b7brann\u00b7ten", "Glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erh\u00f6b' es sich vielleicht gesund.", "tokens": ["Er\u00b7h\u00f6b'", "es", "sich", "viel\u00b7leicht", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun aber wird der laute Tag,", "tokens": ["Nun", "a\u00b7ber", "wird", "der", "lau\u00b7te", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der ihn gesch\u00e4ftig will zerst\u00f6ren,", "tokens": ["Der", "ihn", "ge\u00b7sch\u00e4f\u00b7tig", "will", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Herzens Qual nur noch vermehren,", "tokens": ["Des", "Her\u00b7zens", "Qual", "nur", "noch", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht stark es machen, sondern schwach.", "tokens": ["Nicht", "stark", "es", "ma\u00b7chen", ",", "son\u00b7dern", "schwach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPER", "VVINF", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch sei's getragen \u2013 nach dem Wie", "tokens": ["Doch", "sei's", "ge\u00b7tra\u00b7gen", "\u2013", "nach", "dem", "Wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "VVPP", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nicht fragt der Selbstbeherrschung Wille;", "tokens": ["Nicht", "fragt", "der", "Selbst\u00b7be\u00b7herr\u00b7schung", "Wil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Aug' und Lippe, haltet stille,", "tokens": ["Nur", "Aug'", "und", "Lip\u00b7pe", ",", "hal\u00b7tet", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Das inn're Leid verrathet nie!", "tokens": ["Das", "inn'\u00b7re", "Leid", "ver\u00b7ra\u00b7thet", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}