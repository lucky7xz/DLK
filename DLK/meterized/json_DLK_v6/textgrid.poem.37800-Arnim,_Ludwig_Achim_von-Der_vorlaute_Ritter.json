{"textgrid.poem.37800": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der vorlaute Ritter", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren drey Gesellen,", "tokens": ["Es", "wa\u00b7ren", "drey", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die th\u00e4ten, was sie w\u00f6llen,", "tokens": ["Die", "th\u00e4\u00b7ten", ",", "was", "sie", "w\u00f6l\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hielten alle drey", "tokens": ["Sie", "hiel\u00b7ten", "al\u00b7le", "drey"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Viel heimlichen Rath,", "tokens": ["Viel", "heim\u00b7li\u00b7chen", "Rath", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wer wohl in dieser Nacht", "tokens": ["Wer", "wohl", "in", "die\u00b7ser", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das beste M\u00e4del h\u00e4tt.", "tokens": ["Das", "bes\u00b7te", "M\u00e4\u00b7del", "h\u00e4tt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der J\u00fcngste der darunter,", "tokens": ["Der", "J\u00fcngs\u00b7te", "der", "da\u00b7run\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der sprach da auch sehr munter,", "tokens": ["Der", "sprach", "da", "auch", "sehr", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie ihm noch gestern sp\u00e4t", "tokens": ["Wie", "ihm", "noch", "ge\u00b7stern", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ein M\u00e4del zugeredt.", "tokens": ["Ein", "M\u00e4\u00b7del", "zu\u00b7ge\u00b7redt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er stiege diese Nacht,", "tokens": ["Er", "stie\u00b7ge", "die\u00b7se", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wohl in ihr Federbett.", "tokens": ["Wohl", "in", "ihr", "Fe\u00b7der\u00b7bett", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das M\u00e4del kam geschlichen", "tokens": ["Das", "M\u00e4\u00b7del", "kam", "ge\u00b7schli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und w\u00e4re fast verblichen,", "tokens": ["Und", "w\u00e4\u00b7re", "fast", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie h\u00f6rte an der Wand,", "tokens": ["Sie", "h\u00f6r\u00b7te", "an", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur ihre eigne Schand,", "tokens": ["Nur", "ih\u00b7re", "eig\u00b7ne", "Schand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie weinte heimlich aus,", "tokens": ["Sie", "wein\u00b7te", "heim\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie lief zur\u00fcck nach Haus.", "tokens": ["Sie", "lief", "zu\u00b7r\u00fcck", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Nacht war bis zur Mitten,", "tokens": ["Die", "Nacht", "war", "bis", "zur", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Ritter kam geritten,", "tokens": ["Der", "Rit\u00b7ter", "kam", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er klopfet freundlich an,", "tokens": ["Er", "klop\u00b7fet", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem goldnen Ring:", "tokens": ["Mit", "sei\u00b7nem", "gold\u00b7nen", "Ring", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbey schl\u00e4f'st du oder wachst,", "tokens": ["\u00bb", "ey", "schl\u00e4f'st", "du", "o\u00b7der", "wachst", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mein auserw\u00e4hltes Kind.\u00ab", "tokens": ["Mein", "au\u00b7ser\u00b7w\u00e4hl\u00b7tes", "Kind", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbwas w\u00e4re, wenn ich schliefe,", "tokens": ["\u00bb", "was", "w\u00e4\u00b7re", ",", "wenn", "ich", "schlie\u00b7fe", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und dich heut nicht einlie\u00dfe?", "tokens": ["Und", "dich", "heut", "nicht", "ein\u00b7lie\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hast mir gestern sp\u00e4t", "tokens": ["Du", "hast", "mir", "ge\u00b7stern", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein falsche Red gethan.", "tokens": ["Ein", "fal\u00b7sche", "Red", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich schlafe heute Nacht,", "tokens": ["Ich", "schla\u00b7fe", "heu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn du vorm Fenster wachst.\u00ab", "tokens": ["Wenn", "du", "vorm", "Fens\u00b7ter", "wachst", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbwo soll ich denn hinreiten?", "tokens": ["\u00bb", "wo", "soll", "ich", "denn", "hin\u00b7rei\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es regnet und es schneiet,", "tokens": ["Es", "reg\u00b7net", "und", "es", "schnei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es geht ein k\u00fchler Wind,", "tokens": ["Es", "geht", "ein", "k\u00fch\u00b7ler", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nun schlafen alle Leut", "tokens": ["Nun", "schla\u00b7fen", "al\u00b7le", "Leut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und alle B\u00fcrgers Kind,", "tokens": ["Und", "al\u00b7le", "B\u00fcr\u00b7gers", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mach auf du s\u00fc\u00dfes Kind!\u00ab", "tokens": ["Mach", "auf", "du", "s\u00fc\u00b7\u00dfes", "Kind", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbreit du nach jener Stra\u00dfe,", "tokens": ["\u00bb", "reit", "du", "nach", "je\u00b7ner", "Stra\u00b7\u00dfe", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Reit du nach jener Heyde,", "tokens": ["Reit", "du", "nach", "je\u00b7ner", "Hey\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo du gekommen bist,", "tokens": ["Wo", "du", "ge\u00b7kom\u00b7men", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da liegt ein breiter Stein,", "tokens": ["Da", "liegt", "ein", "brei\u00b7ter", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Kopf darauf nur leg,", "tokens": ["Den", "Kopf", "da\u00b7rauf", "nur", "leg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ADV", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Tr\u00e4gst keine Federn weg.\u00ab", "tokens": ["Tr\u00e4gst", "kei\u00b7ne", "Fe\u00b7dern", "weg", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}