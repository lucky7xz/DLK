{"textgrid.poem.67683": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit allen seinen Kr\u00e4ften schwang", "tokens": ["Mit", "al\u00b7len", "sei\u00b7nen", "Kr\u00e4f\u00b7ten", "schwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Adler sich zur Sonne, drang", "tokens": ["Der", "Ad\u00b7ler", "sich", "zur", "Son\u00b7ne", ",", "drang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon durch die Wolken, reichte", "tokens": ["Schon", "durch", "die", "Wol\u00b7ken", ",", "reich\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum h\u00f6chsten Felsen, keuchte", "tokens": ["Zum", "h\u00f6chs\u00b7ten", "Fel\u00b7sen", ",", "keuch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sprach: \u00bbDa bin ich doch", "tokens": ["Und", "sprach", ":", "\u00bb", "Da", "bin", "ich", "doch"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Erste meines Reichs. Wer fleugt", "tokens": ["Der", "Ers\u00b7te", "mei\u00b7nes", "Reichs", ".", "Wer", "fleugt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mir nach auf diesen unbetretnen Fels? Ist noch,", "tokens": ["Mir", "nach", "auf", "die\u00b7sen", "un\u00b7be\u00b7tret\u00b7nen", "Fels", "?", "Ist", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "PDAT", "ADJA", "NN", "$.", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo ich bin, wer?\u00ab \u00bbIch etwa noch!\u00ab", "tokens": ["Wo", "ich", "bin", ",", "wer", "?", "\u00ab", "\u00bb", "Ich", "et\u00b7wa", "noch", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "PWS", "$.", "$(", "$(", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zischt's neben ihm. Er sieht zu seinen F\u00fc\u00dfen nieder:", "tokens": ["Zischt's", "ne\u00b7ben", "ihm", ".", "Er", "sieht", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Ein Erdwurm kreucht.", "tokens": ["Ein", "Erd\u00b7wurm", "kreucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "\u00bbund wir sind Br\u00fcder?", "tokens": ["\u00bb", "und", "wir", "sind", "Br\u00fc\u00b7der", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Wo kommst Du her?\u00ab \u00bbVom Schlamm.\u00ab", "tokens": ["Wo", "kommst", "Du", "her", "?", "\u00ab", "\u00bb", "Vom", "Schlamm", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "$(", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "\u00bbund wie denn her?\u00ab \u00bbEi doch!", "tokens": ["\u00bb", "und", "wie", "denn", "her", "?", "\u00ab", "\u00bb", "Ei", "doch", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "KOKOM", "ADV", "PTKVZ", "$.", "$(", "$(", "NN", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Verzeihen Sie, ich kroch.\u00ab", "tokens": ["Ver\u00b7zei\u00b7hen", "Sie", ",", "ich", "kroch", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Minister, Weiser, General", "tokens": ["Mi\u00b7nis\u00b7ter", ",", "Wei\u00b7ser", ",", "Ge\u00b7ne\u00b7ral"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Canzellar und Cardinal,", "tokens": ["Und", "Can\u00b7zel\u00b7lar", "und", "Car\u00b7di\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Eurer r\u00fchmlichen, mit edler M\u00fch und Qual", "tokens": ["Auf", "Eu\u00b7rer", "r\u00fchm\u00b7li\u00b7chen", ",", "mit", "ed\u00b7ler", "M\u00fch", "und", "Qual"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "VVINF", "$,", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erflognen H\u00f6h,", "tokens": ["E\u00b7rflog\u00b7nen", "H\u00f6h", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ihr gro\u00dfen M\u00e4nner allzumal,", "tokens": ["Ihr", "gro\u00b7\u00dfen", "M\u00e4n\u00b7ner", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Seht nicht, wer bei Euch steh':", "tokens": ["Seht", "nicht", ",", "wer", "bei", "Euch", "steh'", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Durch Kriechen kommt man hoch.", "tokens": ["Durch", "Krie\u00b7chen", "kommt", "man", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}