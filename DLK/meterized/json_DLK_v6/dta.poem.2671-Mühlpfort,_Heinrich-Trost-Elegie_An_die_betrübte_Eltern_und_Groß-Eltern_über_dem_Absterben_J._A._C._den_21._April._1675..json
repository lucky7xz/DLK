{"dta.poem.2671": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Elegie  \n  An die betr\u00fcbte Eltern und Gro\u00df-Eltern  \n \u00fcber dem Absterben J. A. C. den 21.  \n April. 1675.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Thr\u00e4nen so jetzund aus euren Augen rinnen/", "tokens": ["Die", "Thr\u00e4\u00b7nen", "so", "je\u00b7tzund", "aus", "eu\u00b7ren", "Au\u00b7gen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und noch zu guter Nacht die ", "tokens": ["Und", "noch", "zu", "gu\u00b7ter", "Nacht", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Betr\u00fcbtste/ wird kein Mensch/ als unrecht/ tadeln\nk\u00f6nnen", "tokens": ["Be\u00b7tr\u00b7\u00fcbts\u00b7te", "/", "wird", "kein", "Mensch", "/", "als", "un\u00b7recht", "/", "ta\u00b7deln", "k\u00f6n\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$(", "VAFIN", "PIAT", "NN", "$(", "KOUS", "NN", "$(", "VVINF", "VMINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er m\u00fcste den ein Fel\u00df und kalter Marmel seyn.", "tokens": ["Er", "m\u00fcs\u00b7te", "den", "ein", "Fel\u00df", "und", "kal\u00b7ter", "Mar\u00b7mel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ART", "NN", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier iockt sie die Natur aus treuen Liebes-Quellen/", "tokens": ["Hier", "i\u00b7ockt", "sie", "die", "Na\u00b7tur", "aus", "treu\u00b7en", "Lie\u00b7bes\u00b7Quel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der Regen trocknet nicht so sich ins Hertz ergeust.", "tokens": ["Der", "Re\u00b7gen", "trock\u00b7net", "nicht", "so", "sich", "ins", "Hertz", "er\u00b7geust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zuweilen lassen sich die Z\u00e4hren noch verstellen/", "tokens": ["Zu\u00b7wei\u00b7len", "las\u00b7sen", "sich", "die", "Z\u00e4h\u00b7ren", "noch", "ver\u00b7stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht aber/ wenn der Schmertz bi\u00df in die Seele reist.", "tokens": ["Nicht", "a\u00b7ber", "/", "wenn", "der", "Schmertz", "bi\u00df", "in", "die", "See\u00b7le", "reist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$(", "KOUS", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zudem ist auch ver gonnt die Seinen zubeklagen/", "tokens": ["Zu\u00b7dem", "ist", "auch", "ver", "gonnt", "die", "Sei\u00b7nen", "zu\u00b7be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "NE", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn Thr\u00e4nen bleiben wol Dolmetscher unser Noth/", "tokens": ["Denn", "Thr\u00e4\u00b7nen", "blei\u00b7ben", "wol", "Dol\u00b7met\u00b7scher", "un\u00b7ser", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gesehrten/ so uns erst in dieses Leben tragen/", "tokens": ["Ge\u00b7sehr\u00b7ten", "/", "so", "uns", "erst", "in", "die\u00b7ses", "Le\u00b7ben", "tra\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Begleiter/ wenn aus dem uns rufft der blasse Tod.", "tokens": ["Be\u00b7glei\u00b7ter", "/", "wenn", "aus", "dem", "uns", "rufft", "der", "blas\u00b7se", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "APPR", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch aber sollen sie den jenen Fl\u00fcssen gleichen", "tokens": ["Doch", "a\u00b7ber", "sol\u00b7len", "sie", "den", "je\u00b7nen", "Fl\u00fcs\u00b7sen", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "PDAT", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die nie aus ihrem Bett\u2019 und gr\u00fcnen Ufern gehn.", "tokens": ["Die", "nie", "aus", "ih\u00b7rem", "Bett'", "und", "gr\u00fc\u00b7nen", "U\u00b7fern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein Weiser wird das Ziel bey seinem Leid erreichen", "tokens": ["Ein", "Wei\u00b7ser", "wird", "das", "Ziel", "bey", "sei\u00b7nem", "Leid", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn er bedenckt wie nah so Tod als Leben stehn.", "tokens": ["Wenn", "er", "be\u00b7denckt", "wie", "nah", "so", "Tod", "als", "Le\u00b7ben", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KOKOM", "ADJD", "ADV", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Schlu\u00df ist l\u00e4ngst gemacht/ da\u00df Menschen m\u00fcssen sterben/", "tokens": ["Der", "Schlu\u00df", "ist", "l\u00e4ngst", "ge\u00b7macht", "/", "da\u00df", "Men\u00b7schen", "m\u00fcs\u00b7sen", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$(", "KOUS", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wer di\u00df betrauren wil/ der kennt sich selbsten nicht.", "tokens": ["Wer", "di\u00df", "be\u00b7trau\u00b7ren", "wil", "/", "der", "kennt", "sich", "selbs\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVINF", "VMFIN", "$(", "ART", "VVFIN", "PRF", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie soll nicht dieser Leib/ der Asch und Staub/ verderben/", "tokens": ["Wie", "soll", "nicht", "die\u00b7ser", "Leib", "/", "der", "Asch", "und", "Staub", "/", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "PTKNEG", "PDAT", "NN", "$(", "ART", "NN", "KON", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn uns bey der Geburt die erste Haut schon bricht?", "tokens": ["Wenn", "uns", "bey", "der", "Ge\u00b7burt", "die", "ers\u00b7te", "Haut", "schon", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Der Anfang f\u00fchrt mit sich sein eintzig Kind das Ende/", "tokens": ["Der", "An\u00b7fang", "f\u00fchrt", "mit", "sich", "sein", "eint\u00b7zig", "Kind", "das", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "PPOSAT", "ADJD", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie gro\u00df der Unterscheid macht uns der Ausgang gleich.", "tokens": ["Wie", "gro\u00df", "der", "Un\u00b7ter\u00b7scheid", "macht", "uns", "der", "Aus\u00b7gang", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Auch Kindern ist zuviel die M\u00e4nge vom Elende/", "tokens": ["Auch", "Kin\u00b7dern", "ist", "zu\u00b7viel", "die", "M\u00e4n\u00b7ge", "vom", "E\u00b7len\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PIS", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und f\u00fcr der Jahre Flucht wird ein verlebter bleich.", "tokens": ["Und", "f\u00fcr", "der", "Jah\u00b7re", "Flucht", "wird", "ein", "ver\u00b7leb\u00b7ter", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VAFIN", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sagt/ ", "tokens": ["Sagt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.26": {"text": "So ihrer Jahre Zahl auff hohe Staffeln bracht.", "tokens": ["So", "ih\u00b7rer", "Jah\u00b7re", "Zahl", "auff", "ho\u00b7he", "Staf\u00b7feln", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ob sie bey Angst und Noth sich t\u00e4glich nicht begraben/", "tokens": ["Ob", "sie", "bey", "Angst", "und", "Noth", "sich", "t\u00e4g\u00b7lich", "nicht", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "PRF", "ADJD", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und saurer Tage Last sie todten \u00e4hnlich macht:", "tokens": ["Und", "sau\u00b7rer", "Ta\u00b7ge", "Last", "sie", "tod\u00b7ten", "\u00e4hn\u00b7lich", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Alsdenn legt auch die Zeit auff gleiche Wage-Schalen/", "tokens": ["Als\u00b7denn", "legt", "auch", "die", "Zeit", "auff", "glei\u00b7che", "Wa\u00b7ge\u00b7Scha\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Und messet mir den Raum so unser Leben hat.", "tokens": ["Und", "mes\u00b7set", "mir", "den", "Raum", "so", "un\u00b7ser", "Le\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Gewi\u00df/ die der Natur fr\u00fch ihre Schuld bezahlen", "tokens": ["Ge\u00b7wi\u00df", "/", "die", "der", "Na\u00b7tur", "fr\u00fch", "ih\u00b7re", "Schuld", "be\u00b7zah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "PRELS", "ART", "NN", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sind mehr gl\u00fcckseliger als die so Lebens-sat.", "tokens": ["Sind", "mehr", "gl\u00fcck\u00b7se\u00b7li\u00b7ger", "als", "die", "so", "Le\u00b7bens\u00b7sat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "ART", "ADV", "NN", "$."], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Theils sehen sich zuvor an ihren Kindern sterben/", "tokens": ["Theils", "se\u00b7hen", "sich", "zu\u00b7vor", "an", "ih\u00b7ren", "Kin\u00b7dern", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Theils gehen stets geb\u00fcckt/ voll Eckel/ voll Verdru\u00df/", "tokens": ["Theils", "ge\u00b7hen", "stets", "ge\u00b7b\u00fcckt", "/", "voll", "E\u00b7ckel", "/", "voll", "Ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "VVPP", "$(", "ADJD", "NN", "$(", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und vielen wird der Harm das Leben so erherben/", "tokens": ["Und", "vie\u00b7len", "wird", "der", "Harm", "das", "Le\u00b7ben", "so", "er\u00b7her\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df nichts als Angst und Noth ihr eintzig Uberflu\u00df.", "tokens": ["Da\u00df", "nichts", "als", "Angst", "und", "Noth", "ihr", "eint\u00b7zig", "U\u00b7ber\u00b7flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "NN", "KON", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man heist den jenigen erfreuet sonst wilkommen/", "tokens": ["Man", "heist", "den", "je\u00b7ni\u00b7gen", "er\u00b7freu\u