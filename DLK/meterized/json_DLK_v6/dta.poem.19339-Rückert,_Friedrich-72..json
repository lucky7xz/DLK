{"dta.poem.19339": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sprach zum K\u00f6nige, der mit den Leuten grollte,", "tokens": ["So", "sprach", "zum", "K\u00f6\u00b7ni\u00b7ge", ",", "der", "mit", "den", "Leu\u00b7ten", "groll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sich nicht besserten, und sich nicht bessern wollte,", "tokens": ["Die", "sich", "nicht", "bes\u00b7ser\u00b7ten", ",", "und", "sich", "nicht", "bes\u00b7sern", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "VVFIN", "$,", "KON", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sein Narr Geheimerath, als ihn der K\u00f6nig fragte,", "tokens": ["Sein", "Narr", "Ge\u00b7hei\u00b7me\u00b7rath", ",", "als", "ihn", "der", "K\u00f6\u00b7nig", "frag\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woher der Unmuth sei, der ihn heut sichtlich plagte?", "tokens": ["Wo\u00b7her", "der", "Un\u00b7muth", "sei", ",", "der", "ihn", "heut", "sicht\u00b7lich", "plag\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er sprach: Daher ist er, da\u00df ich der Magd mit Aschen", "tokens": ["Er", "sprach", ":", "Da\u00b7her", "ist", "er", ",", "da\u00df", "ich", "der", "Magd", "mit", "A\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PAV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Wasser heut befahl die Treppe rein zu waschen.", "tokens": ["Und", "Was\u00b7ser", "heut", "be\u00b7fahl", "die", "Trep\u00b7pe", "rein", "zu", "wa\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da wusch sie ungeschickt von unten statt von oben,", "tokens": ["Da", "wusch", "sie", "un\u00b7ge\u00b7schickt", "von", "un\u00b7ten", "statt", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADV", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schelten mu\u00dft' ich sie, wo ich sie wollte loben.", "tokens": ["Und", "schel\u00b7ten", "mu\u00dft'", "ich", "sie", ",", "wo", "ich", "sie", "woll\u00b7te", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VMFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Denn von der obern flo\u00df zur untern Stufe nieder", "tokens": ["Denn", "von", "der", "o\u00b7bern", "flo\u00df", "zur", "un\u00b7tern", "Stu\u00b7fe", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unrath, und beschmutzt ward das Gewaschne wieder.", "tokens": ["Der", "Un\u00b7rath", ",", "und", "be\u00b7schmutzt", "ward", "das", "Ge\u00b7waschne", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVPP", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Ich hab' es ihr gesagt: Umsonst ist was du putzest,", "tokens": ["Ich", "hab'", "es", "ihr", "ge\u00b7sagt", ":", "Um\u00b7sonst", "ist", "was", "du", "put\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVPP", "$.", "ADV", "VAFIN", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn mit dem Obern du das Untre stets beschmutzest.", "tokens": ["Wenn", "mit", "dem", "O\u00b7bern", "du", "das", "Un\u00b7tre", "stets", "be\u00b7schmut\u00b7zest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich sagt' es nochmals ihr, mein Wort war ohne Nutzen:", "tokens": ["Ich", "sagt'", "es", "noch\u00b7mals", "ihr", ",", "mein", "Wort", "war", "oh\u00b7ne", "Nut\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von unten kanst du nicht die Treppe gr\u00fcndlich putzen.", "tokens": ["Von", "un\u00b7ten", "kanst", "du", "nicht", "die", "Trep\u00b7pe", "gr\u00fcnd\u00b7lich", "put\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich sag' es abermals: Wenn sich der Glanz erneuern", "tokens": ["Ich", "sag'", "es", "a\u00b7ber\u00b7mals", ":", "Wenn", "sich", "der", "Glanz", "er\u00b7neu\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "KOUS", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der ganzen Treppe soll, fang oben an zu scheuern!", "tokens": ["Der", "gan\u00b7zen", "Trep\u00b7pe", "soll", ",", "fang", "o\u00b7ben", "an", "zu", "scheu\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "$,", "VVFIN", "ADV", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}