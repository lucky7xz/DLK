{"textgrid.poem.50049": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "5. Auff das Gl\u00fcck bey grossen Herren", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der grossen Herren Sinn zu Hof ist so verwirret,", "tokens": ["Der", "gros\u00b7sen", "Her\u00b7ren", "Sinn", "zu", "Hof", "ist", "so", "ver\u00b7wir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "NE", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dass durch ", "tokens": ["Dass", "durch"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Denn wenn ein Freund uns hier bereitet hat die Bahn,", "tokens": ["Denn", "wenn", "ein", "Freund", "uns", "hier", "be\u00b7rei\u00b7tet", "hat", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und man kein Wort gesagt als was sich woll geb\u00fchret,", "tokens": ["Und", "man", "kein", "Wort", "ge\u00b7sagt", "als", "was", "sich", "woll", "ge\u00b7b\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PIAT", "NN", "VVPP", "KOKOM", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So findt man offt, dass man ", "tokens": ["So", "findt", "man", "offt", ",", "dass", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und einen ", "tokens": ["Und", "ei\u00b7nen"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}