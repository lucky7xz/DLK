{"dta.poem.4099": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Bl\u00fcmlein Vergi\u00dfmein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So tief in's Holz hinein?", "tokens": ["So", "tief", "in's", "Holz", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was frommt mir, mich zu bergen", "tokens": ["Was", "frommt", "mir", ",", "mich", "zu", "ber\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im unbelauschten Hain?", "tokens": ["Im", "un\u00b7be\u00b7lauschten", "Hain", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Es bl\u00fcht auf allen Fluren", "tokens": ["Es", "bl\u00fcht", "auf", "al\u00b7len", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bl\u00fcmlein ", "tokens": ["Bl\u00fcm\u00b7lein"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Es schaut vom heitern Himmel", "tokens": ["Es", "schaut", "vom", "hei\u00b7tern", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herab in blauem Licht.", "tokens": ["Her\u00b7ab", "in", "blau\u00b7em", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und soll ich's niedertreten,", "tokens": ["Und", "soll", "ich's", "nie\u00b7der\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bebt mir der Fu\u00df zur\u00fcck,", "tokens": ["Bebt", "mir", "der", "Fu\u00df", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fleht aus jedem Kelche", "tokens": ["Es", "fleht", "aus", "je\u00b7dem", "Kel\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein wohlbekannter Blick.", "tokens": ["Ein", "wohl\u00b7be\u00b7kann\u00b7ter", "Blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wei\u00dft du, in welchem Garten", "tokens": ["Wei\u00dft", "du", ",", "in", "wel\u00b7chem", "Gar\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bl\u00fcmlein ", "tokens": ["Bl\u00fcm\u00b7lein"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Das Bl\u00fcmlein mu\u00df ich suchen,", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "mu\u00df", "ich", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie auch die Stra\u00dfe geht.", "tokens": ["Wie", "auch", "die", "Stra\u00b7\u00dfe", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "'s ist nicht f\u00fcr M\u00e4dchenbusen,", "tokens": ["'s", "ist", "nicht", "f\u00fcr", "M\u00e4d\u00b7chen\u00b7bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So sch\u00f6n sieht es nicht aus:", "tokens": ["So", "sch\u00f6n", "sieht", "es", "nicht", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Schwarz, schwarz ist seine Farbe,", "tokens": ["Schwarz", ",", "schwarz", "ist", "sei\u00b7ne", "Far\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es pa\u00dft in keinen Strau\u00df.", "tokens": ["Es", "pa\u00dft", "in", "kei\u00b7nen", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hat keine gr\u00fcne Bl\u00e4tter,", "tokens": ["Hat", "kei\u00b7ne", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat keinen Bl\u00fcthenduft,", "tokens": ["Hat", "kei\u00b7nen", "Bl\u00fc\u00b7then\u00b7duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es windet sich am Boden", "tokens": ["Es", "win\u00b7det", "sich", "am", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In n\u00e4chtig dumpfer Luft.", "tokens": ["In", "n\u00e4ch\u00b7tig", "dum\u00b7pfer", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "W\u00e4chst auch an einem Ufer,", "tokens": ["W\u00e4chst", "auch", "an", "ei\u00b7nem", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch unten flie\u00dft kein Bach,", "tokens": ["Doch", "un\u00b7ten", "flie\u00dft", "kein", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und willst das Bl\u00fcmlein pfl\u00fccken,", "tokens": ["Und", "willst", "das", "Bl\u00fcm\u00b7lein", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich zieht der Abgrund nach.", "tokens": ["Dich", "zieht", "der", "Ab\u00b7grund", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das ist der rechte Garten,", "tokens": ["Das", "ist", "der", "rech\u00b7te", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein schwarzer, schwarzer Flor:", "tokens": ["Ein", "schwar\u00b7zer", ",", "schwar\u00b7zer", "Flor", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darauf magst du dich betten \u2014", "tokens": ["Da\u00b7rauf", "magst", "du", "dich", "bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schleu\u00df zu das Gartenthor!", "tokens": ["Schleu\u00df", "zu", "das", "Gar\u00b7ten\u00b7thor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}