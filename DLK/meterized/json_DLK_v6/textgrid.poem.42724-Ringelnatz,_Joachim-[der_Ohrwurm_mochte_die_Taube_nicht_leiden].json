{"textgrid.poem.42724": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[der Ohrwurm mochte die Taube nicht leiden]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ohrwurm mochte die Taube nicht leiden.", "tokens": ["Der", "Ohr\u00b7wurm", "moch\u00b7te", "die", "Tau\u00b7be", "nicht", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie ha\u00dfte den Ohrwurm ebenso.", "tokens": ["Sie", "ha\u00df\u00b7te", "den", "Ohr\u00b7wurm", "e\u00b7ben\u00b7so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da trafen sich eines Tages die beiden", "tokens": ["Da", "tra\u00b7fen", "sich", "ei\u00b7nes", "Ta\u00b7ges", "die", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "PIAT"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In einer Stra\u00dfenbahn irgendwo.", "tokens": ["In", "ei\u00b7ner", "Stra\u00b7\u00dfen\u00b7bahn", "ir\u00b7gend\u00b7wo", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Sie sch\u00fcttelten sich erfreut die H\u00e4nde", "tokens": ["Sie", "sch\u00fct\u00b7tel\u00b7ten", "sich", "er\u00b7freut", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und l\u00e4chelten liebensw\u00fcrdig dabei", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7ten", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "da\u00b7bei"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PAV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sagten einander ganze B\u00e4nde", "tokens": ["Und", "sag\u00b7ten", "ein\u00b7an\u00b7der", "gan\u00b7ze", "B\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von \u00fcbertriebener Schmeichelei.", "tokens": ["Von", "\u00fc\u00b7bert\u00b7rie\u00b7be\u00b7ner", "Schmei\u00b7che\u00b7lei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch beide w\u00fcnschten sich im stillen,", "tokens": ["Doch", "bei\u00b7de", "w\u00fcnschten", "sich", "im", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "APPRART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der andre m\u00f6ge zum Teufel gehn,", "tokens": ["Der", "and\u00b7re", "m\u00f6\u00b7ge", "zum", "Teu\u00b7fel", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und da es geschah nach ihrem Willen,", "tokens": ["Und", "da", "es", "ge\u00b7schah", "nach", "ih\u00b7rem", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "So gab es beim Teufel ein Wiedersehn.", "tokens": ["So", "gab", "es", "beim", "Teu\u00b7fel", "ein", "Wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}