{"dta.poem.19361": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch im Weltverkehr lebt nur f\u00fcr sich allein,", "tokens": ["Der", "Mensch", "im", "Welt\u00b7ver\u00b7kehr", "lebt", "nur", "f\u00fcr", "sich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADV", "APPR", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und erst davon getrennt, im menschlichen Verein.", "tokens": ["Und", "erst", "da\u00b7von", "ge\u00b7trennt", ",", "im", "menschli\u00b7chen", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "VVPP", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Durch Leib, Besitz, Beruf, beschr\u00e4nkt und abgeschieden,", "tokens": ["Durch", "Leib", ",", "Be\u00b7sitz", ",", "Be\u00b7ruf", ",", "be\u00b7schr\u00e4nkt", "und", "ab\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo f\u00e4nde da der Geist, der schrankenlose, Frieden?", "tokens": ["Wo", "f\u00e4n\u00b7de", "da", "der", "Geist", ",", "der", "schran\u00b7ken\u00b7lo\u00b7se", ",", "Frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht im Gedankentausch, der nur verworren ist,", "tokens": ["Nicht", "im", "Ge\u00b7dan\u00b7ken\u00b7tausch", ",", "der", "nur", "ver\u00b7wor\u00b7ren", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht in der Liebe Rausch, der w\u00e4hrt so kurze Frist.", "tokens": ["Nicht", "in", "der", "Lie\u00b7be", "Rausch", ",", "der", "w\u00e4hrt", "so", "kur\u00b7ze", "Frist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur in der Einsamkeit spinnt er ein Traumgewebe,", "tokens": ["Nur", "in", "der", "Ein\u00b7sam\u00b7keit", "spinnt", "er", "ein", "Traum\u00b7ge\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Da\u00df in der Menschheit er, in ihm die Menschheit lebe.", "tokens": ["Da\u00df", "in", "der", "Menschheit", "er", ",", "in", "ihm", "die", "Menschheit", "le\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "$,", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Zur Wahrheit aber wird nur dort das Traumgespinnst,", "tokens": ["Zur", "Wahr\u00b7heit", "a\u00b7ber", "wird", "nur", "dort", "das", "Traum\u00b7ge\u00b7spinnst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo du den Sondrungen der K\u00f6rperwelt entrinnst;", "tokens": ["Wo", "du", "den", "Sond\u00b7run\u00b7gen", "der", "K\u00f6r\u00b7per\u00b7welt", "ent\u00b7rinnst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Wo alle Geister eins im h\u00f6chsten Geiste sind:", "tokens": ["Wo", "al\u00b7le", "Geis\u00b7ter", "eins", "im", "h\u00f6chs\u00b7ten", "Geis\u00b7te", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PIS", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort freut sich des Vereins die Menschheit, Gottes Kind.", "tokens": ["Dort", "freut", "sich", "des", "Ver\u00b7eins", "die", "Menschheit", ",", "Got\u00b7tes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}