{"textgrid.poem.48776": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ambrosie, mein Schatz,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ambrosie, mein Schatz,", "tokens": ["A\u00b7mbro\u00b7sie", ",", "mein", "Schatz", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "mit welcher ich im Lieben", "tokens": ["mit", "wel\u00b7cher", "ich", "im", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "so manche Zeit vertrieben,", "tokens": ["so", "man\u00b7che", "Zeit", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "komm mit mir auf den Platz,", "tokens": ["komm", "mit", "mir", "auf", "den", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "auf der Caninchen Hatz,", "tokens": ["auf", "der", "Ca\u00b7nin\u00b7chen", "Hatz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "da Amor uns will \u00fcben.", "tokens": ["da", "A\u00b7mor", "uns", "will", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Preis ist angeschrieben,", "tokens": ["Der", "Preis", "ist", "an\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "es gilt um einen Schmatz.", "tokens": ["es", "gilt", "um", "ei\u00b7nen", "Schmatz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und das noch ist das Beste,", "tokens": ["Und", "das", "noch", "ist", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wir bleiben seine G\u00e4ste", "tokens": ["wir", "blei\u00b7ben", "sei\u00b7ne", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "auf dieses zahme Wild.", "tokens": ["auf", "die\u00b7ses", "zah\u00b7me", "Wild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fcr Trank schenkt er uns K\u00fcsse.", "tokens": ["F\u00fcr", "Trank", "schenkt", "er", "uns", "K\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Ambrosie, du s\u00fc\u00dfe,", "tokens": ["A\u00b7mbro\u00b7sie", ",", "du", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "komm, weil es K\u00fcssens gilt.", "tokens": ["komm", ",", "weil", "es", "K\u00fcs\u00b7sens", "gilt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}