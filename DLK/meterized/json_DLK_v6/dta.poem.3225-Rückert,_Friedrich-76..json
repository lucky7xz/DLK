{"dta.poem.3225": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch eine Sprach' ist sch\u00f6n? Welch eine Sprach' ist reich?", "tokens": ["Welch", "ei\u00b7ne", "Sprach'", "ist", "sch\u00f6n", "?", "Welch", "ei\u00b7ne", "Sprach'", "ist", "reich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "ADJD", "$.", "PIAT", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschieden an Get\u00f6n, im Sinn sind alle gleich.", "tokens": ["Ver\u00b7schie\u00b7den", "an", "Ge\u00b7t\u00f6n", ",", "im", "Sinn", "sind", "al\u00b7le", "gleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPRART", "NN", "VAFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht dies' und jene Sprach' entz\u00fcckt, erfreuet mich;", "tokens": ["Nicht", "dies'", "und", "je\u00b7ne", "Sprach'", "ent\u00b7z\u00fcckt", ",", "er\u00b7freu\u00b7et", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "PDAT", "NN", "VVPP", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was mich erfreut, entz\u00fcckt, das ist die Sprach' an sich:", "tokens": ["Was", "mich", "er\u00b7freut", ",", "ent\u00b7z\u00fcckt", ",", "das", "ist", "die", "Sprach'", "an", "sich", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVPP", "$,", "PDS", "VAFIN", "ART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df eine Sprach' es gibt, die, was du f\u00fchlst und denkest,", "tokens": ["Da\u00df", "ei\u00b7ne", "Sprach'", "es", "gibt", ",", "die", ",", "was", "du", "f\u00fchlst", "und", "den\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "PRELS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dir deutlich macht, jemehr du dich in sie versenkest;", "tokens": ["Dir", "deut\u00b7lich", "macht", ",", "je\u00b7mehr", "du", "dich", "in", "sie", "ver\u00b7sen\u00b7kest", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Da\u00df eine Sprach' es gibt, kraft deren du verk\u00fcndest", "tokens": ["Da\u00df", "ei\u00b7ne", "Sprach'", "es", "gibt", ",", "kraft", "de\u00b7ren", "du", "ver\u00b7k\u00fcn\u00b7dest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "ADJD", "PDS", "PPER", "VVFIN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Welt geheimen Sinn, so weit du sie ergr\u00fcndest:", "tokens": ["Der", "Welt", "ge\u00b7hei\u00b7men", "Sinn", ",", "so", "weit", "du", "sie", "er\u00b7gr\u00fcn\u00b7dest", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADV", "ADJD", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum ist die sch\u00f6nste Sprach' und beste, die du nennst", "tokens": ["Drum", "ist", "die", "sch\u00f6ns\u00b7te", "Sprach'", "und", "bes\u00b7te", ",", "die", "du", "nennst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADJA", "NN", "KON", "ADJA", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Muttersprache, weil du sie am besten kennst.", "tokens": ["Die", "Mut\u00b7ter\u00b7spra\u00b7che", ",", "weil", "du", "sie", "am", "bes\u00b7ten", "kennst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}