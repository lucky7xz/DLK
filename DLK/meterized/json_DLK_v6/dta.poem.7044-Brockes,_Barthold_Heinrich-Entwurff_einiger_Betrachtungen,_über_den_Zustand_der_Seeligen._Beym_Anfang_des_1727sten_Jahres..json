{"dta.poem.7044": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir in einem andern Lande", "tokens": ["Wenn", "wir", "in", "ei\u00b7nem", "an\u00b7dern", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wohnungen ged\u00e4chten aufzuschlagen;", "tokens": ["Die", "Woh\u00b7nun\u00b7gen", "ge\u00b7d\u00e4ch\u00b7ten", "auf\u00b7zu\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So w\u00fcrden wir vermuthlich flei\u00dfig fragen,", "tokens": ["So", "w\u00fcr\u00b7den", "wir", "ver\u00b7muth\u00b7lich", "flei\u00b7\u00dfig", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rach solches Orts Beschaffenheit und Stande.", "tokens": ["Rach", "sol\u00b7ches", "Orts", "Be\u00b7schaf\u00b7fen\u00b7heit", "und", "Stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da wir nun alle dort in jene seelge H\u00f6hen,", "tokens": ["Da", "wir", "nun", "al\u00b7le", "dort", "in", "je\u00b7ne", "seel\u00b7ge", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach dieser Zeit, verhoffen einzugehen:", "tokens": ["Nach", "die\u00b7ser", "Zeit", ",", "ver\u00b7hof\u00b7fen", "ein\u00b7zu\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was kann denn billiger von uns geschchen,", "tokens": ["Was", "kann", "denn", "bil\u00b7li\u00b7ger", "von", "uns", "ge\u00b7schchen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als die Gedancken auch dahin zu schwingen,", "tokens": ["Als", "die", "Ge\u00b7dan\u00b7cken", "auch", "da\u00b7hin", "zu", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Um, nach Verm\u00f6gen, einzudringen", "tokens": ["Um", ",", "nach", "Ver\u00b7m\u00f6\u00b7gen", ",", "ein\u00b7zu\u00b7drin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["KOUI", "$,", "APPR", "NN", "$,", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In die etwannige Beschaffenheit,", "tokens": ["In", "die", "et\u00b7wan\u00b7ni\u00b7ge", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der, dermahleinst versprochnen Seeligkeit?", "tokens": ["Der", ",", "der\u00b7mah\u00b7le\u00b7inst", "ver\u00b7sproch\u00b7nen", "See\u00b7lig\u00b7keit", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Znmahl es \u00fcberdies dem grossen GOTT zur Ehr,", "tokens": ["Zn\u00b7mahl", "es", "\u00fc\u00b7ber\u00b7dies", "dem", "gros\u00b7sen", "GoTT", "zur", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zum Preis und Ruhm gereicht, wenn man je mehr und mehr", "tokens": ["Zum", "Preis", "und", "Ruhm", "ge\u00b7reicht", ",", "wenn", "man", "je", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "VVPP", "$,", "KOUS", "PIS", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Seelen Eigenschafft,", "tokens": ["Der", "See\u00b7len", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Die uns von Jhm geschencket,", "tokens": ["Die", "uns", "von", "Jhm", "ge\u00b7schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Mit aller Krafft,", "tokens": ["Mit", "al\u00b7ler", "Krafft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Auf einen solchen Vorwurff lencket,", "tokens": ["Auf", "ei\u00b7nen", "sol\u00b7chen", "Vor\u00b7wurff", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wodurch, von Seiner Lieb und Macht,", "tokens": ["Wo\u00b7durch", ",", "von", "Sei\u00b7ner", "Lieb", "und", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Uns recht was herrliches wird beygebracht.", "tokens": ["Uns", "recht", "was", "herr\u00b7li\u00b7ches", "wird", "bey\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PWS", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wenn man aus dem, was man hier sch\u00f6nes siehet,", "tokens": ["Wenn", "man", "aus", "dem", ",", "was", "man", "hier", "sch\u00f6\u00b7nes", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "$,", "PRELS", "PIS", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Von k\u00fcnfft\u2019ger Sch\u00f6nheit auch gerechte Schl\u00fcsse ziehet.", "tokens": ["Von", "k\u00fcnf\u00b7ft'\u00b7ger", "Sch\u00f6n\u00b7heit", "auch", "ge\u00b7rech\u00b7te", "Schl\u00fcs\u00b7se", "zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.22": {"text": "Ja, solch ein GOTT-ergebnes Dencken", "tokens": ["Ja", ",", "solch", "ein", "GoT\u00b7T\u00b7er\u00b7geb\u00b7nes", "Den\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ist \u00fcberdem geschickt in allen Dingen", "tokens": ["Ist", "\u00fc\u00b7ber\u00b7dem", "ge\u00b7schickt", "in", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Besondern Nutzen uns zu bringen,", "tokens": ["Be\u00b7son\u00b7dern", "Nut\u00b7zen", "uns", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Da, wenn das Gl\u00fcck uns lacht, man sich nicht \u00fcberheben,", "tokens": ["Da", ",", "wenn", "das", "Gl\u00fcck", "uns", "lacht", ",", "man", "sich", "nicht", "\u00fc\u00b7ber\u00b7he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "PIS", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und an dem irdischen zu sehr nicht kleben,", "tokens": ["Und", "an", "dem", "ir\u00b7di\u00b7schen", "zu", "sehr", "nicht", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "APPR", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Nein, sondern dencken wird: Gebrauche dich der Welt,", "tokens": ["Nein", ",", "son\u00b7dern", "den\u00b7cken", "wird", ":", "Ge\u00b7brau\u00b7che", "dich", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "VVINF", "VAFIN", "$.", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Doch so, da\u00df es dem Sch\u00f6pffer wohl gef\u00e4llt;", "tokens": ["Doch", "so", ",", "da\u00df", "es", "dem", "Sch\u00f6pf\u00b7fer", "wohl", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Damit du dessen dort, was tausendmal so sch\u00f6n,", "tokens": ["Da\u00b7mit", "du", "des\u00b7sen", "dort", ",", "was", "tau\u00b7send\u00b7mal", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nicht darffst verlustig gehn.", "tokens": ["Nicht", "darffst", "ver\u00b7lus\u00b7tig", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PAV", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}