{"textgrid.poem.59795": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz schm\u00fcckt sich mit dir, wie sich", "tokens": ["Mein", "Herz", "schm\u00fcckt", "sich", "mit", "dir", ",", "wie", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPER", "$,", "PWAV", "PRF"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Himmel mit der Sonne schm\u00fcckt \u2013", "tokens": ["Der", "Him\u00b7mel", "mit", "der", "Son\u00b7ne", "schm\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du giebst ihm Glanz, und ohne dich", "tokens": ["Du", "giebst", "ihm", "Glanz", ",", "und", "oh\u00b7ne", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "KON", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt es in dunkle Nacht entr\u00fcckt.", "tokens": ["Bleibt", "es", "in", "dunk\u00b7le", "Nacht", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gleichwie die Welt all ihre Pracht", "tokens": ["Gleich\u00b7wie", "die", "Welt", "all", "ih\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verh\u00fcllt, wenn Dunkel sie umflie\u00dft,", "tokens": ["Ver\u00b7h\u00fcllt", ",", "wenn", "Dun\u00b7kel", "sie", "um\u00b7flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur, wenn ihr die Sonne lacht,", "tokens": ["Und", "nur", ",", "wenn", "ihr", "die", "Son\u00b7ne", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zeigt, was sie Sch\u00f6nes in sich schlie\u00dft!", "tokens": ["Zeigt", ",", "was", "sie", "Sch\u00f6\u00b7nes", "in", "sich", "schlie\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}