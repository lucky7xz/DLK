{"textgrid.poem.57956": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Baucht\u00e4nzerin", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich komm' von's Provinzi\u00e4\u00f6lmuseum,", "tokens": ["Ich", "komm'", "von's", "Pro\u00b7vin\u00b7zi\u00b7\u00e4\u00f6l\u00b7mu\u00b7se\u00b7um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da war's famos,", "tokens": ["Da", "wa\u00b7r's", "fa\u00b7mos", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,"], "meter": "----+", "measure": "unknown.measure.single"}, "line.3": {"text": "Das Treppenhaus, na, und die Kuppel,", "tokens": ["Das", "Trep\u00b7pen\u00b7haus", ",", "na", ",", "und", "die", "Kup\u00b7pel", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$,", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Noch m\u00e4\u00f6l so gro\u00df.", "tokens": ["Noch", "m\u00e4\u00b7\u00f6l", "so", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Und Bilder sind d\u00e4\u00f6, nich von Pappe,", "tokens": ["Und", "Bil\u00b7der", "sind", "d\u00e4\u00f6", ",", "nich", "von", "Pap\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NE", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ganz kolloss\u00e4\u00f6l,", "tokens": ["Ganz", "kol\u00b7los\u00b7s\u00e4\u00b7\u00f6l", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Von Kunst soll mehrstens nich viel dran sind,", "tokens": ["Von", "Kunst", "soll", "mehrs\u00b7tens", "nich", "viel", "dran", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ADV", "PTKNEG", "ADV", "PAV", "VAFIN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mir putteg\u00e4\u00f6l.", "tokens": ["Mir", "put\u00b7te\u00b7g\u00e4\u00b7\u00f6l", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ich bin auch mehr vor das Pikante,", "tokens": ["Ich", "bin", "auch", "mehr", "vor", "das", "Pi\u00b7kan\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da liegt was drin,", "tokens": ["Da", "liegt", "was", "drin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWS", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zum Baaspiel, wie die ganz fam\u00f6se", "tokens": ["Zum", "Baa\u00b7spiel", ",", "wie", "die", "ganz", "fa\u00b7m\u00f6\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Baucht\u00e4nzerin.", "tokens": ["Bauch\u00b7t\u00e4n\u00b7ze\u00b7rin", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Man sch\u00e4\u00f6de, da\u00df f\u00fcr mehr von so was", "tokens": ["Man", "sch\u00e4\u00f6\u00b7de", ",", "da\u00df", "f\u00fcr", "mehr", "von", "so", "was"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "APPR", "ADV", "APPR", "ADV", "PWS"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So wenig Platz,", "tokens": ["So", "we\u00b7nig", "Platz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Viecher sind ganz \u00fcberfl\u00fcssig,", "tokens": ["Die", "Vie\u00b7cher", "sind", "ganz", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rein f\u00fcr die Katz.", "tokens": ["Rein", "f\u00fcr", "die", "Katz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Drum schmaa\u00dft sie raus und kauft noch M\u00e4chens", "tokens": ["Drum", "schmaa\u00dft", "sie", "raus", "und", "kauft", "noch", "M\u00e4\u00b7chens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Necklischee,", "tokens": ["Ins", "Neck\u00b7li\u00b7schee", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und macht vor Herrens Extrazimmer", "tokens": ["Und", "macht", "vor", "Her\u00b7rens", "Ext\u00b7ra\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit Engtree.", "tokens": ["Und", "mit", "Eng\u00b7tree", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}