{"textgrid.poem.36333": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Deutscher Sinn", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Froh mit Freunden rasch gelebt,", "tokens": ["Froh", "mit", "Freun\u00b7den", "rasch", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz zu Herzen hingestrebt,", "tokens": ["Herz", "zu", "Her\u00b7zen", "hin\u00b7ge\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von des Fr\u00fchlings Lust getr\u00e4nkt,", "tokens": ["Von", "des", "Fr\u00fch\u00b7lings", "Lust", "ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geistes Aug' in Geist versenkt,", "tokens": ["Geis\u00b7tes", "Aug'", "in", "Geist", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist des Deutschen Sitt' und Art,", "tokens": ["Ist", "des", "Deut\u00b7schen", "Sitt'", "und", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die noch nie gewandelt ward.", "tokens": ["Die", "noch", "nie", "ge\u00b7wan\u00b7delt", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was in Kunst und Wissenschaft", "tokens": ["Was", "in", "Kunst", "und", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fremder Himmel Hohes schafft,", "tokens": ["Frem\u00b7der", "Him\u00b7mel", "Ho\u00b7hes", "schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ward von ihm alsbald erkannt,", "tokens": ["Ward", "von", "ihm", "als\u00b7bald", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wuchs so m\u00e4cht'ger seiner Hand.", "tokens": ["Wuchs", "so", "m\u00e4cht'\u00b7ger", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Eines ihm Verderben bringt:", "tokens": ["Ei\u00b7nes", "ihm", "Ver\u00b7der\u00b7ben", "bringt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn ihn fremde Sitte zwingt;", "tokens": ["Wenn", "ihn", "frem\u00b7de", "Sit\u00b7te", "zwingt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Eins emp\u00f6ret sein Gef\u00fchl:", "tokens": ["Eins", "em\u00b7p\u00f6\u00b7ret", "sein", "Ge\u00b7f\u00fchl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Fremder Rechte loses Spiel;", "tokens": ["Frem\u00b7der", "Rech\u00b7te", "lo\u00b7ses", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Ewig bleiben die uns fern,", "tokens": ["E\u00b7wig", "blei\u00b7ben", "die", "uns", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "PRELS", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Ehr' und Freiheit unser Stern.", "tokens": ["Ehr'", "und", "Frei\u00b7heit", "un\u00b7ser", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}