{"dta.poem.17611": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "233.  Wenn der Mensch Gott ist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Eh\u2019 als ich ich noch war/ da war ich Gott in Gott:", "tokens": ["Eh'", "als", "ich", "ich", "noch", "war", "/", "da", "war", "ich", "Gott", "in", "Gott", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PPER", "ADV", "VAFIN", "$(", "ADV", "VAFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum\u0304 kan ichs wieder seyn wen\u0304 ich nur mir bin Todt.", "tokens": ["Drum\u0304", "kan", "ichs", "wie\u00b7der", "seyn", "wen\u0304", "ich", "nur", "mir", "bin", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "ADV", "VAINF", "VVFIN", "PPER", "ADV", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}