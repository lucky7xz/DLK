{"dta.poem.15861": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Bauern Sprichwort sagt, mein Sohn: wenn auf dem Sand", "tokens": ["Der", "Bau\u00b7ern", "Sprich\u00b7wort", "sagt", ",", "mein", "Sohn", ":", "wenn", "auf", "dem", "Sand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$.", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Ernte gut ger\u00e4th, ist Theuerung im Land.", "tokens": ["Die", "Ern\u00b7te", "gut", "ge\u00b7r\u00e4\u00b7th", ",", "ist", "Theu\u00b7e\u00b7rung", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Warum? weil auf dem Sand der Segen nur bekommt", "tokens": ["Wa\u00b7rum", "?", "weil", "auf", "dem", "Sand", "der", "Se\u00b7gen", "nur", "be\u00b7kommt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von soviel Regen, als nicht besserm Boden frommt.", "tokens": ["Von", "so\u00b7viel", "Re\u00b7gen", ",", "als", "nicht", "bes\u00b7serm", "Bo\u00b7den", "frommt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "PTKNEG", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wir haben schlimmen Stand dahier auf unserm Sand;", "tokens": ["Wir", "ha\u00b7ben", "schlim\u00b7men", "Stand", "da\u00b7hier", "auf", "un\u00b7serm", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was w\u00fcnschen wir uns selbst? und was dem andern Land?", "tokens": ["Was", "w\u00fcn\u00b7schen", "wir", "uns", "selbst", "?", "und", "was", "dem", "an\u00b7dern", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "$.", "KON", "PWS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein schlimmer Wunsch: Weh' uns, da\u00df andern wohl es gehe!", "tokens": ["Ein", "schlim\u00b7mer", "Wunsch", ":", "Weh'", "uns", ",", "da\u00df", "an\u00b7dern", "wohl", "es", "ge\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "PPER", "$,", "KOUS", "PIS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und noch ein schlimmerer: Uns wohl, und allen wehe!", "tokens": ["Und", "noch", "ein", "schlim\u00b7me\u00b7rer", ":", "Uns", "wohl", ",", "und", "al\u00b7len", "we\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "$.", "PPER", "ADV", "$,", "KON", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}