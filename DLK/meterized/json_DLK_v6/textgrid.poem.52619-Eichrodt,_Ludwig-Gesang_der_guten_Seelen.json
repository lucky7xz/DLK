{"textgrid.poem.52619": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der guten Seelen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sto\u00dft an mit fr\u00f6hlichem Singen,", "tokens": ["Sto\u00dft", "an", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und seid drei Farben hold,", "tokens": ["Und", "seid", "drei", "Far\u00b7ben", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n dunkel sind die Stra\u00dfen,", "tokens": ["Sch\u00f6n", "dun\u00b7kel", "sind", "die", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sch\u00f6n roth sind unsre Nasen,", "tokens": ["Sch\u00f6n", "roth", "sind", "uns\u00b7re", "Na\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Wein ist pures Gold!", "tokens": ["Der", "Wein", "ist", "pu\u00b7res", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo unsre Farben gelten,", "tokens": ["Wo", "uns\u00b7re", "Far\u00b7ben", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verschwindet alles Leid;", "tokens": ["Ver\u00b7schwin\u00b7det", "al\u00b7les", "Leid", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind ein Volk von Br\u00fcdern,", "tokens": ["Wir", "sind", "ein", "Volk", "von", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Freiheit in den Liedern", "tokens": ["Und", "Frei\u00b7heit", "in", "den", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie kommt in Wirklichkeit.", "tokens": ["Sie", "kommt", "in", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zwar sind verp\u00f6nt die Farben", "tokens": ["Zwar", "sind", "ver\u00b7p\u00f6nt", "die", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im deutschen Vaterland;", "tokens": ["Im", "deut\u00b7schen", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, nur in stillen Symbolen", "tokens": ["Ach", ",", "nur", "in", "stil\u00b7len", "Sym\u00b7bo\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verehret sie verstohlen,", "tokens": ["Ver\u00b7eh\u00b7ret", "sie", "ver\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein trauter Zechverband.", "tokens": ["Ein", "trau\u00b7ter", "Zech\u00b7ver\u00b7band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ob alle untreu werden,", "tokens": ["Ob", "al\u00b7le", "un\u00b7treu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir bleiben den Farben hold!", "tokens": ["Wir", "blei\u00b7ben", "den", "Far\u00b7ben", "hold", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir halten sie hoch in Ehren,", "tokens": ["Wir", "hal\u00b7ten", "sie", "hoch", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wir werden die Welt bekehren", "tokens": ["Wir", "wer\u00b7den", "die", "Welt", "be\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zum Schwarz und Roth und Gold!", "tokens": ["Zum", "Schwarz", "und", "Roth", "und", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}