{"textgrid.poem.35937": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es sitzt in trauter Zelle", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sitzt in trauter Zelle", "tokens": ["Es", "sitzt", "in", "trau\u00b7ter", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Fenster ein M\u00e4gdlein bleich", "tokens": ["Am", "Fens\u00b7ter", "ein", "M\u00e4gd\u00b7lein", "bleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und schaut hinab in die Welle,", "tokens": ["Und", "schaut", "hin\u00b7ab", "in", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da rollen zwei Perlen helle", "tokens": ["Da", "rol\u00b7len", "zwei", "Per\u00b7len", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "NN", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wohl in das Wasser gleich.", "tokens": ["Wohl", "in", "das", "Was\u00b7ser", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie h\u00f6rt eine Fl\u00f6te von Weitem,", "tokens": ["Sie", "h\u00f6rt", "ei\u00b7ne", "Fl\u00f6\u00b7te", "von", "Wei\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie blickt auf Schilf und Rohr;", "tokens": ["Sie", "blickt", "auf", "Schilf", "und", "Rohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da keimen verlorene Freuden,", "tokens": ["Da", "kei\u00b7men", "ver\u00b7lo\u00b7re\u00b7ne", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da sprossen vergessene Leiden", "tokens": ["Da", "spros\u00b7sen", "ver\u00b7ges\u00b7se\u00b7ne", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ihr frisch im Herzen empor.", "tokens": ["Ihr", "frisch", "im", "Her\u00b7zen", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "\u00bbdie Welle rinnt und sch\u00e4umet,", "tokens": ["\u00bb", "die", "Wel\u00b7le", "rinnt", "und", "sch\u00e4u\u00b7met", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gr\u00fcn Laub schm\u00fcckt wieder den Baum.", "tokens": ["Gr\u00fcn", "Laub", "schm\u00fcckt", "wie\u00b7der", "den", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ach, Fr\u00fchling, hast lange ges\u00e4umet!", "tokens": ["Ach", ",", "Fr\u00fch\u00b7ling", ",", "hast", "lan\u00b7ge", "ge\u00b7s\u00e4u\u00b7met", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nun ist mir, als h\u00e4tt' ich getr\u00e4umet", "tokens": ["Nun", "ist", "mir", ",", "als", "h\u00e4tt'", "ich", "ge\u00b7tr\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein'n langen, schweren Traum.", "tokens": ["Ein'n", "lan\u00b7gen", ",", "schwe\u00b7ren", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbich wei\u00df, der Lenz schwebt nieder,", "tokens": ["\u00bb", "ich", "wei\u00df", ",", "der", "Lenz", "schwebt", "nie\u00b7der", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df wohl: es ist Mai;", "tokens": ["Ich", "wei\u00df", "wohl", ":", "es", "ist", "Mai", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch kehren dieselben Lieder,", "tokens": ["Doch", "keh\u00b7ren", "die\u00b7sel\u00b7ben", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dieselben Blumen nicht wieder;", "tokens": ["Die\u00b7sel\u00b7ben", "Blu\u00b7men", "nicht", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ist Alles anders und neu.\u00ab", "tokens": ["Ist", "Al\u00b7les", "an\u00b7ders", "und", "neu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIS", "ADV", "KON", "ADJD", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}