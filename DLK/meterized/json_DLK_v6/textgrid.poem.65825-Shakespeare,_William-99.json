{"textgrid.poem.65825": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "99", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schalt ich fr\u00fcher Veilchen \u00dcbermut:", "tokens": ["So", "schalt", "ich", "fr\u00fc\u00b7her", "Veil\u00b7chen", "\u00dc\u00b7ber\u00b7mut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo stahlt ihr s\u00fc\u00dfen Diebe euern Hauch,", "tokens": ["Wo", "stahlt", "ihr", "s\u00fc\u00b7\u00dfen", "Die\u00b7be", "eu\u00b7ern", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nicht von seinem Mund? Die Purpurglut", "tokens": ["Wenn", "nicht", "von", "sei\u00b7nem", "Mund", "?", "Die", "Pur\u00b7pur\u00b7glut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf euern samtnen W\u00e4nglein habt ihr auch", "tokens": ["Auf", "eu\u00b7ern", "samt\u00b7nen", "W\u00e4ng\u00b7lein", "habt", "ihr", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur schwach gef\u00e4rbt in seiner Adern Blut!", "tokens": ["Nur", "schwach", "ge\u00b7f\u00e4rbt", "in", "sei\u00b7ner", "A\u00b7dern", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Lilien warf ich deine H\u00e4nde vor;", "tokens": ["Den", "Li\u00b7li\u00b7en", "warf", "ich", "dei\u00b7ne", "H\u00e4n\u00b7de", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da\u00df er dein Haar bestahl, dem Majoran.", "tokens": ["Da\u00df", "er", "dein", "Haar", "be\u00b7stahl", ",", "dem", "Ma\u00b7jo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Furchtsam auf Dornen stand der Rosen Chor,", "tokens": ["Furcht\u00b7sam", "auf", "Dor\u00b7nen", "stand", "der", "Ro\u00b7sen", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Teils vor Verzweiflung wei\u00df, teils rot vor Scham:", "tokens": ["Teils", "vor", "Ver\u00b7zwei\u00b7flung", "wei\u00df", ",", "teils", "rot", "vor", "Scham", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und eine, weder rot noch wei\u00df, verma\u00df", "tokens": ["Und", "ei\u00b7ne", ",", "we\u00b7der", "rot", "noch", "wei\u00df", ",", "ver\u00b7ma\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "$,", "KON", "ADJD", "ADV", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Von beidem sich, und stahl noch deinen Atem:", "tokens": ["Von", "bei\u00b7dem", "sich", ",", "und", "stahl", "noch", "dei\u00b7nen", "A\u00b7tem", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PRF", "$,", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Allein zur Strafe kam ein Wurm und fra\u00df", "tokens": ["Al\u00b7lein", "zur", "Stra\u00b7fe", "kam", "ein", "Wurm", "und", "fra\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Im vollsten Prangen sie f\u00fcr ihre Taten. \u2013", "tokens": ["Im", "volls\u00b7ten", "Pran\u00b7gen", "sie", "f\u00fcr", "ih\u00b7re", "Ta\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Nicht eine war von aller Blumen Zahl,", "tokens": ["Nicht", "ei\u00b7ne", "war", "von", "al\u00b7ler", "Blu\u00b7men", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "VAFIN", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die dir nicht Farben oder D\u00fcfte stahl.", "tokens": ["Die", "dir", "nicht", "Far\u00b7ben", "o\u00b7der", "D\u00fcf\u00b7te", "stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}