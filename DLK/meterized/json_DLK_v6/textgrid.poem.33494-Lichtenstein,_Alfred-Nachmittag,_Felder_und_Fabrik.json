{"textgrid.poem.33494": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Nachmittag, Felder und Fabrik", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann die Augen nicht mehr unterbringen.", "tokens": ["Ich", "kann", "die", "Au\u00b7gen", "nicht", "mehr", "un\u00b7ter\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich kann die Knochen nicht zusammenhalten.", "tokens": ["Ich", "kann", "die", "Kno\u00b7chen", "nicht", "zu\u00b7sam\u00b7men\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Herz ist stier. Kopf mu\u00df zerspringen.", "tokens": ["Das", "Herz", "ist", "stier", ".", "Kopf", "mu\u00df", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Rings weiche Masse. Nichts will sich gestalten.", "tokens": ["Rings", "wei\u00b7che", "Mas\u00b7se", ".", "Nichts", "will", "sich", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "PIS", "VMFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Zunge bricht mir. Und das Maul verbiegt sich.", "tokens": ["Die", "Zun\u00b7ge", "bricht", "mir", ".", "Und", "das", "Maul", "ver\u00b7biegt", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "KON", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In meinem Sch\u00e4del ist nicht Lust noch Ziele.", "tokens": ["In", "mei\u00b7nem", "Sch\u00e4\u00b7del", "ist", "nicht", "Lust", "noch", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne, eine Butterblume, wiegt sich", "tokens": ["Die", "Son\u00b7ne", ",", "ei\u00b7ne", "But\u00b7ter\u00b7blu\u00b7me", ",", "wiegt", "sich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf einem Schornstein, ihrem schlanken Stiele.", "tokens": ["Auf", "ei\u00b7nem", "Schorn\u00b7stein", ",", "ih\u00b7rem", "schlan\u00b7ken", "Stie\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}