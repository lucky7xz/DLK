{"textgrid.poem.50765": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "13", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst dich freventlich emanzipieren", "tokens": ["Du", "willst", "dich", "fre\u00b7vent\u00b7lich", "e\u00b7man\u00b7zi\u00b7pie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und aufstehn wider mich mit keckem Sinn,", "tokens": ["Und", "auf\u00b7stehn", "wi\u00b7der", "mich", "mit", "ke\u00b7ckem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIZU", "APPR", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aufs eigne F\u00e4ustchen deine Wirtschaft f\u00fchren,", "tokens": ["Aufs", "eig\u00b7ne", "F\u00e4ust\u00b7chen", "dei\u00b7ne", "Wirt\u00b7schaft", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sch\u00f6ne kleine Jakobinerin?", "tokens": ["Du", "sch\u00f6\u00b7ne", "klei\u00b7ne", "Ja\u00b7ko\u00b7bi\u00b7ne\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zur Politik nun auch dein W\u00f6rtlein sagen,", "tokens": ["Zur", "Po\u00b7li\u00b7tik", "nun", "auch", "dein", "W\u00f6rt\u00b7lein", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus trauter Kammer in den Ratsaal fliehn?", "tokens": ["Aus", "trau\u00b7ter", "Kam\u00b7mer", "in", "den", "Rat\u00b7saal", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl gar mit weicher Hand die Trommel schlagen,", "tokens": ["Wohl", "gar", "mit", "wei\u00b7cher", "Hand", "die", "Trom\u00b7mel", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann einst wir gegen die Tyrannen ziehn?", "tokens": ["Wann", "einst", "wir", "ge\u00b7gen", "die", "Ty\u00b7ran\u00b7nen", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Berufest dich auf meine eignen Lehren", "tokens": ["Be\u00b7ru\u00b7fest", "dich", "auf", "mei\u00b7ne", "eig\u00b7nen", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Freiheit, Gleichheit und von Menschenrecht?", "tokens": ["Von", "Frei\u00b7heit", ",", "Gleich\u00b7heit", "und", "von", "Men\u00b7schen\u00b7recht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O la\u00df, mein Kind, mit K\u00fcssen dich bekehren,", "tokens": ["O", "la\u00df", ",", "mein", "Kind", ",", "mit", "K\u00fcs\u00b7sen", "dich", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies eine Mal errietest du mich schlecht!", "tokens": ["Dies", "ei\u00b7ne", "Mal", "er\u00b7rie\u00b7test", "du", "mich", "schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir, mir, mein Schatz! mu\u00dft du dich nun verpflichten,", "tokens": ["Mir", ",", "mir", ",", "mein", "Schatz", "!", "mu\u00dft", "du", "dich", "nun", "ver\u00b7pflich\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "$,", "PPOSAT", "NN", "$.", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Liebster und dein Herr ist f\u00fcr dich frei!", "tokens": ["Dein", "Liebs\u00b7ter", "und", "dein", "Herr", "ist", "f\u00fcr", "dich", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Da\u00df er allein dein siegreich Banner sei!", "tokens": ["Da\u00df", "er", "al\u00b7lein", "dein", "sieg\u00b7reich", "Ban\u00b7ner", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Ketten all, von denen ich entbinden", "tokens": ["Die", "Ket\u00b7ten", "all", ",", "von", "de\u00b7nen", "ich", "ent\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "$,", "APPR", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die V\u00f6lker m\u00f6chte, o Geliebte mein!", "tokens": ["Die", "V\u00f6l\u00b7ker", "m\u00f6ch\u00b7te", ",", "o", "Ge\u00b7lieb\u00b7te", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "FM", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Blumenketten eng dir umzuwinden", "tokens": ["Als", "Blu\u00b7men\u00b7ket\u00b7ten", "eng", "dir", "um\u00b7zu\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll einzig dann mein Tun und Trachten sein!", "tokens": ["Soll", "ein\u00b7zig", "dann", "mein", "Tun", "und", "Trach\u00b7ten", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein fest Gef\u00e4ngnis will ich dir erbauen", "tokens": ["Ein", "fest", "Ge\u00b7f\u00e4ng\u00b7nis", "will", "ich", "dir", "er\u00b7bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Rosen, Lilien, Myrten, duftend, weich;", "tokens": ["Von", "Ro\u00b7sen", ",", "Li\u00b7li\u00b7en", ",", "Myr\u00b7ten", ",", "duf\u00b7tend", ",", "weich", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$,", "NN", "$,", "VVPP", "$,", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Draus sollst du nur des Himmels Sterne schauen", "tokens": ["Draus", "sollst", "du", "nur", "des", "Him\u00b7mels", "Ster\u00b7ne", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich, den Kerkermeister, froh und reich.", "tokens": ["Und", "mich", ",", "den", "Ker\u00b7ker\u00b7meis\u00b7ter", ",", "froh", "und", "reich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich will zur Kurzweil s\u00fc\u00dfe Lieder singen,", "tokens": ["Ich", "will", "zur", "Kurz\u00b7weil", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darinnen du dich lachend spiegeln magst;", "tokens": ["Da\u00b7rin\u00b7nen", "du", "dich", "la\u00b7chend", "spie\u00b7geln", "magst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Liedern dir die Welt zu F\u00fc\u00dfen bringen,", "tokens": ["In", "Lie\u00b7dern", "dir", "die", "Welt", "zu", "F\u00fc\u00b7\u00dfen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn \u00fcber Einsamkeit du dich beklagst.", "tokens": ["Wenn", "\u00fc\u00b7ber", "Ein\u00b7sam\u00b7keit", "du", "dich", "be\u00b7klagst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch wann die lieben Nachtigallen schlagen", "tokens": ["Doch", "wann", "die", "lie\u00b7ben", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wann das Abendrot verglommen ist:", "tokens": ["Und", "wann", "das", "A\u00b7ben\u00b7drot", "ver\u00b7glom\u00b7men", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sollst du als K\u00f6nigin die Krone tragen,", "tokens": ["Sollst", "du", "als", "K\u00f6\u00b7ni\u00b7gin", "die", "Kro\u00b7ne", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Solange Luna ihre Bahn durchmi\u00dft!", "tokens": ["So\u00b7lan\u00b7ge", "Lu\u00b7na", "ih\u00b7re", "Bahn", "durch\u00b7mi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}