{"dta.poem.8896": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Steht hier Johannes, um zu reinern Sph\u00e4ren", "tokens": ["Steht", "hier", "Jo\u00b7han\u00b7nes", ",", "um", "zu", "rei\u00b7nern", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NE", "$,", "KOUI", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Einsamkeit die Seele zu verkl\u00e4ren,", "tokens": ["Durch", "Ein\u00b7sam\u00b7keit", "die", "See\u00b7le", "zu", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die hohe, gro\u00dfgestimmte, gotteswarme.", "tokens": ["Die", "ho\u00b7he", ",", "gro\u00df\u00b7ges\u00b7timm\u00b7te", ",", "got\u00b7tes\u00b7war\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Voll von Begeisterung, von heil'gem Harme", "tokens": ["Voll", "von", "Be\u00b7geis\u00b7te\u00b7rung", ",", "von", "heil'\u00b7gem", "Har\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ergl\u00e4nzt sein ew'ger, ernster Blick von Z\u00e4hren,", "tokens": ["Er\u00b7gl\u00e4nzt", "sein", "ew'\u00b7ger", ",", "erns\u00b7ter", "Blick", "von", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach Jenem, den Maria soll geb\u00e4ren,", "tokens": ["Nach", "Je\u00b7nem", ",", "den", "Ma\u00b7ria", "soll", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "ART", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Scheint er zu deuten mit erhobnem Arme.", "tokens": ["Scheint", "er", "zu", "deu\u00b7ten", "mit", "er\u00b7hob\u00b7nem", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer kann sich weg von diesem Bilde kehren,", "tokens": ["Wer", "kann", "sich", "weg", "von", "die\u00b7sem", "Bil\u00b7de", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und m\u00f6chte nicht, mit br\u00fcnstigen Geberden,", "tokens": ["Und", "m\u00f6ch\u00b7te", "nicht", ",", "mit", "br\u00fcns\u00b7ti\u00b7gen", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Gott im Busen Tizians verehren?", "tokens": ["Den", "Gott", "im", "Bu\u00b7sen", "Ti\u00b7zi\u00b7ans", "ver\u00b7eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O goldne Zeit, die nicht mehr ist im Werden,", "tokens": ["O", "gold\u00b7ne", "Zeit", ",", "die", "nicht", "mehr", "ist", "im", "Wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als noch die Kunst vermocht die Welt zu lehren,", "tokens": ["Als", "noch", "die", "Kunst", "ver\u00b7mocht", "die", "Welt", "zu", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nur das Sch\u00f6ne heilig war auf Erden!", "tokens": ["Und", "nur", "das", "Sch\u00f6\u00b7ne", "hei\u00b7lig", "war", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}