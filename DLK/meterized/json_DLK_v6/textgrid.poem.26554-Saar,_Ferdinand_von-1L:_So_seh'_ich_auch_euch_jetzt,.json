{"textgrid.poem.26554": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So seh' ich auch euch jetzt,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So seh' ich auch euch jetzt,", "tokens": ["So", "seh'", "ich", "auch", "euch", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihr sonnigen Blumenaugen des Lenzes,", "tokens": ["Ihr", "son\u00b7ni\u00b7gen", "Blu\u00b7men\u00b7au\u00b7gen", "des", "Len\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In zierliche T\u00f6pfe verpflanzt", "tokens": ["In", "zier\u00b7li\u00b7che", "T\u00f6p\u00b7fe", "ver\u00b7pflanzt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und in japanischen Vasen;", "tokens": ["Und", "in", "ja\u00b7pa\u00b7ni\u00b7schen", "Va\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seh' euch mit leisem Schmerz", "tokens": ["Seh'", "euch", "mit", "lei\u00b7sem", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Kunstvoll zum Strau\u00dfe gereiht,", "tokens": ["Kunst\u00b7voll", "zum", "Strau\u00b7\u00dfe", "ge\u00b7reiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Und als schimmernden Brust- und Lockenschmuck", "tokens": ["Und", "als", "schim\u00b7mern\u00b7den", "Brust", "und", "Lo\u00b7cken\u00b7schmuck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "TRUNC", "KON", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Erh\u00f6hen buhlender Sch\u00f6nheit Reiz.", "tokens": ["Er\u00b7h\u00f6\u00b7hen", "buh\u00b7len\u00b7der", "Sch\u00f6n\u00b7heit", "Reiz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mehr stets liebt' ich euch", "tokens": ["Mehr", "stets", "liebt'", "ich", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Als die ersten Veilchen", "tokens": ["Als", "die", "ers\u00b7ten", "Veil\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und die thaufrischen Hagerosen.", "tokens": ["Und", "die", "thauf\u00b7ri\u00b7schen", "Ha\u00b7ge\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Denn jene, ob auch verborgen dem Aug',", "tokens": ["Denn", "je\u00b7ne", ",", "ob", "auch", "ver\u00b7bor\u00b7gen", "dem", "Aug'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "KOUS", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Locken dringenden Duft's Pfl\u00fccker heran \u2013", "tokens": ["Lo\u00b7cken", "drin\u00b7gen\u00b7den", "Duft's", "Pfl\u00fc\u00b7cker", "he\u00b7ran", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "PTKVZ", "$("], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und diese, fesselnd mit scharfem Dorn,", "tokens": ["Und", "die\u00b7se", ",", "fes\u00b7selnd", "mit", "schar\u00b7fem", "Dorn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dr\u00e4ngen ber\u00fcckend am Strauch sich entgegen.", "tokens": ["Dr\u00e4n\u00b7gen", "be\u00b7r\u00fc\u00b7ckend", "am", "Strauch", "sich", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Ihr aber,", "tokens": ["Ihr", "a\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Keusch und unentweiht,", "tokens": ["Keusch", "und", "un\u00b7ent\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Selig des eig'nen Lichts,", "tokens": ["Se\u00b7lig", "des", "eig'\u00b7nen", "Lichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Bl\u00fchtet", "tokens": ["Bl\u00fch\u00b7tet"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Und verbl\u00fchtet ihr,", "tokens": ["Und", "ver\u00b7bl\u00fch\u00b7tet", "ihr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "An der Erde heilige Mutterbrust", "tokens": ["An", "der", "Er\u00b7de", "hei\u00b7li\u00b7ge", "Mut\u00b7ter\u00b7brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Dicht geschmiegt.", "tokens": ["Dicht", "ge\u00b7schmiegt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "H\u00f6chstens, da\u00df fr\u00f6hlich euch", "tokens": ["H\u00f6chs\u00b7tens", ",", "da\u00df", "fr\u00f6h\u00b7lich", "euch"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ADJD", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ein l\u00e4ndliches Kind dem braunen Haar gesellt,", "tokens": ["Ein", "l\u00e4nd\u00b7li\u00b7ches", "Kind", "dem", "brau\u00b7nen", "Haar", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oder der sinnende Dichter", "tokens": ["O\u00b7der", "der", "sin\u00b7nen\u00b7de", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "And\u00e4chtig euch losgel\u00f6s't", "tokens": ["An\u00b7d\u00e4ch\u00b7tig", "euch", "los\u00b7ge\u00b7l\u00f6s't"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PPER", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Von der wurzelumh\u00fcllenden Scholle,", "tokens": ["Von", "der", "wur\u00b7ze\u00b7lum\u00b7h\u00fcl\u00b7len\u00b7den", "Schol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Damit ihr, im schlichten Glase getr\u00e4nkt,", "tokens": ["Da\u00b7mit", "ihr", ",", "im", "schlich\u00b7ten", "Gla\u00b7se", "ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Erhelltet seiner d\u00fcsteren Stube Einsamkeit.", "tokens": ["Er\u00b7hell\u00b7tet", "sei\u00b7ner", "d\u00fcs\u00b7te\u00b7ren", "Stu\u00b7be", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und doch! Wo immer", "tokens": ["Und", "doch", "!", "Wo", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$.", "PWAV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Euer sanfter Glanz auch leuchtet \u2013", "tokens": ["Eu\u00b7er", "sanf\u00b7ter", "Glanz", "auch", "leuch\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst in menschenvoller Gassen Kehricht noch:", "tokens": ["Selbst", "in", "men\u00b7schen\u00b7vol\u00b7ler", "Gas\u00b7sen", "Keh\u00b7richt", "noch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wehen um euch,", "tokens": ["We\u00b7hen", "um", "euch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Unschuldvoll,", "tokens": ["Un\u00b7schuld\u00b7voll", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Die ersten,", "tokens": ["Die", "ers\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Die reinsten Hauche der Sch\u00f6pfung!", "tokens": ["Die", "reins\u00b7ten", "Hau\u00b7che", "der", "Sch\u00f6p\u00b7fung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}