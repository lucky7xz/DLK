{"textgrid.poem.31417": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gleichwie der Wind sich pflegt gem\u00e4hlig anzufangen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie der Wind sich pflegt gem\u00e4hlig anzufangen /", "tokens": ["Gleich\u00b7wie", "der", "Wind", "sich", "pflegt", "ge\u00b7m\u00e4h\u00b7lig", "an\u00b7zu\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVFIN", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und mehrt sich mehr und mehr / bi\u00df er mit gro\u00dfem Schall", "tokens": ["und", "mehrt", "sich", "mehr", "und", "mehr", "/", "bi\u00df", "er", "mit", "gro\u00b7\u00dfem", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "KON", "ADV", "$(", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "die dicken B\u00fcsch durchdringt und brauset \u00fcberall /", "tokens": ["die", "di\u00b7cken", "B\u00fcsch", "durch\u00b7dringt", "und", "brau\u00b7set", "\u00fc\u00b7be\u00b7rall", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "weil immer fort mehr D\u00e4mpf' und L\u00fcfft' ihm zugegangen:", "tokens": ["weil", "im\u00b7mer", "fort", "mehr", "D\u00e4mpf'", "und", "L\u00fcfft'", "ihm", "zu\u00b7ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKVZ", "PIAT", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Also vermehrt sich auch / Herr Jesu / mein verlangen", "tokens": ["Al\u00b7so", "ver\u00b7mehrt", "sich", "auch", "/", "Herr", "Je\u00b7su", "/", "mein", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PRF", "ADV", "$(", "NN", "NE", "$(", "PPOSAT", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "nach dir / dieweil es mehrt der Gnaden-starke Schwall /", "tokens": ["nach", "dir", "/", "die\u00b7weil", "es", "mehrt", "der", "Gna\u00b7den\u00b7star\u00b7ke", "Schwall", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "KOUS", "PPER", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "der im Ged\u00e4chtnu\u00df fand den Lobes-Widerhall /", "tokens": ["der", "im", "Ge\u00b7d\u00e4cht\u00b7nu\u00df", "fand", "den", "Lo\u00b7bes\u00b7Wi\u00b7der\u00b7hall", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da\u00df aus dem Vorzug sie der hell-erklingung rangen.", "tokens": ["da\u00df", "aus", "dem", "Vor\u00b7zug", "sie", "der", "hell\u00b7er\u00b7klin\u00b7gung", "ran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "zu Zeiten bringt der Wind auch Regens Fruchtbarkeit:", "tokens": ["zu", "Zei\u00b7ten", "bringt", "der", "Wind", "auch", "Re\u00b7gens", "Frucht\u00b7bar\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ingleichen Gottes Gnad / der Amteswerk gedeyen.", "tokens": ["in\u00b7glei\u00b7chen", "Got\u00b7tes", "Gnad", "/", "der", "Am\u00b7tes\u00b7werk", "ge\u00b7de\u00b7yen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "bi\u00dfweilen w\u00fcrket er die Sonnenklare Zeit:", "tokens": ["bi\u00df\u00b7wei\u00b7len", "w\u00fcr\u00b7ket", "er", "die", "Son\u00b7nen\u00b7kla\u00b7re", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "wie diese uns auch kan herzinniglich erfreuen.", "tokens": ["wie", "die\u00b7se", "uns", "auch", "kan", "her\u00b7zin\u00b7nig\u00b7lich", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PPER", "ADV", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "der Wind hat in dem Wald nie so viel Laub bewegt /", "tokens": ["der", "Wind", "hat", "in", "dem", "Wald", "nie", "so", "viel", "Laub", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "als Gottes Gnad' in mir hat Hoffnungs-Trost erregt.", "tokens": ["als", "Got\u00b7tes", "Gnad'", "in", "mir", "hat", "Hoff\u00b7nungs\u00b7Trost", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "APPR", "PPER", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}