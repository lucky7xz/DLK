{"textgrid.poem.34620": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Ein anders/ auff eben dieselbe bilder-s\u00e4ule", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der stoltze Ludewig zeigt hier/ wie er gekriegt/", "tokens": ["Der", "stolt\u00b7ze", "Lu\u00b7de\u00b7wig", "zeigt", "hier", "/", "wie", "er", "ge\u00b7kriegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADV", "$(", "PWAV", "PPER", "VVPP", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie er die feinde bindt/ die er doch nie besiegt/", "tokens": ["Wie", "er", "die", "fein\u00b7de", "bindt", "/", "die", "er", "doch", "nie", "be\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$(", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie er den frieden giebt/ den er doch schl\u00fcssen m\u00fcssen/", "tokens": ["Wie", "er", "den", "frie\u00b7den", "giebt", "/", "den", "er", "doch", "schl\u00fcs\u00b7sen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$(", "PRELS", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie er die eintracht sucht/ die er doch stets zerrissen;", "tokens": ["Wie", "er", "die", "ein\u00b7tracht", "sucht", "/", "die", "er", "doch", "stets", "zer\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$(", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Wie er ein land verschenckt/ das ihm niemahls geb\u00fchrt.", "tokens": ["Wie", "er", "ein", "land", "ver\u00b7schenckt", "/", "das", "ihm", "nie\u00b7mahls", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$(", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Das aber zeigt er nicht/ wie er die welt auffr\u00fchrt:", "tokens": ["Das", "a\u00b7ber", "zeigt", "er", "nicht", "/", "wie", "er", "die", "welt", "auf\u00b7fr\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "PTKNEG", "$(", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie er die kirche kr\u00e4nckt/ die er doch soll besch\u00fctzen;", "tokens": ["Wie", "er", "die", "kir\u00b7che", "kr\u00e4nckt", "/", "die", "er", "doch", "soll", "be\u00b7sch\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$(", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie er auff Pabst und Rom l\u00e4st seinen donner blitzen:", "tokens": ["Wie", "er", "auff", "Pabst", "und", "Rom", "l\u00e4st", "sei\u00b7nen", "don\u00b7ner", "blit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NE", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den allerbesten freund um cron und scepter bringt/", "tokens": ["Den", "al\u00b7ler\u00b7bes\u00b7ten", "freund", "um", "cron", "und", "scep\u00b7ter", "bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auff katzen steuren legt/ sein volck zu betteln zwingt/", "tokens": ["Auff", "kat\u00b7zen", "steu\u00b7ren", "legt", "/", "sein", "volck", "zu", "bet\u00b7teln", "zwingt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VVFIN", "$(", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und fremde reiche sucht/ die seines fast verschlingen:", "tokens": ["Und", "frem\u00b7de", "rei\u00b7che", "sucht", "/", "die", "sei\u00b7nes", "fast", "ver\u00b7schlin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "VVFIN", "$(", "ART", "PPOSAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was mu\u00df den k\u00f6nig doch zu dieser thorheit bringen?", "tokens": ["Was", "mu\u00df", "den", "k\u00f6\u00b7nig", "doch", "zu", "die\u00b7ser", "thor\u00b7heit", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich glaub/ er l\u00e4st uns hier/ weil nicht die that geschehn/", "tokens": ["Ich", "glaub", "/", "er", "l\u00e4st", "uns", "hier", "/", "weil", "nicht", "die", "that", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PTKNEG", "ART", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ihm die krafft gebricht/ nur seinen willen sehn.", "tokens": ["Und", "ihm", "die", "krafft", "ge\u00b7bricht", "/", "nur", "sei\u00b7nen", "wil\u00b7len", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$(", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach aber armer held/ verspare deinen willen!", "tokens": ["Ach", "a\u00b7ber", "ar\u00b7mer", "held", "/", "ver\u00b7spa\u00b7re", "dei\u00b7nen", "wil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADJA", "VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man l\u00e4st sich heute nicht mit leeren wollen f\u00fcllen.", "tokens": ["Man", "l\u00e4st", "sich", "heu\u00b7te", "nicht", "mit", "lee\u00b7ren", "wol\u00b7len", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "PTKNEG", "APPR", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein leben ist uns schon so gut als dir bekandt:", "tokens": ["Dein", "le\u00b7ben", "ist", "uns", "schon", "so", "gut", "als", "dir", "be\u00b7kandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Drum meide nur den schein und allen falschen tand;", "tokens": ["Drum", "mei\u00b7de", "nur", "den", "schein", "und", "al\u00b7len", "fal\u00b7schen", "tand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "KON", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die nachwelt m\u00f6chte sonst/ wenn sie dein lob wird lesen/", "tokens": ["Die", "nach\u00b7welt", "m\u00f6ch\u00b7te", "sonst", "/", "wenn", "sie", "dein", "lob", "wird", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Gedencken/ dieses bild sey Leopold gewesen.", "tokens": ["Ge\u00b7den\u00b7cken", "/", "die\u00b7ses", "bild", "sey", "Leo\u00b7pold", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDAT", "NN", "VAFIN", "NE", "VAPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}