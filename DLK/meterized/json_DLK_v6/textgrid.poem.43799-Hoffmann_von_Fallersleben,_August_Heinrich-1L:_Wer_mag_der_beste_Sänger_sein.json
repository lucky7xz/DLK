{"textgrid.poem.43799": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer mag der beste S\u00e4nger sein", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer mag der beste S\u00e4nger sein", "tokens": ["Wer", "mag", "der", "bes\u00b7te", "S\u00e4n\u00b7ger", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allen S\u00e4ngern gro\u00df und klein?", "tokens": ["Von", "al\u00b7len", "S\u00e4n\u00b7gern", "gro\u00df", "und", "klein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun rathet, wer da rathen kann,", "tokens": ["Nun", "ra\u00b7thet", ",", "wer", "da", "ra\u00b7then", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besinnt euch recht und sagt es dann.", "tokens": ["Be\u00b7sinnt", "euch", "recht", "und", "sagt", "es", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist das liebe V\u00f6gelein,", "tokens": ["Das", "ist", "das", "lie\u00b7be", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das singet also h\u00fcbsch und fein,", "tokens": ["Das", "sin\u00b7get", "al\u00b7so", "h\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie's kann kein S\u00e4nger in der Stadt:", "tokens": ["Wie's", "kann", "kein", "S\u00e4n\u00b7ger", "in", "der", "Stadt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es singet Alles frisch vom Blatt.", "tokens": ["Es", "sin\u00b7get", "Al\u00b7les", "frisch", "vom", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}