{"textgrid.poem.59147": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "366. Ich bin der Weg \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r doch, warum ich kommen bin", "tokens": ["H\u00f6r", "doch", ",", "wa\u00b7rum", "ich", "kom\u00b7men", "bin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PWAV", "PPER", "VVINF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Himmelsthron zum Stall auf Erden!", "tokens": ["Vom", "Him\u00b7melst\u00b7hron", "zum", "Stall", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df du durch mich mit Herz und Sinn", "tokens": ["Da\u00df", "du", "durch", "mich", "mit", "Herz", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Himmel sollst gef\u00fchret werden.", "tokens": ["Zum", "Him\u00b7mel", "sollst", "ge\u00b7f\u00fch\u00b7ret", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}