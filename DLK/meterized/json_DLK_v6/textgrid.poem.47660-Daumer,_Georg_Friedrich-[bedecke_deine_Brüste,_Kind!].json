{"textgrid.poem.47660": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[bedecke deine Br\u00fcste, Kind!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbbedecke deine Br\u00fcste, Kind!", "tokens": ["\u00bb", "be\u00b7de\u00b7cke", "dei\u00b7ne", "Br\u00fcs\u00b7te", ",", "Kind", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Knabe sieht sie, ein loser Dieb.\u00ab \u2013", "tokens": ["Ein", "Kna\u00b7be", "sieht", "sie", ",", "ein", "lo\u00b7ser", "Dieb", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er sehe sie oder er sehe sie nicht;", "tokens": ["Er", "se\u00b7he", "sie", "o\u00b7der", "er", "se\u00b7he", "sie", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Denn diesen Knaben, ich hab' ihn lieb.", "tokens": ["Denn", "die\u00b7sen", "Kna\u00b7ben", ",", "ich", "hab'", "ihn", "lieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}