{"textgrid.poem.66501": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stimmen der Nacht!", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stimmen der Nacht!", "tokens": ["Stim\u00b7men", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Hebt aus der Tiefe", "tokens": ["Hebt", "aus", "der", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "D\u00e4mmerndem Grunde", "tokens": ["D\u00e4m\u00b7mern\u00b7dem", "Grun\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Leise die Schwingen", "tokens": ["Lei\u00b7se", "die", "Schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ueber die Welt!", "tokens": ["Ue\u00b7ber", "die", "Welt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wehen des Windes,", "tokens": ["We\u00b7hen", "des", "Win\u00b7des", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Rauschen der Wellen,", "tokens": ["Rau\u00b7schen", "der", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Nachtigallt\u00f6ne,", "tokens": ["Nach\u00b7ti\u00b7gall\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Wecket den Wiederhall", "tokens": ["We\u00b7cket", "den", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Fern im Gekl\u00fcft!", "tokens": ["Fern", "im", "Ge\u00b7kl\u00fcft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ihr nimmer rastenden,", "tokens": ["Ihr", "nim\u00b7mer", "ras\u00b7ten\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr die Stunde", "tokens": ["Die", "ihr", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wisset, da m\u00e4chtiger", "tokens": ["Wis\u00b7set", ",", "da", "m\u00e4ch\u00b7ti\u00b7ger"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ADJA"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Schwillt durch die Th\u00e4ler", "tokens": ["Schwillt", "durch", "die", "Th\u00e4\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Euer Gesang:", "tokens": ["Eu\u00b7er", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Unter dem bergenden", "tokens": ["Un\u00b7ter", "dem", "ber\u00b7gen\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Schleier des Dunkels", "tokens": ["Schlei\u00b7er", "des", "Dun\u00b7kels"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Singet ein Traumlied", "tokens": ["Sin\u00b7get", "ein", "Traum\u00b7lied"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Der schlummernden Erde,", "tokens": ["Der", "schlum\u00b7mern\u00b7den", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Stimmen der Nacht!", "tokens": ["Stim\u00b7men", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "\u00bbwas wir verk\u00fcnden,", "tokens": ["\u00bb", "was", "wir", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "H\u00f6ren die Schl\u00e4fer nicht,", "tokens": ["H\u00f6\u00b7ren", "die", "Schl\u00e4\u00b7fer", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Tr\u00e4umen nur weiter", "tokens": ["Tr\u00e4u\u00b7men", "nur", "wei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "All' ihres Tages", "tokens": ["All'", "ih\u00b7res", "Ta\u00b7ges"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Fl\u00fcchtigen Schein.", "tokens": ["Fl\u00fcch\u00b7ti\u00b7gen", "Schein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Aber der Wachende,", "tokens": ["A\u00b7ber", "der", "Wa\u00b7chen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.7": {"text": "Der in die Seele", "tokens": ["Der", "in", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.8": {"text": "Senket die Blicke,", "tokens": ["Sen\u00b7ket", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Pr\u00fcfend und schauernd,", "tokens": ["Pr\u00fc\u00b7fend", "und", "schau\u00b7ernd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "H\u00f6re das Lied!", "tokens": ["H\u00f6\u00b7re", "das", "Lied", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Unter den Wogen", "tokens": ["Un\u00b7ter", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Starrt es von Klippen,", "tokens": ["Starrt", "es", "von", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Senken sich Felsen,", "tokens": ["Sen\u00b7ken", "sich", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "St\u00fcrzet zum Abgrund", "tokens": ["St\u00fcr\u00b7zet", "zum", "Ab\u00b7grund"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wirbelnd die Fluth.", "tokens": ["Wir\u00b7belnd", "die", "Fluth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Droben die Fl\u00e4che", "tokens": ["Dro\u00b7ben", "die", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Kr\u00e4uselt nur spielend,", "tokens": ["Kr\u00e4u\u00b7selt", "nur", "spie\u00b7lend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Was aus dem Kampfe", "tokens": ["Was", "aus", "dem", "Kamp\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Senden die Tiefen,", "tokens": ["Sen\u00b7den", "die", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Als leichten Schaum.", "tokens": ["Als", "leich\u00b7ten", "Schaum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wehen des Windes", "tokens": ["We\u00b7hen", "des", "Win\u00b7des"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Streift um die Knospen,", "tokens": ["Streift", "um", "die", "Knos\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "K\u00fc\u00dft die erwachenden,", "tokens": ["K\u00fc\u00dft", "die", "er\u00b7wa\u00b7chen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df sie als Bl\u00fcthen", "tokens": ["Da\u00df", "sie", "als", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Gr\u00fc\u00dfen den Tag.", "tokens": ["Gr\u00fc\u00b7\u00dfen", "den", "Tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Kommt es auf Fl\u00fcgeln", "tokens": ["Kommt", "es", "auf", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sausenden Sturmes,", "tokens": ["Sau\u00b7sen\u00b7den", "Stur\u00b7mes", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Bricht es die Knospen,", "tokens": ["Bricht", "es", "die", "Knos\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Streut es zu Boden", "tokens": ["Streut", "es", "zu", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Bl\u00fcthen und Laub.", "tokens": ["Bl\u00fc\u00b7then", "und", "Laub", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.