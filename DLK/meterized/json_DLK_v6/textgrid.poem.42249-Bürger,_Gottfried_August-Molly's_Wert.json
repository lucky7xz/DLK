{"textgrid.poem.42249": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Molly's Wert", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, k\u00f6nnt' ich Molly kaufen", "tokens": ["Ach", ",", "k\u00f6nnt'", "ich", "Mol\u00b7ly", "kau\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr Gold und Edelstein,", "tokens": ["F\u00fcr", "Gold", "und", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00e4tte gro\u00dfe Haufen;", "tokens": ["Und", "h\u00e4t\u00b7te", "gro\u00b7\u00dfe", "Hau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sollten mich nicht reu'n.", "tokens": ["Die", "soll\u00b7ten", "mich", "nicht", "reu'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zwar w\u00fchlt sich's h\u00fcbsch im Golde;", "tokens": ["Zwar", "w\u00fchlt", "sich's", "h\u00fcbsch", "im", "Gol\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohl dem, der w\u00fchlen kann! \u2013", "tokens": ["Wohl", "dem", ",", "der", "w\u00fch\u00b7len", "kann", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "VVINF", "VMFIN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Doch ohne sie, die Holde,", "tokens": ["Doch", "oh\u00b7ne", "sie", ",", "die", "Hol\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was h\u00e4tt' ich Frohes d'ran?", "tokens": ["Was", "h\u00e4tt'", "ich", "Fro\u00b7hes", "d'\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ja, wenn ich der Regente", "tokens": ["Ja", ",", "wenn", "ich", "der", "Re\u00b7gen\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von ganz Europa w\u00e4r',", "tokens": ["Von", "ganz", "Eu\u00b7ro\u00b7pa", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NE", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Molly kaufen k\u00f6nnte;", "tokens": ["Und", "Mol\u00b7ly", "kau\u00b7fen", "k\u00f6nn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So g\u00e4b ich alles her.", "tokens": ["So", "g\u00e4b", "ich", "al\u00b7les", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vor St\u00e4dten, Schl\u00f6ssern, Thronen,", "tokens": ["Vor", "St\u00e4d\u00b7ten", ",", "Schl\u00f6s\u00b7sern", ",", "Thro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und mancher fetten Flur,", "tokens": ["Und", "man\u00b7cher", "fet\u00b7ten", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "W\u00e4hlt' ich mit ihr zu wohnen,", "tokens": ["W\u00e4hlt'", "ich", "mit", "ihr", "zu", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Gartenh\u00fcttchen nur.", "tokens": ["Ein", "Gar\u00b7ten\u00b7h\u00fctt\u00b7chen", "nur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein liebes Leben enden", "tokens": ["Mein", "lie\u00b7bes", "Le\u00b7ben", "en\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darf nur der Herr der Welt.", "tokens": ["Darf", "nur", "der", "Herr", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch d\u00fcrft' ich es verspenden,", "tokens": ["Doch", "d\u00fcrft'", "ich", "es", "ver\u00b7spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wie mein Gut und Geld;", "tokens": ["So", "wie", "mein", "Gut", "und", "Geld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So g\u00e4b' ich gern, ich schw\u00f6re!", "tokens": ["So", "g\u00e4b'", "ich", "gern", ",", "ich", "schw\u00f6\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr jeden Tag ein Jahr,", "tokens": ["F\u00fcr", "je\u00b7den", "Tag", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da sie mein eigen w\u00e4re,", "tokens": ["Da", "sie", "mein", "ei\u00b7gen", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein eigen ganz und gar.", "tokens": ["Mein", "ei\u00b7gen", "ganz", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}