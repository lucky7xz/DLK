{"dta.poem.5875": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es will der Geist nicht gern daran,", "tokens": ["Es", "will", "der", "Geist", "nicht", "gern", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem, welches ", "tokens": ["Dem", ",", "wel\u00b7ches"], "token_info": ["word", "punct", "word"], "pos": ["PDS", "$,", "PRELS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und dies jedoch alleine kan", "tokens": ["Und", "dies", "je\u00b7doch", "al\u00b7lei\u00b7ne", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "ADV", "VMFIN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Uns sicher zu der Wahrheit f\u00fchren.", "tokens": ["Uns", "si\u00b7cher", "zu", "der", "Wahr\u00b7heit", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was auch die w\u00fcrckende Natur", "tokens": ["Was", "auch", "die", "w\u00fcr\u00b7cken\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr unterschiedne Mittel wehlet;", "tokens": ["F\u00fcr", "un\u00b7ter\u00b7schied\u00b7ne", "Mit\u00b7tel", "weh\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So sucht sie allezeit des k\u00fcrtzten Weges Spur,", "tokens": ["So", "sucht", "sie", "al\u00b7le\u00b7zeit", "des", "k\u00fcrtz\u00b7ten", "We\u00b7ges", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und, sonder da\u00df sie jemals fehlet,", "tokens": ["Und", ",", "son\u00b7der", "da\u00df", "sie", "je\u00b7mals", "feh\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gelangt sie allezeit", "tokens": ["Ge\u00b7langt", "sie", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Zum vorgesteckten Zweck, blos durch die Einzelnheit,", "tokens": ["Zum", "vor\u00b7ge\u00b7steck\u00b7ten", "Zweck", ",", "blos", "durch", "die", "Ein\u00b7zeln\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vom ersten Ursprung an verbleibt sie immerfort", "tokens": ["Vom", "ers\u00b7ten", "Ur\u00b7sprung", "an", "ver\u00b7bleibt", "sie", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu aller Zeit, an jedem Ort,", "tokens": ["Zu", "al\u00b7ler", "Zeit", ",", "an", "je\u00b7dem", "Ort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dieselbige. Die C\u00f6rper m\u00f6gen seyn", "tokens": ["Die\u00b7sel\u00b7bi\u00b7ge", ".", "Die", "C\u00f6r\u00b7per", "m\u00f6\u00b7gen", "seyn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "$.", "ART", "NN", "VMFIN", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Voll Dunckelheit, voll Glantz, Gold, Koht, grob oder zart,", "tokens": ["Voll", "Dun\u00b7ckel\u00b7heit", ",", "voll", "Glantz", ",", "Gold", ",", "Koht", ",", "grob", "o\u00b7der", "zart", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,", "NN", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vom Abgrund an, bis zu des Himmels H\u00f6h\u2019;", "tokens": ["Vom", "Ab\u00b7grund", "an", ",", "bis", "zu", "des", "Him\u00b7mels", "H\u00f6h'", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KOUS", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Sind sie von einerley Materie", "tokens": ["Sind", "sie", "von", "ei\u00b7ner\u00b7ley", "Ma\u00b7te\u00b7rie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Hervorgebracht, und zwar auf gleiche Art.", "tokens": ["Her\u00b7vor\u00b7ge\u00b7bracht", ",", "und", "zwar", "auf", "glei\u00b7che", "Art", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wenn wir auf Dinge, die entstehen,", "tokens": ["Wenn", "wir", "auf", "Din\u00b7ge", ",", "die", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Aufmercksam sehen;", "tokens": ["Auf\u00b7merck\u00b7sam", "se\u00b7hen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "So werden wir sogleich befinden:", "tokens": ["So", "wer\u00b7den", "wir", "sog\u00b7leich", "be\u00b7fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df sie auf gleiche Art sich binden,", "tokens": ["Da\u00df", "sie", "auf", "glei\u00b7che", "Art", "sich", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Als wie die Bau-Kunst, wenn sie bauen", "tokens": ["Als", "wie", "die", "Bau\u00b7Kunst", ",", "wenn", "sie", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}