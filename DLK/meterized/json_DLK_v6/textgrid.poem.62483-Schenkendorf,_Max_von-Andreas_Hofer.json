{"textgrid.poem.62483": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Andreas Hofer", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als der Sandwirth von Passeier", "tokens": ["Als", "der", "Sand\u00b7wirth", "von", "Pas\u00b7sei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Inspruck hat mit Sturm genommen,", "tokens": ["I\u00b7nspruck", "hat", "mit", "Sturm", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Studenten, ihm zur Feier,", "tokens": ["Die", "Stu\u00b7den\u00b7ten", ",", "ihm", "zur", "Fei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit den Geigen Mittags kommen,", "tokens": ["Mit", "den", "Gei\u00b7gen", "Mit\u00b7tags", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Laufen alle aus der Lehre,", "tokens": ["Lau\u00b7fen", "al\u00b7le", "aus", "der", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm ein Hochvivat zu bringen,", "tokens": ["Ihm", "ein", "Hoch\u00b7vi\u00b7vat", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollen ihm zu seiner Ehre", "tokens": ["Wol\u00b7len", "ihm", "zu", "sei\u00b7ner", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine Heldenthaten singen.", "tokens": ["Sei\u00b7ne", "Hel\u00b7den\u00b7tha\u00b7ten", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch der Held gebietet Stille,", "tokens": ["Doch", "der", "Held", "ge\u00b7bie\u00b7tet", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht dann ernst: legt hin die Geigen!", "tokens": ["Spricht", "dann", "ernst", ":", "legt", "hin", "die", "Gei\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$.", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ernst ist Gottes Kriegeswille,", "tokens": ["Ernst", "ist", "Got\u00b7tes", "Krie\u00b7ges\u00b7wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir sind all' dem Tode eigen.", "tokens": ["Wir", "sind", "all'", "dem", "To\u00b7de", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich lie\u00df nicht um lust'ge Spiele", "tokens": ["Ich", "lie\u00df", "nicht", "um", "lust'\u00b7ge", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Weib und Kind in Thr\u00e4nen liegen;", "tokens": ["Weib", "und", "Kind", "in", "Thr\u00e4\u00b7nen", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil ich nach dem Himmel ziele,", "tokens": ["Weil", "ich", "nach", "dem", "Him\u00b7mel", "zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann ich ird'sche Feind' besiegen.", "tokens": ["Kann", "ich", "ird'\u00b7sche", "Feind'", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kniet bei euren Rosenkr\u00e4nzen,", "tokens": ["Kniet", "bei", "eu\u00b7ren", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dies sind meine frohsten Geigen;", "tokens": ["Dies", "sind", "mei\u00b7ne", "frohs\u00b7ten", "Gei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Augen betend gl\u00e4nzen,", "tokens": ["Wenn", "die", "Au\u00b7gen", "be\u00b7tend", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sich Gott der Herr drin zeigen.", "tokens": ["Wird", "sich", "Gott", "der", "Herr", "drin", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Betet leise f\u00fcr mich Armen,", "tokens": ["Be\u00b7tet", "lei\u00b7se", "f\u00fcr", "mich", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Betet laut f\u00fcr unsern Kaiser,", "tokens": ["Be\u00b7tet", "laut", "f\u00fcr", "un\u00b7sern", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dies ist mir das liebste Carmen:", "tokens": ["Dies", "ist", "mir", "das", "liebs\u00b7te", "Car\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gott sch\u00fctz' edle F\u00fcrstenh\u00e4user!", "tokens": ["Gott", "sch\u00fctz'", "ed\u00b7le", "F\u00fcrs\u00b7ten\u00b7h\u00e4u\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich hab' keine Zeit zum Beten,", "tokens": ["Ich", "hab'", "kei\u00b7ne", "Zeit", "zum", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt dem Herrn der Welt, wie's stehe,", "tokens": ["Sagt", "dem", "Herrn", "der", "Welt", ",", "wie's", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "PWAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie viel Leichen wir hier s\u00e4ten", "tokens": ["Wie", "viel", "Lei\u00b7chen", "wir", "hier", "s\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Thal und auf der H\u00f6he,", "tokens": ["In", "dem", "Thal", "und", "auf", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie wir hungern, wie wir wachen", "tokens": ["Wie", "wir", "hun\u00b7gern", ",", "wie", "wir", "wa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wie viele brave Sch\u00fctzen", "tokens": ["Und", "wie", "vie\u00b7le", "bra\u00b7ve", "Sch\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIAT", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.7": {"text": "Nicht mehr schie\u00dfen, nicht mehr lachen \u2013", "tokens": ["Nicht", "mehr", "schie\u00b7\u00dfen", ",", "nicht", "mehr", "la\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$,", "PTKNEG", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gott allein kann uns besch\u00fctzen!", "tokens": ["Gott", "al\u00b7lein", "kann", "uns", "be\u00b7sch\u00fct\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}