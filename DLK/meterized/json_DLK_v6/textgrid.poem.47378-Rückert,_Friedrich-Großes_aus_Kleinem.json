{"textgrid.poem.47378": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dfes aus Kleinem", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sagest mir: \u00bbO nicht zersplittre", "tokens": ["Du", "sa\u00b7gest", "mir", ":", "\u00bb", "O", "nicht", "zer\u00b7splitt\u00b7re"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "$(", "NE", "PTKNEG", "VVFIN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Lieder dich! web' ein Gedicht!\u00ab", "tokens": ["In", "Lie\u00b7der", "dich", "!", "web'", "ein", "Ge\u00b7dicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PPER", "$.", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich aber sage dir: Verbittre", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", "dir", ":", "Ver\u00b7bitt\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir die unschuld'ge Freude nicht!", "tokens": ["Mir", "die", "un\u00b7schuld'\u00b7ge", "Freu\u00b7de", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Sieh hin, wie auf der Aue", "tokens": ["Sieh", "hin", ",", "wie", "auf", "der", "Au\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Sonne Licht sich bricht", "tokens": ["Der", "Son\u00b7ne", "Licht", "sich", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In jedem Tropfen Taue!", "tokens": ["In", "je\u00b7dem", "Trop\u00b7fen", "Tau\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich mich kann in Tropfen spiegeln,", "tokens": ["Wenn", "ich", "mich", "kann", "in", "Trop\u00b7fen", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was soll ich Teiche legen an?", "tokens": ["Was", "soll", "ich", "Tei\u00b7che", "le\u00b7gen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Meere st\u00fcrmisch aufzuwiegeln,", "tokens": ["Und", "Mee\u00b7re", "st\u00fcr\u00b7misch", "auf\u00b7zu\u00b7wie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheint vollends mir nicht wohlgethan.", "tokens": ["Scheint", "vol\u00b7lends", "mir", "nicht", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir gn\u00fcgt's am leisen Klange,", "tokens": ["Mir", "gn\u00fcgt's", "am", "lei\u00b7sen", "Klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den ich gewann zum Bann", "tokens": ["Den", "ich", "ge\u00b7wann", "zum", "Bann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jedwedem Herzensdrange.", "tokens": ["Jed\u00b7we\u00b7dem", "Her\u00b7zens\u00b7dran\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ein Teppich scheinet mir mein Leben,", "tokens": ["Ein", "Tep\u00b7pich", "schei\u00b7net", "mir", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer sticket meine Hand;", "tokens": ["Und", "im\u00b7mer", "sti\u00b7cket", "mei\u00b7ne", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An welcher Stell' ich auch mag weben,", "tokens": ["An", "wel\u00b7cher", "Stell'", "ich", "auch", "mag", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am obern oder untern Rand;", "tokens": ["Am", "o\u00b7bern", "o\u00b7der", "un\u00b7tern", "Rand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zuletzt, wo so viel Kleinstes", "tokens": ["Zu\u00b7letzt", ",", "wo", "so", "viel", "Kleins\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich still verband, entstand", "tokens": ["Sich", "still", "ver\u00b7band", ",", "ent\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PRF", "ADJD", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Gro\u00dfes, Allgemeinstes.", "tokens": ["Ein", "Gro\u00b7\u00dfes", ",", "All\u00b7ge\u00b7meins\u00b7tes", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}