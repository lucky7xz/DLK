{"dta.poem.7537": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Weiser sprach: Ich hab' auf meines Lebens Bahn", "tokens": ["Ein", "Wei\u00b7ser", "sprach", ":", "Ich", "hab'", "auf", "mei\u00b7nes", "Le\u00b7bens", "Bahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie einem Menschen weh, nie einem wohl gethan.", "tokens": ["Nie", "ei\u00b7nem", "Men\u00b7schen", "weh", ",", "nie", "ei\u00b7nem", "wohl", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$,", "ADV", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Unweise fragten ihn: Was also thatest du?", "tokens": ["Un\u00b7wei\u00b7se", "frag\u00b7ten", "ihn", ":", "Was", "al\u00b7so", "tha\u00b7test", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "PWS", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sprach: Ich that nur mir, was ich f\u00fcgt' andern zu.", "tokens": ["Er", "sprach", ":", "Ich", "that", "nur", "mir", ",", "was", "ich", "f\u00fcgt'", "an\u00b7dern", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PPER", "$,", "PWS", "PPER", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wohl that mir, was ich wohl gethan, und weh, was wehe;", "tokens": ["Wohl", "that", "mir", ",", "was", "ich", "wohl", "ge\u00b7than", ",", "und", "weh", ",", "was", "we\u00b7he", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVPP", "$,", "KON", "PTKVZ", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum that ich keinem, was ich that da\u00df mir's geschehe.", "tokens": ["Drum", "that", "ich", "kei\u00b7nem", ",", "was", "ich", "that", "da\u00df", "mir's", "ge\u00b7sche\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIS", "$,", "PWS", "PPER", "VVFIN", "KOUS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}