{"dta.poem.18896": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcnftiger Divan .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ja! Lieben ist ein gross Verdienst!", "tokens": ["Ja", "!", "Lie\u00b7ben", "ist", "ein", "gross", "Ver\u00b7dienst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJA", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer findet sch\u00f6neren Gewinnst? \u2014", "tokens": ["Wer", "fin\u00b7det", "sch\u00f6\u00b7ne\u00b7ren", "Ge\u00b7winnst", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst nicht m\u00e4chtig, wirst nicht reich;", "tokens": ["Du", "wirst", "nicht", "m\u00e4ch\u00b7tig", ",", "wirst", "nicht", "reich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jedoch den gr\u00f6ssten Helden gleich.", "tokens": ["Je\u00b7doch", "den", "gr\u00f6ss\u00b7ten", "Hel\u00b7den", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man wird, so gut wie vom Propheten,", "tokens": ["Man", "wird", ",", "so", "gut", "wie", "vom", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "ADV", "ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Wamik und von Asra reden. \u2014", "tokens": ["Von", "Wa\u00b7mik", "und", "von", "As\u00b7ra", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht reden wird man, wird sie nennen:", "tokens": ["Nicht", "re\u00b7den", "wird", "man", ",", "wird", "sie", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VAFIN", "PIS", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Namen m\u00fcssen alle kennen.", "tokens": ["Die", "Na\u00b7men", "m\u00fcs\u00b7sen", "al\u00b7le", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Was sie gethan, was sie ge\u00fcbt", "tokens": ["Was", "sie", "ge\u00b7than", ",", "was", "sie", "ge\u00b7\u00fcbt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das weiss kein Mensch! Dass sie geliebt", "tokens": ["Das", "weiss", "kein", "Mensch", "!", "Dass", "sie", "ge\u00b7liebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$.", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das wissen wir. Genug gesagt!", "tokens": ["Das", "wis\u00b7sen", "wir", ".", "Ge\u00b7nug", "ge\u00b7sagt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn man nach Wamik und Asra fragt.", "tokens": ["Wenn", "man", "nach", "Wa\u00b7mik", "und", "As\u00b7ra", "fragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "KON", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}