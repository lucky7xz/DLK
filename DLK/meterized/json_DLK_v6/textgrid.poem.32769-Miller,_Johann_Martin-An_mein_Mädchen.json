{"textgrid.poem.32769": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An mein M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist doch nie so wohl zu Mut,", "tokens": ["Mir", "ist", "doch", "nie", "so", "wohl", "zu", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn du bei mir bist,", "tokens": ["Als", "wenn", "du", "bei", "mir", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und deine Brust an meiner ruht,", "tokens": ["Und", "dei\u00b7ne", "Brust", "an", "mei\u00b7ner", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Mund den meinen k\u00fc\u00dft;", "tokens": ["Dein", "Mund", "den", "mei\u00b7nen", "k\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann schwindet alles um mich her,", "tokens": ["Dann", "schwin\u00b7det", "al\u00b7les", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df von aller Welt nichts mehr.", "tokens": ["Ich", "wei\u00df", "von", "al\u00b7ler", "Welt", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Freundekreis, beim Becher Wein,", "tokens": ["Im", "Freun\u00b7de\u00b7kreis", ",", "beim", "Be\u00b7cher", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da bin ich freilich gern:", "tokens": ["Da", "bin", "ich", "frei\u00b7lich", "gern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch, f\u00e4llst du mir, mein M\u00e4dchen, ein,", "tokens": ["Doch", ",", "f\u00e4llst", "du", "mir", ",", "mein", "M\u00e4d\u00b7chen", ",", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schnell ist die Freude fern;", "tokens": ["Schnell", "ist", "die", "Freu\u00b7de", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und bis ich wieder bei dir bin,", "tokens": ["Und", "bis", "ich", "wie\u00b7der", "bei", "dir", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "K\u00f6mmt keine Ruh' in meinen Sinn.", "tokens": ["K\u00f6mmt", "kei\u00b7ne", "Ruh'", "in", "mei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O w\u00e4re doch die Zeit schon da,", "tokens": ["O", "w\u00e4\u00b7re", "doch", "die", "Zeit", "schon", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die noch so ferne scheint,", "tokens": ["Die", "noch", "so", "fer\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da am Altar ein freudig Ja", "tokens": ["Da", "am", "Al\u00b7tar", "ein", "freu\u00b7dig", "Ja"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ewig uns vereint!", "tokens": ["Auf", "e\u00b7wig", "uns", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann w\u00e4r' ich Tag und Nacht bei dir,", "tokens": ["Dann", "w\u00e4r'", "ich", "Tag", "und", "Nacht", "bei", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann raubte nur der Tod dich mir!", "tokens": ["Dann", "raub\u00b7te", "nur", "der", "Tod", "dich", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}