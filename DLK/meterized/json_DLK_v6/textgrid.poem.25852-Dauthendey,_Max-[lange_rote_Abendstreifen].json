{"textgrid.poem.25852": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[lange rote Abendstreifen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange rote Abendstreifen,", "tokens": ["Lan\u00b7ge", "ro\u00b7te", "A\u00b7bend\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die W\u00e4lder schweifen schwarz", "tokens": ["Und", "die", "W\u00e4l\u00b7der", "schwei\u00b7fen", "schwarz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um den toten, bleichen See.", "tokens": ["Um", "den", "to\u00b7ten", ",", "blei\u00b7chen", "See", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem Ufer", "tokens": ["An", "dem", "U\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "In dem d\u00e4mmerdunklen Klee", "tokens": ["In", "dem", "d\u00e4m\u00b7mer\u00b7dunk\u00b7len", "Klee"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Grasen junge graue L\u00e4mmer.", "tokens": ["Gra\u00b7sen", "jun\u00b7ge", "grau\u00b7e", "L\u00e4m\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Eine Welle schluchzt,", "tokens": ["Ei\u00b7ne", "Wel\u00b7le", "schluchzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und die Steine weinen.", "tokens": ["Und", "die", "Stei\u00b7ne", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}