{"textgrid.poem.38676": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam gr\u00fcnender Oelbaum, der am wilden", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam gr\u00fcnender Oelbaum, der am wilden", "tokens": ["Ein\u00b7sam", "gr\u00fc\u00b7nen\u00b7der", "O\u00b7el\u00b7baum", ",", "der", "am", "wil\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Moosgesteine sich traurend hinbeugt, athme", "tokens": ["Moos\u00b7ge\u00b7stei\u00b7ne", "sich", "trau\u00b7rend", "hin\u00b7beugt", ",", "ath\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PRF", "ADJD", "VVPP", "$,", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "K\u00fchlung \u00fcber den Fremdling; Sommergluten", "tokens": ["K\u00fch\u00b7lung", "\u00fc\u00b7ber", "den", "Fremd\u00b7ling", ";", "Som\u00b7mer\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Spr\u00fchte der Maitag.", "tokens": ["Spr\u00fch\u00b7te", "der", "Mai\u00b7tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Hier wohnt Stille des Herzens; goldne Bilder", "tokens": ["Hier", "wohnt", "Stil\u00b7le", "des", "Her\u00b7zens", ";", "gold\u00b7ne", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "$.", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Steigen aus der Gew\u00e4sser klarem Dunkel;", "tokens": ["Stei\u00b7gen", "aus", "der", "Ge\u00b7w\u00e4s\u00b7ser", "kla\u00b7rem", "Dun\u00b7kel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "H\u00f6rbar waltet am Quell der leise Fittig", "tokens": ["H\u00f6r\u00b7bar", "wal\u00b7tet", "am", "Quell", "der", "lei\u00b7se", "Fit\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Segnender Geister.", "tokens": ["Seg\u00b7nen\u00b7der", "Geis\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Fleuch, des K\u00fcnftigen Traum! verwallt in Nebel,", "tokens": ["Fleuch", ",", "des", "K\u00fcnf\u00b7ti\u00b7gen", "Traum", "!", "ver\u00b7wallt", "in", "Ne\u00b7bel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "$.", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Eitle Schattengebilde des Vergangnen!", "tokens": ["Eit\u00b7le", "Schat\u00b7ten\u00b7ge\u00b7bil\u00b7de", "des", "Ver\u00b7gang\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Einen Tropfen der Lethe nur, und Psyche", "tokens": ["Ei\u00b7nen", "Trop\u00b7fen", "der", "Le\u00b7the", "nur", ",", "und", "Psy\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,", "KON", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Schauert vor Wonne.", "tokens": ["Schau\u00b7ert", "vor", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}