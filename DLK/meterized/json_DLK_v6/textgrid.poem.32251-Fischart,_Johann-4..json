{"textgrid.poem.32251": {"metadata": {"author": {"name": "Fischart, Johann", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1568, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dann welches schreit au\u00df seinem standt,", "tokens": ["Dann", "wel\u00b7ches", "schreit", "au\u00df", "sei\u00b7nem", "standt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAT", "NN", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dasselb zerreist das Menschlich Band,", "tokens": ["Das\u00b7selb", "zer\u00b7reist", "das", "Menschlich", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Schafft vnwill vnd gro\u00df mi\u00dfuerstandt,", "tokens": ["Schafft", "vn\u00b7will", "vnd", "gro\u00df", "mi\u00b7\u00df\u00b7uer\u00b7standt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Vnd verunr\u00fchigt Statt vnd Landt,", "tokens": ["Vnd", "ver\u00b7un\u00b7r\u00fc\u00b7higt", "Statt", "vnd", "Landt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Weil hochmut findet widerstandt.", "tokens": ["Weil", "hoch\u00b7mut", "fin\u00b7det", "wi\u00b7der\u00b7standt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darumb Gott alles recht erschuff,", "tokens": ["Da\u00b7rumb", "Gott", "al\u00b7les", "recht", "er\u00b7schuff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein jdes Geschlecht in seim beruff,", "tokens": ["Ein", "jdes", "Ge\u00b7schlecht", "in", "seim", "be\u00b7ruff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Mann dapffer mit Rath vnd Hand,", "tokens": ["Den", "Mann", "dapf\u00b7fer", "mit", "Rath", "vnd", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Weib bl\u00f6d, still zu der Hau\u00dfhaltung;", "tokens": ["Das", "Weib", "bl\u00f6d", ",", "still", "zu", "der", "Hau\u00df\u00b7hal\u00b7tung", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Vnd je stiller ist jhr verwaltung,", "tokens": ["Vnd", "je", "stil\u00b7ler", "ist", "jhr", "ver\u00b7wal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Je besser ist dieselb bestellt,", "tokens": ["Je", "bes\u00b7ser", "ist", "die\u00b7selb", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dann ins Hau\u00df gh\u00f6rt kein Rechten, fechten,", "tokens": ["Dann", "ins", "Hau\u00df", "gh\u00f6rt", "kein", "Rech\u00b7ten", ",", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Es wirdt sonst b\u00f6ses Garn sich flechten,", "tokens": ["Es", "wirdt", "sonst", "b\u00f6\u00b7ses", "Garn", "sich", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sonder auffs Rahthau\u00df vnd ins Feldt.", "tokens": ["Son\u00b7der", "auffs", "Rah\u00b7thau\u00df", "vnd", "ins", "Feldt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}