{"dta.poem.10157": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Tuberose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst trat ich in mein Schlaf-Gemach,", "tokens": ["J\u00fcngst", "trat", "ich", "in", "mein", "Schlaf\u00b7Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stutzte fast, als ein gar strenger Dufft", "tokens": ["Und", "stutz\u00b7te", "fast", ",", "als", "ein", "gar", "stren\u00b7ger", "Dufft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von einer fast ambrirten Lufft,", "tokens": ["Von", "ei\u00b7ner", "fast", "am\u00b7br\u00b7ir\u00b7ten", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als wie im Schwall, mir recht entgegen brach.", "tokens": ["Als", "wie", "im", "Schwall", ",", "mir", "recht", "ent\u00b7ge\u00b7gen", "brach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPRART", "NN", "$,", "PPER", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sucht\u2019, und fand so gleich von dieser Lieblichkeit", "tokens": ["Ich", "sucht'", ",", "und", "fand", "so", "gleich", "von", "die\u00b7ser", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Quelle, die so s\u00fc\u00df, als sch\u00f6n,", "tokens": ["Die", "Quel\u00b7le", ",", "die", "so", "s\u00fc\u00df", ",", "als", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In einem blauen Topf\u2019 an meinem Fenster stehn.", "tokens": ["In", "ei\u00b7nem", "blau\u00b7en", "Topf'", "an", "mei\u00b7nem", "Fens\u00b7ter", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die\u00df war, in bl\u00fchender Vollkommenheit,", "tokens": ["Die\u00df", "war", ",", "in", "bl\u00fc\u00b7hen\u00b7der", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Tuberosen-Topf\u2019; wovon der Glantz, die Zier,", "tokens": ["Ein", "Tu\u00b7be\u00b7ro\u00b7sen\u00b7Topf'", ";", "wo\u00b7von", "der", "Glantz", ",", "die", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWAV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die pr\u00e4chtige Figur, der Bl\u00e4tter Silber, mir", "tokens": ["Die", "pr\u00e4ch\u00b7ti\u00b7ge", "Fi\u00b7gur", ",", "der", "Bl\u00e4t\u00b7ter", "Sil\u00b7ber", ",", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,", "PPER"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sehr viel zu sehn, und mehr noch zu bewundern gab.", "tokens": ["Sehr", "viel", "zu", "sehn", ",", "und", "mehr", "noch", "zu", "be\u00b7wun\u00b7dern", "gab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKZU", "VVINF", "$,", "KON", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Bluhme zeigt, da sie so zierlich ist, als pr\u00e4chtig,", "tokens": ["Die", "Bluh\u00b7me", "zeigt", ",", "da", "sie", "so", "zier\u00b7lich", "ist", ",", "als", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df, Der, Der sie gemacht, so liebreich ist, als m\u00e4chtig.", "tokens": ["Da\u00df", ",", "Der", ",", "Der", "sie", "ge\u00b7macht", ",", "so", "lieb\u00b7reich", "ist", ",", "als", "m\u00e4ch\u00b7tig", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "ART", "$,", "ART", "PPER", "VVPP", "$,", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es kam mir vor, als ob mit s\u00fcss- und sanftem Thon", "tokens": ["Es", "kam", "mir", "vor", ",", "als", "ob", "mit", "s\u00fcss", "und", "sanf\u00b7tem", "Thon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOKOM", "KOUS", "APPR", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie dieses zu mir sagt\u2019: \u201eich warte lange schon,", "tokens": ["Sie", "die\u00b7ses", "zu", "mir", "sagt'", ":", "\u201e", "ich", "war\u00b7te", "lan\u00b7ge", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "APPR", "PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich setzte mich darauf gleich bey den Bluhmen nieder", "tokens": ["Ich", "setz\u00b7te", "mich", "da\u00b7rauf", "gleich", "bey", "den", "Bluh\u00b7men", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "ADV", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besah, bewunderte, mit inniglicher Freude,", "tokens": ["Be\u00b7sah", ",", "be\u00b7wun\u00b7der\u00b7te", ",", "mit", "in\u00b7nig\u00b7li\u00b7cher", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und macht\u2019 ihr von Natur so nett erricht\u2019t Geb\u00e4ude", "tokens": ["Und", "macht'", "ihr", "von", "Na\u00b7tur", "so", "nett", "er\u00b7richt't", "Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ADV", "ADJD", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum Zweck und Vorwurff meiner Lieder.", "tokens": ["Zum", "Zweck", "und", "Vor\u00b7wurff", "mei\u00b7ner", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was soll ich doch zuerst f\u00fcr eine Zier,", "tokens": ["Was", "soll", "ich", "doch", "zu\u00b7erst", "f\u00fcr", "ei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O sch\u00f6nste Tuberos\u2019, an dir", "tokens": ["O", "sch\u00f6ns\u00b7te", "Tu\u00b7be\u00b7ros'", ",", "an", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bewundern, r\u00fchmen und beschreiben?", "tokens": ["Be\u00b7wun\u00b7dern", ",", "r\u00fch\u00b7men", "und", "be\u00b7schrei\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Farbe, der Geruch, das gl\u00e4ntzen, die Figur", "tokens": ["Die", "Far\u00b7be", ",", "der", "Ge\u00b7ruch", ",", "das", "gl\u00e4nt\u00b7zen", ",", "die", "Fi\u00b7gur"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PDS", "VVINF", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die wollen mich zugleich zu deinem Lobe treiben.", "tokens": ["Die", "wol\u00b7len", "mich", "zu\u00b7gleich", "zu", "dei\u00b7nem", "Lo\u00b7be", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn alles, was an dir der Finger der Natur", "tokens": ["Denn", "al\u00b7les", ",", "was", "an", "dir", "der", "Fin\u00b7ger", "der", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "APPR", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gebildet, ist bewunderns wehrt.", "tokens": ["Ge\u00b7bil\u00b7det", ",", "ist", "be\u00b7wun\u00b7derns", "wehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dein sch\u00f6nes Kraut, das aus der Zwiebel bricht,", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Kraut", ",", "das", "aus", "der", "Zwie\u00b7bel", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und welches sie mit ihren S\u00e4fften n\u00e4hrt,", "tokens": ["Und", "wel\u00b7ches", "sie", "mit", "ih\u00b7ren", "S\u00e4ff\u00b7ten", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergn\u00fcget ein drauf achtendes Gesicht", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "ein", "drauf", "ach\u00b7ten\u00b7des", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "PAV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch sein so lieblich gr\u00fcn. Der Stiel, der aus der Mitten", "tokens": ["Durch", "sein", "so", "lieb\u00b7lich", "gr\u00fcn", ".", "Der", "Stiel", ",", "der", "aus", "der", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADV", "ADJD", "ADJD", "$.", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In schlancker L\u00e4ng\u2019 und R\u00fcnde steiget,", "tokens": ["In", "schlan\u00b7cker", "L\u00e4ng'", "und", "R\u00fcn\u00b7de", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sich, als w\u00e4r\u2019 er recht mit Flei\u00df und Kunst geschnitten,", "tokens": ["Und", "sich", ",", "als", "w\u00e4r'", "er", "recht", "mit", "Flei\u00df", "und", "Kunst", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In gelblich gr\u00fcnem Glantze zeiget,", "tokens": ["In", "gelb\u00b7lich", "gr\u00fc\u00b7nem", "Glant\u00b7ze", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gebiehrt und bringet wunderbar", "tokens": ["Ge\u00b7biehrt", "und", "brin\u00b7get", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die sch\u00f6nen Kinder Paar bey Paar,", "tokens": ["Die", "sch\u00f6\u00b7nen", "Kin\u00b7der", "Paar", "bey", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als Zwillinge hervor; die, eh\u2019 sie offen gehn,", "tokens": ["Als", "Zwil\u00b7lin\u00b7ge", "her\u00b7vor", ";", "die", ",", "eh'", "sie", "of\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKVZ", "$.", "ART", "$,", "KOUS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "In einer lieblichen und s\u00fcssen R\u00f6the stehn.", "tokens": ["In", "ei\u00b7ner", "lieb\u00b7li\u00b7chen", "und", "s\u00fcs\u00b7sen", "R\u00f6\u00b7the", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kaum aber \u00f6ffnen sie die Spitzen,", "tokens": ["Kaum", "a\u00b7ber", "\u00f6ff\u00b7nen", "sie", "die", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Erblickt man ein so weisses blitzen,", "tokens": ["Er\u00b7blickt", "man", "ein", "so", "weis\u00b7ses", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das selbst den Schnee besch\u00e4mt. Sie gleichen weissen", "tokens": ["Das", "selbst", "den", "Schnee", "be\u00b7sch\u00e4mt", ".", "Sie", "glei\u00b7chen", "weis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "VVPP", "$.", "PPER", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "In sechs-geeckter Form. Es ist ein iedes Blat", "tokens": ["In", "sechs\u00b7ge\u00b7eck\u00b7ter", "Form", ".", "Es", "ist", "ein", "ie\u00b7des", "Blat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein wenig ausgeh\u00f6let, glatt,", "tokens": ["Ein", "we\u00b7nig", "aus\u00b7ge\u00b7h\u00f6\u00b7let", ",", "glatt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und sind sie, in der Bluhmen Reich,", "tokens": ["Und", "sind", "sie", ",", "in", "der", "Bluh\u00b7men", "Reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Den Fr\u00fchlings-Hyacinthen gleich,", "tokens": ["Den", "Fr\u00fch\u00b7lings\u00b7Hya\u00b7cin\u00b7then", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Doch weit ansehnlicher und gr\u00f6sser:", "tokens": ["Doch", "weit", "an\u00b7sehn\u00b7li\u00b7cher", "und", "gr\u00f6s\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Daher man sie des Herbstes-, ja noch besser", "tokens": ["Da\u00b7her", "man", "sie", "des", "Herbs\u00b7tes", ",", "ja", "noch", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PIS", "PPER", "ART", "TRUNC", "$,", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die Hyacinth der Indianer nennt,", "tokens": ["Die", "Hya\u00b7cin\u00b7th", "der", "In\u00b7di\u00b7a\u00b7ner", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Die ihres gleichen nicht an Gr\u00f6sse kennt.", "tokens": ["Die", "ih\u00b7res", "glei\u00b7chen", "nicht", "an", "Gr\u00f6s\u00b7se", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wer wird von deiner Balsam-Krafft,", "tokens": ["Wer", "wird", "von", "dei\u00b7ner", "Bal\u00b7sam\u00b7Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit welcher deine Bluhm\u2019 erf\u00fcllet,", "tokens": ["Mit", "wel\u00b7cher", "dei\u00b7ne", "Bluhm'", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die, recht wie ein trockner Safft,", "tokens": ["Und", "die", ",", "recht", "wie", "ein", "trock\u00b7ner", "Safft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "w