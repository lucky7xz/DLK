{"textgrid.poem.24173": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Lehmann wollt sich malen lan,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Lehmann wollt sich malen lan,", "tokens": ["Herr", "Leh\u00b7mann", "wollt", "sich", "ma\u00b7len", "lan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hub drum zu Lenbach z'reden an:", "tokens": ["Hub", "drum", "zu", "Len\u00b7bach", "z'\u00b7re\u00b7den", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00bbherr Meister, Ihr sollt mich konterfein!", "tokens": ["\u00bb", "herr", "Meis\u00b7ter", ",", "Ihr", "sollt", "mich", "kon\u00b7ter\u00b7fein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "PPER", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch solls ein feines Bildnus sein:", "tokens": ["Doch", "solls", "ein", "fei\u00b7nes", "Bild\u00b7nus", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Bild voll Sch\u00f6nheit, Geist und Kraft,", "tokens": ["Ein", "Bild", "voll", "Sch\u00f6n\u00b7heit", ",", "Geist", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Ehrenmal der Lehmannschaft.", "tokens": ["Ein", "Eh\u00b7ren\u00b7mal", "der", "Leh\u00b7mann\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein treues Auge, deutsch und blau,", "tokens": ["Mein", "treu\u00b7es", "Au\u00b7ge", ",", "deutsch", "und", "blau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df es recht gottesf\u00fcrchtig schau!", "tokens": ["Da\u00df", "es", "recht", "got\u00b7tes\u00b7f\u00fcrch\u00b7tig", "schau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Meiner Lippen roter Bogenschwung", "tokens": ["Mei\u00b7ner", "Lip\u00b7pen", "ro\u00b7ter", "Bo\u00b7gen\u00b7schwung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Verrate heilge Begeisterung", "tokens": ["Ver\u00b7ra\u00b7te", "heil\u00b7ge", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "F\u00fcr alles, was da gro\u00df und wahr:", "tokens": ["F\u00fcr", "al\u00b7les", ",", "was", "da", "gro\u00df", "und", "wahr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Baut meine Stirne hoch und klar,", "tokens": ["Baut", "mei\u00b7ne", "Stir\u00b7ne", "hoch", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und la\u00dft die Locken golden wallen!", "tokens": ["Und", "la\u00dft", "die", "Lo\u00b7cken", "gol\u00b7den", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Meine Nase soll meiner Frau gefallen", "tokens": ["Mei\u00b7ne", "Na\u00b7se", "soll", "mei\u00b7ner", "Frau", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "(sie liebt die langen, graden, schmalen):", "tokens": ["(", "sie", "liebt", "die", "lan\u00b7gen", ",", "gra\u00b7den", ",", "schma\u00b7len", ")", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "$,", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Was Ihr verlangt, ich wills bezahlen.\u00ab", "tokens": ["Was", "Ihr", "ver\u00b7langt", ",", "ich", "wills", "be\u00b7zah\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Meister durch das Brillenrund", "tokens": ["Der", "Meis\u00b7ter", "durch", "das", "Bril\u00b7len\u00b7rund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schaut nieder auf Herrn Lehmann und", "tokens": ["Schaut", "nie\u00b7der", "auf", "Herrn", "Leh\u00b7mann", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "NN", "NE", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er spricht:", "tokens": ["Er", "spricht", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "\u00bbherr Lehmann, Euer wohledel Gesicht", "tokens": ["\u00bb", "herr", "Leh\u00b7mann", ",", "Eu\u00b7er", "woh\u00b7le\u00b7del", "Ge\u00b7sicht"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "NN", "NE", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Eignet sich zu einem Adonis nicht!", "tokens": ["Eig\u00b7net", "sich", "zu", "ei\u00b7nem", "A\u00b7do\u00b7nis", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+----+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Ihr seid ein guter Lehmann zwar,", "tokens": ["Ihr", "seid", "ein", "gu\u00b7ter", "Leh\u00b7mann", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch ein Apoll nicht eben gar.", "tokens": ["Doch", "ein", "A\u00b7poll", "nicht", "e\u00b7ben", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADV", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Euer Auge blickt ein wenig schiel,", "tokens": ["Eu\u00b7er", "Au\u00b7ge", "blickt", "ein", "we\u00b7nig", "schiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Eure Nase staunt zum Himmel zu viel,", "tokens": ["Eu\u00b7re", "Na\u00b7se", "staunt", "zum", "Him\u00b7mel", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "APPR", "PIS", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Eurer Locken blonden Scheitelkranz,", "tokens": ["Eu\u00b7rer", "Lo\u00b7cken", "blon\u00b7den", "Schei\u00b7tel\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Die Zeit hat ihn gelichtet ganz.", "tokens": ["Die", "Zeit", "hat", "ihn", "ge\u00b7lich\u00b7tet", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ihr seid ein B\u00fcrger unzweifelhaft bieder,", "tokens": ["Ihr", "seid", "ein", "B\u00fcr\u00b7ger", "un\u00b7zwei\u00b7fel\u00b7haft", "bie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch Eure Stirn ist gedr\u00fcckt und nieder,", "tokens": ["Doch", "Eu\u00b7re", "Stirn", "ist", "ge\u00b7dr\u00fcckt", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "KON", "PTKVZ", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Auch geht Eurer Lippen Schwung die Quere \u2013", "tokens": ["Auch", "geht", "Eu\u00b7rer", "Lip\u00b7pen", "Schwung", "die", "Que\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "ART", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Herr Lehmann, ich bedaure sehre.\u00ab", "tokens": ["Herr", "Leh\u00b7mann", ",", "ich", "be\u00b7dau\u00b7re", "seh\u00b7re", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VVFIN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Gro\u00df sah den Meister Herr Lehmann an,", "tokens": ["Gro\u00df", "sah", "den", "Meis\u00b7ter", "Herr", "Leh\u00b7mann", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "NE", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Dacht bei sich: Das ist ein grober Mann!", "tokens": ["Dacht", "bei", "sich", ":", "Das", "ist", "ein", "gro\u00b7ber", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Ist auch von den Realisten verdorben;", "tokens": ["Ist", "auch", "von", "den", "Re\u00b7a\u00b7lis\u00b7ten", "ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Der Idealismus ist ausgestorben.", "tokens": ["Der", "I\u00b7dea\u00b7lis\u00b7mus", "ist", "aus\u00b7ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Oh, diese Zeiten, diese krassen!!", "tokens": ["Oh", ",", "die\u00b7se", "Zei\u00b7ten", ",", "die\u00b7se", "kras\u00b7sen", "!!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDAT", "NN", "$,", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Kein Biedermann kann sich mehr malen lassen.", "tokens": ["Kein", "Bie\u00b7der\u00b7mann", "kann", "sich", "mehr", "ma\u00b7len", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}