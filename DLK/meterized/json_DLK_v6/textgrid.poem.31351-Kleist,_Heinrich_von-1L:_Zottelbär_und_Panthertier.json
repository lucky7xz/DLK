{"textgrid.poem.31351": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zottelb\u00e4r und Panthertier", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zottelb\u00e4r und Panthertier", "tokens": ["Zot\u00b7tel\u00b7b\u00e4r", "und", "Pan\u00b7ther\u00b7tier"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat der Pfeil bezwungen;", "tokens": ["Hat", "der", "Pfeil", "be\u00b7zwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nur f\u00fcr Geld, im Drahtspalier,", "tokens": ["Nur", "f\u00fcr", "Geld", ",", "im", "Drahts\u00b7pa\u00b7lier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigt man noch die Jungen.", "tokens": ["Zeigt", "man", "noch", "die", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Auf den Wolf, soviel ich wei\u00df,", "tokens": ["Auf", "den", "Wolf", ",", "so\u00b7viel", "ich", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein Preis gesetzet;", "tokens": ["Ist", "ein", "Preis", "ge\u00b7set\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo er immer hungerhei\u00df", "tokens": ["Wo", "er", "im\u00b7mer", "hun\u00b7ger\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Naht, wird er gehetzet.", "tokens": ["Naht", ",", "wird", "er", "ge\u00b7het\u00b7zet", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Reinecke, der Fuchs, der sitzt", "tokens": ["Rein\u00b7e\u00b7cke", ",", "der", "Fuchs", ",", "der", "sitzt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "NE", "$,", "PRELS", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lichtscheu in der Erden,", "tokens": ["Licht\u00b7scheu", "in", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Und verzehrt, was er stipitzt,", "tokens": ["Und", "ver\u00b7zehrt", ",", "was", "er", "sti\u00b7pitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ohne fett zu werden.", "tokens": ["Oh\u00b7ne", "fett", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aar und Geier nisten nur", "tokens": ["Aar", "und", "Gei\u00b7er", "nis\u00b7ten", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Felsen R\u00fccken,", "tokens": ["Auf", "der", "Fel\u00b7sen", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo kein Sterblicher die Spur", "tokens": ["Wo", "kein", "Sterb\u00b7li\u00b7cher", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Sand mag dr\u00fccken.", "tokens": ["In", "den", "Sand", "mag", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Schlangen sieht man gar nicht mehr,", "tokens": ["Schlan\u00b7gen", "sieht", "man", "gar", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ottern und dergleichen,", "tokens": ["Ot\u00b7tern", "und", "derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Drachen Greuelheer,", "tokens": ["Und", "der", "Dra\u00b7chen", "Greu\u00b7el\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit geschwollnen B\u00e4uchen.", "tokens": ["Mit", "ge\u00b7schwoll\u00b7nen", "B\u00e4u\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Nur der Franzmann zeigt sich noch", "tokens": ["Nur", "der", "Franz\u00b7mann", "zeigt", "sich", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem deutschen Reiche;", "tokens": ["In", "dem", "deut\u00b7schen", "Rei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Br\u00fcder, nehmt die Keule doch,", "tokens": ["Br\u00fc\u00b7der", ",", "nehmt", "die", "Keu\u00b7le", "doch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er gleichfalls weiche.", "tokens": ["Da\u00df", "er", "gleich\u00b7falls", "wei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}