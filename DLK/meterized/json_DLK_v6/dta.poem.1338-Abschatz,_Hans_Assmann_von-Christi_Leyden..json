{"dta.poem.1338": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Christi Leyden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer schonet einen Wurm? mu\u00df nicht ein ieder Stein/", "tokens": ["Wer", "scho\u00b7net", "ei\u00b7nen", "Wurm", "?", "mu\u00df", "nicht", "ein", "ie\u00b7der", "Stein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "VMFIN", "PTKNEG", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df nicht ein ieder Fu\u00df desselben M\u00f6rder seyn/", "tokens": ["Mu\u00df", "nicht", "ein", "ie\u00b7der", "Fu\u00df", "des\u00b7sel\u00b7ben", "M\u00f6r\u00b7der", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "PIAT", "NN", "PDAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem er hin und her auff schwartzer Erde kreucht/", "tokens": ["In\u00b7dem", "er", "hin", "und", "her", "auff", "schwart\u00b7zer", "Er\u00b7de", "kreucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seinem Feinde sich durch blosse Flucht entzeucht.", "tokens": ["Und", "sei\u00b7nem", "Fein\u00b7de", "sich", "durch", "blos\u00b7se", "Flucht", "ent\u00b7zeucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So eben geht es dir/ o JEsu GOttes Sohn/", "tokens": ["So", "e\u00b7ben", "geht", "es", "dir", "/", "o", "Je\u00b7su", "Got\u00b7tes", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "$(", "FM", "NE", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein himmelischer Sitz/ dein hoher Ehren-Thron", "tokens": ["Dein", "him\u00b7me\u00b7li\u00b7scher", "Sitz", "/", "dein", "ho\u00b7her", "Eh\u00b7ren\u00b7Thron"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist itzund Erd und Staub. Des glatten Leibes Zier", "tokens": ["Ist", "it\u00b7zund", "Erd", "und", "Staub", ".", "Des", "glat\u00b7ten", "Lei\u00b7bes", "Zier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit Narben angef\u00fcllt/ verwandelt sich bey dir", "tokens": ["Mit", "Nar\u00b7ben", "an\u00b7ge\u00b7f\u00fcllt", "/", "ver\u00b7wan\u00b7delt", "sich", "bey", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$(", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In he\u00dfliche Gestalt/ o \u00fcberh\u00e4uffte Pein!", "tokens": ["In", "he\u00df\u00b7li\u00b7che", "Ge\u00b7stalt", "/", "o", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7te", "Pein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es dringen sich ins Haubt die scharffen Dornen ein/", "tokens": ["Es", "drin\u00b7gen", "sich", "ins", "Haubt", "die", "scharf\u00b7fen", "Dor\u00b7nen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es schneiden Haut und Fleisch die Riemen schwer von Bley/", "tokens": ["Es", "schnei\u00b7den", "Haut", "und", "Fleisch", "die", "Rie\u00b7men", "schwer", "von", "Bley", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "ART", "NN", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es schneiden Marck und Bein/ die L\u00e4ster-Wort entzwey/", "tokens": ["Es", "schnei\u00b7den", "Marck", "und", "Bein", "/", "die", "L\u00e4s\u00b7ter\u00b7Wort", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$(", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So die erbo\u00dfte Schaar der Juden speyet aus.", "tokens": ["So", "die", "er\u00b7bo\u00df\u00b7te", "Schaar", "der", "Ju\u00b7den", "spey\u00b7et", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Pilatus f\u00fchret dich vors hohe Richter-Hau\u00df/", "tokens": ["Pi\u00b7la\u00b7tus", "f\u00fch\u00b7ret", "dich", "vors", "ho\u00b7he", "Rich\u00b7ter\u00b7Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wo das geh\u00e4uffte Volck in langer Reihe steht/", "tokens": ["Wo", "das", "ge\u00b7h\u00e4uff\u00b7te", "Volck", "in", "lan\u00b7ger", "Rei\u00b7he", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er weiset/ wie das Blutt aus allen Adern geht/", "tokens": ["Er", "wei\u00b7set", "/", "wie", "das", "Blutt", "aus", "al\u00b7len", "A\u00b7dern", "geht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "PDS", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie durch das strenge Band die Glieder seyn umschr\u00e4nckt/", "tokens": ["Wie", "durch", "das", "stren\u00b7ge", "Band", "die", "Glie\u00b7der", "seyn", "um\u00b7schr\u00e4nckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VAINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und dein zufleischter Leib kaum an einander heuckt.", "tokens": ["Und", "dein", "zu\u00b7fleischter", "Leib", "kaum", "an", "ein\u00b7an\u00b7der", "heuckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "Hier solt ein Diamant und Felsen-harter Stein/", "tokens": ["Hier", "solt", "ein", "Di\u00b7a\u00b7mant", "und", "Fel\u00b7sen\u00b7har\u00b7ter", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie von der Hitze Schnee und Ey\u00df zuschmoltzen seyn/", "tokens": ["Wie", "von", "der", "Hit\u00b7ze", "Schnee", "und", "Ey\u00df", "zu\u00b7schmolt\u00b7zen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "KON", "NN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und dein erw\u00e4hltes Volck sieht hocherfreuet an/", "tokens": ["Und", "dein", "er\u00b7w\u00e4hl\u00b7tes", "Volck", "sieht", "ho\u00b7cher\u00b7freu\u00b7et", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was Tag und Sonne nicht ohn Schrecken sehen kan/", "tokens": ["Was", "Tag", "und", "Son\u00b7ne", "nicht", "ohn", "Schre\u00b7cken", "se\u00b7hen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Indem nun \u00fcber dich der Eyfer-volle Sturm", "tokens": ["In\u00b7dem", "nun", "\u00fc\u00b7ber", "dich", "der", "Ey\u00b7fer\u00b7vol\u00b7le", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Des blinden Volckes geht/ indem du als ein Wurm", "tokens": ["Des", "blin\u00b7den", "Vol\u00b7ckes", "geht", "/", "in\u00b7dem", "du", "als", "ein", "Wurm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KOUS", "PPER", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zutretten und zuknirscht in eignem Blutte schwimmst/", "tokens": ["Zu\u00b7tret\u00b7ten", "und", "zu\u00b7knirscht", "in", "eig\u00b7nem", "Blut\u00b7te", "schwimmst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "An Schmertzen immer zu- und ab an Kr\u00e4fften nimmst.", "tokens": ["An", "Schmert\u00b7zen", "im\u00b7mer", "zu", "und", "ab", "an", "Kr\u00e4ff\u00b7ten", "nimmst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "TRUNC", "KON", "PTKVZ", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "W\u00e4chst mit den Plagen auch die heilige Geduld/", "tokens": ["W\u00e4chst", "mit", "den", "Pla\u00b7gen", "auch", "die", "hei\u00b7li\u00b7ge", "Ge\u00b7duld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.28": {"text": "Die der ergrimmten Schaar vergiebet alle Schuld/", "tokens": ["Die", "der", "er\u00b7grimm\u00b7ten", "Schaar", "ver\u00b7gie\u00b7bet", "al\u00b7le", "Schuld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und ohne Zucken sich zu tode martern l\u00e4st.", "tokens": ["Und", "oh\u00b7ne", "Zu\u00b7cken", "sich", "zu", "to\u00b7de", "mar\u00b7tern", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PRF", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "O mehr als wohl gethan! denn also wird zur Pest", "tokens": ["O", "mehr", "als", "wohl", "ge\u00b7than", "!", "denn", "al\u00b7so", "wird", "zur", "Pest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "KOKOM", "ADV", "VVPP", "$.", "KON", "ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dem Tode dieser Tod. Du s\u00fcsser JEsu siegst/", "tokens": ["Dem", "To\u00b7de", "die\u00b7ser", "Tod", ".", "Du", "s\u00fcs\u00b7ser", "Je\u00b7su", "siegst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$.", "PPER", "ADJA", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Besiegt von Noth und Tod/ du starcker JEsu kriegst", "tokens": ["Be\u00b7siegt", "von", "Noth", "und", "Tod", "/", "du", "star\u00b7cker", "Je\u00b7su", "kriegst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$(", "PPER", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Gefangen deinen Feind/ der dich gefangen h\u00e4lt/", "tokens": ["Ge\u00b7fan\u00b7gen", "dei\u00b7nen", "Feind", "/", "der", "dich", "ge\u00b7fan\u00b7gen", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "PRELS", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und f\u00fchrest im Triumph Tod/ Teufel/ H\u00f6lle/ Welt.", "tokens": ["Und", "f\u00fch\u00b7rest", "im", "Tri\u00b7umph", "Tod", "/", "Teu\u00b7fel", "/", "H\u00f6l\u00b7le", "/", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NN", "$(", "NE", "$(", "NN", "$(", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Das Sieges-Mahl hastu dir selber auffgesteckt/", "tokens": ["Das", "Sie\u00b7ges\u00b7Mahl", "has\u00b7tu", "dir", "sel\u00b7ber", "auff\u00b7ge\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Als du das m\u00fcde Paar der Armen ausgestreckt/", "tokens": ["Als", "du", "das", "m\u00fc\u00b7de", "Paar", "der", "Ar\u00b7men", "aus\u00b7ge\u00b7streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Genagelt an das Holtz. La\u00df unsre Ehre seyn/", "tokens": ["Ge\u00b7na\u00b7gelt", "an", "das", "Holtz", ".", "La\u00df", "uns\u00b7re", "Eh\u00b7re", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "VVIMP", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "O JEsu/ deine Schmach: Von S\u00fcnden wasch uns rein", "tokens": ["O", "Je\u00b7su", "/", "dei\u00b7ne", "Schmach", ":", "Von", "S\u00fcn\u00b7den", "wasch", "uns", "rein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "PPOSAT", "NN", "$.", "APPR", "NN", "ADJD", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dein rosin-farbnes Blutt/ der theure edle Safft/", "tokens": ["Dein", "ro\u00b7sin\u00b7fa\u00b7rb\u00b7nes", "Blutt", "/", "der", "theu\u00b7re", "ed\u00b7le", "Safft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.40": {"text": "Von Wunden mach uns heil/ HErr/ deiner Wunden Krafft.", "tokens": ["Von", "Wun\u00b7den", "mach", "uns", "heil", "/", "Herr", "/", "dei\u00b7ner", "Wun\u00b7den", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "$(", "NN", "$(", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}