{"textgrid.poem.50145": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "33. An den K\u00f6nig Sesostres", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhebe stoltzer F\u00fcrst dich nicht in deinem Gl\u00fcck,", "tokens": ["Er\u00b7he\u00b7be", "stolt\u00b7zer", "F\u00fcrst", "dich", "nicht", "in", "dei\u00b7nem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil dessen ", "tokens": ["Weil", "des\u00b7sen"], "token_info": ["word", "word"], "pos": ["KOUS", "PDS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "An den ", "tokens": ["An", "den"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Zur ", "tokens": ["Zur"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Du irrst, wo du zu viel auff dessen Treue baust!", "tokens": ["Du", "irrst", ",", "wo", "du", "zu", "viel", "auff", "des\u00b7sen", "Treu\u00b7e", "baust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PTKA", "PIS", "APPR", "PRELAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es pflegt sich wie ", "tokens": ["Es", "pflegt", "sich", "wie"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da kanst ", "tokens": ["Da", "kanst"], "token_info": ["word", "word"], "pos": ["ADV", "VM