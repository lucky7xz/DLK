{"textgrid.poem.42155": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck und Segen und alles Gute", "tokens": ["Gl\u00fcck", "und", "Se\u00b7gen", "und", "al\u00b7les", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "PIAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Gie\u00df dir hernieder ein sch\u00fctzender Stern;", "tokens": ["Gie\u00df", "dir", "her\u00b7nie\u00b7der", "ein", "sch\u00fct\u00b7zen\u00b7der", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "K\u00f6nnt ich's erkaufen mit meinem Blute,", "tokens": ["K\u00f6nnt", "ich's", "er\u00b7kau\u00b7fen", "mit", "mei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Oh, wie erkauft ich es dir so gern.", "tokens": ["Oh", ",", "wie", "er\u00b7kauft", "ich", "es", "dir", "so", "gern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Freu dich sorglos der sonnigen Tage!", "tokens": ["Freu", "dich", "sorg\u00b7los", "der", "son\u00b7ni\u00b7gen", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Klarblauer Himmel verkl\u00e4ret den Blick;", "tokens": ["Klar\u00b7blau\u00b7er", "Him\u00b7mel", "ver\u00b7kl\u00e4\u00b7ret", "den", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "$."], "meter": "++-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Aber mit weicher melodischer Klage", "tokens": ["A\u00b7ber", "mit", "wei\u00b7cher", "me\u00b7lo\u00b7di\u00b7scher", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "D\u00e4mpfe die Schmerzen im Mi\u00dfgeschick.", "tokens": ["D\u00e4mp\u00b7fe", "die", "Schmer\u00b7zen", "im", "Mi\u00df\u00b7ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Durch die T\u00e4ler und \u00fcber die H\u00f6hen", "tokens": ["Durch", "die", "T\u00e4\u00b7ler", "und", "\u00fc\u00b7ber", "die", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wandr ich indessen die steinige Bahn;", "tokens": ["Wandr", "ich", "in\u00b7des\u00b7sen", "die", "stei\u00b7ni\u00b7ge", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Fernher winkendes Wiedersehen", "tokens": ["Fern\u00b7her", "win\u00b7ken\u00b7des", "Wie\u00b7der\u00b7se\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Spornt die erm\u00fcdeten Schritte an.", "tokens": ["Spornt", "die", "er\u00b7m\u00fc\u00b7de\u00b7ten", "Schrit\u00b7te", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Breitet sich abends dann mir zu F\u00fc\u00dfen", "tokens": ["Brei\u00b7tet", "sich", "a\u00b7bends", "dann", "mir", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "PPER", "APPR", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Reich die herrliche Lenzesflur,", "tokens": ["Reich", "die", "herr\u00b7li\u00b7che", "Len\u00b7zes\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dr\u00fcben die dunklen Berge gr\u00fc\u00dfen", "tokens": ["Dr\u00fc\u00b7ben", "die", "dunk\u00b7len", "Ber\u00b7ge", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und der Fl\u00fcsse leuchtende Spur.", "tokens": ["Und", "der", "Fl\u00fcs\u00b7se", "leuch\u00b7ten\u00b7de", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Seh ich's alles weit \u00fcbergossen", "tokens": ["Seh", "ich's", "al\u00b7les", "weit", "\u00fc\u00b7ber\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PIS", "ADJD", "VVPP"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Von der sinkenden Sonne Glut,", "tokens": ["Von", "der", "sin\u00b7ken\u00b7den", "Son\u00b7ne", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Oh, wie wird mir das Herz erschlossen,", "tokens": ["Oh", ",", "wie", "wird", "mir", "das", "Herz", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein gedenkend mit neuem Mut.", "tokens": ["Dein", "ge\u00b7den\u00b7kend", "mit", "neu\u00b7em", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Dein gedenkend, steig ich zu Tale,", "tokens": ["Dein", "ge\u00b7den\u00b7kend", ",", "steig", "ich", "zu", "Ta\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "$,", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Nacht umf\u00e4ngt mich mit d\u00fcstren Wehn;", "tokens": ["Nacht", "um\u00b7f\u00e4ngt", "mich", "mit", "d\u00fcst\u00b7ren", "Wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Aber im Morgensonnenstrahle", "tokens": ["A\u00b7ber", "im", "Mor\u00b7gen\u00b7son\u00b7nen\u00b7strah\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wei\u00df ich ein freudiges Wiedersehn.", "tokens": ["Wei\u00df", "ich", "ein", "freu\u00b7di\u00b7ges", "Wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}