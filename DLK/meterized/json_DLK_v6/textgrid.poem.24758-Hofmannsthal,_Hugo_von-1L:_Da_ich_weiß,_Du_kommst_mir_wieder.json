{"textgrid.poem.24758": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da ich wei\u00df, Du kommst mir wieder", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ich wei\u00df, Du kommst mir wieder", "tokens": ["Da", "ich", "wei\u00df", ",", "Du", "kommst", "mir", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machen mich die Wolken froh,", "tokens": ["Ma\u00b7chen", "mich", "die", "Wol\u00b7ken", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und am Georginenbeete", "tokens": ["Und", "am", "Ge\u00b7or\u00b7gi\u00b7nen\u00b7bee\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Abendstille freut mich so!", "tokens": ["A\u00b7bend\u00b7stil\u00b7le", "freut", "mich", "so", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fr\u00f6hlich such ich mir den Schatten,", "tokens": ["Fr\u00f6h\u00b7lich", "such", "ich", "mir", "den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis die Sonne fast versinkt.", "tokens": ["Bis", "die", "Son\u00b7ne", "fast", "ver\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nachts im kleinen dunkeln Tale", "tokens": ["Nachts", "im", "klei\u00b7nen", "dun\u00b7keln", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freut mich jedes Licht, das blinkt ...", "tokens": ["Freut", "mich", "je\u00b7des", "Licht", ",", "das", "blinkt", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "PDS", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob ich einsam steig am H\u00fcgel,", "tokens": ["Ob", "ich", "ein\u00b7sam", "steig", "am", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horch ich doch an Deiner T\u00fcre.", "tokens": ["Horch", "ich", "doch", "an", "Dei\u00b7ner", "T\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steh ich hier in fremdem Garten,", "tokens": ["Steh", "ich", "hier", "in", "frem\u00b7dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du doch bist es, die ich sp\u00fcre.", "tokens": ["Du", "doch", "bist", "es", ",", "die", "ich", "sp\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}