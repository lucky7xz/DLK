{"textgrid.poem.57303": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Lenz, ist, A\u00ebdi, gekommen;", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lenz, ist, A\u00ebdi, gekommen;", "tokens": ["Der", "Lenz", ",", "ist", ",", "A\u00eb\u00b7di", ",", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "$,", "NE", "$,", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Luft ist hell, der Himmel blau, die Blume duftet,", "tokens": ["Die", "Luft", "ist", "hell", ",", "der", "Him\u00b7mel", "blau", ",", "die", "Blu\u00b7me", "duf\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit lieblichem Wehen athmen die Weste,", "tokens": ["Mit", "lieb\u00b7li\u00b7chem", "We\u00b7hen", "ath\u00b7men", "die", "Wes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Zeit des Gesangs ist, A\u00ebdi, gekommen!", "tokens": ["Die", "Zeit", "des", "Ge\u00b7sangs", "ist", ",", "A\u00eb\u00b7di", ",", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,", "NE", "$,", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbich mag nicht singen, die Zeisige haben", "tokens": ["\u00bb", "ich", "mag", "nicht", "sin\u00b7gen", ",", "die", "Zei\u00b7si\u00b7ge", "ha\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "ART", "NN", "VAFIN"], "meter": "++-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Das Ohr mir taub gezwitschert!", "tokens": ["Das", "Ohr", "mir", "taub", "ge\u00b7zwit\u00b7schert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel lieber mag ich am Aste mich schwenken,", "tokens": ["Viel", "lie\u00b7ber", "mag", "ich", "am", "As\u00b7te", "mich", "schwen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und unten in dem krystallenen Bache mich sehn.\u00ab", "tokens": ["Und", "un\u00b7ten", "in", "dem", "krys\u00b7tal\u00b7le\u00b7nen", "Ba\u00b7che", "mich", "sehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Nicht singen? Denkest du, dass deine Mutter", "tokens": ["Nicht", "sin\u00b7gen", "?", "Den\u00b7kest", "du", ",", "dass", "dei\u00b7ne", "Mut\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "$.", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht auch z\u00fcrnen k\u00f6nne?", "tokens": ["Nicht", "auch", "z\u00fcr\u00b7nen", "k\u00f6n\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lernen musst du, der Lenz ist da!", "tokens": ["Ler\u00b7nen", "musst", "du", ",", "der", "Lenz", "ist", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Viel sind der Zaubereyen der Kunst;", "tokens": ["Viel", "sind", "der", "Zau\u00b7be\u00b7re\u00b7yen", "der", "Kunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wenig der Tage des Lenzes.", "tokens": ["Und", "we\u00b7nig", "der", "Ta\u00b7ge", "des", "Len\u00b7zes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Weg von dem schwankenden Aste,", "tokens": ["Weg", "von", "dem", "schwan\u00b7ken\u00b7den", "As\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Und h\u00f6re, was einst vom Zauber der Kunst mir sang", "tokens": ["Und", "h\u00f6\u00b7re", ",", "was", "einst", "vom", "Zau\u00b7ber", "der", "Kunst", "mir", "sang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die K\u00f6nigin der Nachtigallen, Orphea.", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "Or\u00b7phea", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6r', ich beb' es zu singen,", "tokens": ["H\u00f6r'", ",", "ich", "beb'", "es", "zu", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Aber h\u00f6r', und sing es mir nach.", "tokens": ["A\u00b7ber", "h\u00f6r'", ",", "und", "sing", "es", "mir", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Also sang Orphea:", "tokens": ["Al\u00b7so", "sang", "Or\u00b7phea", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Fl\u00f6ten musst du, bald mit immer st\u00e4rkerem Laute,", "tokens": ["Fl\u00f6\u00b7ten", "musst", "du", ",", "bald", "mit", "im\u00b7mer", "st\u00e4r\u00b7ke\u00b7rem", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Bald mit leiserem, bis sich verlieren die T\u00f6ne;", "tokens": ["Bald", "mit", "lei\u00b7se\u00b7rem", ",", "bis", "sich", "ver\u00b7lie\u00b7ren", "die", "T\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "$,", "KOUS", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "+-+---+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schmettern dann, dass es die Wipfel des Waldes durchrauscht!", "tokens": ["Schmet\u00b7tern", "dann", ",", "dass", "es", "die", "Wip\u00b7fel", "des", "Wal\u00b7des", "durc\u00b7hrauscht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.4": {"text": "Fl\u00f6ten, fl\u00f6ten, bis sich bey den Rosenknospen", "tokens": ["Fl\u00f6\u00b7ten", ",", "fl\u00f6\u00b7ten", ",", "bis", "sich", "bey", "den", "Ro\u00b7sen\u00b7knos\u00b7pen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "KOUS", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Verlieren die T\u00f6ne.", "tokens": ["Ver\u00b7lie\u00b7ren", "die", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbach ich sing' es nicht nach, wie kann ich!", "tokens": ["\u00bb", "ach", "ich", "sing'", "es", "nicht", "nach", ",", "wie", "kann", "ich", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "PWAV", "VMFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Z\u00fcrne nicht, Mutter, ich sing' es nicht nach.", "tokens": ["Z\u00fcr\u00b7ne", "nicht", ",", "Mut\u00b7ter", ",", "ich", "sing'", "es", "nicht", "nach", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Aber sang sie nichts mehr", "tokens": ["A\u00b7ber", "sang", "sie", "nichts", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die K\u00f6nigin der Nachtigallen?", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts von dem, was die Wange bleich macht,", "tokens": ["Nichts", "von", "dem", ",", "was", "die", "Wan\u00b7ge", "bleich", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Gl\u00fchen die Wang', und rinnen, und str\u00f6men die Thr\u00e4ne macht?\u00ab", "tokens": ["Gl\u00fc\u00b7hen", "die", "Wang'", ",", "und", "rin\u00b7nen", ",", "und", "str\u00f6\u00b7men", "die", "Thr\u00e4\u00b7ne", "macht", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "VVINF", "$,", "KON", "VVFIN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}}, "stanza.7": {"line.1": {"text": "Noch mehr! noch mehr!", "tokens": ["Noch", "mehr", "!", "noch", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ach dass du dieses mich fragtest,", "tokens": ["Ach", "dass", "du", "die\u00b7ses", "mich", "frag\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PDAT", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie freut mich das, A\u00ebdi!", "tokens": ["Wie", "freut", "mich", "das", ",", "A\u00eb\u00b7di", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "$,", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie sang, sie sang auch Herzeusgesang!", "tokens": ["Sie", "sang", ",", "sie", "sang", "auch", "Her\u00b7zeus\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Nun will ich das j\u00fcngste B\u00e4umchen dir suchen,", "tokens": ["Nun", "will", "ich", "das", "j\u00fcngs\u00b7te", "B\u00e4um\u00b7chen", "dir", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Spross dir biegen helfen,", "tokens": ["Den", "Spross", "dir", "bie\u00b7gen", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dass du dich n\u00e4her sehen k\u00f6nnest im Silberbach.", "tokens": ["Dass", "du", "dich", "n\u00e4\u00b7her", "se\u00b7hen", "k\u00f6n\u00b7nest", "im", "Sil\u00b7ber\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auch dieses liess erschallen", "tokens": ["Auch", "die\u00b7ses", "liess", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PDS", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Liederk\u00f6nigin, Orphea:", "tokens": ["Die", "Lie\u00b7der\u00b7k\u00f6\u00b7ni\u00b7gin", ",", "Or\u00b7phea", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der J\u00fcngling stand, und flocht den Kranz,", "tokens": ["Der", "J\u00fcng\u00b7ling", "stand", ",", "und", "flocht", "den", "Kranz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und liess ihn weinend sinken!", "tokens": ["Und", "liess", "ihn", "wei\u00b7nend", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das M\u00e4dchen stand, vermocht' es \u00fcber sich", "tokens": ["Das", "M\u00e4d\u00b7chen", "stand", ",", "ver\u00b7mocht'", "es", "\u00fc\u00b7ber", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit trocknem Blick den J\u00fcngling anzusehen.", "tokens": ["Mit", "trock\u00b7nem", "Blick", "den", "J\u00fcng\u00b7ling", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da sang die Nachtigall ihr h\u00f6heres,", "tokens": ["Da", "sang", "die", "Nach\u00b7ti\u00b7gall", "ihr", "h\u00f6\u00b7he\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihr seelenersch\u00fctterndes Lied.", "tokens": ["Ihr", "see\u00b7le\u00b7ner\u00b7sch\u00fct\u00b7tern\u00b7des", "Lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Da flog das M\u00e4dchen zu dem J\u00fcngling hin!", "tokens": ["Da", "flog", "das", "M\u00e4d\u00b7chen", "zu", "dem", "J\u00fcng\u00b7ling", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der J\u00fcngling zu dem M\u00e4dchen hin!", "tokens": ["Der", "J\u00fcng\u00b7ling", "zu", "dem", "M\u00e4d\u00b7chen", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da weinten sie der Liebe Wonne!", "tokens": ["Da", "wein\u00b7ten", "sie", "der", "Lie\u00b7be", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}