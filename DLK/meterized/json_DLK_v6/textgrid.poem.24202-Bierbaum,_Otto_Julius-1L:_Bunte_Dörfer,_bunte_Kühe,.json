{"textgrid.poem.24202": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bunte D\u00f6rfer, bunte K\u00fche,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bunte D\u00f6rfer, bunte K\u00fche,", "tokens": ["Bun\u00b7te", "D\u00f6r\u00b7fer", ",", "bun\u00b7te", "K\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ackerpracht und Ackerm\u00fche,", "tokens": ["A\u00b7cker\u00b7pracht", "und", "A\u00b7cker\u00b7m\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reichsten Lebens frischer Lauf.", "tokens": ["Reichs\u00b7ten", "Le\u00b7bens", "fri\u00b7scher", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dreht sich alles weit im Kreise;", "tokens": ["Dreht", "sich", "al\u00b7les", "weit", "im", "Krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mittendurch geht deine Reise:", "tokens": ["Mit\u00b7ten\u00b7durch", "geht", "dei\u00b7ne", "Rei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Thu nur Herz und Augen auf.", "tokens": ["Thu", "nur", "Herz", "und", "Au\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}