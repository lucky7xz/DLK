{"dta.poem.6022": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sonnen W\u00e4rm und Licht wird man vergleichen k\u00f6n\u0303en", "tokens": ["Der", "Son\u00b7nen", "W\u00e4rm", "und", "Licht", "wird", "man", "ver\u00b7glei\u00b7chen", "k\u00f6\u00f1en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "VAFIN", "PIS", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit unsern irrdschen Feur. Sie w\u00fcrde selbst verbrennen", "tokens": ["Mit", "un\u00b7sern", "irrd\u00b7schen", "Feur", ".", "Sie", "w\u00fcr\u00b7de", "selbst", "ver\u00b7bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "(war unterschiedner Weisen Lehre)", "tokens": ["(", "war", "un\u00b7ter\u00b7schied\u00b7ner", "Wei\u00b7sen", "Leh\u00b7re", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da sie mit so viel Gluht so weite Circkel f\u00fcllt,", "tokens": ["Da", "sie", "mit", "so", "viel", "Gluht", "so", "wei\u00b7te", "Cir\u00b7ckel", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PIAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Wenn nicht die Feuchtigkeit, die aus den D\u00fcfften quillt,", "tokens": ["Wenn", "nicht", "die", "Feuch\u00b7tig\u00b7keit", ",", "die", "aus", "den", "D\u00fcff\u00b7ten", "quillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr eine Rahrung w\u00e4r", "tokens": ["Ihr", "ei\u00b7ne", "Rah\u00b7rung", "w\u00e4r"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}