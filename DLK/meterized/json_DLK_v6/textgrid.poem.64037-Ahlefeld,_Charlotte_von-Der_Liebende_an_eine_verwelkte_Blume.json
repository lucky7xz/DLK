{"textgrid.poem.64037": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Der Liebende an eine verwelkte Blume", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Blume \u2013 ach sie kam von ihr!", "tokens": ["Die\u00b7se", "Blu\u00b7me", "\u2013", "ach", "sie", "kam", "von", "ihr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "XY", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Auch verwelkt noch ist sie heilig mir.", "tokens": ["Auch", "ver\u00b7welkt", "noch", "ist", "sie", "hei\u00b7lig", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VAFIN", "PPER", "ADJD", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "L\u00e4ngst sind ihre Farben hingeschwunden,", "tokens": ["L\u00e4ngst", "sind", "ih\u00b7re", "Far\u00b7ben", "hin\u00b7ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie die Seeligkeit vergangner Stunden \u2013", "tokens": ["Wie", "die", "See\u00b7lig\u00b7keit", "ver\u00b7gang\u00b7ner", "Stun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aber dennoch bleibt sie heilig mir,", "tokens": ["A\u00b7ber", "den\u00b7noch", "bleibt", "sie", "hei\u00b7lig", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Diese Blume \u2013 denn sie kam von ihr.", "tokens": ["Die\u00b7se", "Blu\u00b7me", "\u2013", "denn", "sie", "kam", "von", "ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "KON", "PPER", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Tausend bl\u00fchen schimmernd jetzt im Hain \u2013", "tokens": ["Tau\u00b7send", "bl\u00fc\u00b7hen", "schim\u00b7mernd", "jetzt", "im", "Hain", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Farb' und Duft erf\u00fcllt ihr kurzes Seyn \u2013", "tokens": ["Fa\u00b7rb'", "und", "Duft", "er\u00b7f\u00fcllt", "ihr", "kur\u00b7zes", "Seyn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aber mich reizt ihre Sch\u00f6nheit nicht,", "tokens": ["A\u00b7ber", "mich", "reizt", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wenn nicht ", "tokens": ["Wenn", "nicht"], "token_info": ["word", "word"], "pos": ["KOUS", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "L\u00e4ngst verblichne Blume, Du allein", "tokens": ["L\u00e4ngst", "ver\u00b7blich\u00b7ne", "Blu\u00b7me", ",", "Du", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Sollst mir Weihgeschenk des Fr\u00fchlings seyn.", "tokens": ["Sollst", "mir", "Weih\u00b7ge\u00b7schenk", "des", "Fr\u00fch\u00b7lings", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Thr\u00e4nen tr\u00fcben schwellend meinen Blick,", "tokens": ["Thr\u00e4\u00b7nen", "tr\u00fc\u00b7ben", "schwel\u00b7lend", "mei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Denk' ich an den sch\u00f6nen Tag zur\u00fcck,", "tokens": ["Denk'", "ich", "an", "den", "sch\u00f6\u00b7nen", "Tag", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und ich z\u00e4rtlich an mein Herz Dich dr\u00fcckte.", "tokens": ["Und", "ich", "z\u00e4rt\u00b7lich", "an", "mein", "Herz", "Dich", "dr\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Theure Blume \u2013 \u2013 mein entfloh'nes Gl\u00fcck", "tokens": ["Theu\u00b7re", "Blu\u00b7me", "\u2013", "\u2013", "mein", "ent\u00b7floh'\u00b7nes", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "$(", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Kehrt wie deine Farbe nie zur\u00fcck!", "tokens": ["Kehrt", "wie", "dei\u00b7ne", "Far\u00b7be", "nie", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}