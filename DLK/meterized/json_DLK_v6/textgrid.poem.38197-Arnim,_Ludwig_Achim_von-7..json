{"textgrid.poem.38197": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Weinen thu ich meine Zeit vertreiben,", "tokens": ["Mit", "Wei\u00b7nen", "thu", "ich", "mei\u00b7ne", "Zeit", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Mensch auf Erd den Jammer kann beschreiben,", "tokens": ["Kein", "Mensch", "auf", "Erd", "den", "Jam\u00b7mer", "kann", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den ich erduld bey Nacht und auch bey Tage,", "tokens": ["Den", "ich", "er\u00b7duld", "bey", "Nacht", "und", "auch", "bey", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "APPR", "NN", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und red ich nicht, so t\u00f6dtet mich die Plage.", "tokens": ["Und", "red", "ich", "nicht", ",", "so", "t\u00f6d\u00b7tet", "mich", "die", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Augen mein, vertrocknet tiefe Brunnen,", "tokens": ["Die", "Au\u00b7gen", "mein", ",", "ver\u00b7trock\u00b7net", "tie\u00b7fe", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Weinen sind so g\u00e4nzlich ausgerunnen,", "tokens": ["Durch", "Wei\u00b7nen", "sind", "so", "g\u00e4nz\u00b7lich", "aus\u00b7ge\u00b7run\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich deswegen mu\u00df gar bald verschmachten", "tokens": ["Da\u00df", "ich", "des\u00b7we\u00b7gen", "mu\u00df", "gar", "bald", "ver\u00b7schmach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beym vollen Brunnen, wo ich n\u00e4chtlich wachte.", "tokens": ["Beym", "vol\u00b7len", "Brun\u00b7nen", ",", "wo", "ich", "n\u00e4cht\u00b7lich", "wach\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}