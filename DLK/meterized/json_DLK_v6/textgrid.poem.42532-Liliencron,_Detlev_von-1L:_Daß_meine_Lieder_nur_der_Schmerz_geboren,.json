{"textgrid.poem.42532": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df meine Lieder nur der Schmerz geboren,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df meine Lieder nur der Schmerz geboren,", "tokens": ["Da\u00df", "mei\u00b7ne", "Lie\u00b7der", "nur", "der", "Schmerz", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich besinge nur, was ich verloren:", "tokens": ["Da\u00df", "ich", "be\u00b7sin\u00b7ge", "nur", ",", "was", "ich", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr meint, das sei doch eitle M\u00fchsal nur.", "tokens": ["Ihr", "meint", ",", "das", "sei", "doch", "eit\u00b7le", "M\u00fch\u00b7sal", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich, was ich besa\u00df, nicht kann vergessen,", "tokens": ["Da\u00df", "ich", ",", "was", "ich", "be\u00b7sa\u00df", ",", "nicht", "kann", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKNEG", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df bittre Tr\u00e4nen meine Runzeln n\u00e4ssen:", "tokens": ["Da\u00df", "bitt\u00b7re", "Tr\u00e4\u00b7nen", "mei\u00b7ne", "Run\u00b7zeln", "n\u00e4s\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ists nicht vergangner Freuden letzte Spur?", "tokens": ["Ists", "nicht", "ver\u00b7gang\u00b7ner", "Freu\u00b7den", "letz\u00b7te", "Spur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}