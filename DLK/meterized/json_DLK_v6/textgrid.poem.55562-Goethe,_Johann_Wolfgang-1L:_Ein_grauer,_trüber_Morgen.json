{"textgrid.poem.55562": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein grauer, tr\u00fcber Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein grauer, tr\u00fcber Morgen", "tokens": ["Ein", "grau\u00b7er", ",", "tr\u00fc\u00b7ber", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bedeckt mein liebes Feld,", "tokens": ["Be\u00b7deckt", "mein", "lie\u00b7bes", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Nebel tief verborgen", "tokens": ["Im", "Ne\u00b7bel", "tief", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Liegt um mich her die Welt.", "tokens": ["Liegt", "um", "mich", "her", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APZR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O liebliche Friedrike,", "tokens": ["O", "lieb\u00b7li\u00b7che", "Frie\u00b7dri\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "D\u00fcrft ich nach dir zur\u00fcck,", "tokens": ["D\u00fcrft", "ich", "nach", "dir", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In einem deiner Blicke", "tokens": ["In", "ei\u00b7nem", "dei\u00b7ner", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Liegt Sonnenschein und Gl\u00fcck.", "tokens": ["Liegt", "Son\u00b7nen\u00b7schein", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Baum, in dessen Rinde", "tokens": ["Der", "Baum", ",", "in", "des\u00b7sen", "Rin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Nam bei deinem steht,", "tokens": ["Mein", "Nam", "bei", "dei\u00b7nem", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird bleich vom rauhen Winde,", "tokens": ["Wird", "bleich", "vom", "rau\u00b7hen", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der jede Lust verweht.", "tokens": ["Der", "je\u00b7de", "Lust", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Wiesen gr\u00fcner Schimmer", "tokens": ["Der", "Wie\u00b7sen", "gr\u00fc\u00b7ner", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird tr\u00fcb wie mein Gesicht,", "tokens": ["Wird", "tr\u00fcb", "wie", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie sehen die Sonne nimmer,", "tokens": ["Sie", "se\u00b7hen", "die", "Son\u00b7ne", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und ich Friedriken nicht.", "tokens": ["Und", "ich", "Frie\u00b7dri\u00b7ken", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bald geh ich in die Reben", "tokens": ["Bald", "geh", "ich", "in", "die", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und herbste Trauben ein;", "tokens": ["Und", "herbs\u00b7te", "Trau\u00b7ben", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umher ist ", "tokens": ["Um\u00b7her", "ist"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Es strudelt neuer Wein.", "tokens": ["Es", "stru\u00b7delt", "neu\u00b7er", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch in der \u00f6den Laube,", "tokens": ["Doch", "in", "der", "\u00f6\u00b7den", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach, denk ich, w\u00e4r ", "tokens": ["Ach", ",", "denk", "ich", ",", "w\u00e4r"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ich br\u00e4cht ihr diese Traube,", "tokens": ["Ich", "br\u00e4cht", "ihr", "die\u00b7se", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}