{"dta.poem.12065": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Alles kann der Mensch mit offnen Augen sehn,", "tokens": ["Nicht", "Al\u00b7les", "kann", "der", "Mensch", "mit", "off\u00b7nen", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch manches will und mu\u00df durchs Auge nur geschehn.", "tokens": ["Doch", "man\u00b7ches", "will", "und", "mu\u00df", "durchs", "Au\u00b7ge", "nur", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "KON", "VMFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem was sich sehen l\u00e4\u00dft, schlie\u00df nicht die Augen zu;", "tokens": ["Dem", "was", "sich", "se\u00b7hen", "l\u00e4\u00dft", ",", "schlie\u00df", "nicht", "die", "Au\u00b7gen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PRF", "VVINF", "VVFIN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was sich nicht l\u00e4\u00dft sehn, im Herzen hege du.", "tokens": ["Und", "was", "sich", "nicht", "l\u00e4\u00dft", "sehn", ",", "im", "Her\u00b7zen", "he\u00b7ge", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PTKNEG", "VVFIN", "VVINF", "$,", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Gleich \u00fcbel ist es, statt zu sehn Sichtbares tr\u00e4umen,", "tokens": ["Gleich", "\u00fc\u00b7bel", "ist", "es", ",", "statt", "zu", "sehn", "Sicht\u00b7ba\u00b7res", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "KOUI", "PTKZU", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Unsichtbarem kein Gebiet und Recht einr\u00e4umen.", "tokens": ["Und", "Un\u00b7sicht\u00b7ba\u00b7rem", "kein", "Ge\u00b7biet", "und", "Recht", "ein\u00b7r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}