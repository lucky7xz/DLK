{"textgrid.poem.24084": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Frau G\u00fcte", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut sagte ich die ganze Nacht", "tokens": ["Heut", "sag\u00b7te", "ich", "die", "gan\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Traum: Ich wollt, ich w\u00e4re tot.", "tokens": ["Im", "Traum", ":", "Ich", "wollt", ",", "ich", "w\u00e4\u00b7re", "tot", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, als ich morgens aufgewacht,", "tokens": ["Doch", ",", "als", "ich", "mor\u00b7gens", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begr\u00fc\u00dften mich zwei Lippen rot.", "tokens": ["Be\u00b7gr\u00fc\u00df\u00b7ten", "mich", "zwei", "Lip\u00b7pen", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Frau G\u00fcte hat mich angelacht", "tokens": ["Frau", "G\u00fc\u00b7te", "hat", "mich", "an\u00b7ge\u00b7lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fl\u00fcsterte: Es hat nicht not,", "tokens": ["Und", "fl\u00fcs\u00b7ter\u00b7te", ":", "Es", "hat", "nicht", "not", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Beiseite ist das Gift gebracht,", "tokens": ["Bei\u00b7sei\u00b7te", "ist", "das", "Gift", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da, nimm und i\u00df des Lebens Brot!", "tokens": ["Da", ",", "nimm", "und", "i\u00df", "des", "Le\u00b7bens", "Brot", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVIMP", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den ganzen Tag hab ich gelacht:", "tokens": ["Den", "gan\u00b7zen", "Tag", "hab", "ich", "ge\u00b7lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Meister Tod, gut Nacht, gut Nacht!", "tokens": ["Herr", "Meis\u00b7ter", "Tod", ",", "gut", "Nacht", ",", "gut", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "$,", "ADJD", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist nicht not! Es ist nicht not!", "tokens": ["Es", "ist", "nicht", "not", "!", "Es", "ist", "nicht", "not", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}