{"dta.poem.10314": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.57", "sv:0.28", "no:0.14"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "\u201emit Trommelklang und Pfeif\u2019nget\u00f6n", "tokens": ["\u201e", "mit", "Trom\u00b7mel\u00b7klang", "und", "Pfeif'\u00b7nge\u00b7t\u00f6n"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch frommer Held war begraben,", "tokens": ["Manch", "from\u00b7mer", "Held", "war", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf gr\u00fcner Heid\u2019 gefallen sch\u00f6n,", "tokens": ["Auf", "gr\u00fc\u00b7ner", "Heid'", "ge\u00b7fal\u00b7len", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unsterblichen Ruhm thut er haben!\u201c", "tokens": ["U\u00b7nsterb\u00b7li\u00b7chen", "Ruhm", "thut", "er", "ha\u00b7ben", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VAFIN", "$.", "$("], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}}}}