{"textgrid.poem.43160": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sinnend stand ich bei dem Grabe", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sinnend stand ich bei dem Grabe", "tokens": ["Sin\u00b7nend", "stand", "ich", "bei", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rabby L\u00f6v's, des j\u00fcd'schen Weisen,", "tokens": ["Rab\u00b7by", "L\u00f6\u00b7v's", ",", "des", "j\u00fcd'\u00b7schen", "Wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "H\u00f6rte wie im Traum den F\u00fchrer", "tokens": ["H\u00f6r\u00b7te", "wie", "im", "Traum", "den", "F\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine todten Ahnherrn preisen.", "tokens": ["Sei\u00b7ne", "tod\u00b7ten", "Ahn\u00b7herrn", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und warum, so frug ich staunend,", "tokens": ["Und", "wa\u00b7rum", ",", "so", "frug", "ich", "stau\u00b7nend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "All' die Juden, gro\u00df und kleine,", "tokens": ["All'", "die", "Ju\u00b7den", ",", "gro\u00df", "und", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf das Grab mit leisem Murmeln", "tokens": ["Auf", "das", "Grab", "mit", "lei\u00b7sem", "Mur\u00b7meln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werfen bunte Kieselsteine?", "tokens": ["Wer\u00b7fen", "bun\u00b7te", "Kie\u00b7sel\u00b7stei\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es wurde mir die Antwort:", "tokens": ["Und", "es", "wur\u00b7de", "mir", "die", "Ant\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbum zu ehren, ist geboten,", "tokens": ["\u00bb", "um", "zu", "eh\u00b7ren", ",", "ist", "ge\u00b7bo\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KOUI", "PTKZU", "VVINF", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir Blumen streu'n Lebend'gen,", "tokens": ["Da\u00df", "wir", "Blu\u00b7men", "streu'n", "Le\u00b7ben\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Steine auf das Grab der Todten.\u00ab", "tokens": ["Stei\u00b7ne", "auf", "das", "Grab", "der", "Tod\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Von solch' heidnischem Gebrauche", "tokens": ["Von", "solch'", "heid\u00b7ni\u00b7schem", "Ge\u00b7brau\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind wir Christen l\u00e4ngst gereinigt:", "tokens": ["Sind", "wir", "Chris\u00b7ten", "l\u00e4ngst", "ge\u00b7rei\u00b7nigt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir ", "tokens": ["Wir"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Jener, welche wir ", "tokens": ["Je\u00b7ner", ",", "wel\u00b7che", "wir"], "token_info": ["word", "punct", "word", "word"], "pos": ["PDAT", "$,", "PRELS", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}