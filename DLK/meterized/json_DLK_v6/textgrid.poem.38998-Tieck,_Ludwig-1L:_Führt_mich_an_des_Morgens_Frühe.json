{"textgrid.poem.38998": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fchrt mich an des Morgens Fr\u00fche", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchrt mich an des Morgens Fr\u00fche", "tokens": ["F\u00fchrt", "mich", "an", "des", "Mor\u00b7gens", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die sonnenhellen Gassen,", "tokens": ["Durch", "die", "son\u00b7nen\u00b7hel\u00b7len", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber die zierlichen ebnen Steine", "tokens": ["Ue\u00b7ber", "die", "zier\u00b7li\u00b7chen", "eb\u00b7nen", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der Genius der Neugier durch das Volksgedr\u00e4nge.", "tokens": ["Der", "Ge\u00b7nius", "der", "Neu\u00b7gier", "durch", "das", "Volks\u00b7ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Welche F\u00fclle von Blumen und Fr\u00fcchten", "tokens": ["Wel\u00b7che", "F\u00fcl\u00b7le", "von", "Blu\u00b7men", "und", "Fr\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Bunt und lockend ausgelegt!", "tokens": ["Bunt", "und", "lo\u00b7ckend", "aus\u00b7ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch Geschrei von Verk\u00e4ufern und K\u00e4ufern,", "tokens": ["Welch", "Ge\u00b7schrei", "von", "Ver\u00b7k\u00e4u\u00b7fern", "und", "K\u00e4u\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie lustig ist dieses Marktes Get\u00fcmmel!", "tokens": ["Wie", "lus\u00b7tig", "ist", "die\u00b7ses", "Mark\u00b7tes", "Ge\u00b7t\u00fcm\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Fortgeschoben", "tokens": ["Fort\u00b7ge\u00b7scho\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Seh ich in hohen K\u00f6rben", "tokens": ["Seh", "ich", "in", "ho\u00b7hen", "K\u00f6r\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der sanften Tauben Geschlechter,", "tokens": ["Der", "sanf\u00b7ten", "Tau\u00b7ben", "Ge\u00b7schlech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ruhig liegend, an F\u00fc\u00dfen gebunden,", "tokens": ["Ru\u00b7hig", "lie\u00b7gend", ",", "an", "F\u00fc\u00b7\u00dfen", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Hoch auf einander gepackt.", "tokens": ["Hoch", "auf", "ein\u00b7an\u00b7der", "ge\u00b7packt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und aufgehoben", "tokens": ["Und", "auf\u00b7ge\u00b7ho\u00b7ben"], "token_info": ["word", "word"], "pos": ["KON", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Eine nach der andern,", "tokens": ["Ei\u00b7ne", "nach", "der", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Nimmt sie behende der Alte,", "tokens": ["Nimmt", "sie", "be\u00b7hen\u00b7de", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Oeffnet leicht den Schnabel,", "tokens": ["Oeff\u00b7net", "leicht", "den", "Schna\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Streut einige feine K\u00f6rner hinein.", "tokens": ["Streut", "ei\u00b7ni\u00b7ge", "fei\u00b7ne", "K\u00f6r\u00b7ner", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ein Zweiter empf\u00e4ngt sie,", "tokens": ["Ein", "Zwei\u00b7ter", "emp\u00b7f\u00e4ngt", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Ein kleiner Trichter", "tokens": ["Ein", "klei\u00b7ner", "Trich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Wird ihr in den zarten Schnabel gethan,", "tokens": ["Wird", "ihr", "in", "den", "zar\u00b7ten", "Schna\u00b7bel", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und einige Wassertropfen eingefl\u00f6\u00dft.", "tokens": ["Und", "ei\u00b7ni\u00b7ge", "Was\u00b7ser\u00b7trop\u00b7fen", "ein\u00b7ge\u00b7fl\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Dann wirft er sie neben sich in den Korb,", "tokens": ["Dann", "wirft", "er", "sie", "ne\u00b7ben", "sich", "in", "den", "Korb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und so eine nach der andern,", "tokens": ["Und", "so", "ei\u00b7ne", "nach", "der", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Bis jede genossen,", "tokens": ["Bis", "je\u00b7de", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Was sie in der Hitze bedarf.", "tokens": ["Was", "sie", "in", "der", "Hit\u00b7ze", "be\u00b7darf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.19": {"text": "Noch stand ich l\u00e4chelnd,", "tokens": ["Noch", "stand", "ich", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Und die beiden F\u00fctterer l\u00e4chelten mir entgegen,", "tokens": ["Und", "die", "bei\u00b7den", "F\u00fct\u00b7te\u00b7rer", "l\u00e4\u00b7chel\u00b7ten", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Weil sie meine Unwissenheit merkten,", "tokens": ["Weil", "sie", "mei\u00b7ne", "Un\u00b7wis\u00b7sen\u00b7heit", "merk\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Da\u00df ich nie dergleichen gesehn.", "tokens": ["Da\u00df", "ich", "nie", "derg\u00b7lei\u00b7chen", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Doch sinnend ging ich weiter,", "tokens": ["Doch", "sin\u00b7nend", "ging", "ich", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tiefer Gedanken voll,", "tokens": ["Tie\u00b7fer", "Ge\u00b7dan\u00b7ken", "voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und meine Seele weilte", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "weil\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heimathlicher Gef\u00fchle schwanger", "tokens": ["Hei\u00b7math\u00b7li\u00b7cher", "Ge\u00b7f\u00fch\u00b7le", "schwan\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Im lieben Vaterlande.", "tokens": ["Im", "lie\u00b7ben", "Va\u00b7ter\u00b7lan\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dachte der Lesezirkel,", "tokens": ["Dach\u00b7te", "der", "Le\u00b7se\u00b7zir\u00b7kel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Der Journal-Gesellschaften,", "tokens": ["Der", "Jour\u00b7na\u00b7lGe\u00b7sell\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo den Aufeinandergepackten,", "tokens": ["Wo", "den", "Auf\u00b7ein\u00b7an\u00b7der\u00b7ge\u00b7pack\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Nach Bildung L\u00fcsternen,", "tokens": ["Nach", "Bil\u00b7dung", "L\u00fcs\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "Auch so das M\u00e4ulchen ge\u00f6ffnet wird,", "tokens": ["Auch", "so", "das", "M\u00e4ul\u00b7chen", "ge\u00b7\u00f6ff\u00b7net", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und wenig zarte K\u00f6rner", "tokens": ["Und", "we\u00b7nig", "zar\u00b7te", "K\u00f6r\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und einige Tr\u00f6pflein Wasser", "tokens": ["Und", "ei\u00b7ni\u00b7ge", "Tr\u00f6pf\u00b7lein", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Ihnen zuflie\u00dft von geschickten Fingern.", "tokens": ["Ih\u00b7nen", "zu\u00b7flie\u00dft", "von", "ge\u00b7schick\u00b7ten", "Fin\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "O armes Florenz,", "tokens": ["O", "ar\u00b7mes", "Flo\u00b7renz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das du nur bildlich", "tokens": ["Das", "du", "nur", "bild\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Von unsrer Bildung", "tokens": ["Von", "uns\u00b7rer", "Bil\u00b7dung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die schwache Ahndung hegst!", "tokens": ["Die", "schwa\u00b7che", "Ahn\u00b7dung", "hegst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}