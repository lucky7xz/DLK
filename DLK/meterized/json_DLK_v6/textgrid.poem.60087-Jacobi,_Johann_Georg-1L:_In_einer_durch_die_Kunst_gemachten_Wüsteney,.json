{"textgrid.poem.60087": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer durch die Kunst gemachten W\u00fcsteney,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer durch die Kunst gemachten W\u00fcsteney,", "tokens": ["In", "ei\u00b7ner", "durch", "die", "Kunst", "ge\u00b7mach\u00b7ten", "W\u00fcs\u00b7te\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An einer Garten-Klaus', erbaut f\u00fcr junge Damen", "tokens": ["An", "ei\u00b7ner", "Gar\u00b7ten\u00b7Klaus'", ",", "er\u00b7baut", "f\u00fcr", "jun\u00b7ge", "Da\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Ritter, die nicht oft hineinzuschauen kamen,", "tokens": ["Und", "Rit\u00b7ter", ",", "die", "nicht", "oft", "hin\u00b7ein\u00b7zu\u00b7schau\u00b7en", "ka\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hing eine Spinne, froh und frey,", "tokens": ["Hing", "ei\u00b7ne", "Spin\u00b7ne", ",", "froh", "und", "frey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Eremit im engen Fenster-Rahmen,", "tokens": ["Als", "E\u00b7re\u00b7mit", "im", "en\u00b7gen", "Fens\u00b7ter\u00b7Rah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Begann ihr Werk, und sah dabey", "tokens": ["Be\u00b7gann", "ihr", "Werk", ",", "und", "sah", "da\u00b7bey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im wilden Lustgeh\u00f6lz von Birken Ulmen, Buchen,", "tokens": ["Im", "wil\u00b7den", "Lust\u00b7ge\u00b7h\u00f6lz", "von", "Bir\u00b7ken", "Ul\u00b7men", ",", "Bu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verschiedne V\u00f6gel Mancherley", "tokens": ["Ver\u00b7schied\u00b7ne", "V\u00f6\u00b7gel", "Man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu Nestern sich zusammensuchen.", "tokens": ["Zu", "Nes\u00b7tern", "sich", "zu\u00b7sam\u00b7men\u00b7su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein wohlerfahrner H\u00e4nfling zog", "tokens": ["Ein", "woh\u00b7ler\u00b7fahr\u00b7ner", "H\u00e4nf\u00b7ling", "zog"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf einen Ast, er seine Zweige bog,", "tokens": ["Auf", "ei\u00b7nen", "Ast", ",", "er", "sei\u00b7ne", "Zwei\u00b7ge", "bog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Spinne Fenster zu beschatten.", "tokens": ["Der", "Spin\u00b7ne", "Fens\u00b7ter", "zu", "be\u00b7schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "In voller Arbeit h\u00fcpft' und flog", "tokens": ["In", "vol\u00b7ler", "Ar\u00b7beit", "h\u00fcpft'", "und", "flog"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das H\u00e4nflingsweibchen hin und wieder mit dem Gatten;", "tokens": ["Das", "H\u00e4nf\u00b7lings\u00b7weib\u00b7chen", "hin", "und", "wie\u00b7der", "mit", "dem", "Gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Indessen jene blo\u00df auf ihre F\u00e4den sann,", "tokens": ["In\u00b7des\u00b7sen", "je\u00b7ne", "blo\u00df", "auf", "ih\u00b7re", "F\u00e4\u00b7den", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und aus sich selbst den Zeug der H\u00fctte spann.", "tokens": ["Und", "aus", "sich", "selbst", "den", "Zeug", "der", "H\u00fct\u00b7te", "spann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die armen V\u00f6glein! hub sie an:", "tokens": ["Die", "ar\u00b7men", "V\u00f6\u00b7glein", "!", "hub", "sie", "an", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Mann und Weibchen sich um ihren Bann ermatten!", "tokens": ["Wie", "Mann", "und", "Weib\u00b7chen", "sich", "um", "ih\u00b7ren", "Bann", "er\u00b7mat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hohlen sie von Ost und West", "tokens": ["Was", "hoh\u00b7len", "sie", "von", "Ost", "und", "West"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "PPER", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht alles her! Und steht das Nest \u2013", "tokens": ["Nicht", "al\u00b7les", "her", "!", "Und", "steht", "das", "Nest", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "PTKVZ", "$.", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann neue Sorge, stetes Reisen", "tokens": ["Dann", "neu\u00b7e", "Sor\u00b7ge", ",", "ste\u00b7tes", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Garten, Hof und Feld, die junge Brut zu speisen!", "tokens": ["Durch", "Gar\u00b7ten", ",", "Hof", "und", "Feld", ",", "die", "jun\u00b7ge", "Brut", "zu", "spei\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dann f\u00fcrchten sie des Hauses j\u00e4hen Sturz,", "tokens": ["Dann", "f\u00fcrch\u00b7ten", "sie", "des", "Hau\u00b7ses", "j\u00e4\u00b7hen", "Sturz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn Knaben durch die Hecken rauschen;", "tokens": ["Wenn", "Kna\u00b7ben", "durch", "die", "He\u00b7cken", "rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und flattern auf, und jammern: Kurz!", "tokens": ["Und", "flat\u00b7tern", "auf", ",", "und", "jam\u00b7mern", ":", "Kurz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KON", "VVINF", "$.", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich m\u00f6chte nicht mit ihnen tauschen.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "nicht", "mit", "ih\u00b7nen", "tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der H\u00e4nfling war so eben recht", "tokens": ["Der", "H\u00e4nf\u00b7ling", "war", "so", "e\u00b7ben", "recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Horchen auf den Ast gekommen,", "tokens": ["Zum", "Hor\u00b7chen", "auf", "den", "Ast", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hatt' \u00fcber sich und sein Geschlecht", "tokens": ["Hatt'", "\u00fc\u00b7ber", "sich", "und", "sein", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PRF", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die weise Rede wohl vernommen,", "tokens": ["Die", "wei\u00b7se", "Re\u00b7de", "wohl", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und flog zum Fenster-Rahmen hin!", "tokens": ["Und", "flog", "zum", "Fens\u00b7ter\u00b7Rah\u00b7men", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sagte: \u00bbLiebe Nachbarinn!", "tokens": ["Und", "sag\u00b7te", ":", "\u00bb", "Lie\u00b7be", "Nach\u00b7ba\u00b7rinn", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich lobe deinen klugen Sinn,", "tokens": ["Ich", "lo\u00b7be", "dei\u00b7nen", "klu\u00b7gen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der zwischen kahlen, finstern Mauern", "tokens": ["Der", "zwi\u00b7schen", "kah\u00b7len", ",", "fins\u00b7tern", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dich hier so gl\u00fccklich macht in deinem Selbst-Gespinn:", "tokens": ["Dich", "hier", "so", "gl\u00fcck\u00b7lich", "macht", "in", "dei\u00b7nem", "Selbst\u00b7Ge\u00b7spinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als ich im gr\u00fcnen Walde bin;", "tokens": ["Als", "ich", "im", "gr\u00fc\u00b7nen", "Wal\u00b7de", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Uns aber mu\u00dft du nicht bedauern.", "tokens": ["Uns", "a\u00b7ber", "mu\u00dft", "du", "nicht", "be\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Im gr\u00fcnen Walde gibt es zwar", "tokens": ["Im", "gr\u00fc\u00b7nen", "Wal\u00b7de", "gibt", "es", "zwar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nicht wenig Arbeit und Gefahr;", "tokens": ["Nicht", "we\u00b7nig", "Ar\u00b7beit", "und", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "KON", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Jedoch auf Freude hofft umsonst, wer nie will trauern.", "tokens": ["Je\u00b7doch", "auf", "Freu\u00b7de", "hofft", "um\u00b7sonst", ",", "wer", "nie", "will", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ADV", "$,", "PWS", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Schon \u00f6fter wurde mir um Nest und Futter bang;", "tokens": ["Schon", "\u00f6f\u00b7ter", "wur\u00b7de", "mir", "um", "Nest", "und", "Fut\u00b7ter", "bang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dann regt' ich mich, entfloh dem Untergang;", "tokens": ["Dann", "regt'", "ich", "mich", ",", "ent\u00b7floh", "dem", "Un\u00b7ter\u00b7gang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und heller durch den Busch ert\u00f6nte mein Gesang.", "tokens": ["Und", "hel\u00b7ler", "durch", "den", "Busch", "er\u00b7t\u00f6n\u00b7te", "mein", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich d\u00e4chte, liebe Nachbarinn!", "tokens": ["Ich", "d\u00e4ch\u00b7te", ",", "lie\u00b7be", "Nach\u00b7ba\u00b7rinn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Es w\u00e4re wohl in diesem Leben", "tokens": ["Es", "w\u00e4\u00b7re", "wohl", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Verlust bey jeglichem Gewinn;", "tokens": ["Ver\u00b7lust", "bey", "jeg\u00b7li\u00b7chem", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ich d\u00e4chte, liebe Nachbarinn!", "tokens": ["Ich", "d\u00e4ch\u00b7te", ",", "lie\u00b7be", "Nach\u00b7ba\u00b7rinn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wir nutzten das, was uns Natur gegeben,", "tokens": ["Wir", "nutz\u00b7ten", "das", ",", "was", "uns", "Na\u00b7tur", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PRELS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Zum Nisten mir, und dir zum Weben.\u00ab", "tokens": ["Zum", "Nis\u00b7ten", "mir", ",", "und", "dir", "zum", "We\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "KON", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}