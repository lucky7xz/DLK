{"textgrid.poem.40951": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Dauer des Herzens", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Saumtier tr\u00e4get still", "tokens": ["Ein", "Saum\u00b7tier", "tr\u00e4\u00b7get", "still"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und sanft die Zentnerlast,", "tokens": ["Und", "sanft", "die", "Zent\u00b7ner\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin der Treiber will,", "tokens": ["Wo\u00b7hin", "der", "Trei\u00b7ber", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Begehrend keine Rast.", "tokens": ["Be\u00b7geh\u00b7rend", "kei\u00b7ne", "Rast", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Wagen rollt daher,", "tokens": ["Ein", "Wa\u00b7gen", "rollt", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Schildkr\u00f6t' ihm nicht weicht,", "tokens": ["Die", "Schild\u00b7kr\u00f6t'", "ihm", "nicht", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4r' er noch so schwer,", "tokens": ["Und", "w\u00e4r'", "er", "noch", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tr\u00e4gt seine Last sie leicht.", "tokens": ["Tr\u00e4gt", "sei\u00b7ne", "Last", "sie", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch all die Last ist Scherz,", "tokens": ["Doch", "all", "die", "Last", "ist", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bedenkst du das Gewicht,", "tokens": ["Be\u00b7denkst", "du", "das", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das oft ein Menschenherz", "tokens": ["Das", "oft", "ein", "Men\u00b7schen\u00b7herz"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Still tr\u00e4get und nicht bricht.", "tokens": ["Still", "tr\u00e4\u00b7get", "und", "nicht", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}