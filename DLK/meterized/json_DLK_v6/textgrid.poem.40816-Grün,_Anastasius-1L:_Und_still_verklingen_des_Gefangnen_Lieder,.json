{"textgrid.poem.40816": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und still verklingen des Gefangnen Lieder,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und still verklingen des Gefangnen Lieder,", "tokens": ["Und", "still", "ver\u00b7klin\u00b7gen", "des", "Ge\u00b7fang\u00b7nen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wellen wimmern, fahle Wolken reisen;", "tokens": ["Die", "Wel\u00b7len", "wim\u00b7mern", ",", "fah\u00b7le", "Wol\u00b7ken", "rei\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da jauchzt es unfern mir und jauchzet wieder", "tokens": ["Da", "jauchzt", "es", "un\u00b7fern", "mir", "und", "jauch\u00b7zet", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und singt, mir fast zur Unzeit, lust'ge Weisen.", "tokens": ["Und", "singt", ",", "mir", "fast", "zur", "Un\u00b7zeit", ",", "lust'\u00b7ge", "Wei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "ADV", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir naht ein Greis mit silberwei\u00dfen Haaren,", "tokens": ["Mir", "naht", "ein", "Greis", "mit", "sil\u00b7ber\u00b7wei\u00b7\u00dfen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch Morgenroth des Frohsinns auf der Wange!", "tokens": ["Doch", "Mor\u00b7gen\u00b7roth", "des", "Froh\u00b7sinns", "auf", "der", "Wan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ei, selt'ne Nachbarschaft! Wie Rosenschaaren,", "tokens": ["Ei", ",", "selt'\u00b7ne", "Nach\u00b7bar\u00b7schaft", "!", "Wie", "Ro\u00b7sen\u00b7schaa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umbl\u00fchend Gletschereis am Alpenhange!", "tokens": ["Um\u00b7bl\u00fc\u00b7hend", "Glet\u00b7sche\u00b7reis", "am", "Al\u00b7pen\u00b7han\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Willkommen, Greis! Du mu\u00dft wohl Kunde wissen", "tokens": ["Will\u00b7kom\u00b7men", ",", "Greis", "!", "Du", "mu\u00dft", "wohl", "Kun\u00b7de", "wis\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "PPER", "VMFIN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von diesem d\u00fcstern grauenvollen Hause,", "tokens": ["Von", "die\u00b7sem", "d\u00fcs\u00b7tern", "grau\u00b7en\u00b7vol\u00b7len", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer einst ge\u00e4chzt in seinen Finsternissen?", "tokens": ["Wer", "einst", "ge\u00b7\u00e4chzt", "in", "sei\u00b7nen", "Fins\u00b7ter\u00b7nis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "We\u00df Ketten klirrten durch die dunkle Klause?", "tokens": ["We\u00df", "Ket\u00b7ten", "klirr\u00b7ten", "durch", "die", "dunk\u00b7le", "Klau\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbge\u00e4chzt hat Niemand als die Wetterfahne,", "tokens": ["\u00bb", "ge\u00b7\u00e4chzt", "hat", "Nie\u00b7mand", "als", "die", "Wet\u00b7ter\u00b7fah\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie der Wind gedreht im spr\u00f6den Gleise!", "tokens": ["Wenn", "sie", "der", "Wind", "ge\u00b7dreht", "im", "spr\u00f6\u00b7den", "Glei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geklirrt hat nichts hier, als von dem Altane", "tokens": ["Ge\u00b7klirrt", "hat", "nichts", "hier", ",", "als", "von", "dem", "Al\u00b7ta\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PIS", "ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Becher all' in lust'ger Br\u00fcder Kreise!", "tokens": ["Die", "Be\u00b7cher", "all'", "in", "lust'\u00b7ger", "Br\u00fc\u00b7der", "Krei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Leuchtthurm war die\u00df Haus in alten Tagen,", "tokens": ["Ein", "Leucht\u00b7thurm", "war", "die\u00df", "Haus", "in", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerfallen nun, seit dort gebaut der neue;", "tokens": ["Zer\u00b7fal\u00b7len", "nun", ",", "seit", "dort", "ge\u00b7baut", "der", "neu\u00b7e", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "ADV", "VVPP", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Anstatt des Invaliden, lahmgeschlagen,", "tokens": ["An\u00b7statt", "des", "In\u00b7va\u00b7li\u00b7den", ",", "lahm\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Trat der Rekrute in die offne Reihe.", "tokens": ["Trat", "der", "Rek\u00b7ru\u00b7te", "in", "die", "off\u00b7ne", "Rei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich war sein W\u00e4chtersmann, der wohlbestallte,", "tokens": ["Ich", "war", "sein", "W\u00e4ch\u00b7ters\u00b7mann", ",", "der", "wohl\u00b7be\u00b7stall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gottlob, da\u00df Pech und Wein dem Land nicht fehlen!", "tokens": ["Gott\u00b7lob", ",", "da\u00df", "Pech", "und", "Wein", "dem", "Land", "nicht", "feh\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NE", "KON", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ha, wie, wenn Wind und Wetter pfiff und hallte,", "tokens": ["Ha", ",", "wie", ",", "wenn", "Wind", "und", "Wet\u00b7ter", "pfiff", "und", "hall\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geflammt die Leuchten, und gejauchzt die Kehlen!\u00ab", "tokens": ["Ge\u00b7flammt", "die", "Leuch\u00b7ten", ",", "und", "ge\u00b7jauchzt", "die", "Keh\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVPP", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "So sprach der Greis; noch leuchtet des Gelages", "tokens": ["So", "sprach", "der", "Greis", ";", "noch", "leuch\u00b7tet", "des", "Ge\u00b7la\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erinnerung ums Haupt dem alten Zecher,", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "ums", "Haupt", "dem", "al\u00b7ten", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie durch der D\u00e4mm'rung Grau Nachglanz des Tages,", "tokens": ["Wie", "durch", "der", "D\u00e4m\u00b7m'\u00b7rung", "Grau", "Nach\u00b7glanz", "des", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie Reste Rebenbluts durch leere Becher.", "tokens": ["Wie", "Res\u00b7te", "Re\u00b7ben\u00b7bluts", "durch", "lee\u00b7re", "Be\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So sang ich in des Lichtes Heiligthumen", "tokens": ["So", "sang", "ich", "in", "des", "Lich\u00b7tes", "Hei\u00b7lig\u00b7thu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Finsternissen und verdorrten Lenzen!", "tokens": ["Von", "Fins\u00b7ter\u00b7nis\u00b7sen", "und", "ver\u00b7dorr\u00b7ten", "Len\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der G\u00e4rtner zieht zu Wonn' und Lust die Blumen", "tokens": ["Der", "G\u00e4rt\u00b7ner", "zieht", "zu", "Wonn'", "und", "Lust", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und, ach, verbraucht sie oft zu Todtenkr\u00e4nzen!", "tokens": ["Und", ",", "ach", ",", "ver\u00b7braucht", "sie", "oft", "zu", "Tod\u00b7ten\u00b7kr\u00e4n\u00b7zen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "So war der Hain des Friedens und der Liebe", "tokens": ["So", "war", "der", "Hain", "des", "Frie\u00b7dens", "und", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir \u00fcberschattet von dem Baum der Schmerzen!", "tokens": ["Mir", "\u00fc\u00b7bersc\u00b7hat\u00b7tet", "von", "dem", "Baum", "der", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich d\u00fcnkt wohl gar, des dunklen Stammes Triebe,", "tokens": ["Mich", "d\u00fcnkt", "wohl", "gar", ",", "des", "dunk\u00b7len", "Stam\u00b7mes", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie wurzeln nur in meinem eignen Herzen.", "tokens": ["Sie", "wur\u00b7zeln", "nur", "in", "mei\u00b7nem", "eig\u00b7nen", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Verglommen m\u00e4hlich ist die Abendr\u00f6the,", "tokens": ["Ver\u00b7glom\u00b7men", "m\u00e4h\u00b7lich", "ist", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es senkt die Nacht des schwarzen Mantels Schwere", "tokens": ["Es", "senkt", "die", "Nacht", "des", "schwar\u00b7zen", "Man\u00b7tels", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rings um die Tr\u00fcmmer und die Blumenbeete", "tokens": ["Rings", "um", "die", "Tr\u00fcm\u00b7mer", "und", "die", "Blu\u00b7men\u00b7bee\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcber weites Land und ew'ge Meere.", "tokens": ["Und", "\u00fc\u00b7ber", "wei\u00b7tes", "Land", "und", "ew'\u00b7ge", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Da l\u00e4\u00dft der Himmel Mond und Stern' erglimmen,", "tokens": ["Da", "l\u00e4\u00dft", "der", "Him\u00b7mel", "Mond", "und", "Stern'", "er\u00b7glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da gl\u00fchn am Golf empor des Leuchtthurms Flammen:", "tokens": ["Da", "gl\u00fchn", "am", "Golf", "em\u00b7por", "des", "Leucht\u00b7thurms", "Flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Licht! Licht! ihr Losungswort, das gro\u00dfe, stimmen", "tokens": ["Licht", "!", "Licht", "!", "ihr", "Lo\u00b7sungs\u00b7wort", ",", "das", "gro\u00b7\u00dfe", ",", "stim\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$.", "NN", "$.", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt Erd' und Himmel, Gott und Mensch zusammen.", "tokens": ["Jetzt", "Erd'", "und", "Him\u00b7mel", ",", "Gott", "und", "Mensch", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}