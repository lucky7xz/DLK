{"textgrid.poem.50862": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "5. \u00c4nnchen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Liebste will ich nehmen:", "tokens": ["Drei", "Liebs\u00b7te", "will", "ich", "neh\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der erste mu\u00df ein Kaufmann sein,", "tokens": ["Der", "ers\u00b7te", "mu\u00df", "ein", "Kauf\u00b7mann", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der andere ein G\u00e4rtner,", "tokens": ["Der", "an\u00b7de\u00b7re", "ein", "G\u00e4rt\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der dritt ein Betteljung.", "tokens": ["Der", "dritt", "ein", "Bet\u00b7tel\u00b7jung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Kaufmann soll mir bringen", "tokens": ["Der", "Kauf\u00b7mann", "soll", "mir", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl Perlen, Gold und Edelstein,", "tokens": ["Wohl", "Per\u00b7len", ",", "Gold", "und", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der G\u00e4rtner s\u00fc\u00dfe Fr\u00fcchte", "tokens": ["Der", "G\u00e4rt\u00b7ner", "s\u00fc\u00b7\u00dfe", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "All f\u00fcr den Betteljung.", "tokens": ["All", "f\u00fcr", "den", "Bet\u00b7tel\u00b7jung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Kaufmann soll mir bauen", "tokens": ["Der", "Kauf\u00b7mann", "soll", "mir", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Haus mit einem sch\u00f6nen Saal,", "tokens": ["Ein", "Haus", "mit", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der G\u00e4rtner gr\u00fcne Reben", "tokens": ["Der", "G\u00e4rt\u00b7ner", "gr\u00fc\u00b7ne", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr meinen Betteljung.", "tokens": ["F\u00fcr", "mei\u00b7nen", "Bet\u00b7tel\u00b7jung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Kaufmann soll mich kleiden", "tokens": ["Der", "Kauf\u00b7mann", "soll", "mich", "klei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Seiden und in blauen Samt,", "tokens": ["In", "Sei\u00b7den", "und", "in", "blau\u00b7en", "Samt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Haar der G\u00e4rtner kr\u00e4nzen", "tokens": ["Mein", "Haar", "der", "G\u00e4rt\u00b7ner", "kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sch\u00f6n f\u00fcr den Betteljung.", "tokens": ["Sch\u00f6n", "f\u00fcr", "den", "Bet\u00b7tel\u00b7jung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Der Kaufmann und der G\u00e4rtner,", "tokens": ["Der", "Kauf\u00b7mann", "und", "der", "G\u00e4rt\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie sollen haben keinen Lohn,", "tokens": ["Sie", "sol\u00b7len", "ha\u00b7ben", "kei\u00b7nen", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch viele tausend K\u00fcsse", "tokens": ["Doch", "vie\u00b7le", "tau\u00b7send", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein lieber Betteljung.", "tokens": ["Mein", "lie\u00b7ber", "Bet\u00b7tel\u00b7jung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wenn wir sind gestorben", "tokens": ["Und", "wenn", "wir", "sind", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schaun die ewige Seligkeit:", "tokens": ["Und", "schaun", "die", "e\u00b7wi\u00b7ge", "Se\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann sollen sie begraben", "tokens": ["Dann", "sol\u00b7len", "sie", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich und den Betteljung.", "tokens": ["Mich", "und", "den", "Bet\u00b7tel\u00b7jung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Kaufmann soll errichten", "tokens": ["Der", "Kauf\u00b7mann", "soll", "er\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Marmor einen Leichenstein,", "tokens": ["Von", "Mar\u00b7mor", "ei\u00b7nen", "Lei\u00b7chen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der G\u00e4rtner Rosen pflanzen", "tokens": ["Der", "G\u00e4rt\u00b7ner", "Ro\u00b7sen", "pflan\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir und dem Betteljung!", "tokens": ["Mir", "und", "dem", "Bet\u00b7tel\u00b7jung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}