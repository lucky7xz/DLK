{"textgrid.poem.42174": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Meiner entz\u00fcckenden Kollegin Mary I.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von vorn besehn bist du die sch\u00f6nste Maid,", "tokens": ["Von", "vorn", "be\u00b7sehn", "bist", "du", "die", "sch\u00f6ns\u00b7te", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVINF", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die je mein Herz aus Liebesnot befreit;", "tokens": ["Die", "je", "mein", "Herz", "aus", "Lie\u00b7bes\u00b7not", "be\u00b7freit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wenn du halb nur dich zur Seite kehrst,", "tokens": ["Doch", "wenn", "du", "halb", "nur", "dich", "zur", "Sei\u00b7te", "kehrst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann d\u00fcnkt mich schon, da\u00df du ein Knabe w\u00e4rst.", "tokens": ["Dann", "d\u00fcnkt", "mich", "schon", ",", "da\u00df", "du", "ein", "Kna\u00b7be", "w\u00e4rst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Drum bleib ich wie dem Gl\u00fccksrad stets dir nah,", "tokens": ["Drum", "bleib", "ich", "wie", "dem", "Gl\u00fccks\u00b7rad", "stets", "dir", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du \u2013 Venus Duplex Amathusia!", "tokens": ["Du", "\u2013", "Ve\u00b7nus", "Du\u00b7plex", "A\u00b7mat\u00b7hu\u00b7sia", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "NE", "NE", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}