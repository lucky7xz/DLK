{"textgrid.poem.55528": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Elegie auf den Tod des Bruders", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im d\u00fcstern Wald, auf der gespaltnen Eiche,", "tokens": ["Im", "d\u00fcs\u00b7tern", "Wald", ",", "auf", "der", "ge\u00b7spalt\u00b7nen", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die einst der Donner hingestreckt,", "tokens": ["Die", "einst", "der", "Don\u00b7ner", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sing ich um deines Bruders Leiche,", "tokens": ["Sing", "ich", "um", "dei\u00b7nes", "Bru\u00b7ders", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die fern von uns ein fremdes Grab bedeckt.", "tokens": ["Die", "fern", "von", "uns", "ein", "frem\u00b7des", "Grab", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nah schon dem Herbste seiner Jahre,", "tokens": ["Nah", "schon", "dem", "Herbs\u00b7te", "sei\u00b7ner", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hofft' er getrost der Taten Lohn;", "tokens": ["Hofft'", "er", "ge\u00b7trost", "der", "Ta\u00b7ten", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch unaufhaltsam trug die Bahre", "tokens": ["Doch", "un\u00b7auf\u00b7halt\u00b7sam", "trug", "die", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn schnell davon.", "tokens": ["Ihn", "schnell", "da\u00b7von", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Du weinest nicht? \u2013 Dir nahm ein langes Scheiden", "tokens": ["Du", "wei\u00b7nest", "nicht", "?", "\u2013", "Dir", "nahm", "ein", "lan\u00b7ges", "Schei\u00b7den"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Hoffnung, ihn hier noch einmal zu sehn.", "tokens": ["Die", "Hoff\u00b7nung", ",", "ihn", "hier", "noch", "ein\u00b7mal", "zu", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gott lie\u00df vor dir ihn zu dem Himmel gehn;", "tokens": ["Gott", "lie\u00df", "vor", "dir", "ihn", "zu", "dem", "Him\u00b7mel", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du sahst's und konntest nichts als ihn beneiden.", "tokens": ["Du", "sahst's", "und", "konn\u00b7test", "nichts", "als", "ihn", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "PIS", "KOKOM", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch horch \u2013 Welch eine Stimm voll Schmerz", "tokens": ["Doch", "horch", "\u2013", "Welch", "ei\u00b7ne", "Stimm", "voll", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$(", "PIAT", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "T\u00f6nt in mein Ohr von seinem Grabe?", "tokens": ["T\u00f6nt", "in", "mein", "Ohr", "von", "sei\u00b7nem", "Gra\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich eil, ich seh, sie ist's! Ihr Herz", "tokens": ["Ich", "eil", ",", "ich", "seh", ",", "sie", "ist's", "!", "Ihr", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt mit in seinem Grabe.", "tokens": ["Liegt", "mit", "in", "sei\u00b7nem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Verlassen, ohne Trost liegt hie,", "tokens": ["Ver\u00b7las\u00b7sen", ",", "oh\u00b7ne", "Trost", "liegt", "hie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUI", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit \u00e4ngstlicher Geb\u00e4rde", "tokens": ["Mit", "\u00e4ngst\u00b7li\u00b7cher", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu Gott gekehrt, als hoffte sie,", "tokens": ["Zu", "Gott", "ge\u00b7kehrt", ",", "als", "hoff\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KOUS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sch\u00f6nste M\u00e4dchen an der Erde.", "tokens": ["Das", "sch\u00f6ns\u00b7te", "M\u00e4d\u00b7chen", "an", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nie hat ein Herz so viel gelitten,", "tokens": ["Nie", "hat", "ein", "Herz", "so", "viel", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herr, sieh herab auf ihre Not,", "tokens": ["Herr", ",", "sieh", "her\u00b7ab", "auf", "ih\u00b7re", "Not", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schenke gn\u00e4dig ihren Bitten", "tokens": ["Und", "schen\u00b7ke", "gn\u00e4\u00b7dig", "ih\u00b7ren", "Bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Leben oder ihren Tod.", "tokens": ["Sein", "Le\u00b7ben", "o\u00b7der", "ih\u00b7ren", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O Gott, bestrafest du die Liebe,", "tokens": ["O", "Gott", ",", "be\u00b7stra\u00b7fest", "du", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Wesen voller Lieb und Huld?", "tokens": ["Du", "We\u00b7sen", "vol\u00b7ler", "Lieb", "und", "Huld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn nichts als eine heil'ge Liebe", "tokens": ["Denn", "nichts", "als", "ei\u00b7ne", "heil'\u00b7ge", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War dieser Ungl\u00fccksel'gen Schuld.", "tokens": ["War", "die\u00b7ser", "Un\u00b7gl\u00fcck\u00b7sel'\u00b7gen", "Schuld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie hofft' im hochzeitlichen Kleide", "tokens": ["Sie", "hofft'", "im", "hoch\u00b7zeit\u00b7li\u00b7chen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Bald mit ihm zum Altar zu ziehn;", "tokens": ["Bald", "mit", "ihm", "zum", "Al\u00b7tar", "zu", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da ri\u00df sein F\u00fcrst von ihrer Seite", "tokens": ["Da", "ri\u00df", "sein", "F\u00fcrst", "von", "ih\u00b7rer", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tyrannisch ihn.", "tokens": ["Ty\u00b7ran\u00b7nisch", "ihn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "O F\u00fcrst, du kannst die Menschen zwingen,", "tokens": ["O", "F\u00fcrst", ",", "du", "kannst", "die", "Men\u00b7schen", "zwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr dich allein ihr Leben zuzubringen,", "tokens": ["F\u00fcr", "dich", "al\u00b7lein", "ihr", "Le\u00b7ben", "zu\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das wird man deinem Stolz verzeihn;", "tokens": ["Das", "wird", "man", "dei\u00b7nem", "Stolz", "ver\u00b7zeihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch willst du ihre Seelen binden,", "tokens": ["Doch", "willst", "du", "ih\u00b7re", "See\u00b7len", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch dich zu denken, zu empfinden,", "tokens": ["Durch", "dich", "zu", "den\u00b7ken", ",", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das mu\u00df zu Gott um Rache schrein.", "tokens": ["Das", "mu\u00df", "zu", "Gott", "um", "Ra\u00b7che", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie ward sein gro\u00dfes Herz durchstochen,", "tokens": ["Wie", "ward", "sein", "gro\u00b7\u00dfes", "Herz", "durch\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als er, der nie sein Wort gebrochen,", "tokens": ["Als", "er", ",", "der", "nie", "sein", "Wort", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Wort zum ersten Male brach,", "tokens": ["Sein", "Wort", "zum", "ers\u00b7ten", "Ma\u00b7le", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum ersten Mal es der Geliebten brach,", "tokens": ["Zum", "ers\u00b7ten", "Mal", "es", "der", "Ge\u00b7lieb\u00b7ten", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der, eh es noch sein Mund versprach,", "tokens": ["Der", ",", "eh", "es", "noch", "sein", "Mund", "ver\u00b7sprach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Herz ein ewig Band versprochen.", "tokens": ["Sein", "Herz", "ein", "e\u00b7wig", "Band", "ver\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbals B\u00fcrger der bedr\u00e4ngten Erde\u00ab,", "tokens": ["\u00bb", "als", "B\u00fcr\u00b7ger", "der", "be\u00b7dr\u00e4ng\u00b7ten", "Er\u00b7de", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "NN", "ART", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach er, \u00bbkann ich nie deine sein;", "tokens": ["Sprach", "er", ",", "\u00bb", "kann", "ich", "nie", "dei\u00b7ne", "sein", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "$(", "VMFIN", "PPER", "ADV", "PPOSAT", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Doch von der Furcht, da\u00df ich dir untreu werde,", "tokens": ["Doch", "von", "der", "Furcht", ",", "da\u00df", "ich", "dir", "un\u00b7treu", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll dich mein Tod befrein.", "tokens": ["Soll", "dich", "mein", "Tod", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Leb wohl, es wein bei meinem Grabe", "tokens": ["Leb", "wohl", ",", "es", "wein", "bei", "mei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jed' z\u00e4rtlich Herz, ger\u00fchrt von meiner Treu,", "tokens": ["Jed'", "z\u00e4rt\u00b7lich", "Herz", ",", "ge\u00b7r\u00fchrt", "von", "mei\u00b7ner", "Treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann eil die stolze Tyrannei,", "tokens": ["Dann", "eil", "die", "stol\u00b7ze", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ich schon l\u00e4ngst vergeben habe,", "tokens": ["Der", "ich", "schon", "l\u00e4ngst", "ver\u00b7ge\u00b7ben", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df sie des Grabes Ursach sei,", "tokens": ["Da\u00df", "sie", "des", "Gra\u00b7bes", "Ur\u00b7sach", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Unwillig f\u00fchlend, schnell vorbei.\u00ab", "tokens": ["Un\u00b7wil\u00b7lig", "f\u00fch\u00b7lend", ",", "schnell", "vor\u00b7bei", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}