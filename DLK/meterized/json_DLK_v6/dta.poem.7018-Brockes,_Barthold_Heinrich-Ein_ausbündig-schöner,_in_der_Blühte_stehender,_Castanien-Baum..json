{"dta.poem.7018": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein ausb\u00fcndig-sch\u00f6ner, in der  \n Bl\u00fchte stehender, Castanien-Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die gr\u00fcne Nacht, die in dem Baum regieret,", "tokens": ["Die", "gr\u00fc\u00b7ne", "Nacht", ",", "die", "in", "dem", "Baum", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird durch das sch\u00f6ne Laub erzeuget und formiret:", "tokens": ["Wird", "durch", "das", "sch\u00f6\u00b7ne", "Laub", "er\u00b7zeu\u00b7get", "und", "for\u00b7mi\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das Laub, dergleichen Laub man sonsten nirgends findet,", "tokens": ["Das", "Laub", ",", "derg\u00b7lei\u00b7chen", "Laub", "man", "sons\u00b7ten", "nir\u00b7gends", "fin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "NN", "PIS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist nah sowohl, als auch von weiten, sch\u00f6n", "tokens": ["Ist", "nah", "so\u00b7wohl", ",", "als", "auch", "von", "wei\u00b7ten", ",", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADJD", "KON", "$,", "KOUS", "ADV", "APPR", "ADJA", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An Gr\u00f6sse, Farb\u2019 und Bildung anzusehn.", "tokens": ["An", "Gr\u00f6s\u00b7se", ",", "Fa\u00b7rb'", "und", "Bil\u00b7dung", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Fast einer halben Elle lang", "tokens": ["Fast", "ei\u00b7ner", "hal\u00b7ben", "El\u00b7le", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist manches Blat; derselben Rang", "tokens": ["Ist", "man\u00b7ches", "Blat", ";", "der\u00b7sel\u00b7ben", "Rang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "$.", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist frembd und sehr verwunderlich:", "tokens": ["Ist", "frembd", "und", "sehr", "ver\u00b7wun\u00b7der\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von einer Seite spannt es sich,", "tokens": ["Von", "ei\u00b7ner", "Sei\u00b7te", "spannt", "es", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In einer angenehmen R\u00fcnde,", "tokens": ["In", "ei\u00b7ner", "an\u00b7ge\u00b7neh\u00b7men", "R\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Gleich einem F\u00e4chtel aus. Die Untersten sind klein,", "tokens": ["Gleich", "ei\u00b7nem", "F\u00e4ch\u00b7tel", "aus", ".", "Die", "Un\u00b7ters\u00b7ten", "sind", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Indem die Mittelsten die allergr\u00f6sten seyn.", "tokens": ["In\u00b7dem", "die", "Mit\u00b7tel\u00b7sten", "die", "al\u00b7ler\u00b7gr\u00f6s\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein jedes Blatt", "tokens": ["Ein", "je\u00b7des", "Blatt"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Ist unten zierlich spitz, und oben zierlich breit,", "tokens": ["Ist", "un\u00b7ten", "zier\u00b7lich", "spitz", ",", "und", "o\u00b7ben", "zier\u00b7lich", "breit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADJD", "$,", "KON", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wodurch es die Gestalt von einer Birne hat.", "tokens": ["Wo\u00b7durch", "es", "die", "Ge\u00b7stalt", "von", "ei\u00b7ner", "Bir\u00b7ne", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der allersch\u00f6nste Tafft ist nicht so sch\u00f6n,", "tokens": ["Der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Tafft", "ist", "nicht", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Als solch ein Blatt, zumahl", "tokens": ["Als", "solch", "ein", "Blatt", ",", "zu\u00b7mahl"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIAT", "ART", "NN", "$,", "KOUS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Jm hellen Sonnen-Strahl,", "tokens": ["Jm", "hel\u00b7len", "Son\u00b7nen\u00b7Strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Wenn man es recht betrachtet, anzusehn.", "tokens": ["Wenn", "man", "es", "recht", "be\u00b7trach\u00b7tet", ",", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVPP", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der Adern Menge, die in sch\u00f6nster Ordnung steht,", "tokens": ["Der", "A\u00b7dern", "Men\u00b7ge", ",", "die", "in", "sch\u00f6ns\u00b7ter", "Ord\u00b7nung", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "L\u00e4sst recht, als w\u00e4re sie gew\u00fcrcket und geneht.", "tokens": ["L\u00e4sst", "recht", ",", "als", "w\u00e4\u00b7re", "sie", "ge\u00b7w\u00fcr\u00b7cket", "und", "ge\u00b7neht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOKOM", "VAFIN", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auf eines jeden Zweiges Ecken,", "tokens": ["Auf", "ei\u00b7nes", "je\u00b7den", "Zwei\u00b7ges", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die mit dem sch\u00f6nsten Laubwerck prangt,", "tokens": ["Die", "mit", "dem", "sch\u00f6ns\u00b7ten", "Laub\u00b7werck", "prangt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "So wie ein gr\u00fcner Busch von Federn abw\u00e4rts hangt,", "tokens": ["So", "wie", "ein", "gr\u00fc\u00b7ner", "Busch", "von", "Fe\u00b7dern", "ab\u00b7w\u00e4rts", "hangt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sieht man die weisse Bl\u00fcth\u2019 sich pr\u00e4chtig aufwerts strecken,", "tokens": ["Sieht", "man", "die", "weis\u00b7se", "Bl\u00fcth'", "sich", "pr\u00e4ch\u00b7tig", "auf\u00b7werts", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "PRF", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der sch\u00f6nsten Pyramide gleich.", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Py\u00b7ra\u00b7mi\u00b7de", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Es gehen dieser Bl\u00fcth\u2019, im sch\u00f6nen Bluhmen-Reich,", "tokens": ["Es", "ge\u00b7hen", "die\u00b7ser", "Bl\u00fcth'", ",", "im", "sch\u00f6\u00b7nen", "Bluh\u00b7men\u00b7Reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "An Gr\u00f6sse, Nettigkeit, an Farben, Form und Zier,", "tokens": ["An", "Gr\u00f6s\u00b7se", ",", "Net\u00b7tig\u00b7keit", ",", "an", "Far\u00b7ben", ",", "Form", "und", "Zier", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sehr wenig Bluhmen f\u00fcr.", "tokens": ["Sehr", "we\u00b7nig", "Bluh\u00b7men", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Wer dies nicht glauben will, beschau\u2019 sie in der N\u00e4he,", "tokens": ["Wer", "dies", "nicht", "glau\u00b7ben", "will", ",", "be\u00b7schau'", "sie", "in", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "PTKNEG", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So wird es ihm gewi\u00df, wie mir, ergehn,", "tokens": ["So", "wird", "es", "ihm", "ge\u00b7wi\u00df", ",", "wie", "mir", ",", "er\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "$,", "PWAV", "PPER", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Ich hielte sie von weiten artig, sch\u00f6n:", "tokens": ["Ich", "hiel\u00b7te", "sie", "von", "wei\u00b7ten", "ar\u00b7tig", ",", "sch\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Allein, ich stutzte recht, als ich sie nah\u2019 erblickte,", "tokens": ["Al\u00b7lein", ",", "ich", "stutz\u00b7te", "recht", ",", "als", "ich", "sie", "nah'", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und wie, mit solcher Seltenheit,", "tokens": ["Und", "wie", ",", "mit", "sol\u00b7cher", "Sel\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Auch nie bisher von mir gesp\u00fchrter Lieblichkeit,", "tokens": ["Auch", "nie", "bis\u00b7her", "von", "mir", "ge\u00b7sp\u00fchr\u00b7ter", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sie von Natur an Farb\u2019 und Bildung schm\u00fcckte.", "tokens": ["Sie", "von", "Na\u00b7tur", "an", "Fa\u00b7rb'", "und", "Bil\u00b7dung", "schm\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Es ist der Stiel, der sonderlich formirt,", "tokens": ["Es", "ist", "der", "Stiel", ",", "der", "son\u00b7der\u00b7lich", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Mit vierzig Stengeln ausgeziert,", "tokens": ["Mit", "vier\u00b7zig", "Sten\u00b7geln", "aus\u00b7ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Die alle, erstlich auf-hernach sich abwerts biegen,", "tokens": ["Die", "al\u00b7le", ",", "erst\u00b7lich", "auf\u00b7her\u00b7nach", "sich", "ab\u00b7werts", "bie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADJD", "VVFIN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und welche oberw\u00e4rts allein", "tokens": ["Und", "wel\u00b7che", "o\u00b7berw\u00b7\u00e4rts", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRELS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Mit Bluhmen ausgezieret seyn,", "tokens": ["Mit", "Bluh\u00b7men", "aus\u00b7ge\u00b7zie\u00b7ret", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Die sie, und zwar blo\u00df an der Spitze, decken,", "tokens": ["Die", "sie", ",", "und", "zwar", "blo\u00df", "an", "der", "Spit\u00b7ze", ",", "de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "KON", "ADV", "ADV", "APPR", "ART", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Da denn sie sich ein wenig abwerts strecken,", "tokens": ["Da", "denn", "sie", "sich", "ein", "we\u00b7nig", "ab\u00b7werts", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "ART", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Ich eine angenehme R\u00fcnde", "tokens": ["Ich", "ei\u00b7ne", "an\u00b7ge\u00b7neh\u00b7me", "R\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Dadurch formiret finde:", "tokens": ["Da\u00b7durch", "for\u00b7mi\u00b7ret", "fin\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.46": {"text": "Und weil die Obern etwas klein,", "tokens": ["Und", "weil", "die", "O\u00b7bern", "et\u00b7was", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Die untern Stengel l\u00e4nger seyn;", "tokens": ["Die", "un\u00b7tern", "Sten\u00b7gel", "l\u00e4n\u00b7ger", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Entsteht dadurch an einer jeden", "tokens": ["Ent\u00b7steht", "da\u00b7durch", "an", "ei\u00b7ner", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "APPR", "ART", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Die 