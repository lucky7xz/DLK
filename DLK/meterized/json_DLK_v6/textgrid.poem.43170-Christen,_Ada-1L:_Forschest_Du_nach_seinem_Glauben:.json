{"textgrid.poem.43170": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Forschest Du nach seinem Glauben:", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Forschest Du nach seinem Glauben:", "tokens": ["For\u00b7schest", "Du", "nach", "sei\u00b7nem", "Glau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klimpert er mit den Dukaten,", "tokens": ["Klim\u00b7pert", "er", "mit", "den", "Du\u00b7ka\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Fragst Du ihn nach seinem Namen:", "tokens": ["Fragst", "Du", "ihn", "nach", "sei\u00b7nem", "Na\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird er nach dem Deinen rathen.", "tokens": ["Wird", "er", "nach", "dem", "Dei\u00b7nen", "ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stiefelknarrend \u2013 H\u00fcftenwiegend", "tokens": ["Stie\u00b7fel\u00b7knar\u00b7rend", "\u2013", "H\u00fcf\u00b7ten\u00b7wie\u00b7gend"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$(", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeigt die S\u00e4le er, die gro\u00dfen,", "tokens": ["Zeigt", "die", "S\u00e4\u00b7le", "er", ",", "die", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erregt von Zukunftspl\u00e4nen,", "tokens": ["Und", "er\u00b7regt", "von", "Zu\u00b7kunfts\u00b7pl\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleppt er Dich zu seinen Spro\u00dfen. \u2013", "tokens": ["Schleppt", "er", "Dich", "zu", "sei\u00b7nen", "Spro\u00b7\u00dfen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Klein und schmutzig sind die Jungen,", "tokens": ["Klein", "und", "schmut\u00b7zig", "sind", "die", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grob und protzig, gleich den Alten,", "tokens": ["Grob", "und", "prot\u00b7zig", ",", "gleich", "den", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die gro\u00dfen krummen Nasen", "tokens": ["Um", "die", "gro\u00b7\u00dfen", "krum\u00b7men", "Na\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zieh'n sie pfiffig-dumme Falten. \u2013", "tokens": ["Zieh'n", "sie", "pfif\u00b7fig\u00b7dum\u00b7me", "Fal\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sprichst Du auch von seinen Freunden", "tokens": ["Sprichst", "Du", "auch", "von", "sei\u00b7nen", "Freun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder seinen Anverwandten,", "tokens": ["O\u00b7der", "sei\u00b7nen", "An\u00b7ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigt er nach den Bildersch\u00e4tzen, \u2013", "tokens": ["Zeigt", "er", "nach", "den", "Bil\u00b7der\u00b7sch\u00e4t\u00b7zen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Prahlt mit f\u00fcrstlichen Bekannten.", "tokens": ["Prahlt", "mit", "f\u00fcrst\u00b7li\u00b7chen", "Be\u00b7kann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Suchst Du mit poet'schen Worten", "tokens": ["Suchst", "Du", "mit", "po\u00b7et'\u00b7schen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm die Seele zu bewegen:", "tokens": ["Ihm", "die", "See\u00b7le", "zu", "be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Starrt aus seinen trock'nen Z\u00fcgen", "tokens": ["Starrt", "aus", "sei\u00b7nen", "trock'\u00b7nen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir das gold'ne \u2013 Kalb entgegen!", "tokens": ["Dir", "das", "gold'\u00b7ne", "\u2013", "Kalb", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "$(", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}