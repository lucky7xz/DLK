{"textgrid.poem.43026": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Im October", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcbe Bilder ziehn vor\u00fcber,", "tokens": ["Tr\u00fc\u00b7be", "Bil\u00b7der", "ziehn", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es bluten alte Wunden!", "tokens": ["Und", "es", "blu\u00b7ten", "al\u00b7te", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "V\u00f6lker sch\u00fcttelt rings das Fieber:", "tokens": ["V\u00f6l\u00b7ker", "sch\u00fct\u00b7telt", "rings", "das", "Fie\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ein ", "tokens": ["Kann", "ein"], "token_info": ["word", "word"], "pos": ["VMFIN", "ART"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Seit der heil'gen Nacht des M\u00e4rzen", "tokens": ["Seit", "der", "heil'\u00b7gen", "Nacht", "des", "M\u00e4r\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ging manch' sch\u00f6ne Hoffnung unter,", "tokens": ["Ging", "man\u00b7ch'", "sch\u00f6\u00b7ne", "Hoff\u00b7nung", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und der jungen Freiheit Kerzen", "tokens": ["Und", "der", "jun\u00b7gen", "Frei\u00b7heit", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Brannten matt und tief herunter!", "tokens": ["Brann\u00b7ten", "matt", "und", "tief", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wir stehn mit dumpfem Schweigen", "tokens": ["Und", "wir", "stehn", "mit", "dum\u00b7pfem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei dem fr\u00fch erloschnen Glanze;", "tokens": ["Bei", "dem", "fr\u00fch", "er\u00b7lo\u00b7schnen", "Glan\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Begeist'rung wilder Reigen", "tokens": ["Der", "Be\u00b7geist'\u00b7rung", "wil\u00b7der", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward zu fr\u00fch zum Todtentanze.", "tokens": ["Ward", "zu", "fr\u00fch", "zum", "Tod\u00b7ten\u00b7tan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Tr\u00fcbe Bilder ziehn vor\u00fcber,", "tokens": ["Tr\u00fc\u00b7be", "Bil\u00b7der", "ziehn", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es bluten alte Wunden!", "tokens": ["Und", "es", "blu\u00b7ten", "al\u00b7te", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "V\u00f6lker sch\u00fcttelt rings das Fieber:", "tokens": ["V\u00f6l\u00b7ker", "sch\u00fct\u00b7telt", "rings", "das", "Fie\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ein ", "tokens": ["Kann", "ein"], "token_info": ["word", "word"], "pos": ["VMFIN", "ART"], "meter": "-+", "measure": "iambic.single"}}}}}