{"textgrid.poem.56494": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein K\u00f6nigreich. Provinzen roter Wiesen.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein K\u00f6nigreich. Provinzen roter Wiesen.", "tokens": ["Ein", "K\u00f6\u00b7nig\u00b7reich", ".", "Pro\u00b7vin\u00b7zen", "ro\u00b7ter", "Wie\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein W\u00e4rter, eine Peitsche, eine Kette.", "tokens": ["Ein", "W\u00e4r\u00b7ter", ",", "ei\u00b7ne", "Peit\u00b7sche", ",", "ei\u00b7ne", "Ket\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So klappern wir in Nessel, Dorn und Klette", "tokens": ["So", "klap\u00b7pern", "wir", "in", "Nes\u00b7sel", ",", "Dorn", "und", "Klet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch wilder Himmel schreckliche Devisen,", "tokens": ["Durch", "wil\u00b7der", "Him\u00b7mel", "schreck\u00b7li\u00b7che", "De\u00b7vi\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die uns bedrohn mit den gezackten Flammen,", "tokens": ["Die", "uns", "be\u00b7drohn", "mit", "den", "ge\u00b7zack\u00b7ten", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit gro\u00dfer Hieroglyphen roter Schrift.", "tokens": ["Mit", "gro\u00b7\u00dfer", "Hie\u00b7ro\u00b7gly\u00b7phen", "ro\u00b7ter", "Schrift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unsrer Schlangenadern blaues Gift", "tokens": ["Und", "uns\u00b7rer", "Schlan\u00b7ge\u00b7na\u00b7dern", "blau\u00b7es", "Gift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zieht krampfhaft sich in unserm Kopf zusammen.", "tokens": ["Zieht", "krampf\u00b7haft", "sich", "in", "un\u00b7serm", "Kopf", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df tausend Disteln unsere Beine schlagen,", "tokens": ["Da\u00df", "tau\u00b7send", "Dis\u00b7teln", "un\u00b7se\u00b7re", "Bei\u00b7ne", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df manchen Regenw\u00fcrmchens K\u00f6pfchen knackt,", "tokens": ["Da\u00df", "man\u00b7chen", "Re\u00b7gen\u00b7w\u00fcrm\u00b7chens", "K\u00f6pf\u00b7chen", "knackt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu unseres wilden Volks Bacchanten-Takt,", "tokens": ["Zu", "un\u00b7se\u00b7res", "wil\u00b7den", "Volks", "Bac\u00b7chan\u00b7ten\u00b7Takt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wir h\u00f6ren's ferne nur in unsere Klagen.", "tokens": ["Wir", "h\u00f6\u00b7ren's", "fer\u00b7ne", "nur", "in", "un\u00b7se\u00b7re", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein gl\u00e4sern leichter Fu\u00df ward uns gegeben,", "tokens": ["Ein", "gl\u00e4\u00b7sern", "leich\u00b7ter", "Fu\u00df", "ward", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Scharlachfl\u00fcgel w\u00e4chst aus unserm R\u00fccken.", "tokens": ["Und", "Schar\u00b7lach\u00b7fl\u00fc\u00b7gel", "w\u00e4chst", "aus", "un\u00b7serm", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So tanzen wir zum Krach der Scherben-St\u00fccken,", "tokens": ["So", "tan\u00b7zen", "wir", "zum", "Krach", "der", "Scher\u00b7ben\u00b7St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch lauter Unrat feierlich zu schweben.", "tokens": ["Durch", "lau\u00b7ter", "Un\u00b7rat", "fei\u00b7er\u00b7lich", "zu", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Welch g\u00f6ttlich sch\u00f6nes Spiel. Ein Meer von Feuer.", "tokens": ["Welch", "g\u00f6tt\u00b7lich", "sch\u00f6\u00b7nes", "Spiel", ".", "Ein", "Meer", "von", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "$.", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ganze Himmel brennt. Wir sind allein,", "tokens": ["Der", "gan\u00b7ze", "Him\u00b7mel", "brennt", ".", "Wir", "sind", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Halbg\u00f6tter wir. Und unser haarig Bein", "tokens": ["Halb\u00b7g\u00f6t\u00b7ter", "wir", ".", "Und", "un\u00b7ser", "haa\u00b7rig", "Bein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "KON", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Springt nackt auf altem Steine im Gem\u00e4uer.", "tokens": ["Springt", "nackt", "auf", "al\u00b7tem", "Stei\u00b7ne", "im", "Ge\u00b7m\u00e4u\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Verfallner Ort, versunken tief im Schutte,", "tokens": ["Ver\u00b7fall\u00b7ner", "Ort", ",", "ver\u00b7sun\u00b7ken", "tief", "im", "Schut\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo wie ein K\u00f6nigshaupt der Ginster schwankt,", "tokens": ["Wo", "wie", "ein", "K\u00f6\u00b7nigs\u00b7haupt", "der", "Gins\u00b7ter", "schwankt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOKOM", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des goldner Arm nach unsern Kn\u00f6cheln langt", "tokens": ["Des", "gold\u00b7ner", "Arm", "nach", "un\u00b7sern", "Kn\u00f6\u00b7cheln", "langt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00fcstern f\u00e4hrt herauf in unsrer Kutte.", "tokens": ["Und", "l\u00fcs\u00b7tern", "f\u00e4hrt", "her\u00b7auf", "in", "uns\u00b7rer", "Kut\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wo eine alte Weide, d\u00fcrr und stumm,", "tokens": ["Wo", "ei\u00b7ne", "al\u00b7te", "Wei\u00b7de", ",", "d\u00fcrr", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Talismanen ihren Bauch beh\u00e4ngt,", "tokens": ["Mit", "Ta\u00b7lis\u00b7ma\u00b7nen", "ih\u00b7ren", "Bauch", "be\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor unsrer G\u00f6ttlichkeit die Arme senkt,", "tokens": ["Vor", "uns\u00b7rer", "G\u00f6tt\u00b7lich\u00b7keit", "die", "Ar\u00b7me", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und uns beschielt mit Augen, wei\u00df und krumm.", "tokens": ["Und", "uns", "be\u00b7schielt", "mit", "Au\u00b7gen", ",", "wei\u00df", "und", "krumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Aus ihrem Loch springt eine alte Maus,", "tokens": ["Aus", "ih\u00b7rem", "Loch", "springt", "ei\u00b7ne", "al\u00b7te", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verr\u00fcckt wie wir. Ein goldner Schnabel blinkt", "tokens": ["Ver\u00b7r\u00fcckt", "wie", "wir", ".", "Ein", "gold\u00b7ner", "Schna\u00b7bel", "blinkt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KOKOM", "PPER", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Himmelsrand. Ein leises Lied erklingt,", "tokens": ["Am", "Him\u00b7mels\u00b7rand", ".", "Ein", "lei\u00b7ses", "Lied", "er\u00b7klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Schwan zieht in das Feuer uns voraus.", "tokens": ["Ein", "Schwan", "zieht", "in", "das", "Feu\u00b7er", "uns", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O s\u00fc\u00dfer Sterbeton, den wir geschl\u00fcrft.", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Ster\u00b7be\u00b7ton", ",", "den", "wir", "ge\u00b7schl\u00fcrft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Breitschwingig flattert er im goldnen West,", "tokens": ["Breit\u00b7schwin\u00b7gig", "flat\u00b7tert", "er", "im", "gold\u00b7nen", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo hoher Pappeln zitterndes Ge\u00e4st", "tokens": ["Wo", "ho\u00b7her", "Pap\u00b7peln", "zit\u00b7tern\u00b7des", "Ge\u00b7\u00e4st"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf unsere Stirnen Gitterschatten wirft.", "tokens": ["Auf", "un\u00b7se\u00b7re", "Stir\u00b7nen", "Git\u00b7ter\u00b7schat\u00b7ten", "wirft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Die Sonne sinkt auf dunkelroter Bahn,", "tokens": ["Die", "Son\u00b7ne", "sinkt", "auf", "dun\u00b7kel\u00b7ro\u00b7ter", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In einer Wetterwolke klemmt sie fest.", "tokens": ["In", "ei\u00b7ner", "Wet\u00b7ter\u00b7wol\u00b7ke", "klemmt", "sie", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Macht schnell und rei\u00dft aus seinem schwarzen Nest", "tokens": ["Macht", "schnell", "und", "rei\u00dft", "aus", "sei\u00b7nem", "schwar\u00b7zen", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Zangen aus den goldnen Wolken-Zahn.", "tokens": ["Mit", "Zan\u00b7gen", "aus", "den", "gold\u00b7nen", "Wol\u00b7ken\u00b7Zahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Hui. Er ist fort. Der dunkle Himmel sinkt", "tokens": ["Hui", ".", "Er", "ist", "fort", ".", "Der", "dunk\u00b7le", "Him\u00b7mel", "sinkt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM", "$.", "PPER", "VAFIN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll Zorn herab in einen schwarzen Teich,", "tokens": ["Voll", "Zorn", "her\u00b7ab", "in", "ei\u00b7nen", "schwar\u00b7zen", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Abgrund droht, mit fahlen Wolken bleich,", "tokens": ["Des", "Ab\u00b7grund", "droht", ",", "mit", "fah\u00b7len", "Wol\u00b7ken", "bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unheimlich, eine Nacht, die Unheil bringt.", "tokens": ["Un\u00b7heim\u00b7lich", ",", "ei\u00b7ne", "Nacht", ",", "die", "Un\u00b7heil", "bringt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und eine Leiche wohnt im tiefen Grund,", "tokens": ["Und", "ei\u00b7ne", "Lei\u00b7che", "wohnt", "im", "tie\u00b7fen", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um die ein Aale-Volk geschmeidig h\u00fcpft.", "tokens": ["Um", "die", "ein", "Aal\u00b7e\u00b7Volk", "ge\u00b7schmei\u00b7dig", "h\u00fcpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Uralt, ein Fisch, der ein zum Ohre schl\u00fcpft", "tokens": ["Ur\u00b7alt", ",", "ein", "Fisch", ",", "der", "ein", "zum", "Oh\u00b7re", "schl\u00fcpft"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "PRELS", "ART", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wieder ausf\u00e4hrt aus dem offnen Mund.", "tokens": ["Und", "wie\u00b7der", "aus\u00b7f\u00e4hrt", "aus", "dem", "off\u00b7nen", "Mund", "."], "token_info