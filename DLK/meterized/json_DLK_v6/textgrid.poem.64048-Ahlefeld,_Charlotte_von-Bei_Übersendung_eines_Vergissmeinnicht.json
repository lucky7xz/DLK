{"textgrid.poem.64048": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Bei \u00dcbersendung eines Vergissmeinnicht", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Blume, deren blaue Bl\u00fcthe", "tokens": ["Die\u00b7se", "Blu\u00b7me", ",", "de\u00b7ren", "blau\u00b7e", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deutungsvoll der sch\u00f6nste Nahme schm\u00fcckt,", "tokens": ["Deu\u00b7tungs\u00b7voll", "der", "sch\u00f6ns\u00b7te", "Nah\u00b7me", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der als Wunsch mir l\u00e4ngst im Herzen gl\u00fchte,", "tokens": ["Der", "als", "Wunsch", "mir", "l\u00e4ngst", "im", "Her\u00b7zen", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hab' ich einsam heut' im Thal gepfl\u00fcckt.", "tokens": ["Hab'", "ich", "ein\u00b7sam", "heut'", "im", "Thal", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "S\u00fcss umschwebt von Deinem theuern Bilde,", "tokens": ["S\u00fcss", "um\u00b7schwebt", "von", "Dei\u00b7nem", "theu\u00b7ern", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schien sie w\u00fcrdig zur Gesandtin mir;", "tokens": ["Schien", "sie", "w\u00fcr\u00b7dig", "zur", "Ge\u00b7sand\u00b7tin", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hin in ferne, trennende Gefilde,", "tokens": ["Hin", "in", "fer\u00b7ne", ",", "tren\u00b7nen\u00b7de", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bringe sie den Gruss der Freundschaft Dir.", "tokens": ["Brin\u00b7ge", "sie", "den", "Gruss", "der", "Freund\u00b7schaft", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ehe sie Dir naht wird sie verbleichen \u2013", "tokens": ["E\u00b7he", "sie", "Dir", "naht", "wird", "sie", "ver\u00b7blei\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VAFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schnell verl\u00f6schet ihrer Farbe Licht,", "tokens": ["Schnell", "ver\u00b7l\u00f6\u00b7schet", "ih\u00b7rer", "Far\u00b7be", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch die Bitte m\u00f6ge Dich erreichen,", "tokens": ["Doch", "die", "Bit\u00b7te", "m\u00f6\u00b7ge", "Dich", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die ihr Nahme z\u00e4rtlich zu Dir spricht.", "tokens": ["Die", "ihr", "Nah\u00b7me", "z\u00e4rt\u00b7lich", "zu", "Dir", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}