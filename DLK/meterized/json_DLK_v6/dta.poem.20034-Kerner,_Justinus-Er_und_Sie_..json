{"dta.poem.20034": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Er und Sie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er.", "tokens": ["Er", "."], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Seh' ich in das stille Thal,", "tokens": ["Seh'", "ich", "in", "das", "stil\u00b7le", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo im Sonnenscheine", "tokens": ["Wo", "im", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Blumen stehen ohne Zahl,", "tokens": ["Blu\u00b7men", "ste\u00b7hen", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blick' ich nur auf Eine.", "tokens": ["Blick'", "ich", "nur", "auf", "Ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ach! es blickt Ihr Auge blau", "tokens": ["Ach", "!", "es", "blickt", "Ihr", "Au\u00b7ge", "blau"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jezt auch auf die Auen;", "tokens": ["Jezt", "auch", "auf", "die", "Au\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Im Vergi\u00dfmeinnicht voll Thau", "tokens": ["Im", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "voll", "Thau"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Kann ich es erschauen.", "tokens": ["Kann", "ich", "es", "er\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sie.", "tokens": ["Sie", "."], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Tret' ich an mein Fensterlein,", "tokens": ["Tret'", "ich", "an", "mein", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann die Sterne scheinen,", "tokens": ["Wann", "die", "Ster\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "M\u00f6gen alle sch\u00f6ner seyn,", "tokens": ["M\u00f6\u00b7gen", "al\u00b7le", "sch\u00f6\u00b7ner", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blick' ich nur auf Einen;", "tokens": ["Blick'", "ich", "nur", "auf", "Ei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dort gen Abend blickt Er mild", "tokens": ["Dort", "gen", "A\u00b7bend", "blickt", "Er", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wohl nach Himmelsh\u00f6hen!", "tokens": ["Wohl", "nach", "Him\u00b7mels\u00b7h\u00f6\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Denn dort ist ein liebes Bild", "tokens": ["Denn", "dort", "ist", "ein", "lie\u00b7bes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "In dem Stern zu sehen.", "tokens": ["In", "dem", "Stern", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}