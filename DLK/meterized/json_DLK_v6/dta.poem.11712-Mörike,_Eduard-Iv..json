{"dta.poem.11712": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Warum, Geliebte, denk' ich dein", "tokens": ["Wa\u00b7rum", ",", "Ge\u00b7lieb\u00b7te", ",", "denk'", "ich", "dein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Einmal mit viel tausend Thr\u00e4nen,", "tokens": ["Auf", "Ein\u00b7mal", "mit", "viel", "tau\u00b7send", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und kann gar nicht zufrieden seyn,", "tokens": ["Und", "kann", "gar", "nicht", "zu\u00b7frie\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will die Brust in alle Ferne dehnen?", "tokens": ["Und", "will", "die", "Brust", "in", "al\u00b7le", "Fer\u00b7ne", "deh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, gestern in den hellen Kindersaal", "tokens": ["Ach", ",", "ge\u00b7stern", "in", "den", "hel\u00b7len", "Kin\u00b7der\u00b7saal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Beim Flimmer zierlich aufgesteckter Kerzen,", "tokens": ["Beim", "Flim\u00b7mer", "zier\u00b7lich", "auf\u00b7ge\u00b7steck\u00b7ter", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo ich mein selbst verga\u00df in L\u00e4rm und Scherzen,", "tokens": ["Wo", "ich", "mein", "selbst", "ver\u00b7ga\u00df", "in", "L\u00e4rm", "und", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tratst du, o Bildni\u00df mitleid-sch\u00f6ner Qual;", "tokens": ["Tratst", "du", ",", "o", "Bild\u00b7ni\u00df", "mit\u00b7lei\u00b7dsch\u00f6\u00b7ner", "Qual", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es war dein Geist, er sezte sich an's Mahl,", "tokens": ["Es", "war", "dein", "Geist", ",", "er", "sez\u00b7te", "sich", "an's", "Mahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wir sa\u00dfen fremd mit stumm verhaltnen Schmerzen,", "tokens": ["Wir", "sa\u00b7\u00dfen", "fremd", "mit", "stumm", "ver\u00b7halt\u00b7nen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zulezt brach ich in lautes Schluchzen aus,", "tokens": ["Zu\u00b7lezt", "brach", "ich", "in", "lau\u00b7tes", "Schluch\u00b7zen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Hand in Hand verlie\u00dfen wir das Haus.", "tokens": ["Und", "Hand", "in", "Hand", "ver\u00b7lie\u00b7\u00dfen", "wir", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}