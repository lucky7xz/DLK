{"textgrid.poem.32279": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Der trunkne Dichter lobt den Wein", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Ehren, Wein, von dir bemeistert,", "tokens": ["Mit", "Eh\u00b7ren", ",", "Wein", ",", "von", "dir", "be\u00b7meis\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und deinem fl\u00fc\u00df'gen Feu'r begeistert,", "tokens": ["Und", "dei\u00b7nem", "fl\u00fc\u00df'\u00b7gen", "Feu'r", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stimm ich zum Danke, wenn ich kann,", "tokens": ["Stimm", "ich", "zum", "Dan\u00b7ke", ",", "wenn", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "PTKANT", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein dir geheiligt Loblied an.", "tokens": ["Ein", "dir", "ge\u00b7hei\u00b7ligt", "Lob\u00b7lied", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wie? in was f\u00fcr k\u00fchnen Weisen", "tokens": ["Doch", "wie", "?", "in", "was", "f\u00fcr", "k\u00fch\u00b7nen", "Wei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$.", "APPR", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Werd' ich, o G\u00f6ttertrank, dich preisen?", "tokens": ["Werd'", "ich", ",", "o", "G\u00f6t\u00b7ter\u00b7trank", ",", "dich", "prei\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Ruhm, h\u00f6r' ihn summarisch an,", "tokens": ["Dein", "Ruhm", ",", "h\u00f6r'", "ihn", "sum\u00b7ma\u00b7risch", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist, da\u00df ich ihn nicht singen kann.", "tokens": ["Ist", ",", "da\u00df", "ich", "ihn", "nicht", "sin\u00b7gen", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}