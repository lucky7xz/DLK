{"dta.poem.18865": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Sie schl\u00fcrften die Geister des Schlafes,", "tokens": ["Sie", "schl\u00fcrf\u00b7ten", "die", "Geis\u00b7ter", "des", "Schla\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Aber wie sie mit den K\u00f6pfen nickten", "tokens": ["A\u00b7ber", "wie", "sie", "mit", "den", "K\u00f6p\u00b7fen", "nick\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schlugen wir sie", "tokens": ["Schlu\u00b7gen", "wir", "sie"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und sie waren dahin.", "tokens": ["Und", "sie", "wa\u00b7ren", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PAV", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}