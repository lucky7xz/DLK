{"dta.poem.5345": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Johannis-Beere.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die liebliche Johannis-Beer", "tokens": ["Die", "lieb\u00b7li\u00b7che", "Jo\u00b7han\u00b7nis\u00b7Beer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bewegt mich jetzt, zu GOttes Ehr,", "tokens": ["Be\u00b7wegt", "mich", "jetzt", ",", "zu", "Got\u00b7tes", "Ehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein neues Liedchen anzustimmen.", "tokens": ["Ein", "neu\u00b7es", "Lied\u00b7chen", "an\u00b7zu\u00b7stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich seh\u2019 sie durch der Bl\u00e4tter Gr\u00fcn,", "tokens": ["Ich", "seh'", "sie", "durch", "der", "Bl\u00e4t\u00b7ter", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich einem funckelnden Rubin,", "tokens": ["Gleich", "ei\u00b7nem", "fun\u00b7ckeln\u00b7den", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In rohtem Schimmer gl\u00e4ntzend gl\u00fchn,", "tokens": ["In", "roh\u00b7tem", "Schim\u00b7mer", "gl\u00e4nt\u00b7zend", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In rothem Licht und Feuer glimmen.", "tokens": ["In", "ro\u00b7them", "Licht", "und", "Feu\u00b7er", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn man zuerst die gelblich gr\u00fcne Bl\u00fcth\u2019,", "tokens": ["Wenn", "man", "zu\u00b7erst", "die", "gelb\u00b7lich", "gr\u00fc\u00b7ne", "Bl\u00fcth'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie zierlich sie gebildet, sieht;", "tokens": ["Wie", "zier\u00b7lich", "sie", "ge\u00b7bil\u00b7det", ",", "sieht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ergetzet sich mit Recht ein Menschliches Gem\u00fcth.", "tokens": ["Er\u00b7get\u00b7zet", "sich", "mit", "Recht", "ein", "Menschli\u00b7ches", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Indem die spielende Natur", "tokens": ["In\u00b7dem", "die", "spie\u00b7len\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie mit der zierlichsten Figur", "tokens": ["Sie", "mit", "der", "zier\u00b7lichs\u00b7ten", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und sanften Farben ausgeschm\u00fccket:", "tokens": ["Und", "sanf\u00b7ten", "Far\u00b7ben", "aus\u00b7ge\u00b7schm\u00fc\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Allein was wird gar bald an ihr erblicket?", "tokens": ["Al\u00b7lein", "was", "wird", "gar", "bald", "an", "ihr", "er\u00b7bli\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ein jedes Bl\u00fcmchen dehnet sich", "tokens": ["Ein", "je\u00b7des", "Bl\u00fcm\u00b7chen", "deh\u00b7net", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Am Stengel in vollkommner R\u00fcnde,", "tokens": ["Am", "Sten\u00b7gel", "in", "voll\u00b7komm\u00b7ner", "R\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und zeiget recht verwunderlich", "tokens": ["Und", "zei\u00b7get", "recht", "ver\u00b7wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein K\u00fcgelchen, so ich erst gr\u00fcnlich finde;", "tokens": ["Ein", "K\u00fc\u00b7gel\u00b7chen", ",", "so", "ich", "erst", "gr\u00fcn\u00b7lich", "fin\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Doch wird nicht lang\u2019 hernach aus diesem Gr\u00fcnen", "tokens": ["Doch", "wird", "nicht", "lang'", "her\u00b7nach", "aus", "die\u00b7sem", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das allersch\u00f6nste Roth. Man sieht es, wie Carbunckeln,", "tokens": ["Das", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Roth", ".", "Man", "sieht", "es", ",", "wie", "Car\u00b7bun\u00b7ckeln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PIS", "VVFIN", "PPER", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durchsichtig gl\u00e4ntzen, gl\u00fchn und funckeln.", "tokens": ["Durch\u00b7sich\u00b7tig", "gl\u00e4nt\u00b7zen", ",", "gl\u00fchn", "und", "fun\u00b7ckeln", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "KON", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Man r\u00fcnde funckelnde Rubinen,", "tokens": ["Man", "r\u00fcn\u00b7de", "fun\u00b7ckeln\u00b7de", "Ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.16": {"text": "Mit allen Flei\u00df, mit aller Kunst und M\u00fch,", "tokens": ["Mit", "al\u00b7len", "Flei\u00df", ",", "mit", "al\u00b7ler", "Kunst", "und", "M\u00fch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Man schleiffe, man poliere sie,", "tokens": ["Man", "schleif\u00b7fe", ",", "man", "po\u00b7lie\u00b7re", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und sehe denn, ob man in ihnen", "tokens": ["Und", "se\u00b7he", "denn", ",", "ob", "man", "in", "ih\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PIS", "APPR", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "An R\u00fcnde mehr Vollkommenheit,", "tokens": ["An", "R\u00fcn\u00b7de", "mehr", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "An R\u00f6th\u2019 und Glut mehr Glantz und Lieblichkeit,", "tokens": ["An", "R\u00f6\u00b7th'", "und", "Glut", "mehr", "Glantz", "und", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.21": {"text": "Als an der funckelnden Johannis-Beer,", "tokens": ["Als", "an", "der", "fun\u00b7ckeln\u00b7den", "Jo\u00b7han\u00b7nis\u00b7Beer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Zu ihrer und zu unsers Sch\u00f6pfers Ehr,", "tokens": ["Zu", "ih\u00b7rer", "und", "zu", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ersehn und finden kann?", "tokens": ["Er\u00b7sehn", "und", "fin\u00b7den", "kann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zumahl sie all an kleinen Stangen,", "tokens": ["Zu\u00b7mahl", "sie", "all", "an", "klei\u00b7nen", "Stan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In andrer Ordnung noch, als wie die Trauben, hangen.", "tokens": ["In", "an\u00b7drer", "Ord\u00b7nung", "noch", ",", "als", "wie", "die", "Trau\u00b7ben", ",", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja, was noch mehr, man trift in ihnen an", "tokens": ["Ja", ",", "was", "noch", "mehr", ",", "man", "trift", "in", "ih\u00b7nen", "an"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ADV", "$,", "PIS", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An Farben, und Geschmack von s\u00fcsser S\u00e4urlichkeit", "tokens": ["An", "Far\u00b7ben", ",", "und", "Ge\u00b7schmack", "von", "s\u00fcs\u00b7ser", "S\u00e4ur\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Solch einen grossen Unterscheid,", "tokens": ["Solch", "ei\u00b7nen", "gros\u00b7sen", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als man nicht leicht an andern Fr\u00fcchten findet;", "tokens": ["Als", "man", "nicht", "leicht", "an", "an\u00b7dern", "Fr\u00fcch\u00b7ten", "fin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da mit der R\u00f6hte sich bey vielen wei\u00df verbindet,", "tokens": ["Da", "mit", "der", "R\u00f6h\u00b7te", "sich", "bey", "vie\u00b7len", "wei\u00df", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "APPR", "PIS", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da viele g\u00e4ntzlich wei\u00df, theils leibfarb, schwartz so gar.", "tokens": ["Da", "vie\u00b7le", "g\u00e4ntz\u00b7lich", "wei\u00df", ",", "theils", "leib\u00b7farb", ",", "schwartz", "so", "gar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "$,", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die alle nun bedeckt der gr\u00fcnen Bl\u00e4tter-Schaar.", "tokens": ["Die", "al\u00b7le", "nun", "be\u00b7deckt", "der", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter\u00b7Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die ebenfals mit sondrer Zierlichkeit", "tokens": ["Die", "e\u00b7ben\u00b7fals", "mit", "sond\u00b7rer", "Zier\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Von Fingern der Natur formiret,", "tokens": ["Von", "Fin\u00b7gern", "der", "Na\u00b7tur", "for\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und nett, wie Wein-Laub fast, gezieret.", "tokens": ["Und", "nett", ",", "wie", "Wein\u00b7Laub", "fast", ",", "ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie angenehm, wie lieblich, und wie sch\u00f6n", "tokens": ["Wie", "an\u00b7ge\u00b7nehm", ",", "wie", "lieb\u00b7lich", ",", "und", "wie", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "KON", "PWAV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die Frucht nun anzusehn;", "tokens": ["Die", "Frucht", "nun", "an\u00b7zu\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "So lieb- und n\u00fctzlich ist der s\u00e4urlich s\u00fcsse Saft", "tokens": ["So", "lie\u00b7b", "und", "n\u00fctz\u00b7lich", "ist", "der", "s\u00e4ur\u00b7lich", "s\u00fcs\u00b7se", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "TRUNC", "KON", "ADJD", "VAFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Und sein\u2019 erquickende sanft k\u00fchlend\u2019 Eigenschaft,", "tokens": ["Und", "sein'", "er\u00b7qui\u00b7cken\u00b7de", "sanft", "k\u00fch\u00b7lend'", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Da sie nicht roh\u2019 allein,", "tokens": ["Da", "sie", "nicht", "roh'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Der Zungen angenehm, dem Blut erfrischend seyn;", "tokens": ["Der", "Zun\u00b7gen", "an\u00b7ge\u00b7nehm", ",", "dem", "Blut", "er\u00b7fri\u00b7schend", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nein, da sie auch, in Zucker eingeleget,", "tokens": ["Nein", ",", "da", "sie", "auch", ",", "in", "Zu\u00b7cker", "ein\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "So wol den Krancken, als Gesunden,", "tokens": ["So", "wol", "den", "Kran\u00b7cken", ",", "als", "Ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Zu laben zu ergetzen pfleget.", "tokens": ["Zu", "la\u00b7ben", "zu", "er\u00b7get\u00b7zen", "pfle\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Welch eine Linderung wird nicht im Stein empfunden,", "tokens": ["Welch", "ei\u00b7ne", "Lin\u00b7de\u00b7rung", "wird", "nicht", "im", "Stein", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "PTKNEG", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Durch ihre schwartze Frucht! Wenn uns das Wasser", "tokens": ["Durch", "ih\u00b7re", "schwart\u00b7ze", "Frucht", "!", "Wenn", "uns", "das", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Nicht minder in der Gicht und Winden auch", "tokens": ["Nicht", "min\u00b7der", "in", "der", "Gicht", "und", "Win\u00b7den", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Ist heilsam, ist bewehrt und dienlich ihr Gebrauch.", "tokens": ["Ist", "heil\u00b7sam", ",", "ist", "be\u00b7wehrt", "und", "dien\u00b7lich", "ihr", "Ge\u00b7brauch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VAFIN", "VVPP", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ach m\u00f6gten wir denn doch, wenn wir dich sehn und essen,", "tokens": ["Ach", "m\u00f6g\u00b7ten", "wir", "denn", "doch", ",", "wenn", "wir", "dich", "sehn", "und", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Beliebte Frucht, in dir des Gebers G\u00fct\u2019 ermessen", "tokens": ["Be\u00b7lieb\u00b7te", "Frucht", ",", "in", "dir", "des", "Ge\u00b7bers", "G\u00fct'", "er\u00b7mes\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und ihm in unsrer Lust zu dancken nicht vergessen!", "tokens": ["Und", "ihm", "in", "uns\u00b7rer", "Lust", "zu", "dan\u00b7cken", "nicht", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}