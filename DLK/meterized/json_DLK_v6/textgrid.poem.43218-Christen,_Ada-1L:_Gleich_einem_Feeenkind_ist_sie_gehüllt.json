{"textgrid.poem.43218": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gleich einem Feeenkind ist sie geh\u00fcllt", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich einem Feeenkind ist sie geh\u00fcllt", "tokens": ["Gleich", "ei\u00b7nem", "Fe\u00b7e\u00b7en\u00b7kind", "ist", "sie", "ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In wei\u00dfer Spitzen kostbar-leichte Flocken,", "tokens": ["In", "wei\u00b7\u00dfer", "Spit\u00b7zen", "kost\u00b7ba\u00b7rleich\u00b7te", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Diamanten strahlen Arm und Locken,", "tokens": ["Von", "Di\u00b7a\u00b7man\u00b7ten", "strah\u00b7len", "Arm", "und", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Wesen ist von banger Scheu erf\u00fcllt. \u2013", "tokens": ["Ihr", "We\u00b7sen", "ist", "von", "ban\u00b7ger", "Scheu", "er\u00b7f\u00fcllt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie schaut so \u00e4ngstlich, zerrt an ihrem Kleid", "tokens": ["Sie", "schaut", "so", "\u00e4ngst\u00b7lich", ",", "zerrt", "an", "ih\u00b7rem", "Kleid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und singt das Herz Dir krank im jungen Leibe,", "tokens": ["Und", "singt", "das", "Herz", "Dir", "krank", "im", "jun\u00b7gen", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein D\u00e4mon, w\u00e4hnst Du, singt aus diesem Weibe,", "tokens": ["Ein", "D\u00e4\u00b7mon", ",", "w\u00e4hnst", "Du", ",", "singt", "aus", "die\u00b7sem", "Wei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "$,", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein D\u00e4mon oder wahres Seelenleid \u2013 \u2013 \u2013", "tokens": ["Ein", "D\u00e4\u00b7mon", "o\u00b7der", "wah\u00b7res", "See\u00b7len\u00b7leid", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.10": {"text": "Wenn sie die gro\u00dfen, dunklen Augen schlie\u00dft,", "tokens": ["Wenn", "sie", "die", "gro\u00b7\u00dfen", ",", "dunk\u00b7len", "Au\u00b7gen", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Von ihren Lippen matt die T\u00f6ne beben,", "tokens": ["Von", "ih\u00b7ren", "Lip\u00b7pen", "matt", "die", "T\u00f6\u00b7ne", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Allm\u00e4hlig schwellend ihrer Brust entschweben,", "tokens": ["All\u00b7m\u00e4h\u00b7lig", "schwel\u00b7lend", "ih\u00b7rer", "Brust", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn sie das grelle Lampenlicht vergi\u00dft,", "tokens": ["Wenn", "sie", "das", "grel\u00b7le", "Lam\u00b7pen\u00b7licht", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wenn sie aufjubelt wie die Nachtigall,", "tokens": ["Wenn", "sie", "auf\u00b7ju\u00b7belt", "wie", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wenn Harfenkl\u00e4nge wehen durch ihr Singen,", "tokens": ["Wenn", "Har\u00b7fen\u00b7kl\u00e4n\u00b7ge", "we\u00b7hen", "durch", "ihr", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wenn schmerzdurchgl\u00fcht sich aus der Seele ringen", "tokens": ["Wenn", "schmerz\u00b7durch\u00b7gl\u00fcht", "sich", "aus", "der", "See\u00b7le", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Die scharfen Laute einer wilden Qual \u2013", "tokens": ["Die", "schar\u00b7fen", "Lau\u00b7te", "ei\u00b7ner", "wil\u00b7den", "Qual", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und wenn sie dann, wie aus dem Traum erwacht,", "tokens": ["Und", "wenn", "sie", "dann", ",", "wie", "aus", "dem", "Traum", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Erstaunt und langsam aufschl\u00e4gt ihre Augen,", "tokens": ["Er\u00b7staunt", "und", "lang\u00b7sam", "auf\u00b7schl\u00e4gt", "ih\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die Blicke sich an eine Stelle saugen,", "tokens": ["Die", "Bli\u00b7cke", "sich", "an", "ei\u00b7ne", "Stel\u00b7le", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wenn sie aufathmet, wenn sie kindlich lacht,", "tokens": ["Wenn", "sie", "auf\u00b7ath\u00b7met", ",", "wenn", "sie", "kind\u00b7lich", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wenn ihre H\u00e4nde, zagend und verwirrt,", "tokens": ["Wenn", "ih\u00b7re", "H\u00e4n\u00b7de", ",", "za\u00b7gend", "und", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Von einem Kranze zu dem andern langen,", "tokens": ["Von", "ei\u00b7nem", "Kran\u00b7ze", "zu", "dem", "an\u00b7dern", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und wenn sie endlich zitternd und befangen", "tokens": ["Und", "wenn", "sie", "end\u00b7lich", "zit\u00b7ternd", "und", "be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Mit einer Rose schlicht ihr Mieder ziert,", "tokens": ["Mit", "ei\u00b7ner", "Ro\u00b7se", "schlicht", "ihr", "Mie\u00b7der", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Wenn sie sich neigt gleich einem Heiligenbild,", "tokens": ["Wenn", "sie", "sich", "neigt", "gleich", "ei\u00b7nem", "Hei\u00b7li\u00b7gen\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "Gesenkten Hauptes, mit dem\u00fcth'gem Lauschen,", "tokens": ["Ge\u00b7senk\u00b7ten", "Haup\u00b7tes", ",", "mit", "de\u00b7m\u00fcth'\u00b7gem", "Lau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Die Beifallsfluthen l\u00e4\u00dft vor\u00fcberrauschen,", "tokens": ["Die", "Bei\u00b7falls\u00b7flut\u00b7hen", "l\u00e4\u00dft", "vor\u00b7\u00fc\u00b7berr\u00b7au\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Dann kannst Du glauben, da\u00df sie \u2013 gut ", "tokens": ["Dann", "kannst", "Du", "glau\u00b7ben", ",", "da\u00df", "sie", "\u2013", "gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "$(", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}