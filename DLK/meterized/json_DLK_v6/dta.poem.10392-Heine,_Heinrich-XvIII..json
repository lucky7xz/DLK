{"dta.poem.10392": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich grolle nicht, und wenn das Herz auch bricht,", "tokens": ["Ich", "grol\u00b7le", "nicht", ",", "und", "wenn", "das", "Herz", "auch", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ewig verlor'nes Lieb! ich grolle nicht.", "tokens": ["E\u00b7wig", "ver\u00b7lor'\u00b7nes", "Lieb", "!", "ich", "grol\u00b7le", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie du auch strahlst in Diamantenpracht,", "tokens": ["Wie", "du", "auch", "strahlst", "in", "Di\u00b7a\u00b7man\u00b7ten\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es f\u00e4llt kein Strahl in deines Herzens Nacht.", "tokens": ["Es", "f\u00e4llt", "kein", "Strahl", "in", "dei\u00b7nes", "Her\u00b7zens", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das wei\u00df ich l\u00e4ngst. Ich sah dich ja im Traume,", "tokens": ["Das", "wei\u00df", "ich", "l\u00e4ngst", ".", "Ich", "sah", "dich", "ja", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sah die Nacht in deines Herzens Raume,", "tokens": ["Und", "sah", "die", "Nacht", "in", "dei\u00b7nes", "Her\u00b7zens", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sah die Schlang', die dir am Herzen fri\u00dft,", "tokens": ["Und", "sah", "die", "Schlang'", ",", "die", "dir", "am", "Her\u00b7zen", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah mein Lieb, wie sehr du elend bist.", "tokens": ["Ich", "sah", "mein", "Lieb", ",", "wie", "sehr", "du", "e\u00b7lend", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}