{"textgrid.poem.65229": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Demagoge", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch, ihr edlen deutschen Reben,", "tokens": ["Euch", ",", "ihr", "ed\u00b7len", "deut\u00b7schen", "Re\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei mein Lied geweiht!", "tokens": ["Sei", "mein", "Lied", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sing' ein Andrer von den Helden", "tokens": ["Sing'", "ein", "A\u00b7ndrer", "von", "den", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser lieben Zeit.", "tokens": ["Die\u00b7ser", "lie\u00b7ben", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Fehlen mir auf ihre Namen", "tokens": ["Feh\u00b7len", "mir", "auf", "ih\u00b7re", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reime zum Gedicht,", "tokens": ["Rei\u00b7me", "zum", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und zum Ungereimten brauchen", "tokens": ["Und", "zum", "Un\u00b7ge\u00b7reim\u00b7ten", "brau\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie den Dichter nicht.", "tokens": ["Sie", "den", "Dich\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hab' mich in dem Geist der Zeiten", "tokens": ["Hab'", "mich", "in", "dem", "Geist", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch einmal berauscht;", "tokens": ["Auch", "ein\u00b7mal", "be\u00b7rauscht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hab' den Rausch nun ausgeschlafen", "tokens": ["Hab'", "den", "Rausch", "nun", "aus\u00b7ge\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Trank vertauscht.", "tokens": ["Und", "den", "Trank", "ver\u00b7tauscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Deutsch und frei und stark und lauter", "tokens": ["Deutsch", "und", "frei", "und", "stark", "und", "lau\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem deutschen Land", "tokens": ["In", "dem", "deut\u00b7schen", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ist der Wein allein geblieben", "tokens": ["Ist", "der", "Wein", "al\u00b7lein", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Rheines Strand.", "tokens": ["An", "des", "Rhei\u00b7nes", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und er l\u00e4\u00dft die deutsche Tugend,", "tokens": ["Und", "er", "l\u00e4\u00dft", "die", "deut\u00b7sche", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft den deutschen Muth", "tokens": ["L\u00e4\u00dft", "den", "deut\u00b7schen", "Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Frank und frei im Glase sprudeln,", "tokens": ["Frank", "und", "frei", "im", "Gla\u00b7se", "spru\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und man hei\u00dft es gut.", "tokens": ["Und", "man", "hei\u00dft", "es", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Und er zieht durch Deutschlands Gauen,", "tokens": ["Und", "er", "zieht", "durch", "Deutschlands", "Gau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Predigt deutschen Geist,", "tokens": ["Pre\u00b7digt", "deut\u00b7schen", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn durch froher M\u00e4nner Runde", "tokens": ["Wenn", "durch", "fro\u00b7her", "M\u00e4n\u00b7ner", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er im Becher kreist.", "tokens": ["Er", "im", "Be\u00b7cher", "kreist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Landsmann! gr\u00fc\u00dft ihn mit Entz\u00fccken", "tokens": ["Lands\u00b7mann", "!", "gr\u00fc\u00dft", "ihn", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder deutsche Mund,", "tokens": ["Je\u00b7der", "deut\u00b7sche", "Mund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und er h\u00e4lt in alter Treue", "tokens": ["Und", "er", "h\u00e4lt", "in", "al\u00b7ter", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen deutschen Bund.", "tokens": ["Sei\u00b7nen", "deut\u00b7schen", "Bund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Fr\u00e4gt nicht nach der Herren Wechsel,", "tokens": ["Fr\u00e4gt", "nicht", "nach", "der", "Her\u00b7ren", "Wech\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der Seelen Tausch,", "tokens": ["Nach", "der", "See\u00b7len", "Tausch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kennt nur ", "tokens": ["Kennt", "nur"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Wer soll einer sein?", "tokens": ["Wer", "soll", "ei\u00b7ner", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mainz, du heil'ge Bundesfeste,", "tokens": ["Mainz", ",", "du", "heil'\u00b7ge", "Bun\u00b7des\u00b7fes\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sperr' ihn nur nicht ein!", "tokens": ["Sperr'", "ihn", "nur", "nicht", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}