{"dta.poem.3930": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Der dritte  Titul  soll von baar verleiten Sachen,  \n was da zu halten sey, ein kurtz Gesetze machen.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Das junge Musen-Volck in dem ber\u00fchmte\u0303 Leiden,", "tokens": ["Das", "jun\u00b7ge", "Mu\u00b7sen\u00b7Volck", "in", "dem", "be\u00b7r\u00fchm\u00b7t\u1ebd", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist bey der Schlittenfahrt vorjetzo voller Freuden,", "tokens": ["Ist", "bey", "der", "Schlit\u00b7ten\u00b7fahrt", "vor\u00b7jet\u00b7zo", "vol\u00b7ler", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Spricht der Professor gleich: ", "tokens": ["Spricht", "der", "Pro\u00b7fes\u00b7sor", "gleich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "So \u00fcberhebt man ihn doch aller seiner M\u00fch.", "tokens": ["So", "\u00fc\u00b7ber\u00b7hebt", "man", "ihn", "doch", "al\u00b7ler", "sei\u00b7ner", "M\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Schlitten gehen vor; Die Herren Stall-", "tokens": ["Die", "Schlit\u00b7ten", "ge\u00b7hen", "vor", ";", "Die", "Her\u00b7ren", "Stall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Philister", "tokens": ["Phi\u00b7lis\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Sind mehr als Ph\u00f6bus werth, und alle Pallas-", "tokens": ["Sind", "mehr", "als", "Ph\u00f6\u00b7bus", "werth", ",", "und", "al\u00b7le", "Pal\u00b7las"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "KOUS", "NN", "ADJD", "$,", "KON", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Priester.", "tokens": ["Pries\u00b7ter", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und weil man auch dabey auf Zucht und Ordnung", "tokens": ["Und", "weil", "man", "auch", "da\u00b7bey", "auf", "Zucht", "und", "Ord\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADV", "PAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "h\u00e4lt,", "tokens": ["h\u00e4lt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "So hat mn folgendes zur Richtschnur vorgestellt:", "tokens": ["So", "hat", "mn", "fol\u00b7gen\u00b7des", "zur", "Richt\u00b7schnur", "vor\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn nur ein Bi\u00dfgen Schnee der Himmel fal-", "tokens": ["Wenn", "nur", "ein", "Bi\u00df\u00b7gen", "Schnee", "der", "Him\u00b7mel", "fa\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "len lassen,", "tokens": ["len", "las\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Und \u00fcberdeckte kaum das Pflaster auf den Gassen,", "tokens": ["Und", "\u00fc\u00b7berd\u00b7eck\u00b7te", "kaum", "das", "Pflas\u00b7ter", "auf", "den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So fahret immer fort; wenn aus den Steinen gleich", "tokens": ["So", "fah\u00b7ret", "im\u00b7mer", "fort", ";", "wenn", "aus", "den", "Stei\u00b7nen", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$.", "KOUS", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das Feuer h\u00e4ufig springt. Ist doch die Lust vor euch.", "tokens": ["Das", "Feu\u00b7er", "h\u00e4u\u00b7fig", "springt", ".", "Ist", "doch", "die", "Lust", "vor", "euch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$.", "VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sucht sch\u00f6ne Schlitten aus, mit Schnitzwerck aus-", "tokens": ["Sucht", "sch\u00f6\u00b7ne", "Schlit\u00b7ten", "aus", ",", "mit", "Schnitz\u00b7werck", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "gezieret,", "tokens": ["ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Mit Silber und mit Gold gemahlet und beschmieret,", "tokens": ["Mit", "Sil\u00b7ber", "und", "mit", "Gold", "ge\u00b7mah\u00b7let", "und", "be\u00b7schmie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Damit es F\u00fcrstlich l\u00e4st, wenn gleich das Schlitten-", "tokens": ["Da\u00b7mit", "es", "F\u00fcrst\u00b7lich", "l\u00e4st", ",", "wenn", "gleich", "das", "Schlit\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "KOUS", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Pferd", "tokens": ["Pferd"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Als wie ein Wind-Hund sieht, und kaum das Fut-", "tokens": ["Als", "wie", "ein", "Win\u00b7dHund", "sieht", ",", "und", "kaum", "das", "Fut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$,", "KON", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "ter werth.", "tokens": ["ter", "werth", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "Vergest die Schellen nicht; Dieselben an der M\u00fctze,", "tokens": ["Ver\u00b7gest", "die", "Schel\u00b7len", "nicht", ";", "Die\u00b7sel\u00b7ben", "an", "der", "M\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$.", "PDAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die manchen erblich sind, sind dieses mahl nichts", "tokens": ["Die", "man\u00b7chen", "er\u00b7blich", "sind", ",", "sind", "die\u00b7ses", "mahl", "nichts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJD", "VAFIN", "$,", "VAFIN", "PDAT", "ADV", "PIS"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.26": {"text": "n\u00fctze.", "tokens": ["n\u00fct\u00b7ze", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Sonst weicht euch niemand aus. Die Lust wird", "tokens": ["Sonst", "weicht", "euch", "nie\u00b7mand", "aus", ".", "Die", "Lust", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "auch vermehrt,", "tokens": ["auch", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Wenn man sein Schlitten-Geld am Pferde klin-", "tokens": ["Wenn", "man", "sein", "Schlit\u00b7ten\u00b7Geld", "am", "Pfer\u00b7de", "klin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "gen h\u00f6rt.", "tokens": ["gen", "h\u00f6rt", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.31": {"text": "Fahrt lieber in der Stadt als drausse\u0303 auf dem Lande,", "tokens": ["Fahrt", "lie\u00b7ber", "in", "der", "Stadt", "als", "draus\u00b7s\u1ebd", "auf", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "KOKOM", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Wen\u0303 da die Bahne gleich nicht in so guten Stande,", "tokens": ["We\u00f1", "da", "die", "Bah\u00b7ne", "gleich", "nicht", "in", "so", "gu\u00b7ten", "Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "PTKNEG", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So prahlts doch desto mehr. Denn w\u00fcrcklich jedes", "tokens": ["So", "prahlts", "doch", "des\u00b7to", "mehr", ".", "Denn", "w\u00fcrck\u00b7lich", "je\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "$.", "KON", "ADJD", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Hau\u00df", "tokens": ["Hau\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.35": {"text": "St\u00f6st, wenn ein Schlitten k\u00f6mmt, fast alle Schei-", "tokens": ["St\u00f6st", ",", "wenn", "ein", "Schlit\u00b7ten", "k\u00f6mmt", ",", "fast", "al\u00b7le", "Schei"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "ben raus.", "tokens": ["ben", "raus", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.37": {"text": "Wer nun ein M\u00e4gdgen liebt, und wei\u00df nichts aus-", "tokens": ["Wer", "nun", "ein", "M\u00e4gd\u00b7gen", "liebt", ",", "und", "wei\u00df", "nichts", "aus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "PIS", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "zusinnen,", "tokens": ["zu\u00b7sin\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "Wie er sich zeigen soll, der kan sehr viel gewinnen,", "tokens": ["Wie", "er", "sich", "zei\u00b7gen", "soll", ",", "der", "kan", "sehr", "viel", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,", "PRELS", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wen\u0303 er nett angeputzt, nebst seinem Schellen-Pferd", "tokens": ["We\u00f1", "er", "nett", "an\u00b7ge\u00b7putzt", ",", "nebst", "sei\u00b7nem", "Schel\u00b7len\u00b7Pferd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Vor der Geliebten Hau\u00df fein offt vor\u00fcber f\u00e4hrt.", "tokens": ["Vor", "der", "Ge\u00b7lieb\u00b7ten", "Hau\u00df", "fein", "offt", "vor\u00b7\u00fc\u00b7ber", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das mu\u00df am Tage seyn; Wenn schon das Licht", "tokens": ["Das", "mu\u00df", "am", "Ta\u00b7ge", "seyn", ";", "Wenn", "schon", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "VAINF", "$.", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "vergangen,", "tokens": ["ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.44": {"text": "So kan man nicht gar viel vor seine Kosten prangen,", "tokens": ["So", "kan", "man", "nicht", "gar", "viel", "vor", "sei\u00b7ne", "Kos\u00b7ten", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PTKNEG", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Man h\u00e4tte den\u0303 etwan ein M\u00e4gdgen statt der Fracht,", "tokens": ["Man", "h\u00e4t\u00b7te", "de\u00f1", "et\u00b7wan", "ein", "M\u00e4gd\u00b7gen", "statt", "der", "Fracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Das schickt sich allerdings am besten in der Nacht.", "tokens": ["Das", "schickt", "sich", "al\u00b7ler\u00b7dings", "am", "bes\u00b7ten", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "PTKA", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wer Frauenzim\u0303er f\u00e4hrt, mu\u00df sich auf viel befleissen,", "tokens": ["Wer", "Frau\u00b7en\u00b7zim\u0303er", "f\u00e4hrt", ",", "mu\u00df", "sich", "auf", "viel", "be\u00b7fleis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VMFIN", "PRF", "APPR", "ADV", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.48": {"text": "Und ihr das Lenck-Seil nicht an ihre Backen", "tokens": ["Und", "ihr", "das", "Len\u00b7ck\u00b7Seil", "nicht", "an", "ih\u00b7re", "Ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.49": {"text": "schmeissen.", "tokens": ["schmeis\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.50": {"text": "Er werffe sie nicht um, weil das verd\u00e4chtig klingt,", "tokens": ["Er", "werf\u00b7fe", "sie", "nicht", "um", ",", "weil", "das", "ver\u00b7d\u00e4ch\u00b7tig", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "KOUS", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Wenn eine Man\u0303s Person ein Weib zu Falle bringt.", "tokens": ["Wenn", "ei\u00b7ne", "Ma\u00f1s", "Per\u00b7son", "ein", "Weib", "zu", "Fal\u00b7le", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Schafft gute Peitschen an, die gantz entsetzlich knalle\u0303,", "tokens": ["Schafft", "gu\u00b7te", "Peit\u00b7schen", "an", ",", "die", "gantz", "ent\u00b7setz\u00b7lich", "knall\u1ebd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Da\u00df selbst der m\u00fcde Gaul zu Boden m\u00f6chte fallen,", "tokens": ["Da\u00df", "selbst", "der", "m\u00fc\u00b7de", "Gaul", "zu", "Bo\u00b7den", "m\u00f6ch\u00b7te", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Es l\u00e4st studen \u03c4\u03b9\u03ba\u1ff6\u03c2; wenn gleich die Einfalt spricht:", "tokens": ["Es", "l\u00e4st", "stu\u00b7den", "\u03c4\u03b9\u03ba\u1ff6\u03c2", ";", "wenn", "gleich", "die", "Ein\u00b7falt", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$.", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.55": {"text": "Vor Kutscher st\u00fcnd es wohl, doch vor Studenten", "tokens": ["Vor", "Kut\u00b7scher", "st\u00fcnd", "es", "wohl", ",", "doch", "vor", "Stu\u00b7den\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.56": {"text": "nicht.", "tokens": ["nicht", "."], "token_info": ["word", "punct"], "pos": ["PTKNEG", 