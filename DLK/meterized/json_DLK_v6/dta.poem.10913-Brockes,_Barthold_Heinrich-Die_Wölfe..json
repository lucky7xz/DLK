{"dta.poem.10913": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die W\u00f6lfe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welchen Wild und \u00f6den Ort, welche grause W\u00fcsteney,", "tokens": ["Wel\u00b7chen", "Wild", "und", "\u00f6\u00b7den", "Ort", ",", "wel\u00b7che", "grau\u00b7se", "W\u00fcs\u00b7te\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "ADJA", "NN", "$,", "PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "F\u00fcllt hier ein nicht minder wild - und entsetzliches Ge-", "tokens": ["F\u00fcllt", "hier", "ein", "nicht", "min\u00b7der", "wild", "und", "ent\u00b7setz\u00b7li\u00b7ches", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "PTKNEG", "ADV", "ADJD", "$(", "KON", "ADJA", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ein abscheuliches Geheul! welch ein grimmig Klagget\u00f6n", "tokens": ["Ein", "ab\u00b7scheu\u00b7li\u00b7ches", "Ge\u00b7heul", "!", "welch", "ein", "grim\u00b7mig", "Klag\u00b7ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PWAT", "ART", "ADJD", "NN"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Kann man, in f\u00fcnf offnen Rachen Fleisch-begierger W\u00f6lfe, sehn!", "tokens": ["Kann", "man", ",", "in", "f\u00fcnf", "off\u00b7nen", "Ra\u00b7chen", "Fleischbe\u00b7gier\u00b7ger", "W\u00f6l\u00b7fe", ",", "sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "APPR", "CARD", "ADJA", "NN", "NN", "NN", "$,", "VVINF", "$."], "meter": "----+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ja mich deucht, ich sehe gar, an verschiednen schroffen Stellen", "tokens": ["Ja", "mich", "deucht", ",", "ich", "se\u00b7he", "gar", ",", "an", "ver\u00b7schied\u00b7nen", "schrof\u00b7fen", "Stel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,", "APPR", "ADJA", "ADJA", "NN"], "meter": "-++-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Dieser Klippen - reichen Gegend, selbst die Felsen wiederbellen.", "tokens": ["Die\u00b7ser", "Klip\u00b7pen", "rei\u00b7chen", "Ge\u00b7gend", ",", "selbst", "die", "Fel\u00b7sen", "wie\u00b7der\u00b7bel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "ADJA", "NN", "$,", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "So nat\u00fcrlich ist die Stellung, so lebendig die Figur", "tokens": ["So", "na\u00b7t\u00fcr\u00b7lich", "ist", "die", "Stel\u00b7lung", ",", "so", "le\u00b7ben\u00b7dig", "die", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Dieser Thiere vorgestellt, und die felsigte Natur,", "tokens": ["Die\u00b7ser", "Thie\u00b7re", "vor\u00b7ge\u00b7stellt", ",", "und", "die", "fel\u00b7sig\u00b7te", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+---+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Da\u00df man fast ein Echo h\u00f6rt; man vermeynt hier, nicht die", "tokens": ["Da\u00df", "man", "fast", "ein", "E\u00b7cho", "h\u00f6rt", ";", "man", "ver\u00b7meynt", "hier", ",", "nicht", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$.", "PIS", "VVFIN", "ADV", "$,", "PTKNEG", "ART"], "meter": "--+-+-+--+-++", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Nur zu h\u00f6ren und zu sehn; man sieht nicht nur Wuth und", "tokens": ["Nur", "zu", "h\u00f6\u00b7ren", "und", "zu", "sehn", ";", "man", "sieht", "nicht", "nur", "Wuth", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$.", "PIS", "VVFIN", "PTKNEG", "ADV", "NN", "KON"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "In den t\u00fcckisch schielen Augen, welches ein paar Strich uns", "tokens": ["In", "den", "t\u00fc\u00b7ckisch", "schie\u00b7len", "Au\u00b7gen", ",", "wel\u00b7ches", "ein", "paar", "Strich", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,", "PRELS", "ART", "PIAT", "NN", "PPER"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Man sieht gar, in schwarzen K\u00f6rpern, selbst der W\u00f6lfe schwar-", "tokens": ["Man", "sieht", "gar", ",", "in", "schwar\u00b7zen", "K\u00f6r\u00b7pern", ",", "selbst", "der", "W\u00f6l\u00b7fe", "schwa\u00b7r"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "ART", "NN", "TRUNC"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Machet die\u00df denn nicht aufs neue, wie der menschliche Verstand", "tokens": ["Ma\u00b7chet", "die\u00df", "denn", "nicht", "aufs", "neu\u00b7e", ",", "wie", "der", "menschli\u00b7che", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "ADV", "PTKNEG", "APPRART", "ADJA", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.14": {"text": "Fast zu Wundern f\u00e4hig sey; unserm Gott zum Ruhm bekannt?", "tokens": ["Fast", "zu", "Wun\u00b7dern", "f\u00e4\u00b7hig", "sey", ";", "un\u00b7serm", "Gott", "zum", "Ruhm", "be\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "VAFIN", "$.", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}