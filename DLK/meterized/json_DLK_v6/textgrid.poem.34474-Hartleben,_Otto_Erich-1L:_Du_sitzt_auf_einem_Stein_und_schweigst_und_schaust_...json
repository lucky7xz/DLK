{"textgrid.poem.34474": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du sitzt auf einem Stein und schweigst und schaust ..", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sitzt auf einem Stein und schweigst und schaust ..", "tokens": ["Du", "sitzt", "auf", "ei\u00b7nem", "Stein", "und", "schweigst", "und", "schaust", ".."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gewitterleuchtend zog die Nacht herauf.", "tokens": ["Ge\u00b7wit\u00b7ter\u00b7leuch\u00b7tend", "zog", "die", "Nacht", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Ebne, die vom Fusse des Gebirges", "tokens": ["Die", "Eb\u00b7ne", ",", "die", "vom", "Fus\u00b7se", "des", "Ge\u00b7bir\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sich hinstreckt in den schwarzen Horizont,", "tokens": ["sich", "hins\u00b7treckt", "in", "den", "schwar\u00b7zen", "Ho\u00b7ri\u00b7zont", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "sie lechzt und d\u00fcrstet. Mit verhaltnem Athem", "tokens": ["sie", "lechzt", "und", "d\u00fcrs\u00b7tet", ".", "Mit", "ver\u00b7halt\u00b7nem", "A\u00b7them"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "harrt sie des Wetters und der Regenfluthen.", "tokens": ["harrt", "sie", "des", "Wet\u00b7ters", "und", "der", "Re\u00b7gen\u00b7flu\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Dort links, fern im Gefilde, wallumringt,", "tokens": ["Dort", "links", ",", "fern", "im", "Ge\u00b7fil\u00b7de", ",", "wal\u00b7lum\u00b7ringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJD", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ragt eine Stadt mit steilem Thurm und D\u00e4chern.", "tokens": ["ragt", "ei\u00b7ne", "Stadt", "mit", "stei\u00b7lem", "Thurm", "und", "D\u00e4\u00b7chern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sieh: herniederz\u00fcngelnd in den Thurm", "tokens": ["Und", "sieh", ":", "her\u00b7nie\u00b7der\u00b7z\u00fcn\u00b7gelnd", "in", "den", "Thurm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "einbiss der Blitz. Da springt die rothe Gluth", "tokens": ["ein\u00b7biss", "der", "Blitz", ".", "Da", "springt", "die", "ro\u00b7the", "Gluth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "des Feuers jauchzend auf und greift im Tanz", "tokens": ["des", "Feu\u00b7ers", "jauch\u00b7zend", "auf", "und", "greift", "im", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "berauscht umher und fasst die spitzen Giebel,", "tokens": ["be\u00b7rauscht", "um\u00b7her", "und", "fasst", "die", "spit\u00b7zen", "Gie\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und d\u00fcster leuchtets von den Wolken wieder.", "tokens": ["und", "d\u00fcs\u00b7ter", "leuch\u00b7tets", "von", "den", "Wol\u00b7ken", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u2013 Du wendest deine heissen Augen ab.", "tokens": ["\u2013", "Du", "wen\u00b7dest", "dei\u00b7ne", "heis\u00b7sen", "Au\u00b7gen", "ab", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da siehst du rechts, im Berge einen Spalt,", "tokens": ["Da", "siehst", "du", "rechts", ",", "im", "Ber\u00b7ge", "ei\u00b7nen", "Spalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ein g\u00e4hnend Loch, schwarz, tief und ohne Ende.", "tokens": ["ein", "g\u00e4h\u00b7nend", "Loch", ",", "schwarz", ",", "tief", "und", "oh\u00b7ne", "En\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus rohen Bl\u00f6cken ist ein Tisch gebildet", "tokens": ["Aus", "ro\u00b7hen", "Bl\u00f6\u00b7cken", "ist", "ein", "Tisch", "ge\u00b7bil\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "ein Grubenlicht mit gr\u00fcnlich mattem Schein", "tokens": ["ein", "Gru\u00b7ben\u00b7licht", "mit", "gr\u00fcn\u00b7lich", "mat\u00b7tem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "h\u00e4ngt schwelend von dem feuchten Felsen nieder:", "tokens": ["h\u00e4ngt", "schwe\u00b7lend", "von", "dem", "feuch\u00b7ten", "Fel\u00b7sen", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und vor dem Tisch \u2013 auf einem Stein, wie du \u2013", "tokens": ["und", "vor", "dem", "Tisch", "\u2013", "auf", "ei\u00b7nem", "Stein", ",", "wie", "du", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "sitzt ein Skelett mit gelben, nackten Knochen,", "tokens": ["sitzt", "ein", "Ske\u00b7lett", "mit", "gel\u00b7ben", ",", "nack\u00b7ten", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "und nur im Schoosse noch ein modernd Fleisch.", "tokens": ["und", "nur", "im", "Schoos\u00b7se", "noch", "ein", "mo\u00b7dernd", "Fleisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nach vorn gebeugt, liest es in einem Buche,", "tokens": ["Nach", "vorn", "ge\u00b7beugt", ",", "liest", "es", "in", "ei\u00b7nem", "Bu\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "und langsam wendets mit den Fingerknochen", "tokens": ["und", "lang\u00b7sam", "wen\u00b7dets", "mit", "den", "Fin\u00b7ger\u00b7kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "die Seiten um. Schamlose Lasterbilder", "tokens": ["die", "Sei\u00b7ten", "um", ".", "Scham\u00b7lo\u00b7se", "Las\u00b7ter\u00b7bil\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "bedecken, halbverbleicht, die braunen Bl\u00e4tter.", "tokens": ["be\u00b7de\u00b7cken", ",", "halb\u00b7ver\u00b7bleicht", ",", "die", "brau\u00b7nen", "Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und manchmal h\u00e4lt es an \u2013 klappt mit den Knien \u2013", "tokens": ["Und", "manch\u00b7mal", "h\u00e4lt", "es", "an", "\u2013", "klappt", "mit", "den", "Kni\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$(", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "und knirscht und knackt mit lippenlosen Kiefern.", "tokens": ["und", "knirscht", "und", "knackt", "mit", "lip\u00b7pen\u00b7lo\u00b7sen", "Kie\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein M\u00e4dchenleib, gesch\u00e4ndet und gemordet,", "tokens": ["Ein", "M\u00e4d\u00b7chen\u00b7leib", ",", "ge\u00b7sch\u00e4n\u00b7det", "und", "ge\u00b7mor\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "liegt nackt am Boden, neben dem Gerippe.", "tokens": ["liegt", "nackt", "am", "Bo\u00b7den", ",", "ne\u00b7ben", "dem", "Ge\u00b7rip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Unsicher nur beleuchtet ihn der Schein", "tokens": ["Un\u00b7si\u00b7cher", "nur", "be\u00b7leuch\u00b7tet", "ihn", "der", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "der Grubenlampe, aber deutlich zittert", "tokens": ["der", "Gru\u00b7ben\u00b7lam\u00b7pe", ",", "a\u00b7ber", "deut\u00b7lich", "zit\u00b7tert"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "des alten Crucifixes schr\u00e4ger Schatten", "tokens": ["des", "al\u00b7ten", "Cru\u00b7ci\u00b7fi\u00b7xes", "schr\u00e4\u00b7ger", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "auf ihren Br\u00fcsten. \u2013", "tokens": ["auf", "ih\u00b7ren", "Br\u00fcs\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Eine dicke Kr\u00f6te", "tokens": ["Ei\u00b7ne", "di\u00b7cke", "Kr\u00f6\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "klimmt aus dem Abgrund auf und kriecht gemach", "tokens": ["klimmt", "aus", "dem", "Ab\u00b7grund", "auf", "und", "kriecht", "ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "KON", "VVFIN", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "nach vorn. Jetzt schl\u00fcpft sie auf die M\u00e4dchenleiche,", "tokens": ["nach", "vorn", ".", "Jetzt", "schl\u00fcpft", "sie", "auf", "die", "M\u00e4d\u00b7chen\u00b7lei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "das Bein hinauf mit schleimig rother Spur.", "tokens": ["das", "Bein", "hin\u00b7auf", "mit", "schlei\u00b7mig", "ro\u00b7ther", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sie setzt auf ihrer H\u00fcfte sich zurecht", "tokens": ["Sie", "setzt", "auf", "ih\u00b7rer", "H\u00fcf\u00b7te", "sich", "zu\u00b7recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PRF", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "und glotzt mit ihren grossen, runden Augen", "tokens": ["und", "glotzt", "mit", "ih\u00b7ren", "gros\u00b7sen", ",", "run\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "in deine Augen her ...", "tokens": ["in", "dei\u00b7ne", "Au\u00b7gen", "her", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Da peitscht der Sturm", "tokens": ["Da", "peitscht", "der", "Sturm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "gl\u00fchend vorbei. Nur eine Fackel noch,", "tokens": ["gl\u00fc\u00b7hend", "vor\u00b7bei", ".", "Nur", "ei\u00b7ne", "Fa\u00b7ckel", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "leuchtet und raucht die Stadt von der Ebne her.", "tokens": ["leuch\u00b7tet", "und", "raucht", "die", "Stadt", "von", "der", "Eb\u00b7ne", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "Vorw\u00e4rts w\u00fcthet der Sturm, und die dorrenden Gr\u00e4ser", "tokens": ["Vor\u00b7w\u00e4rts", "w\u00fct\u00b7het", "der", "Sturm", ",", "und", "die", "dor\u00b7ren\u00b7den", "Gr\u00e4\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "flackern in seinem Hauche. Vorw\u00e4rts, vorw\u00e4rts,", "tokens": ["fla\u00b7ckern", "in", "sei\u00b7nem", "Hau\u00b7che", ".", "Vor\u00b7w\u00e4rts", ",", "vor\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$.", "ADV", "$,", "ADV", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "loht es und spr\u00fcht es \u2013 ein Meer.", "tokens": ["loht", "es", "und", "spr\u00fcht", "es", "\u2013", "ein", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "$(", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Was du geschaut hast, es ertrinkt im Blut der Flammen.", "tokens": ["Was", "du", "ge\u00b7schaut", "hast", ",", "es", "er\u00b7trinkt", "im", "Blut", "der", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}