{"textgrid.poem.62566": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Wo werd ich hinger\u00fccket?", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo werd ich hinger\u00fccket?", "tokens": ["Wo", "werd", "ich", "hin\u00b7ge\u00b7r\u00fc\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohin gelange ich?", "tokens": ["Wo\u00b7hin", "ge\u00b7lan\u00b7ge", "ich", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie? ist mein Geist entz\u00fccket?", "tokens": ["Wie", "?", "ist", "mein", "Geist", "ent\u00b7z\u00fc\u00b7cket", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Jesu! seh ich dich?", "tokens": ["O", "Je\u00b7su", "!", "seh", "ich", "dich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja! ja! hier ist gewi\u00df", "tokens": ["Ja", "!", "ja", "!", "hier", "ist", "ge\u00b7wi\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PTKANT", "$.", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das sch\u00f6ne Paradie\u00df;", "tokens": ["Das", "sch\u00f6\u00b7ne", "Pa\u00b7ra\u00b7die\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hier ist der Himmels-Saal;", "tokens": ["Hier", "ist", "der", "Him\u00b7mels\u00b7Saal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Hier h\u00e4lt man Gottes Mahl.", "tokens": ["Hier", "h\u00e4lt", "man", "Got\u00b7tes", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was h\u00f6r ich dorten klingen?", "tokens": ["Was", "h\u00f6r", "ich", "dor\u00b7ten", "klin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O was vernehm ich hier!", "tokens": ["O", "was", "ver\u00b7nehm", "ich", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O unaussprechlich Singen!", "tokens": ["O", "un\u00b7aus\u00b7sprech\u00b7lich", "Sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bin ganz auser mir.", "tokens": ["Ich", "bin", "ganz", "au\u00b7ser", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O unaussprechlich Wort", "tokens": ["O", "un\u00b7aus\u00b7sprech\u00b7lich", "Wort"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vor dieser Gnaden-Pfort!", "tokens": ["Vor", "die\u00b7ser", "Gna\u00b7den\u00b7P\u00b7fort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O Freude! die kein Mann", "tokens": ["O", "Freu\u00b7de", "!", "die", "kein", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Jemahls beschreiben kan.", "tokens": ["Je\u00b7mahls", "be\u00b7schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ich bin mit Glanz erf\u00fcllet,", "tokens": ["Ich", "bin", "mit", "Glanz", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drum blick ich Himmel- auf,", "tokens": ["Drum", "blick", "ich", "Him\u00b7mel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "TRUNC", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu dem, der Thr\u00e4nen stillet;", "tokens": ["Zu", "dem", ",", "der", "Thr\u00e4\u00b7nen", "stil\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Salem steht mein Lauf.", "tokens": ["Nach", "Sa\u00b7lem", "steht", "mein", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich eile durch die Luft", "tokens": ["Ich", "ei\u00b7le", "durch", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu dem, der droben ruft.", "tokens": ["Zu", "dem", ",", "der", "dro\u00b7ben", "ruft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich lasse diese Bahn,", "tokens": ["Ich", "las\u00b7se", "die\u00b7se", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und blick den Himmel an.", "tokens": ["Und", "blick", "den", "Him\u00b7mel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum eil ich nun von hinnen,", "tokens": ["Drum", "eil", "ich", "nun", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und s\u00e4ume weiter nicht,", "tokens": ["Und", "s\u00e4u\u00b7me", "wei\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Lauf ist nach den Zinnen", "tokens": ["Mein", "Lauf", "ist", "nach", "den", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Himmels-Burg gericht.", "tokens": ["Der", "Him\u00b7mels\u00b7Burg", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm Tod und hole mich!", "tokens": ["Komm", "Tod", "und", "ho\u00b7le", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So bleib ich ewiglich", "tokens": ["So", "bleib", "ich", "e\u00b7wig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit meinem Geist und Sinn", "tokens": ["Mit", "mei\u00b7nem", "Geist", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da, wo ich jetzo bin.", "tokens": ["Da", ",", "wo", "ich", "jet\u00b7zo", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Blick! o Wunder schauen!", "tokens": ["O", "Blick", "!", "o", "Wun\u00b7der", "schau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das alle Pracht verst\u00f6hrt.", "tokens": ["Das", "al\u00b7le", "Pracht", "ver\u00b7st\u00f6hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich geh auf Salems Auen,", "tokens": ["Ich", "geh", "auf", "Sa\u00b7lems", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo sich die Lust vermehrt.", "tokens": ["Wo", "sich", "die", "Lust", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier wohnt der Himmels-F\u00fcrst,", "tokens": ["Hier", "wohnt", "der", "Him\u00b7mels\u00b7F\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nachdem mich l\u00e4ngst ged\u00fcrst:", "tokens": ["Nach\u00b7dem", "mich", "l\u00e4ngst", "ge\u00b7d\u00fcrst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich hab und halt ihn fest,", "tokens": ["Ich", "hab", "und", "halt", "ihn", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Weil er sich finden l\u00e4\u00dft.", "tokens": ["Weil", "er", "sich", "fin\u00b7den", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Freude \u00fcber Freude!", "tokens": ["O", "Freu\u00b7de", "\u00fc\u00b7ber", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die meine Seel empfindt.", "tokens": ["Die", "mei\u00b7ne", "Seel", "emp\u00b7findt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Jesu meine Weyde!", "tokens": ["O", "Je\u00b7su", "mei\u00b7ne", "Wey\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie liebst du mich dein Kind:", "tokens": ["Wie", "liebst", "du", "mich", "dein", "Kind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du zeigst mir deinen Glanz,", "tokens": ["Du", "zeigst", "mir", "dei\u00b7nen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie auch den Ehren-Kranz,", "tokens": ["Wie", "auch", "den", "Eh\u00b7ren\u00b7Kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den du mir nach der Nacht", "tokens": ["Den", "du", "mir", "nach", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Des Todes zugedacht.", "tokens": ["Des", "To\u00b7des", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Drum fall ich kniend nieder", "tokens": ["Drum", "fall", "ich", "kni\u00b7end", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor jenes Lammes Thron,", "tokens": ["Vor", "je\u00b7nes", "Lam\u00b7mes", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und singe Freuden-Lieder", "tokens": ["Und", "sin\u00b7ge", "Freu\u00b7den\u00b7Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich lobe Gottes Sohn,", "tokens": ["Ich", "lo\u00b7be", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Vater und den Geist.", "tokens": ["Den", "Va\u00b7ter", "und", "den", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Gottheit wird geprei\u00dft;", "tokens": ["Die", "Got\u00b7theit", "wird", "ge\u00b7prei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich stimme wie ich kan", "tokens": ["Ich", "stim\u00b7me", "wie", "ich", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Vor Gott das Heilig! an.", "tokens": ["Vor", "Gott", "das", "Hei\u00b7lig", "!", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "$.", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Verst\u00f6hrt nicht mein Vergn\u00fcgen,", "tokens": ["Ver\u00b7st\u00f6hrt", "nicht", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darein mich Christus setzt.", "tokens": ["Da\u00b7rein", "mich", "Chris\u00b7tus", "setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O h\u00f6chst-begl\u00fccktes Siegen!", "tokens": ["O", "h\u00f6chst\u00b7be\u00b7gl\u00fcck\u00b7tes", "Sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das mich allein erg\u00f6tzt.", "tokens": ["Das", "mich", "al\u00b7lein", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich ruf: Victoria!", "tokens": ["Ich", "ruf", ":", "Vic\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$.", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ich sing: Halleluja!", "tokens": ["Ich", "sing", ":", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ITJ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "O Freude \u00fcber Freud!", "tokens": ["O", "Freu\u00b7de", "\u00fc\u00b7ber", "Freud", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "O Himmels-Herrlichkeit!", "tokens": ["O", "Him\u00b7mels\u00b7Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}