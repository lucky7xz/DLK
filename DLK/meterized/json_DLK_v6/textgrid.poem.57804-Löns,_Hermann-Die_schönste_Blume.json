{"textgrid.poem.57804": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6nste Blume", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Blumen, ja die Blumen,", "tokens": ["Die", "Blu\u00b7men", ",", "ja", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sind so wundersch\u00f6n,", "tokens": ["Die", "sind", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aber noch sch\u00f6ner sind M\u00e4dchen,", "tokens": ["A\u00b7ber", "noch", "sch\u00f6\u00b7ner", "sind", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sch\u00f6ne M\u00e4dchen anzusehn.", "tokens": ["Sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6ne M\u00e4dchen sind reizend,", "tokens": ["Sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen", "sind", "rei\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Reizend anzusehn,", "tokens": ["Rei\u00b7zend", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aber von allen ist keine,", "tokens": ["A\u00b7ber", "von", "al\u00b7len", "ist", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VAFIN", "PIAT", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wie die eine so sch\u00f6n.", "tokens": ["Wie", "die", "ei\u00b7ne", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "ADV", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n ist sie anzusehen,", "tokens": ["Sch\u00f6n", "ist", "sie", "an\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu k\u00fcssen noch viel mehr,", "tokens": ["Zu", "k\u00fcs\u00b7sen", "noch", "viel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "D\u00fcrfte ich sie nicht k\u00fcssen,", "tokens": ["D\u00fcrf\u00b7te", "ich", "sie", "nicht", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "W\u00fcrde das Herz mir schwer.", "tokens": ["W\u00fcr\u00b7de", "das", "Herz", "mir", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Aber mein Herz ist fr\u00f6hlich,", "tokens": ["A\u00b7ber", "mein", "Herz", "ist", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Fr\u00f6hlich ist es sehr,", "tokens": ["Fr\u00f6h\u00b7lich", "ist", "es", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn ich darf sie k\u00fcssen,", "tokens": ["Denn", "ich", "darf", "sie", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "K\u00fcssen und noch viel mehr.", "tokens": ["K\u00fcs\u00b7sen", "und", "noch", "viel", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADV", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}