{"textgrid.poem.56502": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aufgestanden ist er, welcher lange schlief,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aufgestanden ist er, welcher lange schlief,", "tokens": ["Auf\u00b7ge\u00b7stan\u00b7den", "ist", "er", ",", "wel\u00b7cher", "lan\u00b7ge", "schlief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Aufgestanden unten aus Gew\u00f6lben tief.", "tokens": ["Auf\u00b7ge\u00b7stan\u00b7den", "un\u00b7ten", "aus", "Ge\u00b7w\u00f6l\u00b7ben", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "In der D\u00e4mmrung steht er, gro\u00df und unerkannt,", "tokens": ["In", "der", "D\u00e4mm\u00b7rung", "steht", "er", ",", "gro\u00df", "und", "un\u00b7er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und den Mond zerdr\u00fcckt er in der schwarzen Hand.", "tokens": ["Und", "den", "Mond", "zer\u00b7dr\u00fcckt", "er", "in", "der", "schwar\u00b7zen", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "In den Abendl\u00e4rm der St\u00e4dte f\u00e4llt es weit,", "tokens": ["In", "den", "A\u00b7ben\u00b7dl\u00e4rm", "der", "St\u00e4d\u00b7te", "f\u00e4llt", "es", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Frost und Schatten einer fremden Dunkelheit,", "tokens": ["Frost", "und", "Schat\u00b7ten", "ei\u00b7ner", "frem\u00b7den", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und der M\u00e4rkte runder Wirbel stockt zu Eis.", "tokens": ["Und", "der", "M\u00e4rk\u00b7te", "run\u00b7der", "Wir\u00b7bel", "stockt", "zu", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Es wird still. Sie sehn sich um. Und keiner wei\u00df.", "tokens": ["Es", "wird", "still", ".", "Sie", "sehn", "sich", "um", ".", "Und", "kei\u00b7ner", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PRF", "PTKVZ", "$.", "KON", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "In den Gassen fa\u00dft es ihre Schulter leicht.", "tokens": ["In", "den", "Gas\u00b7sen", "fa\u00dft", "es", "ih\u00b7re", "Schul\u00b7ter", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Eine Frage. Keine Antwort. Ein Gesicht erbleicht.", "tokens": ["Ei\u00b7ne", "Fra\u00b7ge", ".", "Kei\u00b7ne", "Ant\u00b7wort", ".", "Ein", "Ge\u00b7sicht", "er\u00b7bleicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PIAT", "NN", "$.", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "In der Ferne wimmert ein Gel\u00e4ute d\u00fcnn", "tokens": ["In", "der", "Fer\u00b7ne", "wim\u00b7mert", "ein", "Ge\u00b7l\u00e4u\u00b7te", "d\u00fcnn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und die B\u00e4rte zittern um ihr spitzes Kinn.", "tokens": ["Und", "die", "B\u00e4r\u00b7te", "zit\u00b7tern", "um", "ihr", "spit\u00b7zes", "Kinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Auf den Bergen hebt er schon zu tanzen an", "tokens": ["Auf", "den", "Ber\u00b7gen", "hebt", "er", "schon", "zu", "tan\u00b7zen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und er schreit: Ihr Krieger alle, auf und an.", "tokens": ["Und", "er", "schreit", ":", "Ihr", "Krie\u00b7ger", "al\u00b7le", ",", "auf", "und", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPOSAT", "NN", "PIS", "$,", "APPR", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und es schallet, wenn das schwarze Haupt er schwenkt,", "tokens": ["Und", "es", "schal\u00b7let", ",", "wenn", "das", "schwar\u00b7ze", "Haupt", "er", "schwenkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Drum von tausend Sch\u00e4deln laute Kette h\u00e4ngt.", "tokens": ["Drum", "von", "tau\u00b7send", "Sch\u00e4\u00b7deln", "lau\u00b7te", "Ket\u00b7te", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "CARD", "NN", "VVFIN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Einem Turm gleich tritt er aus die letzte Glut,", "tokens": ["Ei\u00b7nem", "Turm", "gleich", "tritt", "er", "aus", "die", "letz\u00b7te", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wo der Tag flieht, sind die Str\u00f6me schon voll Blut.", "tokens": ["Wo", "der", "Tag", "flieht", ",", "sind", "die", "Str\u00f6\u00b7me", "schon", "voll", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VAFIN", "ART", "NN", "ADV", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Zahllos sind die Leichen schon im Schilf gestreckt,", "tokens": ["Zahl\u00b7los", "sind", "die", "Lei\u00b7chen", "schon", "im", "Schilf", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Von des Todes starken V\u00f6geln wei\u00df bedeckt.", "tokens": ["Von", "des", "To\u00b7des", "star\u00b7ken", "V\u00f6\u00b7geln", "wei\u00df", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "\u00dcber runder Mauern blauem Flammenschwall", "tokens": ["\u00dc\u00b7ber", "run\u00b7der", "Mau\u00b7ern", "blau\u00b7em", "Flam\u00b7men\u00b7schwall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Steht er, \u00fcber schwarzer Gassen Waffenschall.", "tokens": ["Steht", "er", ",", "\u00fc\u00b7ber", "schwar\u00b7zer", "Gas\u00b7sen", "Waf\u00b7fen\u00b7schall", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "\u203a\u00fcber Toren, wo die W\u00e4chter liegen quer,", "tokens": ["\u203a", "\u00fc\u00b7ber", "To\u00b7ren", ",", "wo", "die", "W\u00e4ch\u00b7ter", "lie\u00b7gen", "quer", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "PWAV", "ART", "NE", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "\u00dcber Br\u00fccken, die von Bergen Toter schwer.\u2039", "tokens": ["\u00dc\u00b7ber", "Br\u00fc\u00b7cken", ",", "die", "von", "Ber\u00b7gen", "To\u00b7ter", "schwer", ".", "\u2039"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NN", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "In die Nacht er jagt das Feuer querfeldein", "tokens": ["In", "die", "Nacht", "er", "jagt", "das", "Feu\u00b7er", "quer\u00b7fel\u00b7dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Einen roten Hund mit wilder M\u00e4uler Schrein.", "tokens": ["Ei\u00b7nen", "ro\u00b7ten", "Hund", "mit", "wil\u00b7der", "M\u00e4u\u00b7ler", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Aus dem Dunkel springt der N\u00e4chte schwarze Welt,", "tokens": ["Aus", "dem", "Dun\u00b7kel", "springt", "der", "N\u00e4ch\u00b7te", "schwar\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Von Vulkanen furchtbar ist ihr Rand erhellt.", "tokens": ["Von", "Vul\u00b7ka\u00b7nen", "furcht\u00b7bar", "ist", "ihr", "Rand", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Und mit tausend roten Zipfelm\u00fctzen weit", "tokens": ["Und", "mit", "tau\u00b7send", "ro\u00b7ten", "Zip\u00b7fel\u00b7m\u00fct\u00b7zen", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sind die finstren Ebnen flackend \u00fcberstreut,", "tokens": ["Sind", "die", "finst\u00b7ren", "Eb\u00b7nen", "fla\u00b7ckend", "\u00fc\u00b7bers\u00b7treut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und was unten auf den Stra\u00dfen wimmelt hin und her,", "tokens": ["Und", "was", "un\u00b7ten", "auf", "den", "Stra\u00b7\u00dfen", "wim\u00b7melt", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "\u203afegt er in die Feuerhaufen, da\u00df die Flamme brenne mehr.\u2039", "tokens": ["\u203a", "fegt", "er", "in", "die", "Feu\u00b7er\u00b7hau\u00b7fen", ",", "da\u00df", "die", "Flam\u00b7me", "bren\u00b7ne", "mehr", ".", "\u2039"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Und die Flammen fressen brennend Wald um Wald,", "tokens": ["Und", "die", "Flam\u00b7men", "fres\u00b7sen", "bren\u00b7nend", "Wald", "um", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Gelbe Flederm\u00e4use zackig in das Laub gekrallt.", "tokens": ["Gel\u00b7be", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", "za\u00b7ckig", "in", "das", "Laub", "ge\u00b7krallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Seine Stange haut er wie ein K\u00f6hlerknecht", "tokens": ["Sei\u00b7ne", "Stan\u00b7ge", "haut", "er", "wie", "ein", "K\u00f6h\u00b7ler\u00b7knecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "In die B\u00e4ume, da\u00df das Feuer brause recht.", "tokens": ["In", "die", "B\u00e4u\u00b7me", ",", "da\u00df", "das", "Feu\u00b7er", "brau\u00b7se", "recht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.10": {"line.1": {"text": "Eine gro\u00dfe Stadt versank in gelbem Rauch,", "tokens": ["Ei\u00b7ne", "gro\u00b7\u00dfe", "Stadt", "ver\u00b7sank", "in", "gel\u00b7bem", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Warf sich lautlos in des Abgrunds Bauch.", "tokens": ["Warf", "sich", "laut\u00b7los", "in", "des", "Ab\u00b7grunds", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber riesig \u00fcber gl\u00fchnden Tr\u00fcmmern steht", "tokens": ["A\u00b7ber", "rie\u00b7sig", "\u00fc\u00b7ber", "gl\u00fchn\u00b7den", "Tr\u00fcm\u00b7mern", "steht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Der in wilde Himmel dreimal seine Fackel dreht,", "tokens": ["Der", "in", "wil\u00b7de", "Him\u00b7mel", "drei\u00b7mal", "sei\u00b7ne", "Fa\u00b7ckel", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.11": {"line.1": {"text": "\u00dcber sturmzerfetzter Wolken Widerschein,", "tokens": ["\u00dc\u00b7ber", "sturm\u00b7zer\u00b7fetz\u00b7ter", "Wol\u00b7ken", "Wi\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "In des toten Dunkels kalte W\u00fcstenein,", "tokens": ["In", "des", "to\u00b7ten", "Dun\u00b7kels", "kal\u00b7te", "W\u00fcs\u00b7ten\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Da\u00df er mit dem Brande weit die Nacht verdorr,", "tokens": ["Da\u00df", "er", "mit", "dem", "Bran\u00b7de", "weit", "die", "Nacht", "ver\u00b7dorr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Pech und Feuer tr\u00e4ufet unten auf Gomorrh.", "tokens": ["Pech", "und", "Feu\u00b7er", "tr\u00e4u\u00b7fet", "un\u00b7ten", "auf", "Go\u00b7morrh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}