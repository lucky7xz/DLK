{"dta.poem.4409": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Spring-Brunn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kann sonst von unserm kurzen Leben", "tokens": ["Kann", "sonst", "von", "un\u00b7serm", "kur\u00b7zen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Wassers rege Fl\u00fcchtigkeit", "tokens": ["Des", "Was\u00b7sers", "re\u00b7ge", "Fl\u00fcch\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns \u00fcberhaupt ein Lehr-Bild geben;", "tokens": ["Uns", "\u00fc\u00b7ber\u00b7haupt", "ein", "Lehr\u00b7Bild", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "So zeigen hier die feuchten Fluten", "tokens": ["So", "zei\u00b7gen", "hier", "die", "feuch\u00b7ten", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So gar die fl\u00fcchtigen Minuten", "tokens": ["So", "gar", "die", "fl\u00fcch\u00b7ti\u00b7gen", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von uns\u2019rer schnellen Lebens-Zeit.", "tokens": ["Von", "un\u00b7s'\u00b7rer", "schnel\u00b7len", "Le\u00b7bens\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}