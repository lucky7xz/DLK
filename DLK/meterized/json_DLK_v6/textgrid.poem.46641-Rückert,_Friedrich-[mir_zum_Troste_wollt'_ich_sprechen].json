{"textgrid.poem.46641": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mir zum Troste wollt' ich sprechen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir zum Troste wollt' ich sprechen:", "tokens": ["Mir", "zum", "Tros\u00b7te", "wollt'", "ich", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ein T\u00f6chterchen begraben,", "tokens": ["Die", "ein", "T\u00f6ch\u00b7ter\u00b7chen", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat noch Hoffnung, da\u00df sie eine", "tokens": ["Hat", "noch", "Hoff\u00b7nung", ",", "da\u00df", "sie", "ei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "$,", "KOUS", "PPER", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Andere geb\u00e4re.", "tokens": ["An\u00b7de\u00b7re", "ge\u00b7b\u00e4\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch mir d\u00fcnkt es ein Verbrechen,", "tokens": ["Doch", "mir", "d\u00fcnkt", "es", "ein", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses nur gedacht zu haben;", "tokens": ["Die\u00b7ses", "nur", "ge\u00b7dacht", "zu", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den Gedanken wein', o weine", "tokens": ["Den", "Ge\u00b7dan\u00b7ken", "wein'", ",", "o", "wei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "FM", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weg, hinweg, o Z\u00e4hre!", "tokens": ["Weg", ",", "hin\u00b7weg", ",", "o", "Z\u00e4h\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$,", "FM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Himmelsm\u00e4chte, die zum Stechen", "tokens": ["Him\u00b7mels\u00b7m\u00e4ch\u00b7te", ",", "die", "zum", "Ste\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Liebesdorn mir gaben,", "tokens": ["Die\u00b7sen", "Lie\u00b7bes\u00b7dorn", "mir", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Rose will ich, keine,", "tokens": ["Kei\u00b7ne", "Ro\u00b7se", "will", "ich", ",", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "$,", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mir lieber w\u00e4re.", "tokens": ["Die", "mir", "lie\u00b7ber", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}