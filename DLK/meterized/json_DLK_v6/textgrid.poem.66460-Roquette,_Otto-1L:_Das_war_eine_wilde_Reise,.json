{"textgrid.poem.66460": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das war eine wilde Reise,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war eine wilde Reise,", "tokens": ["Das", "war", "ei\u00b7ne", "wil\u00b7de", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Da wir froh nach Burschenweise", "tokens": ["Da", "wir", "froh", "nach", "Bur\u00b7schen\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stiegen auf zum Brockenhaupt!", "tokens": ["Stie\u00b7gen", "auf", "zum", "Bro\u00b7cken\u00b7haupt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberall in deutschen Landen", "tokens": ["Ue\u00b7be\u00b7rall", "in", "deut\u00b7schen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ward ein hohes Fest gefeiert:", "tokens": ["Ward", "ein", "ho\u00b7hes", "Fest", "ge\u00b7fei\u00b7ert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Goethefest \u2013 gespielt, geleiert.", "tokens": ["Goe\u00b7the\u00b7fest", "\u2013", "ge\u00b7spielt", ",", "ge\u00b7lei\u00b7ert", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch nach andrer Feier standen", "tokens": ["Doch", "nach", "an\u00b7drer", "Fei\u00b7er", "stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns die Sinne, und wir fanden", "tokens": ["Uns", "die", "Sin\u00b7ne", ",", "und", "wir", "fan\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Uns ein Fest, so recht romantisch,", "tokens": ["Uns", "ein", "Fest", ",", "so", "recht", "ro\u00b7man\u00b7tisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Nicht voll Reden, nicht pedantisch,", "tokens": ["Nicht", "voll", "Re\u00b7den", ",", "nicht", "pe\u00b7dan\u00b7tisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "NN", "$,", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht so professorisch k\u00fchl,", "tokens": ["Nicht", "so", "pro\u00b7fes\u00b7so\u00b7risch", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "ADJD", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.12": {"text": "Nein, so recht im Blocksbergstyl.", "tokens": ["Nein", ",", "so", "recht", "im", "Blocks\u00b7berg\u00b7styl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lustig schien die Herbstessonne", "tokens": ["Lus\u00b7tig", "schien", "die", "Herbs\u00b7tes\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber unsre Wanderwonne,", "tokens": ["Ue\u00b7ber", "uns\u00b7re", "Wan\u00b7der\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6stlich war die Luft, und klar.", "tokens": ["K\u00f6st\u00b7lich", "war", "die", "Luft", ",", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freudig schallten Wanders\u00e4nge", "tokens": ["Freu\u00b7dig", "schall\u00b7ten", "Wan\u00b7der\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch der hohen Felsenmassen", "tokens": ["Durch", "der", "ho\u00b7hen", "Fel\u00b7sen\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ungebahnte, steile Stra\u00dfen.", "tokens": ["Un\u00b7ge\u00b7bahn\u00b7te", ",", "stei\u00b7le", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch die schroffen Bergesh\u00e4nge", "tokens": ["Doch", "die", "schrof\u00b7fen", "Ber\u00b7ges\u00b7h\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stuften sich im Felsgedr\u00e4nge", "tokens": ["Stuf\u00b7ten", "sich", "im", "Fels\u00b7ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Immer h\u00f6her, immer grauer,", "tokens": ["Im\u00b7mer", "h\u00f6\u00b7her", ",", "im\u00b7mer", "grau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und ein k\u00fchler Nebelschauer", "tokens": ["Und", "ein", "k\u00fch\u00b7ler", "Ne\u00b7bel\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Zauberte durch Fichten schwarz", "tokens": ["Zau\u00b7ber\u00b7te", "durch", "Fich\u00b7ten", "schwarz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Um uns her den echten Harz.", "tokens": ["Um", "uns", "her", "den", "ech\u00b7ten", "Harz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn des Blocksbergs wilde Trosse", "tokens": ["Denn", "des", "Blocks\u00b7bergs", "wil\u00b7de", "Tros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollten heut zu Fu\u00df und Rosse", "tokens": ["Woll\u00b7ten", "heut", "zu", "Fu\u00df", "und", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feiern auch ihr Goethefest.", "tokens": ["Fei\u00b7ern", "auch", "ihr", "Goe\u00b7the\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bang die Sonne sich verstecket,", "tokens": ["Bang", "die", "Son\u00b7ne", "sich", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und schon l\u00e4\u00dft der Sturm sich h\u00f6ren", "tokens": ["Und", "schon", "l\u00e4\u00dft", "der", "Sturm", "sich", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PRF", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Dumpf in des Gebirges F\u00f6hren,", "tokens": ["Dumpf", "in", "des", "Ge\u00b7bir\u00b7ges", "F\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sein Brausen ruft und wecket", "tokens": ["Und", "sein", "Brau\u00b7sen", "ruft", "und", "we\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schnell das Heer. Und kreischend strecket", "tokens": ["Schnell", "das", "Heer", ".", "Und", "krei\u00b7schend", "stre\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "$.", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sich begr\u00fc\u00dfend, aus der Lauer", "tokens": ["Sich", "be\u00b7gr\u00fc\u00b7\u00dfend", ",", "aus", "der", "Lau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "All der wilde Koboldschauer,", "tokens": ["All", "der", "wil\u00b7de", "Ko\u00b7bold\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Springt und tanzt mit Teufelssang", "tokens": ["Springt", "und", "tanzt", "mit", "Teu\u00b7fels\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wild von Fels zu Felsenhang.", "tokens": ["Wild", "von", "Fels", "zu", "Fel\u00b7sen\u00b7hang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und nun weiter, immer weiter,", "tokens": ["Und", "nun", "wei\u00b7ter", ",", "im\u00b7mer", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf des Brockens Felsenleiter", "tokens": ["Auf", "des", "Bro\u00b7ckens", "Fel\u00b7sen\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schrillt und br\u00fcllt, und jauchzt und st\u00fcrmt's", "tokens": ["Schrillt", "und", "br\u00fcllt", ",", "und", "jauchzt", "und", "st\u00fcrmt's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Angstvoll fliehn der V\u00f6gel Schaaren,", "tokens": ["Angst\u00b7voll", "fliehn", "der", "V\u00f6\u00b7gel", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Gewild im schwarzen Forste", "tokens": ["Das", "Ge\u00b7wild", "im", "schwar\u00b7zen", "Fors\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Flieht herab vom Waldeshorste.", "tokens": ["Flieht", "her\u00b7ab", "vom", "Wal\u00b7des\u00b7hors\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tannen mit zerzausten Haaren", "tokens": ["Tan\u00b7nen", "mit", "zer\u00b7zaus\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "St\u00fcrzen krachend hin zu Paaren.", "tokens": ["St\u00fcr\u00b7zen", "kra\u00b7chend", "hin", "zu", "Paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Schwarz umw\u00f6lket droht der Himmel", "tokens": ["Schwarz", "um\u00b7w\u00f6l\u00b7ket", "droht", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ueber dem Naturget\u00fcmmel,", "tokens": ["Ue\u00b7ber", "dem", "Na\u00b7tur\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und vom Donner mit Getos", "tokens": ["Und", "vom", "Don\u00b7ner", "mit", "Ge\u00b7tos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Ri\u00df der Wiederhall sich los.", "tokens": ["Ri\u00df", "der", "Wie\u00b7der\u00b7hall", "sich", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Halt! wo ist der Weg? Verloren!", "tokens": ["Halt", "!", "wo", "ist", "der", "Weg", "?", "Ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "$.", "PWAV", "VAFIN", "ART", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die Lust der Erdenthoren", "tokens": ["Auf", "die", "Lust", "der", "Er\u00b7den\u00b7tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Legt der Teufel seinen Schwanz.", "tokens": ["Legt", "der", "Teu\u00b7fel", "sei\u00b7nen", "Schwanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welch ein Schrecken, welch ein Grausen", "tokens": ["Welch", "ein", "Schre\u00b7cken", ",", "welch", "ein", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PWAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Antwort durch die finstern L\u00fcfte", "tokens": ["Ant\u00b7wort", "durch", "die", "fins\u00b7tern", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schreien h\u00f6hnend alle Kl\u00fcfte,", "tokens": ["Schrei\u00b7en", "h\u00f6h\u00b7nend", "al\u00b7le", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wir stehn im Sturmesbrausen", "tokens": ["Und", "wir", "stehn", "im", "Stur\u00b7mes\u00b7brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In des Festes tollstem Hausen.", "tokens": ["In", "des", "Fes\u00b7tes", "tolls\u00b7tem", "Hau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und auf Besen, Ziegenb\u00f6cken,", "tokens": ["Und", "auf", "Be\u00b7sen", ",", "Zie\u00b7gen\u00b7b\u00f6\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hexentanz in allen Ecken,", "tokens": ["He\u00b7xen\u00b7tanz", "in", "al\u00b7len", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Regengu\u00df und Nebelnacht \u2013", "tokens": ["Re\u00b7gen\u00b7gu\u00df", "und", "Ne\u00b7bel\u00b7nacht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Weh, wir sind im H\u00f6llenschacht!", "tokens": ["Weh", ",", "wir", "sind", "im", "H\u00f6l\u00b7len\u00b7schacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohin wenden? Wohin schreiten?", "tokens": ["Wo\u00b7hin", "wen\u00b7den", "?", "Wo\u00b7hin", "schrei\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVINF", "$.", "PWAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn kein Weg mehr will uns leiten!", "tokens": ["Denn", "kein", "Weg", "mehr", "will", "uns", "lei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hurtig, muthig, grade aus!", "tokens": ["Hur\u00b7tig", ",", "mut\u00b7hig", ",", "gra\u00b7de", "aus", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber j\u00e4her nur verdichten", "tokens": ["A\u00b7ber", "j\u00e4\u00b7her", "nur", "ver\u00b7dich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich die Felsen. Auf, und klettert,", "tokens": ["Sich", "die", "Fel\u00b7sen", ".", "Auf", ",", "und", "klet\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$.", "APPR", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob auch Erd' und Himmel wettert!", "tokens": ["Ob", "auch", "Erd'", "und", "Him\u00b7mel", "wet\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seht, schon hellen sich die Fichten,", "tokens": ["Seht", ",", "schon", "hel\u00b7len", "sich", "die", "Fich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nieder gehn die Felsenschichten.", "tokens": ["Nie\u00b7der", "gehn", "die", "Fel\u00b7sen\u00b7schich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber weh! da lauern S\u00fcmpfe,", "tokens": ["A\u00b7ber", "weh", "!", "da", "lau\u00b7ern", "S\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und es bleiben Schuh und Str\u00fcmpfe", "tokens": ["Und", "es", "blei\u00b7ben", "Schuh", "und", "Str\u00fcmp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Stecken in dem Teufelsschlamm,", "tokens": ["Ste\u00b7cken", "in", "dem", "Teu\u00b7fels\u00b7schlamm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Jedem Schritt ein Pfuhl und Damm!", "tokens": ["Je\u00b7dem", "Schritt", "ein", "Pfuhl", "und", "Damm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Zaubermeister, Vater Goethe,", "tokens": ["Zau\u00b7ber\u00b7meis\u00b7ter", ",", "Va\u00b7ter", "Goe\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hilf uns bannen unsre N\u00f6the!", "tokens": ["Hilf", "uns", "ban\u00b7nen", "uns\u00b7re", "N\u00f6\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, so flehten wir im Chor.", "tokens": ["Ach", ",", "so", "fleh\u00b7ten", "wir", "im", "Chor", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Willst du, da\u00df wir sterben sollen?", "tokens": ["Willst", "du", ",", "da\u00df", "wir", "ster\u00b7ben", "sol\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kamen ja zu deinem Feste,", "tokens": ["Ka\u00b7men", "ja", "zu", "dei\u00b7nem", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weih'n dir unsrer Lieder beste! \u2013", "tokens": ["Weih'n", "dir", "uns\u00b7rer", "Lie\u00b7der", "bes\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Horch! da schwieg des Sturmes Grollen,", "tokens": ["Horch", "!", "da", "schwieg", "des", "Stur\u00b7mes", "Grol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Nebel seitw\u00e4rts quollen,", "tokens": ["Und", "die", "Ne\u00b7bel", "seit\u00b7w\u00e4rts", "quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und ein Tagesblick bot Rettung", "tokens": ["Und", "ein", "Ta\u00b7ges\u00b7blick", "bot", "Ret\u00b7tung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Uns aus unsrer Sumpfesbettung,", "tokens": ["Uns", "aus", "uns\u00b7rer", "Sump\u00b7fes\u00b7bet\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Jubelruf: da Leid ist aus,", "tokens": ["Ju\u00b7bel\u00b7ruf", ":", "da", "Leid", "ist", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Droben winkt da Brockenhaus!", "tokens": ["Dro\u00b7ben", "winkt", "da", "Bro\u00b7cken\u00b7haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Rechts und links, und tief und oben,", "tokens": ["Rechts", "und", "links", ",", "und", "tief", "und", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,", "KON", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lie\u00dfen wir den Sturm nun toben,", "tokens": ["Lie\u00b7\u00dfen", "wir", "den", "Sturm", "nun", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heimlich warm war Stub' und Haus.", "tokens": ["Heim\u00b7lich", "warm", "war", "Stub'", "und", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei, wie perlte neues Feuer", "tokens": ["Hei", ",", "wie", "perl\u00b7te", "neu\u00b7es", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jetzt der Wein uns in die Glieder,", "tokens": ["Jetzt", "der", "Wein", "uns", "in", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weckte tausend Jubellieder,", "tokens": ["Weck\u00b7te", "tau\u00b7send", "Ju\u00b7bel\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4hrend drau\u00dfen, nicht geheuer", "tokens": ["W\u00e4h\u00b7rend", "drau\u00b7\u00dfen", ",", "nicht", "ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "$,", "PTKNEG", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schnob die Nacht um das Gem\u00e4uer!", "tokens": ["Schnob", "die", "Nacht", "um", "das", "Ge\u00b7m\u00e4u\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Vater Goethe, du Befreier!", "tokens": ["Va\u00b7ter", "Goe\u00b7the", ",", "du", "Be\u00b7frei\u00b7er", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sahst du unsre lustge Feier?", "tokens": ["Sahst", "du", "uns\u00b7re", "lust\u00b7ge", "Fei\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Jung auch warst du niemals k\u00fchl", "tokens": ["Jung", "auch", "warst", "du", "nie\u00b7mals", "k\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "F\u00fcr ein Fest im Blocksbergstyl!", "tokens": ["F\u00fcr", "ein", "Fest", "im", "Blocks\u00b7berg\u00b7styl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}