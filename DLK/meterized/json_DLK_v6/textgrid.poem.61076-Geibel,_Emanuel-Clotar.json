{"textgrid.poem.61076": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Clotar", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegt am Strand der Spree im Preu\u00dfenland", "tokens": ["Es", "liegt", "am", "Strand", "der", "Spree", "im", "Preu\u00b7\u00dfen\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Stadt Berlin, die jede Zeitung nennt,", "tokens": ["Die", "Stadt", "Ber\u00b7lin", ",", "die", "je\u00b7de", "Zei\u00b7tung", "nennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ber\u00fchmt durch ihren Fritz und ihren Sand", "tokens": ["Be\u00b7r\u00fchmt", "durch", "ih\u00b7ren", "Fritz", "und", "ih\u00b7ren", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und tausend Dichter, welche niemand kennt;", "tokens": ["Und", "tau\u00b7send", "Dich\u00b7ter", ",", "wel\u00b7che", "nie\u00b7mand", "kennt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dort lebte noch vor kurzem unbekannt,", "tokens": ["Dort", "leb\u00b7te", "noch", "vor", "kur\u00b7zem", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch wert, da\u00df ihr ihn kennet, ein Student,", "tokens": ["Doch", "wert", ",", "da\u00df", "ihr", "ihn", "ken\u00b7net", ",", "ein", "Stu\u00b7dent", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und weil mir eben andre Helden fehlen,", "tokens": ["Und", "weil", "mir", "e\u00b7ben", "and\u00b7re", "Hel\u00b7den", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Will ich von meinem Freund Clotar erz\u00e4hlen.", "tokens": ["Will", "ich", "von", "mei\u00b7nem", "Freund", "Clo\u00b7tar", "er\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er war ein seltner Kauz, halb Mann, halb Kind,", "tokens": ["Er", "war", "ein", "selt\u00b7ner", "Kauz", ",", "halb", "Mann", ",", "halb", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ADJD", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Mensch, als h\u00e4tt' ihn der April geboren:", "tokens": ["Ein", "Mensch", ",", "als", "h\u00e4tt'", "ihn", "der", "Ap\u00b7ril", "ge\u00b7bo\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOKOM", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Bald heldenk\u00fchn und rasch zur Tat gesinnt,", "tokens": ["Bald", "hel\u00b7den\u00b7k\u00fchn", "und", "rasch", "zur", "Tat", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald tr\u00e4umerisch in Schw\u00e4rmerei verloren;", "tokens": ["Bald", "tr\u00e4u\u00b7me\u00b7risch", "in", "Schw\u00e4r\u00b7me\u00b7rei", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Tr\u00fcbsinnig heute, wetterlaunisch, blind", "tokens": ["Tr\u00fcb\u00b7sin\u00b7nig", "heu\u00b7te", ",", "wet\u00b7ter\u00b7lau\u00b7nisch", ",", "blind"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ADV", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und morgen jeden Kummer abgeschworen;", "tokens": ["Und", "mor\u00b7gen", "je\u00b7den", "Kum\u00b7mer", "ab\u00b7ge\u00b7schwo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Jetzt wehmutweich, jetzt trotzig, nimmer stet -", "tokens": ["Jetzt", "weh\u00b7mut\u00b7weich", ",", "jetzt", "trot\u00b7zig", ",", "nim\u00b7mer", "stet"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit einem Wort: er war ein St\u00fcck Poet.", "tokens": ["Mit", "ei\u00b7nem", "Wort", ":", "er", "war", "ein", "St\u00fcck", "Po\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In der Gesellschaft, wo am blanken Teetisch", "tokens": ["In", "der", "Ge\u00b7sell\u00b7schaft", ",", "wo", "am", "blan\u00b7ken", "Tee\u00b7tisch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wasser brodelt und der Blaustrumpf gl\u00e4nzt,", "tokens": ["Das", "Was\u00b7ser", "bro\u00b7delt", "und", "der", "Blau\u00b7strumpf", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wo prosaisch bald und bald poetisch", "tokens": ["Und", "wo", "pro\u00b7sa\u00b7isch", "bald", "und", "bald", "po\u00b7e\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "ADV", "KON", "ADV", "ADJD"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Des Geists Rakete durch die Luft sich schw\u00e4nzt,", "tokens": ["Des", "Geists", "Ra\u00b7ke\u00b7te", "durch", "die", "Luft", "sich", "schw\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Langweilt' er sich; er liebt' es nicht, den Fetisch", "tokens": ["Lang\u00b7weilt'", "er", "sich", ";", "er", "liebt'", "es", "nicht", ",", "den", "Fe\u00b7tisch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit anzubeten, den man just bekr\u00e4nzt;", "tokens": ["Mit", "an\u00b7zu\u00b7be\u00b7ten", ",", "den", "man", "just", "be\u00b7kr\u00e4nzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVIZU", "$,", "PRELS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er schwieg darum, und tat er auch den Mund auf,", "tokens": ["Er", "schwieg", "da\u00b7rum", ",", "und", "tat", "er", "auch", "den", "Mund", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So war's zu g\u00e4hnen nur von Herzensgrund auf.", "tokens": ["So", "wa\u00b7r's", "zu", "g\u00e4h\u00b7nen", "nur", "von", "Her\u00b7zens\u00b7grund", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKZU", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Auch ha\u00dft' er Zeremonien und Visiten,", "tokens": ["Auch", "ha\u00dft'", "er", "Ze\u00b7re\u00b7mo\u00b7ni\u00b7en", "und", "Vi\u00b7si\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Manschetten, Binde, Frack, den Hut im Arm,", "tokens": ["Man\u00b7schet\u00b7ten", ",", "Bin\u00b7de", ",", "Frack", ",", "den", "Hut", "im", "Arm", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Mund voll Phrasen und das Herz voll Nieten", "tokens": ["Den", "Mund", "voll", "Phra\u00b7sen", "und", "das", "Herz", "voll", "Nie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fader P\u00fcppchen aufgestutzten Schwarm;", "tokens": ["Und", "fa\u00b7der", "P\u00fcpp\u00b7chen", "auf\u00b7ge\u00b7stutz\u00b7ten", "Schwarm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja, h\u00e4tte manche Dame zu gebieten,", "tokens": ["Ja", ",", "h\u00e4t\u00b7te", "man\u00b7che", "Da\u00b7me", "zu", "ge\u00b7bie\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So w\u00fcrde l\u00e4ngst ihm in der H\u00f6lle warm,", "tokens": ["So", "w\u00fcr\u00b7de", "l\u00e4ngst", "ihm", "in", "der", "H\u00f6l\u00b7le", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Damit er qualvoll dort es lernen m\u00fcsse,", "tokens": ["Da\u00b7mit", "er", "qual\u00b7voll", "dort", "es", "ler\u00b7nen", "m\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie man die sch\u00f6nberingte Hand ihr k\u00fcsse.", "tokens": ["Wie", "man", "die", "sch\u00f6n\u00b7be\u00b7ring\u00b7te", "Hand", "ihr", "k\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dagegen liebt' er alte Folianten,", "tokens": ["Da\u00b7ge\u00b7gen", "liebt'", "er", "al\u00b7te", "Fo\u00b7li\u00b7an\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Woraus der Geist vergangner Gr\u00f6\u00dfe sprach,", "tokens": ["Wo\u00b7raus", "der", "Geist", "ver\u00b7gang\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn bleicher schon des Himmels Sterne brannten,", "tokens": ["Wenn", "blei\u00b7cher", "schon", "des", "Him\u00b7mels", "Ster\u00b7ne", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sa\u00df einsam er noch oft bei ihnen wach.", "tokens": ["Sa\u00df", "ein\u00b7sam", "er", "noch", "oft", "bei", "ih\u00b7nen", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er sp\u00fcrt' in ihrem Schacht den Diamanten", "tokens": ["Er", "sp\u00fcrt'", "in", "ih\u00b7rem", "Schacht", "den", "Di\u00b7a\u00b7man\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Sch\u00f6nheit und dem Gold der Weisheit nach", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "und", "dem", "Gold", "der", "Weis\u00b7heit", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und h\u00f6rte drin mit andachtsvollem Lauschen", "tokens": ["Und", "h\u00f6r\u00b7te", "drin", "mit", "an\u00b7dachts\u00b7vol\u00b7lem", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Lebens tiefverborgne Quellen rauschen.", "tokens": ["Des", "Le\u00b7bens", "tief\u00b7ver\u00b7borg\u00b7ne", "Quel\u00b7len", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ernsthaft ans Werk, zum Frohsinn aufger\u00e4umt,", "tokens": ["Ernst\u00b7haft", "ans", "Werk", ",", "zum", "Froh\u00b7sinn", "auf\u00b7ge\u00b7r\u00e4umt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das war sein Wort, und das war seine Weise.", "tokens": ["Das", "war", "sein", "Wort", ",", "und", "das", "war", "sei\u00b7ne", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seht hin! Die Zither klingt, der Becher sch\u00e4umt,", "tokens": ["Seht", "hin", "!", "Die", "Zi\u00b7ther", "klingt", ",", "der", "Be\u00b7cher", "sch\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er rastet beim Gelag im Freundeskreise;", "tokens": ["Er", "ras\u00b7tet", "beim", "Ge\u00b7lag", "im", "Freun\u00b7des\u00b7krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da gl\u00e4nzt die Stirn, die eben noch getr\u00e4umt,", "tokens": ["Da", "gl\u00e4nzt", "die", "Stirn", ",", "die", "e\u00b7ben", "noch", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die blasse Wange f\u00e4rbt mit Rot sich leise,", "tokens": ["Die", "blas\u00b7se", "Wan\u00b7ge", "f\u00e4rbt", "mit", "Rot", "sich", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Wimpern zucken rasch, die Augen blitzen,", "tokens": ["Die", "Wim\u00b7pern", "zu\u00b7cken", "rasch", "