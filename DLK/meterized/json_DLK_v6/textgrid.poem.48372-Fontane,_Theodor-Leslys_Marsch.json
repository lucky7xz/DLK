{"textgrid.poem.48372": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Leslys Marsch", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer mit, immer mit,", "tokens": ["Im\u00b7mer", "mit", ",", "im\u00b7mer", "mit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Was Teufel, wer h\u00e4lt nicht Schritt?", "tokens": ["Was", "Teu\u00b7fel", ",", "wer", "h\u00e4lt", "nicht", "Schritt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "PWS", "VVFIN", "PTKNEG", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Englischen ziehen von dr\u00fcben heran,", "tokens": ["Die", "Eng\u00b7li\u00b7schen", "zie\u00b7hen", "von", "dr\u00fc\u00b7ben", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Schlie\u00dft euch fester Mann an Mann,", "tokens": ["Schlie\u00dft", "euch", "fes\u00b7ter", "Mann", "an", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Musketiere rasch in die Front,", "tokens": ["Mus\u00b7ke\u00b7tie\u00b7re", "rasch", "in", "die", "Front", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Habt ihr vergessen, was ihr gekonnt:", "tokens": ["Habt", "ihr", "ver\u00b7ges\u00b7sen", ",", "was", "ihr", "ge\u00b7konnt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Schie\u00dfen und Fechten und Sch\u00e4delspalten", "tokens": ["Schie\u00b7\u00dfen", "und", "Fech\u00b7ten", "und", "Sch\u00e4\u00b7del\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und mit der Linken die Bibel halten.", "tokens": ["Und", "mit", "der", "Lin\u00b7ken", "die", "Bi\u00b7bel", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die erste Kirche, in die wir kommen,", "tokens": ["Die", "ers\u00b7te", "Kir\u00b7che", ",", "in", "die", "wir", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drin Rom und die Pfaffen Platz genommen", "tokens": ["Drin", "Rom", "und", "die", "Pfaf\u00b7fen", "Platz", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "KON", "ART", "NN", "NN", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Orgel und Altar hinein erneuert,", "tokens": ["Und", "Or\u00b7gel", "und", "Al\u00b7tar", "hin\u00b7ein", "er\u00b7neu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Werd' ausgefegt und ausgescheuert;", "tokens": ["Werd'", "aus\u00b7ge\u00b7fegt", "und", "aus\u00b7ge\u00b7scheu\u00b7ert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jenny soll das K\u00e4ppchen tragen,", "tokens": ["Jen\u00b7ny", "soll", "das", "K\u00e4pp\u00b7chen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jocky das Chorhemd um sich schlagen,", "tokens": ["Jo\u00b7cky", "das", "Chor\u00b7hemd", "um", "sich", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und nach der Orgel und ihren Pfeifen", "tokens": ["Und", "nach", "der", "Or\u00b7gel", "und", "ih\u00b7ren", "Pfei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Sollen unsre Pfeifer greifen,", "tokens": ["Sol\u00b7len", "uns\u00b7re", "Pfei\u00b7fer", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "K\u00f6nnen drauf spielen den ganzen Tag,", "tokens": ["K\u00f6n\u00b7nen", "drauf", "spie\u00b7len", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Komme, was da kommen mag;", "tokens": ["Kom\u00b7me", ",", "was", "da", "kom\u00b7men", "mag", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Bursche, munter,", "tokens": ["Bur\u00b7sche", ",", "mun\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Bald wird's bunter,", "tokens": ["Bald", "wird's", "bun\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Sch\u00fcrzet die Plaids! sie kommen, Juchhe,", "tokens": ["Sch\u00fcr\u00b7zet", "die", "Plaids", "!", "sie", "kom\u00b7men", ",", "Juch\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PPER", "VVINF", "$,", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Und klappt die M\u00fctzen in die H\u00f6h.", "tokens": ["Und", "klappt", "die", "M\u00fct\u00b7zen", "in", "die", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}