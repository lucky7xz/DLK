{"textgrid.poem.56022": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Blinde", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, er geht und unterbricht die Stadt,", "tokens": ["Sieh", ",", "er", "geht", "und", "un\u00b7ter\u00b7bricht", "die", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "die nicht ist auf seiner dunkeln Stelle,", "tokens": ["die", "nicht", "ist", "auf", "sei\u00b7ner", "dun\u00b7keln", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wie ein dunkler Sprung durch eine helle", "tokens": ["wie", "ein", "dunk\u00b7ler", "Sprung", "durch", "ei\u00b7ne", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Tasse geht. Und wie auf einem Blatt", "tokens": ["Tas\u00b7se", "geht", ".", "Und", "wie", "auf", "ei\u00b7nem", "Blatt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "KON", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "ist auf ihm der Widerschein der Dinge", "tokens": ["ist", "auf", "ihm", "der", "Wi\u00b7der\u00b7schein", "der", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aufgemalt; er nimmt ihn nicht hinein.", "tokens": ["auf\u00b7ge\u00b7malt", ";", "er", "nimmt", "ihn", "nicht", "hin\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur sein F\u00fchlen r\u00fchrt sich, so als finge", "tokens": ["Nur", "sein", "F\u00fch\u00b7len", "r\u00fchrt", "sich", ",", "so", "als", "fin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PRF", "$,", "ADV", "KOKOM", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "es die Welt in kleinen Wellen ein:", "tokens": ["es", "die", "Welt", "in", "klei\u00b7nen", "Wel\u00b7len", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "eine Stille, einen Widerstand \u2013,", "tokens": ["ei\u00b7ne", "Stil\u00b7le", ",", "ei\u00b7nen", "Wi\u00b7der\u00b7stand", "\u2013", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$(", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und dann scheint er wartend wen zu w\u00e4hlen:", "tokens": ["und", "dann", "scheint", "er", "war\u00b7tend", "wen", "zu", "w\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "hingegeben hebt er seine Hand,", "tokens": ["hin\u00b7ge\u00b7ge\u00b7ben", "hebt", "er", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "festlich fast, wie um sich zu verm\u00e4hlen.", "tokens": ["fest\u00b7lich", "fast", ",", "wie", "um", "sich", "zu", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "PWAV", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}