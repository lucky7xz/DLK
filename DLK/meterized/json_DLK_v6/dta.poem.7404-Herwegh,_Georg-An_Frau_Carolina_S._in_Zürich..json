{"dta.poem.7404": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  \n Frau Carolina S. in Z\u00fcrich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur zagend lass' ich meinen Worten", "tokens": ["Nur", "za\u00b7gend", "lass'", "ich", "mei\u00b7nen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor andern Menschen ihren Lauf;", "tokens": ["Vor", "an\u00b7dern", "Men\u00b7schen", "ih\u00b7ren", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von meinem Herzen klingend auf;", "tokens": ["Von", "mei\u00b7nem", "Her\u00b7zen", "klin\u00b7gend", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir ist, ", "tokens": ["Mir", "ist", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die tiefste Seele wird mir flott;", "tokens": ["Die", "tiefs\u00b7te", "See\u00b7le", "wird", "mir", "flott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ich mag in die Saiten schlagen,", "tokens": ["Wie", "ich", "mag", "in", "die", "Sai\u00b7ten", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Um ", "tokens": ["Um"], "token_info": ["word"], "pos": ["KOUI"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Die Welt will, da\u00df man sie betr\u00fcge", "tokens": ["Die", "Welt", "will", ",", "da\u00df", "man", "sie", "be\u00b7tr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ein erheuchelt fromm Gef\u00fchl,", "tokens": ["Durch", "ein", "er\u00b7heu\u00b7chelt", "fromm", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Anstand einen Frieden l\u00fcge,", "tokens": ["Mit", "An\u00b7stand", "ei\u00b7nen", "Frie\u00b7den", "l\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn's in der Brust uns dumpf und schw\u00fcl;", "tokens": ["Wenn's", "in", "der", "Brust", "uns", "dumpf", "und", "schw\u00fcl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du h\u00f6rest, seltenste der Frauen,", "tokens": ["Du", "h\u00f6\u00b7rest", ",", "sel\u00b7tens\u00b7te", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den kecken Schw\u00e4rmer ohne Groll,", "tokens": ["Den", "ke\u00b7cken", "Schw\u00e4r\u00b7mer", "oh\u00b7ne", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du wei\u00dft, man mu\u00df ihn selber bauen,", "tokens": ["Du", "wei\u00dft", ",", "man", "mu\u00df", "ihn", "sel\u00b7ber", "bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Himmel, dran man glauben soll. \u2014", "tokens": ["Den", "Him\u00b7mel", ",", "dran", "man", "glau\u00b7ben", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PIS", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gleichwie am stillen Abend schmettert", "tokens": ["Gleich\u00b7wie", "am", "stil\u00b7len", "A\u00b7bend", "schmet\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch heitre Luft Trompetenklang,", "tokens": ["Durch", "heit\u00b7re", "Luft", "Trom\u00b7pe\u00b7ten\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Gleichwie 's um Rosenb\u00fcsche wettert", "tokens": ["Gleich\u00b7wie", "'s", "um", "Ro\u00b7sen\u00b7b\u00fc\u00b7sche", "wet\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ein bl\u00fchendes Gestad entlang,", "tokens": ["Ein", "bl\u00fc\u00b7hen\u00b7des", "Ge\u00b7stad", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleichwie zum Sturme ruft die Glocke,", "tokens": ["Gleich\u00b7wie", "zum", "Stur\u00b7me", "ruft", "die", "Glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Inde\u00df noch Beter am Altar,", "tokens": ["In\u00b7de\u00df", "noch", "Be\u00b7ter", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie neben eines Kindes Locke", "tokens": ["Wie", "ne\u00b7ben", "ei\u00b7nes", "Kin\u00b7des", "Lo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein graues, ernstes Greisenhaar, \u2014 \u2014", "tokens": ["Ein", "grau\u00b7es", ",", "erns\u00b7tes", "Grei\u00b7sen\u00b7haar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So t\u00f6nt zu meinem stillen Volke", "tokens": ["So", "t\u00f6nt", "zu", "mei\u00b7nem", "stil\u00b7len", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein z\u00fcrnend, freiheitheischend Lied;", "tokens": ["Mein", "z\u00fcr\u00b7nend", ",", "frei\u00b7hei\u00b7thei\u00b7schend", "Lied", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin die schwere, schwarze Wolke,", "tokens": ["Ich", "bin", "die", "schwe\u00b7re", ",", "schwar\u00b7ze", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gott den Donner nur beschied;", "tokens": ["Der", "Gott", "den", "Don\u00b7ner", "nur", "be\u00b7schied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin kein froher, freud'ger Buhle,", "tokens": ["Ich", "bin", "kein", "fro\u00b7her", ",", "freu\u00b7d'\u00b7ger", "Buh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "De\u00df Wappen Rose und Pokal,", "tokens": ["De\u00df", "Wap\u00b7pen", "Ro\u00b7se", "und", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich sitz' als Geist auf Banko's Stuhle", "tokens": ["Ich", "sitz'", "als", "Geist", "auf", "Ban\u00b7ko's", "Stuh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bei jedem frechen K\u00f6nigsmal.", "tokens": ["Bei", "je\u00b7dem", "fre\u00b7chen", "K\u00f6\u00b7nigs\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O k\u00f6nnt' im finstern Rath der Alten", "tokens": ["O", "k\u00f6nnt'", "im", "fins\u00b7tern", "Rath", "der", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Lied ein z\u00fcndend Feuer sein!", "tokens": ["Mein", "Lied", "ein", "z\u00fcn\u00b7dend", "Feu\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ach! die N\u00fcchternen, die Kalten", "tokens": ["Doch", "ach", "!", "die", "N\u00fcch\u00b7ter\u00b7nen", ",", "die", "Kal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "XY", "$.", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verlangen abgelegnen Wein.", "tokens": ["Ver\u00b7lan\u00b7gen", "ab\u00b7ge\u00b7leg\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Zorn oft dr\u00fcck' ich auf die Flasche", "tokens": ["Im", "Zorn", "oft", "dr\u00fcck", "ich", "auf", "die", "Fla\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Kork \u2014 es \u00f6ffnet sich ", "tokens": ["Den", "Kork", "es", "\u00f6ff\u00b7net", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Zu neuen k\u00fchnen Flammen aus.", "tokens": ["Zu", "neu\u00b7en", "k\u00fch\u00b7nen", "Flam\u00b7men", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich trug mein Lied, wie jener Meros", "tokens": ["Ich", "trug", "mein", "Lied", ",", "wie", "je\u00b7ner", "Me\u00b7ros"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PDAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Den Dolch, tief unter meinem Hemd;", "tokens": ["Den", "Dolch", ",", "tief", "un\u00b7ter", "mei\u00b7nem", "Hemd", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es klang, wo man dem Liede Nero's,", "tokens": ["Es", "klang", ",", "wo", "man", "dem", "Lie\u00b7de", "Nero's", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIS", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des neugekr\u00f6nten, lauscht, so fremd.", "tokens": ["Des", "neu\u00b7ge\u00b7kr\u00f6n\u00b7ten", ",", "lauscht", ",", "so", "fremd", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl waren manche Perlen fertig,", "tokens": ["Wohl", "wa\u00b7ren", "man\u00b7che", "Per\u00b7len", "fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch noch der \u00e4chten Taucherhand,", "tokens": ["Doch", "noch", "der", "\u00e4ch\u00b7ten", "Tau\u00b7cher\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch ", "tokens": ["Noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Nimm sie \u2014 und wirf sie in den Sand!", "tokens": ["Nimm", "sie", "und", "wirf", "sie", "in", "den", "Sand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KON", "VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}