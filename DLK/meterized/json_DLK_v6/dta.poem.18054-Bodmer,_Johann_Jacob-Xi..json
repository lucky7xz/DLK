{"dta.poem.18054": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Xi.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es kam ein Frosch auf eine Wiesen,", "tokens": ["Es", "kam", "ein", "Frosch", "auf", "ei\u00b7ne", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da war manch wohlgemachtes Thier,", "tokens": ["Da", "war", "manch", "wohl\u00b7ge\u00b7mach\u00b7tes", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Ansehn, herrlich und gepriesen;", "tokens": ["Von", "An\u00b7sehn", ",", "herr\u00b7lich", "und", "ge\u00b7prie\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die redt er an, und sagte: Glaubt ihr mir,", "tokens": ["Die", "redt", "er", "an", ",", "und", "sag\u00b7te", ":", "Glaubt", "ihr", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$.", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So mu\u00df ich euch Artzneyen geben;", "tokens": ["So", "mu\u00df", "ich", "euch", "Artz\u00b7ne\u00b7yen", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Dadurch verl\u00e4ngert ihr das Leben.", "tokens": ["Da\u00b7durch", "ver\u00b7l\u00e4n\u00b7gert", "ihr", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch meine grosse Wissenschaft", "tokens": ["Durch", "mei\u00b7ne", "gros\u00b7se", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geb ich den Krancken neue Kraft;", "tokens": ["Geb", "ich", "den", "Kran\u00b7cken", "neu\u00b7e", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mir mag in allen K\u00f6nigreichen", "tokens": ["Mir", "mag", "in", "al\u00b7len", "K\u00f6\u00b7nig\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kein andrer Frosch an Weisheit gleichen,", "tokens": ["Kein", "an\u00b7drer", "Frosch", "an", "Weis\u00b7heit", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und selbst der Mensch hat nicht so hohe Kunst.", "tokens": ["Und", "selbst", "der", "Mensch", "hat", "nicht", "so", "ho\u00b7he", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Des hab ich aller Leute Gunst.", "tokens": ["Des", "hab", "ich", "al\u00b7ler", "Leu\u00b7te", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "PPER", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein Fuchs erwiedert drauf: Herr Frosch", "tokens": ["Ein", "Fuchs", "er\u00b7wie\u00b7dert", "drauf", ":", "Herr", "Frosch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "$.", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df sie Artzneyen k\u00f6nnen geben?", "tokens": ["Da\u00df", "sie", "Artz\u00b7ne\u00b7yen", "k\u00f6n\u00b7nen", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VMFIN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Dem widerspricht der gr\u00fcnen Farbe Schein.", "tokens": ["Dem", "wi\u00b7der\u00b7spricht", "der", "gr\u00fc\u00b7nen", "Far\u00b7be", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Verstehn sie diese Kunst, so heilen sie ihr Leben,", "tokens": ["Ver\u00b7stehn", "sie", "die\u00b7se", "Kunst", ",", "so", "hei\u00b7len", "sie", "ihr", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und ihre Kranckheit erst; sie machen sich gesund,", "tokens": ["Und", "ih\u00b7re", "Kran\u00b7ck\u00b7heit", "erst", ";", "sie", "ma\u00b7chen", "sich", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$.", "PPER", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Hernachmals mich; so wird uns kund,", "tokens": ["Her\u00b7nach\u00b7mals", "mich", ";", "so", "wird", "uns", "kund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df sie ein grosser Doctor sind.", "tokens": ["Da\u00df", "sie", "ein", "gros\u00b7ser", "Doc\u00b7tor", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Thun sie das nicht, so sind sie blind.", "tokens": ["Thun", "sie", "das", "nicht", ",", "so", "sind", "sie", "blind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}