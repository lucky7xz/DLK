{"textgrid.poem.43439": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zeitgem\u00e4\u00df und christlich sollte", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeitgem\u00e4\u00df und christlich sollte", "tokens": ["Zeit\u00b7ge\u00b7m\u00e4\u00df", "und", "christ\u00b7lich", "soll\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Jugendbildung sein,", "tokens": ["Uns\u00b7re", "Ju\u00b7gend\u00b7bil\u00b7dung", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber zeitgem\u00e4\u00df und christlich", "tokens": ["A\u00b7ber", "zeit\u00b7ge\u00b7m\u00e4\u00df", "und", "christ\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nicht Griechisch und Latein.", "tokens": ["Ist", "nicht", "Grie\u00b7chisch", "und", "La\u00b7tein."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VAFIN", "PTKNEG", "ADJD", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Singt ein christlicher Scholar:", "tokens": ["Singt", "ein", "christ\u00b7li\u00b7cher", "Scho\u00b7lar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht und sagt mal seinem Vater,", "tokens": ["Geht", "und", "sagt", "mal", "sei\u00b7nem", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was der Nikomedes war.", "tokens": ["Was", "der", "Ni\u00b7ko\u00b7me\u00b7des", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie keusche G\u00f6tterfabeln", "tokens": ["Und", "wie", "keu\u00b7sche", "G\u00f6t\u00b7ter\u00b7fa\u00b7beln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Lernt ein christlicher Scholar:", "tokens": ["Lernt", "ein", "christ\u00b7li\u00b7cher", "Scho\u00b7lar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht und sagt mal seiner Mutter,", "tokens": ["Geht", "und", "sagt", "mal", "sei\u00b7ner", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der Gott Priapus war.", "tokens": ["Was", "der", "Gott", "Pria\u00b7pus", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NE", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wie zeitgem\u00e4\u00df doch ist es,", "tokens": ["Und", "wie", "zeit\u00b7ge\u00b7m\u00e4\u00df", "doch", "ist", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ADV", "VAFIN", "PPER", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Da\u00df man fest den Knaben h\u00e4lt", "tokens": ["Da\u00df", "man", "fest", "den", "Kna\u00b7ben", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie in einem Zauberbanne", "tokens": ["Wie", "in", "ei\u00b7nem", "Zau\u00b7ber\u00b7ban\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der fremden todten Welt!", "tokens": ["In", "der", "frem\u00b7den", "tod\u00b7ten", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seinen deutschen Geist zu bilden", "tokens": ["Sei\u00b7nen", "deut\u00b7schen", "Geist", "zu", "bil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zu sch\u00e4rfen den Verstand,", "tokens": ["Und", "zu", "sch\u00e4r\u00b7fen", "den", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df er seine Jugend theilen", "tokens": ["Mu\u00df", "er", "sei\u00b7ne", "Ju\u00b7gend", "thei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen Rom und Griechenland.", "tokens": ["Zwi\u00b7schen", "Rom", "und", "Grie\u00b7chen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nie gelangt er zum Bewu\u00dftsein,", "tokens": ["Nie", "ge\u00b7langt", "er", "zum", "Be\u00b7wu\u00df\u00b7tsein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er hat ein Vaterland,", "tokens": ["Da\u00df", "er", "hat", "ein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er tritt ins deutsche Leben", "tokens": ["Und", "er", "tritt", "ins", "deut\u00b7sche", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein fremder Ignorant.", "tokens": ["Wie", "ein", "frem\u00b7der", "Ig\u00b7no\u00b7rant", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}