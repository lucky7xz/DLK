{"textgrid.poem.31958": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "66. Von der allerseligsten Jungfrauen", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der jungfr\u00e4uliche Leib, der unser Himmelsbrot", "tokens": ["Der", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Leib", ",", "der", "un\u00b7ser", "Him\u00b7mels\u00b7brot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In sich beschlossen hielt, ist wahrlich nicht mehr tot.", "tokens": ["In", "sich", "be\u00b7schlos\u00b7sen", "hielt", ",", "ist", "wahr\u00b7lich", "nicht", "mehr", "tot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "VVFIN", "$,", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es fault kein Zederbaum: so w\u00e4r es auch nicht fein,", "tokens": ["Es", "fault", "kein", "Ze\u00b7der\u00b7baum", ":", "so", "w\u00e4r", "es", "auch", "nicht", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn au\u00dferm Tempel Gotts sein Arche sollte sein. ", "tokens": ["Wenn", "au\u00b7\u00dferm", "Tem\u00b7pel", "Gotts", "sein", "Ar\u00b7che", "soll\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "NE", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}