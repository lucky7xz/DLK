{"textgrid.poem.66613": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wandrer", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brause nur, Winterwind, brause,", "tokens": ["Brau\u00b7se", "nur", ",", "Win\u00b7ter\u00b7wind", ",", "brau\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "\u00dcber die Berge, das Tal!", "tokens": ["\u00dc\u00b7ber", "die", "Ber\u00b7ge", ",", "das", "Tal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Nirgend bin ich zu Hause,", "tokens": ["Nir\u00b7gend", "bin", "ich", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Wandre nur, wandre voll Qual.", "tokens": ["Wand\u00b7re", "nur", ",", "wand\u00b7re", "voll", "Qual", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ADJD", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Rosen, Syringen und Flieder,", "tokens": ["Ro\u00b7sen", ",", "Sy\u00b7rin\u00b7gen", "und", "Flie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ach, wie so lange verbl\u00fcht!", "tokens": ["Ach", ",", "wie", "so", "lan\u00b7ge", "ver\u00b7bl\u00fcht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Frosthauch sch\u00fcttelt die Glieder \u2013", "tokens": ["Frost\u00b7hauch", "sch\u00fct\u00b7telt", "die", "Glie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bin zum Sterben so m\u00fcd.", "tokens": ["Bin", "zum", "Ster\u00b7ben", "so", "m\u00fcd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}