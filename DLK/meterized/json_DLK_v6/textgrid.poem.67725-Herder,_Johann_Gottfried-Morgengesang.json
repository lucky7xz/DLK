{"textgrid.poem.67725": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Morgengesang", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch schlummr' ich, und der Morgen graut!", "tokens": ["Noch", "schlumm\u00b7r'", "ich", ",", "und", "der", "Mor\u00b7gen", "graut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Bald h\u00e4tt' die Sonne mir", "tokens": ["Bald", "h\u00e4tt'", "die", "Son\u00b7ne", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ins Aug' gestrahlt, die uns zerstreut,", "tokens": ["Ins", "Aug'", "ge\u00b7strahlt", ",", "die", "uns", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns blendet und betr\u00fcgt.", "tokens": ["Uns", "blen\u00b7det", "und", "be\u00b7tr\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die rauhe Pindars-Doris winkt", "tokens": ["Die", "rau\u00b7he", "Pin\u00b7dar\u00b7sDo\u00b7ris", "winkt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum letzten Mal ins Feld", "tokens": ["Zum", "letz\u00b7ten", "Mal", "ins", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Ruhms und rauscht mit ihrer Eul'", "tokens": ["Des", "Ruhms", "und", "rauscht", "mit", "ih\u00b7rer", "Eul'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon lichtscheu in die Nacht.", "tokens": ["Schon", "licht\u00b7scheu", "in", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht fliehe, Pallas, die Du mich", "tokens": ["Nicht", "flie\u00b7he", ",", "Pal\u00b7las", ",", "die", "Du", "mich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$,", "NE", "$,", "PRELS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umsonst bei Mitternacht", "tokens": ["Um\u00b7sonst", "bei", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geweckt! nimm meine sp\u00e4te Reu'", "tokens": ["Ge\u00b7weckt", "!", "nimm", "mei\u00b7ne", "sp\u00e4\u00b7te", "Reu'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und diesen Opferrauch", "tokens": ["Und", "die\u00b7sen", "Op\u00b7fer\u00b7rauch"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und zieh ihn, bis Du wiederkommst,", "tokens": ["Und", "zieh", "ihn", ",", "bis", "Du", "wie\u00b7der\u00b7kommst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rund um mich her zur Nacht,", "tokens": ["Rund", "um", "mich", "her", "zur", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "APZR", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df ich, gesch\u00fctzt vor Glanz und Gold", "tokens": ["Da\u00df", "ich", ",", "ge\u00b7sch\u00fctzt", "vor", "Glanz", "und", "Gold"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Thoren, mich nur seh'", "tokens": ["Und", "Tho\u00b7ren", ",", "mich", "nur", "seh'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und Dich und Jene, deren Bild", "tokens": ["Und", "Dich", "und", "Je\u00b7ne", ",", "de\u00b7ren", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "KON", "PDS", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die tr\u00e4ge Z\u00e4hre k\u00fc\u00dft,", "tokens": ["Die", "tr\u00e4\u00b7ge", "Z\u00e4h\u00b7re", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auf Dich wart' und, wenn Du kommst,", "tokens": ["Und", "auf", "Dich", "wart'", "und", ",", "wenn", "Du", "kommst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "KON", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich, ich zu sein, entschlie\u00df'.", "tokens": ["Mich", ",", "ich", "zu", "sein", ",", "ent\u00b7schlie\u00df'", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPER", "PTKZU", "VAINF", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}