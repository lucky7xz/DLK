{"textgrid.poem.63023": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Faust bekam einst Lust, den Blocksberg zu befahren:", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Faust bekam einst Lust, den Blocksberg zu befahren:", "tokens": ["Herr", "Faust", "be\u00b7kam", "einst", "Lust", ",", "den", "Blocks\u00b7berg", "zu", "be\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADV", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ritt auf seinem Mantel hin", "tokens": ["Er", "ritt", "auf", "sei\u00b7nem", "Man\u00b7tel", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sah die Hexen, wie Husaren,", "tokens": ["Und", "sah", "die", "He\u00b7xen", ",", "wie", "Hu\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf B\u00f6cken zur Parade ziehn.", "tokens": ["Auf", "B\u00f6\u00b7cken", "zur", "Pa\u00b7ra\u00b7de", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Aufzug kam dem schlauen Geisterkenner", "tokens": ["Der", "Auf\u00b7zug", "kam", "dem", "schlau\u00b7en", "Geis\u00b7ter\u00b7ken\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verd\u00e4chtig vor. Mit einem Talisman", "tokens": ["Ver\u00b7d\u00e4ch\u00b7tig", "vor", ".", "Mit", "ei\u00b7nem", "Ta\u00b7lis\u00b7man"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "R\u00fchrt er der Elfen Schaar geh\u00f6rnte Klepper an;", "tokens": ["R\u00fchrt", "er", "der", "El\u00b7fen", "Schaar", "ge\u00b7h\u00f6rn\u00b7te", "Klep\u00b7per", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und siehe da, es waren ihre M\u00e4nner.", "tokens": ["Und", "sie\u00b7he", "da", ",", "es", "wa\u00b7ren", "ih\u00b7re", "M\u00e4n\u00b7ner", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}