{"dta.poem.21513": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "\u201elicht den Lebendigen!\u201c", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Stets habe ich mich denen zugesellt,", "tokens": ["Stets", "ha\u00b7be", "ich", "mich", "de\u00b7nen", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die, ausgesto\u00dfen, nur des Tempels Stufen", "tokens": ["Die", ",", "aus\u00b7ge\u00b7sto\u00b7\u00dfen", ",", "nur", "des", "Tem\u00b7pels", "Stu\u00b7fen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "VVIZU", "$,", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nie das Allerheiligste betreten \u2026", "tokens": ["Und", "nie", "das", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7te", "be\u00b7tre\u00b7ten", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umsonst erklingt ihr banges H\u00fclferufen,", "tokens": ["Um\u00b7sonst", "er\u00b7klingt", "ihr", "ban\u00b7ges", "H\u00fcl\u00b7fe\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Umsonst springt von den Lippen br\u00fcnstig Beten,", "tokens": ["Um\u00b7sonst", "springt", "von", "den", "Lip\u00b7pen", "br\u00fcns\u00b7tig", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Umsonst ersteht aus ihnen \u2014 ach! \u2014 ein ", "tokens": ["Um\u00b7sonst", "er\u00b7steht", "aus", "ih\u00b7nen", "ach", "!", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$(", "ITJ", "$.", "$(", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sie aus ihrer Knechtschaft an das Licht", "tokens": ["Der", "sie", "aus", "ih\u00b7rer", "Knecht\u00b7schaft", "an", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der gold\u2019nen Freiheit f\u00fchren will \u2014 ein ", "tokens": ["Der", "gold'\u00b7nen", "Frei\u00b7heit", "f\u00fch\u00b7ren", "will", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$(", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er f\u00e4llt im Kampf wie ein gemeiner Krieger \u2014", "tokens": ["Er", "f\u00e4llt", "im", "Kampf", "wie", "ein", "ge\u00b7mei\u00b7ner", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Doch die Galeerenketten bricht er nicht! \u2026", "tokens": ["Doch", "die", "Ga\u00b7lee\u00b7ren\u00b7ket\u00b7ten", "bricht", "er", "nicht", "!", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Er bricht den Fluch nicht, der auf ihnen liegt", "tokens": ["Er", "bricht", "den", "Fluch", "nicht", ",", "der", "auf", "ih\u00b7nen", "liegt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PRELS", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Von Anbeginn der Welt als ein Verh\u00e4ngni\u00df \u2014", "tokens": ["Von", "An\u00b7be\u00b7ginn", "der", "Welt", "als", "ein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das Leben ist f\u00fcr sie nur ein Gef\u00e4ngni\u00df \u2014", "tokens": ["Das", "Le\u00b7ben", "ist", "f\u00fcr", "sie", "nur", "ein", "Ge\u00b7f\u00e4ng\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sie sterben in der Tiefe \u2014 Keiner siegt!", "tokens": ["Sie", "ster\u00b7ben", "in", "der", "Tie\u00b7fe", "Kei\u00b7ner", "siegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "PIS", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Stets habe ich mich ihnen zugesellt:", "tokens": ["Stets", "ha\u00b7be", "ich", "mich", "ih\u00b7nen", "zu\u00b7ge\u00b7sellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Frommt dem Poeten denn \u2014 ich frag\u2019 es dreist \u2014", "tokens": ["Frommt", "dem", "Po\u00b7et\u00b7en", "denn", "ich", "frag'", "es", "dreist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "$(", "PPER", "VVFIN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ein ander Loos? Wo sich in bangen Qualen", "tokens": ["Ein", "an\u00b7der", "Loos", "?", "Wo", "sich", "in", "ban\u00b7gen", "Qua\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$.", "PWAV", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um nie gel\u00f6ste R\u00e4thsel m\u00fcht ein Geist;", "tokens": ["Um", "nie", "ge\u00b7l\u00f6s\u00b7te", "R\u00e4th\u00b7sel", "m\u00fcht", "ein", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo auf die Wangen, die verfallnen, fahlen,", "tokens": ["Wo", "auf", "die", "Wan\u00b7gen", ",", "die", "ver\u00b7fall\u00b7nen", ",", "fah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Hunger seine Fingerspur gepr\u00e4gt;", "tokens": ["Der", "Hun\u00b7ger", "sei\u00b7ne", "Fin\u00b7ger\u00b7spur", "ge\u00b7pr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo sich in wildem Ingrimm eine Hand", "tokens": ["Wo", "sich", "in", "wil\u00b7dem", "In\u00b7grimm", "ei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zur Faust zusammenballt; wo, stets verkannt,", "tokens": ["Zur", "Faust", "zu\u00b7sam\u00b7men\u00b7ballt", ";", "wo", ",", "stets", "ver\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$.", "PWAV", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein Mann im Innersten Emp\u00f6rung hegt \u2014", "tokens": ["Ein", "Mann", "im", "In\u00b7ners\u00b7ten", "Em\u00b7p\u00f6\u00b7rung", "hegt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "NN", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Emp\u00f6rung gegen sie, die Kettenschmieder:", "tokens": ["Em\u00b7p\u00f6\u00b7rung", "ge\u00b7gen", "sie", ",", "die", "Ket\u00b7ten\u00b7schmie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da tret\u2019 ich hin und singe meine Lieder \u2014", "tokens": ["Da", "tret'", "ich", "hin", "und", "sin\u00b7ge", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ja! Lieder, die ich nicht erk\u00fcnstelt und erdacht,", "tokens": ["Ja", "!", "Lie\u00b7der", ",", "die", "ich", "nicht", "er\u00b7k\u00fcns\u00b7telt", "und", "er\u00b7dacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die ich aus tiefstem Seelenschacht,", "tokens": ["Die", "ich", "aus", "tiefs\u00b7tem", "See\u00b7len\u00b7schacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Aus meiner ", "tokens": ["Aus", "mei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Und was ich nicht gef\u00fchlt, das sing\u2019 ich nicht!", "tokens": ["Und", "was", "ich", "nicht", "ge\u00b7f\u00fchlt", ",", "das", "sing'", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVPP", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl soll des S\u00e4ngers Lied auf Wunden leise", "tokens": ["Wohl", "soll", "des", "S\u00e4n\u00b7gers", "Lied", "auf", "Wun\u00b7den", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Balsam legen! Von den Stirnen banne", "tokens": ["Den", "Bal\u00b7sam", "le\u00b7gen", "!", "Von", "den", "Stir\u00b7nen", "ban\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Furchen es und Thr\u00e4nen aus den Augen . . . .", "tokens": ["Die", "Fur\u00b7chen", "es", "und", "Thr\u00e4\u00b7nen", "aus", "den", "Au\u00b7gen", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "PPER", "KON", "NN", "APPR", "ART", "NN", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch giebt\u2019s auch Lieder, die ", "tokens": ["Doch", "giebt's", "auch", "Lie\u00b7der", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "NN", "$,", "PRELS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ragen trotzig wie die Wettertanne,               ", "tokens": ["Sie", "ra\u00b7gen", "trot\u00b7zig", "wie", "die", "Wet\u00b7ter\u00b7tan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie zucken wie der Blitz mit loh'nden Zungen,               ", "tokens": ["Sie", "zu\u00b7cken", "wie", "der", "Blitz", "mit", "loh'n\u00b7den", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie hallen wie der Donner krachend hallt &#8212;               ", "tokens": ["Sie", "hal\u00b7len", "wie", "der", "Don\u00b7ner", "kra\u00b7chend", "hallt", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie singen von der Schergen Allgewalt,", "tokens": ["Sie", "sin\u00b7gen", "von", "der", "Scher\u00b7gen", "All\u00b7ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von Buben, die der Knechtschaft sich verdungen!", "tokens": ["Von", "Bu\u00b7ben", ",", "die", "der", "Knecht\u00b7schaft", "sich", "ver\u00b7dun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sie singen eine einz\u2019ge Weise nur:", "tokens": ["Sie", "sin\u00b7gen", "ei\u00b7ne", "einz'\u00b7ge", "Wei\u00b7se", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Weise der Emp\u00f6rung gen Despoten!", "tokens": ["Die", "Wei\u00b7se", "der", "Em\u00b7p\u00f6\u00b7rung", "gen", "Des\u00b7po\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie flammen wild zusammen zu dem Schwur:", "tokens": ["Sie", "flam\u00b7men", "wild", "zu\u00b7sam\u00b7men", "zu", "dem", "Schwur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Licht den Lebendigen &#8212; die Nacht den Todten! &#8230;             ", "tokens": ["Licht", "den", "Le\u00b7ben\u00b7di\u00b7gen", "&#8212;", "die", "Nacht", "den", "Tod\u00b7ten", "!", "&#8230;"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["NN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}}}}