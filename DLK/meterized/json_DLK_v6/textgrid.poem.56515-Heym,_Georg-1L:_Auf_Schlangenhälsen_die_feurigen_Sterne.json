{"textgrid.poem.56515": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf Schlangenh\u00e4lsen die feurigen Sterne", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Schlangenh\u00e4lsen die feurigen Sterne", "tokens": ["Auf", "Schlan\u00b7gen\u00b7h\u00e4l\u00b7sen", "die", "feu\u00b7ri\u00b7gen", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00e4ngen herunter auf schwankende T\u00fcrme,", "tokens": ["H\u00e4n\u00b7gen", "her\u00b7un\u00b7ter", "auf", "schwan\u00b7ken\u00b7de", "T\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Die D\u00e4cher gegei\u00dfelt. Und Feuer springet,", "tokens": ["Die", "D\u00e4\u00b7cher", "ge\u00b7gei\u00b7\u00dfelt", ".", "Und", "Feu\u00b7er", "sprin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Wie ein Gespenst durch die Gasse der St\u00fcrme.", "tokens": ["Wie", "ein", "Ge\u00b7spenst", "durch", "die", "Gas\u00b7se", "der", "St\u00fcr\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Fenster schlagen mit Macht. Und Mauern, die alten,", "tokens": ["Fens\u00b7ter", "schla\u00b7gen", "mit", "Macht", ".", "Und", "Mau\u00b7ern", ",", "die", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$.", "KON", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Rei\u00dfen die Tore auf in zahnlosem Munde.", "tokens": ["Rei\u00b7\u00dfen", "die", "To\u00b7re", "auf", "in", "zahn\u00b7lo\u00b7sem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Aber die Br\u00fccken fallen \u00fcber dem Schlunde", "tokens": ["A\u00b7ber", "die", "Br\u00fc\u00b7cken", "fal\u00b7len", "\u00fc\u00b7ber", "dem", "Schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und der Tod stehet drau\u00dfen, der Alte.", "tokens": ["Und", "der", "Tod", "ste\u00b7het", "drau\u00b7\u00dfen", ",", "der", "Al\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,", "ART", "NN", "$."], "meter": "--++-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber die Menschen rennen, ohne zu wissen", "tokens": ["A\u00b7ber", "die", "Men\u00b7schen", "ren\u00b7nen", ",", "oh\u00b7ne", "zu", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Blind und schreiend, mit Schwertern und Lanzen.", "tokens": ["Blind", "und", "schrei\u00b7end", ",", "mit", "Schwer\u00b7tern", "und", "Lan\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Unten hallet es dumpf, und die Glocken tanzen,", "tokens": ["Un\u00b7ten", "hal\u00b7let", "es", "dumpf", ",", "und", "die", "Glo\u00b7cken", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Schlagend laut auf, von den Winden gerissen.", "tokens": ["Schla\u00b7gend", "laut", "auf", ",", "von", "den", "Win\u00b7den", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKVZ", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Pl\u00e4tze sind rot und tot. Und riesige Monde", "tokens": ["Die", "Pl\u00e4t\u00b7ze", "sind", "rot", "und", "tot", ".", "Und", "rie\u00b7si\u00b7ge", "Mon\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "KON", "ADJA", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Steigen \u00fcber die D\u00e4cher mit steifen Beinen", "tokens": ["Stei\u00b7gen", "\u00fc\u00b7ber", "die", "D\u00e4\u00b7cher", "mit", "stei\u00b7fen", "Bei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Den fiebernden Schl\u00e4fern tief in die Kammer zu scheinen,", "tokens": ["Den", "fie\u00b7bern\u00b7den", "Schl\u00e4\u00b7fern", "tief", "in", "die", "Kam\u00b7mer", "zu", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und die Stirne wird fahl wie frierendes Leinen.", "tokens": ["Und", "die", "Stir\u00b7ne", "wird", "fahl", "wie", "frie\u00b7ren\u00b7des", "Lei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "KOKOM", "ADJA", "NN", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}}}}}