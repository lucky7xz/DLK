{"textgrid.poem.66567": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wir die Menschen sehn, nicht wie sie sind,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wir die Menschen sehn, nicht wie sie sind,", "tokens": ["Wie", "wir", "die", "Men\u00b7schen", "sehn", ",", "nicht", "wie", "sie", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$,", "PTKNEG", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So lieben wir sie. Unser tiefstes Sehen", "tokens": ["So", "lie\u00b7ben", "wir", "sie", ".", "Un\u00b7ser", "tiefs\u00b7tes", "Se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist, wo wir lieben, kinderselig blind,", "tokens": ["Ist", ",", "wo", "wir", "lie\u00b7ben", ",", "kin\u00b7der\u00b7se\u00b7lig", "blind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mag nur mit dem Herzen sich verstehen.", "tokens": ["Und", "mag", "nur", "mit", "dem", "Her\u00b7zen", "sich", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erkenntni\u00df selbst wird eingeh\u00fcllt geschwind,", "tokens": ["Er\u00b7kennt\u00b7ni\u00df", "selbst", "wird", "ein\u00b7ge\u00b7h\u00fcllt", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In sch\u00f6nem Trug mit uns einher zu gehen.", "tokens": ["In", "sch\u00f6\u00b7nem", "Trug", "mit", "uns", "ein\u00b7her", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie reich die Armuth, die das Herz verschwendet!", "tokens": ["Wie", "reich", "die", "Ar\u00b7muth", ",", "die", "das", "Herz", "ver\u00b7schwen\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie arm der Reichthum, wenn der Trug sich wendet!", "tokens": ["Wie", "arm", "der", "Reicht\u00b7hum", ",", "wenn", "der", "Trug", "sich", "wen\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}