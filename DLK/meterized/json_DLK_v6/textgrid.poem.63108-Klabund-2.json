{"textgrid.poem.63108": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, verloren ist verloren \u2013", "tokens": ["Ach", ",", "ver\u00b7lo\u00b7ren", "ist", "ver\u00b7lo\u00b7ren", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVPP", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unaufhaltsam ziehn die Fluten.", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "ziehn", "die", "Flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer dahier zu sp\u00e4t geboren,", "tokens": ["Wer", "da\u00b7hier", "zu", "sp\u00e4t", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt zu sp\u00e4t zu allem Guten.", "tokens": ["Kommt", "zu", "sp\u00e4t", "zu", "al\u00b7lem", "Gu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKA", "ADJD", "APPR", "PIS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, ihn sollt der Teufel holen,", "tokens": ["Ja", ",", "ihn", "sollt", "der", "Teu\u00b7fel", "ho\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Selbst sein Weib: hat schon ein anderer.", "tokens": ["Selbst", "sein", "Weib", ":", "hat", "schon", "ein", "an\u00b7de\u00b7rer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$.", "VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Als ein k\u00fcmmerlicher Wanderer", "tokens": ["Als", "ein", "k\u00fcm\u00b7mer\u00b7li\u00b7cher", "Wan\u00b7de\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Tippelt er auf blanken Sohlen.", "tokens": ["Tip\u00b7pelt", "er", "auf", "blan\u00b7ken", "Soh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, verloren ist verloren \u2013", "tokens": ["Ach", ",", "ver\u00b7lo\u00b7ren", "ist", "ver\u00b7lo\u00b7ren", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVPP", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die schwarzen W\u00fcrfel fallen.", "tokens": ["La\u00df", "die", "schwar\u00b7zen", "W\u00fcr\u00b7fel", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einmal bist du doch erkoren,", "tokens": ["Ein\u00b7mal", "bist", "du", "doch", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die schrillen Fl\u00f6ten schallen.", "tokens": ["Wenn", "die", "schril\u00b7len", "Fl\u00f6\u00b7ten", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Setz dein Sein auf eine Karte:", "tokens": ["Setz", "dein", "Sein", "auf", "ei\u00b7ne", "Kar\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weib und Kind und Gott daneben \u2013", "tokens": ["Weib", "und", "Kind", "und", "Gott", "da\u00b7ne\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "PAV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur im Tode darfst du leben,", "tokens": ["Nur", "im", "To\u00b7de", "darfst", "du", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mors, entfalte die Standarte!", "tokens": ["Mors", ",", "ent\u00b7fal\u00b7te", "die", "Stand\u00b7ar\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}