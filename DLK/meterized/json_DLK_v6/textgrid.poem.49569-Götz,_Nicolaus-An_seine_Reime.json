{"textgrid.poem.49569": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "An seine Reime", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Geschenke der Natur,", "tokens": ["Ihr", "Ge\u00b7schen\u00b7ke", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufgesucht auf Berg und Flur,", "tokens": ["Auf\u00b7ge\u00b7sucht", "auf", "Berg", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klein von Geist, als wie von Leib,", "tokens": ["Klein", "von", "Geist", ",", "als", "wie", "von", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "KOUS", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unschuldvoller Zeitvertreib,", "tokens": ["Un\u00b7schuld\u00b7vol\u00b7ler", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Reime, meiner Jugend Ruhm,", "tokens": ["Rei\u00b7me", ",", "mei\u00b7ner", "Ju\u00b7gend", "Ruhm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein einzig Eigenthum;", "tokens": ["Und", "mein", "ein\u00b7zig", "Ei\u00b7gen\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Von Apollen nicht gezeugt,", "tokens": ["Von", "A\u00b7pol\u00b7len", "nicht", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Von den Musen nicht ges\u00e4ugt,", "tokens": ["Von", "den", "Mu\u00b7sen", "nicht", "ge\u00b7s\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Nur an Amors Seit' erdacht,", "tokens": ["Nur", "an", "A\u00b7mors", "Seit'", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Nur in Fr\u00f6hlichkeit gemacht,", "tokens": ["Nur", "in", "Fr\u00f6h\u00b7lich\u00b7keit", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wandelt, holde Kinderchen,", "tokens": ["Wan\u00b7delt", ",", "hol\u00b7de", "Kin\u00b7der\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.12": {"text": "Ferne von Pedanterie,", "tokens": ["Fer\u00b7ne", "von", "Pe\u00b7dan\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Immer mit den Grazien,", "tokens": ["Im\u00b7mer", "mit", "den", "Gra\u00b7zi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Immer mit der Harmonie!..", "tokens": ["Im\u00b7mer", "mit", "der", "Har\u00b7mo\u00b7nie", "!", ".."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Wenn euch Jemand k\u00fcssen will,", "tokens": ["Wenn", "euch", "Je\u00b7mand", "k\u00fcs\u00b7sen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "O so haltet niemahls still!", "tokens": ["O", "so", "hal\u00b7tet", "nie\u00b7mahls", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Fliehet mit Bescheidenheit!", "tokens": ["Flie\u00b7het", "mit", "Be\u00b7schei\u00b7den\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Sagt, da\u00df ihr, voll Niedrigkeit", "tokens": ["Sagt", ",", "da\u00df", "ihr", ",", "voll", "Nied\u00b7rig\u00b7keit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "$,", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Keiner K\u00fc\u00dfe w\u00fcrdig seid!", "tokens": ["Kei\u00b7ner", "K\u00fc\u00b7\u00dfe", "w\u00fcr\u00b7dig", "seid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}