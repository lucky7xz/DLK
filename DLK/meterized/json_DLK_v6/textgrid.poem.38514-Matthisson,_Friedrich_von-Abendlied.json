{"textgrid.poem.38514": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abend schleiert Flur und Hain", "tokens": ["Der", "A\u00b7bend", "schlei\u00b7ert", "Flur", "und", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In traulichholde D\u00e4mrung ein,", "tokens": ["In", "trau\u00b7lich\u00b7hol\u00b7de", "D\u00e4m\u00b7rung", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Manch W\u00f6lklein hell im Westen schwimt,", "tokens": ["Manch", "W\u00f6lk\u00b7lein", "hell", "im", "Wes\u00b7ten", "schwimt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom sanften Liebesstern durchflimt!", "tokens": ["Vom", "sanf\u00b7ten", "Lie\u00b7bess\u00b7tern", "durch\u00b7flimt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Die Wogenflut t\u00f6nt Schlummerklang,", "tokens": ["Die", "Wo\u00b7gen\u00b7flut", "t\u00f6nt", "Schlum\u00b7mer\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die B\u00e4ume lispeln Abendsang,", "tokens": ["Die", "B\u00e4u\u00b7me", "lis\u00b7peln", "A\u00b7bend\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Wiesengras durchhaucht gelind", "tokens": ["Das", "Wie\u00b7sen\u00b7gras", "durch\u00b7haucht", "ge\u00b7lind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der liebe Sommerabendwind!", "tokens": ["Der", "lie\u00b7be", "Som\u00b7mer\u00b7a\u00b7bend\u00b7wind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Geist der Liebe wirkt und webt", "tokens": ["Der", "Geist", "der", "Lie\u00b7be", "wirkt", "und", "webt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In allem was sich regt und lebt!", "tokens": ["In", "al\u00b7lem", "was", "sich", "regt", "und", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Meer, wo Wog' in Woge flie\u00dft,", "tokens": ["Im", "Meer", ",", "wo", "Wo\u00b7g'", "in", "Wo\u00b7ge", "flie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Hain, wo Blat an Blat sich schlie\u00dft!", "tokens": ["Im", "Hain", ",", "wo", "Blat", "an", "Blat", "sich", "schlie\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Geist der Liebe! f\u00fchre du", "tokens": ["O", "Geist", "der", "Lie\u00b7be", "!", "f\u00fch\u00b7re", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir meine fromme Maja zu,", "tokens": ["Mir", "mei\u00b7ne", "from\u00b7me", "Ma\u00b7ja", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihr, bei dieser Sterne Schein,", "tokens": ["Mit", "ihr", ",", "bei", "die\u00b7ser", "Ster\u00b7ne", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sch\u00f6pfung Gottes mich zu freun!", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Got\u00b7tes", "mich", "zu", "freun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}