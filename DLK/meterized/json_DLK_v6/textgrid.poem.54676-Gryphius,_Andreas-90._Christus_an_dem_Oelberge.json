{"textgrid.poem.54676": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "90. Christus an dem Oelberge", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier brennt mein eigne Lieb' und dort des Vatern Gri i/", "tokens": ["Hier", "brennt", "mein", "eig\u00b7ne", "Lieb'", "und", "dort", "des", "Va\u00b7tern", "Gri", "i", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "KON", "ADV", "ART", "NN", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da f\u00fchl ich wie die Glutt der tiffsten H\u00f6ll entgli i/", "tokens": ["Da", "f\u00fchl", "ich", "wie", "die", "Glutt", "der", "tiffs\u00b7ten", "H\u00f6ll", "ent\u00b7gli", "i", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da/ wie der Menschen Zorn sich auff mich scharff erhitze.", "tokens": ["Da", "/", "wie", "der", "Men\u00b7schen", "Zorn", "sich", "auff", "mich", "scharff", "er\u00b7hit\u00b7ze", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOKOM", "ART", "NN", "NN", "PRF", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wen wunderts da\u00df ich nur Blutt/ nicht die Seel ausschwitze!", "tokens": ["Wen", "wun\u00b7derts", "da\u00df", "ich", "nur", "Blutt", "/", "nicht", "die", "Seel", "aus\u00b7schwit\u00b7ze", "!"], "token_info": ["word", "word", "word", "wor