{"textgrid.poem.45237": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[droht nicht und schreit nicht Ach und Weh]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Droht nicht und schreit nicht Ach und Weh,", "tokens": ["Droht", "nicht", "und", "schreit", "nicht", "Ach", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KON", "VVFIN", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tauben wird umsonst gepredigt,", "tokens": ["Den", "Tau\u00b7ben", "wird", "um\u00b7sonst", "ge\u00b7pre\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die \u00dcbertreibung ist wie der Schnee,", "tokens": ["Die", "\u00dc\u00b7bert\u00b7rei\u00b7bung", "ist", "wie", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der durch die Zeit sich von selbst erledigt.", "tokens": ["Der", "durch", "die", "Zeit", "sich", "von", "selbst", "er\u00b7le\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}