{"textgrid.poem.64249": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Deutschen im Auslande", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Deutschen unter fremden Sternen,", "tokens": ["Ihr", "Deut\u00b7schen", "un\u00b7ter", "frem\u00b7den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meergeschiedenen weiten Fernen,", "tokens": ["In", "meer\u00b7ge\u00b7schie\u00b7de\u00b7nen", "wei\u00b7ten", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr sollt die Sprache nie verlernen.", "tokens": ["Ihr", "sollt", "die", "Spra\u00b7che", "nie", "ver\u00b7ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die wohllautreiche, starke, milde,", "tokens": ["Die", "wohl\u00b7laut\u00b7rei\u00b7che", ",", "star\u00b7ke", ",", "mil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die sch\u00f6nheitvollen Klanggebilde,", "tokens": ["Die", "sch\u00f6n\u00b7heit\u00b7vol\u00b7len", "Klang\u00b7ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die in des alten Lands Gefilde", "tokens": ["Die", "in", "des", "al\u00b7ten", "Lands", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dereinst zu euch die Mutter sprach;", "tokens": ["De\u00b7reinst", "zu", "euch", "die", "Mut\u00b7ter", "sprach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In euren Herzen t\u00f6nt sie nach: \u2013", "tokens": ["In", "eu\u00b7ren", "Her\u00b7zen", "t\u00f6nt", "sie", "nach", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer sie vergi\u00dft \u2013 dem Weh und Schmach! \u2013", "tokens": ["Wer", "sie", "ver\u00b7gi\u00dft", "\u2013", "dem", "Weh", "und", "Schmach", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "ART", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Sprache Shakespeares tr\u00e4gt der Britte \u2013", "tokens": ["Die", "Spra\u00b7che", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7res", "tr\u00e4gt", "der", "Brit\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ich lob' ihn drum! \u2013 wie seine Sitte", "tokens": ["Ich", "lob'", "ihn", "drum", "!", "\u2013", "wie", "sei\u00b7ne", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "$.", "$(", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Getreu in fremder Lande Mitte:", "tokens": ["Ge\u00b7treu", "in", "frem\u00b7der", "Lan\u00b7de", "Mit\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und Schiller soll vergessen sein? \u2013", "tokens": ["Und", "Schil\u00b7ler", "soll", "ver\u00b7ges\u00b7sen", "sein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ihr deutschen M\u00e4nner rufet: \u00bbNein!\u00ab", "tokens": ["Ihr", "deut\u00b7schen", "M\u00e4n\u00b7ner", "ru\u00b7fet", ":", "\u00bb", "Nein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$(", "PTKANT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ihr deutschen Frauen, stimmet ein,", "tokens": ["Ihr", "deut\u00b7schen", "Frau\u00b7en", ",", "stim\u00b7met", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und eure M\u00e4dchen soll'n und Knaben", "tokens": ["Und", "eu\u00b7re", "M\u00e4d\u00b7chen", "soll'n", "und", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Als k\u00f6stlichste von allen Gaben", "tokens": ["Als", "k\u00f6st\u00b7lichs\u00b7te", "von", "al\u00b7len", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Das Kleinod deutscher Sprache haben!", "tokens": ["Das", "Klei\u00b7nod", "deut\u00b7scher", "Spra\u00b7che", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}