{"textgrid.poem.64381": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "104. Die Grenze", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Grenze? Nein nicht Grenze, du alter Rhein!", "tokens": ["Du", "Gren\u00b7ze", "?", "Nein", "nicht", "Gren\u00b7ze", ",", "du", "al\u00b7ter", "Rhein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "PTKANT", "PTKNEG", "NN", "$,", "PPER", "ADJA", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du Lebensblut, dem Herzen Teutoniens", "tokens": ["Du", "Le\u00b7bens\u00b7blut", ",", "dem", "Her\u00b7zen", "Teu\u00b7to\u00b7ni\u00b7ens"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entstr\u00f6mend, beiden Ufern Segen", "tokens": ["Ent\u00b7str\u00f6\u00b7mend", ",", "bei\u00b7den", "U\u00b7fern", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spendend, und hohes Gef\u00fchl, und Freude!", "tokens": ["Spen\u00b7dend", ",", "und", "ho\u00b7hes", "Ge\u00b7f\u00fchl", ",", "und", "Freu\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADJA", "NN", "$,", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Du deutscher Urart, m\u00e4chtiger Rhein! Dein Strom", "tokens": ["Du", "deut\u00b7scher", "Ur\u00b7art", ",", "m\u00e4ch\u00b7ti\u00b7ger", "Rhein", "!", "Dein", "Strom"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "NE", "$.", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist gro\u00df und hehr, nicht rauschend dem Ohre, schnell", "tokens": ["Ist", "gro\u00df", "und", "hehr", ",", "nicht", "rau\u00b7schend", "dem", "Oh\u00b7re", ",", "schnell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$,", "PTKNEG", "ADJD", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In stiller Eile, deine Wirbel", "tokens": ["In", "stil\u00b7ler", "Ei\u00b7le", ",", "dei\u00b7ne", "Wir\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprudeln nicht auf, und sind unaufhaltsam;", "tokens": ["Spru\u00b7deln", "nicht", "auf", ",", "und", "sind", "un\u00b7auf\u00b7halt\u00b7sam", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$,", "KON", "VAFIN", "ADJD", "$."], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sind tief wie Meer, wie Gottes Geschosse schnell", "tokens": ["Sind", "tief", "wie", "Meer", ",", "wie", "Got\u00b7tes", "Ge\u00b7schos\u00b7se", "schnell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN", "$,", "PWAV", "NN", "NN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und kraftvoll, doch befreundend dem flachen Flo\u00df,", "tokens": ["Und", "kraft\u00b7voll", ",", "doch", "be\u00b7freun\u00b7dend", "dem", "fla\u00b7chen", "Flo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das deinen Wogen sich vertrauend,", "tokens": ["Das", "dei\u00b7nen", "Wo\u00b7gen", "sich", "ver\u00b7trau\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fclle des Landes den St\u00e4dten zuf\u00fchrt.", "tokens": ["F\u00fcl\u00b7le", "des", "Lan\u00b7des", "den", "St\u00e4d\u00b7ten", "zu\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Als Gott der Herr die Veste von Fluten schied,", "tokens": ["Als", "Gott", "der", "Herr", "die", "Ves\u00b7te", "von", "Flu\u00b7ten", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Inseln aus der Tiefe sich heben hie\u00df,", "tokens": ["Und", "In\u00b7seln", "aus", "der", "Tie\u00b7fe", "sich", "he\u00b7ben", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Quellen aus dem Scho\u00df der Berge", "tokens": ["Und", "Quel\u00b7len", "aus", "dem", "Scho\u00df", "der", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rief, und dem Ocean Grenze stellte;", "tokens": ["Rief", ",", "und", "dem", "O\u00b7cean", "Gren\u00b7ze", "stell\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gesetz dem Sturme sprach; als das junge Licht", "tokens": ["Ge\u00b7setz", "dem", "Stur\u00b7me", "sprach", ";", "als", "das", "jun\u00b7ge", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die neue Sch\u00f6pfung, welcher es Sch\u00f6ne gab,", "tokens": ["Die", "neu\u00b7e", "Sch\u00f6p\u00b7fung", ",", "wel\u00b7cher", "es", "Sch\u00f6\u00b7ne", "gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Anstaunte: da verweilte freundlich", "tokens": ["An\u00b7staun\u00b7te", ":", "da", "ver\u00b7weil\u00b7te", "freund\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber dem Rhein, und des Rheines Ufern,", "tokens": ["\u00dc\u00b7ber", "dem", "Rhein", ",", "und", "des", "Rhei\u00b7nes", "U\u00b7fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Sein Wonnestrahl; durchdrang mit des Urlichts Kraft", "tokens": ["Sein", "Won\u00b7nes\u00b7trahl", ";", "durch\u00b7drang", "mit", "des", "Ur\u00b7lichts", "Kraft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der rhein'schen Berge Scho\u00df. Er empfing, und barg", "tokens": ["Der", "rhein'\u00b7schen", "Ber\u00b7ge", "Scho\u00df", ".", "Er", "emp\u00b7fing", ",", "und", "barg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "PPER", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Gabe, bis aus Gold und Purpur", "tokens": ["Die", "Ga\u00b7be", ",", "bis", "aus", "Gold", "und", "Pur\u00b7pur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4ufelte Labsal von deutschen Reben,", "tokens": ["Tr\u00e4u\u00b7fel\u00b7te", "Lab\u00b7sal", "von", "deut\u00b7schen", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Des Rheines wert, des Deutschen auch wert! voll Kraft,", "tokens": ["Des", "Rhei\u00b7nes", "wert", ",", "des", "Deut\u00b7schen", "auch", "wert", "!", "voll", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "ADJD", "$.", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu That entflammend und zu Gesang, nicht Schaum", "tokens": ["Zu", "That", "ent\u00b7flam\u00b7mend", "und", "zu", "Ge\u00b7sang", ",", "nicht", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVPP", "KON", "APPR", "NN", "$,", "PTKNEG", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aufsprudelnd, lebenduftend, Helle", "tokens": ["Auf\u00b7spru\u00b7delnd", ",", "le\u00b7ben\u00b7duf\u00b7tend", ",", "Hel\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["VVPP", "$,", "ADJD", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Strahlend dem Geist und das Herz durchgl\u00fchend.", "tokens": ["Strah\u00b7lend", "dem", "Geist", "und", "das", "Herz", "durch\u00b7gl\u00fc\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "An beiden Ufern ranket die Freude! gl\u00fcht", "tokens": ["An", "bei\u00b7den", "U\u00b7fern", "ran\u00b7ket", "die", "Freu\u00b7de", "!", "gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$.", "VVFIN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf hohen Felsen, spielet im Blumenthal,", "tokens": ["Auf", "ho\u00b7hen", "Fel\u00b7sen", ",", "spie\u00b7let", "im", "Blu\u00b7ment\u00b7hal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hier K\u00fchlung aus des Alten Wogen", "tokens": ["Hier", "K\u00fch\u00b7lung", "aus", "des", "Al\u00b7ten", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Saugend, sich kr\u00e4ftiger dort entflammend!", "tokens": ["Sau\u00b7gend", ",", "sich", "kr\u00e4f\u00b7ti\u00b7ger", "dort", "ent\u00b7flam\u00b7mend", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "ADJD", "ADV", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "An beiden Ufern t\u00f6net des Deutschen Sinn", "tokens": ["An", "bei\u00b7den", "U\u00b7fern", "t\u00f6\u00b7net", "des", "Deut\u00b7schen", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Aus deutschem Wort; dem edelsten Weine gleich,", "tokens": ["Aus", "deut\u00b7schem", "Wort", ";", "dem", "e\u00b7dels\u00b7ten", "Wei\u00b7ne", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und dir, o Rhein, ist unsre Sprache,", "tokens": ["Und", "dir", ",", "o", "Rhein", ",", "ist", "uns\u00b7re", "Spra\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NE", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Reich wie dein Strom, mit geheimen Tiefen;", "tokens": ["Reich", "wie", "dein", "Strom", ",", "mit", "ge\u00b7hei\u00b7men", "Tie\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Vom eitlen Nachbar, der sich in Schaum berauscht,", "tokens": ["Vom", "eit\u00b7len", "Nach\u00b7bar", ",", "der", "sich", "in", "Schaum", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Verstanden nimmer, nimmer erfunden! La\u00dft", "tokens": ["Ver\u00b7stan\u00b7den", "nim\u00b7mer", ",", "nim\u00b7mer", "er\u00b7fun\u00b7den", "!", "La\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "$,", "ADV", "VVPP", "$.", "VVIMP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihm seinen Schaum im Becher! ihm die", "tokens": ["Ihm", "sei\u00b7nen", "Schaum", "im", "Be\u00b7cher", "!", "ihm", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "APPRART", "NN", "$.", "PPER", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sprache, die an der Empfindung hinstreift.", "tokens": ["Spra\u00b7che", ",", "die", "an", "der", "Emp\u00b7fin\u00b7dung", "hins\u00b7treift", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.11": {"line.1": {"text": "Ihn haben Schrecken Gottes, und deutsches Herz \u2013", "tokens": ["Ihn", "ha\u00b7ben", "Schre\u00b7cken", "Got\u00b7tes", ",", "und", "deut\u00b7sches", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "NN", "$,", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Heuschrecken gleich, die oft, mit der Fackel Glut,", "tokens": ["Heu\u00b7schre\u00b7cken", "gleich", ",", "die", "oft", ",", "mit", "der", "Fa\u00b7ckel", "Glut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRELS", "ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Landmann von sich scheuchet, bis ihr", "tokens": ["Der", "Land\u00b7mann", "von", "sich", "scheu\u00b7chet", ",", "bis", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwirrender Schwarm in den Rhein sich st\u00fcrzet \u2013", "tokens": ["Schwir\u00b7ren\u00b7der", "Schwarm", "in", "den", "Rhein", "sich", "st\u00fcr\u00b7zet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NE", "PRF", "VVFIN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "So haben Schrecken Gottes, und deutsches Herz,", "tokens": ["So", "ha\u00b7ben", "Schre\u00b7cken", "Got\u00b7tes", ",", "und", "deut\u00b7sches", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Dr\u00e4ngers Horden, welcher der Herrschaft sich", "tokens": ["Des", "Dr\u00e4n\u00b7gers", "Hor\u00b7den", ",", "wel\u00b7cher", "der", "Herr\u00b7schaft", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "ART", "NN", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bei uns verma\u00df, ihn selbst, den Dr\u00e4nger,", "tokens": ["Bei", "uns", "ver\u00b7ma\u00df", ",", "ihn", "selbst", ",", "den", "Dr\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Her von der Oder bis hin zum Rhodan", "tokens": ["Her", "von", "der", "O\u00b7der", "bis", "hin", "zum", "Rho\u00b7dan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADV", "APPRART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Geschreckt, verfolgt, zerstiebet! Er windet sich,", "tokens": ["Ge\u00b7schreckt", ",", "ver\u00b7folgt", ",", "zer\u00b7stie\u00b7bet", "!", "Er", "win\u00b7det", "sich", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und fleht um Frieden! Friede, ja Friede sei", "tokens": ["Und", "fleht", "um", "Frie\u00b7den", "!", "Frie\u00b7de", ",", "ja", "Frie\u00b7de", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "NN", "$,", "ADV", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem eitlen Volk, in alter Grenze;", "tokens": ["Dem", "eit\u00b7len", "Volk", ",", "in", "al\u00b7ter", "Gren\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber dem Deutschen sei deutsche Freiheit,", "tokens": ["A\u00b7ber", "dem", "Deut\u00b7schen", "sei", "deut\u00b7sche", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "So weit die Sprache t\u00f6net, die trauliche,", "tokens": ["So", "weit", "die", "Spra\u00b7che", "t\u00f6\u00b7net", ",", "die", "trau\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die fromme, hehre; sie, der Empfindung, sie,", "tokens": ["Die", "from\u00b7me", ",", "heh\u00b7re", ";", "sie", ",", "der", "Emp\u00b7fin\u00b7dung", ",", "sie", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$.", "PPER", "$,", "ART", "NN", "$,", "PPER", "$,"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Gespielin des Gesangs, der frei im", "tokens": ["Ge\u00b7spie\u00b7lin", "des", "Ge\u00b7sangs", ",", "der", "frei", "im"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Tanze wie Sph\u00e4rengesang einherschwebt!", "tokens": ["Tan\u00b7ze", "wie", "Sph\u00e4\u00b7ren\u00b7ge\u00b7sang", "ein\u00b7her\u00b7schwebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}