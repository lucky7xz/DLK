{"textgrid.poem.55122": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Klaggesang", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist Wei\u00dfes dort am gr\u00fcnen Walde?", "tokens": ["Was", "ist", "Wei\u00b7\u00dfes", "dort", "am", "gr\u00fc\u00b7nen", "Wal\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist es Schnee wohl, oder sind es Schw\u00e4ne?", "tokens": ["Ist", "es", "Schnee", "wohl", ",", "o\u00b7der", "sind", "es", "Schw\u00e4\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "$,", "KON", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4r es Schnee, er w\u00e4re weggeschmolzen;", "tokens": ["W\u00e4r", "es", "Schnee", ",", "er", "w\u00e4\u00b7re", "weg\u00b7ge\u00b7schmol\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4ren's Schw\u00e4ne, w\u00e4ren weggeflogen.", "tokens": ["W\u00e4\u00b7ren's", "Schw\u00e4\u00b7ne", ",", "w\u00e4\u00b7ren", "weg\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ist kein Schnee nicht, es sind keine Schw\u00e4ne,", "tokens": ["Ist", "kein", "Schnee", "nicht", ",", "es", "sind", "kei\u00b7ne", "Schw\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PTKNEG", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "'s ist der Glanz der Zelten Asan Aga.", "tokens": ["'s", "ist", "der", "Glanz", "der", "Zel\u00b7ten", "A\u00b7san", "A\u00b7ga", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "NE", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Nieder liegt er drin an seiner Wunde.", "tokens": ["Nie\u00b7der", "liegt", "er", "drin", "an", "sei\u00b7ner", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ihn besucht die Mutter und die Schwester;", "tokens": ["Ihn", "be\u00b7sucht", "die", "Mut\u00b7ter", "und", "die", "Schwes\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Schamhaft s\u00e4umt sein Weib, zu ihm zu kommen.", "tokens": ["Scham\u00b7haft", "s\u00e4umt", "sein", "Weib", ",", "zu", "ihm", "zu", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Als nun seine Wunde linder wurde,", "tokens": ["Als", "nun", "sei\u00b7ne", "Wun\u00b7de", "lin\u00b7der", "wur\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lie\u00df er seinem treuen Weibe sagen:", "tokens": ["Lie\u00df", "er", "sei\u00b7nem", "treu\u00b7en", "Wei\u00b7be", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbharre mein nicht mehr an meinem Hofe,", "tokens": ["\u00bb", "har\u00b7re", "mein", "nicht", "mehr", "an", "mei\u00b7nem", "Ho\u00b7fe", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht am Hofe und nicht bei den Meinen.\u00ab", "tokens": ["Nicht", "am", "Ho\u00b7fe", "und", "nicht", "bei", "den", "Mei\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "KON", "PTKNEG", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Als die Frau dies harte Wort vernommen,", "tokens": ["Als", "die", "Frau", "dies", "har\u00b7te", "Wort", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stand die Treue starr und voller Schmerzen,", "tokens": ["Stand", "die", "Treu\u00b7e", "starr", "und", "vol\u00b7ler", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00f6rt der Pferde Stampfen vor der T\u00fcre,", "tokens": ["H\u00f6rt", "der", "Pfer\u00b7de", "Stamp\u00b7fen", "vor", "der", "T\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und es deucht ihr, Asan k\u00e4m, ihr Gatte,", "tokens": ["Und", "es", "deucht", "ihr", ",", "A\u00b7san", "k\u00e4m", ",", "ihr", "Gat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "NE", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Springt zum Turme, sich herabzust\u00fcrzen.", "tokens": ["Springt", "zum", "Tur\u00b7me", ",", "sich", "her\u00b7ab\u00b7zu\u00b7st\u00fcr\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PRF", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "\u00c4ngstlich folgen ihr zwei liebe T\u00f6chter,", "tokens": ["\u00c4ngst\u00b7lich", "fol\u00b7gen", "ihr", "zwei", "lie\u00b7be", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Rufen nach ihr, weinend bittre Tr\u00e4nen:", "tokens": ["Ru\u00b7fen", "nach", "ihr", ",", "wei\u00b7nend", "bitt\u00b7re", "Tr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "\u00bbsind nicht unsers Vaters Asan Rosse,", "tokens": ["\u00bb", "sind", "nicht", "un\u00b7sers", "Va\u00b7ters", "A\u00b7san", "Ros\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "PPOSAT", "NN", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Ist dein Bruder Pintorowich kommen!\u00ab", "tokens": ["Ist", "dein", "Bru\u00b7der", "Pin\u00b7to\u00b7ro\u00b7wich", "kom\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und es kehret die Gemahlin Asans,", "tokens": ["Und", "es", "keh\u00b7ret", "die", "Ge\u00b7mah\u00b7lin", "A\u00b7sans", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schlingt die Arme jammernd um den Bruder:", "tokens": ["Schlingt", "die", "Ar\u00b7me", "jam\u00b7mernd", "um", "den", "Bru\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbsieh die Schmach, o Bruder, deiner Schwester!", "tokens": ["\u00bb", "sieh", "die", "Schmach", ",", "o", "Bru\u00b7der", ",", "dei\u00b7ner", "Schwes\u00b7ter", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mich versto\u00dfen, Mutter dieser f\u00fcnfe!\u00ab", "tokens": ["Mich", "ver\u00b7sto\u00b7\u00dfen", ",", "Mut\u00b7ter", "die\u00b7ser", "f\u00fcn\u00b7fe", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "PDAT", "CARD", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Schweigt der Bruder, ziehet aus der Tasche,", "tokens": ["Schweigt", "der", "Bru\u00b7der", ",", "zie\u00b7het", "aus", "der", "Ta\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eingeh\u00fcllet in hochrote Seide,", "tokens": ["Ein\u00b7ge\u00b7h\u00fcl\u00b7let", "in", "hoch\u00b7ro\u00b7te", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ausgefertiget den Brief der Scheidung,", "tokens": ["Aus\u00b7ge\u00b7fer\u00b7ti\u00b7get", "den", "Brief", "der", "Schei\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df sie kehre zu der Mutter Wohnung,", "tokens": ["Da\u00df", "sie", "keh\u00b7re", "zu", "der", "Mut\u00b7ter", "Woh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Frei, sich einem andern zu ergeben.", "tokens": ["Frei", ",", "sich", "ei\u00b7nem", "an\u00b7dern", "zu", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Als die Frau den Trauerscheidbrief sahe,", "tokens": ["Als", "die", "Frau", "den", "Trau\u00b7er\u00b7scheid\u00b7brief", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00fc\u00dfte sie der beiden Knaben Stirne,", "tokens": ["K\u00fc\u00df\u00b7te", "sie", "der", "bei\u00b7den", "Kna\u00b7ben", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "K\u00fc\u00dft' die Wangen ihrer beiden M\u00e4dchen.", "tokens": ["K\u00fc\u00dft'", "die", "Wan\u00b7gen", "ih\u00b7rer", "bei\u00b7den", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber ach! vom S\u00e4ugling in der Wiege", "tokens": ["A\u00b7ber", "ach", "!", "vom", "S\u00e4ug\u00b7ling", "in", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Kann sie sich im bittern Schmerz nicht rei\u00dfen!", "tokens": ["Kann", "sie", "sich", "im", "bit\u00b7tern", "Schmerz", "nicht", "rei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Rei\u00dft sie los der ungest\u00fcme Bruder,", "tokens": ["Rei\u00dft", "sie", "los", "der", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hebt sie auf das muntre Ro\u00df behende,", "tokens": ["Hebt", "sie", "auf", "das", "mun\u00b7tre", "Ro\u00df", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und so eilt er mit der bangen Frauen", "tokens": ["Und", "so", "eilt", "er", "mit", "der", "ban\u00b7gen", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Grad nach seines Vaters hoher Wohnung.", "tokens": ["Grad", "nach", "sei\u00b7nes", "Va\u00b7ters", "ho\u00b7her", "Woh\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Kurze Zeit war's, noch nicht sieben Tage;", "tokens": ["Kur\u00b7ze", "Zeit", "wa\u00b7r's", ",", "noch", "nicht", "sie\u00b7ben", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,", "ADV", "PTKNEG", "CARD", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Kurze Zeit gnug; von viel gro\u00dfen Herren", "tokens": ["Kur\u00b7ze", "Zeit", "gnug", ";", "von", "viel", "gro\u00b7\u00dfen", "Her\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "$.", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unsre Frau in ihrer Witwentrauer,", "tokens": ["Uns\u00b7re", "Frau", "in", "ih\u00b7rer", "Wit\u00b7went\u00b7rau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unsre Frau zum Weib begehret wurde.", "tokens": ["Uns\u00b7re", "Frau", "zum", "Weib", "be\u00b7ge\u00b7hret", "wur\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Und der gr\u00f6\u00dfte war Imoskis Kadi;", "tokens": ["Und", "der", "gr\u00f6\u00df\u00b7te", "war", "I\u00b7mos\u00b7kis", "Ka\u00b7di", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VAFIN", "NE", "NE", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und die Frau bat weinend ihren Bruder:", "tokens": ["Und", "die", "Frau", "bat", "wei\u00b7nend", "ih\u00b7ren", "Bru\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbich beschw\u00f6re dich bei deinem Leben,", "tokens": ["\u00bb", "ich", "be\u00b7schw\u00f6\u00b7re", "dich", "bei", "dei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gib mich keinem andern mehr zur Frauen,", "tokens": ["Gib", "mich", "kei\u00b7nem", "an\u00b7dern", "mehr", "zur", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": [