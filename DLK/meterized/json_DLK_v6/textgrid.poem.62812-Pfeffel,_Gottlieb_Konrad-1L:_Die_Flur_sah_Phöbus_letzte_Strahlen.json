{"textgrid.poem.62812": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Flur sah Ph\u00f6bus letzte Strahlen", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flur sah Ph\u00f6bus letzte Strahlen", "tokens": ["Die", "Flur", "sah", "Ph\u00f6\u00b7bus", "letz\u00b7te", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Horizontes d\u00fcstern Rand", "tokens": ["Des", "Ho\u00b7ri\u00b7zon\u00b7tes", "d\u00fcs\u00b7tern", "Rand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem d\u00fcnnen Roth bemalen,", "tokens": ["Mit", "ei\u00b7nem", "d\u00fcn\u00b7nen", "Roth", "be\u00b7ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als sich in einem fremden Land", "tokens": ["Als", "sich", "in", "ei\u00b7nem", "frem\u00b7den", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Pilger, der den Weg verfehlte,", "tokens": ["Ein", "Pil\u00b7ger", ",", "der", "den", "Weg", "ver\u00b7fehl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Allein in einem Grund befand,", "tokens": ["Al\u00b7lein", "in", "ei\u00b7nem", "Grund", "be\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von dem man Raub und Mord erzehlte.", "tokens": ["Von", "dem", "man", "Raub", "und", "Mord", "er\u00b7zehl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er irret lange bebend fort", "tokens": ["Er", "ir\u00b7ret", "lan\u00b7ge", "be\u00b7bend", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und st\u00f6\u00dft zuletzt auf einen Ort,", "tokens": ["Und", "st\u00f6\u00dft", "zu\u00b7letzt", "auf", "ei\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wo sich zween schmale Pfade scheiden.", "tokens": ["Wo", "sich", "zween", "schma\u00b7le", "Pfa\u00b7de", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Voll neuen Kummers starrt er hier", "tokens": ["Voll", "neu\u00b7en", "Kum\u00b7mers", "starrt", "er", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und seufzet, ach! wer zeiget mir", "tokens": ["Und", "seuf\u00b7zet", ",", "ach", "!", "wer", "zei\u00b7get", "mir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ITJ", "$.", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den rechten Weg von diesen beyden?", "tokens": ["Den", "rech\u00b7ten", "Weg", "von", "die\u00b7sen", "bey\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sein Auge schweift geschreckt umher", "tokens": ["Sein", "Au\u00b7ge", "schweift", "ge\u00b7schreckt", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und weiset ihm von ungefehr", "tokens": ["Und", "wei\u00b7set", "ihm", "von", "un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Zur rechten eine hohe S\u00e4ule", "tokens": ["Zur", "rech\u00b7ten", "ei\u00b7ne", "ho\u00b7he", "S\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Mit einer ausgestreckten Hand,", "tokens": ["Mit", "ei\u00b7ner", "aus\u00b7ge\u00b7streck\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Worauf er diese Worte fand:", "tokens": ["Wo\u00b7rauf", "er", "die\u00b7se", "Wor\u00b7te", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ich f\u00fchre dich nach einer Meile", "tokens": ["Ich", "f\u00fch\u00b7re", "dich", "nach", "ei\u00b7ner", "Mei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "In eine sichre Ruhestadt.", "tokens": ["In", "ei\u00b7ne", "sich\u00b7re", "Ru\u00b7he\u00b7stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der arme Pilger k\u00fc\u00dft die S\u00e4ule,", "tokens": ["Der", "ar\u00b7me", "Pil\u00b7ger", "k\u00fc\u00dft", "die", "S\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die seine Furcht verbannet hat.", "tokens": ["Die", "sei\u00b7ne", "Furcht", "ver\u00b7ban\u00b7net", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Jedoch nach einer kleinen Weile", "tokens": ["Je\u00b7doch", "nach", "ei\u00b7ner", "klei\u00b7nen", "Wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ergreifet ihn ein neuer Graus.", "tokens": ["Er\u00b7grei\u00b7fet", "ihn", "ein", "neu\u00b7er", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ach, liebe S\u00e4ule! ruft er aus,", "tokens": ["Ach", ",", "lie\u00b7be", "S\u00e4u\u00b7le", "!", "ruft", "er", "aus", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wie leicht kann ich die Bahn versehen", "tokens": ["Wie", "leicht", "kann", "ich", "die", "Bahn", "ver\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und dann ist es um mich geschehen,", "tokens": ["Und", "dann", "ist", "es", "um", "mich", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "O, k\u00f6nntest du nur mit mir gehen!", "tokens": ["O", ",", "k\u00f6nn\u00b7test", "du", "nur", "mit", "mir", "ge\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Ey, sprach der Klotz, ich danke dir,", "tokens": ["Ey", ",", "sprach", "der", "Klotz", ",", "ich", "dan\u00b7ke", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Freund, du begehrst zu viel von mir:", "tokens": ["Freund", ",", "du", "be\u00b7gehrst", "zu", "viel", "von", "mir", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ich rathe nur und bleibe stehen.", "tokens": ["Ich", "ra\u00b7the", "nur", "und", "blei\u00b7be", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}