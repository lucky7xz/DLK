{"textgrid.poem.61252": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Gute Stunde", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ward es tief in mir so stille!", "tokens": ["Wie", "ward", "es", "tief", "in", "mir", "so", "stil\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tage Wandeln r\u00fchrt mich kaum.", "tokens": ["Der", "Ta\u00b7ge", "Wan\u00b7deln", "r\u00fchrt", "mich", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der L\u00e4rm der Zeit, der Menschen Wille", "tokens": ["Der", "L\u00e4rm", "der", "Zeit", ",", "der", "Men\u00b7schen", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geht mir vor\u00fcber wie ein Traum.", "tokens": ["Geht", "mir", "vor\u00b7\u00fc\u00b7ber", "wie", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch drinnen ist es warm und helle,", "tokens": ["Doch", "drin\u00b7nen", "ist", "es", "warm", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es lauscht die Seele ungest\u00f6rt", "tokens": ["Es", "lauscht", "die", "See\u00b7le", "un\u00b7ge\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In sich hinein, da\u00df sie die Welle", "tokens": ["In", "sich", "hin\u00b7ein", ",", "da\u00df", "sie", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des eignen Wohllauts fluten h\u00f6rt.", "tokens": ["Des", "eig\u00b7nen", "Wohl\u00b7lauts", "flu\u00b7ten", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als wie aus Flammen neu geboren,", "tokens": ["Als", "wie", "aus", "Flam\u00b7men", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So spielt das Herz mir frisch und rein:", "tokens": ["So", "spielt", "das", "Herz", "mir", "frisch", "und", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergessen ist, was ich verloren,", "tokens": ["Ver\u00b7ges\u00b7sen", "ist", ",", "was", "ich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, was ich liebte, dennoch mein.", "tokens": ["Und", ",", "was", "ich", "lieb\u00b7te", ",", "den\u00b7noch", "mein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es hat der Jugend s\u00fc\u00df Gedenken", "tokens": ["Es", "hat", "der", "Ju\u00b7gend", "s\u00fc\u00df", "Ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich wie ein Himmel aufgetan;", "tokens": ["Sich", "wie", "ein", "Him\u00b7mel", "auf\u00b7ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sch\u00f6n mit seiner Huld Geschenken", "tokens": ["Und", "sch\u00f6n", "mit", "sei\u00b7ner", "Huld", "Ge\u00b7schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erscheint der Gott und r\u00fchrt mich an.", "tokens": ["Er\u00b7scheint", "der", "Gott", "und", "r\u00fchrt", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}