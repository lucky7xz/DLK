{"textgrid.poem.34950": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der b\u00f6se Thanatos,", "tokens": ["Das", "ist", "der", "b\u00f6\u00b7se", "Tha\u00b7na\u00b7tos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er kommt auf einem fahlen Ro\u00df;", "tokens": ["Er", "kommt", "auf", "ei\u00b7nem", "fah\u00b7len", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6r den Hufschlag, h\u00f6r den Trab,", "tokens": ["Ich", "h\u00f6r", "den", "Huf\u00b7schlag", ",", "h\u00f6r", "den", "Trab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dunkle Reiter holt mich ab \u2013", "tokens": ["Der", "dunk\u00b7le", "Rei\u00b7ter", "holt", "mich", "ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er rei\u00dft mich fort, Mathilden soll ich lassen,", "tokens": ["Er", "rei\u00dft", "mich", "fort", ",", "Ma\u00b7thil\u00b7den", "soll", "ich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Oh, den Gedanken kann mein Herz nicht fassen!", "tokens": ["Oh", ",", "den", "Ge\u00b7dan\u00b7ken", "kann", "mein", "Herz", "nicht", "fas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Sie war mir Weib und Kind zugleich,", "tokens": ["Sie", "war", "mir", "Weib", "und", "Kind", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und geh ich in das Schattenreich,", "tokens": ["Und", "geh", "ich", "in", "das", "Schat\u00b7ten\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird Witwe sie und Waise sein!", "tokens": ["Wird", "Wit\u00b7we", "sie", "und", "Wai\u00b7se", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich la\u00df in dieser Welt allein", "tokens": ["Ich", "la\u00df", "in", "die\u00b7ser", "Welt", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Weib, das Kind, das, trauend meinem Mute,", "tokens": ["Das", "Weib", ",", "das", "Kind", ",", "das", ",", "trau\u00b7end", "mei\u00b7nem", "Mu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PDS", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sorglos und treu an meinem Herzen ruhte.", "tokens": ["Sorg\u00b7los", "und", "treu", "an", "mei\u00b7nem", "Her\u00b7zen", "ruh\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr Engel in den Himmelsh\u00f6hn,", "tokens": ["Ihr", "En\u00b7gel", "in", "den", "Him\u00b7mels\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vernehmt mein Schluchzen und mein Flehn:", "tokens": ["Ver\u00b7nehmt", "mein", "Schluch\u00b7zen", "und", "mein", "Flehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Besch\u00fctzt, wenn ich im \u00f6den Grab,", "tokens": ["Be\u00b7sch\u00fctzt", ",", "wenn", "ich", "im", "\u00f6\u00b7den", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Weib, das ich geliebet hab;", "tokens": ["Das", "Weib", ",", "das", "ich", "ge\u00b7lie\u00b7bet", "hab", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seid Schild und V\u00f6gte eurem Ebenbilde,", "tokens": ["Seid", "Schild", "und", "V\u00f6g\u00b7te", "eu\u00b7rem", "E\u00b7ben\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Besch\u00fctzt, beschirmt mein armes Kind, Mathilde.", "tokens": ["Be\u00b7sch\u00fctzt", ",", "be\u00b7schirmt", "mein", "ar\u00b7mes", "Kind", ",", "Ma\u00b7thil\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bei allen Tr\u00e4nen, die ihr je", "tokens": ["Bei", "al\u00b7len", "Tr\u00e4\u00b7nen", ",", "die", "ihr", "je"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geweint um unser Menschenweh,", "tokens": ["Ge\u00b7weint", "um", "un\u00b7ser", "Men\u00b7schen\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beim Wort, das nur der Priester kennt", "tokens": ["Beim", "Wort", ",", "das", "nur", "der", "Pries\u00b7ter", "kennt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und niemals ohne Schauder nennt,", "tokens": ["Und", "nie\u00b7mals", "oh\u00b7ne", "Schau\u00b7der", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bei eurer eignen Sch\u00f6nheit, Huld und Milde,", "tokens": ["Bei", "eu\u00b7rer", "eig\u00b7nen", "Sch\u00f6n\u00b7heit", ",", "Huld", "und", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Beschw\u00f6r ich euch, ihr Engel, sch\u00fctzt Mathilde.", "tokens": ["Be\u00b7schw\u00f6r", "ich", "euch", ",", "ihr", "En\u00b7gel", ",", "sch\u00fctzt", "Ma\u00b7thil\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}