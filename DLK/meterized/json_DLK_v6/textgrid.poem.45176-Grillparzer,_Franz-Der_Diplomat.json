{"textgrid.poem.45176": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Diplomat", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du von Kabinetten tr\u00e4umst,", "tokens": ["Wenn", "du", "von", "Ka\u00b7bi\u00b7net\u00b7ten", "tr\u00e4umst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ists h\u00f6chlich zu bejammern,", "tokens": ["Ists", "h\u00f6ch\u00b7lich", "zu", "be\u00b7jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man hat sie l\u00e4ngst erweitert schon", "tokens": ["Man", "hat", "sie", "l\u00e4ngst", "er\u00b7wei\u00b7tert", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und umgebaut zu Kammern.", "tokens": ["Und", "um\u00b7ge\u00b7baut", "zu", "Kam\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}