{"dta.poem.23767": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Vorwitz das K\u00fcnftige  \n zu wissen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "G\u00fctig h\u00fcllt in Finsternissen", "tokens": ["G\u00fc\u00b7tig", "h\u00fcllt", "in", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott die Zukunft ein:", "tokens": ["Gott", "die", "Zu\u00b7kunft", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Deutlich sie voraus zu wissen", "tokens": ["Deut\u00b7lich", "sie", "vo\u00b7raus", "zu", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrde Strafe seyn.", "tokens": ["W\u00fcr\u00b7de", "Stra\u00b7fe", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "S\u00e4h ich Gl\u00fcck auf meinem Wege;", "tokens": ["S\u00e4h", "ich", "Gl\u00fcck", "auf", "mei\u00b7nem", "We\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrd ich stolz mich bl\u00e4hn,", "tokens": ["W\u00fcrd", "ich", "stolz", "mich", "bl\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und leichtsinnig oder tr\u00e4ge", "tokens": ["Und", "leicht\u00b7sin\u00b7nig", "o\u00b7der", "tr\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Zweck versehn.", "tokens": ["Mei\u00b7nen", "Zweck", "ver\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "S\u00e4h ich Ungl\u00fcck; w\u00fcrd ich zittern:", "tokens": ["S\u00e4h", "ich", "Un\u00b7gl\u00fcck", ";", "w\u00fcrd", "ich", "zit\u00b7tern", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die k\u00fcnftge Zeit", "tokens": ["Und", "die", "k\u00fcnft\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00fcrde mir das Gl\u00fcck verbittern,", "tokens": ["W\u00fcr\u00b7de", "mir", "das", "Gl\u00fcck", "ver\u00b7bit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mich ietzt erfreut.", "tokens": ["Das", "mich", "ietzt", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Was ich habe, will ich n\u00fctzen,", "tokens": ["Was", "ich", "ha\u00b7be", ",", "will", "ich", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fernen Gram nicht scheun:", "tokens": ["Fer\u00b7nen", "Gram", "nicht", "scheun", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und soll ich ein Gl\u00fcck besitzen,", "tokens": ["Und", "soll", "ich", "ein", "Gl\u00fcck", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Meines Gl\u00fccks mich freun.", "tokens": ["Mei\u00b7nes", "Gl\u00fccks", "mich", "freun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}