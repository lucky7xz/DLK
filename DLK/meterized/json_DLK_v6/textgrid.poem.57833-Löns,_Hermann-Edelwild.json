{"textgrid.poem.57833": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Edelwild", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein freier Wildbretsch\u00fctz", "tokens": ["Ich", "bin", "ein", "frei\u00b7er", "Wild\u00b7bret\u00b7sch\u00fctz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hab' ein weit Revier,", "tokens": ["Und", "hab'", "ein", "weit", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So weit die braune Heide geht,", "tokens": ["So", "weit", "die", "brau\u00b7ne", "Hei\u00b7de", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh\u00f6rt das Jagen mir.", "tokens": ["Ge\u00b7h\u00f6rt", "das", "Ja\u00b7gen", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So weit der blaue Himmel geht,", "tokens": ["So", "weit", "der", "blau\u00b7e", "Him\u00b7mel", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh\u00f6rt mir alle Pirsch", "tokens": ["Ge\u00b7h\u00f6rt", "mir", "al\u00b7le", "Pirsch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf Fuchs und Has' und Haselhuhn,", "tokens": ["Auf", "Fuchs", "und", "Has'", "und", "Ha\u00b7sel\u00b7huhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Rehbock und auf Hirsch.", "tokens": ["Auf", "Reh\u00b7bock", "und", "auf", "Hirsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jedoch mein liebstes Edelwild", "tokens": ["Je\u00b7doch", "mein", "liebs\u00b7tes", "E\u00b7del\u00b7wild"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im ganzen Jagdrevier", "tokens": ["Im", "gan\u00b7zen", "Jagd\u00b7re\u00b7vier"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist nicht Hirsch, das ist nicht Reh,", "tokens": ["Das", "ist", "nicht", "Hirsch", ",", "das", "ist", "nicht", "Reh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "NN", "$,", "PDS", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist kein Jagdgetier.", "tokens": ["Das", "ist", "kein", "Jagd\u00b7ge\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ist ein frisches M\u00e4gdelein,", "tokens": ["Es", "ist", "ein", "fri\u00b7sches", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf das ich lieber pirsch,", "tokens": ["Auf", "das", "ich", "lie\u00b7ber", "pirsch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel lieber als auf Has' und Fuchs,", "tokens": ["Viel", "lie\u00b7ber", "als", "auf", "Has'", "und", "Fuchs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "APPR", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Rehbock und auf Hirsch.", "tokens": ["Auf", "Reh\u00b7bock", "und", "auf", "Hirsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und da\u00df es einem andern h\u00f6rt,", "tokens": ["Und", "da\u00df", "es", "ei\u00b7nem", "an\u00b7dern", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht keine Sorge mir,", "tokens": ["Macht", "kei\u00b7ne", "Sor\u00b7ge", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin ein freier Wildbretsch\u00fctz", "tokens": ["Ich", "bin", "ein", "frei\u00b7er", "Wild\u00b7bret\u00b7sch\u00fctz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hab' ein weit Revier.", "tokens": ["Und", "hab'", "ein", "weit", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}