{"textgrid.poem.42181": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Schicksal", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcrme durchtoben die bange Brust;", "tokens": ["St\u00fcr\u00b7me", "durch\u00b7to\u00b7ben", "die", "ban\u00b7ge", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "St\u00fcrmisches Leid und st\u00fcrmische Lust", "tokens": ["St\u00fcr\u00b7mi\u00b7sches", "Leid", "und", "st\u00fcr\u00b7mi\u00b7sche", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sausen hindurch mit schaurigem Wehen,", "tokens": ["Sau\u00b7sen", "hin\u00b7durch", "mit", "schau\u00b7ri\u00b7gem", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schleudern mich aus des Mi\u00dfgeschicks Nacht", "tokens": ["Schleu\u00b7dern", "mich", "aus", "des", "Mi\u00df\u00b7ge\u00b7schicks", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Auf zu des Gl\u00fcckes sonnigen H\u00f6hen.", "tokens": ["Auf", "zu", "des", "Gl\u00fc\u00b7ckes", "son\u00b7ni\u00b7gen", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sprachlos begaff ich die strahlende Pracht", "tokens": ["Sprach\u00b7los", "be\u00b7gaff", "ich", "die", "strah\u00b7len\u00b7de", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Schau ich des Weibes hehre Gestalt,", "tokens": ["Schau", "ich", "des", "Wei\u00b7bes", "heh\u00b7re", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Wie sie die Tr\u00e4ume der Jugend verhei\u00dfen,", "tokens": ["Wie", "sie", "die", "Tr\u00e4u\u00b7me", "der", "Ju\u00b7gend", "ver\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und es ergreift mich, mit blinder Gewalt", "tokens": ["Und", "es", "er\u00b7greift", "mich", ",", "mit", "blin\u00b7der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "An die pochende Brust sie zu rei\u00dfen.", "tokens": ["An", "die", "po\u00b7chen\u00b7de", "Brust", "sie", "zu", "rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Sie aber zieht mich auf schwellende Kissen,", "tokens": ["Sie", "a\u00b7ber", "zieht", "mich", "auf", "schwel\u00b7len\u00b7de", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Pre\u00dft mich an ihren \u00fcppigen Leib,", "tokens": ["Pre\u00dft", "mich", "an", "ih\u00b7ren", "\u00fcp\u00b7pi\u00b7gen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Und \u00fcberw\u00e4ltigt von wilden Gen\u00fcssen", "tokens": ["Und", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7tigt", "von", "wil\u00b7den", "Ge\u00b7n\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Halt ich umklammert das sch\u00f6ne Weib.", "tokens": ["Halt", "ich", "um\u00b7klam\u00b7mert", "das", "sch\u00f6\u00b7ne", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Siehe da, gleich einem wogenden Meer", "tokens": ["Sie\u00b7he", "da", ",", "gleich", "ei\u00b7nem", "wo\u00b7gen\u00b7den", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "W\u00e4lzt sich gewaltig das Ungl\u00fcck her.", "tokens": ["W\u00e4lzt", "sich", "ge\u00b7wal\u00b7tig", "das", "Un\u00b7gl\u00fcck", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "J\u00e4h zerschmetternde Blitze flammen", "tokens": ["J\u00e4h", "zer\u00b7schmet\u00b7tern\u00b7de", "Blit\u00b7ze", "flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Nieder aus d\u00fcsterem Wolkenthron;", "tokens": ["Nie\u00b7der", "aus", "d\u00fcs\u00b7te\u00b7rem", "Wol\u00b7ken\u00b7thron", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "\u00dcber dem trunkenen Erdensohn", "tokens": ["\u00dc\u00b7ber", "dem", "trun\u00b7ke\u00b7nen", "Er\u00b7den\u00b7sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Schlagen die sch\u00e4umenden Fluten zusammen. \u2013 \u2013", "tokens": ["Schla\u00b7gen", "die", "sch\u00e4u\u00b7men\u00b7den", "Flu\u00b7ten", "zu\u00b7sam\u00b7men", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Als die Sonne wiederum schien,", "tokens": ["Als", "die", "Son\u00b7ne", "wie\u00b7de\u00b7rum", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Gleitet ein Nachen dar\u00fcber hin.", "tokens": ["Glei\u00b7tet", "ein", "Na\u00b7chen", "da\u00b7r\u00fc\u00b7ber", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schimmernd steigt aus der Wellen Gischt", "tokens": ["Schim\u00b7mernd", "steigt", "aus", "der", "Wel\u00b7len", "Gischt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ein Regenbogen, der bald erlischt;", "tokens": ["Ein", "Re\u00b7gen\u00b7bo\u00b7gen", ",", "der", "bald", "er\u00b7lischt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Von dem Verungl\u00fcckten fand sich nischt.", "tokens": ["Von", "dem", "Ver\u00b7un\u00b7gl\u00fcck\u00b7ten", "fand", "sich", "nischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}