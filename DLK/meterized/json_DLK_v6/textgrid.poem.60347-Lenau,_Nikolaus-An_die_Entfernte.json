{"textgrid.poem.60347": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An die Entfernte", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Rose pfl\u00fcck ich hier,", "tokens": ["Die\u00b7se", "Ro\u00b7se", "pfl\u00fcck", "ich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der fremden Ferne;", "tokens": ["In", "der", "frem\u00b7den", "Fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Liebes M\u00e4dchen, dir, ach dir", "tokens": ["Lie\u00b7bes", "M\u00e4d\u00b7chen", ",", "dir", ",", "ach", "dir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "$,", "XY", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00e4cht ich sie so gerne!", "tokens": ["Br\u00e4cht", "ich", "sie", "so", "ger\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch bis ich zu dir mag ziehn", "tokens": ["Doch", "bis", "ich", "zu", "dir", "mag", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Viele weite Meilen,", "tokens": ["Vie\u00b7le", "wei\u00b7te", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist die Rose l\u00e4ngst dahin,", "tokens": ["Ist", "die", "Ro\u00b7se", "l\u00e4ngst", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die Rosen eilen.", "tokens": ["Denn", "die", "Ro\u00b7sen", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nie soll weiter sich ins Land", "tokens": ["Nie", "soll", "wei\u00b7ter", "sich", "ins", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PRF", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb von Liebe wagen,", "tokens": ["Lieb", "von", "Lie\u00b7be", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Als sich bl\u00fchend in der Hand", "tokens": ["Als", "sich", "bl\u00fc\u00b7hend", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft die Rose tragen;", "tokens": ["L\u00e4\u00dft", "die", "Ro\u00b7se", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Oder als die Nachtigall", "tokens": ["O\u00b7der", "als", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Halme bringt zum Neste,", "tokens": ["Hal\u00b7me", "bringt", "zum", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oder als ihr s\u00fc\u00dfer Schall", "tokens": ["O\u00b7der", "als", "ihr", "s\u00fc\u00b7\u00dfer", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandert mit dem Weste.", "tokens": ["Wan\u00b7dert", "mit", "dem", "Wes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}