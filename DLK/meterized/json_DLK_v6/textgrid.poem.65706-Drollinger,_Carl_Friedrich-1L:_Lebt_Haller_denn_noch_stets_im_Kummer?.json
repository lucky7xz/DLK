eter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Drum schwand auch Marianens Pracht.", "tokens": ["Drum", "schwand", "auch", "Ma\u00b7ri\u00b7a\u00b7nens", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "O m\u00f6cht ich doch von deinen T\u00f6hnen", "tokens": ["O", "m\u00f6cht", "ich", "doch", "von", "dei\u00b7nen", "T\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die fe\u00fcerreiche Kraft entlehnen,", "tokens": ["Die", "fe\u00b7\u00fcer\u00b7rei\u00b7che", "Kraft", "ent\u00b7leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die uns durch Herz und Sinnen bricht!", "tokens": ["Die", "uns", "durch", "Herz", "und", "Sin\u00b7nen", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von deiner Gattinn Wert zu singen.", "tokens": ["Von", "dei\u00b7ner", "Gat\u00b7tinn", "Wert", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Umsonst! Du must es selbst vollbringen;", "tokens": ["Um\u00b7sonst", "!", "Du", "must", "es", "selbst", "voll\u00b7brin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "met