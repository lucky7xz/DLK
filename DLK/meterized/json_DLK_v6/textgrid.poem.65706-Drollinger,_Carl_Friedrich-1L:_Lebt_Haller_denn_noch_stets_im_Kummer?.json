{"textgrid.poem.65706": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lebt Haller denn noch stets im Kummer?", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebt Haller denn noch stets im Kummer?", "tokens": ["Lebt", "Hal\u00b7ler", "denn", "noch", "stets", "im", "Kum\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will seiner Gattin Todtenschlummer", "tokens": ["Will", "sei\u00b7ner", "Gat\u00b7tin", "Tod\u00b7ten\u00b7schlum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn auch in Gruft und Bahre ziehn?", "tokens": ["Ihn", "auch", "in", "Gruft", "und", "Bah\u00b7re", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Fre\u00fcnd, lern einst dein Leid ertragen!", "tokens": ["O", "Fre\u00fcnd", ",", "lern", "einst", "dein", "Leid", "er\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Weyser soll nicht ewig klagen.", "tokens": ["Ein", "Wey\u00b7ser", "soll", "nicht", "e\u00b7wig", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wirf deinen Unmuht endlich hin!", "tokens": ["Wirf", "dei\u00b7nen", "Un\u00b7muht", "end\u00b7lich", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar sind Dir Tausend seltne Gaben", "tokens": ["Zwar", "sind", "Dir", "Tau\u00b7send", "selt\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der Erbla\u00dften jetzt begraben.", "tokens": ["Mit", "der", "Er\u00b7bla\u00df\u00b7ten", "jetzt", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Wert erscheint aus deiner Wahl.", "tokens": ["Ihr", "Wert", "er\u00b7scheint", "aus", "dei\u00b7ner", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Herze war nicht leicht zu binden.", "tokens": ["Dein", "Her\u00b7ze", "war", "nicht", "leicht", "zu", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die, die es einig konnt entz\u00fcnden,", "tokens": ["Die", ",", "die", "es", "ei\u00b7nig", "konnt", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erweckt ihm sterbend Weh und Qual.", "tokens": ["Er\u00b7weckt", "ihm", "ster\u00b7bend", "Weh", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch kennst Du ja das Haubtges\u00e4tze,", "tokens": ["Doch", "kennst", "Du", "ja", "das", "Haubt\u00b7ge\u00b7s\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das stets der Erden gr\u00f6ste Sch\u00e4tze", "tokens": ["Das", "stets", "der", "Er\u00b7den", "gr\u00f6s\u00b7te", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit der Verg\u00e4nglichkeit gesellt.", "tokens": ["Mit", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sta\u00fcbchen unterbricht das Leben;", "tokens": ["Ein", "Sta\u00fcb\u00b7chen", "un\u00b7ter\u00b7bricht", "das", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dies kan sich jede Stund ergeben:", "tokens": ["Dies", "kan", "sich", "je\u00b7de", "Stund", "er\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie da\u00df es uns denn fremde f\u00e4llt?", "tokens": ["Wie", "da\u00df", "es", "uns", "denn", "frem\u00b7de", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PPER", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie mancher stirbt schon in der Wiege!", "tokens": ["Wie", "man\u00b7cher", "stirbt", "schon", "in", "der", "Wie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du kennst des K\u00f6rpers Kunstgef\u00fcge;", "tokens": ["Du", "kennst", "des", "K\u00f6r\u00b7pers", "Kunst\u00b7ge\u00b7f\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein schwacher Bau kan nicht bestehn.", "tokens": ["Sein", "schwa\u00b7cher", "Bau", "kan", "nicht", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du weist, da\u00df, was man dran erblicke,", "tokens": ["Du", "weist", ",", "da\u00df", ",", "was", "man", "dran", "er\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "$,", "PRELS", "PIS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Ausbund gr\u00f6ster Meisterst\u00fccke,", "tokens": ["Ein", "Aus\u00b7bund", "gr\u00f6s\u00b7ter", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch so gebrechlich sey, als sch\u00f6n.", "tokens": ["Doch", "so", "ge\u00b7brech\u00b7lich", "sey", ",", "als", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Tod verschohnet nicht der Kronen.", "tokens": ["Der", "Tod", "ver\u00b7schoh\u00b7net", "nicht", "der", "Kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er spielt in H\u00fctten und auf Trohnen", "tokens": ["Er", "spielt", "in", "H\u00fct\u00b7ten", "und", "auf", "Troh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein immer gleiches Trauerspiel.", "tokens": ["Ein", "im\u00b7mer", "glei\u00b7ches", "Trau\u00b7er\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Monarchen m\u00fcssen selbst von hinnen.", "tokens": ["Mon\u00b7ar\u00b7chen", "m\u00fcs\u00b7sen", "selbst", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Beyspiel gr\u00f6ster K\u00f6niginnen,", "tokens": ["Das", "Bey\u00b7spiel", "gr\u00f6s\u00b7ter", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Britten Carolina, fiel.", "tokens": ["Der", "Brit\u00b7ten", "Ca\u00b7ro\u00b7li\u00b7na", ",", "fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "O danke vielmehr deinem Gl\u00fccke,", "tokens": ["O", "dan\u00b7ke", "viel\u00b7mehr", "dei\u00b7nem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Marianens holde Blicke", "tokens": ["Das", "Ma\u00b7ri\u00b7a\u00b7nens", "hol\u00b7de", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dir noch so lange Zeit geg\u00f6nnt!", "tokens": ["Dir", "noch", "so", "lan\u00b7ge", "Zeit", "ge\u00b7g\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schicksal, reich an Lust und Schmerzen,", "tokens": ["Das", "Schick\u00b7sal", ",", "reich", "an", "Lust", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hat oft ein Paar der sch\u00f6nsten Herzen", "tokens": ["Hat", "oft", "ein", "Paar", "der", "sch\u00f6ns\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zugleich verkn\u00fcpfet und zertrennt.", "tokens": ["Zu\u00b7gleich", "ver\u00b7kn\u00fcp\u00b7fet", "und", "zer\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wem solch ein Schatz, wie Dir, beschehret,", "tokens": ["Wem", "solch", "ein", "Schatz", ",", "wie", "Dir", ",", "be\u00b7scheh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PIAT", "ART", "NN", "$,", "PWAV", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie kurz auch das Besitzen w\u00e4hret,", "tokens": ["Wie", "kurz", "auch", "das", "Be\u00b7sit\u00b7zen", "w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem gab der Himmel schon genug.", "tokens": ["Dem", "gab", "der", "Him\u00b7mel", "schon", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist uns doch zu nichts verbunden.", "tokens": ["Er", "ist", "uns", "doch", "zu", "nichts", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum, k\u00fcrzt er unsre Gl\u00fcckesstunden,", "tokens": ["Drum", ",", "k\u00fcrzt", "er", "uns\u00b7re", "Gl\u00fc\u00b7ckess\u00b7tun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wolan! Er hat es Macht und Fug.", "tokens": ["Wo\u00b7lan", "!", "Er", "hat", "es", "Macht", "und", "Fug", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun hei\u00dft er dich best\u00e4ndig hoffen.", "tokens": ["Nun", "hei\u00dft", "er", "dich", "be\u00b7st\u00e4n\u00b7dig", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein h\u00e4rtster Streich hat Dich getroffen;", "tokens": ["Sein", "h\u00e4rts\u00b7ter", "Streich", "hat", "Dich", "ge\u00b7trof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die gr\u00f6ste Furcht ist nun vorbey.", "tokens": ["Die", "gr\u00f6s\u00b7te", "Furcht", "ist", "nun", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat er fr\u00fch auf dich geschlagen,", "tokens": ["Und", "hat", "er", "fr\u00fch", "auf", "dich", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So denke, da\u00df es, ihn zu tragen,", "tokens": ["So", "den\u00b7ke", ",", "da\u00df", "es", ",", "ihn", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Jugend Kraft am leichtsten sey!", "tokens": ["Der", "Ju\u00b7gend", "Kraft", "am", "leichts\u00b7ten", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und mu\u00dftest Du von deiner Sch\u00f6nen", "tokens": ["Und", "mu\u00df\u00b7test", "Du", "von", "dei\u00b7ner", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich nicht auch lebend schon entw\u00f6hnen?", "tokens": ["Dich", "nicht", "auch", "le\u00b7bend", "schon", "ent\u00b7w\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stunden sind Dir noch bekannt,", "tokens": ["Die", "Stun\u00b7den", "sind", "Dir", "noch", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Da die Begihr, in B\u00fcsch- und Hecken", "tokens": ["Da", "die", "Be\u00b7gihr", ",", "in", "B\u00fcsch", "und", "He\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sch\u00f6pfung Wunder zu entdecken,", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Wun\u00b7der", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "