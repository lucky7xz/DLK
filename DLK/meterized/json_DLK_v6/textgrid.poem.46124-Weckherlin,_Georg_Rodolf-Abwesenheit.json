{"textgrid.poem.46124": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Abwesenheit", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Recht gleich wie diese erd", "tokens": ["Recht", "gleich", "wie", "die\u00b7se", "erd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KOKOM", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "mit finsternus wird \u00fcberspreitet,", "tokens": ["mit", "fins\u00b7ter\u00b7nus", "wird", "\u00fc\u00b7bers\u00b7prei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wan Ph\u00f6bus seine pferd", "tokens": ["wan", "Ph\u00f6\u00b7bus", "sei\u00b7ne", "pferd"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "hat in den nidergang beleitet;", "tokens": ["hat", "in", "den", "ni\u00b7der\u00b7gang", "be\u00b7lei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "also ist mein gesicht verblichen,", "tokens": ["al\u00b7so", "ist", "mein", "ge\u00b7sicht", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVPP", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "weil meine Myrt, mein Liebelein,", "tokens": ["weil", "mei\u00b7ne", "Myrt", ",", "mein", "Lie\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und meines herzens diebelein", "tokens": ["und", "mei\u00b7nes", "her\u00b7zens", "die\u00b7be\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "von mir hinweggewichen.", "tokens": ["von", "mir", "hin\u00b7weg\u00b7ge\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gleich wie, wan sich die son", "tokens": ["Gleich", "wie", ",", "wan", "sich", "die", "son"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "$,", "PWAV", "PRF", "ART", "XY"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "in ihr westhaus zu nacht verstecket,", "tokens": ["in", "ihr", "west\u00b7haus", "zu", "nacht", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mit sternen klar der mon", "tokens": ["mit", "ster\u00b7nen", "klar", "der", "mon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "das weite firmament bedecket;", "tokens": ["das", "wei\u00b7te", "fir\u00b7ma\u00b7ment", "be\u00b7de\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "also bin ich mit leid umfassen,", "tokens": ["al\u00b7so", "bin", "ich", "mit", "leid", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "weil meine Myrt, mein Nymfelein,", "tokens": ["weil", "mei\u00b7ne", "Myrt", ",", "mein", "Nym\u00b7fel\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "mein tr\u00f6stelein, mein schimpfelein,", "tokens": ["mein", "tr\u00f6s\u00b7tel\u00b7ein", ",", "mein", "schimp\u00b7fe\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "mich hinder ihr gelassen.", "tokens": ["mich", "hin\u00b7der", "ihr", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gleich wie Apollons pracht", "tokens": ["Gleich", "wie", "A\u00b7pol\u00b7lons", "pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "mit dem rotlecht verg\u00fcldten morgen", "tokens": ["mit", "dem", "rot\u00b7lecht", "ver\u00b7g\u00fcld\u00b7ten", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "vertreibet bald die nacht", "tokens": ["ver\u00b7trei\u00b7bet", "bald", "die", "nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "und mit der nacht die finstre sorgen;", "tokens": ["und", "mit", "der", "nacht", "die", "finst\u00b7re", "sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "also wird mein schmerz weggenommen,", "tokens": ["al\u00b7so", "wird", "mein", "schmerz", "weg\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "wan meine Myrt, mein herzelein,", "tokens": ["wan", "mei\u00b7ne", "Myrt", ",", "mein", "her\u00b7ze\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "mein wohnelein, mein scherzelein", "tokens": ["mein", "woh\u00b7nel\u00b7ein", ",", "mein", "scher\u00b7ze\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "wird wider zu mir kommen.", "tokens": ["wird", "wi\u00b7der", "zu", "mir", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gleich wie der sonnen kraft", "tokens": ["Gleich", "wie", "der", "son\u00b7nen", "kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "auf erden alle ding ergetzet", "tokens": ["auf", "er\u00b7den", "al\u00b7le", "ding", "er\u00b7get\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und die gew\u00e4chs mit saft", "tokens": ["und", "die", "ge\u00b7w\u00e4chs", "mit", "saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "mit blumen alles feld besetzet;", "tokens": ["mit", "blu\u00b7men", "al\u00b7les", "feld", "be\u00b7set\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "also soll ich mehr lusts genie\u00dfen,", "tokens": ["al\u00b7so", "soll", "ich", "mehr", "lusts", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "wan meine Myrt, mein sch\u00e4tzelein,", "tokens": ["wan", "mei\u00b7ne", "Myrt", ",", "mein", "sch\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "mein herzk\u00fctzlendes schm\u00e4tzelein,", "tokens": ["mein", "herz\u00b7k\u00fctz\u00b7len\u00b7des", "schm\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "mich k\u00fcssend wird begr\u00fc\u00dfen.", "tokens": ["mich", "k\u00fcs\u00b7send", "wird", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}