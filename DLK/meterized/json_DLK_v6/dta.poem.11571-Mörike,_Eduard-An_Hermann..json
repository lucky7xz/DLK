{"dta.poem.11571": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Hermann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Unter Thr\u00e4nen rissest du dich von meinem Halse;", "tokens": ["Un\u00b7ter", "Thr\u00e4\u00b7nen", "ris\u00b7sest", "du", "dich", "von", "mei\u00b7nem", "Hal\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "In die Finsterni\u00df lang' sah ich verworren dir nach;", "tokens": ["In", "die", "Fins\u00b7ter\u00b7ni\u00df", "lang'", "sah", "ich", "ver\u00b7wor\u00b7ren", "dir", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wie? auf Ewig? sagtest du so? Dann l\u00e4sset auf Ewig", "tokens": ["Wie", "?", "auf", "E\u00b7wig", "?", "sag\u00b7test", "du", "so", "?", "Dann", "l\u00e4s\u00b7set", "auf", "E\u00b7wig"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "APPR", "NE", "$.", "VVFIN", "PPER", "ADV", "$.", "ADV", "VVFIN", "APPR", "NE"], "meter": "--+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Meine Jugend von mir, l\u00e4sset mein Genius mich!", "tokens": ["Mei\u00b7ne", "Ju\u00b7gend", "von", "mir", ",", "l\u00e4s\u00b7set", "mein", "Ge\u00b7nius", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Und warum? bei Allem, was heilig, wei\u00dft du es", "tokens": ["Und", "wa\u00b7rum", "?", "bei", "Al\u00b7lem", ",", "was", "hei\u00b7lig", ",", "wei\u00dft", "du", "es"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "$.", "APPR", "PIS", "$,", "PRELS", "ADJD", "$,", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn es der Uebermuth schw\u00e4rmender Jugend nicht ist?", "tokens": ["Wenn", "es", "der", "Ue\u00b7ber\u00b7muth", "schw\u00e4r\u00b7men\u00b7der", "Ju\u00b7gend", "nicht", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "PTKNEG", "VAFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "O verwegenes Spiel! Komm! nimm das Wort, ruf es", "tokens": ["O", "ver\u00b7we\u00b7ge\u00b7nes", "Spiel", "!", "Komm", "!", "nimm", "das", "Wort", ",", "ruf", "es"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "VVFIN", "$.", "VVIMP", "ART", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "\u2014 Aber du h\u00f6rtest nicht, lie\u00dfest mich staunend allein.", "tokens": ["A\u00b7ber", "du", "h\u00f6r\u00b7test", "nicht", ",", "lie\u00b7\u00dfest", "mich", "stau\u00b7nend", "al\u00b7lein", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Monde vergingen und Jahre; die heimliche Sehnsucht im", "tokens": ["Mon\u00b7de", "ver\u00b7gin\u00b7gen", "und", "Jah\u00b7re", ";", "die", "heim\u00b7li\u00b7che", "Sehn\u00b7sucht", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "NN", "$.", "ART", "ADJA", "NN", "APPRART"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.10": {"text": "Standen wir fremd, es fand Keiner ein muthiges", "tokens": ["Stan\u00b7den", "wir", "fremd", ",", "es", "fand", "Kei\u00b7ner", "ein", "mut\u00b7hi\u00b7ges"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PIS", "ART", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Um den falschen Bann, den luftgewebten, zu brechen,", "tokens": ["Um", "den", "fal\u00b7schen", "Bann", ",", "den", "luft\u00b7ge\u00b7web\u00b7ten", ",", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und der gemeine Tag l\u00f6schte bald jeglichen Wunsch.", "tokens": ["Und", "der", "ge\u00b7mei\u00b7ne", "Tag", "l\u00f6schte", "bald", "jeg\u00b7li\u00b7chen", "Wunsch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Aber heutige Nacht erschien mir wieder im Traume", "tokens": ["A\u00b7ber", "heu\u00b7ti\u00b7ge", "Nacht", "er\u00b7schien", "mir", "wie\u00b7der", "im", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Deine Knabengestalt \u2014 Wehe! wo rett' ich mich hin", "tokens": ["Dei\u00b7ne", "Kna\u00b7ben\u00b7ge\u00b7stalt", "We\u00b7he", "!", "wo", "rett'", "ich", "mich", "hin"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "NN", "$.", "PWAV", "VVFIN", "PPER", "PRF", "APZR"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Vor dem lieblichen Bild! Ich sah dich unter den hohen", "tokens": ["Vor", "dem", "lieb\u00b7li\u00b7chen", "Bild", "!", "Ich", "sah", "dich", "un\u00b7ter", "den", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Maulbeerb\u00e4umen im Hof, wo wir zusammen gespielt.", "tokens": ["Maul\u00b7beer\u00b7b\u00e4u\u00b7men", "im", "Hof", ",", "wo", "wir", "zu\u00b7sam\u00b7men", "ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Und du wandtest dich ab, wie besch\u00e4mt, ich strich dir die", "tokens": ["Und", "du", "wand\u00b7test", "dich", "ab", ",", "wie", "be\u00b7sch\u00e4mt", ",", "ich", "strich", "dir", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ART"], "meter": "+-+-++--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Aus der Stirne: O du, rief ich, was kannst du daf\u00fcr!", "tokens": ["Aus", "der", "Stir\u00b7ne", ":", "O", "du", ",", "rief", "ich", ",", "was", "kannst", "du", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NE", "NE", "$,", "VVFIN", "PPER", "$,", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+-+++-+", "measure": "unknown.measure.octa.plus"}, "line.19": {"text": "Weinend erwacht' ich zulezt, tr\u00fcb schien der Mond auf", "tokens": ["Wei\u00b7nend", "er\u00b7wacht'", "ich", "zu\u00b7lezt", ",", "tr\u00fcb", "schien", "der", "Mond", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "ADJD", "VVFIN", "ART", "NN", "APPR"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.20": {"text": "Aufgerichtet im Bett sa\u00df ich und dachte dir nach;", "tokens": ["Auf\u00b7ge\u00b7rich\u00b7tet", "im", "Bett", "sa\u00df", "ich", "und", "dach\u00b7te", "dir", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "O wie tobte mein Herz! Du f\u00fclltest wieder den", "tokens": ["O", "wie", "tob\u00b7te", "mein", "Herz", "!", "Du", "f\u00fcll\u00b7test", "wie\u00b7der", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "ART"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Mir, wie kein Bruder vermag, wie die Geliebte nicht", "tokens": ["Mir", ",", "wie", "kein", "Bru\u00b7der", "ver\u00b7mag", ",", "wie", "die", "Ge\u00b7lieb\u00b7te", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "PTKNEG"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}