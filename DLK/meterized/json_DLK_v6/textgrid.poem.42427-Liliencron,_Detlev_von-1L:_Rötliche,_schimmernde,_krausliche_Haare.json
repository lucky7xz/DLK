", "seh\u00b7nen", "nach", "se\u00b7li\u00b7ger", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Breite um Nacken und Hals mir die Arme,", "tokens": ["Brei\u00b7te", "um", "Na\u00b7cken", "und", "Hals", "mir", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Lege dein Haupt an die klopfende Brust.", "tokens": ["Le\u00b7ge", "dein", "Haupt", "an", "die", "klop\u00b7fen\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Da\u00df ich an deinem Herzen erwarme,", "tokens": ["Da\u00df", "ich", "an", "dei\u00b7nem", "Her\u00b7zen", "er\u00b7war\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Breite um Nacken und Hals mir die Arme,", "tokens": ["Brei\u00b7te", "um", "Na\u00b7cken", "und", "Hals", "mir", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Siehst du nicht, da\u00df ich vergeh' im Harme", "tokens": ["Siehst", "du", "nicht", ",", "da\u00df", "ich", "ver\u00b7geh'", "im", "Har\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "p