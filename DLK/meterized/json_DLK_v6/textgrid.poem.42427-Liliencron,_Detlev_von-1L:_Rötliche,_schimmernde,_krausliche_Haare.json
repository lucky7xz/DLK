{"textgrid.poem.42427": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: R\u00f6tliche, schimmernde, krausliche Haare", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00f6tliche, schimmernde, krausliche Haare", "tokens": ["R\u00f6t\u00b7li\u00b7che", ",", "schim\u00b7mern\u00b7de", ",", "kraus\u00b7li\u00b7che", "Haa\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Spielen im Wind mir um Schl\u00e4fen und Ohr.", "tokens": ["Spie\u00b7len", "im", "Wind", "mir", "um", "Schl\u00e4\u00b7fen", "und", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Fr\u00fchling ist's, bald kommen gr\u00e4mliche Jahre.", "tokens": ["Fr\u00fch\u00b7ling", "ist's", ",", "bald", "kom\u00b7men", "gr\u00e4m\u00b7li\u00b7che", "Jah\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "VVINF", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "R\u00f6tliche, schimmernde, krausliche Haare,", "tokens": ["R\u00f6t\u00b7li\u00b7che", ",", "schim\u00b7mern\u00b7de", ",", "kraus\u00b7li\u00b7che", "Haa\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Sind eine preisliche, k\u00f6stliche Ware,", "tokens": ["Sind", "ei\u00b7ne", "preis\u00b7li\u00b7che", ",", "k\u00f6st\u00b7li\u0