{"textgrid.poem.36073": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxXIV. Helena.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdem Kinde gieb ein Spielzeug, so weinet es nicht mehr;", "tokens": ["\u00bb", "dem", "Kin\u00b7de", "gieb", "ein", "Spiel\u00b7zeug", ",", "so", "wei\u00b7net", "es", "nicht", "mehr", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVIMP", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch Du bist ", "tokens": ["Doch", "Du", "bist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Muss Dir zum Freudengarten ", "tokens": ["Muss", "Dir", "zum", "Freu\u00b7den\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ei, schreite durch ein Andres nur keck und k\u00fchn hinein!\u00ab", "tokens": ["Ei", ",", "schrei\u00b7te", "durch", "ein", "And\u00b7res", "nur", "keck", "und", "k\u00fchn", "hin\u00b7ein", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "PIS", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbdu grollst, weil wir das M\u00e4gdlein zur Ehe Dir versagt,", "tokens": ["\u00bb", "du", "grollst", ",", "weil", "wir", "das", "M\u00e4gd\u00b7lein", "zur", "E\u00b7he", "Dir", "ver\u00b7sagt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die nach der Haube l\u00fcstern; vergiss die niedre Magd!", "tokens": ["Die", "nach", "der", "Hau\u00b7be", "l\u00fcs\u00b7tern", ";", "ver\u00b7giss", "die", "nied\u00b7re", "Magd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nur Deines Winks bedarf es, so fliegen wir im Nu", "tokens": ["Nur", "Dei\u00b7nes", "Winks", "be\u00b7darf", "es", ",", "so", "flie\u00b7gen", "wir", "im", "Nu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und f\u00fchren K\u00f6niginnen f\u00fcr Deine Lust Dir zu.\u00ab", "tokens": ["Und", "f\u00fch\u00b7ren", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "f\u00fcr", "Dei\u00b7ne", "Lust", "Dir", "zu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "So redet zu dem Magus der listenreiche Geist,", "tokens": ["So", "re\u00b7det", "zu", "dem", "Ma\u00b7gus", "der", "lis\u00b7ten\u00b7rei\u00b7che", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der K\u00f6stliches versagend, noch K\u00f6stlichers verheisst.", "tokens": ["Der", "K\u00f6st\u00b7li\u00b7ches", "ver\u00b7sa\u00b7gend", ",", "noch", "K\u00f6st\u00b7li\u00b7chers", "ver\u00b7heisst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbwir hauchen neues Leben und neue Liebesgluth", "tokens": ["\u00bb", "wir", "hau\u00b7chen", "neu\u00b7es", "Le\u00b7ben", "und", "neu\u00b7e", "Lie\u00b7bes\u00b7gluth"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Sch\u00f6nheit ein, und ob sie Jahrhunderte gernht.\u00ab", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "ein", ",", "und", "ob", "sie", "Jahr\u00b7hun\u00b7der\u00b7te", "gernht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "KOUS", "PPER", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Da blitzt durch Faustus Innres ein Machtgedanke gross;", "tokens": ["Da", "blitzt", "durch", "Faus\u00b7tus", "Inn\u00b7res", "ein", "Macht\u00b7ge\u00b7dan\u00b7ke", "gross", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NE", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er reisst sich von dem D\u00fcster, das ihn umnachtet, los.", "tokens": ["Er", "reisst", "sich", "von", "dem", "D\u00fcs\u00b7ter", ",", "das", "ihn", "um\u00b7nach\u00b7tet", ",", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein wunderbarer Geist tritt ihm nah aus alter Zeit;", "tokens": ["Ein", "wun\u00b7der\u00b7ba\u00b7rer", "Geist", "tritt", "ihm", "nah", "aus", "al\u00b7ter", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er steht, ein zorn'ger Heros, der seinem Knecht gebeut.", "tokens": ["Er", "steht", ",", "ein", "zorn'\u00b7ger", "He\u00b7ros", ",", "der", "sei\u00b7nem", "Knecht", "ge\u00b7beut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Gieb Leben der Verwesung, eh' neu der Morgen tagt!", "tokens": ["Gieb", "Le\u00b7ben", "der", "Ver\u00b7we\u00b7sung", ",", "eh'", "neu", "der", "Mor\u00b7gen", "tagt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ART", "NN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und bringe mir, die Paris mit sel'gen Augen sah,", "tokens": ["Und", "brin\u00b7ge", "mir", ",", "die", "Pa\u00b7ris", "mit", "sel'\u00b7gen", "Au\u00b7gen", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "NE", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und bringe mir zur Buhle die ", "tokens": ["Und", "brin\u00b7ge", "mir", "zur", "Buh\u00b7le", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Fort st\u00e4ubt der Geist, und Faustus nachblicket ihm voll Groll;", "tokens": ["Fort", "st\u00e4ubt", "der", "Geist", ",", "und", "Faus\u00b7tus", "nach\u00b7bli\u00b7cket", "ihm", "voll", "Groll", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "NE", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dann wandelt er im Zimmer ernst und erwartungsvoll.", "tokens": ["Dann", "wan\u00b7delt", "er", "im", "Zim\u00b7mer", "ernst", "und", "er\u00b7war\u00b7tungs\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bald geht er tief verd\u00fcstert, nachdenkend, sonder Ruh,", "tokens": ["Bald", "geht", "er", "tief", "ver\u00b7d\u00fcs\u00b7tert", ",", "nach\u00b7den\u00b7kend", ",", "son\u00b7der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$,", "VVPP", "$,", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bald steht er still und fl\u00fcstert sich selber Fragen zu:", "tokens": ["Bald", "steht", "er", "still", "und", "fl\u00fcs\u00b7tert", "sich", "sel\u00b7ber", "Fra\u00b7gen", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PRF", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbkann er, was ich gefordert, mir wirklich bringen? Nein!", "tokens": ["\u00bb", "kann", "er", ",", "was", "ich", "ge\u00b7for\u00b7dert", ",", "mir", "wirk\u00b7lich", "brin\u00b7gen", "?", "Nein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$,", "PPER", "ADJD", "VVINF", "$.", "PTKANT", "$."], "meter": "+-++----+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wird ein Phantom des Abgrunds nicht die Gew\u00fcnschte sein?", "tokens": ["Wird", "ein", "Phan\u00b7tom", "des", "Ab\u00b7grunds", "nicht", "die", "Ge\u00b7w\u00fcnschte", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ART", "NN", "VAINF", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ist Phantom nicht alles, was unser Auge sieht?", "tokens": ["Und", "ist", "Phan\u00b7tom", "nicht", "al\u00b7les", ",", "was", "un\u00b7ser", "Au\u00b7ge", "sieht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "PTKNEG", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Die Sch\u00f6nheit, wie sie leuchtet, die Blume, wie sie bl\u00fcht?\u00ab", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "wie", "sie", "leuch\u00b7tet", ",", "die", "Blu\u00b7me", ",", "wie", "sie", "bl\u00fcht", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbwer ist, der seinem Dasein ein dauernd Gl\u00fcck erstrebt?", "tokens": ["\u00bb", "wer", "ist", ",", "der", "sei\u00b7nem", "Da\u00b7sein", "ein", "dau\u00b7ernd", "Gl\u00fcck", "er\u00b7strebt", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gl\u00fcck ist ein G\u00f6tzenbildniss, das unser Wunsch er hebt.", "tokens": ["Gl\u00fcck", "ist", "ein", "G\u00f6t\u00b7zen\u00b7bild\u00b7niss", ",", "das", "un\u00b7ser", "Wunsch", "er", "hebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ist unser Wunsch befriedigt, dann, G\u00f6ttin, fahre wohl!", "tokens": ["Ist", "un\u00b7ser", "Wunsch", "be\u00b7frie\u00b7digt", ",", "dann", ",", "G\u00f6t\u00b7tin", ",", "fah\u00b7re", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,", "ADV", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wir finden bald ein neues, ein sch\u00f6neres Idol.\u00ab", "tokens": ["Wir", "fin\u00b7den", "bald", "ein", "neu\u00b7es", ",", "ein", "sch\u00f6\u00b7ne\u00b7res", "I\u00b7dol", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbvon einem Volk im Nordland seltsam die Sage klingt:", "tokens": ["\u00bb", "von", "ei\u00b7nem", "Volk", "im", "Nord\u00b7land", "selt\u00b7sam", "die", "Sa\u00b7ge", "klingt", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "APPRART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es schlage seine G\u00f6tzen, wenn ihm ein Wunsch misslingt.", "tokens": ["Es", "schla\u00b7ge", "sei\u00b7ne", "G\u00f6t\u00b7zen", ",", "wenn", "ihm", "ein", "Wunsch", "miss\u00b7lingt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sind wir um vieles besser? Wir knie'n vor einem Gott,", "tokens": ["Sind", "wir", "um", "vie\u00b7les", "bes\u00b7ser", "?", "Wir", "knie'n", "vor", "ei\u00b7nem", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "ADJD", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gleich, t\u00e4uscht uns Hoffnung, nennet ihn machtlos unser Spott.\u00ab", "tokens": ["Gleich", ",", "t\u00e4uscht", "uns", "Hoff\u00b7nung", ",", "nen\u00b7net", "ihn", "macht\u00b7los", "un\u00b7ser", "Spott", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbund ach, wie sollte dauernd ein Gl\u00fcck auf Erden sein?", "tokens": ["\u00bb", "und", "ach", ",", "wie", "soll\u00b7te", "dau\u00b7ernd", "ein", "Gl\u00fcck", "auf", "Er\u00b7den", "sein", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "XY", "$,", "PWAV", "VMFIN", "ADJD", "ART", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Kinder des Verlangens wiegt Zeit als Amme ein.", "tokens": ["Die", "Kin\u00b7der", "des", "Ver\u00b7lan\u00b7gens", "wiegt", "Zeit", "als", "Am\u00b7me", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NN", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie weinen oft, die Kinder; wenn sie sich m\u00fcd' geweint,", "tokens": ["Sie", "wei\u00b7nen", "oft", ",", "die", "Kin\u00b7der", ";", "wenn", "sie", "sich", "m\u00fcd'", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "$.", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Entschlummern sie, und schlummern vergessen, ja versteint.\u00ab", "tokens": ["Ent\u00b7schlum\u00b7mern", "sie", ",", "und", "schlum\u00b7mern", "ver\u00b7ges\u00b7sen", ",", "ja", "ver\u00b7steint", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "$,", "KON", "ADJD", "VVPP", "$,", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "So gr\u00fcnet \u00fcber Gr\u00e4bern gar \u00fcppig junges Moos.", "tokens": ["So", "gr\u00fc\u00b7net", "\u00fc\u00b7ber", "Gr\u00e4\u00b7bern", "gar", "\u00fcp\u00b7pig", "jun\u00b7ges", "Moos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So weckt zu neuem Leben, was Winterschlaf erstarrt,", "tokens": ["So", "weckt", "zu", "neu\u00b7em", "Le\u00b7ben", ",", "was", "Win\u00b7ter\u00b7schlaf", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,", "PWS", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aus trauerbleichem Schneekleid der Lenz der Gegen wart.\u00ab \u2013", "tokens": ["Aus", "trau\u00b7er\u00b7blei\u00b7chem", "Schnee\u00b7kleid", "der", "Lenz", "der", "Ge\u00b7gen", "wart", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Wie Faustus so vom Rocken der Zeit Gedanken spinnt,", "tokens": ["Wie", "Faus\u00b7tus", "so", "vom", "Ro\u00b7cken", "der", "Zeit", "Ge\u00b7dan\u00b7ken", "spinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und f\u00fcr sein eignes Hoffen sich Bild auf Bild ersinnt,", "tokens": ["Und", "f\u00fcr", "sein", "eig\u00b7nes", "Hof\u00b7fen", "sich", "Bild", "auf", "Bild", "er\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "PRF", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "St\u00f6rt ihn im tiefen Denken ein leiser Odemzug,", "tokens": ["St\u00f6rt", "ihn", "im", "tie\u00b7fen", "Den\u00b7ken", "ein", "lei\u00b7ser", "O\u00b7dem\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der sanft, wie Lenzess\u00e4useln an seine Wange schlug.", "tokens": ["Der", "sanft", ",", "wie", "Len\u00b7zes\u00b7s\u00e4u\u00b7seln", "an", "sei\u00b7ne", "Wan\u00b7ge", "schlug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PWAV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Um blickt er rasch, durchschauert vom Ahnungsschreck, und bebt:", "tokens": ["Um", "blickt", "er", "rasch", ",", "durch\u00b7schau\u00b7ert", "vom", "Ah\u00b7nungs\u00b7schreck", ",", "und", "bebt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ist's Aphroditens Liebreiz, der dem Olymp entschwebt?", "to