{"textgrid.poem.46173": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ode", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf, ihr dapfere soldaten,", "tokens": ["Frisch", "auf", ",", "ihr", "dap\u00b7fe\u00b7re", "sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPOSAT", "ADJA", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ihr, die ihr noch mit teutschem blut,", "tokens": ["ihr", ",", "die", "ihr", "noch", "mit", "teut\u00b7schem", "blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ihr, die ihr noch mit frischem mut", "tokens": ["ihr", ",", "die", "ihr", "noch", "mit", "fri\u00b7schem", "mut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "belebet, suchet gro\u00dfe thaten!", "tokens": ["be\u00b7le\u00b7bet", ",", "su\u00b7chet", "gro\u00b7\u00dfe", "tha\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ihr landsleut, ihr landsknecht frisch auf,", "tokens": ["ihr", "lands\u00b7leut", ",", "ihr", "lands\u00b7knecht", "frisch", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "das land, die freiheit sich verlieret,", "tokens": ["das", "land", ",", "die", "frei\u00b7heit", "sich", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "wan ihr nicht mutig schlaget drauf", "tokens": ["wan", "ihr", "nicht", "mu\u00b7tig", "schla\u00b7get", "drauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "ADJD", "VVFIN", "PAV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "und \u00fcberwindend triumfieret.", "tokens": ["und", "\u00fc\u00b7berw\u00b7in\u00b7dend", "tri\u00b7um\u00b7fie\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der ist ein Teutscher wolgeboren,", "tokens": ["Der", "ist", "ein", "Teut\u00b7scher", "wol\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der von betrug und falschheit frei,", "tokens": ["der", "von", "be\u00b7trug", "und", "falschheit", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "VVFIN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "hat weder redlichkeit, noch treu,", "tokens": ["hat", "we\u00b7der", "red\u00b7lich\u00b7keit", ",", "noch", "treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "KON", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "noch glauben, noch freiheit verloren:", "tokens": ["noch", "glau\u00b7ben", ",", "noch", "frei\u00b7heit", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "der ist ein Teutscher, ehrenwert,", "tokens": ["der", "ist", "ein", "Teut\u00b7scher", ",", "eh\u00b7ren\u00b7wert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "VAFIN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der wacker, herzhaft, unverzaget", "tokens": ["der", "wa\u00b7cker", ",", "herz\u00b7haft", ",", "un\u00b7ver\u00b7za\u00b7get"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJD", "$,", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "f\u00fcr die freiheit mit seinem schwert", "tokens": ["f\u00fcr", "die", "frei\u00b7heit", "mit", "sei\u00b7nem", "schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "in einige gefahr sich waget.", "tokens": ["in", "ei\u00b7ni\u00b7ge", "ge\u00b7fahr", "sich", "wa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dan wan ihn schon die feind verwunden", "tokens": ["Dan", "wan", "ihn", "schon", "die", "feind", "ver\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und nemen ihm das leben hin,", "tokens": ["und", "ne\u00b7men", "ihm", "das", "le\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ist ruhm und ehr doch sein gewin", "tokens": ["ist", "ruhm", "und", "ehr", "doch", "sein", "ge\u00b7win"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und er ist gar nicht \u00fcberwunden:", "tokens": ["und", "er", "ist", "gar", "nicht", "\u00fc\u00b7berw\u00b7un\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ein solcher tod ist ihm nicht schwer,", "tokens": ["ein", "sol\u00b7cher", "tod", "ist", "ihm", "nicht", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "weil sein gewissen ihn vers\u00fc\u00dfet,", "tokens": ["weil", "sein", "ge\u00b7wis\u00b7sen", "ihn", "ver\u00b7s\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "und er erwirbet lob und ehr,", "tokens": ["und", "er", "er\u00b7wir\u00b7bet", "lob", "und", "ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "indem er sein blut so vergie\u00dfet.", "tokens": ["in\u00b7dem", "er", "sein", "blut", "so", "ver\u00b7gie\u00b7\u00dfet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Sein nam und ruhm allzeit erklingen", "tokens": ["Sein", "nam", "und", "ruhm", "all\u00b7zeit", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "in allem land, in jedem mund,", "tokens": ["in", "al\u00b7lem", "land", ",", "in", "je\u00b7dem", "mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sein leben durch den tod wird kund,", "tokens": ["sein", "le\u00b7ben", "durch", "den", "tod", "wird", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAINF", "VVFIN", "APPR", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "weil sein lob die nachk\u00f6mling singen;", "tokens": ["weil", "sein", "lob", "die", "nach\u00b7k\u00f6m\u00b7ling", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "die edle freiheit ist die frucht,", "tokens": ["die", "ed\u00b7le", "frei\u00b7heit", "ist", "die", "frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die er dem vatterland verlasset;", "tokens": ["die", "er", "dem", "vat\u00b7ter\u00b7land", "ver\u00b7las\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "da er herzlos durch seine flucht", "tokens": ["da", "er", "herz\u00b7los", "durch", "sei\u00b7ne", "flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.8": {"text": "wird ganz verachtet und gehasset.", "tokens": ["wird", "ganz", "ver\u00b7ach\u00b7tet", "und", "ge\u00b7has\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Also zu leben und zu sterben", "tokens": ["Al\u00b7so", "zu", "le\u00b7ben", "und", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "gilt dem aufrechten Teutschen gleich", "tokens": ["gilt", "dem", "auf\u00b7rech\u00b7ten", "Teut\u00b7schen", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "der tod und sig seind sch\u00f6n und reich,", "tokens": ["der", "tod", "und", "sig", "seind", "sch\u00f6n", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "durch beed kan er sein heil erwerben;", "tokens": ["durch", "beed", "kan", "er", "sein", "heil", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VMFIN", "PPER", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "hingegen fliehen allen dank", "tokens": ["hin\u00b7ge\u00b7gen", "flie\u00b7hen", "al\u00b7len", "dank"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "die fl\u00fcchtigen und der verr\u00e4ter,", "tokens": ["die", "fl\u00fcch\u00b7ti\u00b7gen", "und", "der", "ver\u00b7r\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und ihnen folget ein gestank,", "tokens": ["und", "ih\u00b7nen", "fol\u00b7get", "ein", "ge\u00b7stank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "weil sie verfluchte \u00fcbelth\u00e4ter.", "tokens": ["weil", "sie", "ver\u00b7fluch\u00b7te", "\u00fc\u00b7belt\u00b7h\u00e4\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wolan deshalb, ihr wahre Teutschen,", "tokens": ["Wo\u00b7lan", "des\u00b7halb", ",", "ihr", "wah\u00b7re", "Teut\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mit teutscher faust, mit teutschem mut,", "tokens": ["mit", "teut\u00b7scher", "faust", ",", "mit", "teut\u00b7schem", "mut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "d\u00e4mpfet nu der tyrannen wut,", "tokens": ["d\u00e4mp\u00b7fet", "nu", "der", "ty\u00b7ran\u00b7nen", "wut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "zerbrechet ihr joch, band und beutschen:", "tokens": ["zer\u00b7bre\u00b7chet", "ihr", "joch", ",", "band", "und", "beut\u00b7schen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "un\u00fcberwindlich r\u00fchmet sie", "tokens": ["un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "r\u00fch\u00b7met", "sie"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ihr titul, torheit und stolzieren;", "tokens": ["ihr", "ti\u00b7tul", ",", "tor\u00b7heit", "und", "stol\u00b7zie\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "aber ihr heer mit schlechter m\u00fch", "tokens": ["a\u00b7ber", "ihr", "heer", "mit", "schlech\u00b7ter", "m\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "mag, \u00fcberwindlich, bald verlieren.", "tokens": ["mag", ",", "\u00fc\u00b7berw\u00b7ind\u00b7lich", ",", "bald", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ADJD", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ha! fallet in sie! ihre fahnen", "tokens": ["Ha", "!", "fal\u00b7let", "in", "sie", "!", "ih\u00b7re", "fah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "APPR", "PPER", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zittern aus forcht: sie trennen sich,", "tokens": ["zit\u00b7tern", "aus", "forcht", ":", "sie", "tren\u00b7nen", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "PRF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "ihr b\u00f6se sach h\u00e4lt nicht den stich,", "tokens": ["ihr", "b\u00f6\u00b7se", "sach", "h\u00e4lt", "nicht", "den", "stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "drum zu der flucht sie sich schon mahnen;", "tokens": ["drum", "zu", "der", "flucht", "sie", "sich", "schon", "mah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "gro\u00df ist ihr heer, klein ist ihr glaub;", "tokens": ["gro\u00df", "ist", "ihr", "heer", ",", "klein", "ist", "ihr", "glaub", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "gut ist ihr zeug, b\u00f6s ihr gewissen.", "tokens": ["gut", "ist", "ihr", "zeug", ",", "b\u00f6s", "ihr", "ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["A