{"dta.poem.7882": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Zufriedenen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df bei jener Linde", "tokens": ["Ich", "sa\u00df", "bei", "je\u00b7ner", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit meinem trauten Kinde,", "tokens": ["Mit", "mei\u00b7nem", "trau\u00b7ten", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sa\u00dfen Hand in Hand.", "tokens": ["Wir", "sa\u00b7\u00dfen", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kein Bl\u00e4ttchen rauscht\u2019 im Winde,", "tokens": ["Kein", "Bl\u00e4tt\u00b7chen", "rauscht'", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Sonne schien gelinde", "tokens": ["Die", "Son\u00b7ne", "schien", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Herab auf\u2019s stille Land.", "tokens": ["Her\u00b7ab", "auf's", "stil\u00b7le", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir sa\u00dfen ganz verschwiegen,", "tokens": ["Wir", "sa\u00b7\u00dfen", "ganz", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit innigem Vergn\u00fcgen,", "tokens": ["Mit", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das Herz kaum merklich schlug.", "tokens": ["Das", "Herz", "kaum", "merk\u00b7lich", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was sollten wir auch sagen?", "tokens": ["Was", "soll\u00b7ten", "wir", "auch", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was konnten wir uns fragen?", "tokens": ["Was", "konn\u00b7ten", "wir", "uns", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir wu\u00dften ja genug.", "tokens": ["Wir", "wu\u00df\u00b7ten", "ja", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es mocht\u2019 uns nichts mehr fehlen,", "tokens": ["Es", "mocht'", "uns", "nichts", "mehr", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Sehnen konnt\u2019 uns qu\u00e4len,", "tokens": ["Kein", "Seh\u00b7nen", "konnt'", "uns", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nichts Liebes war uns fern.", "tokens": ["Nichts", "Lie\u00b7bes", "war", "uns", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus liebem Aug\u2019 ein Gr\u00fcssen,", "tokens": ["Aus", "lie\u00b7bem", "Aug'", "ein", "Gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vom lieben Mund ein K\u00fcssen", "tokens": ["Vom", "lie\u00b7ben", "Mund", "ein", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gab Eins dem Andern gern.", "tokens": ["Gab", "Eins", "dem", "An\u00b7dern", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}