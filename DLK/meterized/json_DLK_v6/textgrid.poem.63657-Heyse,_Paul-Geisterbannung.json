{"textgrid.poem.63657": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Geisterbannung", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag ist tr\u00fcb, die Welt ist grau,", "tokens": ["Der", "Tag", "ist", "tr\u00fcb", ",", "die", "Welt", "ist", "grau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Bodensa\u00df in schalem Wein,", "tokens": ["Wie", "Bo\u00b7den\u00b7sa\u00df", "in", "scha\u00b7lem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00e4tt' ich nicht die liebe Frau,", "tokens": ["Und", "h\u00e4tt'", "ich", "nicht", "die", "lie\u00b7be", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl schlich' ein b\u00f6ser Geist sich ein.", "tokens": ["Wohl", "schlich'", "ein", "b\u00f6\u00b7ser", "Geist", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der M\u00e4rzwind im Kamine raunt,", "tokens": ["Der", "M\u00e4r\u00b7zwind", "im", "Ka\u00b7mi\u00b7ne", "raunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Strichregen an die Scheiben spr\u00fcht,", "tokens": ["Strich\u00b7re\u00b7gen", "an", "die", "Schei\u00b7ben", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nur Sie ist immer holdgelaunt", "tokens": ["Nur", "Sie", "ist", "im\u00b7mer", "hold\u00b7ge\u00b7launt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und um mein Seelenheil bem\u00fcht.", "tokens": ["Und", "um", "mein", "See\u00b7len\u00b7heil", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vom n\u00e4chsten Sommer spricht sie mir,", "tokens": ["Vom", "n\u00e4chs\u00b7ten", "Som\u00b7mer", "spricht", "sie", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo unsre Kinder bei uns sind.", "tokens": ["Wo", "uns\u00b7re", "Kin\u00b7der", "bei", "uns", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir klingt die liebe Stimme schier,", "tokens": ["Mir", "klingt", "die", "lie\u00b7be", "Stim\u00b7me", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als h\u00f6rt' ich schon den Fr\u00fchlingswind.", "tokens": ["Als", "h\u00f6rt'", "ich", "schon", "den", "Fr\u00fch\u00b7lings\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u2013 O sieh, schon l\u00e4nger wird der Tag,", "tokens": ["\u2013", "O", "sieh", ",", "schon", "l\u00e4n\u00b7ger", "wird", "der", "Tag", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "PTKVZ", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne gibt sich redlich M\u00fch'.", "tokens": ["Die", "Son\u00b7ne", "gibt", "sich", "red\u00b7lich", "M\u00fch'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heut morgen ward von Amselschlag", "tokens": ["Heut", "mor\u00b7gen", "ward", "von", "Am\u00b7sel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schon geweckt in aller Fr\u00fch',", "tokens": ["Ich", "schon", "ge\u00b7weckt", "in", "al\u00b7ler", "Fr\u00fch'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und hab' im Garten zart und frisch", "tokens": ["Und", "hab'", "im", "Gar\u00b7ten", "zart", "und", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ersten Veilchen schon gesehn.", "tokens": ["Die", "ers\u00b7ten", "Veil\u00b7chen", "schon", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier stell' ich sie auf deinen Tisch;", "tokens": ["Hier", "stell'", "ich", "sie", "auf", "dei\u00b7nen", "Tisch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du aber sollst ins Freie gehn.", "tokens": ["Du", "a\u00b7ber", "sollst", "ins", "Frei\u00b7e", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du hast zu viel getr\u00e4umt, gedacht,", "tokens": ["Du", "hast", "zu", "viel", "ge\u00b7tr\u00e4umt", ",", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "PIS", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis dir das Blut am Herzen stockt.", "tokens": ["Bis", "dir", "das", "Blut", "am", "Her\u00b7zen", "stockt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Mutter kommt heut auf die Nacht,", "tokens": ["Die", "Mut\u00b7ter", "kommt", "heut", "auf", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann wird geplaudert und tarockt. \u2013 \u2013", "tokens": ["Dann", "wird", "ge\u00b7plau\u00b7dert", "und", "ta\u00b7rockt", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "KON", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Tag ist tr\u00fcb, die Welt ist grau,", "tokens": ["Der", "Tag", "ist", "tr\u00fcb", ",", "die", "Welt", "ist", "grau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch warm und heiter ist's im Haus.", "tokens": ["Doch", "warm", "und", "hei\u00b7ter", "ist's", "im", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das L\u00e4cheln einer lieben Frau", "tokens": ["Das", "L\u00e4\u00b7cheln", "ei\u00b7ner", "lie\u00b7ben", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Treibt alle b\u00f6sen Geister aus.", "tokens": ["Treibt", "al\u00b7le", "b\u00f6\u00b7sen", "Geis\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}