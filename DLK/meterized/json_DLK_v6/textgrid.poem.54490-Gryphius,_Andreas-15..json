{"textgrid.poem.54490": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ewig treue Gott/ hat die nicht wehrte Welt.", "tokens": ["Der", "e\u00b7wig", "treu\u00b7e", "Gott", "/", "hat", "die", "nicht", "wehr\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$(", "VAFIN", "ART", "PTKNEG", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hoch au\u00df heisser Lieb' in seinem Sinn gesch\u00e4tzet/", "tokens": ["So", "hoch", "au\u00df", "heis\u00b7ser", "Lieb'", "in", "sei\u00b7nem", "Sinn", "ge\u00b7sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Er sein einig Kind/ f\u00fcr sie in Tod gesetzet/", "tokens": ["Da\u00df", "Er", "sein", "ei\u00b7nig", "Kind", "/", "f\u00fcr", "sie", "in", "Tod", "ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "$(", "APPR", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein Kind der Erden Heil vnd Rath L\u00f6se-Geld:", "tokens": ["Sein", "Kind", "der", "Er\u00b7den", "Heil", "vnd", "Rath", "L\u00f6\u00b7se\u00b7Geld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Wer sich mit festem Ernst an diesen Mitler h\u00e4llt/", "tokens": ["Wer", "sich", "mit", "fes\u00b7tem", "Ernst", "an", "die\u00b7sen", "Mit\u00b7ler", "h\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den hat kein Vntergang/ kein Hellen-Sturm verletzet", "tokens": ["Den", "hat", "kein", "Vn\u00b7ter\u00b7gang", "/", "kein", "Hel\u00b7len\u00b7Sturm", "ver\u00b7let\u00b7zet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "PIAT", "NN", "$(", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er wird im Freuden-Schlo\u00df de\u00df Himmel stets ergetzet", "tokens": ["Er", "wird", "im", "Freu\u00b7den\u00b7Schlo\u00df", "de\u00df", "Him\u00b7mel", "stets", "er\u00b7get\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn nun der Erden Bau in letzten Brand' einf\u00e4llt.", "tokens": ["Wenn", "nun", "der", "Er\u00b7den", "Bau", "in", "letz\u00b7ten", "Brand'", "ein\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gott hat vns nicht sein Kind zu Straffe senden wollen/", "tokens": ["Gott", "hat", "vns", "nicht", "sein", "Kind", "zu", "Straf\u00b7fe", "sen\u00b7den", "wol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sein Kind/ durch das wir Freud' vnd Heil erlangen sollen.", "tokens": ["Sein", "Kind", "/", "durch", "das", "wir", "Freud'", "vnd", "Heil", "er\u00b7lan\u00b7gen", "sol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "APPR", "PRELS", "PPER", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer standthafft auff ihn traut; den schreckt sein Vrteil nicht", "tokens": ["Wer", "standt\u00b7hafft", "auff", "ihn", "traut", ";", "den", "schreckt", "sein", "Vrt\u00b7eil", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "VVFIN", "$.", "ART", "VVFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer diese H\u00fclff au\u00dfschl\u00e4gt/ vnd sich durch grobe S\u00fcnden", "tokens": ["Wer", "die\u00b7se", "H\u00fclff", "au\u00df\u00b7schl\u00e4gt", "/", "vnd", "sich", "durch", "gro\u00b7be", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$(", "KON", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wil mit Beelzebub vnd Beltal verbinden/", "tokens": ["Wil", "mit", "Beel\u00b7ze\u00b7bub", "vnd", "Bel\u00b7tal", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NE", "KON", "NN", "VVINF", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.14": {"text": "Bleibt ewig schwer verdampt/ vnd schaut kein Himmels-Licht.", "tokens": ["Bleibt", "e\u00b7wig", "schwer", "ver\u00b7dampt", "/", "vnd", "schaut", "kein", "Him\u00b7mels\u00b7Licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "VVFIN", "$(", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}