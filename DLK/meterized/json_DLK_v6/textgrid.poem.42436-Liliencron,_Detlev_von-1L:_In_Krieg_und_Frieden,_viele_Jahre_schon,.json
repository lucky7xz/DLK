{"textgrid.poem.42436": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Krieg und Frieden, viele Jahre schon,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Krieg und Frieden, viele Jahre schon,", "tokens": ["In", "Krieg", "und", "Frie\u00b7den", ",", "vie\u00b7le", "Jah\u00b7re", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Trag' ich, wo immer auch mein Aufenthalt,", "tokens": ["Trag'", "ich", ",", "wo", "im\u00b7mer", "auch", "mein", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Herzen deinen Quickborn, und im Herzen", "tokens": ["Am", "Her\u00b7zen", "dei\u00b7nen", "Quick\u00b7born", ",", "und", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die goldne F\u00fclle seiner Heimatlieder.", "tokens": ["Die", "gold\u00b7ne", "F\u00fcl\u00b7le", "sei\u00b7ner", "Hei\u00b7mat\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im harten Winter Siebzig-Einundsiebzig", "tokens": ["Im", "har\u00b7ten", "Win\u00b7ter", "Sieb\u00b7zig\u00b7Ein\u00b7und\u00b7sieb\u00b7zig"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stand vor Peronne ich zur Umzingelung.", "tokens": ["Stand", "vor", "Pe\u00b7ron\u00b7ne", "ich", "zur", "Um\u00b7zin\u00b7ge\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Einst, als drei Tage und drei N\u00e4chte wir", "tokens": ["Einst", ",", "als", "drei", "Ta\u00b7ge", "und", "drei", "N\u00e4ch\u00b7te", "wir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "CARD", "NN", "KON", "CARD", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Schnee gelegen ohne Schutz und Feuer,", "tokens": ["Im", "Schnee", "ge\u00b7le\u00b7gen", "oh\u00b7ne", "Schutz", "und", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erhielt ich endlich als Quartier ein H\u00e4uschen.", "tokens": ["Er\u00b7hielt", "ich", "end\u00b7lich", "als", "Quar\u00b7tier", "ein", "H\u00e4usc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nur eine Stube gab's: Ein M\u00fctterchen", "tokens": ["Nur", "ei\u00b7ne", "Stu\u00b7be", "gab's", ":", "Ein", "M\u00fct\u00b7ter\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Sa\u00df h\u00fcstelnd, stier und stumpfsinnig im Bett.", "tokens": ["Sa\u00df", "h\u00fcs\u00b7telnd", ",", "stier", "und", "stumpf\u00b7sin\u00b7nig", "im", "Bett", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihr h\u00fcbsches sechzehnj\u00e4hriges Enkelkind", "tokens": ["Ihr", "h\u00fcb\u00b7sches", "sech\u00b7zehn\u00b7j\u00e4h\u00b7ri\u00b7ges", "En\u00b7kel\u00b7kind"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "\u2013 Zigeuner waren's \u2013 machte die Honneurs.", "tokens": ["\u2013", "Zi\u00b7geu\u00b7ner", "wa\u00b7ren's", "\u2013", "mach\u00b7te", "die", "Hon\u00b7neurs", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VAFIN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Rasch schob mein flinker Bursche am Kamin", "tokens": ["Rasch", "schob", "mein", "flin\u00b7ker", "Bur\u00b7sche", "am", "Ka\u00b7min"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das Stroh zusammen, da\u00df ich ruhen konnte,", "tokens": ["Das", "Stroh", "zu\u00b7sam\u00b7men", ",", "da\u00df", "ich", "ru\u00b7hen", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und wie der ganz erstarrte Frosch, so taute", "tokens": ["Und", "wie", "der", "ganz", "er\u00b7starr\u00b7te", "Frosch", ",", "so", "tau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADV", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Allm\u00e4hlig ich zu warmem Leben auf.", "tokens": ["All\u00b7m\u00e4h\u00b7lig", "ich", "zu", "war\u00b7mem", "Le\u00b7ben", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Behaglich nahm ich deinen Quickborn her", "tokens": ["Be\u00b7hag\u00b7lich", "nahm", "ich", "dei\u00b7nen", "Quick\u00b7born", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und las, den Kopf in meine H\u00e4nde st\u00fctzend,", "tokens": ["Und", "las", ",", "den", "Kopf", "in", "mei\u00b7ne", "H\u00e4n\u00b7de", "st\u00fct\u00b7zend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Gestreckten Leibes, laut die lieben Verse.", "tokens": ["Ge\u00b7streck\u00b7ten", "Lei\u00b7bes", ",", "laut", "die", "lie\u00b7ben", "Ver\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.17": {"text": "Mir gegen\u00fcber, zaghaft erst, dann dreister,", "tokens": ["Mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ",", "zag\u00b7haft", "erst", ",", "dann", "dreis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADJD", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Haupt gegen Haupt, dieselbe Stellung findend,", "tokens": ["Haupt", "ge\u00b7gen", "Haupt", ",", "die\u00b7sel\u00b7be", "Stel\u00b7lung", "fin\u00b7dend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PDAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "Das Kinn auf die geballten F\u00e4ustchen lastend,", "tokens": ["Das", "Kinn", "auf", "die", "ge\u00b7ball\u00b7ten", "F\u00e4ust\u00b7chen", "las\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Nahm Platz das M\u00e4dchen und \u2013 ich las ihr vor:", "tokens": ["Nahm", "Platz", "das", "M\u00e4d\u00b7chen", "und", "\u2013", "ich", "las", "ihr", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "KON", "$(", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Von \u00bbUnruh Hans\u00ab ... Noch seh' ich ihre Augen,", "tokens": ["Von", "\u00bb", "Un\u00b7ruh", "Hans", "\u00ab", "...", "Noch", "seh'", "ich", "ih\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "NN", "NE", "$(", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die dunkelbraunen, staunend mich betrachten;", "tokens": ["Die", "dun\u00b7kel\u00b7brau\u00b7nen", ",", "stau\u00b7nend", "mich", "be\u00b7trach\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Seh' auf der broncefarbnen Stirn ein L\u00f6ckchen,", "tokens": ["Seh'", "auf", "der", "bron\u00b7ce\u00b7farb\u00b7nen", "Stirn", "ein", "L\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "So schwarz als w\u00e4r' es aus der Nacht gesprungen.", "tokens": ["So", "schwarz", "als", "w\u00e4r'", "es", "aus", "der", "Nacht", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Dann fing sie an zu lachen und so k\u00f6stlich", "tokens": ["Dann", "fing", "sie", "an", "zu", "la\u00b7chen", "und", "so", "k\u00f6st\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Durchschimmerte der Zahn die roten Lippen,", "tokens": ["Durch\u00b7schim\u00b7mer\u00b7te", "der", "Zahn", "die", "ro\u00b7ten", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Da\u00df ich wahrhaftig in Versuchung kam,", "tokens": ["Da\u00df", "ich", "wahr\u00b7haf\u00b7tig", "in", "Ver\u00b7su\u00b7chung", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Ihr einen Finger in den Mund zu tauchen.", "tokens": ["Ihr", "ei\u00b7nen", "Fin\u00b7ger", "in", "den", "Mund", "zu", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und als ich weiter vortrug, das Gedicht:", "tokens": ["Und", "als", "ich", "wei\u00b7ter", "vor\u00b7trug", ",", "das", "Ge\u00b7dicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "\u2013 \u00bbIk sprung noch in de Kinnerb\u00fcx, da w\u00e4r", "tokens": ["\u2013", "\u00bb", "Ik", "sprung", "noch", "in", "de", "Kin\u00b7ner\u00b7b\u00fcx", ",", "da", "w\u00e4r"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "$(", "NE", "VVFIN", "ADV", "APPR", "NE", "NE", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ik all en\u00ab \u2013 kam ein Zischen, Heulen, Wuchten,", "tokens": ["Ik", "all", "en", "\u00ab", "\u2013", "kam", "ein", "Zi\u00b7schen", ",", "Heu\u00b7len", ",", "Wuch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "PIAT", "FM", "$(", "$(", "VVFIN", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ein Donnerschlag ... und eine Stille dann.", "tokens": ["Ein", "Don\u00b7ner\u00b7schlag", "...", "und", "ei\u00b7ne", "Stil\u00b7le", "dann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Das ganze H\u00fcttchen zittert, sch\u00fcttert, bebt,", "tokens": ["Das", "gan\u00b7ze", "H\u00fctt\u00b7chen", "zit\u00b7tert", ",", "sch\u00fct\u00b7tert", ",", "bebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Und an den W\u00e4nden rieselt es hinunter.", "tokens": ["Und", "an", "den", "W\u00e4n\u00b7den", "rie\u00b7selt", "es", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Wir aus dem Stroh. Das M\u00e4dchen, toterschrocken,", "tokens": ["Wir", "aus", "dem", "Stroh", ".", "Das", "M\u00e4d\u00b7chen", ",", "to\u00b7ter\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$.", "ART", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Liegt, wie das Lamm dem Hirten, mir im Arm.", "tokens": ["Liegt", ",", "wie", "das", "Lamm", "dem", "Hir\u00b7ten", ",", "mir", "im", "Arm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Bald fanden wir die unliebsame St\u00f6rung", "tokens": ["Bald", "fan\u00b7den", "wir", "die", "un\u00b7lieb\u00b7sa\u00b7me", "St\u00f6\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Erkl\u00e4rt: Es hatte in den Hof sich eine", "tokens": ["Er\u00b7kl\u00e4rt", ":", "Es", "hat\u00b7te", "in", "den", "Hof", "sich", "ei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "PPER", "VAFIN", "APPR", "ART", "NN", "PRF", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Granate, Gr\u00fc\u00dfe bringend, eingew\u00fchlt.", "tokens": ["Gra\u00b7na\u00b7te", ",", "Gr\u00fc\u00b7\u00dfe", "brin\u00b7gend", ",", "ein\u00b7ge\u00b7w\u00fchlt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.40": {"text": "Als wieder zum Kamin zur\u00fcck wir kehrten", "tokens": ["Als", "wie\u00b7der", "zum", "Ka\u00b7min", "zu\u00b7r\u00fcck", "wir", "kehr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PTKVZ", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Und ich mich niederbog zu deinem Buch,", "tokens": ["Und", "ich", "mich", "nie\u00b7der\u00b7bog", "zu", "dei\u00b7nem", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Entdeckt' ich auf dem Worte \u00bbDaugenix,\u00ab", "tokens": ["Ent\u00b7deckt'", "ich", "auf", "dem", "Wor\u00b7te", "\u00bb", "Dau\u00b7ge\u00b7nix", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$(", "NE", "$,", "$("], "meter": "-+-+-+-+-+"