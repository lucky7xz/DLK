{"textgrid.poem.23864": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Licht in der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ringsum dunkle Nacht.", "tokens": ["Ring\u00b7sum", "dunk\u00b7le", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "H\u00fcllt in Schwarz mich ein.", "tokens": ["H\u00fcllt", "in", "Schwarz", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zage flimmert gelb", "tokens": ["Za\u00b7ge", "flim\u00b7mert", "gelb"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ferneher ein Schein.", "tokens": ["Fer\u00b7ne\u00b7her", "ein", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ist als wie ein Trost,", "tokens": ["Ist", "als", "wie", "ein", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Eine Stimme still,", "tokens": ["Ei\u00b7ne", "Stim\u00b7me", "still", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die dein Herz aufruft,", "tokens": ["Die", "dein", "Herz", "auf\u00b7ruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Das verzagen will.", "tokens": ["Das", "ver\u00b7za\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Kleines, gelbes Licht,", "tokens": ["Klei\u00b7nes", ",", "gel\u00b7bes", "Licht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Bist mir wie der Stern", "tokens": ["Bist", "mir", "wie", "der", "Stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ueberm Hause einst", "tokens": ["Ue\u00b7berm", "Hau\u00b7se", "einst"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Jesuchrists des Herrn.", "tokens": ["Je\u00b7suc\u00b7hrists", "des", "Herrn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Und da l\u00f6scht es aus.", "tokens": ["Und", "da", "l\u00f6scht", "es", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und die Nacht wird schwer.", "tokens": ["Und", "die", "Nacht", "wird", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Schlafe, Herz, du h\u00f6rst", "tokens": ["Schla\u00b7fe", ",", "Herz", ",", "du", "h\u00f6rst"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Keine Stimme mehr.", "tokens": ["Kei\u00b7ne", "Stim\u00b7me", "mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}