{"textgrid.poem.65888": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Byron", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So gro\u00dfer Lord, der Sonne liebster Dichter,", "tokens": ["So", "gro\u00b7\u00dfer", "Lord", ",", "der", "Son\u00b7ne", "liebs\u00b7ter", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich Byron, ehrten Menschen, liebten Feeen;", "tokens": ["Dich", "By\u00b7ron", ",", "ehr\u00b7ten", "Men\u00b7schen", ",", "lieb\u00b7ten", "Fe\u00b7e\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Gefeiter J\u00e4ger, Blicke auch von Rehen", "tokens": ["Ge\u00b7fei\u00b7ter", "J\u00e4\u00b7ger", ",", "Bli\u00b7cke", "auch", "von", "Re\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerbrach dein Herz; du sterntest sie als Lichter.", "tokens": ["Zer\u00b7brach", "dein", "Herz", ";", "du", "stern\u00b7test", "sie", "als", "Lich\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein starkes Atmen trotzte warm dem Richter \u2013", "tokens": ["Dein", "star\u00b7kes", "At\u00b7men", "trotz\u00b7te", "warm", "dem", "Rich\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Heuchlerland \u2013 \u00fcber entkrampften Ehen:", "tokens": ["Im", "Heuch\u00b7ler\u00b7land", "\u2013", "\u00fc\u00b7ber", "ent\u00b7krampf\u00b7ten", "E\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "O gl\u00fccklich warst du nie \u2013 bei Wonnewehen \u2013", "tokens": ["O", "gl\u00fcck\u00b7lich", "warst", "du", "nie", "\u2013", "bei", "Won\u00b7ne\u00b7we\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "PPER", "ADV", "$(", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entschminkte rasch dein Morgenstrahl Gesichter.", "tokens": ["Ent\u00b7schmink\u00b7te", "rasch", "dein", "Mor\u00b7gen\u00b7strahl", "Ge\u00b7sich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dich feire ich, du bist ein Held geworden:", "tokens": ["Dich", "fei\u00b7re", "ich", ",", "du", "bist", "ein", "Held", "ge\u00b7wor\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Dichter, wo du bliebst, erfreut, verw\u00f6hnt,", "tokens": ["Mein", "Dich\u00b7ter", ",", "wo", "du", "bliebst", ",", "er\u00b7freut", ",", "ver\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verl\u00e4\u00dft mich niemand, wird kein Freund mich morden.", "tokens": ["Ver\u00b7l\u00e4\u00dft", "mich", "nie\u00b7mand", ",", "wird", "kein", "Freund", "mich", "mor\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "VAFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer ehrte mein Gedicht, da\u00df ers verh\u00f6hnt?", "tokens": ["Wer", "ehr\u00b7te", "mein", "Ge\u00b7dicht", ",", "da\u00df", "ers", "ver\u00b7h\u00f6hnt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch weil' ich froh \u2013 verwitternd unter Horden,", "tokens": ["Doch", "weil'", "ich", "froh", "\u2013", "ver\u00b7wit\u00b7ternd", "un\u00b7ter", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$(", "VVPP", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die du befreit h\u00e4ttest \u2013 auch fast verp\u00f6nt.", "tokens": ["Die", "du", "be\u00b7freit", "h\u00e4t\u00b7test", "\u2013", "auch", "fast", "ver\u00b7p\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$(", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}