{"textgrid.poem.24004": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Tanz auf der Tenne", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kreiste die Sense mit scharfem Schwung,", "tokens": ["Es", "kreis\u00b7te", "die", "Sen\u00b7se", "mit", "schar\u00b7fem", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es fielen die Halme, es sank das Gras,", "tokens": ["Es", "fie\u00b7len", "die", "Hal\u00b7me", ",", "es", "sank", "das", "Gras", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und die Sonne lachte der Ernte.", "tokens": ["Und", "die", "Son\u00b7ne", "lach\u00b7te", "der", "Ern\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Himmel war blau, und die Luft war hei\u00df,", "tokens": ["Der", "Him\u00b7mel", "war", "blau", ",", "und", "die", "Luft", "war", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und die Schnitterin schnitt und lachte dazu:", "tokens": ["Und", "die", "Schnit\u00b7te\u00b7rin", "schnitt", "und", "lach\u00b7te", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Oh, du Sonne, du Sonne, du gute!", "tokens": ["Oh", ",", "du", "Son\u00b7ne", ",", "du", "Son\u00b7ne", ",", "du", "gu\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "NN", "$,", "PPER", "NN", "$,", "PPER", "ADJA", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun ist es gesammelt, das goldene Korn,", "tokens": ["Nun", "ist", "es", "ge\u00b7sam\u00b7melt", ",", "das", "gol\u00b7de\u00b7ne", "Korn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und das duftige Heu liegt wolkenschwer", "tokens": ["Und", "das", "duf\u00b7ti\u00b7ge", "Heu", "liegt", "wol\u00b7ken\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Im Haus, unterm Dach: Nun sind wir dich los,", "tokens": ["Im", "Haus", ",", "un\u00b7term", "Dach", ":", "Nun", "sind", "wir", "dich", "los", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$.", "ADV", "VAFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Frau Sorge!", "tokens": ["Frau", "Sor\u00b7ge", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Nun klingen die Glocken zum Erntefest,", "tokens": ["Nun", "klin\u00b7gen", "die", "Glo\u00b7cken", "zum", "Ern\u00b7te\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nun wollen wir tanzen zwischen dem Heu,", "tokens": ["Nun", "wol\u00b7len", "wir", "tan\u00b7zen", "zwi\u00b7schen", "dem", "Heu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo unsere Schlegel dem K\u00f6rnertanz", "tokens": ["Wo", "un\u00b7se\u00b7re", "Schle\u00b7gel", "dem", "K\u00f6r\u00b7ner\u00b7tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Laut schlugen den Takt:", "tokens": ["Laut", "schlu\u00b7gen", "den", "Takt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Auf der Tenne.", "tokens": ["Auf", "der", "Ten\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Nun Schnitterin komm und reich mir die Hand,", "tokens": ["Nun", "Schnit\u00b7te\u00b7rin", "komm", "und", "reich", "mir", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "KON", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun will ich mal sehn, du fr\u00f6hliche Dirn,", "tokens": ["Nun", "will", "ich", "mal", "sehn", ",", "du", "fr\u00f6h\u00b7li\u00b7che", "Dirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ob deine Beine so lustig sind,", "tokens": ["Ob", "dei\u00b7ne", "Bei\u00b7ne", "so", "lus\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So voll Kraft und voll Schwung,", "tokens": ["So", "voll", "Kraft", "und", "voll", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie die Arme.", "tokens": ["Wie", "die", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Und die Geige singt,", "tokens": ["Und", "die", "Gei\u00b7ge", "singt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und der Brummba\u00df brummt,", "tokens": ["Und", "der", "Brumm\u00b7ba\u00df", "brummt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und die Pfeifen kichern und kullern wie toll,", "tokens": ["Und", "die", "Pfei\u00b7fen", "ki\u00b7chern", "und", "kul\u00b7lern", "wie", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "KON", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wir drehen uns wild", "tokens": ["Und", "wir", "dre\u00b7hen", "uns", "wild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Rundum, rundum", "tokens": ["Run\u00b7dum", ",", "run\u00b7dum"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "FM.la"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Zwischen duftendem Heu auf der Tenne.", "tokens": ["Zwi\u00b7schen", "duf\u00b7ten\u00b7dem", "Heu", "auf", "der", "Ten\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Warm f\u00fchl ich mir nah deine Fr\u00fchlingsbrust,", "tokens": ["Warm", "f\u00fchl", "ich", "mir", "nah", "dei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du flinkes M\u00e4del; ich halte dich fest,", "tokens": ["Du", "flin\u00b7kes", "M\u00e4\u00b7del", ";", "ich", "hal\u00b7te", "dich", "fest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich seh in dein Auge, es jauchzt mein Herz:", "tokens": ["Ich", "seh", "in", "dein", "Au\u00b7ge", ",", "es", "jauchzt", "mein", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Oh, du Sonne, du Sonne, du gute!", "tokens": ["Oh", ",", "du", "Son\u00b7ne", ",", "du", "Son\u00b7ne", ",", "du", "gu\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "NN", "$,", "PPER", "NN", "$,", "PPER", "ADJA", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}