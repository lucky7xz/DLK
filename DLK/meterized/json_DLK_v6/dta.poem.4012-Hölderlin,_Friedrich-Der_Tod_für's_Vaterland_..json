{"dta.poem.4012": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod f\u00fcr's Vaterland .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Du kommst, o Schlacht! schon wogen die J\u00fcnglinge", "tokens": ["Du", "kommst", ",", "o", "Schlacht", "!", "schon", "wo\u00b7gen", "die", "J\u00fcng\u00b7lin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinab von ihren H\u00fcgeln, hinab in's Thal,", "tokens": ["Hin\u00b7ab", "von", "ih\u00b7ren", "H\u00fc\u00b7geln", ",", "hin\u00b7ab", "in's", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo keck herauf die W\u00fcrger dringen,", "tokens": ["Wo", "keck", "her\u00b7auf", "die", "W\u00fcr\u00b7ger", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sicher der Kunst und des Arms, doch sichrer", "tokens": ["Si\u00b7cher", "der", "Kunst", "und", "des", "Arms", ",", "doch", "sich\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$,", "ADV", "ADJA"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "K\u00f6mmt \u00fcber sie die Seele der J\u00fcnglinge,", "tokens": ["K\u00f6mmt", "\u00fc\u00b7ber", "sie", "die", "See\u00b7le", "der", "J\u00fcng\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn die Gerechten schlagen, wie Zauberer,", "tokens": ["Denn", "die", "Ge\u00b7rech\u00b7ten", "schla\u00b7gen", ",", "wie", "Zau\u00b7be\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und ihre Vaterlandsges\u00e4nge", "tokens": ["Und", "ih\u00b7re", "Va\u00b7ter\u00b7lands\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4hmen die Kniee der Ehrelosen.", "tokens": ["L\u00e4h\u00b7men", "die", "Kni\u00b7ee", "der", "Eh\u00b7re\u00b7lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "O nehmt mich, nehmt mich mit in die Reihen auf,", "tokens": ["O", "nehmt", "mich", ",", "nehmt", "mich", "mit", "in", "die", "Rei\u00b7hen", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVFIN", "PRF", "APPR", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Damit ich einst nicht sterbe gemeinen Tods!", "tokens": ["Da\u00b7mit", "ich", "einst", "nicht", "ster\u00b7be", "ge\u00b7mei\u00b7nen", "Tods", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umsonst zu sterben, lieb' ich nicht, doch", "tokens": ["Um\u00b7sonst", "zu", "ster\u00b7ben", ",", "lieb'", "ich", "nicht", ",", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Lieb' ich, zu fallen am Opferh\u00fcgel", "tokens": ["Lieb'", "ich", ",", "zu", "fal\u00b7len", "am", "Op\u00b7fer\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PTKZU", "VVINF", "APPRART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "F\u00fcr's Vaterland, zu bluten des Herzens Blut", "tokens": ["F\u00fcr's", "Va\u00b7ter\u00b7land", ",", "zu", "blu\u00b7ten", "des", "Her\u00b7zens", "Blut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PTKZU", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "F\u00fcr's Vaterland \u2014 und bald ist's gescheh'n! Zu euch", "tokens": ["F\u00fcr's", "Va\u00b7ter\u00b7land", "und", "bald", "ist's", "ge\u00b7scheh'n", "!", "Zu", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$(", "KON", "ADV", "VAFIN", "VVPP", "$.", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Theuern! komm' ich, die mich leben", "tokens": ["Ihr", "Theu\u00b7ern", "!", "komm'", "ich", ",", "die", "mich", "le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,", "PRELS", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lehrten und sterben, zu euch hinunter!", "tokens": ["Lehr\u00b7ten", "und", "ster\u00b7ben", ",", "zu", "euch", "hin\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Wie oft im Lichte d\u00fcrstet' ich euch zu seh'n,", "tokens": ["Wie", "oft", "im", "Lich\u00b7te", "d\u00fcrs\u00b7tet'", "ich", "euch", "zu", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "VMFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr Helden und ihr Dichter aus alter Zeit!", "tokens": ["Ihr", "Hel\u00b7den", "und", "ihr", "Dich\u00b7ter", "aus", "al\u00b7ter", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun gr\u00fc\u00dft ihr freundlich den geringen", "tokens": ["Nun", "gr\u00fc\u00dft", "ihr", "freund\u00b7lich", "den", "ge\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fremdling und br\u00fcderlich ist's hier unten.", "tokens": ["Fremd\u00b7ling", "und", "br\u00fc\u00b7der\u00b7lich", "ist's", "hier", "un\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ADV", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}