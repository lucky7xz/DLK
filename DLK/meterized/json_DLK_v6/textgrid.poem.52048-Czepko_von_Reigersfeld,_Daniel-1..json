{"textgrid.poem.52048": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Maro seinen Geist, das Flaccus seinen Mund,", "tokens": ["Da\u00df", "Ma\u00b7ro", "sei\u00b7nen", "Geist", ",", "das", "Flac\u00b7cus", "sei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In solcher Pracht erhebt, so zierlich wei\u00df zu zwingen,", "tokens": ["In", "sol\u00b7cher", "Pracht", "er\u00b7hebt", ",", "so", "zier\u00b7lich", "wei\u00df", "zu", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geschicht, weil sie die Ver\u00df in denen Ha\u00fcsern singen,", "tokens": ["Ge\u00b7schicht", ",", "weil", "sie", "die", "Ver\u00df", "in", "de\u00b7nen", "Ha\u00fc\u00b7sern", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "PRELS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allda die Majest\u00e4t der Welt Beherrschung stund.", "tokens": ["All\u00b7da", "die", "Ma\u00b7jes\u00b7t\u00e4t", "der", "Welt", "Be\u00b7herr\u00b7schung", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich tichte weit von Rom. Allhier wo alle Kunst,", "tokens": ["Ich", "tich\u00b7te", "weit", "von", "Rom", ".", "All\u00b7hier", "wo", "al\u00b7le", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NE", "$.", "ADV", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo alle Wi\u00dfenschafft umb Arctos liegt verfroren,", "tokens": ["Wo", "al\u00b7le", "Wi\u00b7\u00dfen\u00b7schafft", "umb", "Arc\u00b7tos", "liegt", "ver\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "NE", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es leiht mir kein August und kein M\u00e6cenas Ohren,", "tokens": ["Es", "leiht", "mir", "kein", "Au\u00b7gust", "und", "kein", "M\u00e6\u00b7ce\u00b7nas", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "KON", "PIAT", "NN", "NN", "$,"], "meter": "-+--+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kein Leser st\u00f6hrt den Flei\u00df durch solche Gnad und Gunst.", "tokens": ["Kein", "Le\u00b7ser", "st\u00f6hrt", "den", "Flei\u00df", "durch", "sol\u00b7che", "Gnad", "und", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch ist noch mancher Freund, der meine B\u00fccher tr\u00e4gt,", "tokens": ["Doch", "ist", "noch", "man\u00b7cher", "Freund", ",", "der", "mei\u00b7ne", "B\u00fc\u00b7cher", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit dem Nachbar list: Sie fluchen auff die Waffen,", "tokens": ["Und", "mit", "dem", "Nach\u00b7bar", "list", ":", "Sie", "flu\u00b7chen", "auff", "die", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sofern ihr Czepko nicht kan im Parnassus schlaffen,", "tokens": ["So\u00b7fern", "ihr", "Czep\u00b7ko", "nicht", "kan", "im", "Par\u00b7nas\u00b7sus", "schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wiewol er sich dahin offt auff ein Ohr gelegt.", "tokens": ["Wie\u00b7wol", "er", "sich", "da\u00b7hin", "offt", "auff", "ein", "Ohr", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr Freund, ich wolte wol mein Deutschland zu verehrn,", "tokens": ["Ihr", "Freund", ",", "ich", "wol\u00b7te", "wol", "mein", "Deutschland", "zu", "ver\u00b7ehrn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So gut als ein Virgil Arma Virumque ruffen:", "tokens": ["So", "gut", "als", "ein", "Vir\u00b7gil", "Ar\u00b7ma", "Vi\u00b7rum\u00b7que", "ruf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NE", "NE", "NE", "VVFIN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch eher d\u00fcrfft ihr nicht auff solche Bl\u00e4tter hoffen,", "tokens": ["Doch", "e\u00b7her", "d\u00fcrfft", "ihr", "nicht", "auff", "sol\u00b7che", "Bl\u00e4t\u00b7ter", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bi\u00df mich wird ein August und ein M\u00e6cenas h\u00f6hrn.", "tokens": ["Bi\u00df", "mich", "wird", "ein", "Au\u00b7gust", "und", "ein", "M\u00e6\u00b7ce\u00b7nas", "h\u00f6hrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}