{"textgrid.poem.38351": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "L l Lerche", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Lerchlein in den L\u00fcften schwebt, und singt den Himmel an,", "tokens": ["Das", "Lerc\u00b7hlein", "in", "den", "L\u00fcf\u00b7ten", "schwebt", ",", "und", "singt", "den", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Vom gr\u00fcnen Feld es sich erhebt, und tr\u00f6st den Ackermann,", "tokens": ["Vom", "gr\u00fc\u00b7nen", "Feld", "es", "sich", "er\u00b7hebt", ",", "und", "tr\u00f6st", "den", "A\u00b7cker\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "PRF", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Gar hoch thut es sich schwingen, da\u00df mans kaum sehen mag,", "tokens": ["Gar", "hoch", "thut", "es", "sich", "schwin\u00b7gen", ",", "da\u00df", "mans", "kaum", "se\u00b7hen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "VVINF", "$,", "KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im Kreis herum thuts singen, lobt Gott den ganzen Tag.", "tokens": ["Im", "Kreis", "he\u00b7rum", "thuts", "sin\u00b7gen", ",", "lobt", "Gott", "den", "gan\u00b7zen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "VVFIN", "VVINF", "$,", "VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}