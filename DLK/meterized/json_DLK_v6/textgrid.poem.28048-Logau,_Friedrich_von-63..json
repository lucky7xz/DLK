{"textgrid.poem.28048": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergeben heist: umsonst vergebens was erlassen.", "tokens": ["Ver\u00b7ge\u00b7ben", "heist", ":", "um\u00b7sonst", "ver\u00b7ge\u00b7bens", "was", "er\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll Schuld vergeben seyn, wie kan ich dann nun fassen,", "tokens": ["Soll", "Schuld", "ver\u00b7ge\u00b7ben", "seyn", ",", "wie", "kan", "ich", "dann", "nun", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVPP", "VAINF", "$,", "PWAV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df sie verdienet sey? Was abgedient soll seyn,", "tokens": ["Da\u00df", "sie", "ver\u00b7die\u00b7net", "sey", "?", "Was", "ab\u00b7ge\u00b7dient", "soll", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$.", "PWS", "VVPP", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum darff ich allererst nicht bitten um verzeihn.", "tokens": ["Drum", "darff", "ich", "al\u00b7le\u00b7rerst", "nicht", "bit\u00b7ten", "um", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}