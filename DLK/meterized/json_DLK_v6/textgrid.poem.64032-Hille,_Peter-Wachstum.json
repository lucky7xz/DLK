{"textgrid.poem.64032": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Wachstum", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will nun innerer Herrscher werden.", "tokens": ["Ich", "will", "nun", "in\u00b7ne\u00b7rer", "Herr\u00b7scher", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mag Inneres mich als Sklave schlau umschleichen.", "tokens": ["Mag", "In\u00b7ne\u00b7res", "mich", "als", "Skla\u00b7ve", "schlau", "um\u00b7schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "KOUS", "NN", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es stockt der Schritt, so nur sich etwas regt.", "tokens": ["Es", "stockt", "der", "Schritt", ",", "so", "nur", "sich", "et\u00b7was", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "PRF", "ADV", "ADJA"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da drau\u00dfen, wo nicht reicht das Sein.", "tokens": ["Da", "drau\u00b7\u00dfen", ",", "wo", "nicht", "reicht", "das", "Sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Also weiter wachsen!", "tokens": ["Al\u00b7so", "wei\u00b7ter", "wach\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Haben's gut die Pflanzen!", "tokens": ["Ha\u00b7ben's", "gut", "die", "Pflan\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Weh und qu\u00e4lend w\u00e4chst, was ein Mensch ist, in sich hinein.", "tokens": ["Weh", "und", "qu\u00e4\u00b7lend", "w\u00e4chst", ",", "was", "ein", "Mensch", "ist", ",", "in", "sich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$,", "PRELS", "ART", "NN", "VAFIN", "$,", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Immer mehr hinein. Nicht hinaus wie die Pflanzen.", "tokens": ["Im\u00b7mer", "mehr", "hin\u00b7ein", ".", "Nicht", "hin\u00b7aus", "wie", "die", "Pflan\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$.", "PTKNEG", "PTKVZ", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Wie eine Traumeswand mit H\u00e4nden unserer Seele wir schieben. \u2013", "tokens": ["Wie", "ei\u00b7ne", "Trau\u00b7mes\u00b7wand", "mit", "H\u00e4n\u00b7den", "un\u00b7se\u00b7rer", "See\u00b7le", "wir", "schie\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Aus grellem, f\u00fcrchterlichem Urgebilde.", "tokens": ["Aus", "grel\u00b7lem", ",", "f\u00fcrch\u00b7ter\u00b7li\u00b7chem", "Ur\u00b7ge\u00b7bil\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da sollen wir denn hausen, in rundergossenem Kerker.", "tokens": ["Da", "sol\u00b7len", "wir", "denn", "hau\u00b7sen", ",", "in", "run\u00b7der\u00b7gos\u00b7se\u00b7nem", "Ker\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Eingekerkert.", "tokens": ["Ein\u00b7ge\u00b7ker\u00b7kert", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Und nun freie klare Luft der gro\u00dfen Wirklichkeit.", "tokens": ["Und", "nun", "frei\u00b7e", "kla\u00b7re", "Luft", "der", "gro\u00b7\u00dfen", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "O Traum, du furchtbar naher Nachbar.", "tokens": ["O", "Traum", ",", "du", "furcht\u00b7bar", "na\u00b7her", "Nach\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und wild, ganz anderer.", "tokens": ["Und", "wild", ",", "ganz", "an\u00b7de\u00b7rer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJA", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Und was werden dann f\u00fcr andere kommen. \u2013", "tokens": ["Und", "was", "wer\u00b7den", "dann", "f\u00fcr", "an\u00b7de\u00b7re", "kom\u00b7men", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADV", "APPR", "PIS", "VVINF", "$.", "$("], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "O Welt, bist du furchtbar:", "tokens": ["O", "Welt", ",", "bist", "du", "furcht\u00b7bar", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Denn du hast einen Sinn.", "tokens": ["Denn", "du", "hast", "ei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und den erf\u00fcllst du und marterst uns zu deinem Leben.", "tokens": ["Und", "den", "er\u00b7f\u00fcllst", "du", "und", "mar\u00b7terst", "uns", "zu", "dei\u00b7nem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Und darum Geschlechtsfeste, denen Fleisch w\u00e4chst.", "tokens": ["Und", "da\u00b7rum", "Ge\u00b7schlechts\u00b7fes\u00b7te", ",", "de\u00b7nen", "Fleisch", "w\u00e4chst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "NN", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "So, nun, ihr schweren, scheuen Kymren Sch\u00f6\u00dfe,", "tokens": ["So", ",", "nun", ",", "ihr", "schwe\u00b7ren", ",", "scheu\u00b7en", "Kym\u00b7ren", "Sch\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "PPER", "VVINF", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Sollt ihr euch schwingen wie \u00fcppig bleiche Sterne,", "tokens": ["Sollt", "ihr", "euch", "schwin\u00b7gen", "wie", "\u00fcp\u00b7pig", "blei\u00b7che", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "KOKOM", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.23": {"text": "Wie Anemonenseelen.", "tokens": ["Wie", "A\u00b7ne\u00b7mo\u00b7nen\u00b7see\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "\u00c4olsharfenglutend.", "tokens": ["\u00c4\u00b7ols\u00b7har\u00b7fen\u00b7glu\u00b7tend", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Maskenzug.", "tokens": ["Mas\u00b7ken\u00b7zug", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "Cider.", "tokens": ["Ci\u00b7der", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Wollt ihr Cider dazu trinken?", "tokens": ["Wollt", "ihr", "Ci\u00b7der", "da\u00b7zu", "trin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Ernst berauscht sollt ihr Kinder wollen,", "tokens": ["Ernst", "be\u00b7rauscht", "sollt", "ihr", "Kin\u00b7der", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VMFIN", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.29": {"text": "Nichtsverhohlen, verstohlen, insgeheim.", "tokens": ["Nichts\u00b7ver\u00b7hoh\u00b7len", ",", "ver\u00b7stoh\u00b7len", ",", "ins\u00b7ge\u00b7heim", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Nein, Kinder des Volkes. \u2013", "tokens": ["Nein", ",", "Kin\u00b7der", "des", "Vol\u00b7kes", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Zugewollt; wie einen Becher euch dem Vaterlande zugetragen.", "tokens": ["Zu\u00b7ge\u00b7wollt", ";", "wie", "ei\u00b7nen", "Be\u00b7cher", "euch", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "zu\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PWAV", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.32": {"text": "Der heilige Gral.", "tokens": ["Der", "hei\u00b7li\u00b7ge", "Gral", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.33": {"text": "Und wollt ihr nicht?", "tokens": ["Und", "wollt", "ihr", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.34": {"text": "Wollt ihr nicht die Wonnestr\u00f6me durch eures", "tokens": ["Wollt", "ihr", "nicht", "die", "Won\u00b7nes\u00b7tr\u00f6\u00b7me", "durch", "eu\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "PPOSAT"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Lebens Str\u00f6me flie\u00dfen lassen, so lebt euch geistig,", "tokens": ["Le\u00b7bens", "Str\u00f6\u00b7me", "flie\u00b7\u00dfen", "las\u00b7sen", ",", "so", "lebt", "euch", "geis\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.36": {"text": "Frei und geistig aneinander hoch.", "tokens": ["Frei", "und", "geis\u00b7tig", "an\u00b7ein\u00b7an\u00b7der", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Und fallt ihr:", "tokens": ["Und", "fallt", "ihr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.38": {"text": "Nicht gar so schlimm,", "tokens": ["Nicht", "gar", "so", "schlimm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.39": {"text": "So fallt auf Blumen ihr und Kr\u00e4uter hin.", "tokens": ["So", "fallt", "auf", "Blu\u00b7men", "ihr", "und", "Kr\u00e4u\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPER", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Und eine Nachtigall fliegt weg.", "tokens": ["Und", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "fliegt", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und sprengt ein paar Wipfel weiter ihr Liederherz.", "tokens": ["Und", "sprengt", "ein", "paar", "Wip\u00b7fel", "wei\u00b7ter", "ihr", "Lie\u00b7der\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Des Dichters weihefarbne Sehnsucht", "tokens": ["Des", "Dich\u00b7ters", "wei\u00b7he\u00b7farb\u00b7ne", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Sind die Erf\u00fcllung der V\u00f6lker, das Lied der Welt.", "tokens": ["Sind", "die", "Er\u00b7f\u00fcl\u00b7lung", "der", "V\u00f6l\u00b7ker", ",", "das", "Lied", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}