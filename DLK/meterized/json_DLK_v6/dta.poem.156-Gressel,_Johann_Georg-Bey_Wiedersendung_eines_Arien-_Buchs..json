{"dta.poem.156": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Bey Wiedersendung eines  Arien- Buchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So nimm von meiner Hand dasselbe wieder an/", "tokens": ["So", "nimm", "von", "mei\u00b7ner", "Hand", "das\u00b7sel\u00b7be", "wie\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "PPOSAT", "NN", "PDAT", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich an deiner Statt in tieffster Pflicht verehret:", "tokens": ["Da\u00df", "ich", "an", "dei\u00b7ner", "Statt", "in", "tieffs\u00b7ter", "Pflicht", "ver\u00b7eh\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwar bin ich erst vergn\u00fcgt/ wenn ich dich sehen kan/", "tokens": ["Zwar", "bin", "ich", "erst", "ver\u00b7gn\u00fcgt", "/", "wenn", "ich", "dich", "se\u00b7hen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$(", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch auch da mir das Gl\u00fcck hat dein ", "tokens": ["Doch", "auch", "da", "mir", "das", "Gl\u00fcck", "hat", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "PPER", "ART", "NN", "VAFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist in diesem Buch vortrefflich ausgedr\u00fcckt;", "tokens": ["Es", "ist", "in", "die\u00b7sem", "Buch", "vor\u00b7treff\u00b7lich", "aus\u00b7ge\u00b7dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hergegen kan ich auch allhier mein eignes finden:", "tokens": ["Her\u00b7ge\u00b7gen", "kan", "ich", "auch", "all\u00b7hier", "mein", "eig\u00b7nes", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Ich/ als ein Sclave bin von deiner Hand bestrickt/", "tokens": ["Ich", "/", "als", "ein", "Scla\u00b7ve", "bin", "von", "dei\u00b7ner", "Hand", "be\u00b7strickt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KOUS", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du aber h\u00f6rst nicht auf mich mehr und mehr zu binden.", "tokens": ["Du", "a\u00b7ber", "h\u00f6rst", "nicht", "auf", "mich", "mehr", "und", "mehr", "zu", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKNEG", "APPR", "PPER", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich schrieb ein schlechtes Lied in selbiges hinein/", "tokens": ["Ich", "schrieb", "ein", "schlech\u00b7tes", "Lied", "in", "sel\u00b7bi\u00b7ges", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zum Zeugni\u00df/ da\u00df in mir so Marck als Adern brennen/", "tokens": ["Zum", "Zeug\u00b7ni\u00df", "/", "da\u00df", "in", "mir", "so", "Marck", "als", "A\u00b7dern", "bren\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "KOUS", "APPR", "PPER", "ADV", "NN", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch kan es meiner Pein kein rechter Ausdruck seyn/", "tokens": ["Doch", "kan", "es", "mei\u00b7ner", "Pein", "kein", "rech\u00b7ter", "Aus\u00b7druck", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil ich zu selben nicht die Feder finden k\u00f6nnen.", "tokens": ["Weil", "ich", "zu", "sel\u00b7ben", "nicht", "die", "Fe\u00b7der", "fin\u00b7den", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "PTKNEG", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Ungl\u00fcck kan kein Mensch/ als der allein verstehn/", "tokens": ["Mein", "Un\u00b7gl\u00fcck", "kan", "kein", "Mensch", "/", "als", "der", "al\u00b7lein", "ver\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIAT", "NN", "$(", "KOUS", "ART", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der auch ein gleiches hat in seiner Seel empfunden/", "tokens": ["Der", "auch", "ein", "glei\u00b7ches", "hat", "in", "sei\u00b7ner", "Seel", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn selber weis nicht recht in meine Pein zu sehn/", "tokens": ["Denn", "sel\u00b7ber", "weis", "nicht", "recht", "in", "mei\u00b7ne", "Pein", "zu", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dem von der Liebe nicht/ gleich mir/ das Hertz gebunden.", "tokens": ["Dem", "von", "der", "Lie\u00b7be", "nicht", "/", "gleich", "mir", "/", "das", "Hertz", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PTKNEG", "$(", "ADV", "PPER", "$(", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Sprich/ liebste Seele sprich/ da\u00df du gebunden bist/", "tokens": ["Sprich", "/", "liebs\u00b7te", "See\u00b7le", "sprich", "/", "da\u00df", "du", "ge\u00b7bun\u00b7den", "bist", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "ADJA", "NN", "ADJD", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So kanst du auch ein Bild von meiner Seelen f\u00fchlen;", "tokens": ["So", "kanst", "du", "auch", "ein", "Bild", "von", "mei\u00b7ner", "See\u00b7len", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch wo dir eben so als mir zu muhte ist", "tokens": ["Doch", "wo", "dir", "e\u00b7ben", "so", "als", "mir", "zu", "muh\u00b7te", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "KOUS", "PPER", "PTKZU", "VVFIN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So mu\u00df die Flamme traun auf was gewisses ziehlen.", "tokens": ["So", "mu\u00df", "die", "Flam\u00b7me", "traun", "auf", "was", "ge\u00b7wis\u00b7ses", "zieh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN", "APPR", "PRELS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein Leben/ la\u00df mich bald hiervon die Funcken schaun:", "tokens": ["Mein", "Le\u00b7ben", "/", "la\u00df", "mich", "bald", "hier\u00b7von", "die", "Fun\u00b7cken", "schaun", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVIMP", "PPER", "ADV", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es sind dieselben nur ein angenehmes Schreiben:", "tokens": ["Es", "sind", "die\u00b7sel\u00b7ben", "nur", "ein", "an\u00b7ge\u00b7neh\u00b7mes", "Schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Himmel wird den Oehl in deine Flammen thaun/", "tokens": ["Der", "Him\u00b7mel", "wird", "den", "O\u00b7ehl", "in", "dei\u00b7ne", "Flam\u00b7men", "thaun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und ich wil bis ins Grab dein treuer Diener bleiben.", "tokens": ["Und", "ich", "wil", "bis", "ins", "Grab", "dein", "treu\u00b7er", "Die\u00b7ner", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}