{"textgrid.poem.66157": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Frage", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum ist mir das Morgenroth", "tokens": ["Wa\u00b7rum", "ist", "mir", "das", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So blutgestreift? die Welt so todt?", "tokens": ["So", "blut\u00b7ge\u00b7streift", "?", "die", "Welt", "so", "todt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum strahlt mir das Sonnenlicht", "tokens": ["Wa\u00b7rum", "strahlt", "mir", "das", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Oft so beschwerlich ins Gesicht?", "tokens": ["Oft", "so", "be\u00b7schwer\u00b7lich", "ins", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und warum weint die Wolke mir?", "tokens": ["Und", "wa\u00b7rum", "weint", "die", "Wol\u00b7ke", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was trau'rt der Linde Bl\u00fctenzier?", "tokens": ["Was", "trau'rt", "der", "Lin\u00b7de", "Bl\u00fc\u00b7ten\u00b7zier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die L\u00fcfte wimmern: jedes Bild", "tokens": ["Die", "L\u00fcf\u00b7te", "wim\u00b7mern", ":", "je\u00b7des", "Bild"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist mir in Trauerflor geh\u00fcllt!", "tokens": ["Ist", "mir", "in", "Trau\u00b7er\u00b7flor", "ge\u00b7h\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Thau, begl\u00e4nzt vom Sonnenschein,", "tokens": ["Der", "Thau", ",", "be\u00b7gl\u00e4nzt", "vom", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "D\u00e4ucht mir, vom ", "tokens": ["D\u00e4ucht", "mir", ",", "vom"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "APPRART"], "meter": "+--", "measure": "dactylic.init"}, "line.11": {"text": "Die Wohlger\u00fcche in der Luft", "tokens": ["Die", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "in", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Umschwimmen mich, wie Gr\u00e4berduft;", "tokens": ["Um\u00b7schwim\u00b7men", "mich", ",", "wie", "Gr\u00e4\u00b7ber\u00b7duft", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die lieben Bl\u00fcmlein allzumal", "tokens": ["Die", "lie\u00b7ben", "Bl\u00fcm\u00b7lein", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sind mir versengt vom Sonnenstrahl.", "tokens": ["Sind", "mir", "ver\u00b7sengt", "vom", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Vogel aus der Luft herab", "tokens": ["Der", "Vo\u00b7gel", "aus", "der", "Luft", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "T\u00f6nt mir, wie Sterbgesang am Grab;", "tokens": ["T\u00f6nt", "mir", ",", "wie", "Sterb\u00b7ge\u00b7sang", "am", "Grab", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und alles, alles um mich her", "tokens": ["Und", "al\u00b7les", ",", "al\u00b7les", "um", "mich", "her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PIS", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Scheint kummervoll und thr\u00e4nenschwer.", "tokens": ["Scheint", "kum\u00b7mer\u00b7voll", "und", "thr\u00e4\u00b7nen\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die Farben gr\u00fcn und wei\u00df und roth,", "tokens": ["Die", "Far\u00b7ben", "gr\u00fcn", "und", "wei\u00df", "und", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sind abgestanden, schwarz und todt.", "tokens": ["Sind", "ab\u00b7ge\u00b7stan\u00b7den", ",", "schwarz", "und", "todt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Menschen, deren Trost ich such',", "tokens": ["Die", "Men\u00b7schen", ",", "de\u00b7ren", "Trost", "ich", "such'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sind Geister, die im Leichentuch", "tokens": ["Sind", "Geis\u00b7ter", ",", "die", "im", "Lei\u00b7chen\u00b7tuch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Mich ansehn bleich, und furchtbarstumm.", "tokens": ["Mich", "an\u00b7sehn", "bleich", ",", "und", "furcht\u00b7bar\u00b7stumm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Du guter Gott! warum, warum?", "tokens": ["Du", "gu\u00b7ter", "Gott", "!", "wa\u00b7rum", ",", "wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PWAV", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Hast du der ganzen Erde Pracht", "tokens": ["Hast", "du", "der", "gan\u00b7zen", "Er\u00b7de", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Zu einem Todtenschlund gemacht?", "tokens": ["Zu", "ei\u00b7nem", "Tod\u00b7ten\u00b7schlund", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ach nein! die Welt ist noch, wie vor,", "tokens": ["Ach", "nein", "!", "die", "Welt", "ist", "noch", ",", "wie", "vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "ART", "NN", "VAFIN", "ADV", "$,", "PWAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Nur dem, der, Freiheit! dich verlor,", "tokens": ["Nur", "dem", ",", "der", ",", "Frei\u00b7heit", "!", "dich", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "$,", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ist diese Welt, so sch\u00f6n gemacht,", "tokens": ["Ist", "die\u00b7se", "Welt", ",", "so", "sch\u00f6n", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ein Todtenschlund voll Fluch und Nacht;", "tokens": ["Ein", "Tod\u00b7ten\u00b7schlund", "voll", "Fluch", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Wo alles heult, den Sch\u00e4del schl\u00e4gt,", "tokens": ["Wo", "al\u00b7les", "heult", ",", "den", "Sch\u00e4\u00b7del", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Verzweiflung br\u00fcllt, und Ketten tr\u00e4gt!", "tokens": ["Ver\u00b7zwei\u00b7flung", "br\u00fcllt", ",", "und", "Ket\u00b7ten", "tr\u00e4gt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "O Gott im Himmel, mach' mich frei", "tokens": ["O", "Gott", "im", "Him\u00b7mel", ",", "mach'", "mich", "frei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Aus dieser H\u00f6llent\u00e4uscherei!", "tokens": ["Aus", "die\u00b7ser", "H\u00f6l\u00b7len\u00b7t\u00e4u\u00b7sche\u00b7rei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}