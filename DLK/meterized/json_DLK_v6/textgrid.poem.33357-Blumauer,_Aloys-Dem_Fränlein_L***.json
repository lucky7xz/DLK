{"textgrid.poem.33357": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Dem Fr\u00e4nlein L***", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiesen, Auen gr\u00fcnen wieder,", "tokens": ["Wie\u00b7sen", ",", "Au\u00b7en", "gr\u00fc\u00b7nen", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcmchen prangen auf der Flur,", "tokens": ["Bl\u00fcm\u00b7chen", "pran\u00b7gen", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es t\u00f6nen Finkenlieder,", "tokens": ["Und", "es", "t\u00f6\u00b7nen", "Fin\u00b7ken\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neu erwacht ist die Natur.", "tokens": ["Neu", "er\u00b7wacht", "ist", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf zu frohen, sch\u00f6nen Trieben", "tokens": ["Auf", "zu", "fro\u00b7hen", ",", "sch\u00f6\u00b7nen", "Trie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PTKZU", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlt bewegt sich jede Brust,", "tokens": ["F\u00fchlt", "be\u00b7wegt", "sich", "je\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zu jubeln und zu lieben", "tokens": ["Nur", "zu", "ju\u00b7beln", "und", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sey f\u00fcr uns die gr\u00f6\u00dfte Lust.", "tokens": ["Sey", "f\u00fcr", "uns", "die", "gr\u00f6\u00df\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzo nun von deinen Lippen,", "tokens": ["Jet\u00b7zo", "nun", "von", "dei\u00b7nen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich bl\u00e4hen, frisch und voll,", "tokens": ["Die", "sich", "bl\u00e4\u00b7hen", ",", "frisch", "und", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcsse rauben \u2013 nicht zu nippen,", "tokens": ["K\u00fcs\u00b7se", "rau\u00b7ben", "\u2013", "nicht", "zu", "nip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie's die Gluth gebiethen soll.", "tokens": ["Wie's", "die", "Gluth", "ge\u00b7bie\u00b7then", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hin an deine Brust zu sinken,", "tokens": ["Hin", "an", "dei\u00b7ne", "Brust", "zu", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich \u00fcber's Mieder dr\u00e4ngt,", "tokens": ["Die", "sich", "\u00fc\u00b7ber's", "Mie\u00b7der", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollust aus dem Blick zu trinken,", "tokens": ["Wol\u00b7lust", "aus", "dem", "Blick", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem liebend mein Herz h\u00e4ngt.", "tokens": ["An", "dem", "lie\u00b7bend", "mein", "Herz", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und so sollen alle Tage", "tokens": ["Und", "so", "sol\u00b7len", "al\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieblich wie der Mai vergeh'n,", "tokens": ["Lieb\u00b7lich", "wie", "der", "Mai", "ver\u00b7geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Schmerz und ohne Plage", "tokens": ["Oh\u00b7ne", "Schmerz", "und", "oh\u00b7ne", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns're Liebe stets besteh'n.", "tokens": ["Un\u00b7s'\u00b7re", "Lie\u00b7be", "stets", "be\u00b7steh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}