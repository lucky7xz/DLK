{"textgrid.poem.60362": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "4. Stimme des Kindes", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein schlafend Kind! o still! in diesen Z\u00fcgen", "tokens": ["Ein", "schla\u00b7fend", "Kind", "!", "o", "still", "!", "in", "die\u00b7sen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$.", "FM", "FM", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00f6nnt ihr das Paradies zur\u00fcckbeschw\u00f6ren;", "tokens": ["K\u00f6nnt", "ihr", "das", "Pa\u00b7ra\u00b7dies", "zu\u00b7r\u00fcck\u00b7be\u00b7schw\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es l\u00e4chelt s\u00fc\u00df, als lauscht es Engelch\u00f6ren,", "tokens": ["Es", "l\u00e4\u00b7chelt", "s\u00fc\u00df", ",", "als", "lauscht", "es", "En\u00b7gelc\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Mund ums\u00e4uselt himmlisches Vergn\u00fcgen.", "tokens": ["Den", "Mund", "um\u00b7s\u00e4u\u00b7selt", "himm\u00b7li\u00b7sches", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O schweige, Welt, mit deinen lauten L\u00fcgen,", "tokens": ["O", "schwei\u00b7ge", ",", "Welt", ",", "mit", "dei\u00b7nen", "lau\u00b7ten", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wahrheit dieses Traumes nicht zu st\u00f6ren!", "tokens": ["Die", "Wahr\u00b7heit", "die\u00b7ses", "Trau\u00b7mes", "nicht", "zu", "st\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mich das Kind im Traume sprechen h\u00f6ren", "tokens": ["La\u00df", "mich", "das", "Kind", "im", "Trau\u00b7me", "spre\u00b7chen", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich, vergessend, in die Unschuld f\u00fcgen!", "tokens": ["Und", "mich", ",", "ver\u00b7ges\u00b7send", ",", "in", "die", "Un\u00b7schuld", "f\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Kind, nicht ahnend mein bewegtes Lauschen,", "tokens": ["Das", "Kind", ",", "nicht", "ah\u00b7nend", "mein", "be\u00b7weg\u00b7tes", "Lau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit dunklen Leuten hat mein Herz gesegnet,", "tokens": ["Mit", "dunk\u00b7len", "Leu\u00b7ten", "hat", "mein", "Herz", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mehr als im stillen Wald des Baumes Rauschen;", "tokens": ["Mehr", "als", "im", "stil\u00b7len", "Wald", "des", "Bau\u00b7mes", "Rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "APPRART", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein tiefres Heimweh hat mich \u00fcberfallen,", "tokens": ["Ein", "tief\u00b7res", "Heim\u00b7weh", "hat", "mich", "\u00fc\u00b7berf\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wenn es auf die stille Heide regnet,", "tokens": ["Als", "wenn", "es", "auf", "die", "stil\u00b7le", "Hei\u00b7de", "reg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn im Gebirg die fernen Glocken hallen.", "tokens": ["Wenn", "im", "Ge\u00b7birg", "die", "fer\u00b7nen", "Glo\u00b7cken", "hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}