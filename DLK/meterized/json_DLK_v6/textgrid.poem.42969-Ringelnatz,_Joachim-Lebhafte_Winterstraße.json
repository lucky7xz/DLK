{"textgrid.poem.42969": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Lebhafte Winterstra\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gehen Menschen vor mir hin", "tokens": ["Es", "ge\u00b7hen", "Men\u00b7schen", "vor", "mir", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gehen mir vorbei, und keiner", "tokens": ["Und", "ge\u00b7hen", "mir", "vor\u00b7bei", ",", "und", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Davon ist so, wie ich es bin.", "tokens": ["Da\u00b7von", "ist", "so", ",", "wie", "ich", "es", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "$,", "PWAV", "PPER", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es blickt ein jedes so nach seiner", "tokens": ["Es", "blickt", "ein", "je\u00b7des", "so", "nach", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gegebenen Art in seine Welt.", "tokens": ["Ge\u00b7ge\u00b7be\u00b7nen", "Art", "in", "sei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer hat die Menschen so entstellt??", "tokens": ["Wer", "hat", "die", "Men\u00b7schen", "so", "ent\u00b7stellt", "??"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sehe sie getrieben treiben.", "tokens": ["Ich", "se\u00b7he", "sie", "ge\u00b7trie\u00b7ben", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warum sie wohl nie stehenbleiben,", "tokens": ["Wa\u00b7rum", "sie", "wohl", "nie", "ste\u00b7hen\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sehen, was nach ihnen sieht?", "tokens": ["Zu", "se\u00b7hen", ",", "was", "nach", "ih\u00b7nen", "sieht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warum der Mensch vorm Menschen flieht?", "tokens": ["Wa\u00b7rum", "der", "Mensch", "vorm", "Men\u00b7schen", "flieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und eine wei\u00dfe Weite Schnee", "tokens": ["Und", "ei\u00b7ne", "wei\u00b7\u00dfe", "Wei\u00b7te", "Schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verdreckt sich unter ihren F\u00fc\u00dfen.", "tokens": ["Ver\u00b7dreckt", "sich", "un\u00b7ter", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So viele Menschen. Mir ist weh:", "tokens": ["So", "vie\u00b7le", "Men\u00b7schen", ".", "Mir", "ist", "weh", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Keinen von ihnen darf ich gr\u00fc\u00dfen.", "tokens": ["Kei\u00b7nen", "von", "ih\u00b7nen", "darf", "ich", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}