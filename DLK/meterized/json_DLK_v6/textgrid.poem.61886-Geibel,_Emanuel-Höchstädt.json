{"textgrid.poem.61886": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6chst\u00e4dt", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Marlbrough zieht aus zum Kriege,", "tokens": ["Marl\u00b7brough", "zieht", "aus", "zum", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Fahnen l\u00e4\u00dft er wehn;", "tokens": ["Die", "Fah\u00b7nen", "l\u00e4\u00dft", "er", "wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da reicht zu Kampf und Siege", "tokens": ["Da", "reicht", "zu", "Kampf", "und", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Hand ihm Prinz Eugen.", "tokens": ["Die", "Hand", "ihm", "Prinz", "Eu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "NN", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie mustern ihre Truppen", "tokens": ["Sie", "mus\u00b7tern", "ih\u00b7re", "Trup\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei H\u00f6chst\u00e4dt auf dem Plan:", "tokens": ["Bei", "H\u00f6chs\u00b7t\u00e4dt", "auf", "dem", "Plan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbgut stehn im Brett die Puppen,", "tokens": ["\u00bb", "gut", "stehn", "im", "Brett", "die", "Pup\u00b7pen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frisch auf, wir greifen an!\u00ab", "tokens": ["Frisch", "auf", ",", "wir", "grei\u00b7fen", "an", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wie sie mit den Haufen", "tokens": ["Und", "wie", "sie", "mit", "den", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Feind entgegenziehn,", "tokens": ["Dem", "Feind", "ent\u00b7ge\u00b7gen\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kommt gejagt mit Schnaufen", "tokens": ["Da", "kommt", "ge\u00b7jagt", "mit", "Schnau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Hofkurier aus Wien.", "tokens": ["Ein", "Hof\u00b7ku\u00b7rier", "aus", "Wi\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er springt im bunten Staate", "tokens": ["Er", "springt", "im", "bun\u00b7ten", "Staa\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Ro\u00df und neigt sich tief:", "tokens": ["Vom", "Ro\u00df", "und", "neigt", "sich", "tief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbvom hohen Kriegshofrate,", "tokens": ["\u00bb", "vom", "ho\u00b7hen", "Kriegs\u00b7hof\u00b7ra\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Durchlauchtigster, ein Brief!\u00ab", "tokens": ["Durch\u00b7lauch\u00b7tigs\u00b7ter", ",", "ein", "Brief", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Der kleine Kapuziner", "tokens": ["Der", "klei\u00b7ne", "Ka\u00b7pu\u00b7zi\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schiebt ihn ins Wams bedacht:", "tokens": ["Schiebt", "ihn", "ins", "Wams", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbder Herrn ergebner Diener!", "tokens": ["\u00bb", "der", "Herrn", "er\u00b7geb\u00b7ner", "Die\u00b7ner", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das les' ich nach der Schlacht.", "tokens": ["Das", "les'", "ich", "nach", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Jetzt ist kein Zaudern n\u00fctze,", "tokens": ["Jetzt", "ist", "kein", "Zau\u00b7dern", "n\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jetzt hei\u00dft es: Dran und drauf!", "tokens": ["Jetzt", "hei\u00dft", "es", ":", "Dran", "und", "drauf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PAV", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon spielen die Gesch\u00fctze", "tokens": ["Schon", "spie\u00b7len", "die", "Ge\u00b7sch\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tallards zum Kampf uns auf!\u00ab", "tokens": ["Tal\u00b7lards", "zum", "Kampf", "uns", "auf", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Er wirft sich auf die Franzen,", "tokens": ["Er", "wirft", "sich", "auf", "die", "Fran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Marlbrough bleibt nicht zur\u00fcck;", "tokens": ["Marl\u00b7brough", "bleibt", "nicht", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei H\u00f6chst\u00e4dt an den Schanzen", "tokens": ["Bei", "H\u00f6chs\u00b7t\u00e4dt", "an", "den", "Schan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ward ihr Meisterst\u00fcck.", "tokens": ["Das", "ward", "ihr", "Meis\u00b7ter\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wohl kracht's von Wall und Turme,", "tokens": ["Wohl", "kracht's", "von", "Wall", "und", "Tur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl sinken Ro\u00df und Mann,", "tokens": ["Wohl", "sin\u00b7ken", "Ro\u00df", "und", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch vorw\u00e4rts geht's im Sturme,", "tokens": ["Doch", "vor\u00b7w\u00e4rts", "geht's", "im", "Stur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Feldherrn hoch voran.", "tokens": ["Die", "Feld\u00b7herrn", "hoch", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Im dichten Kugelregen,", "tokens": ["Im", "dich\u00b7ten", "Ku\u00b7gel\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Degen in der Hand,", "tokens": ["Den", "De\u00b7gen", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erklimmen sie verwegen", "tokens": ["Er\u00b7klim\u00b7men", "sie", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Lagers steilen Rand.", "tokens": ["Des", "La\u00b7gers", "stei\u00b7len", "Rand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da packt den Feind ein Grausen,", "tokens": ["Da", "packt", "den", "Feind", "ein", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da flieht er fern und nah,", "tokens": ["Da", "flieht", "er", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hinter ihm mit Brausen", "tokens": ["Und", "hin\u00b7ter", "ihm", "mit", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erschallt's: Viktoria!", "tokens": ["Er\u00b7schallt's", ":", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Und wie des Kaisers Reiter", "tokens": ["Und", "wie", "des", "Kai\u00b7sers", "Rei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nachrasseln Sto\u00df auf Sto\u00df,", "tokens": ["Nach\u00b7ras\u00b7seln", "Sto\u00df", "auf", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da frommt kein Haltruf weiter,", "tokens": ["Da", "frommt", "kein", "Halt\u00b7ruf", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geworfen ist das Los.", "tokens": ["Ge\u00b7wor\u00b7fen", "ist", "das", "Los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ersiegte Fahnen prangen", "tokens": ["Er\u00b7sieg\u00b7te", "Fah\u00b7nen", "pran\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zweihundert an der Zahl,", "tokens": ["Zwei\u00b7hun\u00b7dert", "an", "der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man bringt daher gefangen", "tokens": ["Man", "bringt", "da\u00b7her", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PAV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tallard, den General.", "tokens": ["Tal\u00b7lard", ",", "den", "Ge\u00b7ne\u00b7ral", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.13": {"line.1": {"text": "Doch abends, als die Flaschen", "tokens": ["Doch", "a\u00b7bends", ",", "als", "die", "Fla\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Kreis ums Feuer gehn,", "tokens": ["Im", "Kreis", "ums", "Feu\u00b7er", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da zieht aus seiner Taschen", "tokens": ["Da", "zieht", "aus", "sei\u00b7ner", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Brieflein Prinz Eugen,", "tokens": ["Sein", "Brie\u00b7flein", "Prinz", "Eu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.14": {"line.1": {"text": "Studiert's und reicht's dem Briten;", "tokens": ["Stu\u00b7diert's", "und", "reicht's", "dem", "Bri\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der blickt hinein und lacht:", "tokens": ["Der", "blickt", "hin\u00b7ein", "und", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbparbleu! Die Herrn verbitten", "tokens": ["\u00bb", "par\u00b7bleu", "!", "Die", "Herrn", "ver\u00b7bit\u00b7ten"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Wien sich jede Schlacht.", "tokens": ["In", "Wi\u00b7en", "sich", "je\u00b7de", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PRF", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Nur kluge Retirade", "tokens": ["Nur", "klu\u00b7ge", "Re\u00b7ti\u00b7ra\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sauvier' uns, meint der Wisch;", "tokens": ["Sau\u00b7vier'", "uns", ",", "meint", "der", "Wisch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Erlesner Senf! Nur schade,", "tokens": ["Er\u00b7les\u00b7ner", "Senf", "!", "Nur", "scha\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr diesmal Senf nach Tisch!\u00ab", "tokens": ["F\u00fcr", "dies\u00b7mal", "Senf", "nach", "Tisch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}