{"textgrid.poem.49688": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Feinen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei i.a. C.B.'s und drei C.B.'s,", "tokens": ["Zwei", "i.", "a.", "C.", "B.", "'s", "und", "drei", "C.", "B.", "'s", ","], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation", "abbreviation", "word", "word", "word", "abbreviation", "abbreviation", "word", "punct"], "pos": ["CARD", "NE", "NE", "NE", "NE", "PPER", "KON", "CARD", "NE", "NE", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die man niemals im Kollege sah,", "tokens": ["Die", "man", "nie\u00b7mals", "im", "Kol\u00b7le\u00b7ge", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Bummelten infolge Katerwehs,", "tokens": ["Bum\u00b7mel\u00b7ten", "in\u00b7fol\u00b7ge", "Ka\u00b7ter\u00b7wehs", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da begegnet' ihnen ein a.H.", "tokens": ["Da", "be\u00b7geg\u00b7net'", "ih\u00b7nen", "ein", "a.", "H."], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "ART", "XY", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seines Zeichens war er Ref'rendar.", "tokens": ["Sei\u00b7nes", "Zei\u00b7chens", "war", "er", "Re\u00b7f'\u00b7ren\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Protokolle schreibend beim Gericht,", "tokens": ["Pro\u00b7to\u00b7kol\u00b7le", "schrei\u00b7bend", "beim", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dient' dem Staate er seit einem Jahr.", "tokens": ["Dient'", "dem", "Staa\u00b7te", "er", "seit", "ei\u00b7nem", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Manche Narbe schm\u00fcckte sein Gesicht.", "tokens": ["Man\u00b7che", "Nar\u00b7be", "schm\u00fcck\u00b7te", "sein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ihm entgegen schritt jetzt der F.M.,", "tokens": ["Ihm", "ent\u00b7ge\u00b7gen", "schritt", "jetzt", "der", "F.", "M.", ","], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "abbreviation", "punct"], "pos": ["PPER", "PTKVZ", "VVFIN", "ADV", "ART", "NE", "NE", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Jeder gr\u00fc\u00dfte einfach tadellos,", "tokens": ["Je\u00b7der", "gr\u00fc\u00df\u00b7te", "ein\u00b7fach", "ta\u00b7del\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schnarrte: \u00bbMahlzeit!\u00ab und: \u00bbSehr angenehm!\u00ab", "tokens": ["Schnarr\u00b7te", ":", "\u00bb", "Mahl\u00b7zeit", "!", "\u00ab", "und", ":", "\u00bb", "Sehr", "an\u00b7ge\u00b7nehm", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "NN", "$.", "$(", "KON", "$.", "$(", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gegenseitig war die Achtung gro\u00df.", "tokens": ["Ge\u00b7gen\u00b7sei\u00b7tig", "war", "die", "Ach\u00b7tung", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Jeder hatte vor sich selbst Respekt,", "tokens": ["Je\u00b7der", "hat\u00b7te", "vor", "sich", "selbst", "Res\u00b7pekt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PRF", "ADV", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Jeder kam sich \u00e4u\u00dferst wichtig vor", "tokens": ["Je\u00b7der", "kam", "sich", "\u00e4u\u00b7\u00dferst", "wich\u00b7tig", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADJD", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und benahm sich dabei sehr korrekt,", "tokens": ["Und", "be\u00b7nahm", "sich", "da\u00b7bei", "sehr", "kor\u00b7rekt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "ADV", "ADJD", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wie es \u00fcblich ist im feinen Korps.", "tokens": ["Wie", "es", "\u00fcb\u00b7lich", "ist", "im", "fei\u00b7nen", "Korps", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Da ihm dies gefiel, sprach sehr gelehrt", "tokens": ["Da", "ihm", "dies", "ge\u00b7fiel", ",", "sprach", "sehr", "ge\u00b7lehrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,", "VVFIN", "ADV", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Auf der Kneipe abends der a.H.", "tokens": ["Auf", "der", "Knei\u00b7pe", "a\u00b7bends", "der", "a.", "H."], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "abbreviation"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "XY", "XY"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Von der Korps erzieherischem Wert.", "tokens": ["Von", "der", "Korps", "er\u00b7zie\u00b7he\u00b7ri\u00b7schem", "Wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Stinkbesoffen wurde jeder da.", "tokens": ["Stink\u00b7be\u00b7sof\u00b7fen", "wur\u00b7de", "je\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}