{"textgrid.poem.32015": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "4. Das geheime Nazareth und die geistliche Verk\u00fcndigung", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maria, Nazareth und Gabriel, der Bot,", "tokens": ["Ma\u00b7ria", ",", "Na\u00b7za\u00b7reth", "und", "Ga\u00b7bri\u00b7el", ",", "der", "Bot", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NE", "$,", "ART", "NN", "$,"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ist meine Seel, mein Herz und neues Licht von Gott.", "tokens": ["Ist", "mei\u00b7ne", "Seel", ",", "mein", "Herz", "und", "neu\u00b7es", "Licht", "von", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Herze zwar, wenn es ein Blumental geworden,", "tokens": ["Mein", "Her\u00b7ze", "zwar", ",", "wenn", "es", "ein", "Blu\u00b7men\u00b7tal", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Seele, wenn sie steht im keuschen Jungfern Orden", "tokens": ["Die", "See\u00b7le", ",", "wenn", "sie", "steht", "im", "keu\u00b7schen", "Jung\u00b7fern", "Or\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wohnt in diesem Tal; das neue Gnadenlicht,", "tokens": ["Und", "wohnt", "in", "die\u00b7sem", "Tal", ";", "das", "neu\u00b7e", "Gna\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn Gott sein ewges Wort in ihrem Geiste spricht. ", "tokens": ["Wenn", "Gott", "sein", "ew\u00b7ges", "Wort", "in", "ih\u00b7rem", "Geis\u00b7te", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}