{"textgrid.poem.40805": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdurch meines Kerkers Eisengitter rangen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdurch meines Kerkers Eisengitter rangen", "tokens": ["\u00bb", "durch", "mei\u00b7nes", "Ker\u00b7kers", "Ei\u00b7sen\u00b7git\u00b7ter", "ran\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich meine Blick' empor zum Himmel droben,", "tokens": ["Sich", "mei\u00b7ne", "Blick'", "em\u00b7por", "zum", "Him\u00b7mel", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "PTKVZ", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Ball des Mondes sah ich leuchtend prangen,", "tokens": ["Den", "Ball", "des", "Mon\u00b7des", "sah", "ich", "leuch\u00b7tend", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom goldnen Kranz der Sterne rings umwoben.", "tokens": ["Vom", "gold\u00b7nen", "Kranz", "der", "Ster\u00b7ne", "rings", "um\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da klang's aus ihnen in mein Herz und keimte", "tokens": ["Da", "klang's", "aus", "ih\u00b7nen", "in", "mein", "Herz", "und", "ke\u00b7im\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleichwie ein kindisch M\u00e4rchen alter Tage,", "tokens": ["Gleich\u00b7wie", "ein", "kin\u00b7disch", "M\u00e4r\u00b7chen", "al\u00b7ter", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bevor der G\u00f6tter Schaar die Erde r\u00e4umte", "tokens": ["Be\u00b7vor", "der", "G\u00f6t\u00b7ter", "Schaar", "die", "Er\u00b7de", "r\u00e4um\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Menschenvolke von gemein'rem Schlage.", "tokens": ["Dem", "Men\u00b7schen\u00b7vol\u00b7ke", "von", "ge\u00b7mein'\u00b7rem", "Schla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es war ein Ries' einst, hochgewaltig, t\u00fcchtig,", "tokens": ["Es", "war", "ein", "Ries'", "einst", ",", "hoch\u00b7ge\u00b7wal\u00b7tig", ",", "t\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sprach zum Mond: Dein Licht behagt mir eben,", "tokens": ["Der", "sprach", "zum", "Mond", ":", "Dein", "Licht", "be\u00b7hagt", "mir", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch bist du mir zu wanderlustig, fl\u00fcchtig", "tokens": ["Doch", "bist", "du", "mir", "zu", "wan\u00b7der\u00b7lus\u00b7tig", ",", "fl\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "PTKA", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und solltest fein an festem Wohnsitz kleben.", "tokens": ["Und", "soll\u00b7test", "fein", "an", "fes\u00b7tem", "Wohn\u00b7sitz", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht \u00fcbel st\u00fcndest du mir \u00fcber'm Bette", "tokens": ["Nicht", "\u00fc\u00b7bel", "st\u00fcn\u00b7dest", "du", "mir", "\u00fc\u00b7ber'm", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Abendlamp' in meinem Schlafgemache!", "tokens": ["Als", "A\u00b7bend\u00b7lamp'", "in", "mei\u00b7nem", "Schlaf\u00b7ge\u00b7ma\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er spricht's und schmiedet eine goldne Kette", "tokens": ["Er", "spricht's", "und", "schmie\u00b7det", "ei\u00b7ne", "gold\u00b7ne", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00e4ngt den Mond dran auf am Himmelsdache.", "tokens": ["Und", "h\u00e4ngt", "den", "Mond", "dran", "auf", "am", "Him\u00b7mels\u00b7da\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch der rollt fort und fort unaufgehalten,", "tokens": ["Doch", "der", "rollt", "fort", "und", "fort", "un\u00b7auf\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PTKVZ", "KON", "PTKVZ", "ADJA", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und klingend ri\u00df die Riesenkette droben,", "tokens": ["Und", "klin\u00b7gend", "ri\u00df", "die", "Rie\u00b7sen\u00b7ket\u00b7te", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df in Millionen Tr\u00fcmmer rasch zerspalten,", "tokens": ["Da\u00df", "in", "Mil\u00b7lion\u00b7en", "Tr\u00fcm\u00b7mer", "rasch", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weithin ges\u00e4't, die goldnen Splitter stoben!", "tokens": ["Weit\u00b7hin", "ge\u00b7s\u00e4't", ",", "die", "gold\u00b7nen", "Split\u00b7ter", "sto\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und sieh, als Sterne sind sie dort geblieben,", "tokens": ["Und", "sieh", ",", "als", "Ster\u00b7ne", "sind", "sie", "dort", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da leuchten sie ins Herz mir ihre Kunde,", "tokens": ["Da", "leuch\u00b7ten", "sie", "ins", "Herz", "mir", "ih\u00b7re", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Freiheitshymn', in goldner Schrift geschrieben", "tokens": ["Als", "Frei\u00b7heits\u00b7hymn'", ",", "in", "gold\u00b7ner", "Schrift", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tief auf des Himmels dunklem, ew'gen Grunde.", "tokens": ["Tief", "auf", "des", "Him\u00b7mels", "dunk\u00b7lem", ",", "ew'\u00b7gen", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Es fl\u00fcchtet gern mit seinen stillen Sch\u00e4tzen", "tokens": ["Es", "fl\u00fcch\u00b7tet", "gern", "mit", "sei\u00b7nen", "stil\u00b7len", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Menschenherz in die gestirnte Ferne;", "tokens": ["Das", "Men\u00b7schen\u00b7herz", "in", "die", "ge\u00b7stirn\u00b7te", "Fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es will der Mann in Fesseln gern versetzen", "tokens": ["Es", "will", "der", "Mann", "in", "Fes\u00b7seln", "gern", "ver\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Selbst seine Ketten in die ew'gen Sterne.\u00ab", "tokens": ["Selbst", "sei\u00b7ne", "Ket\u00b7ten", "in", "die", "ew'\u00b7gen", "Ster\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}}}}