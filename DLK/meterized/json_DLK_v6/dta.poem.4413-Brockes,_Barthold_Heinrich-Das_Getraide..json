{"dta.poem.4413": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Getraide.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach GOTT, es giebt dein Gnaden-Wille", "tokens": ["Ach", "GoTT", ",", "es", "giebt", "dein", "Gna\u00b7den\u00b7Wil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem menschlichen Geschlecht\u2019 in solcher F\u00fclle", "tokens": ["Dem", "menschli\u00b7chen", "Ge\u00b7schlecht'", "in", "sol\u00b7cher", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Korn, Gersten, Habern, Rocken, Weizen.", "tokens": ["Korn", ",", "Gers\u00b7ten", ",", "Ha\u00b7bern", ",", "Ro\u00b7cken", ",", "Wei\u00b7zen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So viel, so mancherley Getreide", "tokens": ["So", "viel", ",", "so", "man\u00b7cher\u00b7ley", "Ge\u00b7trei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ern\u00e4hrt uns blo\u00df durch Dich allein.", "tokens": ["Er\u00b7n\u00e4hrt", "uns", "blo\u00df", "durch", "Dich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach la\u00df, o GOTT, daf\u00fcr, aus Gnaden, uns\u2019re Freude", "tokens": ["Ach", "la\u00df", ",", "o", "GoTT", ",", "da\u00b7f\u00fcr", ",", "aus", "Gna\u00b7den", ",", "un\u00b7s'\u00b7re", "Freu\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "VVFIN", "$,", "FM", "NE", "$,", "PAV", "$,", "APPR", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Dir ein gef\u00e4llig Opfer seyn!", "tokens": ["Dir", "ein", "ge\u00b7f\u00e4l\u00b7lig", "Op\u00b7fer", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}