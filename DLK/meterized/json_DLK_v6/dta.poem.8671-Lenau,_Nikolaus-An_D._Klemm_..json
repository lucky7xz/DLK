{"dta.poem.8671": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An D. Klemm .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "O s\u00e4ume nicht, mit Wein, Gesang und Kosen", "tokens": ["O", "s\u00e4u\u00b7me", "nicht", ",", "mit", "Wein", ",", "Ge\u00b7sang", "und", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Herz zu frischen! sieh, die Jugend flicht", "tokens": ["Dein", "Herz", "zu", "fri\u00b7schen", "!", "sieh", ",", "die", "Ju\u00b7gend", "flicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$.", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In deinen Strau\u00df schon ihre lezten Rosen,", "tokens": ["In", "dei\u00b7nen", "Strau\u00df", "schon", "ih\u00b7re", "lez\u00b7ten", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald wendet sie das holde Angesicht,", "tokens": ["Bald", "wen\u00b7det", "sie", "das", "hol\u00b7de", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und flieht und schwindet tief und tiefer immer", "tokens": ["Und", "flieht", "und", "schwin\u00b7det", "tief", "und", "tie\u00b7fer", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Im Hain Vergangenheit \u2014 und kehret nimmer.", "tokens": ["Im", "Hain", "Ver\u00b7gan\u00b7gen\u00b7heit", "und", "keh\u00b7ret", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$(", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann gilt's, empor zur Lebensh\u00f6h' zu dringen,", "tokens": ["Dann", "gilt's", ",", "em\u00b7por", "zur", "Le\u00b7bens\u00b7h\u00f6h'", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PTKVZ", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann h\u00f6rst du hinter dir im Bl\u00fcthenthal", "tokens": ["Dann", "h\u00f6rst", "du", "hin\u00b7ter", "dir", "im", "Bl\u00fc\u00b7thent\u00b7hal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das: \u201e", "tokens": ["Das", ":", "\u201e"], "token_info": ["word", "punct", "punct"], "pos": ["PDS", "$.", "$("], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und deine Bahn wird gl\u00fchend, schroff und kahl:", "tokens": ["Und", "dei\u00b7ne", "Bahn", "wird", "gl\u00fc\u00b7hend", ",", "schroff", "und", "kahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Am Strau\u00dfe, den die Jugend dir gewunden,", "tokens": ["Am", "Strau\u00b7\u00dfe", ",", "den", "die", "Ju\u00b7gend", "dir", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist bald so Duft wie Farbenpracht verschwunden.", "tokens": ["Ist", "bald", "so", "Duft", "wie", "Far\u00b7ben\u00b7pracht", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wallst du einst zur Abendherberg nieder,", "tokens": ["Und", "wallst", "du", "einst", "zur", "A\u00b7bend\u00b7her\u00b7berg", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tr\u00e4nkt k\u00fchler Thau den welken Blumenstrau\u00df,", "tokens": ["Tr\u00e4nkt", "k\u00fch\u00b7ler", "Thau", "den", "wel\u00b7ken", "Blu\u00b7men\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann bl\u00fcht er neu mit Duft und Farbe wieder;", "tokens": ["Dann", "bl\u00fcht", "er", "neu", "mit", "Duft", "und", "Far\u00b7be", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du setzest m\u00fcde dich vor's stille Haus,", "tokens": ["Du", "set\u00b7zest", "m\u00fc\u00b7de", "dich", "vor's", "stil\u00b7le", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Spielst mit dem Strau\u00df, dem Kinde sch\u00f6ner Zeiten,", "tokens": ["Spielst", "mit", "dem", "Strau\u00df", ",", "dem", "Kin\u00b7de", "sch\u00f6\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NE", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schlummerst ein, \u2014 die Blumen dir entgleiten.", "tokens": ["Und", "schlum\u00b7merst", "ein", ",", "die", "Blu\u00b7men", "dir", "ent\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "$(", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}