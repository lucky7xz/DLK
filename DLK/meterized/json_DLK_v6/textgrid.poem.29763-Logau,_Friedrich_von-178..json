{"textgrid.poem.29763": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "178.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieben Herren, wie so sehr habet ihr das Eitel lieb!", "tokens": ["Lie\u00b7ben", "Her\u00b7ren", ",", "wie", "so", "sehr", "ha\u00b7bet", "ihr", "das", "Ei\u00b7tel", "lieb", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+---+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und wie gerne haben auch L\u00fcgen bey euch ihren Trieb!", "tokens": ["Und", "wie", "ger\u00b7ne", "ha\u00b7ben", "auch", "L\u00fc\u00b7gen", "bey", "euch", "ih\u00b7ren", "Trieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VAFIN", "ADV", "NN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Lieben Herren, mercket drauff! lieben Herren, dencket nach!", "tokens": ["Lie\u00b7ben", "Her\u00b7ren", ",", "mer\u00b7cket", "drauff", "!", "lie\u00b7ben", "Her\u00b7ren", ",", "den\u00b7cket", "nach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$.", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Eitelkeit gebiert nur Reu; L\u00fcgen bringen Ungemach.", "tokens": ["Ei\u00b7tel\u00b7keit", "ge\u00b7biert", "nur", "Reu", ";", "L\u00fc\u00b7gen", "brin\u00b7gen", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NE", "$.", "NN", "VVINF", "NN", "$."], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}}}}}