{"textgrid.poem.38826": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[warum, ihr Menschen, so spricht sie in Klagen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum, ihr Menschen, so spricht sie in Klagen,", "tokens": ["Wa\u00b7rum", ",", "ihr", "Men\u00b7schen", ",", "so", "spricht", "sie", "in", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df ihr so gern dem Himmel euch entziehet?", "tokens": ["Da\u00df", "ihr", "so", "gern", "dem", "Him\u00b7mel", "euch", "ent\u00b7zie\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Euch ruft so Furcht, als Lieb' und Lust: entfliehet!", "tokens": ["Euch", "ruft", "so", "Furcht", ",", "als", "Lieb'", "und", "Lust", ":", "ent\u00b7flie\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "KOUS", "NN", "KON", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die T\u00f6ne macht ihr wild, bis sie verzagen.", "tokens": ["Die", "T\u00f6\u00b7ne", "macht", "ihr", "wild", ",", "bis", "sie", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie k\u00f6nnt ihr Erz und armes Holz so plagen", "tokens": ["Wie", "k\u00f6nnt", "ihr", "Erz", "und", "ar\u00b7mes", "Holz", "so", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch selber qu\u00e4lend? Da\u00df kein Herz ergl\u00fchet,", "tokens": ["Euch", "sel\u00b7ber", "qu\u00e4\u00b7lend", "?", "Da\u00df", "kein", "Herz", "er\u00b7gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$.", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im liebenden Gesang zum Himmel bl\u00fchet,", "tokens": ["Im", "lie\u00b7ben\u00b7den", "Ge\u00b7sang", "zum", "Him\u00b7mel", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus tiefen N\u00e4chten zu den heitern Tagen?", "tokens": ["Aus", "tie\u00b7fen", "N\u00e4ch\u00b7ten", "zu", "den", "hei\u00b7tern", "Ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verschm\u00e4ht Metall, verachtet Holz, versch\u00f6nen", "tokens": ["Ver\u00b7schm\u00e4ht", "Me\u00b7tall", ",", "ver\u00b7ach\u00b7tet", "Holz", ",", "ver\u00b7sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "NN", "$,", "VVFIN", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will ich den Stand, euch Mund und Zunge leihen,", "tokens": ["Will", "ich", "den", "Stand", ",", "euch", "Mund", "und", "Zun\u00b7ge", "lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erretten euch von S\u00fcnd' und wildem Toben,", "tokens": ["Er\u00b7ret\u00b7ten", "euch", "von", "S\u00fcnd'", "und", "wil\u00b7dem", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr sollt auch Gott, der euch erschaffen, loben,", "tokens": ["Ihr", "sollt", "auch", "Gott", ",", "der", "euch", "er\u00b7schaf\u00b7fen", ",", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Kirchendienst soll meine Orgel weihen,", "tokens": ["Den", "Kir\u00b7chen\u00b7dienst", "soll", "mei\u00b7ne", "Or\u00b7gel", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Glauben st\u00e4rken mit allm\u00e4cht'gen T\u00f6nen.", "tokens": ["Den", "Glau\u00b7ben", "st\u00e4r\u00b7ken", "mit", "all\u00b7m\u00e4cht'\u00b7gen", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}