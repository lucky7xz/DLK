{"textgrid.poem.63984": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "2. Garten", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh mal, Hold, da unser Garten", "tokens": ["Sieh", "mal", ",", "Hold", ",", "da", "un\u00b7ser", "Gar\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "NE", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann Liebseelchen nicht erwarten.", "tokens": ["Kann", "Lieb\u00b7seel\u00b7chen", "nicht", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kuck', die wilden Bl\u00fcten fliegen", "tokens": ["Kuck'", ",", "die", "wil\u00b7den", "Bl\u00fc\u00b7ten", "flie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir ans Knie,", "tokens": ["Dir", "ans", "Knie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ans fein behende,", "tokens": ["Ans", "fein", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Nehmen l\u00e4chelnd,", "tokens": ["Neh\u00b7men", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Leuchtend wie die Wolke oben,", "tokens": ["Leuch\u00b7tend", "wie", "die", "Wol\u00b7ke", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich bei H\u00e4nden.", "tokens": ["Dich", "bei", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Wollen dir im Haare liegen,", "tokens": ["Wol\u00b7len", "dir", "im", "Haa\u00b7re", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Tief ins gold'ne Nest.", "tokens": ["Tief", "ins", "gold'\u00b7ne", "Nest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Hasche sie!", "tokens": ["Ha\u00b7sche", "sie", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Halt' sie fest!", "tokens": ["Halt'", "sie", "fest", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}