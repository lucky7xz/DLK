{"textgrid.poem.35746": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5. Der Flausrock", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Regensturm mit Schnee und Schlo\u00dfen", "tokens": ["Ein", "Re\u00b7gen\u00b7sturm", "mit", "Schnee", "und", "Schlo\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zog d\u00fcster \u00fcber Land und Meer,", "tokens": ["Zog", "d\u00fcs\u00b7ter", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df traufengleich die D\u00e4cher gossen;", "tokens": ["Da\u00df", "trau\u00b7fen\u00b7gleich", "die", "D\u00e4\u00b7cher", "gos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die K\u00fch' im Felde br\u00fcllten sehr.", "tokens": ["Die", "K\u00fch'", "im", "Fel\u00b7de", "br\u00fcll\u00b7ten", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Frau K\u00e4the, die zwar niemals zanket,", "tokens": ["Frau", "K\u00e4\u00b7the", ",", "die", "zwar", "nie\u00b7mals", "zan\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sprach hastig: Geh doch, lieber Mann,", "tokens": ["Sprach", "has\u00b7tig", ":", "Geh", "doch", ",", "lie\u00b7ber", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "NE", "ADV", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geh hin, eh' Bl\u00e4\u00dfchen uns erkranket,", "tokens": ["Geh", "hin", ",", "eh'", "Bl\u00e4\u00df\u00b7chen", "uns", "er\u00b7kran\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und zieh' den alten Flausrock an.", "tokens": ["Und", "zieh'", "den", "al\u00b7ten", "Flaus\u00b7rock", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die beste Kuh ist unser Bl\u00e4\u00dfchen;", "tokens": ["Die", "bes\u00b7te", "Kuh", "ist", "un\u00b7ser", "Bl\u00e4\u00df\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6re, wie sie kl\u00e4glich br\u00fcllt!", "tokens": ["Und", "h\u00f6\u00b7re", ",", "wie", "sie", "kl\u00e4g\u00b7lich", "br\u00fcllt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hat uns schon manch liebes F\u00e4\u00dfchen", "tokens": ["Sie", "hat", "uns", "schon", "manch", "lie\u00b7bes", "F\u00e4\u00df\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Milch und Butter angef\u00fcllt.", "tokens": ["Mit", "Milch", "und", "But\u00b7ter", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entsetzlich tobt des Sturms Gesause!", "tokens": ["Ent\u00b7setz\u00b7lich", "tobt", "des", "Sturms", "Ge\u00b7sau\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geh hin, mein lieber guter Mann,", "tokens": ["Geh", "hin", ",", "mein", "lie\u00b7ber", "gu\u00b7ter", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und hole Bl\u00e4\u00dfchen mir zu Hause,", "tokens": ["Und", "ho\u00b7le", "Bl\u00e4\u00df\u00b7chen", "mir", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und zieh' den alten Flausrock an. \u2013", "tokens": ["Und", "zieh'", "den", "al\u00b7ten", "Flaus\u00b7rock", "an", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbmein Flausrock dient' in Sturm und Regen,", "tokens": ["\u00bb", "mein", "Flaus\u00b7rock", "dient'", "in", "Sturm", "und", "Re\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So lang' er neu und wollig war.", "tokens": ["So", "lang'", "er", "neu", "und", "wol\u00b7lig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch jetzo h\u00e4lt er schwerlich gegen;", "tokens": ["Doch", "jet\u00b7zo", "h\u00e4lt", "er", "schwer\u00b7lich", "ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich trag' ihn schon an drei\u00dfig Jahr.", "tokens": ["Ich", "trag'", "ihn", "schon", "an", "drei\u00b7\u00dfig", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Frau, la\u00df uns nicht so n\u00e4hrig geizen.", "tokens": ["Frau", ",", "la\u00df", "uns", "nicht", "so", "n\u00e4h\u00b7rig", "gei\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer wei\u00df, wie bald man sterben kann!", "tokens": ["Wer", "wei\u00df", ",", "wie", "bald", "man", "ster\u00b7ben", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bedenk, f\u00fcr Eine Tonne Weizen", "tokens": ["Be\u00b7denk", ",", "f\u00fcr", "Ei\u00b7ne", "Ton\u00b7ne", "Wei\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schafft sich ein neuer Flausrock an.\u00ab \u2013", "tokens": ["Schafft", "sich", "ein", "neu\u00b7er", "Flaus\u00b7rock", "an", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "++-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fcr so viel Weizen trug zur Feier", "tokens": ["F\u00fcr", "so", "viel", "Wei\u00b7zen", "trug", "zur", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herzog Ulrich seinen Rock,", "tokens": ["Der", "Her\u00b7zog", "Ul\u00b7rich", "sei\u00b7nen", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und murrte doch, er sei zu teuer,", "tokens": ["Und", "murr\u00b7te", "doch", ",", "er", "sei", "zu", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schalt den Schneider einen Bock.", "tokens": ["Und", "schalt", "den", "Schnei\u00b7der", "ei\u00b7nen", "Bock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der fromme Herr war F\u00fcrst im Lande,", "tokens": ["Der", "from\u00b7me", "Herr", "war", "F\u00fcrst", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und du bist ein gemeiner Mann.", "tokens": ["Und", "du", "bist", "ein", "ge\u00b7mei\u00b7ner", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Hochmut f\u00fchrt in S\u00fcnd und Schande!", "tokens": ["Der", "Hoch\u00b7mut", "f\u00fchrt", "in", "S\u00fcnd", "und", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Drum zieh' den alten Flausrock an. \u2013", "tokens": ["Drum", "zieh'", "den", "al\u00b7ten", "Flaus\u00b7rock", "an", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbnicht prunken will ich, liebes K\u00e4thchen,", "tokens": ["\u00bb", "nicht", "prun\u00b7ken", "will", "ich", ",", "lie\u00b7bes", "K\u00e4th\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "VVINF", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur warm durch Sturm und Regen gehn.", "tokens": ["Nur", "warm", "durch", "Sturm", "und", "Re\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon z\u00e4hlen l\u00e4\u00dft sich jedes Dr\u00e4htchen,", "tokens": ["Schon", "z\u00e4h\u00b7len", "l\u00e4\u00dft", "sich", "je\u00b7des", "Dr\u00e4ht\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja F\u00e4serchen und Fetzen wehn.", "tokens": ["Ja", "F\u00e4\u00b7ser\u00b7chen", "und", "Fet\u00b7zen", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Sieh' Roberts, Wilms und Bartels Kleider;", "tokens": ["Sieh'", "Ro\u00b7berts", ",", "Wilms", "und", "Bar\u00b7tels", "Klei\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wann gehen die so lumpicht, wann?", "tokens": ["Wann", "ge\u00b7hen", "die", "so", "lum\u00b7picht", ",", "wann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADV", "VVFIN", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch Werkeltag und Sonntag leider", "tokens": ["Doch", "Wer\u00b7kel\u00b7tag", "und", "Sonn\u00b7tag", "lei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zieh' ich den alten Flausrock an!\u00ab \u2013", "tokens": ["Zieh'", "ich", "den", "al\u00b7ten", "Flaus\u00b7rock", "an", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Flausrock, d\u00e4ucht mir, ist noch billig;", "tokens": ["Der", "Flaus\u00b7rock", ",", "d\u00e4ucht", "mir", ",", "ist", "noch", "bil\u00b7lig", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab' ihn gestern erst geflickt.", "tokens": ["Ich", "hab'", "ihn", "ge\u00b7stern", "erst", "ge\u00b7flickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Du wei\u00dft, wie sorgsam ich und willig", "tokens": ["Du", "wei\u00dft", ",", "wie", "sorg\u00b7sam", "ich", "und", "wil\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich stets gepfleget und geschm\u00fcckt.", "tokens": ["Dich", "stets", "ge\u00b7pfle\u00b7get", "und", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du findest hier ein warmes St\u00fcbchen,", "tokens": ["Du", "fin\u00b7dest", "hier", "ein", "war\u00b7mes", "St\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und eine warme Suppe dann.", "tokens": ["Und", "ei\u00b7ne", "war\u00b7me", "Sup\u00b7pe", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So geh' denn hin, mein wackres B\u00fcbchen,", "tokens": ["So", "geh'", "denn", "hin", ",", "mein", "wack\u00b7res", "B\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und zieh' den alten Flausrock an. \u2013", "tokens": ["Und", "zieh'", "den", "al\u00b7ten", "Flaus\u00b7rock", "an", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbein jedes Land hat seine Weise,", "tokens": ["\u00bb", "ein", "je\u00b7des", "Land", "hat", "sei\u00b7ne", "Wei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seine H\u00fcls' ein jedes Korn.", "tokens": ["Und", "sei\u00b7ne", "H\u00fcls'", "ein", "je\u00b7des", "Korn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wirtschaft, Frau, k\u00f6mmt aus dem Gleise,", "tokens": ["Die", "Wirt\u00b7schaft", ",", "Frau", ",", "k\u00f6mmt", "aus", "dem", "Glei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-