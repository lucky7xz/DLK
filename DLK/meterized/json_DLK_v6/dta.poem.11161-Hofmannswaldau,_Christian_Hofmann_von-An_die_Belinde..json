{"dta.poem.11161": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An die Belinde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wjrff, falsche! deinen blick, wohin es dir beliebet,", "tokens": ["Wjrff", ",", "fal\u00b7sche", "!", "dei\u00b7nen", "blick", ",", "wo\u00b7hin", "es", "dir", "be\u00b7lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$.", "PPOSAT", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er nur lauter rauch und keine flammen giebet.", "tokens": ["Weil", "er", "nur", "lau\u00b7ter", "rauch", "und", "kei\u00b7ne", "flam\u00b7men", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "KON", "PIAT", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein andrer wickle sich in deinen netzen ein.", "tokens": ["Ein", "an\u00b7drer", "wick\u00b7le", "sich", "in", "dei\u00b7nen", "net\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es reimt sich nicht bey mir: Mein und gemeine seyn.", "tokens": ["Es", "reimt", "sich", "nicht", "bey", "mir", ":", "Mein", "und", "ge\u00b7mei\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR