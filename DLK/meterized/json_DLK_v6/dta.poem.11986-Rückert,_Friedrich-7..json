{"dta.poem.11986": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Silberbecher nahm der Dieb aus einer Zelle,", "tokens": ["Den", "Sil\u00b7ber\u00b7be\u00b7cher", "nahm", "der", "Dieb", "aus", "ei\u00b7ner", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch einen goldenen stellt' er an dessen Stelle.", "tokens": ["Doch", "ei\u00b7nen", "gol\u00b7de\u00b7nen", "stellt'", "er", "an", "des\u00b7sen", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PPER", "APPR", "PRELAT", "NN", "$."], "meter": "-+-+--+----+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So kehrt das Schicksal ein und raubet dir ein Gl\u00fcck,", "tokens": ["So", "kehrt", "das", "Schick\u00b7sal", "ein", "und", "rau\u00b7bet", "dir", "ein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4\u00dft ein gr\u00f6\u00dferes, Ergebung, dir zur\u00fcck.", "tokens": ["Und", "l\u00e4\u00dft", "ein", "gr\u00f6\u00b7\u00dfe\u00b7res", ",", "Er\u00b7ge\u00b7bung", ",", "dir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "$,", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Silberbecher hat ins Auge Lust gefunkelt,", "tokens": ["Der", "Sil\u00b7ber\u00b7be\u00b7cher", "hat", "ins", "Au\u00b7ge", "Lust", "ge\u00b7fun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom goldnen aber wird der Sonne Glanz verdunkelt.", "tokens": ["Vom", "gold\u00b7nen", "a\u00b7ber", "wird", "der", "Son\u00b7ne", "Glanz", "ver\u00b7dun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}