{"textgrid.poem.39894": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00f6hler", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bl\u00fcht ein schlesisch Haus", "tokens": ["Es", "bl\u00fcht", "ein", "schle\u00b7sisch", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In F\u00f6hren k\u00fchl und still,", "tokens": ["In", "F\u00f6h\u00b7ren", "k\u00fchl", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das warnt der Tod mit Graus,", "tokens": ["Das", "warnt", "der", "Tod", "mit", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn er drin w\u00fcrgen will.", "tokens": ["Wenn", "er", "drin", "w\u00fcr\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er schleicht durch Schlo\u00df und Wald", "tokens": ["Er", "schleicht", "durch", "Schlo\u00df", "und", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als ru\u00df'ger K\u00f6hlerknecht.", "tokens": ["Als", "ru\u00df'\u00b7ger", "K\u00f6h\u00b7ler\u00b7knecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da zittert Jung und Alt", "tokens": ["Da", "zit\u00b7tert", "Jung", "und", "Alt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im adligen Geschlecht.", "tokens": ["Im", "ad\u00b7li\u00b7gen", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Froh bleibt nur Ein's vom Stamm,", "tokens": ["Froh", "bleibt", "nur", "Ein's", "vom", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Schwarzen sieht es nicht;", "tokens": ["Den", "Schwar\u00b7zen", "sieht", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist das Opferlamm,", "tokens": ["Das", "ist", "das", "Op\u00b7fer\u00b7lamm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das durch das Herz er sticht. \u2013", "tokens": ["Das", "durch", "das", "Herz", "er", "sticht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Schlo\u00df t\u00f6nt Sang und Klang;", "tokens": ["Im", "Schlo\u00df", "t\u00f6nt", "Sang", "und", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch, der die Saiten greift,", "tokens": ["Doch", ",", "der", "die", "Sai\u00b7ten", "greift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Harfner stocket bang:", "tokens": ["Der", "Harf\u00b7ner", "sto\u00b7cket", "bang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vorbei der K\u00f6hler streift.", "tokens": ["Vor\u00b7bei", "der", "K\u00f6h\u00b7ler", "streift", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vom Tanz l\u00e4\u00dft Paar um Paar,", "tokens": ["Vom", "Tanz", "l\u00e4\u00dft", "Paar", "um", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Br\u00e4utigam wird bleich,", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "wird", "bleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zerstoben ist die Schaar.", "tokens": ["Zer\u00b7sto\u00b7ben", "ist", "die", "Schaar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Braut gl\u00fcht Rosen gleich.", "tokens": ["Die", "Braut", "gl\u00fcht", "Ro\u00b7sen", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr ungesehen schwebt", "tokens": ["Ihr", "un\u00b7ge\u00b7se\u00b7hen", "schwebt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der schwarze Geist fernab;", "tokens": ["Der", "schwar\u00b7ze", "Geist", "fern\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcbermorgen gr\u00e4bt", "tokens": ["Und", "\u00fc\u00b7ber\u00b7mor\u00b7gen", "gr\u00e4bt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Man an des M\u00e4gdleins Grab. \u2013", "tokens": ["Man", "an", "des", "M\u00e4gd\u00b7leins", "Grab", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Im Schlo\u00df ist Feiermahl,", "tokens": ["Im", "Schlo\u00df", "ist", "Fei\u00b7er\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es hebt der feste Greis", "tokens": ["Es", "hebt", "der", "fes\u00b7te", "Greis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Recht mutig den Pokal", "tokens": ["Recht", "mu\u00b7tig", "den", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und f\u00fchlt sich jugendhei\u00df.", "tokens": ["Und", "f\u00fchlt", "sich", "ju\u00b7gend\u00b7hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbei, sitzet nicht so stumm!", "tokens": ["\u00bb", "ei", ",", "sit\u00b7zet", "nicht", "so", "stumm", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wollt ihr nicht fr\u00f6hlich sein?", "tokens": ["Wollt", "ihr", "nicht", "fr\u00f6h\u00b7lich", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was sehet ihr euch um?", "tokens": ["Was", "se\u00b7het", "ihr", "euch", "um", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir zechen ganz allein!\u00ab", "tokens": ["Wir", "ze\u00b7chen", "ganz", "al\u00b7lein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch an der Pforte stand", "tokens": ["Doch", "an", "der", "Pfor\u00b7te", "stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der ungeladne Gast;", "tokens": ["Der", "un\u00b7ge\u00b7lad\u00b7ne", "Gast", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Mitternacht schon fand", "tokens": ["Und", "Mit\u00b7ter\u00b7nacht", "schon", "fand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Greis im Stuhl erbla\u00dft. \u2013", "tokens": ["Den", "Greis", "im", "Stuhl", "er\u00b7bla\u00dft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Im Schlo\u00df rauscht Kinderlust,", "tokens": ["Im", "Schlo\u00df", "rauscht", "Kin\u00b7der\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die holden Enkel dr\u00fcckt", "tokens": ["Die", "hol\u00b7den", "En\u00b7kel", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Wittib an die Brust,", "tokens": ["Ein", "Wit\u00b7tib", "an", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und d\u00fcnkt sich jungbegl\u00fcckt.", "tokens": ["Und", "d\u00fcnkt", "sich", "jung\u00b7be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie h\u00fcllt der j\u00fcngste Knab'", "tokens": ["Sie", "h\u00fcllt", "der", "j\u00fcngs\u00b7te", "Knab'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In blonde Locken ein:", "tokens": ["In", "blon\u00b7de", "Lo\u00b7cken", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwas wendest du dich ab", "tokens": ["\u00bb", "was", "wen\u00b7dest", "du", "dich", "ab"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PRF", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So bla\u00df, lieb M\u00fctterlein?\u00ab", "tokens": ["So", "bla\u00df", ",", "lieb", "M\u00fct\u00b7ter\u00b7lein", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sein bl\u00fchend Angesicht", "tokens": ["Sein", "bl\u00fc\u00b7hend", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dr\u00fcckt er in ihren Schoos;", "tokens": ["Dr\u00fcckt", "er", "in", "ih\u00b7ren", "Schoos", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sah den K\u00f6hler nicht,", "tokens": ["Er", "sah", "den", "K\u00f6h\u00b7ler", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der in den Arm ihn schlo\u00df.", "tokens": ["Der", "in", "den", "Arm", "ihn", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "So geht's von Jahr zu Jahr,", "tokens": ["So", "geht's", "von", "Jahr", "zu", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Noch lebt's und webt's im Saal;", "tokens": ["Noch", "lebt's", "und", "webt's", "im", "Saal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Schatten unsichtbar", "tokens": ["Der", "Schat\u00b7ten", "un\u00b7sicht\u00b7bar"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Naht Jedem doch einmal.", "tokens": ["Naht", "Je\u00b7dem", "doch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und endlich wird es leer,", "tokens": ["Und", "end\u00b7lich", "wird", "es", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und alles ward sein Raub.", "tokens": ["Und", "al\u00b7les", "ward", "sein", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch F\u00f6hren wandelt er", "tokens": ["Durch", "F\u00f6h\u00b7ren", "wan\u00b7delt", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hoch \u00fcber Kohl' und Staub.", "tokens": ["Hoch", "\u00fc\u00b7ber", "Kohl'", "und", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}