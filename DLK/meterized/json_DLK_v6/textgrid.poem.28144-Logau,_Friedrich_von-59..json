{"textgrid.poem.28144": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hansen dienet keine Magd", "tokens": ["Han\u00b7sen", "die\u00b7net", "kei\u00b7ne", "Magd"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ausser seiner alten Greten,", "tokens": ["Aus\u00b7ser", "sei\u00b7ner", "al\u00b7ten", "Gre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil es keine mit ihm wagt,", "tokens": ["Weil", "es", "kei\u00b7ne", "mit", "ihm", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sich scheut f\u00fcr Kindes-N\u00f6then.", "tokens": ["Die", "sich", "scheut", "f\u00fcr", "Kin\u00b7des\u00b7N\u00f6\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}