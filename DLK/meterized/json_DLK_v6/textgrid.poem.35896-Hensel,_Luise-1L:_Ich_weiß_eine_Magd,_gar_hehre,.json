{"textgrid.poem.35896": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df eine Magd, gar hehre,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df eine Magd, gar hehre,", "tokens": ["Ich", "wei\u00df", "ei\u00b7ne", "Magd", ",", "gar", "heh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die tr\u00e4gt den h\u00f6chsten Preis.", "tokens": ["Die", "tr\u00e4gt", "den", "h\u00f6chs\u00b7ten", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer ringt zu ihrer Ehre,", "tokens": ["Wer", "ringt", "zu", "ih\u00b7rer", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ist an Tugenden weis'.", "tokens": ["Der", "ist", "an", "Tu\u00b7gen\u00b7den", "weis'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Gen ihr sind andre Frauen", "tokens": ["Gen", "ihr", "sind", "and\u00b7re", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur D\u00f6rnlein auf der Auen", "tokens": ["Nur", "D\u00f6rn\u00b7lein", "auf", "der", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bei einem Lilienreis.", "tokens": ["Bei", "ei\u00b7nem", "Li\u00b7li\u00b7en\u00b7reis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ihr rein weiblich Gebilde,", "tokens": ["Ihr", "rein", "weib\u00b7lich", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihr' Keuschheit ist so gro\u00df,", "tokens": ["Ih\u00b7r'", "Keuschheit", "ist", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sich ein Einhorn wilde", "tokens": ["Da\u00df", "sich", "ein", "Ein\u00b7horn", "wil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gab hin in ihren Schoo\u00df,", "tokens": ["Gab", "hin", "in", "ih\u00b7ren", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das war so stark an Kraft,", "tokens": ["Das", "war", "so", "stark", "an", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df seine Meisterschaft", "tokens": ["Da\u00df", "sei\u00b7ne", "Meis\u00b7ter\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Himmel nicht beschlo\u00df.", "tokens": ["Der", "Him\u00b7mel", "nicht", "be\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von Sternenglanz eine Krone,", "tokens": ["Von", "Ster\u00b7nen\u00b7glanz", "ei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die tr\u00e4gt sie wohlgethan;", "tokens": ["Die", "tr\u00e4gt", "sie", "wohl\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sah sie in ihrem Throne", "tokens": ["Es", "sah", "sie", "in", "ih\u00b7rem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der F\u00fcrst Octavian", "tokens": ["Der", "F\u00fcrst", "Oc\u00b7ta\u00b7vi\u00b7an"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In himmelsch\u00f6ner Wonne,", "tokens": ["In", "him\u00b7mel\u00b7sch\u00f6\u00b7ner", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bekleidet mit der Sonne,", "tokens": ["Be\u00b7klei\u00b7det", "mit", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Mond ihr unterthan.", "tokens": ["Den", "Mond", "ihr", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Lamm als Kindlein s\u00fc\u00dfe", "tokens": ["Ein", "Lamm", "als", "Kin\u00b7dlein", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ihren Armen lag;", "tokens": ["In", "ih\u00b7ren", "Ar\u00b7men", "lag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Alte war's, der Riese,", "tokens": ["Der", "Al\u00b7te", "wa\u00b7r's", ",", "der", "Rie\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der schuf den ersten Tag,", "tokens": ["Der", "schuf", "den", "ers\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der mannlichste der Ritter;", "tokens": ["Der", "mann\u00b7lichs\u00b7te", "der", "Rit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Ihm ward sein Leben bitter", "tokens": ["Ihm", "ward", "sein", "Le\u00b7ben", "bit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch schwerstes Ungemach.", "tokens": ["Durch", "schwers\u00b7tes", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sein Herz ward ihm durchstochen", "tokens": ["Sein", "Herz", "ward", "ihm", "durch\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit einem scharfen Speer,", "tokens": ["Mit", "ei\u00b7nem", "schar\u00b7fen", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit hat er durchbrochen", "tokens": ["Da\u00b7mit", "hat", "er", "durch\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die H\u00f6ll' und all' ihr Heer.", "tokens": ["Die", "H\u00f6ll'", "und", "all'", "ihr", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er l\u00f6ste, die gefangen,", "tokens": ["Er", "l\u00f6s\u00b7te", ",", "die", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Leid war da vergangen,", "tokens": ["Ihr", "Leid", "war", "da", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohl uns der guten M\u00e4r'!", "tokens": ["Wohl", "uns", "der", "gu\u00b7ten", "M\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er stand auf aus dem Grabe,", "tokens": ["Er", "stand", "auf", "aus", "dem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der edle F\u00fcrste gut,", "tokens": ["Der", "ed\u00b7le", "F\u00fcrs\u00b7te", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit seinem Kreuzes-Stabe;", "tokens": ["Mit", "sei\u00b7nem", "Kreu\u00b7zes\u00b7Sta\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein' Wunden waren roth.", "tokens": ["Sein'", "Wun\u00b7den", "wa\u00b7ren", "roth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als er mit gro\u00dfen Ehren", "tokens": ["Als", "er", "mit", "gro\u00b7\u00dfen", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Himmel wollte kehren,", "tokens": ["Zum", "Him\u00b7mel", "woll\u00b7te", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ueberwunden war der Tod.", "tokens": ["Ue\u00b7ber\u00b7wun\u00b7den", "war", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Er ist zum Himmel gefahren", "tokens": ["Er", "ist", "zum", "Him\u00b7mel", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit wundergro\u00dfer Macht.", "tokens": ["Mit", "wun\u00b7der\u00b7gro\u00b7\u00dfer", "Macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wol unz\u00e4hlbare Schaaren", "tokens": ["Wol", "un\u00b7z\u00e4hl\u00b7ba\u00b7re", "Schaa\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Hat Er mit Sich gebracht.", "tokens": ["Hat", "Er", "mit", "Sich", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Himmel, einst geschlossen,", "tokens": ["Der", "Him\u00b7mel", ",", "einst", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Steht nun uns allen offen;", "tokens": ["Steht", "nun", "uns", "al\u00b7len", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Heil, der drein kommen mag!", "tokens": ["Heil", ",", "der", "drein", "kom\u00b7men", "mag", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Nun sitzt Er auf dem Throne", "tokens": ["Nun", "sitzt", "Er", "auf", "dem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Seines Vaters Hand.", "tokens": ["Zu", "Sei\u00b7nes", "Va\u00b7ters", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm singen die Engel zum Lohne", "tokens": ["Ihm", "sin\u00b7gen", "die", "En\u00b7gel", "zum", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein heil'ger Mensch und wahrer Gott", "tokens": ["Ein", "heil'\u00b7ger", "Mensch", "und", "wah\u00b7rer", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Christus ist genannt.", "tokens": ["Chris\u00b7tus", "ist", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}