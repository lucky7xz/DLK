{"textgrid.poem.31715": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf meiner Lippe brennend Rot", "tokens": ["Auf", "mei\u00b7ner", "Lip\u00b7pe", "bren\u00b7nend", "Rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bl\u00fcht nun die f\u00fcrchterlichste Not,", "tokens": ["Bl\u00fcht", "nun", "die", "f\u00fcrch\u00b7ter\u00b7lichs\u00b7te", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da bl\u00fcht wie auf verdorrter Flur", "tokens": ["Da", "bl\u00fcht", "wie", "auf", "ver\u00b7dorr\u00b7ter", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das bittre Kraut des Durstes nur.", "tokens": ["Das", "bitt\u00b7re", "Kraut", "des", "Durs\u00b7tes", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar hab ich fr\u00fche schon und sp\u00e4t", "tokens": ["Zwar", "hab", "ich", "fr\u00fc\u00b7he", "schon", "und", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versucht, was mich kurieren t\u00e4t:", "tokens": ["Ver\u00b7sucht", ",", "was", "mich", "ku\u00b7rie\u00b7ren", "t\u00e4t", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liebfrauenmilch geno\u00df ich schon", "tokens": ["Lieb\u00b7frau\u00b7en\u00b7milch", "ge\u00b7no\u00df", "ich", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als neugeborner junger Sohn.", "tokens": ["Als", "neu\u00b7ge\u00b7bor\u00b7ner", "jun\u00b7ger", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und frischte drauf den trocknen Schlund", "tokens": ["Und", "frischte", "drauf", "den", "trock\u00b7nen", "Schlund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Wein aus Spanien und Burgund.", "tokens": ["Mit", "Wein", "aus", "Spa\u00b7ni\u00b7en", "und", "Bur\u00b7gund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja mehr des goldnen Weins ich trank,", "tokens": ["Ja", "mehr", "des", "gold\u00b7nen", "Weins", "ich", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Regen auf die Felder sank,", "tokens": ["Als", "Re\u00b7gen", "auf", "die", "Fel\u00b7der", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als Wasser einst im Meere flo\u00df,", "tokens": ["Als", "Was\u00b7ser", "einst", "im", "Mee\u00b7re", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drin Pharao mit Mann und Ro\u00df", "tokens": ["Drin", "Pha\u00b7rao", "mit", "Mann", "und", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zugrunde ging! Ja Wein soviel,", "tokens": ["Zu\u00b7grun\u00b7de", "ging", "!", "Ja", "Wein", "so\u00b7viel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PTKANT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Wasser \u00fcbern Rheinfall fiel! \u2013", "tokens": ["Als", "Was\u00b7ser", "\u00fc\u00b7bern", "Rhein\u00b7fall", "fiel", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "VVFIN", "NE", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch immer, wie zu alter Zeit,", "tokens": ["Doch", "im\u00b7mer", ",", "wie", "zu", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Plagt mich dasselbe Kreuz und Leid;", "tokens": ["Plagt", "mich", "das\u00b7sel\u00b7be", "Kreuz", "und", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es stachelt mich des Durstes Dolch,", "tokens": ["Es", "sta\u00b7chelt", "mich", "des", "Durs\u00b7tes", "Dolch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als bissen Schlangen mich und Molch.", "tokens": ["Als", "bis\u00b7sen", "Schlan\u00b7gen", "mich", "und", "Molch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und pre\u00dftet ihr am ganzen Rhein", "tokens": ["Und", "pre\u00df\u00b7tet", "ihr", "am", "gan\u00b7zen", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All Trauben in ein Fa\u00df hinein:", "tokens": ["All", "Trau\u00b7ben", "in", "ein", "Fa\u00df", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich tr\u00e4nk es aus auf einen Zug \u2013", "tokens": ["Ich", "tr\u00e4nk", "es", "aus", "auf", "ei\u00b7nen", "Zug", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4tt noch immer nicht genug.", "tokens": ["Und", "h\u00e4tt", "noch", "im\u00b7mer", "nicht", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und n\u00e4hmt ihr aus dem ew'gen Rom", "tokens": ["Und", "n\u00e4hmt", "ihr", "aus", "dem", "ew'\u00b7gen", "Rom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kuppel von Sankt Petri Dom", "tokens": ["Die", "Kup\u00b7pel", "von", "Sankt", "Pe\u00b7tri", "Dom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fclltet sie mit rotem Wein \u2013", "tokens": ["Und", "f\u00fcll\u00b7tet", "sie", "mit", "ro\u00b7tem", "Wein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Becher w\u00e4r mir noch zu klein!", "tokens": ["Der", "Be\u00b7cher", "w\u00e4r", "mir", "noch", "zu", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Drum hab ich lange schon gesagt:", "tokens": ["Drum", "hab", "ich", "lan\u00b7ge", "schon", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O schrecklich, wen das D\u00fcrsten plagt!", "tokens": ["O", "schreck\u00b7lich", ",", "wen", "das", "D\u00fcrs\u00b7ten", "plagt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist wie ein verla\u00dfnes Kind,", "tokens": ["Er", "ist", "wie", "ein", "ver\u00b7la\u00df\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das nirgends Ruh und Freude find't.", "tokens": ["Das", "nir\u00b7gends", "Ruh", "und", "Freu\u00b7de", "find'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PDS", "ADV", "NN", "KON", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}