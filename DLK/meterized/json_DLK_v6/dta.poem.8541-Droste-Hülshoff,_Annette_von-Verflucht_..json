{"dta.poem.8541": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Verflucht .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was sch&#228;umt das Meer, was w&#228;lzt es sich", "tokens": ["Was", "sch", "&#228;", "umt", "das", "Meer", ",", "was", "w", "&#228;", "lzt", "es", "sich"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "punct", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["PWS", "ADJD", "$(", "VVFIN", "ART", "NN", "$,", "PRELS", "NE", "$(", "VVFIN", "PPER", "PRF"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und b\u00e4umt an das Gestade?", "tokens": ["Und", "b\u00e4umt", "an", "das", "Ge\u00b7sta\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist\u2019s Str\u00f6mung, was da drunten w\u00fchlt?", "tokens": ["Ist's", "Str\u00f6\u00b7mung", ",", "was", "da", "drun\u00b7ten", "w\u00fchlt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist\u2019s unterirdisch Feuer?", "tokens": ["Ist's", "un\u00b7ter\u00b7ir\u00b7disch", "Feu\u00b7er", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht Str\u00f6mung ist es, was da w\u00fchlt,", "tokens": ["Nicht", "Str\u00f6\u00b7mung", "ist", "es", ",", "was", "da", "w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "PPER", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht unterirdisch Feuer,", "tokens": ["Nicht", "un\u00b7ter\u00b7ir\u00b7disch", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Leichnam fiel in seinen Schoo\u00df,", "tokens": ["Ein", "Leich\u00b7nam", "fiel", "in", "sei\u00b7nen", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein siebenmal verfluchter,", "tokens": ["Ein", "sie\u00b7ben\u00b7mal", "ver\u00b7fluch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Des Kaufmanns, der um schn\u00f6des Gold", "tokens": ["Des", "Kauf\u00b7manns", ",", "der", "um", "schn\u00f6\u00b7des", "Gold"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erschlug den eignen Bruder.", "tokens": ["Er\u00b7schlug", "den", "eig\u00b7nen", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}