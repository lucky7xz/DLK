{"textgrid.poem.65919": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Erscheinung", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes Bild, du sollst bei mir erwarmen:", "tokens": ["Ge\u00b7lieb\u00b7tes", "Bild", ",", "du", "sollst", "bei", "mir", "er\u00b7war\u00b7men", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Weib und Wesen, weil in mich verliebt!", "tokens": ["Du", "Weib", "und", "We\u00b7sen", ",", "weil", "in", "mich", "ver\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$,", "KOUS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Wunder w\u00e4ret nicht, wenn ihr verbliebt;", "tokens": ["Ihr", "Wun\u00b7der", "w\u00e4\u00b7ret", "nicht", ",", "wenn", "ihr", "ver\u00b7bliebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und doch, mein Weib, noch halt ich dich in Armen.", "tokens": ["Und", "doch", ",", "mein", "Weib", ",", "noch", "halt", "ich", "dich", "in", "Ar\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geheime Nacht bist herzlich: Stern-Erbarmen.", "tokens": ["Ge\u00b7hei\u00b7me", "Nacht", "bist", "herz\u00b7lich", ":", "Stern\u00b7Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf einmal weinst du: wer uns Hilfe gibt?", "tokens": ["Auf", "ein\u00b7mal", "weinst", "du", ":", "wer", "uns", "Hil\u00b7fe", "gibt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$.", "PWS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Freude durch der Triebe Schwermut siebt:", "tokens": ["Wie", "Freu\u00b7de", "durch", "der", "Trie\u00b7be", "Schwer\u00b7mut", "siebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich kann nach deinem Ku\u00df nicht mehr verarmen!", "tokens": ["Ich", "kann", "nach", "dei\u00b7nem", "Ku\u00df", "nicht", "mehr", "ver\u00b7ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Vater: Gott! Mir graut vor Deiner G\u00fcte.", "tokens": ["Mein", "Va\u00b7ter", ":", "Gott", "!", "Mir", "graut", "vor", "Dei\u00b7ner", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo unverlangt, kam \u00fcber uns das Gl\u00fcck.", "tokens": ["Wo", "un\u00b7ver\u00b7langt", ",", "kam", "\u00fc\u00b7ber", "uns", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun st\u00fctz mich Weib, da\u00df ich dein Gut beh\u00fcte!", "tokens": ["Nun", "st\u00fctz", "mich", "Weib", ",", "da\u00df", "ich", "dein", "Gut", "be\u00b7h\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gew\u00e4hr mir auch, da\u00df Wonne dich ber\u00fcck!", "tokens": ["Ge\u00b7w\u00e4hr", "mir", "auch", ",", "da\u00df", "Won\u00b7ne", "dich", "be\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "KOUS", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erspr\u00fchtes W\u00fcnschen wurde Glut zur Bl\u00fcte:", "tokens": ["Er\u00b7spr\u00fch\u00b7tes", "W\u00fcn\u00b7schen", "wur\u00b7de", "Glut", "zur", "Bl\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An mich schenkst du, was Gott dir gab, zur\u00fcck.", "tokens": ["An", "mich", "schenkst", "du", ",", "was", "Gott", "dir", "gab", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,", "PRELS", "NN", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}