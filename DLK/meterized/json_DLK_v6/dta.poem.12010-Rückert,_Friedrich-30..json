{"dta.poem.12010": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sowahr als aus dem Eins die Zahlenreihe flie\u00dft,", "tokens": ["So\u00b7wahr", "als", "aus", "dem", "Eins", "die", "Zah\u00b7len\u00b7rei\u00b7he", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sowahr aus einem Keim des Baumes Krone sprie\u00dft;", "tokens": ["So\u00b7wahr", "aus", "ei\u00b7nem", "Keim", "des", "Bau\u00b7mes", "Kro\u00b7ne", "sprie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sowahr erkennest du, da\u00df der ist einzig Einer,", "tokens": ["So\u00b7wahr", "er\u00b7ken\u00b7nest", "du", ",", "da\u00df", "der", "ist", "ein\u00b7zig", "Ei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "VAFIN", "ADJD", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus welchem alles ist, und gleich ihm ewig keiner.", "tokens": ["Aus", "wel\u00b7chem", "al\u00b7les", "ist", ",", "und", "gleich", "ihm", "e\u00b7wig", "kei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "VAFIN", "$,", "KON", "ADV", "PPER", "ADJD", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch f\u00fchlt der Mensch soweit vom Ursprung sich getrennt,", "tokens": ["Doch", "f\u00fchlt", "der", "Mensch", "so\u00b7weit", "vom", "Ur\u00b7sprung", "sich", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Mittelstufen er nothwendig anerkennt.", "tokens": ["Da\u00df", "Mit\u00b7tel\u00b7stu\u00b7fen", "er", "noth\u00b7wen\u00b7dig", "an\u00b7er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ob er sie G\u00f6tter mag, Kr\u00e4ft' oder Geister nennen,", "tokens": ["Ob", "er", "sie", "G\u00f6t\u00b7ter", "mag", ",", "Kr\u00e4ft'", "o\u00b7der", "Geis\u00b7ter", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VMFIN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihn binden sollen sie an Gott, von Gott nicht trennen.", "tokens": ["Ihn", "bin\u00b7den", "sol\u00b7len", "sie", "an", "Gott", ",", "von", "Gott", "nicht", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie sollen das Geweb vom Mittelpunkt ausbreiten,", "tokens": ["Sie", "sol\u00b7len", "das", "Ge\u00b7web", "vom", "Mit\u00b7tel\u00b7punkt", "aus\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis in sein kleines Ich die Lebensf\u00e4den leiten.", "tokens": ["Bis", "in", "sein", "klei\u00b7nes", "Ich", "die", "Le\u00b7bens\u00b7f\u00e4\u00b7den", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was also streitet ihr um wechselnde Betitlung", "tokens": ["Was", "al\u00b7so", "strei\u00b7tet", "ihr", "um", "wech\u00b7seln\u00b7de", "Be\u00b7tit\u00b7lung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Von Heilsanstalt und Amt der S\u00fchnung und Vermittlung?", "tokens": ["Von", "Heil\u00b7san\u00b7stalt", "und", "Amt", "der", "S\u00fch\u00b7nung", "und", "Ver\u00b7mitt\u00b7lung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ob hier der Sch\u00f6pfer sich verborgen im Erhalter,", "tokens": ["Ob", "hier", "der", "Sch\u00f6p\u00b7fer", "sich", "ver\u00b7bor\u00b7gen", "im", "Er\u00b7hal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Hausherr dort zur\u00fcck trat hinterm Hausverwalter?", "tokens": ["Der", "Haus\u00b7herr", "dort", "zu\u00b7r\u00fcck", "trat", "hin\u00b7term", "Haus\u00b7ver\u00b7wal\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ihr m\u00f6gt mit Fr\u00f6mmigkeit und gl\u00e4ubigem Vertraun", "tokens": ["Ihr", "m\u00f6gt", "mit", "Fr\u00f6m\u00b7mig\u00b7keit", "und", "gl\u00e4u\u00b7bi\u00b7gem", "Ver\u00b7traun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sichtbares als ein Bild des Unsichtbaren schaun.", "tokens": ["Sicht\u00b7ba\u00b7res", "als", "ein", "Bild", "des", "Un\u00b7sicht\u00b7ba\u00b7ren", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Doch stehts dem Geiste frei, wenn er dazu hat Schwingen,", "tokens": ["Doch", "stehts", "dem", "Geis\u00b7te", "frei", ",", "wenn", "er", "da\u00b7zu", "hat", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "PAV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Allerheiligste unmittelbar zu dringen.", "tokens": ["Ins", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7te", "un\u00b7mit\u00b7tel\u00b7bar", "zu", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}