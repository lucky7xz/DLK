{"textgrid.poem.24281": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Erz\u00e4hlung", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00e4dchen besa\u00df ich, fein wie ein Fig\u00fcrchen", "tokens": ["Ein", "M\u00e4d\u00b7chen", "be\u00b7sa\u00df", "ich", ",", "fein", "wie", "ein", "Fi\u00b7g\u00fcr\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf Rokokotischen galanter Marquisen;", "tokens": ["Auf", "Ro\u00b7ko\u00b7ko\u00b7ti\u00b7schen", "ga\u00b7lan\u00b7ter", "Mar\u00b7qui\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es war wohl auch wirklich verwandt mit diesen:", "tokens": ["Es", "war", "wohl", "auch", "wirk\u00b7lich", "ver\u00b7wandt", "mit", "die\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "APPR", "PDAT", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Halb war es ein Nobelchen, halb ein H\u00fcrchen.", "tokens": ["Halb", "war", "es", "ein", "No\u00b7bel\u00b7chen", ",", "halb", "ein", "H\u00fcr\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ich fand sie entz\u00fcckend mit ihrem Geschw\u00e4nzel,", "tokens": ["Ich", "fand", "sie", "ent\u00b7z\u00fc\u00b7ckend", "mit", "ih\u00b7rem", "Ge\u00b7schw\u00e4n\u00b7zel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Getrippel, Ge\u00e4ugel, Gel\u00e4chel, Geplapper.", "tokens": ["Ge\u00b7trip\u00b7pel", ",", "Ge\u00b7\u00e4u\u00b7gel", ",", "Ge\u00b7l\u00e4\u00b7chel", ",", "Ge\u00b7plap\u00b7per", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich war so ein junger mutwilliger Tapper,", "tokens": ["Ich", "war", "so", "ein", "jun\u00b7ger", "mut\u00b7wil\u00b7li\u00b7ger", "Tap\u00b7per", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mein Gehen war auch noch Geh\u00fcpf und Get\u00e4nzel.", "tokens": ["Mein", "Ge\u00b7hen", "war", "auch", "noch", "Ge\u00b7h\u00fcpf", "und", "Ge\u00b7t\u00e4n\u00b7zel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Auch war ich ein Tr\u00e4umer und Wolkenbeschauer;", "tokens": ["Auch", "war", "ich", "ein", "Tr\u00e4u\u00b7mer", "und", "Wol\u00b7ken\u00b7be\u00b7schau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich sah um die Dinge noch goldene R\u00e4nder.", "tokens": ["Ich", "sah", "um", "die", "Din\u00b7ge", "noch", "gol\u00b7de\u00b7ne", "R\u00e4n\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Mond war mein Krongut; in meinem Kalender", "tokens": ["Der", "Mond", "war", "mein", "Kron\u00b7gut", ";", "in", "mei\u00b7nem", "Ka\u00b7len\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hatte der Fr\u00fchling zw\u00f6lf Monate Dauer.", "tokens": ["Hat\u00b7te", "der", "Fr\u00fch\u00b7ling", "zw\u00f6lf", "Mo\u00b7na\u00b7te", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "CARD", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "So waren wir also ein passendes P\u00e4rchen.", "tokens": ["So", "wa\u00b7ren", "wir", "al\u00b7so", "ein", "pas\u00b7sen\u00b7des", "P\u00e4r\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie tanzte, ich dichtete, Gott blies die Fl\u00f6te", "tokens": ["Sie", "tanz\u00b7te", ",", "ich", "dich\u00b7te\u00b7te", ",", "Gott", "blies", "die", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und freute sich selber der purpurnen R\u00f6te", "tokens": ["Und", "freu\u00b7te", "sich", "sel\u00b7ber", "der", "pur\u00b7pur\u00b7nen", "R\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des Himmels, in dem wir das munterste M\u00e4rchen", "tokens": ["Des", "Him\u00b7mels", ",", "in", "dem", "wir", "das", "mun\u00b7ters\u00b7te", "M\u00e4r\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ART", "ADJA", "NN"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und aller Romane verliebtesten lebten:", "tokens": ["Und", "al\u00b7ler", "Ro\u00b7ma\u00b7ne", "ver\u00b7lieb\u00b7tes\u00b7ten", "leb\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Tr\u00e4umen getragen, von Liedern belogen,", "tokens": ["Von", "Tr\u00e4u\u00b7men", "ge\u00b7tra\u00b7gen", ",", "von", "Lie\u00b7dern", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "In goldener Nu\u00dfschale schwimmend auf Wogen", "tokens": ["In", "gol\u00b7de\u00b7ner", "Nu\u00df\u00b7scha\u00b7le", "schwim\u00b7mend", "auf", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPR", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und Wolken, die rosig ins Nichts verschwebten.", "tokens": ["Und", "Wol\u00b7ken", ",", "die", "ro\u00b7sig", "ins", "Nichts", "ver\u00b7schweb\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADJD", "APPRART", "PIS", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": " ... Ins Nichts verschwebten; verrannen; vergingen;", "tokens": ["...", "Ins", "Nichts", "ver\u00b7schweb\u00b7ten", ";", "ver\u00b7ran\u00b7nen", ";", "ver\u00b7gin\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "APPRART", "PIS", "VVINF", "$.", "VVINF", "$.", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zerflossen, zerrissen, \u2013 ins Nichts, in die Leere ...", "tokens": ["Zer\u00b7flos\u00b7sen", ",", "zer\u00b7ris\u00b7sen", ",", "\u2013", "ins", "Nichts", ",", "in", "die", "Lee\u00b7re", "..."], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "$(", "APPRART", "PIS", "$,", "APPR", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Uns aber erfa\u00dfte die irdische Schwere", "tokens": ["Uns", "a\u00b7ber", "er\u00b7fa\u00df\u00b7te", "die", "ir\u00b7di\u00b7sche", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und zerrte uns nieder mit w\u00fcrgenden Schlingen.", "tokens": ["Und", "zerr\u00b7te", "uns", "nie\u00b7der", "mit", "w\u00fcr\u00b7gen\u00b7den", "Schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Da half uns kein Gott. Es verstummte die Fl\u00f6te", "tokens": ["Da", "half", "uns", "kein", "Gott", ".", "Es", "ver\u00b7stumm\u00b7te", "die", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Des M\u00e4rchenpapas und Idyllenrhapsoden.", "tokens": ["Des", "M\u00e4r\u00b7chen\u00b7pa\u00b7pas", "und", "I\u00b7dyl\u00b7lenr\u00b7ha\u00b7pso\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir fielen auf dornigen, steinigen Boden,", "tokens": ["Wir", "fie\u00b7len", "auf", "dor\u00b7ni\u00b7gen", ",", "stei\u00b7ni\u00b7gen", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und zwischen uns sa\u00df eine zankende Kr\u00f6te:", "tokens": ["Und", "zwi\u00b7schen", "uns", "sa\u00df", "ei\u00b7ne", "zan\u00b7ken\u00b7de", "Kr\u00f6\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Die kahle Entt\u00e4uschung. Es lehrte ihr Zanken", "tokens": ["Die", "kah\u00b7le", "Ent\u00b7t\u00e4u\u00b7schung", ".", "Es", "lehr\u00b7te", "ihr", "Zan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Unlieblich uns beide einander erkennen.", "tokens": ["Un\u00b7lieb\u00b7lich", "uns", "bei\u00b7de", "ein\u00b7an\u00b7der", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PIS", "PRF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es war wie ein Aneinanderverbrennen", "tokens": ["Es", "war", "wie", "ein", "An\u00b7ein\u00b7an\u00b7der\u00b7ver\u00b7bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bis tief auf den grundallerletzten Gedanken", "tokens": ["Bis", "tief", "auf", "den", "grun\u00b7dal\u00b7ler\u00b7letz\u00b7ten", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "An jenes Schmarotzen im M\u00e4rchengel\u00e4nde. \u2013", "tokens": ["An", "je\u00b7nes", "Schma\u00b7rot\u00b7zen", "im", "M\u00e4r\u00b7chen\u00b7ge\u00b7l\u00e4n\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wir haben die Hand uns zum Abschied gegeben", "tokens": ["Wir", "ha\u00b7ben", "die", "Hand", "uns", "zum", "Ab\u00b7schied", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie Fremde. Nie sah ich sie wieder im Leben.", "tokens": ["Wie", "Frem\u00b7de", ".", "Nie", "sah", "ich", "sie", "wie\u00b7der", "im", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und k\u00e4nnte sie nicht, auch wenn ich sie f\u00e4nde.", "tokens": ["Und", "k\u00e4nn\u00b7te", "sie", "nicht", ",", "auch", "wenn", "ich", "sie", "f\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}