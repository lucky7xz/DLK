{"textgrid.poem.33785": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Persergebet", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast gest\u00fcrzt, o Tagespracht,", "tokens": ["Du", "hast", "ge\u00b7st\u00fcrzt", ",", "o", "Ta\u00b7ge\u00b7spracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nacht zum Meeresgrunde,", "tokens": ["Die", "Nacht", "zum", "Mee\u00b7res\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du wandelst deine helle Wacht", "tokens": ["Du", "wan\u00b7delst", "dei\u00b7ne", "hel\u00b7le", "Wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und machst die Segensrunde.", "tokens": ["Und", "machst", "die", "Se\u00b7gens\u00b7run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Alles atmet Seligkeit,", "tokens": ["Und", "Al\u00b7les", "at\u00b7met", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wieder du erschienen;", "tokens": ["Da", "wie\u00b7der", "du", "er\u00b7schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So will auch ich in T\u00e4tigkeit", "tokens": ["So", "will", "auch", "ich", "in", "T\u00e4\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in Gebet dir dienen.", "tokens": ["Und", "in", "Ge\u00b7bet", "dir", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Was dir zuwider, tilg' ich fort,", "tokens": ["Was", "dir", "zu\u00b7wi\u00b7der", ",", "til\u00b7g'", "ich", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Sumpf- und Moderscharen,", "tokens": ["Die", "Sumpf", "und", "Mo\u00b7der\u00b7scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor Lug, Verrat, gebrochnem Wort", "tokens": ["Vor", "Lug", ",", "Ver\u00b7rat", ",", "ge\u00b7broch\u00b7nem", "Wort"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will ich mein Herz bewahren.", "tokens": ["Will", "ich", "mein", "Herz", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ha\u00dft der Mensch die Kr\u00f6tenbrut,", "tokens": ["Es", "ha\u00dft", "der", "Mensch", "die", "Kr\u00f6\u00b7ten\u00b7brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schlangen Giftgewinde,", "tokens": ["Der", "Schlan\u00b7gen", "Gift\u00b7ge\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ha\u00dft des Blitzes reine Glut", "tokens": ["Es", "ha\u00dft", "des", "Blit\u00b7zes", "rei\u00b7ne", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das t\u00fcckisch Seelenblinde.", "tokens": ["Das", "t\u00fc\u00b7ckisch", "See\u00b7len\u00b7blin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Herr, der du im Lichtgewand", "tokens": ["O", "Herr", ",", "der", "du", "im", "Licht\u00b7ge\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Feuerdienst geboten,", "tokens": ["Den", "Feu\u00b7er\u00b7dienst", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Luft wird schw\u00fcl, vom Abendland", "tokens": ["Die", "Luft", "wird", "schw\u00fcl", ",", "vom", "A\u00b7bend\u00b7land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nahn deine Feuerboten.", "tokens": ["Nahn", "dei\u00b7ne", "Feu\u00b7er\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gib, da\u00df ich nicht in Furcht erbleich'", "tokens": ["Gib", ",", "da\u00df", "ich", "nicht", "in", "Furcht", "er\u00b7bleich'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor deinen Ungewittern;", "tokens": ["Vor", "dei\u00b7nen", "Un\u00b7ge\u00b7wit\u00b7tern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich der reinen Blume gleich", "tokens": ["La\u00df", "mich", "der", "rei\u00b7nen", "Blu\u00b7me", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Liebe nur erzittern.", "tokens": ["In", "Lie\u00b7be", "nur", "er\u00b7zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gib, da\u00df mich deine Flammenkraft", "tokens": ["Gib", ",", "da\u00df", "mich", "dei\u00b7ne", "Flam\u00b7men\u00b7kraft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erf\u00fclle mit der St\u00e4rke,", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "mit", "der", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die f\u00fcr die Nacht den Frieden schafft", "tokens": ["Die", "f\u00fcr", "die", "Nacht", "den", "Frie\u00b7den", "schafft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr den Tag die Werke.", "tokens": ["Und", "f\u00fcr", "den", "Tag", "die", "Wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}