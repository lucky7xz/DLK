{"textgrid.poem.24665": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vernichtunglodernd,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vernichtunglodernd,", "tokens": ["Ver\u00b7nich\u00b7tun\u00b7glo\u00b7dernd", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "T\u00f6dlich leuchtend,", "tokens": ["T\u00f6d\u00b7lich", "leuch\u00b7tend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Lebenversengend", "tokens": ["Le\u00b7ben\u00b7ver\u00b7sen\u00b7gend"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gl\u00fcht uns im Innern", "tokens": ["Gl\u00fcht", "uns", "im", "In\u00b7nern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Flammender Genius.", "tokens": ["Flam\u00b7men\u00b7der", "Ge\u00b7nius", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Aber es sch\u00fctzt uns", "tokens": ["A\u00b7ber", "es", "sch\u00fctzt", "uns"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Vor dem Verglimmen", "tokens": ["Vor", "dem", "Ver\u00b7glim\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "K\u00fchlenden Unkrauts dichte Decke,", "tokens": ["K\u00fch\u00b7len\u00b7den", "Un\u00b7krauts", "dich\u00b7te", "De\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Die unser Herz feucht wuchernd umspinnt:", "tokens": ["Die", "un\u00b7ser", "Herz", "feucht", "wu\u00b7chernd", "um\u00b7spinnt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Gewohnheit und gedankenlose", "tokens": ["Ge\u00b7wohn\u00b7heit", "und", "ge\u00b7dan\u00b7ken\u00b7lo\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Lust am Leben,", "tokens": ["Lust", "am", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Und tr\u00f6stende L\u00fcge,", "tokens": ["Und", "tr\u00f6s\u00b7ten\u00b7de", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Und s\u00fc\u00dfer Selbstbetrug,", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "Selbst\u00b7be\u00b7trug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und trauliches D\u00e4mmern", "tokens": ["Und", "trau\u00b7li\u00b7ches", "D\u00e4m\u00b7mern"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Von heut auf morgen ...", "tokens": ["Von", "heut", "auf", "mor\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADV", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Wir tragen im Innern", "tokens": ["Wir", "tra\u00b7gen", "im", "In\u00b7nern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Leuchtend die Charis,", "tokens": ["Leuch\u00b7tend", "die", "Cha\u00b7ris", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Die strahlende Ahnung der Kunst.", "tokens": ["Die", "strah\u00b7len\u00b7de", "Ah\u00b7nung", "der", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.19": {"text": "Aber die G\u00f6tter haben sie t\u00fcckisch", "tokens": ["A\u00b7ber", "die", "G\u00f6t\u00b7ter", "ha\u00b7ben", "sie", "t\u00fc\u00b7ckisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "Mit dem Heph\u00e4stos verm\u00e4hlt:", "tokens": ["Mit", "dem", "He\u00b7ph\u00e4s\u00b7tos", "ver\u00b7m\u00e4hlt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Dem schmierigen Handwerk,", "tokens": ["Dem", "schmie\u00b7ri\u00b7gen", "Hand\u00b7werk", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Der hinkenden Plage,", "tokens": ["Der", "hin\u00b7ken\u00b7den", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Der humpelnden, keuchenden Unzul\u00e4nglichkeit.", "tokens": ["Der", "hum\u00b7peln\u00b7den", ",", "keu\u00b7chen\u00b7den", "Un\u00b7zu\u00b7l\u00e4ng\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Wir tragen im Innern", "tokens": ["Wir", "tra\u00b7gen", "im", "In\u00b7nern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Den Tr\u00e4umer Hamlet, den D\u00e4nenprinzen,", "tokens": ["Den", "Tr\u00e4u\u00b7mer", "Ham\u00b7let", ",", "den", "D\u00e4\u00b7nen\u00b7prin\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Den schaurig klugen,", "tokens": ["Den", "schau\u00b7rig", "klu\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Den K\u00fcnstler der Lebensverneinung,", "tokens": ["Den", "K\u00fcnst\u00b7ler", "der", "Le\u00b7bens\u00b7ver\u00b7nei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.28": {"text": "Der den Schrei der Verzweiflung noch geistreich umrankt mit funkelndem Witz.", "tokens": ["Der", "den", "Schrei", "der", "Ver\u00b7zwei\u00b7flung", "noch", "geist\u00b7reich", "um\u00b7rankt", "mit", "fun\u00b7keln\u00b7dem", "Witz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+--+-+--+", "measure": "anapaest.tetra.plus"}, "line.29": {"text": "Aber bei ihm sitzt", "tokens": ["A\u00b7ber", "bei", "ihm", "sitzt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.30": {"text": "In unserer Seele enger Zelle", "tokens": ["In", "un\u00b7se\u00b7rer", "See\u00b7le", "en\u00b7ger", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Mit bl\u00f6dem M\u00f6nchsflei\u00df,", "tokens": ["Mit", "bl\u00f6\u00b7dem", "M\u00f6nchs\u00b7flei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "Und emsig das Leben bejahend,", "tokens": ["Und", "em\u00b7sig", "das", "Le\u00b7ben", "be\u00b7ja\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.33": {"text": "Gr\u00e4ber schaufelnd der schmerzenden Wahrheit,", "tokens": ["Gr\u00e4\u00b7ber", "schau\u00b7felnd", "der", "schmer\u00b7zen\u00b7den", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.34": {"text": "Gr\u00e4ber von B\u00fcchern, Worten, Staub,", "tokens": ["Gr\u00e4\u00b7ber", "von", "B\u00fc\u00b7chern", ",", "Wor\u00b7ten", ",", "Staub", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.35": {"text": "Der eignen Beschr\u00e4nktheit in Ehren froh,", "tokens": ["Der", "eig\u00b7nen", "Be\u00b7schr\u00e4nk\u00b7theit", "in", "Eh\u00b7ren", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Ein lallender Kobold: der deutsche Professor ...", "tokens": ["Ein", "lal\u00b7len\u00b7der", "Ko\u00b7bold", ":", "der", "deut\u00b7sche", "Pro\u00b7fes\u00b7sor", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wir tragen im Innern den Faust, den Titanen,", "tokens": ["Wir", "tra\u00b7gen", "im", "In\u00b7nern", "den", "Faust", ",", "den", "Ti\u00b7ta\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.38": {"text": "Und Sganarelle, die Bedientenseele,", "tokens": ["Und", "Sga\u00b7na\u00b7rel\u00b7le", ",", "die", "Be\u00b7dien\u00b7ten\u00b7see\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Den weinenden Werther \u2013 und Voltaire, den Zweifler,", "tokens": ["Den", "wei\u00b7nen\u00b7den", "Wert\u00b7her", "\u2013", "und", "Vol\u00b7tai\u00b7re", ",", "den", "Zweif\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "NE", "$,", "ART", "NN", "$,"], "meter": "-+--+--++--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Und des Propheten gellenden Wehruf", "tokens": ["Und", "des", "Pro\u00b7phe\u00b7ten", "gel\u00b7len\u00b7den", "We\u00b7hruf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Und das Jauchzen sch\u00f6nheittrunkner Griechen:", "tokens": ["Und", "das", "Jauch\u00b7zen", "sch\u00f6n\u00b7heit\u00b7trunk\u00b7ner", "Grie\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Die Toten dreier Jahrtausende,", "tokens": ["Die", "To\u00b7ten", "drei\u00b7er", "Jahr\u00b7tau\u00b7sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "Ein Bacchanal von Gespenstern.", "tokens": ["Ein", "Bac\u00b7cha\u00b7nal", "von", "Ge\u00b7spens\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "Von andern ersonnen, von andern gezeugt,", "tokens": ["Von", "an\u00b7dern", "er\u00b7son\u00b7nen", ",", "von", "an\u00b7dern", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "$,", "APPR", "PIS", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.45": {"text": "Fremde Parasiten,", "tokens": ["Frem\u00b7de", "Pa\u00b7ra\u00b7si\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.46": {"text": "Anempfunden,", "tokens": ["An\u00b7em\u00b7pfun\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.47": {"text": "Krank, vergiftet. \u2013", "tokens": ["Krank", ",", "ver\u00b7gif\u00b7tet", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "VVPP", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.48": {"text": "Sie wimmern, sie fluchen, sie jauchzen, sie streiten:", "tokens": ["Sie", "wim\u00b7mern", ",", "sie", "flu\u00b7chen", ",", "sie", "jauch\u00b7zen", ",", "sie", "strei\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,", "PPER", "VVINF", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.49": {"text": "Was wir reden, ist heisrer Widerhall", "tokens": ["Was", "wir", "re\u00b7den", ",", "ist", "heis\u00b7rer", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "$,", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "Ihres gellenden Chors.", "tokens": ["Ih\u00b7res", "gel\u00b7len\u00b7den", "Chors", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.51": {"text": "Sie zanken wie taumelnde Zecher", "tokens": ["Sie", "zan\u00b7ken", "wie", "tau\u00b7meln\u00b7de", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.52": {"text": "Uns zur Qual!", "tokens": ["Uns", "zur", "Qual", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.53": {"text": "Aber es eint sie die Orgie", "tokens": ["A\u00b7ber", "es", "eint", "sie", "die", "Or\u00b7gie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.54": {"text": "Uns zur Qual!", "tokens": ["Uns", "zur", "Qual", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.55": {"text": "Sie trinken aus unsrem Sch\u00e4del", "tokens": ["Sie", "trin\u00b7ken", "aus", "uns\u00b7rem", "Sch\u00e4\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.56": {"text": "Jauchzend den Saft unsres Lebens \u2013", "tokens": ["Jauch\u00b7zend", "den", "Saft", "uns\u00b7res", "Le\u00b7bens", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.57": {"text": "Sie ranken sich erstickend,", "tokens": ["Sie", "ran\u00b7ken", "sich", "er\u00b7sti\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Zischende Schlangen,", "tokens": ["Zi\u00b7schen\u00b7de", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.59": {"text": "Um unser Bewu\u00dftsein \u2013", "tokens": ["Um", "un\u00b7ser", "Be\u00b7wu\u00df\u00b7tsein", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.60": {"text": "Sie r\u00fctteln am \u00e4chzenden Baum unsres Gl\u00fccks", "tokens": ["Sie", "r\u00fct\u00b7teln", "am", "\u00e4ch\u00b7zen\u00b7den", "Baum", "uns\u00b7res", "Gl\u00fccks"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.61": {"text": "Im Fiebersturm \u2013", "tokens": ["Im", "Fie\u00