{"textgrid.poem.46771": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich hab' ein M\u00e4rchen geh\u00f6rt einmal]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' ein M\u00e4rchen geh\u00f6rt einmal:", "tokens": ["Ich", "hab'", "ein", "M\u00e4r\u00b7chen", "ge\u00b7h\u00f6rt", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Mann waldeinw\u00e4rts machte", "tokens": ["Ein", "Mann", "wal\u00b7de\u00b7in\u00b7w\u00e4rts", "mach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den Weg, da sa\u00df im Abendstral", "tokens": ["Den", "Weg", ",", "da", "sa\u00df", "im", "A\u00b7bend\u00b7stral"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kinderpaar und lachte.", "tokens": ["Ein", "Kin\u00b7der\u00b7paar", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das lachende Paar ihn Wunder nahm,", "tokens": ["Das", "la\u00b7chen\u00b7de", "Paar", "ihn", "Wun\u00b7der", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch ging er den Weg hernieder,", "tokens": ["Doch", "ging", "er", "den", "Weg", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und als hinaus vor den Wald er kam,", "tokens": ["Und", "als", "hin\u00b7aus", "vor", "den", "Wald", "er", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APZR", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da sa\u00df das P\u00e4rchen wieder.", "tokens": ["Da", "sa\u00df", "das", "P\u00e4r\u00b7chen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit Staunen hat er hier wie dort", "tokens": ["Mit", "Stau\u00b7nen", "hat", "er", "hier", "wie", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "KOKOM", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das lachende Paar gefunden;", "tokens": ["Das", "la\u00b7chen\u00b7de", "Paar", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch einmal sieht er hin zum Ort,", "tokens": ["Noch", "ein\u00b7mal", "sieht", "er", "hin", "zum", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ist das Paar verschwunden.", "tokens": ["Da", "ist", "das", "Paar", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo ich nun geh' durch Wald und Flur,", "tokens": ["Wo", "ich", "nun", "geh'", "durch", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ist mir's wie ein M\u00e4rchen,", "tokens": ["Da", "ist", "mir's", "wie", "ein", "M\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An Ein- und Ausgang seh' ich nur,", "tokens": ["An", "Ein", "und", "Aus\u00b7gang", "seh'", "ich", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein l\u00e4chelndes lachendes P\u00e4rchen.", "tokens": ["Ein", "l\u00e4\u00b7cheln\u00b7des", "la\u00b7chen\u00b7des", "P\u00e4r\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "So hab' ich selber nie gelacht,", "tokens": ["So", "hab'", "ich", "sel\u00b7ber", "nie", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch h\u00f6ren und sehen lachen;", "tokens": ["Noch", "h\u00f6\u00b7ren", "und", "se\u00b7hen", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVINF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sonst hat es das Herz mir froh gemacht,", "tokens": ["Sonst", "hat", "es", "das", "Herz", "mir", "froh", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Solls nun mich traurig machen?", "tokens": ["Solls", "nun", "mich", "trau\u00b7rig", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nein, da\u00df es einst mich froh gemacht,", "tokens": ["Nein", ",", "da\u00df", "es", "einst", "mich", "froh", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll jetzt noch froh mich machen;", "tokens": ["Soll", "jetzt", "noch", "froh", "mich", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz in seinem Jammer lacht,", "tokens": ["Mein", "Herz", "in", "sei\u00b7nem", "Jam\u00b7mer", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seh' ich mein P\u00e4rchen lachen.", "tokens": ["Seh'", "ich", "mein", "P\u00e4r\u00b7chen", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}