{"dta.poem.20422": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn keine brust sich zeigt/ wenn lippen schweigen m\u00fcssen/", "tokens": ["Wenn", "kei\u00b7ne", "brust", "sich", "zeigt", "/", "wenn", "lip\u00b7pen", "schwei\u00b7gen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "VVFIN", "PRF", "VVFIN", "$(", "KOUS", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So reden wir alsdenn durch unsern klaren schein/", "tokens": ["So", "re\u00b7den", "wir", "als\u00b7denn", "durch", "un\u00b7sern", "kla\u00b7ren", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir f\u00fcgen offtermahls durch einen blick zuwissen/", "tokens": ["Wir", "f\u00fc\u00b7gen", "off\u00b7ter\u00b7mahls", "durch", "ei\u00b7nen", "blick", "zu\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df adern/ blut und marck voll glut und flammen seyn.", "tokens": ["Da\u00df", "a\u00b7dern", "/", "blut", "und", "marck", "voll", "glut", "und", "flam\u00b7men", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "$(", "NN", "KON", "NN", "ADJD", "NN", "KON", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lust/ hoffnung/ liebe/ zorn/ kan ieder in uns lesen/", "tokens": ["Lust", "/", "hoff\u00b7nung", "/", "lie\u00b7be", "/", "zorn", "/", "kan", "ie\u00b7der", "in", "uns", "le\u00b7sen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "VVFIN", "$(", "NN", "$(", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir reden ohne wort/ und sprechen ohne mund;", "tokens": ["Wir", "re\u00b7den", "oh\u00b7ne", "wort", "/", "und", "spre\u00b7chen", "oh\u00b7ne", "mund", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df/ was noch kommen soll/ und allezeit gewesen/", "tokens": ["Di\u00df", "/", "was", "noch", "kom\u00b7men", "soll", "/", "und", "al\u00b7le\u00b7zeit", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PWS", "ADV", "VVINF", "VMFIN", "$(", "KON", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Di\u00df macht das augen-lied durch kluge blicke kund.", "tokens": ["Di\u00df", "macht", "das", "au\u00b7gen\u00b7lied", "durch", "klu\u00b7ge", "bli\u00b7cke", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}