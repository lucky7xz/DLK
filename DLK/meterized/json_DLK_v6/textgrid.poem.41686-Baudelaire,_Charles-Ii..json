{"textgrid.poem.41686": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Aas, Stamm Abels, wird verwesen,", "tokens": ["Dein", "Aas", ",", "Stamm", "A\u00b7bels", ",", "wird", "ver\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df es den Boden fetter macht!", "tokens": ["Da\u00df", "es", "den", "Bo\u00b7den", "fet\u00b7ter", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Stamm Kains, die Tat, die dir erlesen,", "tokens": ["Stamm", "Kains", ",", "die", "Tat", ",", "die", "dir", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast nicht gen\u00fcgend du vollbracht.", "tokens": ["Hast", "nicht", "ge\u00b7n\u00fc\u00b7gend", "du", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Stamm Abels, h\u00f6r des Urteils Stimme:", "tokens": ["Stamm", "A\u00b7bels", ",", "h\u00f6r", "des", "Ur\u00b7teils", "Stim\u00b7me", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Fangspie\u00df ward das Schwert zum Spott!", "tokens": ["Dem", "Fang\u00b7spie\u00df", "ward", "das", "Schwert", "zum", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Stamm Kains, empor zum Himmel klimme,", "tokens": ["Stamm", "Kains", ",", "em\u00b7por", "zum", "Him\u00b7mel", "klim\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PTKVZ", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und auf die Erde schleudre Gott!", "tokens": ["Und", "auf", "die", "Er\u00b7de", "schleud\u00b7re", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}