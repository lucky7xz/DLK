{"textgrid.poem.60444": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Ver\u00e4nderte Welt", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Menschheit ist dahinter kommen,", "tokens": ["Die", "Menschheit", "ist", "da\u00b7hin\u00b7ter", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Trotz aller Gaukelei der Frommen,", "tokens": ["Trotz", "al\u00b7ler", "Gau\u00b7ke\u00b7lei", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mit dem Leben vor dem Grabe", "tokens": ["Da\u00df", "mit", "dem", "Le\u00b7ben", "vor", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Man endlich Ernst zu machen habe.", "tokens": ["Man", "end\u00b7lich", "Ernst", "zu", "ma\u00b7chen", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zerbrochen ist des Wahnes Kette,", "tokens": ["Zer\u00b7bro\u00b7chen", "ist", "des", "Wah\u00b7nes", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde sei nur \u00dcbungsst\u00e4tte,", "tokens": ["Die", "Er\u00b7de", "sei", "nur", "\u00dc\u00b7bungs\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Voltigierbock sei das Leben,", "tokens": ["Nur", "Vol\u00b7ti\u00b7gier\u00b7bock", "sei", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs Ro\u00df werd uns der Himmel heben.", "tokens": ["Aufs", "Ro\u00df", "werd", "uns", "der", "Him\u00b7mel", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf freiem gr\u00fcnem Erdengrunde", "tokens": ["Auf", "frei\u00b7em", "gr\u00fc\u00b7nem", "Er\u00b7den\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird jeder bald schon hier, zur Stunde,", "tokens": ["Wird", "je\u00b7der", "bald", "schon", "hier", ",", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bevor das Grab ihn deckt mit Schollen,", "tokens": ["Be\u00b7vor", "das", "Grab", "ihn", "deckt", "mit", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein R\u00f6\u00dflein weiden, tummeln wollen.", "tokens": ["Sein", "R\u00f6\u00df\u00b7lein", "wei\u00b7den", ",", "tum\u00b7meln", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}