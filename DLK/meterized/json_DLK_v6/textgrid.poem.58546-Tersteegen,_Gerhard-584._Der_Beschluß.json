{"textgrid.poem.58546": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "584. Der Beschlu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gut's lesen nutzt oft viel, Gut's schreiben ist auch gut,", "tokens": ["Gut's", "le\u00b7sen", "nutzt", "oft", "viel", ",", "Gut's", "schrei\u00b7ben", "ist", "auch", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VVFIN", "ADV", "ADV", "$,", "NE", "VVINF", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch sind es Bilder nur, bis man's erf\u00e4hrt und tut.", "tokens": ["Doch", "sind", "es", "Bil\u00b7der", "nur", ",", "bis", "man's", "er\u00b7f\u00e4hrt", "und", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "ADV", "$,", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich lass' die Bilder da und mich ins Wesen wende,", "tokens": ["Ich", "lass'", "die", "Bil\u00b7der", "da", "und", "mich", "ins", "We\u00b7sen", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "KON", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Leser, tu es auch! Dies ist des Lesens Ende.", "tokens": ["Mein", "Le\u00b7ser", ",", "tu", "es", "auch", "!", "Dies", "ist", "des", "Le\u00b7sens", "En\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", 