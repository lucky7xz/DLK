{"textgrid.poem.62757": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Wandersmann, der nicht ein Wort", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wandersmann, der nicht ein Wort", "tokens": ["Ein", "Wan\u00b7ders\u00b7mann", ",", "der", "nicht", "ein", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Apis der Aegypter wu\u00dfte,", "tokens": ["Vom", "A\u00b7pis", "der", "A\u00b7e\u00b7gyp\u00b7ter", "wu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und einst nach Memphis reisen mu\u00dfte,", "tokens": ["Und", "einst", "nach", "Mem\u00b7phis", "rei\u00b7sen", "mu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NE", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Betrat den weltber\u00fchmten Ort", "tokens": ["Be\u00b7trat", "den", "welt\u00b7be\u00b7r\u00fchm\u00b7ten", "Ort"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit forschbegierigem Vergn\u00fcgen.", "tokens": ["Mit", "forschbe\u00b7gie\u00b7ri\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er folgt der ersten besten Bahn", "tokens": ["Er", "folgt", "der", "ers\u00b7ten", "bes\u00b7ten", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sieht auf einem weiten Plan", "tokens": ["Und", "sieht", "auf", "ei\u00b7nem", "wei\u00b7ten", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Itzt einen Tempel vor sich liegen,", "tokens": ["Itzt", "ei\u00b7nen", "Tem\u00b7pel", "vor", "sich", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der dem geblendeten Gesicht", "tokens": ["Der", "dem", "ge\u00b7blen\u00b7de\u00b7ten", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein achtes Wunderwerk verspricht.", "tokens": ["Ein", "ach\u00b7tes", "Wun\u00b7der\u00b7werk", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er gaft und staunt, und um noch mehr zu sehen,", "tokens": ["Er", "gaft", "und", "staunt", ",", "und", "um", "noch", "mehr", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "$,", "KON", "APPR", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Beschlie\u00dft er ganz hinein zu gehen.", "tokens": ["Be\u00b7schlie\u00dft", "er", "ganz", "hin\u00b7ein", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Doch kaum setzt er den Fu\u00df hinein,", "tokens": ["Doch", "kaum", "setzt", "er", "den", "Fu\u00df", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "So bleibt er angeheftet stehen.", "tokens": ["So", "bleibt", "er", "an\u00b7ge\u00b7hef\u00b7tet", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sein Auge will, wie kann es anders seyn?", "tokens": ["Sein", "Au\u00b7ge", "will", ",", "wie", "kann", "es", "an\u00b7ders", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "$,", "PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Zu gleicher Zeit an jedem Vorwurf kleben,", "tokens": ["Zu", "glei\u00b7cher", "Zeit", "an", "je\u00b7dem", "Vor\u00b7wurf", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Den hohe Kunst und unsch\u00e4zbare Pracht", "tokens": ["Den", "ho\u00b7he", "Kunst", "und", "un\u00b7sch\u00e4z\u00b7ba\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der ersten Gottheit w\u00fcrdig macht.", "tokens": ["Der", "ers\u00b7ten", "Got\u00b7theit", "w\u00fcr\u00b7dig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Erz, Marmor, Elfenbein, und Bilder voller Leben", "tokens": ["Erz", ",", "Mar\u00b7mor", ",", "El\u00b7fen\u00b7bein", ",", "und", "Bil\u00b7der", "vol\u00b7ler", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sind \u00fcberall mit Weisheit angebracht.", "tokens": ["Sind", "\u00fc\u00b7be\u00b7rall", "mit", "Weis\u00b7heit", "an\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Den starren Wandersmann ergreift ein heilig Beben.", "tokens": ["Den", "star\u00b7ren", "Wan\u00b7ders\u00b7mann", "er\u00b7greift", "ein", "hei\u00b7lig", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er n\u00e4hert sich, den Herrn so vieler Herrlichkeit,", "tokens": ["Er", "n\u00e4\u00b7hert", "sich", ",", "den", "Herrn", "so", "vie\u00b7ler", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den Weihrauchwolken dicht umgeben,", "tokens": ["Den", "Weih\u00b7rauch\u00b7wol\u00b7ken", "dicht", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Mit tiefer Unterw\u00fcrfigkeit", "tokens": ["Mit", "tie\u00b7fer", "Un\u00b7ter\u00b7w\u00fcr\u00b7fig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "In stummen Hymnen zu verehren.", "tokens": ["In", "stum\u00b7men", "Hym\u00b7nen", "zu", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Allein wie stutzt er nicht, als er den Gott erblickt!", "tokens": ["Al\u00b7lein", "wie", "stutzt", "er", "nicht", ",", "als", "er", "den", "Gott", "er\u00b7blickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein goldner Ochse wars, mit Perlen ausgeschm\u00fcckt.", "tokens": ["Ein", "gold\u00b7ner", "O\u00b7chse", "wars", ",", "mit", "Per\u00b7len", "aus\u00b7ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Kaum kann er sich des Lachens noch erwehren.", "tokens": ["Kaum", "kann", "er", "sich", "des", "La\u00b7chens", "noch", "er\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Ein gro\u00dfes Gl\u00fcck f\u00fcr ihn! Wird diesen fremden Gast", "tokens": ["Ein", "gro\u00b7\u00dfes", "Gl\u00fcck", "f\u00fcr", "ihn", "!", "Wird", "die\u00b7sen", "frem\u00b7den", "Gast"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$.", "VAFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein guter Wind einst nach Europa wehen,", "tokens": ["Ein", "gu\u00b7ter", "Wind", "einst", "nach", "Eu\u00b7ro\u00b7pa", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "So kann er, ohne weit zu gehen,", "tokens": ["So", "kann", "er", ",", "oh\u00b7ne", "weit", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "In manchem gl\u00e4nzenden Pallast", "tokens": ["In", "man\u00b7chem", "gl\u00e4n\u00b7zen\u00b7den", "Pal\u00b7last"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Dergleichen G\u00f6tter t\u00e4glich sehen.", "tokens": ["Derg\u00b7lei\u00b7chen", "G\u00f6t\u00b7ter", "t\u00e4g\u00b7lich", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}