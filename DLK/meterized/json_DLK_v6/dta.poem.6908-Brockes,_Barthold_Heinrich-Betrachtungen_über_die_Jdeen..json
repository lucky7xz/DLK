{"dta.poem.6908": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gantz allgemein und klar ist jede Wahrheit,", "tokens": ["Gantz", "all\u00b7ge\u00b7mein", "und", "klar", "ist", "je\u00b7de", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die so nothwendig,", "tokens": ["Die", "so", "noth\u00b7wen\u00b7dig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Als auch best\u00e4ndig:", "tokens": ["Als", "auch", "be\u00b7st\u00e4n\u00b7dig", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und ihre ew\u2019ge Klarheit,", "tokens": ["Und", "ih\u00b7re", "ew'\u00b7ge", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die unserm Geist allein", "tokens": ["Die", "un\u00b7serm", "Geist", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur eigen, kan durchaus nicht so willk\u00fchrlich seyn.", "tokens": ["Nur", "ei\u00b7gen", ",", "kan", "durc\u00b7haus", "nicht", "so", "will\u00b7k\u00fchr\u00b7lich", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VMFIN", "ADV", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob unser Sinn nun gleich sie in uns rege macht;", "tokens": ["Ob", "un\u00b7ser", "Sinn", "nun", "gleich", "sie", "in", "uns", "re\u00b7ge", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "PPER", "APPR", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "So sind sie doch von uns mit auf die Welt gebracht.", "tokens": ["So", "sind", "sie", "doch", "von", "uns", "mit", "auf", "die", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}