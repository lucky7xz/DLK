{"textgrid.poem.55761": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Bekenntnis", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe zerw\u00fchlt und zerbissen", "tokens": ["Ich", "ha\u00b7be", "zer\u00b7w\u00fchlt", "und", "zer\u00b7bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mein Kissen", "tokens": ["Mein", "Kis\u00b7sen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "In \u00e4chzenden Qualen der Nacht,", "tokens": ["In", "\u00e4ch\u00b7zen\u00b7den", "Qua\u00b7len", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Am Tage dann Witze gerissen,", "tokens": ["Am", "Ta\u00b7ge", "dann", "Wit\u00b7ze", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Das Gewissen", "tokens": ["Das", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Bet\u00e4ubt und die Schmerzen verlacht.", "tokens": ["Be\u00b7t\u00e4ubt", "und", "die", "Schmer\u00b7zen", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ich tr\u00e4umte von Lilienstirnen,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "von", "Li\u00b7li\u00b7ens\u00b7tir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von dunkler Augen Gewalt,", "tokens": ["Von", "dunk\u00b7ler", "Au\u00b7gen", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und umgab mich mit Bestiengehirnen", "tokens": ["Und", "um\u00b7gab", "mich", "mit", "Be\u00b7sti\u00b7en\u00b7ge\u00b7hir\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und umkrampfte der w\u00fcstesten Dirnen", "tokens": ["Und", "um\u00b7krampf\u00b7te", "der", "w\u00fcs\u00b7tes\u00b7ten", "Dir\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Lusttolle Gestalt.", "tokens": ["Lust\u00b7tol\u00b7le", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ich schien ein Apostel des Lebens", "tokens": ["Ich", "schien", "ein", "A\u00b7pos\u00b7tel", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "In der F\u00e4ulnis bet\u00f6rendem Schein", "tokens": ["In", "der", "F\u00e4ul\u00b7nis", "be\u00b7t\u00f6\u00b7ren\u00b7dem", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und spottete jeglichen Strebens,", "tokens": ["Und", "spot\u00b7te\u00b7te", "jeg\u00b7li\u00b7chen", "Stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Weil ich selber gerungen vergebens,", "tokens": ["Weil", "ich", "sel\u00b7ber", "ge\u00b7run\u00b7gen", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "ADV", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Um \u00bbEtwas\u00ab zu sein.", "tokens": ["Um", "\u00bb", "Et\u00b7was", "\u00ab", "zu", "sein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "$(", "ADV", "$(", "PTKZU", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Ich sprach von G\u00f6ttergen\u00fcssen,", "tokens": ["Ich", "sprach", "von", "G\u00f6t\u00b7ter\u00b7ge\u00b7n\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von K\u00fcssen,", "tokens": ["Von", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Von schwellender Glieder Pracht,", "tokens": ["Von", "schwel\u00b7len\u00b7der", "Glie\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von Freude und Gl\u00fccklichsein-m\u00fcssen,", "tokens": ["Von", "Freu\u00b7de", "und", "Gl\u00fc\u00b7ck\u00b7lich\u00b7sein\u00b7m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zerrissen", "tokens": ["Zer\u00b7ris\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Von \u00e4chzenden Qualen der Nacht.", "tokens": ["Von", "\u00e4ch\u00b7zen\u00b7den", "Qua\u00b7len", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}