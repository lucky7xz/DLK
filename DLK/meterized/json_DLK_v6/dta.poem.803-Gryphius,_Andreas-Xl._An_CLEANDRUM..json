{"dta.poem.803": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xl.  \n An  CLEANDRUM.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dvfragst wie Bibulus die zeit zu Rom vertreibe? ", "tokens": ["Dvfragst", "wie", "Bi\u00b7bu\u00b7lus", "die", "zeit", "zu", "Rom", "ver\u00b7trei\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Er sucht kein\u2019 alte Schrifft/ noch Bild/ noch Buch/ wie", "tokens": ["Er", "sucht", "kein'", "al\u00b7te", "Schrifft", "/", "noch", "Bild", "/", "noch", "Buch", "/", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$(", "ADV", "NN", "$(", "ADV", "NN", "$(", "KOKOM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "du. ", "tokens": ["du", "."], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Er k\u00fcmmert sich nicht viel was man zu Hofe thu/", "tokens": ["Er", "k\u00fcm\u00b7mert", "sich", "nicht", "viel", "was", "man", "zu", "Ho\u00b7fe", "thu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ADV", "PWS", "PIS", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er fragt nicht ob der Bapst/ Bann oder Abla\u00df schreibe:", "tokens": ["Er", "fragt", "nicht", "ob", "der", "Bapst", "/", "Bann", "o\u00b7der", "Ab\u00b7la\u00df", "schrei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "ART", "NN", "$(", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}}}}