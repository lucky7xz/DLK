{"textgrid.poem.27923": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer vielleichte soll ertrincken,", "tokens": ["Wer", "viel\u00b7leich\u00b7te", "soll", "er\u00b7trin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darff ins Wasser nicht versincken,", "tokens": ["Darff", "ins", "Was\u00b7ser", "nicht", "ver\u00b7sin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alldieweil ein Deutscher Mann", "tokens": ["All\u00b7die\u00b7weil", "ein", "Deut\u00b7scher", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch im Glas ersauffen kan.", "tokens": ["Auch", "im", "Glas", "er\u00b7sauf\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}