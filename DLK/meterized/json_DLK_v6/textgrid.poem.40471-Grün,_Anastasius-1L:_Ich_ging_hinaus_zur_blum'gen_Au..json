{"textgrid.poem.40471": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging hinaus zur blum'gen Au.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging hinaus zur blum'gen Au.", "tokens": ["Ich", "ging", "hin\u00b7aus", "zur", "blum'\u00b7gen", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ruhte Braut Natur im gr\u00fcnen Sammtkleid,", "tokens": ["Da", "ruh\u00b7te", "Braut", "Na\u00b7tur", "im", "gr\u00fc\u00b7nen", "Sammt\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Haar den frischen Kranz, das Haupt entschleiert:", "tokens": ["Im", "Haar", "den", "fri\u00b7schen", "Kranz", ",", "das", "Haupt", "ent\u00b7schlei\u00b7ert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den wei\u00dfen Schleier hatte sie gelegt", "tokens": ["Den", "wei\u00b7\u00dfen", "Schlei\u00b7er", "hat\u00b7te", "sie", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf ihren Putztisch: jenen alten Gletscher.", "tokens": ["Auf", "ih\u00b7ren", "Putz\u00b7tisch", ":", "je\u00b7nen", "al\u00b7ten", "Glet\u00b7scher", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Man sieht ihr's an, sie harrt des Br\u00e4utigams. \u2013", "tokens": ["Man", "sieht", "ih\u00b7r's", "an", ",", "sie", "harrt", "des", "Br\u00e4u\u00b7ti\u00b7gams", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NE", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Doch ziemt's wohl Br\u00e4uten, so mit Fremden buhlen?", "tokens": ["Doch", "ziemt's", "wohl", "Br\u00e4u\u00b7ten", ",", "so", "mit", "Frem\u00b7den", "buh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$,", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es wogt entbl\u00f6\u00dft ihr voller Lilienbusen", "tokens": ["Es", "wogt", "ent\u00b7bl\u00f6\u00dft", "ihr", "vol\u00b7ler", "Li\u00b7li\u00b7en\u00b7bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Mit seinem \u00fcpp'gen Rosenknospenpaar;", "tokens": ["Mit", "sei\u00b7nem", "\u00fcpp'\u00b7gen", "Ro\u00b7sen\u00b7knos\u00b7pen\u00b7paar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mit ihren gro\u00dfen lichten Blumenaugen", "tokens": ["Mit", "ih\u00b7ren", "gro\u00b7\u00dfen", "lich\u00b7ten", "Blu\u00b7men\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Lieb\u00e4ugelt sie ringsum und wirft muthwillig", "tokens": ["Lie\u00b7b\u00e4u\u00b7gelt", "sie", "ring\u00b7sum", "und", "wirft", "mut\u00b7hwil\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJD"], "meter": "++--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Mir Dutzende von ihren Liebesbriefchen,", "tokens": ["Mir", "Dut\u00b7zen\u00b7de", "von", "ih\u00b7ren", "Lie\u00b7bes\u00b7brief\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Den wei\u00dfen Bl\u00fcthen, scherzend in den Schoo\u00df.", "tokens": ["Den", "wei\u00b7\u00dfen", "Bl\u00fc\u00b7then", ",", "scher\u00b7zend", "in", "den", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mir war ganz wohl, klar stand's in meinem Sinn,", "tokens": ["Mir", "war", "ganz", "wohl", ",", "klar", "stan\u00b7d's", "in", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Da\u00df man wohl gl\u00fccklich kann auf Erden sein.", "tokens": ["Da\u00df", "man", "wohl", "gl\u00fcck\u00b7lich", "kann", "auf", "Er\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "VMFIN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wallte in der blum'gen Au.", "tokens": ["Ich", "wall\u00b7te", "in", "der", "blum'\u00b7gen", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sa\u00df der junge Lenz an einer Quelle,", "tokens": ["Da", "sa\u00df", "der", "jun\u00b7ge", "Lenz", "an", "ei\u00b7ner", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah, er r\u00fcstet sich zur Braut zu gehn;", "tokens": ["Ich", "sah", ",", "er", "r\u00fcs\u00b7tet", "sich", "zur", "Braut", "zu", "gehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ins sonnenstrahlige Gelocke hat", "tokens": ["Ins", "son\u00b7nen\u00b7strah\u00b7li\u00b7ge", "Ge\u00b7lo\u00b7cke", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein blitzend Diadem er aufgedr\u00fcckt,", "tokens": ["Ein", "blit\u00b7zend", "Di\u00b7a\u00b7dem", "er", "auf\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er wusch das reine, klare Antlitz sich", "tokens": ["Er", "wusch", "das", "rei\u00b7ne", ",", "kla\u00b7re", "Ant\u00b7litz", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und \u00fcberspritzte sch\u00e4kernd dann auch mich", "tokens": ["Und", "\u00fc\u00b7bers\u00b7pritz\u00b7te", "sch\u00e4\u00b7kernd", "dann", "auch", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit Quellenschaum vom Wirbel bis zur Zehe.", "tokens": ["Mit", "Quel\u00b7len\u00b7schaum", "vom", "Wir\u00b7bel", "bis", "zur", "Ze\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch, zur Entsch\u00e4d'gung gleichsam, brach er drauf", "tokens": ["Doch", ",", "zur", "Ent\u00b7sch\u00e4d'\u00b7gung", "gleich\u00b7sam", ",", "brach", "er", "drauf"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPRART", "NN", "ADJD", "$,", "VVFIN", "PPER", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Rasch eine Hand voll Perlen aus der Kron'", "tokens": ["Rasch", "ei\u00b7ne", "Hand", "voll", "Per\u00b7len", "aus", "der", "Kron'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und warf sie mir zu F\u00fc\u00dfen in das Gras.", "tokens": ["Und", "warf", "sie", "mir", "zu", "F\u00fc\u00b7\u00dfen", "in", "das", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich war so heiter, fast schien mir's ein Traum,", "tokens": ["Ich", "war", "so", "hei\u00b7ter", ",", "fast", "schien", "mir's", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Da man auf Erden elend k\u00f6nne sein.", "tokens": ["Da", "man", "auf", "Er\u00b7den", "e\u00b7lend", "k\u00f6n\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wallte heim aus blum'ger Au.", "tokens": ["Ich", "wall\u00b7te", "heim", "aus", "blum'\u00b7ger", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Brautpaar war sich an die Brust gesunken. \u2013", "tokens": ["Das", "Braut\u00b7paar", "war", "sich", "an", "die", "Brust", "ge\u00b7sun\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich zog, das Herz voll Lust, den Mund voll Lieder,", "tokens": ["Ich", "zog", ",", "das", "Herz", "voll", "Lust", ",", "den", "Mund", "voll", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Frohlockend heimw\u00e4rts in die dumpfe Stadt;", "tokens": ["Froh\u00b7lo\u00b7ckend", "heim\u00b7w\u00e4rts", "in", "die", "dump\u00b7fe", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Da schwebt an mir vorbei ein liebend Paar,", "tokens": ["Da", "schwebt", "an", "mir", "vor\u00b7bei", "ein", "lie\u00b7bend", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zwei und doch Eins! wie sich zwei Nachbarst\u00e4mme", "tokens": ["Zwei", "und", "doch", "Eins", "!", "wie", "sich", "zwei", "Nach\u00b7bar\u00b7st\u00e4m\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "KON", "ADV", "NN", "$.", "PWAV", "PRF", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Kron' und Wurzeln in einander ranken.", "tokens": ["In", "Kron'", "und", "Wur\u00b7zeln", "in", "ein\u00b7an\u00b7der", "ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wollt ihr das Gl\u00fcck sehn; seht in ihre Augen!", "tokens": ["Wollt", "ihr", "das", "Gl\u00fcck", "sehn", ";", "seht", "in", "ih\u00b7re", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wollt ihr die Freude schau'n: schaut ihre Wangen!", "tokens": ["Wollt", "ihr", "die", "Freu\u00b7de", "schau'n", ":", "schaut", "ih\u00b7re", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sucht ihr die Liebe: horchet ihren Lippen! \u2013", "tokens": ["Sucht", "ihr", "die", "Lie\u00b7be", ":", "hor\u00b7chet", "ih\u00b7ren", "Lip\u00b7pen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Doch seltsam, jetzt erst f\u00fchlt' ich's, da\u00df auf Erden", "tokens": ["Doch", "selt\u00b7sam", ",", "jetzt", "erst", "f\u00fchlt'", "ich's", ",", "da\u00df", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADV", "ADV", "VVFIN", "PIS", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Man elend auch, recht elend k\u00f6nne sein!", "tokens": ["Man", "e\u00b7lend", "auch", ",", "recht", "e\u00b7lend", "k\u00f6n\u00b7ne", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADV", "$,", "ADV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}