{"textgrid.poem.34089": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Welt", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich die Welt im Ganzen und Gro\u00dfen betrachte, so glaub' ich's,", "tokens": ["Wenn", "ich", "die", "Welt", "im", "Gan\u00b7zen", "und", "Gro\u00b7\u00dfen", "be\u00b7trach\u00b7te", ",", "so", "glaub'", "ich's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "KON", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df sie von Ewigkeit ist, ja und allein durch sich selbst;", "tokens": ["Da\u00df", "sie", "von", "E\u00b7wig\u00b7keit", "ist", ",", "ja", "und", "al\u00b7lein", "durch", "sich", "selbst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "$,", "ADV", "KON", "ADV", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Wenn ich mir aber sodann das Einzelne n\u00e4her beschaue,", "tokens": ["Wenn", "ich", "mir", "a\u00b7ber", "so\u00b7dann", "das", "Ein\u00b7zel\u00b7ne", "n\u00e4\u00b7her", "be\u00b7schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ART", "ADJA", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Kommt sie mir vor, wie der Witz eines gewaltigen Ichs.", "tokens": ["Kommt", "sie", "mir", "vor", ",", "wie", "der", "Witz", "ei\u00b7nes", "ge\u00b7wal\u00b7ti\u00b7gen", "Ichs", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}}}}}