{"dta.poem.19013": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Neptunus an das Frawen Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.85", "nl:0.14"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wje sunst ein Potentat/ der weit vn\u0303 brait regieret/", "tokens": ["Wie", "sunst", "ein", "Po\u00b7ten\u00b7tat", "/", "der", "weit", "v\u00f1", "brait", "re\u00b7gie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$(", "ART", "ADJD", "ADJD", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wol da\u00df Er erzaig seine leutseeligkeit/", "tokens": ["So", "wol", "da\u00df", "Er", "er\u00b7zaig", "sei\u00b7ne", "leut\u00b7see\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Als auch da\u00df er sein volck/ in friden/ einigkeit/", "tokens": ["Als", "auch", "da\u00df", "er", "sein", "volck", "/", "in", "fri\u00b7den", "/", "ei\u00b7nig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "KOUS", "PPER", "PPOSAT", "NN", "$(", "APPR", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd gutter lieb erhalt/ dieselbe visitieret:", "tokens": ["Vnd", "gut\u00b7ter", "lieb", "er\u00b7halt", "/", "die\u00b7sel\u00b7be", "vi\u00b7si\u00b7tie\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJD", "VVFIN", "$(", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Also th\u00e4t ich auch l\u00e4ngst (vn\u0303 billich zwar) beschliessen/", "tokens": ["Al\u00b7so", "th\u00e4t", "ich", "auch", "l\u00e4ngst", "(", "v\u00f1", "bil\u00b7lich", "zwar", ")", "be\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$(", "ADV", "ADJD", "ADV", "$(", "VVPP", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die quellen/ see/ vnd fl\u00fc\u00df/ die mit standhaffter trew/", "tokens": ["Die", "quel\u00b7len", "/", "see", "/", "vnd", "fl\u00fc\u00df", "/", "die", "mit", "stand\u00b7haff\u00b7ter", "trew", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NE", "$(", "KON", "VVFIN", "$(", "ART", "APPR", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von anbegin von Mir jhr lehen halten frey", "tokens": ["Von", "an\u00b7be\u00b7gin", "von", "Mir", "jhr", "le\u00b7hen", "hal\u00b7ten", "frey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NE", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch diser welt vmbkrai\u00df einst freindlich zu be-", "tokens": ["Durch", "di\u00b7ser", "welt", "vmb\u00b7krai\u00df", "einst", "freind\u00b7lich", "zu", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADV", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "gr\u00fcssen.", "tokens": ["gr\u00fcs\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Demnach Ich mich numehr auff dise rai\u00df begeben/", "tokens": ["Dem\u00b7nach", "Ich", "mich", "nu\u00b7mehr", "auff", "di\u00b7se", "rai\u00df", "be\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Traf (vngefehr) Ich an auff meinem nassen Reich", "tokens": ["Traf", "(", "vn\u00b7ge\u00b7fehr", ")", "Ich", "an", "auff", "mei\u00b7nem", "nas\u00b7sen", "Reich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADJD", "$(", "PPER", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In diser Insul gr\u00fcn zw\u00f6lf Ritter (die sehr gleich", "tokens": ["In", "di\u00b7ser", "In\u00b7sul", "gr\u00fcn", "zw\u00f6lf", "Rit\u00b7ter", "(", "die", "sehr", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJD", "CARD", "NN", "$(", "ART", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An dapferkeit vn\u0303 muht) forchtlo\u00df hin vn\u0303 herschwebe\u0304.", "tokens": ["An", "dap\u00b7fer\u00b7keit", "v\u00f1", "muht", ")", "forcht\u00b7lo\u00df", "hin", "v\u00f1", "her\u00b7schwe\u00b7b\u0113", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}}}}}