{"textgrid.poem.38281": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schweitzerlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mi's B\u00fcbli is wohl \u00e4netem Rhin,", "tokens": ["Mi's", "B\u00fcb\u00b7li", "is", "wohl", "\u00e4\u00b7ne\u00b7tem", "Rhin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "I wollt' \u00e4 klini Wile bi ihm si;", "tokens": ["I", "wollt'", "\u00e4", "kli\u00b7ni", "Wi\u00b7le", "bi", "ihm", "si", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Lauberl, lauberl, liri lauberl,", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7ri", "lau\u00b7berl", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauberl, lauberl, litum da.", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7tum", "da", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Mi's B\u00fcbli kauf mir \u00e4 Buchsigs L\u00f6ffeli,", "tokens": ["Mi's", "B\u00fcb\u00b7li", "kauf", "mir", "\u00e4", "Buch\u00b7sigs", "L\u00f6f\u00b7fe\u00b7li", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "ADJA", "NN", "NE", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Giri, Giri, Gitzeli,", "tokens": ["Gi\u00b7ri", ",", "Gi\u00b7ri", ",", "Git\u00b7ze\u00b7li", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Lauberl, lauberl, liri lauberl,", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7ri", "lau\u00b7berl", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauberl, lauberl, litum da.", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7tum", "da", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Buchsigs L\u00f6ffeli ohne Stiel:", "tokens": ["Buch\u00b7sigs", "L\u00f6f\u00b7fe\u00b7li", "oh\u00b7ne", "Stiel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Der schmutzigen Sennen giebt es viel;", "tokens": ["Der", "schmut\u00b7zi\u00b7gen", "Sen\u00b7nen", "giebt", "es", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Lauberl, lauberl, liri lauberl,", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7ri", "lau\u00b7berl", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauberl, lauberl, litum da.", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7tum", "da", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Mi Mueter ist a Schwitzeri,", "tokens": ["Mi", "Mu\u00b7e\u00b7ter", "ist", "a", "Schwit\u00b7ze\u00b7ri", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "NE", "NE", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Giri, giri Gitzeli;", "tokens": ["Gi\u00b7ri", ",", "gi\u00b7ri", "Git\u00b7ze\u00b7li", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "FM.la", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Lauberl, lauberl, liri lauberl,", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7ri", "lau\u00b7berl", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauberl, lauberl, litum da.", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7tum", "da", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Mi Vater ist a Appenzeller,", "tokens": ["Mi", "Va\u00b7ter", "ist", "a", "Ap\u00b7pen\u00b7zel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat weder Win noch Most im Keller;", "tokens": ["Hat", "we\u00b7der", "Win", "noch", "Most", "im", "Kel\u00b7ler", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "NN", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lauberl, lauberl, liri lauberl,", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7ri", "lau\u00b7berl", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauberl, lauberl, litum da.", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7tum", "da", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Mi Vater hat a rothen Stier,", "tokens": ["Mi", "Va\u00b7ter", "hat", "a", "ro\u00b7then", "Stier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist mir lieber weder diese all vier;", "tokens": ["Ist", "mir", "lie\u00b7ber", "we\u00b7der", "die\u00b7se", "all", "vier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KON", "PDAT", "PIAT", "CARD", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Lauberl, lauberl, liri lauberl,", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7ri", "lau\u00b7berl", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauberl, lauberl, litum da.", "tokens": ["Lau\u00b7berl", ",", "lau\u00b7berl", ",", "li\u00b7tum", "da", "."], "token_info": ["word", "punct", "word