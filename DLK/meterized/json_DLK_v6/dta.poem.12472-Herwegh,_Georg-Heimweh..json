{"dta.poem.12472": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Heimweh.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Land, das mich so gastlich aufgenommen,", "tokens": ["O", "Land", ",", "das", "mich", "so", "gast\u00b7lich", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O rebenlaubumkr\u00e4nzter, stolzer Flu\u00df \u2014", "tokens": ["O", "re\u00b7ben\u00b7lau\u00b7bum\u00b7kr\u00e4nz\u00b7ter", ",", "stol\u00b7zer", "Flu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kaum bin ich eurer Schwelle nahgekommen,", "tokens": ["Kaum", "bin", "ich", "eu\u00b7rer", "Schwel\u00b7le", "nah\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Klingt schon mein Gru\u00df herb wie ein Scheidegru\u00df.", "tokens": ["Klingt", "schon", "mein", "Gru\u00df", "herb", "wie", "ein", "Schei\u00b7de\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was soll dem Auge eure Sch\u00f6nheit frommen,", "tokens": ["Was", "soll", "dem", "Au\u00b7ge", "eu\u00b7re", "Sch\u00f6n\u00b7heit", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn diese arme Seele betteln mu\u00df?", "tokens": ["Wenn", "die\u00b7se", "ar\u00b7me", "See\u00b7le", "bet\u00b7teln", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er ist so kalt der fremde Sonnenschein;", "tokens": ["Er", "ist", "so", "kalt", "der", "frem\u00b7de", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich m\u00f6chte, ja ich m\u00f6cht' zu Hause sein!", "tokens": ["Ich", "m\u00f6ch\u00b7te", ",", "ja", "ich", "m\u00f6cht'", "zu", "Hau\u00b7se", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "PPER", "VMFIN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Schwalben seh' ich schon im stillen Flug", "tokens": ["Die", "Schwal\u00b7ben", "seh'", "ich", "schon", "im", "stil\u00b7len", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die H\u00e4user \u2014 nur das meine nicht \u2014 umschweben;", "tokens": ["Die", "H\u00e4u\u00b7ser", "nur", "das", "mei\u00b7ne", "nicht", "um\u00b7schwe\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PDS", "VVFIN", "PTKNEG", "$(", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O warme Luft, und doch nicht warm genug,", "tokens": ["O", "war\u00b7me", "Luft", ",", "und", "doch", "nicht", "warm", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "KON", "ADV", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verpflanzte Blumen wieder zu beleben!", "tokens": ["Ver\u00b7pflanz\u00b7te", "Blu\u00b7men", "wie\u00b7der", "zu", "be\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Baum, der seine jungen Sprossen schlug,", "tokens": ["Der", "Baum", ",", "der", "sei\u00b7ne", "jun\u00b7gen", "Spros\u00b7sen", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was wird dem Fremdling er im Herbste geben?", "tokens": ["Was", "wird", "dem", "Fremd\u00b7ling", "er", "im", "Herbs\u00b7te", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vielleicht ein Kreuz und einen Todtenschrein \u2014", "tokens": ["Viel\u00b7leicht", "ein", "Kreuz", "und", "ei\u00b7nen", "Tod\u00b7ten\u00b7schrein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mich friert, mich friert! ich m\u00f6cht' zu Hause sein! \u2014", "tokens": ["Mich", "friert", ",", "mich", "friert", "!", "ich", "m\u00f6cht'", "zu", "Hau\u00b7se", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "APPR", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}