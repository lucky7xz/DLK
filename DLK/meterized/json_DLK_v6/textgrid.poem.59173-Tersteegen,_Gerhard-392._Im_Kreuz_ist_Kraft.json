{"textgrid.poem.59173": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "392. Im Kreuz ist Kraft", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du k\u00f6stlich's Kreuz, ich liebe dich,", "tokens": ["Du", "k\u00f6st\u00b7lich's", "Kreuz", ",", "ich", "lie\u00b7be", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Meisterin will ich dich w\u00e4hlen;", "tokens": ["Zur", "Meis\u00b7te\u00b7rin", "will", "ich", "dich", "w\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In dir ist immer Kraft f\u00fcr mich,", "tokens": ["In", "dir", "ist", "im\u00b7mer", "Kraft", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie mir will in allen Dingen fehlen.", "tokens": ["Wenn", "sie", "mir", "will", "in", "al\u00b7len", "Din\u00b7gen", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}