{"textgrid.poem.49724": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist denn los?", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist denn los?", "tokens": ["Was", "ist", "denn", "los", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In unsrer alten Kirche Scho\u00df?", "tokens": ["In", "uns\u00b7rer", "al\u00b7ten", "Kir\u00b7che", "Scho\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das kann nicht mehr zur Ruhe kommen,", "tokens": ["Das", "kann", "nicht", "mehr", "zur", "Ru\u00b7he", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das qu\u00e4lt und \u00e4ngstet alle Frommen,", "tokens": ["Das", "qu\u00e4lt", "und", "\u00e4ngs\u00b7tet", "al\u00b7le", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das brodelt, g\u00e4rt und sch\u00e4umt und zischt,", "tokens": ["Das", "bro\u00b7delt", ",", "g\u00e4rt", "und", "sch\u00e4umt", "und", "zischt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als h\u00e4tt' der Teufel was gemischt,", "tokens": ["Als", "h\u00e4tt'", "der", "Teu\u00b7fel", "was", "ge\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "PWS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das riecht verflucht nach Ketzern,", "tokens": ["Das", "riecht", "ver\u00b7flucht", "nach", "Ket\u00b7zern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach Neuerern und Hetzern!", "tokens": ["Nach", "Neu\u00b7e\u00b7rern", "und", "Het\u00b7zern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Es wird gebessert, aufgehellt,", "tokens": ["Es", "wird", "ge\u00b7bes\u00b7sert", ",", "auf\u00b7ge\u00b7hellt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das Alte auf den Kopf gestellt,", "tokens": ["Das", "Al\u00b7te", "auf", "den", "Kopf", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es regen sich die Zweifler,", "tokens": ["Es", "re\u00b7gen", "sich", "die", "Zweif\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die Nicht-so-ganz-Begreifler \u2013 \u2013 \u2013", "tokens": ["Die", "Nicht\u00b7so\u00b7ganz\u00b7Be\u00b7greif\u00b7ler", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$(", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Da aber schallt", "tokens": ["Da", "a\u00b7ber", "schallt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Zu Rom ein donnernd Halt.", "tokens": ["Zu", "Rom", "ein", "don\u00b7nernd", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Wir finden keine neuen Wege.", "tokens": ["Wir", "fin\u00b7den", "kei\u00b7ne", "neu\u00b7en", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wer sie betritt, kommt ab vom Stege,", "tokens": ["Wer", "sie", "be\u00b7tritt", ",", "kommt", "ab", "vom", "Ste\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der immer noch so eng und schmal", "tokens": ["Der", "im\u00b7mer", "noch", "so", "eng", "und", "schmal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Zu Gott f\u00fchrt aus dem Jammertal.", "tokens": ["Zu", "Gott", "f\u00fchrt", "aus", "dem", "Jam\u00b7mer\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.19": {"text": "Ihr eifervollen Umgestalter!", "tokens": ["Ihr", "ei\u00b7fer\u00b7vol\u00b7len", "Um\u00b7ge\u00b7stal\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Was Dummheit und was hohes Alter", "tokens": ["Was", "Dumm\u00b7heit", "und", "was", "ho\u00b7hes", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "PWS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Der guten Menschheit heilig macht,", "tokens": ["Der", "gu\u00b7ten", "Menschheit", "hei\u00b7lig", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Wird nie in andre Form gebracht!", "tokens": ["Wird", "nie", "in", "and\u00b7re", "Form", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wie wollt ihr Halben und ihr Lauen", "tokens": ["Wie", "wollt", "ihr", "Hal\u00b7ben", "und", "ihr", "Lau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Das Eingest\u00fcrzte neu erbauen?", "tokens": ["Das", "Ein\u00b7ge\u00b7st\u00fcrz\u00b7te", "neu", "er\u00b7bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Entweder Heide \u2013 oder Christ,", "tokens": ["Ent\u00b7we\u00b7der", "Hei\u00b7de", "\u2013", "o\u00b7der", "Christ", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$(", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und nehmt die Kirche, wie sie ist!", "tokens": ["Und", "nehmt", "die", "Kir\u00b7che", ",", "wie", "sie", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}