{"textgrid.poem.59527": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zieh mich auf! Zieh mich auf zu dir!", "tokens": ["Zieh", "mich", "auf", "!", "Zieh", "mich", "auf", "zu", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du, der im Himmel wohnet.", "tokens": ["Du", ",", "der", "im", "Him\u00b7mel", "woh\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O wie sch\u00f6n, o wie sch\u00f6n bei dir,", "tokens": ["O", "wie", "sch\u00f6n", ",", "o", "wie", "sch\u00f6n", "bei", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "$,", "FM", "KOKOM", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Der \u00fcberschwenglich lohnet!", "tokens": ["Der", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "loh\u00b7net", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jugend flieht, Freude fliehet fr\u00fch,", "tokens": ["Ju\u00b7gend", "flieht", ",", "Freu\u00b7de", "flie\u00b7het", "fr\u00fch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gl\u00fcck wechselt leicht abwendig,", "tokens": ["Gl\u00fcck", "wech\u00b7selt", "leicht", "ab\u00b7wen\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Gott vers\u00e4umt, Gott vergisset nie,", "tokens": ["Gott", "ver\u00b7s\u00e4umt", ",", "Gott", "ver\u00b7gis\u00b7set", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ist immer gleich best\u00e4ndig.", "tokens": ["Ist", "im\u00b7mer", "gleich", "be\u00b7st\u00e4n\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sei denn fromm, sei denn still in mir,", "tokens": ["Sei", "denn", "fromm", ",", "sei", "denn", "still", "in", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz in s\u00fc\u00dfer Freude!", "tokens": ["Mein", "Herz", "in", "s\u00fc\u00b7\u00dfer", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn er wohnt und er zieht in dir", "tokens": ["Denn", "er", "wohnt", "und", "er", "zieht", "in", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "APPR", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und kennt die Kindlein beide.", "tokens": ["Und", "kennt", "die", "Kin\u00b7dlein", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}