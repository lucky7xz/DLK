{"textgrid.poem.41204": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er hat es zum Schutze gez\u00fccket", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er hat es zum Schutze gez\u00fccket", "tokens": ["Er", "hat", "es", "zum", "Schut\u00b7ze", "ge\u00b7z\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Herrscher sein Eisen, o feire, Gesang!", "tokens": ["Der", "Herr\u00b7scher", "sein", "Ei\u00b7sen", ",", "o", "fei\u00b7re", ",", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "FM", "FM", "$,", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er hat die gewaltige Stahlwand", "tokens": ["Er", "hat", "die", "ge\u00b7wal\u00b7ti\u00b7ge", "Stahl\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ans Ufer der Elbe gepflanzt.", "tokens": ["Ans", "U\u00b7fer", "der", "El\u00b7be", "ge\u00b7pflanzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Hoch stand er im Heldengeschmeide,", "tokens": ["Hoch", "stand", "er", "im", "Hel\u00b7den\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Tapfersten seiner Gebieter zur Hand,", "tokens": ["Die", "Tap\u00b7fers\u00b7ten", "sei\u00b7ner", "Ge\u00b7bie\u00b7ter", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Von strebenden Fahnen umrauschet,", "tokens": ["Von", "stre\u00b7ben\u00b7den", "Fah\u00b7nen", "um\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vom Schalle der H\u00f6rner erg\u00f6tzt;", "tokens": ["Vom", "Schal\u00b7le", "der", "H\u00f6r\u00b7ner", "er\u00b7g\u00f6tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Hoch stand er, und harrte der Sonne", "tokens": ["Hoch", "stand", "er", ",", "und", "harr\u00b7te", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Zeugin der gro\u00dfen Entscheidung. Sie kam,", "tokens": ["Der", "Zeu\u00b7gin", "der", "gro\u00b7\u00dfen", "Ent\u00b7schei\u00b7dung", ".", "Sie", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und harrte des dr\u00e4uenden K\u00f6nigs", "tokens": ["Und", "harr\u00b7te", "des", "dr\u00e4u\u00b7en\u00b7den", "K\u00f6\u00b7nigs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der blauen Geschwader. Er kam.", "tokens": ["Der", "blau\u00b7en", "Ge\u00b7schwa\u00b7der", ".", "Er", "kam", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Still zog er, ein herrlicher Anblick!", "tokens": ["Still", "zog", "er", ",", "ein", "herr\u00b7li\u00b7cher", "An\u00b7blick", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In tiefen geschlossenen Reihen heran.", "tokens": ["In", "tie\u00b7fen", "ge\u00b7schlos\u00b7se\u00b7nen", "Rei\u00b7hen", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die schauernde Gegend ergl\u00e4nzte", "tokens": ["Die", "schau\u00b7ern\u00b7de", "Ge\u00b7gend", "er\u00b7gl\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von Waffen, wie feurige Flut.", "tokens": ["Von", "Waf\u00b7fen", ",", "wie", "feu\u00b7ri\u00b7ge", "Flut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Da schwang sich in f\u00fcrstlicher Seele", "tokens": ["Da", "schwang", "sich", "in", "f\u00fcrst\u00b7li\u00b7cher", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von Joseph der edelste Seufzer empor:", "tokens": ["Von", "Jo\u00b7se\u00b7ph", "der", "e\u00b7dels\u00b7te", "Seuf\u00b7zer", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O K\u00f6nig, am Ende der Tage", "tokens": ["O", "K\u00f6\u00b7nig", ",", "am", "En\u00b7de", "der", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie w\u00fcrdig in Frieden zu ruhn!", "tokens": ["Wie", "w\u00fcr\u00b7dig", "in", "Frie\u00b7den", "zu", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Noch zog er. Da schwang sich ein zweiter:", "tokens": ["Noch", "zog", "er", ".", "Da", "schwang", "sich", "ein", "zwei\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "PRF", "ART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ach, Schwerter von Deutschen gez\u00fccket auf mich!", "tokens": ["Ach", ",", "Schwer\u00b7ter", "von", "Deut\u00b7schen", "ge\u00b7z\u00fc\u00b7cket", "auf", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "APPR", "NN", "VVPP", "APPR", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Noch zog er. Da brach es dem Herrscher", "tokens": ["Noch", "zog", "er", ".", "Da", "brach", "es", "dem", "Herr\u00b7scher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vom Munde: Nicht weiter! Er stand.", "tokens": ["Vom", "Mun\u00b7de", ":", "Nicht", "wei\u00b7ter", "!", "Er", "stand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PTKNEG", "PTKVZ", "$.", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Er sah die gef\u00e4hrlichen H\u00f6hen", "tokens": ["Er", "sah", "die", "ge\u00b7f\u00e4hr\u00b7li\u00b7chen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von Joseph's erwartenden Tausenden voll,", "tokens": ["Von", "Jo\u00b7se\u00b7ph's", "er\u00b7war\u00b7ten\u00b7den", "Tau\u00b7sen\u00b7den", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die g\u00e4hnenden ehernen Schl\u00fcnde", "tokens": ["Die", "g\u00e4h\u00b7nen\u00b7den", "e\u00b7her\u00b7nen", "Schl\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit Tode gef\u00fcllet, und stand.", "tokens": ["Mit", "To\u00b7de", "ge\u00b7f\u00fcl\u00b7let", ",", "und", "stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KON", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Und sing' ich den Gauen, wie lange", "tokens": ["Und", "sing'", "ich", "den", "Gau\u00b7en", ",", "wie", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN", "$,", "PWAV", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vor Joseph unth\u00e4tig das Brennenheer stand?", "tokens": ["Vor", "Jo\u00b7se\u00b7ph", "un\u00b7th\u00e4\u00b7tig", "das", "Bren\u00b7nen\u00b7heer", "stand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+---+--+-++", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und h\u00f6rten die Gauen vom Heere", "tokens": ["Und", "h\u00f6r\u00b7ten", "die", "Gau\u00b7en", "vom", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nicht fl\u00fcchtige Zeugen genug?", "tokens": ["Nicht", "fl\u00fcch\u00b7ti\u00b7ge", "Zeu\u00b7gen", "ge\u00b7nug", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Und sing' ich, wie Friedrich versuchte", "tokens": ["Und", "sing'", "ich", ",", "wie", "Fried\u00b7rich", "ver\u00b7such\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "$,", "PWAV", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Berge dem tapferen Bruder zu nah'n?", "tokens": ["Durch", "Ber\u00b7ge", "dem", "tap\u00b7fe\u00b7ren", "Bru\u00b7der", "zu", "nah'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und sagten die fl\u00fcchtigen Zeugen", "tokens": ["Und", "sag\u00b7ten", "die", "fl\u00fcch\u00b7ti\u00b7gen", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nicht lange den Gauen: Umsonst!", "tokens": ["Nicht", "lan\u00b7ge", "den", "Gau\u00b7en", ":", "Um\u00b7sonst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$.", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Zwar st\u00fcrzte \u2013 verschweig es, Gesang! nicht \u2013", "tokens": ["Zwar", "st\u00fcrz\u00b7te", "\u2013", "ver\u00b7schweig", "es", ",", "Ge\u00b7sang", "!", "nicht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "PPER", "$,", "NN", "$.", "PTKNEG", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von Sachsen geleitet, ein Bergstrom in Nacht,", "tokens": ["Von", "Sach\u00b7sen", "ge\u00b7lei\u00b7tet", ",", "ein", "Berg\u00b7strom", "in", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der tapfere Bruder des Helden", "tokens": ["Der", "tap\u00b7fe\u00b7re", "Bru\u00b7der", "des", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Fluren von B\u00f6hmen heran.", "tokens": ["Die", "Flu\u00b7ren", "von", "B\u00f6h\u00b7men", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Doch wie sich dem Schoo\u00dfe des Wetters,", "tokens": ["Doch", "wie", "sich", "dem", "Schoo\u00b7\u00dfe", "des", "Wet\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das schweigend unwirthliche Berge bedeckt,", "tokens": ["Das", "schwei\u00b7gend", "un\u00b7wirth\u00b7li\u00b7che", "Ber\u00b7ge", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zur schrecklichen Reise gekochet", "tokens": ["Zur", "schreck\u00b7li\u00b7chen", "Rei\u00b7se", "ge\u00b7ko\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Fl\u00fcgel des Keiles entrei\u00dft,", "tokens": ["Der", "Fl\u00fc\u00b7gel", "des", "Kei\u00b7les", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "So ri\u00df sich ans Joseph's Versuchten", "tokens": ["So", "ri\u00df", "sich", "ans", "Jo\u00b7se\u00b7ph's", "Ver\u00b7such\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NE", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein grosser, gef\u00fcrchteter Namen hervor.", "tokens": ["Ein", "gros\u00b7ser", ",", "ge\u00b7f\u00fcrch\u00b7te\u00b7ter", "Na\u00b7men", "her\u00b7vor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Krieger h\u00f6rt: Laudon! und jauchzet:", "tokens": ["Der", "Krie\u00b7ger", "h\u00f6rt", ":", "Lau\u00b7don", "!", "und", "jauch\u00b7zet", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "$.", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gelingen ha mu\u00df es mit ihm!", "tokens": ["Ge\u00b7lin\u00b7gen", "ha", "mu\u00df", "es", "mit", "ihm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Gelungen! Der f\u00fcrstliche Gegner", "tokens": ["Ge\u00b7lun\u00b7gen", "!", "Der", "f\u00fcrst\u00b7li\u00b7che", "Geg\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Versprach sich den Winter in B\u00f6hmen umsonst.", "tokens": ["Ver\u00b7sprach", "sich", "den", "Win\u00b7ter", "in", "B\u00f6h\u00b7men", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "NE", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er kehrte mit schw\u00e4cherem Heere", "tokens": ["Er", "kehr\u00b7te", "mit", "schw\u00e4\u00b7che\u00b7rem", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vom nahenden Sch\u00fctzer gedr\u00e4ngt.", "tokens": ["Vom", "na\u00b7hen\u00b7den", "Sch\u00fct\u00b7zer", "ge\u00b7dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measur