{"textgrid.poem.48650": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Eile zum Lieben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebste, die du's warlich bist,", "tokens": ["Liebs\u00b7te", ",", "die", "du's", "war\u00b7lich", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wilt du mehr sein als nur hei\u00dfen,", "tokens": ["wilt", "du", "mehr", "sein", "als", "nur", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VAINF", "KOKOM", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "so la\u00df sich dir nicht entrei\u00dfen", "tokens": ["so", "la\u00df", "sich", "dir", "nicht", "ent\u00b7rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "PTKNEG", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "dieser Jahre kurze Frist,", "tokens": ["die\u00b7ser", "Jah\u00b7re", "kur\u00b7ze", "Frist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "welche Fl\u00fcssen gleich und Pfeilen", "tokens": ["wel\u00b7che", "Fl\u00fcs\u00b7sen", "gleich", "und", "Pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "unvermutet von uns eilen!", "tokens": ["un\u00b7ver\u00b7mu\u00b7tet", "von", "uns", "ei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jugend liebt und wird geliebt.", "tokens": ["Ju\u00b7gend", "liebt", "und", "wird", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wilst du mich und dich betr\u00fcben?", "tokens": ["Wilst", "du", "mich", "und", "dich", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "KON", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es ist ja das s\u00fc\u00dfe Lieben", "tokens": ["Es", "ist", "ja", "das", "s\u00fc\u00b7\u00dfe", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "eine Tat, die Alles \u00fcbt,", "tokens": ["ei\u00b7ne", "Tat", ",", "die", "Al\u00b7les", "\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "bevoraus wenn man noch gr\u00fcnet,", "tokens": ["be\u00b7vo\u00b7raus", "wenn", "man", "noch", "gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "das uns Gegengunst verdienet.", "tokens": ["das", "uns", "Ge\u00b7gen\u00b7gunst", "ver\u00b7die\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Di\u00df vermischte Milch und Blut,", "tokens": ["Di\u00df", "ver\u00b7mischte", "Milch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "der Hals, diese weichen H\u00e4nde", "tokens": ["der", "Hals", ",", "die\u00b7se", "wei\u00b7chen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PDAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "schlei\u00dfen hin. Es nimmt ein Ende,", "tokens": ["schlei\u00b7\u00dfen", "hin", ".", "Es", "nimmt", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was uns itzt so s\u00fc\u00dfe tut.", "tokens": ["was", "uns", "itzt", "so", "s\u00fc\u00b7\u00dfe", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und von dem wir itzund leben,", "tokens": ["Und", "von", "dem", "wir", "it\u00b7zund", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wird uns bald dem Tode geben.", "tokens": ["wird", "uns", "bald", "dem", "To\u00b7de", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df uns bl\u00fchen, wie wir bl\u00fchn,", "tokens": ["La\u00df", "uns", "bl\u00fc\u00b7hen", ",", "wie", "wir", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "eh' der Winter welker Jahre", "tokens": ["eh'", "der", "Win\u00b7ter", "wel\u00b7ker", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "dir die goldgemengten Haare", "tokens": ["dir", "die", "gold\u00b7ge\u00b7meng\u00b7ten", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wird mit Silber unterziehn,", "tokens": ["wird", "mit", "Sil\u00b7ber", "un\u00b7ter\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "eh' mir dieser Mund erblasset,", "tokens": ["eh'", "mir", "die\u00b7ser", "Mund", "er\u00b7blas\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "der denn ha\u00dft und wird gehasset!", "tokens": ["der", "denn", "ha\u00dft", "und", "wird", "ge\u00b7has\u00b7set", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Geb dich mir, wie ich mich dir,", "tokens": ["Geb", "dich", "mir", ",", "wie", "ich", "mich", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "PWAV", "PPER", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und versichre dich beineben,", "tokens": ["und", "ver\u00b7sich\u00b7re", "dich", "bei\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "da\u00df ich dir kan wiedergeben,", "tokens": ["da\u00df", "ich", "dir", "kan", "wie\u00b7der\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was du hast gegeben mir!", "tokens": ["was", "du", "hast", "ge\u00b7ge\u00b7ben", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "VVPP", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was du hast, das bleibet deine;", "tokens": ["Was", "du", "hast", ",", "das", "blei\u00b7bet", "dei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "PDS", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "doch so ists nicht minder meine,", "tokens": ["doch", "so", "ists", "nicht", "min\u00b7der", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Stimmt ihr G\u00f6tter ein mit mir!", "tokens": ["Stimmt", "ihr", "G\u00f6t\u00b7ter", "ein", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Helft mir ihren Ruhm erheben!", "tokens": ["Helft", "mir", "ih\u00b7ren", "Ruhm", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie ist meines Lebens Leben,", "tokens": ["Sie", "ist", "mei\u00b7nes", "Le\u00b7bens", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sie ist aller Zierde Zier,", "tokens": ["sie", "ist", "al\u00b7ler", "Zier\u00b7de", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und allein der Preis der Sch\u00f6nen,", "tokens": ["und", "al\u00b7lein", "der", "Preis", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "der geb\u00fchrt nur ", "tokens": ["der", "ge\u00b7b\u00fchrt", "nur"], "token_info": ["word", "word", "word"], "pos": ["ART", "VVFIN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}}}}}