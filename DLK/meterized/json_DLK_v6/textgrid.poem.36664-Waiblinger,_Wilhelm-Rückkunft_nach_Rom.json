{"textgrid.poem.36664": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckkunft nach Rom", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seine Beute, die Sch\u00e4tze der Welt, hat der Feldherr, der C\u00e4sar,", "tokens": ["Sei\u00b7ne", "Beu\u00b7te", ",", "die", "Sch\u00e4t\u00b7ze", "der", "Welt", ",", "hat", "der", "Feld\u00b7herr", ",", "der", "C\u00e4\u00b7sar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,", "ART", "NE", "$,"], "meter": "--+--+--+--+---+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Dankbar aufs Kapitol einst im Triumphe gebracht,", "tokens": ["Dank\u00b7bar", "aufs", "Ka\u00b7pi\u00b7tol", "einst", "im", "Tri\u00b7um\u00b7phe", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Kronen bring' ich dir nicht, mir mangelt selbst noch der Lorbeer,", "tokens": ["Kro\u00b7nen", "bring'", "ich", "dir", "nicht", ",", "mir", "man\u00b7gelt", "selbst", "noch", "der", "Lor\u00b7beer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Nimm meine Lieder daf\u00fcr, Jupiter Xenius, an!", "tokens": ["Nimm", "mei\u00b7ne", "Lie\u00b7der", "da\u00b7f\u00fcr", ",", "Ju\u00b7pi\u00b7ter", "Xe\u00b7nius", ",", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PAV", "$,", "NE", "NE", "$,", "PTKVZ", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}}}}}