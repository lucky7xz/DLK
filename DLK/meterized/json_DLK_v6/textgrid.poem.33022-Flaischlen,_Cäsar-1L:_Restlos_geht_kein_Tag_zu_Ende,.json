{"textgrid.poem.33022": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Restlos geht kein Tag zu Ende,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Restlos geht kein Tag zu Ende,", "tokens": ["Rest\u00b7los", "geht", "kein", "Tag", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "richt es, schicht es, wie du willst ...", "tokens": ["richt", "es", ",", "schicht", "es", ",", "wie", "du", "willst", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "r\u00fchrst du noch so sehr die H\u00e4nde", "tokens": ["r\u00fchrst", "du", "noch", "so", "sehr", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und liegt alles glatt und fertig,", "tokens": ["und", "liegt", "al\u00b7les", "glatt", "und", "fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "was da fertig werden sollte,", "tokens": ["was", "da", "fer\u00b7tig", "wer\u00b7den", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "richt es, schicht es, wie du willst,", "tokens": ["richt", "es", ",", "schicht", "es", ",", "wie", "du", "willst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "restlos bringst du's nie zu Ende ...", "tokens": ["rest\u00b7los", "bringst", "du's", "nie", "zu", "En\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "ADV", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "zwirnt sich stets zum andern Tage,", "tokens": ["zwirnt", "sich", "stets", "zum", "an\u00b7dern", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "flicht es, schlicht es, wie du willst ...", "tokens": ["flicht", "es", ",", "schlicht", "es", ",", "wie", "du", "willst", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch vielleicht auch eine Freude,", "tokens": ["Doch", "viel\u00b7leicht", "auch", "ei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "w\u00e4gst du mit gerechter Wage!", "tokens": ["w\u00e4gst", "du", "mit", "ge\u00b7rech\u00b7ter", "Wa\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}