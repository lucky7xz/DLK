{"dta.poem.845": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.  Auff den Sontag de\u00df zu dem Vatter gehen-  \n den Gottes/ oder  Cantate  Johann. 16.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was acht ich trawren! furcht! angst! Jammer! grimme ", "tokens": ["Was", "acht", "ich", "traw\u00b7ren", "!", "furcht", "!", "angst", "!", "Jam\u00b7mer", "!", "grim\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PWS", "CARD", "PPER", "VVINF", "$.", "NN", "$.", "VVPP", "$.", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schmertzen! ", "tokens": ["schmert\u00b7zen", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Creutz/ plagen/ schmach vnd todt. Mein ", "tokens": ["Creutz", "/", "pla\u00b7gen", "/", "schmach", "vnd", "todt", ".", "Mein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$(", "VVFIN", "$(", "VVFIN", "KON", "ADJD", "$.", "PPOSAT"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Durch den nicht gleichen steg/ zum Vater! zweifelt man?", "tokens": ["Durch", "den", "nicht", "glei\u00b7chen", "steg", "/", "zum", "Va\u00b7ter", "!", "zwei\u00b7felt", "man", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "ADJA", "NN", "$(", "APPRART", "NN", "$.", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df mehr denn kurtze Leidt ist nichts als lauter schertzen.", "tokens": ["Di\u00df", "mehr", "denn", "kurt\u00b7ze", "Leidt", "ist", "nichts", "als", "lau\u00b7ter", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJA", "NN", "VAFIN", "PIS", "KOKOM", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist nur eine Wolck/ ein dunst/ ein sturm de\u00df Mertzen!", "tokens": ["Es", "ist", "nur", "ei\u00b7ne", "Wolck", "/", "ein", "dunst", "/", "ein", "sturm", "de\u00df", "Mert\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn mir mein K\u00f6nig selbst/ der alle tr\u00f6sten kan/", "tokens": ["Wenn", "mir", "mein", "K\u00f6\u00b7nig", "selbst", "/", "der", "al\u00b7le", "tr\u00f6s\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "$(", "ART", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den grossen Tr\u00f6ster schickt/ vnd in dem wilden Plan", "tokens": ["Den", "gros\u00b7sen", "Tr\u00f6s\u00b7ter", "schickt", "/", "vnd", "in", "dem", "wil\u00b7den", "Plan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich ab, von abweg f\u00fchrt/ durch seiner warheit Kertzen!", "tokens": ["Mich", "ab", ",", "von", "ab\u00b7weg", "f\u00fchrt", "/", "durch", "sei\u00b7ner", "war\u00b7heit", "Kert\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "APPR", "ADV", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die zeit ist vor der Th\u00fcr/ in der die blinde Welt", "tokens": ["Die", "zeit", "ist", "vor", "der", "Th\u00fcr", "/", "in", "der", "die", "blin\u00b7de", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$(", "APPR", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die/ was nicht jrrdisch ist/ f\u00fcr Fluch vnd schew-sal h\u00e4lt", "tokens": ["Die", "/", "was", "nicht", "jrr\u00b7disch", "ist", "/", "f\u00fcr", "Fluch", "vnd", "sche\u00b7wsal", "h\u00e4lt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "PWS", "PTKNEG", "ADJD", "VAFIN", "$(", "APPR", "NN", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor Gottes Richt-Stuel wird die schwere Straff empfind?", "tokens": ["Vor", "Got\u00b7tes", "Richt\u00b7\u00b7S\u00b7tu\u00b7el", "wird", "die", "schwe\u00b7re", "Straff", "emp\u00b7find", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Die Straff\u2019 vmb da\u00df sie nicht in festem glauben steht", "tokens": ["Die", "Straff'", "vmb", "da\u00df", "sie", "nicht", "in", "fes\u00b7tem", "glau\u00b7ben", "steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "KOUS", "PPER", "PTKNEG", "APPR", "ADJA", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Christus von Jhr zeucht/ vnd da\u00df die Rach angeht", "tokens": ["Da\u00df", "Chris\u00b7tus", "von", "Ihr", "zeucht", "/", "vnd", "da\u00df", "die", "Rach", "an\u00b7geht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "PPER", "VVFIN", "$(", "KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die jhren Printzen sol mit fluch vnd straffe binden.", "tokens": ["Die", "jhren", "Print\u00b7zen", "sol", "mit", "fluch", "vnd", "straf\u00b7fe", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "APPR", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}