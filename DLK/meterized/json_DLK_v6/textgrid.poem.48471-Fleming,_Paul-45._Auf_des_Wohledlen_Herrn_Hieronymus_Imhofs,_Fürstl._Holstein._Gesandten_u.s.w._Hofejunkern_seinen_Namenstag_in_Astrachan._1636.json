{"textgrid.poem.48471": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "45. Auf des Wohledlen Herrn Hieronymus Imhofs, F\u00fcrstl. Holstein. Gesandten u.s.w. Hofejunkern seinen Namenstag in Astrachan. 1636", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6nnt Gott ink\u00fcnftig uns das liebliche Gel\u00fccke,", "tokens": ["G\u00f6nnt", "Gott", "in\u00b7k\u00fcnf\u00b7tig", "uns", "das", "lieb\u00b7li\u00b7che", "Ge\u00b7l\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da unser Deutschland uns sieht kommen wol zur\u00fccke,", "tokens": ["da", "un\u00b7ser", "Deutschland", "uns", "sieht", "kom\u00b7men", "wol", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "VVFIN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "das liebe Vaterland, das, wie man sagen will,", "tokens": ["das", "lie\u00b7be", "Va\u00b7ter\u00b7land", ",", "das", ",", "wie", "man", "sa\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "des Leides und der Angst noch wei\u00df, noch hat kein Ziel,", "tokens": ["des", "Lei\u00b7des", "und", "der", "Angst", "noch", "wei\u00df", ",", "noch", "hat", "kein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "VVFIN", "$,", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "weil Mars noch drinnen rast: alsdenn so wird sichs schicken,", "tokens": ["weil", "Mars", "noch", "drin\u00b7nen", "rast", ":", "als\u00b7denn", "so", "wird", "sichs", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADV", "VVFIN", "$.", "ADV", "ADV", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df wir bei deutscher Kost uns nach der Lust erquicken", "tokens": ["da\u00df", "wir", "bei", "deut\u00b7scher", "Kost", "uns", "nach", "der", "Lust", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und unsrer Freunde Schar, die durch die lange Nacht", "tokens": ["und", "uns\u00b7rer", "Freun\u00b7de", "Schar", ",", "die", "durch", "die", "lan\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NE", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "in s\u00fc\u00dfer Fr\u00f6ligkeit an unsrer Tafel wacht,", "tokens": ["in", "s\u00fc\u00b7\u00dfer", "Fr\u00f6\u00b7lig\u00b7keit", "an", "uns\u00b7rer", "Ta\u00b7fel", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "erz\u00e4hlen, was wir hier nach langer L\u00e4nge sehen,", "tokens": ["er\u00b7z\u00e4h\u00b7len", ",", "was", "wir", "hier", "nach", "lan\u00b7ger", "L\u00e4n\u00b7ge", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "was \u00dcbels und was Guts uns hier und da geschehen.", "tokens": ["was", "\u00dc\u00b7bels", "und", "was", "Guts", "uns", "hier", "und", "da", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "PWS", "NN", "PPER", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn wird uns eine Lust das zu erw\u00e4hnen sein,", "tokens": ["Denn", "wird", "uns", "ei\u00b7ne", "Lust", "das", "zu", "er\u00b7w\u00e4h\u00b7nen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "ART", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "was ohne Reu' und Leid uns itzt kaum f\u00e4llet ein.", "tokens": ["was", "oh\u00b7ne", "Reu'", "und", "Leid", "uns", "itzt", "kaum", "f\u00e4l\u00b7let", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "KON", "NN", "PPER", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Itzt ist es keine Zeit, da\u00df du dir machest Sorgen,", "tokens": ["Itzt", "ist", "es", "kei\u00b7ne", "Zeit", ",", "da\u00df", "du", "dir", "ma\u00b7chest", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "wie du uns w\u00fcrdig wollst bewirten an dem Morgen", "tokens": ["wie", "du", "uns", "w\u00fcr\u00b7dig", "wollst", "be\u00b7wir\u00b7ten", "an", "dem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "bis in die tiefe Nacht, nun wir um deine Hand,", "tokens": ["bis", "in", "die", "tie\u00b7fe", "Nacht", ",", "nun", "wir", "um", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "der H\u00f6hers was geb\u00fchrt, dir kn\u00fcpfen dieses Band,", "tokens": ["der", "H\u00f6\u00b7hers", "was", "ge\u00b7b\u00fchrt", ",", "dir", "kn\u00fcp\u00b7fen", "die\u00b7ses", "Band", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWS", "VVFIN", "$,", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "das reich an Armut ist. La\u00df diesen Kummer fahren!", "tokens": ["das", "reich", "an", "Ar\u00b7mut", "ist", ".", "La\u00df", "die\u00b7sen", "Kum\u00b7mer", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VAFIN", "$.", "VVIMP", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wir wollen diese Lust bis auf die R\u00fcckkunft sparen.", "tokens": ["Wir", "wol\u00b7len", "die\u00b7se", "Lust", "bis", "auf", "die", "R\u00fcck\u00b7kunft", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch da\u00df dem Tage noch geschehe halb sein Recht,", "tokens": ["Doch", "da\u00df", "dem", "Ta\u00b7ge", "noch", "ge\u00b7sche\u00b7he", "halb", "sein", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "wolan, so halte dich auch hier nicht allzuschlecht!", "tokens": ["wo\u00b7lan", ",", "so", "hal\u00b7te", "dich", "auch", "hier", "nicht", "all\u00b7zu\u00b7schlecht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sei gastfrei, wie du pflegst! Wir wolln dir gerne folgen", "tokens": ["Sei", "gast\u00b7frei", ",", "wie", "du", "pflegst", "!", "Wir", "wolln", "dir", "ger\u00b7ne", "fol\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "in die ber\u00fchmte Stadt der weitgepreisten Wolgen:", "tokens": ["in", "die", "be\u00b7r\u00fchm\u00b7te", "Stadt", "der", "weit\u00b7ge\u00b7preis\u00b7ten", "Wol\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "sie mein' ich, Astrachan, die k\u00f6nigliche Stadt,", "tokens": ["sie", "mein'", "ich", ",", "A\u00b7strac\u00b7han", ",", "die", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Stadt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "die viel an voller Lust, wie unser Deutschland, hat.", "tokens": ["die", "viel", "an", "vol\u00b7ler", "Lust", ",", "wie", "un\u00b7ser", "Deutschland", ",", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "$,", "PWAV", "PPOSAT", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Zu Schiffe schickt sichs nicht, da\u00df wir von ganzem Herzen", "tokens": ["Zu", "Schif\u00b7fe", "schickt", "sichs", "nicht", ",", "da\u00df", "wir", "von", "gan\u00b7zem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "nach unsrer sch\u00f6nen Art an solchen Festen scherzen.", "tokens": ["nach", "uns\u00b7rer", "sch\u00f6\u00b7nen", "Art", "an", "sol\u00b7chen", "Fes\u00b7ten", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wir wollen an das Land, in unser Lusthaus gehn;", "tokens": ["Wir", "wol\u00b7len", "an", "das", "Land", ",", "in", "un\u00b7ser", "Lust\u00b7haus", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "da la\u00df die Tafel denn f\u00fcr uns gedecket stehn!", "tokens": ["da", "la\u00df", "die", "Ta\u00b7fel", "denn", "f\u00fcr", "uns", "ge\u00b7de\u00b7cket", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "APPR", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Frei mu\u00df ein Herze sein, das recht sich will erfreuen.", "tokens": ["Frei", "mu\u00df", "ein", "Her\u00b7ze", "sein", ",", "das", "recht", "sich", "will", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "VVFIN", "VAINF", "$,", "PRELS", "ADJD", "PRF", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.30": {"text": "Es i\u00dft sich nicht gut froh, wenn man schon was mu\u00df scheuen.", "tokens": ["Es", "i\u00dft", "sich", "nicht", "gut", "froh", ",", "wenn", "man", "schon", "was", "mu\u00df", "scheu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ADJD", "ADJD", "$,", "KOUS", "PIS", "ADV", "PWS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "G\u00f6nnst du uns volle Lust, so schaff' uns einen Plan,", "tokens": ["G\u00f6nnst", "du", "uns", "vol\u00b7le", "Lust", ",", "so", "schaff'", "uns", "ei\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "den man beschleichen nicht, auch nicht behorchen kan!", "tokens": ["den", "man", "be\u00b7schlei\u00b7chen", "nicht", ",", "auch", "nicht", "be\u00b7hor\u00b7chen", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKNEG", "$,", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der sch\u00f6ne Herbsttag selbst giebt Anla\u00df sich zu setzen", "tokens": ["Der", "sch\u00f6\u00b7ne", "Herbst\u00b7tag", "selbst", "giebt", "An\u00b7la\u00df", "sich", "zu", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "und in der gr\u00fcnen Scho\u00df des Gartens zu ergetzen.", "tokens": ["und", "in", "der", "gr\u00fc\u00b7nen", "Scho\u00df", "des", "Gar\u00b7tens", "zu", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der nahe Weinstock beut die braunen Trauben dar,", "tokens": ["Der", "na\u00b7he", "Wein\u00b7stock", "beut", "die", "brau\u00b7nen", "Trau\u00b7ben", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "mit wei\u00dfen untersteckt. Das fruchtgef\u00fcllte Jahr", "tokens": ["mit", "wei\u00b7\u00dfen", "un\u00b7ter\u00b7steckt", ".", "Das", "frucht\u00b7ge\u00b7f\u00fcll\u00b7te", "Jahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "reicht ganz sein Reichtum her, tr\u00e4gt Pfirschken auf und Quitten,", "tokens": ["reicht", "ganz", "sein", "Reich\u00b7tum", "her", ",", "tr\u00e4gt", "Pfirschken", "auf", "und", "Quit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "NN", "PTKVZ", "KON", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.38": {"text": "l\u00e4\u00dft \u00c4pfel aller Art hin auf den Teppich sch\u00fctten.", "tokens": ["l\u00e4\u00dft", "\u00c4p\u00b7fel", "al\u00b7ler", "Art", "hin", "auf", "den", "Tep\u00b7pich", "sch\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Vater der Revier, gesteifet an sein Rohr,", "tokens": ["Der", "Va\u00b7ter", "der", "Re\u00b7vier", ",", "ge\u00b7stei\u00b7fet", "an", "sein", "Rohr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "setzt frischen Kaffiar und gute Krebse vor.", "tokens": ["setzt", "fri\u00b7schen", "Kaf\u00b7fi\u00b7ar", "und", "gu\u00b7te", "Kreb\u00b7se", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So hei\u00dfe bringen auch Melonen und Arpusen,", "tokens": ["So", "hei\u00b7\u00dfe", "brin\u00b7gen", "auch", "Me\u00b7lo\u00b7nen", "und", "Ar\u00b7pu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "die angenehme Kost f\u00fcr Mutter Amathusen", "tokens": ["die", "an\u00b7ge\u00b7neh\u00b7me", "Kost", "f\u00fcr", "Mut\u00b7ter", "A\u00b7mat\u00b7hu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "und ihr verbuhltes Volk, das nur den Zucker liebt,", "tokens": ["und", "ihr", "ver\u00b7buhl\u00b7tes", "Volk", ",", "das", "nur", "den", "Zu\u00b7cker", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "den Paphos Nachbar hei\u00dft und Kandien uns giebt!", "tokens": ["den", "Pa\u00b7phos", "Nach\u00b7bar", "hei\u00dft", "und", "Kan\u00b7di\u00b7en", "uns", "giebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Di\u00df ist genung Confect. Nun wirst du selber wissen,", "tokens": ["Di\u00df", "ist", "ge\u00b7nung", "Con\u00b7fect", ".", "Nun", "wirst", "du", "sel\u00b7ber", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NE", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "da\u00df sich ein guter Trunk auf einen guten Bissen", "tokens": ["da\u00df", "sich", "ein", "gu\u00b7ter", "Trunk", "auf", "ei\u00b7nen", "gu\u00b7ten", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "geh\u00f6re recht und wol. Doch da sei du darf\u00fcr!", "tokens": ["ge\u00b7h\u00f6\u00b7re", "recht", "und", "wol", ".", "Doch", "da", "sei", "du", "dar\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADV", "$.", "KON", "ADV", "VAFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Hier ist ein \u00dcberflu\u00df an Weine, Meth und Bier'", "tokens": ["Hier", "ist", "ein", "\u00dc\u00b7berf\u00b7lu\u00df", "an", "Wei\u00b7ne", ",", "Me\u00b7th", "und", "Bier'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.49": {"text": "und was den Durst wol l\u00f6scht; es sei denn, da\u00df vielleichte", "tokens": ["und", "was", "den", "Durst", "wol", "l\u00f6scht", ";", "es", "sei", "denn", ",", "da\u00df", "viel\u00b7leich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "der Reu\u00dfen ihr Getr\u00e4nk' dich gar zu schlecht sein deuchte", "tokens": ["der", "Reu\u00b7\u00dfen", "ihr", "Ge\u00b7tr\u00e4nk'", "dich", "gar", "zu", "schlecht", "sein", "deuch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "PPER", "ADV", "PTKA", "ADJD", "VAINF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "f\u00fcr unsern deutschen Mund: so ist der beste Rat,", "tokens": ["f\u00fcr", "un\u00b7sern", "deut\u00b7schen", "Mund", ":", "so", "ist", "der", "bes\u00b7te", "Rat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "sprich unser Schiff drum an, das gnung von Spanien hat,", "tokens": ["sprich", "un\u00b7ser", "Schiff", "drum", "an", ",", "das", "gnung", "von", "Spa\u00b7ni\u00b7en", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PAV", "PTKVZ", "$,", "PRELS", "ADV", "APPR", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.53": {"text": "von Frankreich und vom Rhein'! Ich will vor Willen nemen", "tokens": ["von", "Fran\u00b7kreich", "und", "vom", "Rhein'", "!", "Ich", "will", "vor", "Wil\u00b7len", "ne\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPRART", "NE", "$.", "PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "und mich auf allen Fall und, was du giebst, bequemen.", "tokens": ["und", "mich", "auf", "al\u00b7len", "Fall", "und", ",", "was", "du", "giebst", ",", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN", "KON", "$,", "PWS", "PPER", "VVFIN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "So ekel bin ich nicht, ich bin auch nicht so stolz.", "tokens": ["So", "e\u00b7kel", "bin", "ich", "nicht", ",", "ich", "bin", "auch", "nicht", "so", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Ein guter reiner Trunk schmeckt ja so wol aus Holz", "tokens": ["Ein", "gu\u00b7ter", "rei\u00b7ner", "Trunk", "schmeckt", "ja", "so", "wol", "aus", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "und gar aus blo\u00dfer Hand, als aus dem teuren Golde,", "tokens": ["und", "gar", "aus", "blo\u00b7\u00dfer", "Hand", ",", "als", "aus", "dem", "teu\u00b7ren", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "des mancher mehr vertut, als er sein hat zu Solde.", "tokens": ["des", "man\u00b7cher", "mehr", "ver\u00b7tut", ",", "als", "er", "sein", "hat", "zu", "Sol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVPP", "$,", "KOUS", "PPER", "VAINF", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was aber denk' ich erst: ists heute Freitag nicht,", "tokens": ["Was", "a\u00b7ber", "denk'", "ich", "erst", ":", "ists", "heu\u00b7te", "Frei\u00b7tag", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV", "$.", "VAFIN", "ADV", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dein Fasttag, den ein Man nicht allzuleichtlich bricht,", "tokens": ["dein", "Fast\u00b7tag", ",", "den", "ein", "Man", "nicht", "all\u00b7zu\u00b7leicht\u00b7lich", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "PIS", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der ein Gewissen hat? Wir haben uns zu fr\u00fche", "tokens": ["der", "ein", "Ge\u00b7wis\u00b7sen", "hat", "?", "Wir", "ha\u00b7ben", "uns", "zu", "fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VAFIN", "$.", "PPER", "VAFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "auf diesen Tag gefreut. Tut weiter keine M\u00fche,", "tokens": ["auf", "die\u00b7sen", "Tag", "ge\u00b7freut", ".", "Tut", "wei\u00b7ter", "kei\u00b7ne", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$.", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ihr Br\u00fcder, heut' ists nichts! Wer will sich legen drein?", "tokens": ["ihr", "Br\u00fc\u00b7der", ",", "heut'", "ists", "nichts", "!", "Wer", "will", "sich", "le\u00b7gen", "drein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VAFIN", "PIS", "$.", "PWS", "VMFIN", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was man gelobet, hat, das mu\u00df gehalten sein.", "tokens": ["Was", "man", "ge\u00b7lo\u00b7bet", ",", "hat", ",", "das", "mu\u00df", "ge\u00b7hal\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "VAFIN", "$,", "PDS", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir wollen heute dich in deiner Andacht lassen.", "tokens": ["Wir", "wol\u00b7len", "heu\u00b7te", "dich", "in", "dei\u00b7ner", "An\u00b7dacht", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Freund, morgen wirst du dir ein weltlichs Herze fassen", "tokens": ["Freund", ",", "mor\u00b7gen", "wirst", "du", "dir", "ein", "welt\u00b7lichs", "Her\u00b7ze", "fas\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "ART", "PIS", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und, wie es billig ist, uns alle laden ein:", "tokens": ["und", ",", "wie", "es", "bil\u00b7lig", "ist", ",", "uns", "al\u00b7le", "la\u00b7den", "ein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "PPER", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "so woll'n wir einen Tag f\u00fcr zweie lustig sein.", "tokens": ["so", "woll'n", "wir", "ei\u00b7nen", "Tag", "f\u00fcr", "zwei\u00b7e", "lus\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "APPR", "CARD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}