{"textgrid.poem.35312": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rein die Luft, der Himmel spiegeleben;", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rein die Luft, der Himmel spiegeleben;", "tokens": ["Rein", "die", "Luft", ",", "der", "Him\u00b7mel", "spie\u00b7ge\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nur da\u00df \u00fcber jener Berge Kranz", "tokens": ["Nur", "da\u00df", "\u00fc\u00b7ber", "je\u00b7ner", "Ber\u00b7ge", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In der Abendsonne mattem Glanz", "tokens": ["In", "der", "A\u00b7bend\u00b7son\u00b7ne", "mat\u00b7tem", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wei\u00dfe Wolken langsam sich erheben.", "tokens": ["Wei\u00b7\u00dfe", "Wol\u00b7ken", "lang\u00b7sam", "sich", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Unter mir die Schlucht,", "tokens": ["Un\u00b7ter", "mir", "die", "Schlucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Palmengruppen, schwellende Bananen,", "tokens": ["Pal\u00b7men\u00b7grup\u00b7pen", ",", "schwel\u00b7len\u00b7de", "Ba\u00b7na\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Steinger\u00f6lle, H\u00e4user mit Altanen", "tokens": ["Stein\u00b7ge\u00b7r\u00f6l\u00b7le", ",", "H\u00e4u\u00b7ser", "mit", "Al\u00b7ta\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und die vielbesungne Inselbucht.", "tokens": ["Und", "die", "viel\u00b7be\u00b7sung\u00b7ne", "In\u00b7sel\u00b7bucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Rio de Janeiro! In der fr\u00fchen", "tokens": ["Rio", "de", "Ja\u00b7nei\u00b7ro", "!", "In", "der", "fr\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "NE", "$.", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "D\u00e4mmrungsstunde ruhst du, schon besiegt,", "tokens": ["D\u00e4mm\u00b7rungs\u00b7stun\u00b7de", "ruhst", "du", ",", "schon", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Um die H\u00fcgelkette hingeschmiegt,", "tokens": ["Um", "die", "H\u00fc\u00b7gel\u00b7ket\u00b7te", "hin\u00b7ge\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sterbend nach des Tages hei\u00dfen M\u00fchen.", "tokens": ["Ster\u00b7bend", "nach", "des", "Ta\u00b7ges", "hei\u00b7\u00dfen", "M\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Bis der Morgen graut,", "tokens": ["Bis", "der", "Mor\u00b7gen", "graut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bis die H\u00e4hne von den D\u00e4chern kr\u00e4hen,", "tokens": ["Bis", "die", "H\u00e4h\u00b7ne", "von", "den", "D\u00e4\u00b7chern", "kr\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wird der Tod die Opfer ausersp\u00e4hen,", "tokens": ["Wird", "der", "Tod", "die", "Op\u00b7fer", "au\u00b7ser\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die das Los ihm heute angetraut.", "tokens": ["Die", "das", "Los", "ihm", "heu\u00b7te", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "O des Schattens auf dem Zauberbilde! \u2013", "tokens": ["O", "des", "Schat\u00b7tens", "auf", "dem", "Zau\u00b7ber\u00b7bil\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dunkle F\u00e4rbung liegt auf Wald und Flur;", "tokens": ["Dunk\u00b7le", "F\u00e4r\u00b7bung", "liegt", "auf", "Wald", "und", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kr\u00e4ftig weht der Odem der Natur", "tokens": ["Kr\u00e4f\u00b7tig", "weht", "der", "O\u00b7dem", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Durch der Tropen bl\u00fchende Gefilde;", "tokens": ["Durch", "der", "Tro\u00b7pen", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7fil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und den Herrscherstab", "tokens": ["Und", "den", "Herr\u00b7scher\u00b7stab"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "F\u00fchrt in diesem Reiche kein Herodes;", "tokens": ["F\u00fchrt", "in", "die\u00b7sem", "Rei\u00b7che", "kein", "He\u00b7ro\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NE", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Doch ist dies das Land des j\u00e4hen Todes", "tokens": ["Doch", "ist", "dies", "das", "Land", "des", "j\u00e4\u00b7hen", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PDS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und des Fremdlings nimmersattes Grab.", "tokens": ["Und", "des", "Fremd\u00b7lings", "nim\u00b7mer\u00b7sat\u00b7tes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Eden, wo des Geistes Bl\u00fcten sterben,", "tokens": ["E\u00b7den", ",", "wo", "des", "Geis\u00b7tes", "Bl\u00fc\u00b7ten", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sch\u00f6nes, aber unwirtbares Land,", "tokens": ["Sch\u00f6\u00b7nes", ",", "a\u00b7ber", "un\u00b7wirt\u00b7ba\u00b7res", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wildnis, von verschwenderischer Hand", "tokens": ["Wild\u00b7nis", ",", "von", "ver\u00b7schwen\u00b7de\u00b7ri\u00b7scher", "Hand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Auserw\u00e4hlt, um elend zu verderben.", "tokens": ["Au\u00b7ser\u00b7w\u00e4hlt", ",", "um", "e\u00b7lend", "zu", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUI", "ADJD", "PTKZU", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Durch die reine Luft", "tokens": ["Durch", "die", "rei\u00b7ne", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Zittern unsichtbare Fieberschauer,", "tokens": ["Zit\u00b7tern", "un\u00b7sicht\u00b7ba\u00b7re", "Fie\u00b7ber\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und der Denker schaut, in tiefer Trauer,", "tokens": ["Und", "der", "Den\u00b7ker", "schaut", ",", "in", "tie\u00b7fer", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Nieder auf die gro\u00dfe Totengruft.", "tokens": ["Nie\u00b7der", "auf", "die", "gro\u00b7\u00dfe", "To\u00b7ten\u00b7gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Schleichend kam der Feind; doch immer fester,", "tokens": ["Schlei\u00b7chend", "kam", "der", "Feind", ";", "doch", "im\u00b7mer", "fes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Immer rascher, k\u00fchner ward sein Gang.", "tokens": ["Im\u00b7mer", "ra\u00b7scher", ",", "k\u00fch\u00b7ner", "ward", "sein", "Gang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seine Herkunft ist ein Schreckensklang,", "tokens": ["Sei\u00b7ne", "Her\u00b7kunft", "ist", "ein", "Schre\u00b7ckens\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Pest die Mutter, Cholera die Schwester;", "tokens": ["Pest", "die", "Mut\u00b7ter", ",", "Cho\u00b7le\u00b7ra", "die", "Schwes\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mitleidslos sein Blick,", "tokens": ["Mit\u00b7leids\u00b7los", "sein", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Der aus schwarzen Augenh\u00f6hlen z\u00fcndet.", "tokens": ["Der", "aus", "schwar\u00b7zen", "Au\u00b7gen\u00b7h\u00f6h\u00b7len", "z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Unerwartet, ach! und unergr\u00fcndet", "tokens": ["Un\u00b7er\u00b7war\u00b7tet", ",", "ach", "!", "und", "un\u00b7er\u00b7gr\u00fcn\u00b7det"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ITJ", "$.", "KON", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Kam er, wie ein r\u00e4chendes Geschick.", "tokens": ["Kam", "er", ",", "wie", "ein", "r\u00e4\u00b7chen\u00b7des", "Ge\u00b7schick", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Ausgespieen von Guineas K\u00fcste,", "tokens": ["Aus\u00b7ge\u00b7spi\u00b7een", "von", "Gui\u00b7neas", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Deren arme Kinder Ihr geraubt,", "tokens": ["De\u00b7ren", "ar\u00b7me", "Kin\u00b7der", "Ihr", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihr, die an Vergeltung nie geglaubt,", "tokens": ["Ihr", ",", "die", "an", "Ver\u00b7gel\u00b7tung", "nie", "ge\u00b7glaubt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Stillt er jetzt d\u00e4monische Gel\u00fcste;", "tokens": ["Stillt", "er", "jetzt", "d\u00e4\u00b7mo\u00b7ni\u00b7sche", "Ge\u00b7l\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Aber selten bricht", "tokens": ["A\u00b7ber", "sel\u00b7ten", "bricht"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Er mit kecker Faust des Sklaven Ketten,", "tokens": ["Er", "mit", "ke\u00b7cker", "Faust", "des", "Skla\u00b7ven", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Geht vor\u00fcber an der Henker Betten,", "tokens": ["Geht", "vor\u00b7\u00fc\u00b7ber", "an", "der", "Hen\u00b7ker", "Bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Nur die fremden G\u00e4ste schont er nicht.", "tokens": ["Nur", "die", "frem\u00b7den", "G\u00e4s\u00b7te", "schont", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "F\u00fcr den Frevel mu\u00df die Unschuld b\u00fc\u00dfen;", "tokens": ["F\u00fcr", "den", "Fre\u00b7vel", "mu\u00df", "die", "Un\u00b7schuld", "b\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hier ist Untergang ihr sichrer Lohn.", "tokens": ["Hier", "ist", "Un\u00b7ter\u00b7gang", "ihr", "sich\u00b7rer", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Flehend kr\u00fcmmt des Nordens blonder Sohn,", "tokens": ["Fle\u00b7hend", "kr\u00fcmmt", "des", "Nor\u00b7dens", "blon\u00b7der", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Todesengel! sich zu deinen F\u00fc\u00dfen;", "tokens": ["To\u00b7de\u00b7sen\u00b7gel", "!", "sich", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Doch dein Atem haucht", "tokens": ["Doch", "dein", "A\u00b7tem", "haucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Tr\u00fcbe Wolken vor des Tages Helle,", "tokens": ["Tr\u00fc\u00b7be", "Wol\u00b7ken", "vor", "des", "Ta\u00b7ges", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["AD