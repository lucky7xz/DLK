{"dta.poem.10491": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Kind! Es w\u00e4re dein Verderben,", "tokens": ["Kind", "!", "Es", "w\u00e4\u00b7re", "dein", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich geb' mir selber M\u00fche,", "tokens": ["Und", "ich", "geb'", "mir", "sel\u00b7ber", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dein liebes Herz in Liebe", "tokens": ["Da\u00df", "dein", "lie\u00b7bes", "Herz", "in", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmermehr f\u00fcr mich ergl\u00fche.", "tokens": ["Nim\u00b7mer\u00b7mehr", "f\u00fcr", "mich", "er\u00b7gl\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur da\u00df mir's so leicht gelinget,", "tokens": ["Nur", "da\u00df", "mir's", "so", "leicht", "ge\u00b7lin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NE", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will mich dennoch fast betr\u00fcben,", "tokens": ["Will", "mich", "den\u00b7noch", "fast", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich denke manchmal dennoch:", "tokens": ["Und", "ich", "den\u00b7ke", "manch\u00b7mal", "den\u00b7noch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "--+-+-++", "measure": "anapaest.init"}, "line.4": {"text": "M\u00f6chtest du mich dennoch lieben!", "tokens": ["M\u00f6ch\u00b7test", "du", "mich", "den\u00b7noch", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}