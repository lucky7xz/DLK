{"dta.poem.2773": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Han\u00df L\u00e4cheln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als C\u00e4sar in sein Rom siegprangend eingezogen/", "tokens": ["Als", "C\u00e4\u00b7sar", "in", "sein", "Rom", "sieg\u00b7pran\u00b7gend", "ein\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPOSAT", "NE", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "R\u00fcff das erfreute Volck mit voller Stimm/ Gl\u00fcck zu!", "tokens": ["R\u00fcff", "das", "er\u00b7freu\u00b7te", "Volck", "mit", "vol\u00b7ler", "Stimm", "/", "Gl\u00fcck", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$(", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der K\u00e4yser leb in Fried/ der R\u00f6msche Staat in Ruh/", "tokens": ["Der", "K\u00e4y\u00b7ser", "leb", "in", "Fried", "/", "der", "R\u00f6m\u00b7sche", "Staat", "in", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$(", "ART", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es streute Blumen aus/ und baut\u2019 ihm Ehrenbogen.", "tokens": ["Es", "streu\u00b7te", "Blu\u00b7men", "aus", "/", "und", "baut'", "ihm", "Eh\u00b7ren\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$(", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So oft nun dieser Tag im Jahrbuch wieder kam/", "tokens": ["So", "oft", "nun", "die\u00b7ser", "Tag", "im", "Jahr\u00b7buch", "wie\u00b7der", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PDAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So sahe man wie Rom in neuen Freuden glam.", "tokens": ["So", "sa\u00b7he", "man", "wie", "Rom", "in", "neu\u00b7en", "Freu\u00b7den", "glam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "NE", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir/ obzwar L\u00e4chel nicht/ der Weise/ der Verschmitzte/", "tokens": ["Wir", "/", "ob\u00b7zwar", "L\u00e4\u00b7chel", "nicht", "/", "der", "Wei\u00b7se", "/", "der", "Ver\u00b7schmitz\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$(", "KOUS", "NN", "PTKNEG", "$(", "ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den hohen K\u00f6nigs-Stab in seiner Hand gef\u00fchrt/", "tokens": ["Den", "ho\u00b7hen", "K\u00f6\u00b7nigs\u00b7Stab", "in", "sei\u00b7ner", "Hand", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bekennen/ da\u00df viel mehr sein Leben ward geziert/", "tokens": ["Be\u00b7ken\u00b7nen", "/", "da\u00df", "viel", "mehr", "sein", "Le\u00b7ben", "ward", "ge\u00b7ziert", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PIS", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Wenn er sich in dem Feld auf den Schwein-Zepter st\u00fctzte/", "tokens": ["Wenn", "er", "sich", "in", "dem", "Feld", "auf", "den", "Schwein\u00b7Zep\u00b7ter", "st\u00fctz\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und weil der Thaten Ruhm sich nicht verschweigen l\u00e4st/", "tokens": ["Und", "weil", "der", "Tha\u00b7ten", "Ruhm", "sich", "nicht", "ver\u00b7schwei\u00b7gen", "l\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "PRF", "PTKNEG", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So bricht er jetzt heraus bey seinem Namens-Fest.", "tokens": ["So", "bricht", "er", "jetzt", "he\u00b7raus", "bey", "sei\u00b7nem", "Na\u00b7mens\u00b7Fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir thun was Rom gethan/ in gleichen Andachts-Flammen/", "tokens": ["Wir", "thun", "was", "Rom", "ge\u00b7than", "/", "in", "glei\u00b7chen", "An\u00b7dachts\u00b7Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "NE", "VVPP", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und w\u00fcnschen nur im Geist die Grittle noch zu sehn/", "tokens": ["Und", "w\u00fcn\u00b7schen", "nur", "im", "Geist", "die", "Gritt\u00b7le", "noch", "zu", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie er als seiner Braut must hertzbeweglich flehn/", "tokens": ["Wie", "er", "als", "sei\u00b7ner", "Braut", "must", "hertz\u00b7be\u00b7weg\u00b7lich", "flehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df sie die gantze Heerd der Schweine r\u00fcff zusammen.", "tokens": ["Da\u00df", "sie", "die", "gant\u00b7ze", "Heerd", "der", "Schwei\u00b7ne", "r\u00fcff", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was kan nicht kluger Witz? Wenn andern was gefehlt/", "tokens": ["Was", "kan", "nicht", "klu\u00b7ger", "Witz", "?", "Wenn", "an\u00b7dern", "was", "ge\u00b7fehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ADJA", "NN", "$.", "KOUS", "PIS", "PWS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So hat doch herr Johanns sein Vieh stets voll gezehlt.", "tokens": ["So", "hat", "doch", "herr", "Jo\u00b7hanns", "sein", "Vieh", "stets", "voll", "ge\u00b7zehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "NE", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Jugend lie\u00df sich so in solchen R\u00e4ncken sp\u00fcren/", "tokens": ["Die", "Ju\u00b7gend", "lie\u00df", "sich", "so", "in", "sol\u00b7chen", "R\u00e4n\u00b7cken", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und wiese da\u00df di\u00df Holtz ein G\u00f6tze w\u00fcrde seyn.", "tokens": ["Und", "wie\u00b7se", "da\u00df", "di\u00df", "Holtz", "ein", "G\u00f6t\u00b7ze", "w\u00fcr\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PDS", "NN", "ART", "NN", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Thaten traffen auch noch \u00fcber Hoffen ein.", "tokens": ["Die", "Tha\u00b7ten", "traf\u00b7fen", "auch", "noch", "\u00fc\u00b7ber", "Hof\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er wust\u2019 als ", "tokens": ["Er", "wust'", "als"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Ja durch den Zippel-Peltz guckt schon ein Amptmann rau\u00df/", "tokens": ["Ja", "durch", "den", "Zip\u00b7pel\u00b7Peltz", "guckt", "schon", "ein", "Ampt\u00b7mann", "rau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und B\u00e4rndorff ward vergn\u00fcgt da ", "tokens": ["Und", "B\u00e4rn\u00b7dorff", "ward", "ver\u00b7gn\u00fcgt", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "VVPP", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Sein ehrbegierig Geist blieb nicht beym Ofen sitzen.", "tokens": ["Sein", "ehr\u00b7be\u00b7gie\u00b7rig", "Geist", "blieb", "nicht", "beym", "O\u00b7fen", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Er wuste da\u00df der Weg der Tugend rauh und hart/", "tokens": ["Er", "wus\u00b7te", "da\u00df", "der", "Weg", "der", "Tu\u00b7gend", "rauh", "und", "hart", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df so ein feiges Blut und weiche Z\u00e4rtlings-Art/", "tokens": ["Da\u00df", "so", "ein", "fei\u00b7ges", "Blut", "und", "wei\u00b7che", "Z\u00e4rt\u00b7lings\u00b7Art", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit grossen Diensten nicht den Menschen k\u00f6nte n\u00fctzen.", "tokens": ["Mit", "gros\u00b7sen", "Diens\u00b7ten", "nicht", "den", "Men\u00b7schen", "k\u00f6n\u00b7te", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Er gieng den Adlern gleich in freye Lufft und Land/", "tokens": ["Er", "gieng", "den", "Ad\u00b7lern", "gleich", "in", "frey\u00b7e", "Lufft", "und", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und macht in dem Geb\u00fcrg als Ritter sich bekandt.", "tokens": ["Und", "macht", "in", "dem", "Ge\u00b7b\u00fcrg", "als", "Rit\u00b7ter", "sich", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KOUS", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das Gl\u00fcck erstarrte selbst ob solchen Helden Proben/", "tokens": ["Das", "Gl\u00fcck", "er\u00b7starr\u00b7te", "selbst", "ob", "sol\u00b7chen", "Hel\u00b7den", "Pro\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KOUS", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und dachte ", "tokens": ["Und", "dach\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Es gab ihm Donnerblick/ und \u00e4nderte den Schein.", "tokens": ["Es", "gab", "ihm", "Don\u00b7ner\u00b7blick", "/", "und", "\u00e4n\u00b7der\u00b7te", "den", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$(", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Kurtz/ ", "tokens": ["Kurtz", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.35": {"text": "Als Hunger ihn geplagt/ und heisser Durst gequ\u00e4lt/", "tokens": ["Als", "Hun\u00b7ger", "ihn", "ge\u00b7plagt", "/", "und", "heis\u00b7ser", "Durst", "ge\u00b7qu\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "$(", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ja da ihm auch Taback sein Himmelbrod gefehlt;", "tokens": ["Ja", "da", "ihm", "auch", "Ta\u00b7back", "sein", "Him\u00b7mel\u00b7brod", "ge\u00b7fehlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "ADV", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Bi\u00df endlich Gl\u00fcck und Zeit ihn wiederumb begnadet.", "tokens": ["Bi\u00df", "end\u00b7lich", "Gl\u00fcck", "und", "Zeit", "ihn", "wie\u00b7de\u00b7rumb", "be\u00b7gna\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "KON", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Er stieg wie eine Flamm vom Abgrund in die H\u00f6h;", "tokens": ["Er", "stieg", "wie", "ei\u00b7ne", "Flamm", "vom", "Ab\u00b7grund", "in", "die", "H\u00f6h", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Hat jemand sich verkn\u00fcpfft/ durchs Band der keuschen Eh/", "tokens": ["Hat", "je\u00b7mand", "sich", "ver\u00b7kn\u00fcpfft", "/", "durchs", "Band", "der", "keu\u00b7schen", "Eh", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PRF", "VVPP", "$(", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So spricht man ", "tokens": ["So", "spricht", "man"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.41": {"text": "Und seinem Marschalcks-Ampt ein voll Gen\u00fcgen thut/", "tokens": ["Und", "sei\u00b7nem", "Mar\u00b7schalcks\u00b7Ampt", "ein", "voll", "Ge\u00b7n\u00fc\u00b7gen", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der Stab prangt in der Hand/ die B\u00e4nder auf dem Hut.", "tokens": ["Der", "Stab", "prangt", "in", "der", "Hand", "/", "die", "B\u00e4n\u00b7der", "auf", "dem", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Als Ferdinand der Viert\u2019 ins K\u00e4yserthum getretten/", "tokens": ["Als", "Fer\u00b7di\u00b7nand", "der", "Viert'", "ins", "K\u00e4y\u00b7ser\u00b7thum", "ge\u00b7tret\u00b7ten", "/"], "token_info": ["word", "w