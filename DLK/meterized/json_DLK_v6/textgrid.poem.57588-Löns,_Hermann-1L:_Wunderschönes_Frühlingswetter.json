{"textgrid.poem.57588": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wundersch\u00f6nes Fr\u00fchlingswetter", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wundersch\u00f6nes Fr\u00fchlingswetter", "tokens": ["Wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Fr\u00fch\u00b7lings\u00b7wet\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glitzert durch die bunten Scheiben,", "tokens": ["Glit\u00b7zert", "durch", "die", "bun\u00b7ten", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldne Sonnenst\u00e4ubchen tanzen", "tokens": ["Gold\u00b7ne", "Son\u00b7nen\u00b7st\u00e4ub\u00b7chen", "tan\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig um den Hochaltar.", "tokens": ["Lus\u00b7tig", "um", "den", "Hoc\u00b7hal\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf der Kanzel spricht der Pater", "tokens": ["Auf", "der", "Kan\u00b7zel", "spricht", "der", "Pa\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Donnernd gegen Lust und Unzucht,", "tokens": ["Don\u00b7nernd", "ge\u00b7gen", "Lust", "und", "Un\u00b7zucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem breiten, keuschen Schmerbauch", "tokens": ["Auf", "dem", "brei\u00b7ten", ",", "keu\u00b7schen", "Schmer\u00b7bauch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcpft ein goldner Lichtreflex.", "tokens": ["H\u00fcpft", "ein", "gold\u00b7ner", "Lich\u00b7tre\u00b7flex", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und um seine rote Nase", "tokens": ["Und", "um", "sei\u00b7ne", "ro\u00b7te", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Flattert ein Zitronenfalter,", "tokens": ["Flat\u00b7tert", "ein", "Zit\u00b7ro\u00b7nen\u00b7fal\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Fliegt zu einem sch\u00f6nen M\u00e4dchen,", "tokens": ["Fliegt", "zu", "ei\u00b7nem", "sch\u00f6\u00b7nen", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mit scheuem Herzen horcht.", "tokens": ["Das", "mit", "scheu\u00b7em", "Her\u00b7zen", "horcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nachgefolgt dem gelben Falter", "tokens": ["Nach\u00b7ge\u00b7folgt", "dem", "gel\u00b7ben", "Fal\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind des Paters strenge Augen,", "tokens": ["Sind", "des", "Pa\u00b7ters", "stren\u00b7ge", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treffen schlie\u00dflich auch das liebe,", "tokens": ["Tref\u00b7fen", "schlie\u00df\u00b7lich", "auch", "das", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfe M\u00e4dchenangesicht.", "tokens": ["S\u00fc\u00b7\u00dfe", "M\u00e4d\u00b7chen\u00b7an\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Pl\u00f6tzlich stockt der Pater Thomas,", "tokens": ["Pl\u00f6tz\u00b7lich", "stockt", "der", "Pa\u00b7ter", "Tho\u00b7mas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er, der gro\u00dfe Kanzelredner,", "tokens": ["Er", ",", "der", "gro\u00b7\u00dfe", "Kan\u00b7zel\u00b7red\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er, der gro\u00dfe Reuebringer,", "tokens": ["Er", ",", "der", "gro\u00b7\u00dfe", "Reu\u00b7e\u00b7brin\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird verwirrt, er stockt und schweigt.", "tokens": ["Wird", "ver\u00b7wirrt", ",", "er", "stockt", "und", "schweigt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00dcber einem Strebepfeiler", "tokens": ["\u00dc\u00b7ber", "ei\u00b7nem", "Stre\u00b7be\u00b7pfei\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt ein feister Marmorengel,", "tokens": ["Sitzt", "ein", "feis\u00b7ter", "Mar\u00b7mo\u00b7ren\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser grinst mit kleinen Augen", "tokens": ["Die\u00b7ser", "grinst", "mit", "klei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig Pater Thomas an.", "tokens": ["Lus\u00b7tig", "Pa\u00b7ter", "Tho\u00b7mas", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}