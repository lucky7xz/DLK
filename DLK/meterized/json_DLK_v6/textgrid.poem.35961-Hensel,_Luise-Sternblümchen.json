{"textgrid.poem.35961": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Sternbl\u00fcmchen", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaust du sehnend in die Ferne,", "tokens": ["Schaust", "du", "seh\u00b7nend", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo manch' lichtes Sternlein blinkt?", "tokens": ["Wo", "man\u00b7ch'", "lich\u00b7tes", "Stern\u00b7lein", "blinkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ach, sie bleiben fern, die Sterne,", "tokens": ["Ach", ",", "sie", "blei\u00b7ben", "fern", ",", "die", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob dir mancher freundlich winkt. \u2013", "tokens": ["Ob", "dir", "man\u00b7cher", "freund\u00b7lich", "winkt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kannst die Lichten nicht erlangen", "tokens": ["Kannst", "die", "Lich\u00b7ten", "nicht", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erde h\u00e4lt dich noch zur\u00fcck \u2013", "tokens": ["Er\u00b7de", "h\u00e4lt", "dich", "noch", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des hei\u00dfen Herzens Bangen", "tokens": ["Und", "des", "hei\u00b7\u00dfen", "Her\u00b7zens", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spricht sich aus im feuchten Blick.", "tokens": ["Spricht", "sich", "aus", "im", "feuch\u00b7ten", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch erfreust du dich am Bilde", "tokens": ["Doch", "er\u00b7freust", "du", "dich", "am", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen, was dein Herz begehrt;", "tokens": ["Des\u00b7sen", ",", "was", "dein", "Herz", "be\u00b7gehrt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh', der ew'ge Vater milde", "tokens": ["Sieh'", ",", "der", "ew'\u00b7ge", "Va\u00b7ter", "mil\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat mich dir zum Trost bescheert.", "tokens": ["Hat", "mich", "dir", "zum", "Trost", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}