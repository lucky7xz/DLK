{"textgrid.poem.65655": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Mainottenknabe", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter, meinen Pfeil und Bogen werf' ich vor die F\u00fc\u00dfe dir!", "tokens": ["Mut\u00b7ter", ",", "mei\u00b7nen", "Pfeil", "und", "Bo\u00b7gen", "wer\u00b7f'", "ich", "vor", "die", "F\u00fc\u00b7\u00dfe", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "KON", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Nach den Scheiben und den Puppen noch zu schie\u00dfen, ekelt mir.", "tokens": ["Nach", "den", "Schei\u00b7ben", "und", "den", "Pup\u00b7pen", "noch", "zu", "schie\u00b7\u00dfen", ",", "e\u00b7kelt", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "La\u00df den Vater T\u00fcrkenk\u00f6pfe doch mir schicken aus dem Feld,", "tokens": ["La\u00df", "den", "Va\u00b7ter", "T\u00fcr\u00b7ken\u00b7k\u00f6p\u00b7fe", "doch", "mir", "schi\u00b7cken", "aus", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dann, dann, Mutter, sollst du sehen, da\u00df ich bin ein Sch\u00fctzenheld!", "tokens": ["Dann", ",", "dann", ",", "Mut\u00b7ter", ",", "sollst", "du", "se\u00b7hen", ",", "da\u00df", "ich", "bin", "ein", "Sch\u00fct\u00b7zen\u00b7held", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Hat vielleicht mein edler Vater zu dem Schicken keine Zeit,", "tokens": ["Hat", "viel\u00b7leicht", "mein", "ed\u00b7ler", "Va\u00b7ter", "zu", "dem", "Schi\u00b7cken", "kei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Ei, so geh' ich selbst hinunter, wo er steht im hei\u00dfen Streit,", "tokens": ["Ei", ",", "so", "geh'", "ich", "selbst", "hin\u00b7un\u00b7ter", ",", "wo", "er", "steht", "im", "hei\u00b7\u00dfen", "Streit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Schneide mir mit meinem Messer selber ab den besten Kopf,", "tokens": ["Schnei\u00b7de", "mir", "mit", "mei\u00b7nem", "Mes\u00b7ser", "sel\u00b7ber", "ab", "den", "bes\u00b7ten", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und herauf nach unsern Bergen trag' ich ihn an seinem Schopf.", "tokens": ["Und", "her\u00b7auf", "nach", "un\u00b7sern", "Ber\u00b7gen", "trag'", "ich", "ihn", "an", "sei\u00b7nem", "Schopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Das soll eine Freude werden! Alle Kinder ruf' ich her,", "tokens": ["Das", "soll", "ei\u00b7ne", "Freu\u00b7de", "wer\u00b7den", "!", "Al\u00b7le", "Kin\u00b7der", "ruf'", "ich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VAINF", "$.", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Alle spannen ihren Bogen, alle laden ihr Gewehr.", "tokens": ["Al\u00b7le", "span\u00b7nen", "ih\u00b7ren", "Bo\u00b7gen", ",", "al\u00b7le", "la\u00b7den", "ihr", "Ge\u00b7wehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Wenn ich dann das Ziel nicht treffe, M\u00fctterchen, so sperr' mich ein,", "tokens": ["Wenn", "ich", "dann", "das", "Ziel", "nicht", "tref\u00b7fe", ",", "M\u00fct\u00b7ter\u00b7chen", ",", "so", "sperr'", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.12": {"text": "Und la\u00df lange Weiberr\u00f6cke meine Sonntagskleider sein!", "tokens": ["Und", "la\u00df", "lan\u00b7ge", "Wei\u00b7ber\u00b7r\u00f6\u00b7cke", "mei\u00b7ne", "Sonn\u00b7tags\u00b7klei\u00b7der", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}