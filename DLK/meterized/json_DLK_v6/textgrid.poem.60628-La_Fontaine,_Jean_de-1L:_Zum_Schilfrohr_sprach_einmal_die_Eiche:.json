{"textgrid.poem.60628": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Schilfrohr sprach einmal die Eiche:", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Schilfrohr sprach einmal die Eiche:", "tokens": ["Zum", "Schilf\u00b7rohr", "sprach", "ein\u00b7mal", "die", "Ei\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich finde, da\u00df du Grund zur Klage hast,", "tokens": ["\u00bb", "ich", "fin\u00b7de", ",", "da\u00df", "du", "Grund", "zur", "Kla\u00b7ge", "hast", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein winziger Vogel schon wird dir zur Last,", "tokens": ["Ein", "win\u00b7zi\u00b7ger", "Vo\u00b7gel", "schon", "wird", "dir", "zur", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der schw\u00e4chste Windhauch, der im Teiche", "tokens": ["Der", "schw\u00e4chs\u00b7te", "Wind\u00b7hauch", ",", "der", "im", "Tei\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Wasser kr\u00e4uselt, biegt dir fast", "tokens": ["Das", "Was\u00b7ser", "kr\u00e4u\u00b7selt", ",", "biegt", "dir", "fast"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Kopf zur Erde, w\u00e4hrend ich mein Haupt,", "tokens": ["Den", "Kopf", "zur", "Er\u00b7de", ",", "w\u00e4h\u00b7rend", "ich", "mein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Stolz wie der Kaukasus, nicht nur dem Licht,", "tokens": ["Stolz", "wie", "der", "Kau\u00b7ka\u00b7sus", ",", "nicht", "nur", "dem", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Nein, auch dem Sturm entgegenhebe,", "tokens": ["Nein", ",", "auch", "dem", "Sturm", "ent\u00b7ge\u00b7gen\u00b7he\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und da, wo ich im Zephir schwebe,", "tokens": ["Und", "da", ",", "wo", "ich", "im", "Ze\u00b7phir", "schwe\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ist dir's, als ob der Nordwind schnaubt.", "tokens": ["Ist", "dir's", ",", "als", "ob", "der", "Nord\u00b7wind", "schnaubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "KOKOM", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ach ja, du Armer, st\u00e4ndest du nur nicht", "tokens": ["Ach", "ja", ",", "du", "Ar\u00b7mer", ",", "st\u00e4n\u00b7dest", "du", "nur", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$,", "PPER", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dort so im Freien, sondern mehr im Schutz", "tokens": ["Dort", "so", "im", "Frei\u00b7en", ",", "son\u00b7dern", "mehr", "im", "Schutz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der Bl\u00e4tter, die nach allen Seiten", "tokens": ["Der", "Bl\u00e4t\u00b7ter", ",", "die", "nach", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sich dicht um meine Zweige breiten,", "tokens": ["Sich", "dicht", "um", "mei\u00b7ne", "Zwei\u00b7ge", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So b\u00f6test du dem Sturm wohl Trutz.", "tokens": ["So", "b\u00f6\u00b7test", "du", "dem", "Sturm", "wohl", "Trutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Jedoch es l\u00e4\u00dft sich nicht bestreiten,", "tokens": ["Je\u00b7doch", "es", "l\u00e4\u00dft", "sich", "nicht", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df euer schw\u00e4chliches Geschlecht", "tokens": ["Da\u00df", "eu\u00b7er", "schw\u00e4ch\u00b7li\u00b7ches", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Stets dort zu Hause, wo die Winde jagen,", "tokens": ["Stets", "dort", "zu", "Hau\u00b7se", ",", "wo", "die", "Win\u00b7de", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und darum mu\u00df ich leider sagen,", "tokens": ["Und", "da\u00b7rum", "mu\u00df", "ich", "lei\u00b7der", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Natur behandelt euch gar ungerecht.\u00ab", "tokens": ["Na\u00b7tur", "be\u00b7han\u00b7delt", "euch", "gar", "un\u00b7ge\u00b7recht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "\u00bbaus gutem Herzen,\u00ab sagte drauf das Rohr,", "tokens": ["\u00bb", "aus", "gu\u00b7tem", "Her\u00b7zen", ",", "\u00ab", "sag\u00b7te", "drauf", "das", "Rohr", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "$,", "$(", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "\u00bbentspringt dein Mitgef\u00fchl; indes bevor", "tokens": ["\u00bb", "ent\u00b7springt", "dein", "Mit\u00b7ge\u00b7f\u00fchl", ";", "in\u00b7des", "be\u00b7vor"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Du urteilst, mu\u00dft du auch bedenken,", "tokens": ["Du", "ur\u00b7teilst", ",", "mu\u00dft", "du", "auch", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Da\u00df mich die Winde wenig kr\u00e4nken.", "tokens": ["Da\u00df", "mich", "die", "Win\u00b7de", "we\u00b7nig", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Ich beuge mich und breche nicht; bis jetzt", "tokens": ["Ich", "beu\u00b7ge", "mich", "und", "bre\u00b7che", "nicht", ";", "bis", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PTKNEG", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Hast du dem Sturm dich tapfer widersetzt", "tokens": ["Hast", "du", "dem", "Sturm", "dich", "tap\u00b7fer", "wi\u00b7der\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und kr\u00fcmmtest nicht vor ihm den R\u00fccken,", "tokens": ["Und", "kr\u00fcmm\u00b7test", "nicht", "vor", "ihm", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Doch warte ab, ob bis zuletzt", "tokens": ["Doch", "war\u00b7te", "ab", ",", "ob", "bis", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Steifnackigkeit und Trotz dir gl\u00fccken.\u00ab", "tokens": ["Stei\u00b7fna\u00b7ckig\u00b7keit", "und", "Trotz", "dir", "gl\u00fc\u00b7cken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.30": {"text": "Kaum war's gesagt, da kam in tollem Flug", "tokens": ["Kaum", "wa\u00b7r's", "ge\u00b7sagt", ",", "da", "kam", "in", "tol\u00b7lem", "Flug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Der schlimmste Sohn dahergebraust,", "tokens": ["Der", "schlimms\u00b7te", "Sohn", "da\u00b7her\u00b7ge\u00b7braust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Den je der Nord in seinen Lenden trug.", "tokens": ["Den", "je", "der", "Nord", "in", "sei\u00b7nen", "Len\u00b7den", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Der Baum steht gut, das Schilfrohr schwankt.", "tokens": ["Der", "Baum", "steht", "gut", ",", "das", "Schilf\u00b7rohr", "schwankt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der Wind verdoppelt seine Kraft und zaust", "tokens": ["Der", "Wind", "ver\u00b7dop\u00b7pelt", "sei\u00b7ne", "Kraft", "und", "zaust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Und r\u00fcttelt an der Eiche, bis sie wankt", "tokens": ["Und", "r\u00fct\u00b7telt", "an", "der", "Ei\u00b7che", ",", "bis", "sie", "wankt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Und st\u00fcrzt. \u2013 Da lag sie, deren Haupt den Himmel f\u00fchlte,", "tokens": ["Und", "st\u00fcrzt", ".", "\u2013", "Da", "lag", "sie", ",", "de\u00b7ren", "Haupt", "den", "Him\u00b7mel", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "$,", "PRELAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Doch deren Fu\u00df im Reich des Todes w\u00fchlte.", "tokens": ["Doch", "de\u00b7ren", "Fu\u00df", "im", "Reich", "des", "To\u00b7des", "w\u00fchl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}