{"dta.poem.9697": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Selimenen.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein angesicht ist eine reiche see/", "tokens": ["Dein", "an\u00b7ge\u00b7sicht", "ist", "ei\u00b7ne", "rei\u00b7che", "see", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo purpur-muscheln wachsen.", "tokens": ["Wo", "pur\u00b7pur\u00b7mu\u00b7scheln", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die br\u00fcste sind der Aetna selbst zu nennen:", "tokens": ["Die", "br\u00fcs\u00b7te", "sind", "der", "A\u00b7et\u00b7na", "selbst", "zu", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NE", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Denn au\u00dfen liegt ein wollen-weicher schnee/", "tokens": ["Denn", "au\u00b7\u00dfen", "liegt", "ein", "wol\u00b7len\u00b7wei\u00b7cher", "schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da innerlich viel tausend flammen brennen.", "tokens": ["Da", "in\u00b7ner\u00b7lich", "viel", "tau\u00b7send", "flam\u00b7men", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vor diesem st\u00fcrtzte sich Empedocles hinein.", "tokens": ["Vor", "die\u00b7sem", "st\u00fcrtz\u00b7te", "sich", "Em\u00b7pe\u00b7do\u00b7cles", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PRF", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Solt auch mein schicksal seyn/", "tokens": ["Solt", "auch", "mein", "schick\u00b7sal", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das lebens-garn im feuer abzuk\u00fcrtzen/", "tokens": ["Das", "le\u00b7bens\u00b7garn", "im", "feu\u00b7er", "ab\u00b7zu\u00b7k\u00fcrt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So m\u00f6cht ich mich in diese flammen st\u00fcrtzen.", "tokens": ["So", "m\u00f6cht", "ich", "mich", "in", "die\u00b7se", "flam\u00b7men", "st\u00fcrt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PDAT", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}