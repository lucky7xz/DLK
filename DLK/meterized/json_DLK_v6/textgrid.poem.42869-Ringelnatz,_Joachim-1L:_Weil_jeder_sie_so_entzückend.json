{"textgrid.poem.42869": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weil jeder sie so entz\u00fcckend", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil jeder sie so entz\u00fcckend", "tokens": ["Weil", "je\u00b7der", "sie", "so", "ent\u00b7z\u00fc\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gr\u00fcn und nat\u00fcrlich fand,", "tokens": ["Gr\u00fcn", "und", "na\u00b7t\u00fcr\u00b7lich", "fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ging die gro\u00dfe Mimose", "tokens": ["Ging", "die", "gro\u00b7\u00dfe", "Mi\u00b7mo\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Von Hand zu Hand.", "tokens": ["Von", "Hand", "zu", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und ging und lebte, ward m\u00fcde und schlief,", "tokens": ["Und", "ging", "und", "leb\u00b7te", ",", "ward", "m\u00fc\u00b7de", "und", "schlief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ward herumgereicht.", "tokens": ["Und", "ward", "her\u00b7um\u00b7ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00fcnschte sich vielleicht \u2013 vielleicht! \u2013", "tokens": ["Und", "w\u00fcnschte", "sich", "viel\u00b7leicht", "\u2013", "viel\u00b7leicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$(", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ganz tief,", "tokens": ["Ganz", "tief", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "So unempfindlich zu sein", "tokens": ["So", "un\u00b7emp\u00b7find\u00b7lich", "zu", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wie ein Stein.", "tokens": ["Wie", "ein", "Stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Und wie sie trotzdem wunderbar", "tokens": ["Und", "wie", "sie", "trotz\u00b7dem", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Organisch gr\u00fcn und wissend klar", "tokens": ["Or\u00b7ga\u00b7nisch", "gr\u00fcn", "und", "wis\u00b7send", "klar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gedieh,", "tokens": ["Ge\u00b7dieh", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Umschw\u00e4rmten, liebten, achteten sie", "tokens": ["Um\u00b7schw\u00e4rm\u00b7ten", ",", "lieb\u00b7ten", ",", "ach\u00b7te\u00b7ten", "sie"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die Menschen und die Tiere,", "tokens": ["Die", "Men\u00b7schen", "und", "die", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Merkten aber fast nie,", "tokens": ["Merk\u00b7ten", "a\u00b7ber", "fast", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Da\u00df sie keine Rose,", "tokens": ["Da\u00df", "sie", "kei\u00b7ne", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Da\u00df sie eine gro\u00dfe Mimose war.", "tokens": ["Da\u00df", "sie", "ei\u00b7ne", "gro\u00b7\u00dfe", "Mi\u00b7mo\u00b7se", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}