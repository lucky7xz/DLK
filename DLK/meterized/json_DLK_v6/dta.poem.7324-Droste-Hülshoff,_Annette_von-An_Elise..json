{"dta.poem.7324": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An Elise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.85", "af:0.14"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Du wei\u00dft es lange wohl wie werth du mir,", "tokens": ["Du", "wei\u00dft", "es", "lan\u00b7ge", "wohl", "wie", "werth", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "ADJD", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was sollt' ich es nicht froh und offen tragen", "tokens": ["Was", "sollt'", "ich", "es", "nicht", "froh", "und", "of\u00b7fen", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Lieben, das so frischer Ranken Zier", "tokens": ["Ein", "Lie\u00b7ben", ",", "das", "so", "fri\u00b7scher", "Ran\u00b7ken", "Zier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um meinen kranken Lebensbaum geschlagen?", "tokens": ["Um", "mei\u00b7nen", "kran\u00b7ken", "Le\u00b7bens\u00b7baum", "ge\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und manchen Abend hab' ich nachgedacht,", "tokens": ["Und", "man\u00b7chen", "A\u00b7bend", "hab'", "ich", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In leiser Stunde tr\u00e4umerischem Sinnen,", "tokens": ["In", "lei\u00b7ser", "Stun\u00b7de", "tr\u00e4u\u00b7me\u00b7ri\u00b7schem", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie deinen Morgen, meine nahnde Nacht", "tokens": ["Wie", "dei\u00b7nen", "Mor\u00b7gen", ",", "mei\u00b7ne", "nahn\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das Schicksal lie\u00df aus Einer Urne rinnen.", "tokens": ["Das", "Schick\u00b7sal", "lie\u00df", "aus", "Ei\u00b7ner", "Ur\u00b7ne", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu alt zur Zwillingsschwester, m\u00f6chte ich", "tokens": ["Zu", "alt", "zur", "Zwil\u00b7lings\u00b7schwes\u00b7ter", ",", "m\u00f6ch\u00b7te", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKA", "ADJD", "APPRART", "NN", "$,", "VMFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein T\u00f6chterchen dich nennen, meinen Sprossen,", "tokens": ["Mein", "T\u00f6ch\u00b7ter\u00b7chen", "dich", "nen\u00b7nen", ",", "mei\u00b7nen", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mir ist, als ob mein fliehend Leben sich,", "tokens": ["Mir", "ist", ",", "als", "ob", "mein", "flie\u00b7hend", "Le\u00b7ben", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPOSAT", "ADJD", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein rinnend Blut in deine Brust ergossen.", "tokens": ["Mein", "rin\u00b7nend", "Blut", "in", "dei\u00b7ne", "Brust", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo stammt im Herzen mir ein Opferheerd,", "tokens": ["Wo", "stammt", "im", "Her\u00b7zen", "mir", "ein", "Op\u00b7fer\u00b7heerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df nicht der deine loderte daneben,", "tokens": ["Da\u00df", "nicht", "der", "dei\u00b7ne", "lo\u00b7der\u00b7te", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "PPOSAT", "VVFIN", "PAV", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Von gleichen Landes lieber Luft gen\u00e4hrt,", "tokens": ["Von", "glei\u00b7chen", "Lan\u00b7des", "lie\u00b7ber", "Luft", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von gleicher Freunde frommem Kreis umgeben?", "tokens": ["Von", "glei\u00b7cher", "Freun\u00b7de", "from\u00b7mem", "Kreis", "um\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und heut', am Sankt Elisabethentag,", "tokens": ["Und", "heut'", ",", "am", "Sankt", "E\u00b7lis\u00b7a\u00b7be\u00b7then\u00b7tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vereinend uns mit gleichen Namens Banden,", "tokens": ["Ver\u00b7ei\u00b7nend", "uns", "mit", "glei\u00b7chen", "Na\u00b7mens", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schlug ich bed\u00e4chtig im Kalender nach,", "tokens": ["Schlug", "ich", "be\u00b7d\u00e4ch\u00b7tig", "im", "Ka\u00b7len\u00b7der", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Welch' Heilige am Taufborn uns gestanden;", "tokens": ["Welch'", "Hei\u00b7li\u00b7ge", "am", "Tauf\u00b7born", "uns", "ge\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da fand ich eine k\u00f6nigliche Frau,", "tokens": ["Da", "fand", "ich", "ei\u00b7ne", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die ihre milde Segenshand gebreitet,", "tokens": ["Die", "ih\u00b7re", "mil\u00b7de", "Se\u00b7gens\u00b7hand", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und eine Patriarchin, ernst und grau,", "tokens": ["Und", "ei\u00b7ne", "Pat\u00b7ri\u00b7ar\u00b7chin", ",", "ernst", "und", "grau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur werth um Den, de\u00df Wege sie bereitet.", "tokens": ["Nur", "werth", "um", "Den", ",", "de\u00df", "We\u00b7ge", "sie", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NE", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Fast war es mir, als ob dies Doppelbild", "tokens": ["Fast", "war", "es", "mir", ",", "als", "ob", "dies", "Dop\u00b7pel\u00b7bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOKOM", "KOUS", "PDS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit strengem Mahnen strebe uns zu trennen,", "tokens": ["Mit", "stren\u00b7gem", "Mah\u00b7nen", "stre\u00b7be", "uns", "zu", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als woll' es dir die F\u00fcrstin zart und mild,", "tokens": ["Als", "woll'", "es", "dir", "die", "F\u00fcrs\u00b7tin", "zart", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir nur die ernste H\u00fcterin verg\u00f6nnen;", "tokens": ["Mir", "nur", "die", "erns\u00b7te", "H\u00fc\u00b7te\u00b7rin", "ver\u00b7g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch \u2014 l\u00e4chle nicht \u2014 ich hab' mich abgekehrt,", "tokens": ["Doch", "l\u00e4ch\u00b7le", "nicht", "ich", "hab'", "mich", "ab\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "PTKNEG", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Bin fast versch\u00e4mt zur Seite dir getreten;", "tokens": ["Bin", "fast", "ver\u00b7sch\u00e4mt", "zur", "Sei\u00b7te", "dir", "ge\u00b7tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nun w\u00e4hle, Lieb, und die du dir bescheert,", "tokens": ["Nun", "w\u00e4h\u00b7le", ",", "Lieb", ",", "und", "die", "du", "dir", "be\u00b7scheert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "KON", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zu der will ich als meiner Heilgen beten.", "tokens": ["Zu", "der", "will", "ich", "als", "mei\u00b7ner", "Heil\u00b7gen", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VMFIN", "PPER", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}