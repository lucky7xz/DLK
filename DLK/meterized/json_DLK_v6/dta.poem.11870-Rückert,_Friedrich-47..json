{"dta.poem.11870": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seelen alle sind umher gestellt im Kreise,", "tokens": ["Die", "See\u00b7len", "al\u00b7le", "sind", "um\u00b7her", "ge\u00b7stellt", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VAFIN", "PTKVZ", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dessen Mitte ruht die Gottheit wirkend leise.", "tokens": ["In", "des\u00b7sen", "Mit\u00b7te", "ruht", "die", "Got\u00b7theit", "wir\u00b7kend", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Punkte, die da sind die Seelen, all in Regung,", "tokens": ["Die", "Punk\u00b7te", ",", "die", "da", "sind", "die", "See\u00b7len", ",", "all", "in", "Re\u00b7gung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "ART", "NN", "$,", "PIAT", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind um den Mittelpunkt in ewiger Bewegung.", "tokens": ["Sind", "um", "den", "Mit\u00b7tel\u00b7punkt", "in", "e\u00b7wi\u00b7ger", "Be\u00b7we\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie k\u00f6nnen, wie sie nah sich aneinander schlie\u00dfen,", "tokens": ["Sie", "k\u00f6n\u00b7nen", ",", "wie", "sie", "nah", "sich", "an\u00b7ein\u00b7an\u00b7der", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PPER", "ADJD", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich doch ber\u00fchren nicht, noch ineinander flie\u00dfen.", "tokens": ["Sich", "doch", "be\u00b7r\u00fch\u00b7ren", "nicht", ",", "noch", "in\u00b7ein\u00b7an\u00b7der", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "PTKNEG", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von jedem Punkte ist zur Mitt' hineingef\u00fchrt", "tokens": ["Von", "je\u00b7dem", "Punk\u00b7te", "ist", "zur", "Mitt'", "hin\u00b7ein\u00b7ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Linie, womit an Gott die Seele r\u00fchrt.", "tokens": ["Die", "Li\u00b7nie", ",", "wo\u00b7mit", "an", "Gott", "die", "See\u00b7le", "r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Der umgekehrte Stral, der, wie er ausgegangen", "tokens": ["Der", "um\u00b7ge\u00b7kehr\u00b7te", "Stral", ",", "der", ",", "wie", "er", "aus\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$,", "PWAV", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Mittelpunkt, dahin zur\u00fcck tr\u00e4gt ein Verlangen.", "tokens": ["Vom", "Mit\u00b7tel\u00b7punkt", ",", "da\u00b7hin", "zu\u00b7r\u00fcck", "tr\u00e4gt", "ein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PAV", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Stralen stralen all im Mittelpunkt zusammen,", "tokens": ["Die", "Stra\u00b7len", "stra\u00b7len", "all", "im", "Mit\u00b7tel\u00b7punkt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werden eins in dem, aus dem sie alle stammen.", "tokens": ["Und", "wer\u00b7den", "eins", "in", "dem", ",", "aus", "dem", "sie", "al\u00b7le", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "APPR", "ART", "$,", "APPR", "PRELS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Seelen all in Streit und unter sich entzweit,", "tokens": ["Die", "See\u00b7len", "all", "in", "Streit", "und", "un\u00b7ter", "sich", "ent\u00b7zweit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "APPR", "NN", "KON", "APPR", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Gott nur haben sie Einheit und Einigkeit.", "tokens": ["In", "Gott", "nur", "ha\u00b7ben", "sie", "Ein\u00b7heit", "und", "Ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Nur die Ber\u00fchrung, die sie in der Gottheit finden,", "tokens": ["Nur", "die", "Be\u00b7r\u00fch\u00b7rung", ",", "die", "sie", "in", "der", "Got\u00b7theit", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann die getrennten im Gef\u00fchl der Liebe binden.", "tokens": ["Kann", "die", "ge\u00b7trenn\u00b7ten", "im", "Ge\u00b7f\u00fchl", "der", "Lie\u00b7be", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und welche Seele nicht zur andern Liebe sp\u00fcrt,", "tokens": ["Und", "wel\u00b7che", "See\u00b7le", "nicht", "zur", "an\u00b7dern", "Lie\u00b7be", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PTKNEG", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der fehlt die Linie, die an die Gottheit r\u00fchrt.", "tokens": ["Der", "fehlt", "die", "Li\u00b7nie", ",", "die", "an", "die", "Got\u00b7theit", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}