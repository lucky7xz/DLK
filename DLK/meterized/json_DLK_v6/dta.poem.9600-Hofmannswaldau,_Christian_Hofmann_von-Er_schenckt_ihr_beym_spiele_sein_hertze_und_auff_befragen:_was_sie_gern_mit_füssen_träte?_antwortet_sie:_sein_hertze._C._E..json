{"dta.poem.9600": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er schenckt ihr beym spiele sein hertze/ und  \n auff befragen: was sie gern mit f\u00fcssen tr\u00e4te?  \n antwortet sie: sein hertze.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die stoltze Phillis tritt mein hertze nur mit f\u00fcssen/", "tokens": ["Die", "stolt\u00b7ze", "Phil\u00b7lis", "tritt", "mein", "hert\u00b7ze", "nur", "mit", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wirfft ihm gallen-tranck statt rosen-zucker f\u00fcr;", "tokens": ["Sie", "wirfft", "ihm", "gal\u00b7len\u00b7tranck", "statt", "ro\u00b7sen\u00b7zu\u00b7cker", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "APPR", "NE", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es sucht zwar sonnenschein/ doch find es nacht bey ihr/", "tokens": ["Es", "sucht", "zwar", "son\u00b7nen\u00b7schein", "/", "doch", "find", "es", "nacht", "bey", "ihr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn ihre blicke sind ihm lauter finsterni\u00dfen.", "tokens": ["Denn", "ih\u00b7re", "bli\u00b7cke", "sind", "ihm", "lau\u00b7ter", "fins\u00b7ter\u00b7ni\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein hertze/ das ich doch ihr zum geschencke bot/", "tokens": ["Mein", "hert\u00b7ze", "/", "das", "ich", "doch", "ihr", "zum", "ge\u00b7schen\u00b7cke", "bot", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "ADV", "PPOSAT", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird itzt so freventlich in schimpff und spott gerissen!", "tokens": ["Wird", "itzt", "so", "fre\u00b7vent\u00b7lich", "in", "schimpff", "und", "spott", "ge\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "APPR", "NE", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das sie/ als g\u00f6ttin/ ehrt/ mu\u00df gar zu peinlich b\u00fcssen/", "tokens": ["Das", "sie", "/", "als", "g\u00f6t\u00b7tin", "/", "ehrt", "/", "mu\u00df", "gar", "zu", "pein\u00b7lich", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$(", "KOUS", "NE", "$(", "VVFIN", "$(", "VMFIN", "ADV", "PTKA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wider alle schuld erleiden schmach und todt.", "tokens": ["Und", "wi\u00b7der", "al\u00b7le", "schuld", "er\u00b7lei\u00b7den", "schmach", "und", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADJD", "VVINF", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach! Phillis/ lohnstu so die treue deiner knechte?", "tokens": ["Ach", "!", "Phil\u00b7lis", "/", "lohns\u00b7tu", "so", "die", "treu\u00b7e", "dei\u00b7ner", "knech\u00b7te", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "$(", "VVFIN", "ADV", "ART", "ADJA", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist di\u00df der s\u00fcsse danck/ den deine gunst verspricht?", "tokens": ["Ist", "di\u00df", "der", "s\u00fcs\u00b7se", "danck", "/", "den", "dei\u00b7ne", "gunst", "ver\u00b7spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nein! Phillis/ solcher huld und liebe mag ich nicht/", "tokens": ["Nein", "!", "Phil\u00b7lis", "/", "sol\u00b7cher", "huld", "und", "lie\u00b7be", "mag", "ich", "nicht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NE", "$(", "PIAT", "NN", "KON", "VVFIN", "VMFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und diese grausamkeit laufft wider alle rechte.", "tokens": ["Und", "die\u00b7se", "grau\u00b7sam\u00b7keit", "laufft", "wi\u00b7der", "al\u00b7le", "rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "APPR", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein hertze/ das sich giebt mit allem/ was es hat/", "tokens": ["Ein", "hert\u00b7ze", "/", "das", "sich", "giebt", "mit", "al\u00b7lem", "/", "was", "es", "hat", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "VVFIN", "APPR", "PIS", "$(", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein hertze/ das bereit die gantze welt zu lassen/", "tokens": ["Ein", "hert\u00b7ze", "/", "das", "be\u00b7reit", "die", "gant\u00b7ze", "welt", "zu", "las\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Welch unmensch k\u00f6nte doch ein solches hertze hassen/", "tokens": ["Welch", "un\u00b7mensch", "k\u00f6n\u00b7te", "doch", "ein", "sol\u00b7ches", "hert\u00b7ze", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VMFIN", "ADV", "ART", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und thun/ wie du gethan/ dergleichen frevel-that?", "tokens": ["Und", "thun", "/", "wie", "du", "ge\u00b7than", "/", "derg\u00b7lei\u00b7chen", "fre\u00b7vel\u00b7that", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "PWAV", "PPER", "VVPP", "$(", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Welch l\u00f6we steht so fest auff seinem steiffen willen?", "tokens": ["Welch", "l\u00f6\u00b7we", "steht", "so", "fest", "auff", "sei\u00b7nem", "steif\u00b7fen", "wil\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Welch tieger nimt/ wie du/ so wenig sanfftmuth an?/", "tokens": ["Welch", "tie\u00b7ger", "nimt", "/", "wie", "du", "/", "so", "we\u00b7nig", "sanfft\u00b7muth", "an", "?", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "$(", "PWAV", "PPER", "$(", "ADV", "ADV", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer wehlt doch fleisch und blut zu seiner wollust bahn", "tokens": ["Wer", "wehlt", "doch", "fleisch", "und", "blut", "zu", "sei\u00b7ner", "wol\u00b7lust", "bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "KON", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und sucht da s\u00fcsse lust/ wo nichts denn thr\u00e4nen qvillen?", "tokens": ["Und", "sucht", "da", "s\u00fcs\u00b7se", "lust", "/", "wo", "nichts", "denn", "thr\u00e4\u00b7nen", "qvil\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$(", "PWAV", "PIS", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Au weh! mein hertze schreyt! tritt Phillis nicht zu hart/", "tokens": ["Au", "weh", "!", "mein", "hert\u00b7ze", "schreyt", "!", "tritt", "Phil\u00b7lis", "nicht", "zu", "hart", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "$.", "VVFIN", "NE", "PTKNEG", "PTKA", "ADJD", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Es wird gantz gerne sich nach deinem fu\u00df bequemen/", "tokens": ["Es", "wird", "gantz", "ger\u00b7ne", "sich", "nach", "dei\u00b7nem", "fu\u00df", "be\u00b7que\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PRF", "APPR", "PPOSAT", "PTKVZ", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich wei\u00df/ du wirst ihm ja nicht itzt das leben nehmen/", "tokens": ["Ich", "wei\u00df", "/", "du", "wirst", "ihm", "ja", "nicht", "itzt", "das", "le\u00b7ben", "neh\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "PDS", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das ihm doch ehermahls durch dich erhalten ward.", "tokens": ["Das", "ihm", "doch", "e\u00b7her\u00b7mahls", "durch", "dich", "er\u00b7hal\u00b7ten", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gib Phillis/ gib doch nach/ mein hertze l\u00e4st sich beugen;", "tokens": ["Gib", "Phil\u00b7lis", "/", "gib", "doch", "nach", "/", "mein", "hert\u00b7ze", "l\u00e4st", "sich", "beu\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "$(", "VVIMP", "ADV", "APPR", "$(", "PPOSAT", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du siehst ja/ wie es sich nach deinem willen h\u00e4lt;", "tokens": ["Du", "siehst", "ja", "/", "wie", "es", "sich", "nach", "dei\u00b7nem", "wil\u00b7len", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWAV", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wiewol du h\u00f6rst mich nicht: thu dann/ was dir gef\u00e4llt.", "tokens": ["Wie\u00b7wol", "du", "h\u00f6rst", "mich", "nicht", ":", "thu", "dann", "/", "was", "dir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "ADV", "$(", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.28": {"text": "Tritt zu! mein hertze kan auch bey der marter schweigen.", "tokens": ["Tritt", "zu", "!", "mein", "hert\u00b7ze", "kan", "auch", "bey", "der", "mar\u00b7ter", "schwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PPOSAT", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}