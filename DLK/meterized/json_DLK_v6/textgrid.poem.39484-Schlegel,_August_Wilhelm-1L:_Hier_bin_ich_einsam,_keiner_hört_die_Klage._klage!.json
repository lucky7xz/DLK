{"textgrid.poem.39484": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier bin ich einsam, keiner h\u00f6rt die Klage. klage!", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier bin ich einsam, keiner h\u00f6rt die Klage. klage!", "tokens": ["Hier", "bin", "ich", "ein\u00b7sam", ",", "kei\u00b7ner", "h\u00f6rt", "die", "Kla\u00b7ge", ".", "kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PIS", "VVFIN", "ART", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Niemand vertrau' ich mein verzagtes St\u00f6hnen. T\u00f6nen.", "tokens": ["Nie\u00b7mand", "ver\u00b7trau'", "ich", "mein", "ver\u00b7zag\u00b7tes", "St\u00f6h\u00b7nen", ".", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll ich stets ungeliebt der Spr\u00f6den fr\u00f6hnen? h\u00f6hnen.", "tokens": ["Soll", "ich", "stets", "un\u00b7ge\u00b7liebt", "der", "Spr\u00f6\u00b7den", "fr\u00f6h\u00b7nen", "?", "h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "ART", "NN", "VVINF", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie lang harr' ich umsonst, da\u00df es mir tage? Tage.", "tokens": ["Wie", "lang", "harr'", "ich", "um\u00b7sonst", ",", "da\u00df", "es", "mir", "ta\u00b7ge", "?", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mich findet Gunst zu leicht auf ihrer Wage. wage!", "tokens": ["Mich", "fin\u00b7det", "Gunst", "zu", "leicht", "auf", "ih\u00b7rer", "Wa\u00b7ge", ".", "wa\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wem liegt wohl dran, mein Leben zu versch\u00f6nen? Sch\u00f6nen.", "tokens": ["Wem", "liegt", "wohl", "dran", ",", "mein", "Le\u00b7ben", "zu", "ver\u00b7sch\u00f6\u00b7nen", "?", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PAV", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird das holde Gl\u00fcck mich endlich kr\u00f6nen? kr\u00f6nen.", "tokens": ["So", "wird", "das", "hol\u00b7de", "Gl\u00fcck", "mich", "end\u00b7lich", "kr\u00f6\u00b7nen", "?", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "VVINF", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer giebt mir frohe Kund' auf jede Frage? frage!", "tokens": ["Wer", "giebt", "mir", "fro\u00b7he", "Kund'", "auf", "je\u00b7de", "Fra\u00b7ge", "?", "fra\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN", "APPR", "PIAT", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was ist dein Thun dort in den Felsenhallen? hallen.", "tokens": ["Was", "ist", "dein", "Thun", "dort", "in", "den", "Fel\u00b7sen\u00b7hal\u00b7len", "?", "hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was ist Schuld, da\u00df du nur Laut geblieben? lieben.", "tokens": ["Und", "was", "ist", "Schuld", ",", "da\u00df", "du", "nur", "Laut", "ge\u00b7blie\u00b7ben", "?", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "VVPP", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So f\u00fchlst du etwas bei Verliebter Schmerzen? Schmerzen.", "tokens": ["So", "f\u00fchlst", "du", "et\u00b7was", "bei", "Ver\u00b7lieb\u00b7ter", "Schmer\u00b7zen", "?", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Glaubst du, dein Spiel k\u00f6nn' irgend wem gefallen? allen.", "tokens": ["Glaubst", "du", ",", "dein", "Spiel", "k\u00f6nn'", "ir\u00b7gend", "wem", "ge\u00b7fal\u00b7len", "?", "al\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "VMFIN", "ADV", "PWS", "VVPP", "$.", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wem wird es denn zu lieb mit uns getrieben? Trieben.", "tokens": ["Wem", "wird", "es", "denn", "zu", "lieb", "mit", "uns", "ge\u00b7trie\u00b7ben", "?", "Trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "APPR", "PPER", "VVPP", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer sehnt sich leeren Wiederhall zu herzen? Herzen.", "tokens": ["Wer", "sehnt", "sich", "lee\u00b7ren", "Wie\u00b7der\u00b7hall", "zu", "her\u00b7zen", "?", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADJA", "NN", "PTKZU", "VVINF", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}