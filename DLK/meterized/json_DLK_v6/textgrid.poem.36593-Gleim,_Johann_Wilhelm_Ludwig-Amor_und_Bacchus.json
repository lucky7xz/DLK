{"textgrid.poem.36593": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Amor und Bacchus", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bacchus streitet sich mit Amor!", "tokens": ["Bac\u00b7chus", "strei\u00b7tet", "sich", "mit", "A\u00b7mor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob es Ernst ist oder Scherz?", "tokens": ["Ob", "es", "Ernst", "ist", "o\u00b7der", "Scherz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VAFIN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ernst mu\u00df es wohl sein, sie streiten", "tokens": ["Ernst", "mu\u00df", "es", "wohl", "sein", ",", "sie", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VAINF", "$,", "PPER", "VVFIN"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sich ja um mein Herz!", "tokens": ["Sich", "ja", "um", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Bacchus mag den Sieg gewinnen!", "tokens": ["Bac\u00b7chus", "mag", "den", "Sieg", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn zu geben, steht bei mir!", "tokens": ["Ihn", "zu", "ge\u00b7ben", ",", "steht", "bei", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nein, vertragt euch lieber,", "tokens": ["A\u00b7ber", "nein", ",", "ver\u00b7tragt", "euch", "lie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O ihr G\u00f6tter! ihr!", "tokens": ["O", "ihr", "G\u00f6t\u00b7ter", "!", "ihr", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gern lieb' ich euch alle beide,", "tokens": ["Gern", "lieb'", "ich", "euch", "al\u00b7le", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIAT", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle beide k\u00f6nnt ihr mich", "tokens": ["Al\u00b7le", "bei\u00b7de", "k\u00f6nnt", "ihr", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PIS", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fccklich machen, o vertraget", "tokens": ["Gl\u00fcck\u00b7lich", "ma\u00b7chen", ",", "o", "ver\u00b7tra\u00b7get"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVINF", "$,", "FM", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch doch nur, bitt' ich!", "tokens": ["Euch", "doch", "nur", ",", "bitt'", "ich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00dft mich trinken, la\u00dft mich lieben,", "tokens": ["La\u00dft", "mich", "trin\u00b7ken", ",", "la\u00dft", "mich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beides la\u00dft mich doch zugleich,", "tokens": ["Bei\u00b7des", "la\u00dft", "mich", "doch", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O ihr allerliebsten G\u00f6tter!", "tokens": ["O", "ihr", "al\u00b7ler\u00b7liebs\u00b7ten", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O vertraget euch!", "tokens": ["O", "ver\u00b7tra\u00b7get", "euch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Euch zu Ehren mich berauschen", "tokens": ["Euch", "zu", "Eh\u00b7ren", "mich", "be\u00b7rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll die Liebe, soll der Wein!", "tokens": ["Soll", "die", "Lie\u00b7be", ",", "soll", "der", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4chelnd schenkt mir unter K\u00fcssen", "tokens": ["L\u00e4\u00b7chelnd", "schenkt", "mir", "un\u00b7ter", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Doris ein.", "tokens": ["Mei\u00b7ne", "Do\u00b7ris", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}