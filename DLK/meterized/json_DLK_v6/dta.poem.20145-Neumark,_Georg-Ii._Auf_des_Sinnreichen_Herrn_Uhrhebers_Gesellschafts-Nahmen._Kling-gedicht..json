{"dta.poem.20145": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n Auf des Sinnreichen Herrn Uhrhebers  \n Gesellschafts-Nahmen.  \n Kling-gedicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Die Teutsche Heldenzunft/ benennet ihn von\nSprossen/", "tokens": ["Die", "Teut\u00b7sche", "Hel\u00b7den\u00b7zunft", "/", "be\u00b7nen\u00b7net", "ihn", "von", "Spros\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herr Neumark/ und gab ihm zum Zeichen", "tokens": ["Herr", "Neu\u00b7mark", "/", "und", "gab", "ihm", "zum", "Zei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$(", "KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Von braunen N\u00e4gelein/ dar\u00fcm ein Daff-", "tokens": ["Von", "brau\u00b7nen", "N\u00e4\u00b7ge\u00b7lein", "/", "da\u00b7r\u00fcm", "ein", "Daff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "PAV", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Jungfern Sinnebild/ zum sch\u00f6nsten war ge-", "tokens": ["Der", "Jung\u00b7fern", "Sin\u00b7ne\u00b7bild", "/", "zum", "sch\u00f6ns\u00b7ten", "war", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "APPRART", "ADJA", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weil aus des Himmels Gunst ihm gleichsam zu-", "tokens": ["Weil", "aus", "des", "Him\u00b7mels", "Gunst", "ihm", "gleich\u00b7sam", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PPER", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Himmels-werthe Kunst/ die nichts vom", "tokens": ["Die", "Him\u00b7mels\u00b7wert\u00b7he", "Kunst", "/", "die", "nichts", "vom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PIS", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Flei\u00df stieg hoch empor/ und sprosset", "tokens": ["Sein", "Flei\u00df", "stieg", "hoch", "em\u00b7por", "/", "und", "spros\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Nutz-bereichter Frucht/ so Deutschland hat", "tokens": ["Mit", "Nutz\u00b7be\u00b7reich\u00b7ter", "Frucht", "/", "so", "Deutschland", "hat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "ADV", "NE", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Fr\u00fchling ", "tokens": ["Der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "In seiner Jugend Flor/ da s\u00fc\u00dfe Lieblichkeit", "tokens": ["In", "sei\u00b7ner", "Ju\u00b7gend", "Flor", "/", "da", "s\u00fc\u00b7\u00dfe", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit Kunst den Wettstreit f\u00fchrt\u2019 in keuschen Lust-", "tokens": ["Mit", "Kunst", "den", "Wett\u00b7streit", "f\u00fchrt'", "in", "keu\u00b7schen", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Sommer ", "tokens": ["Der", "Som\u00b7mer"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Nunmehr wil sich sein Herbst ", "tokens": ["Nun\u00b7mehr", "wil", "sich", "sein", "Herbst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "PPOSAT", "NN"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Die kluge Welt wird ihn und seine Sprossen", "tokens": ["Die", "klu\u00b7ge", "Welt", "wird", "ihn", "und", "sei\u00b7ne", "Spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}