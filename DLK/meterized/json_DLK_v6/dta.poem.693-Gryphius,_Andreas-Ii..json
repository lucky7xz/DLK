{"dta.poem.693": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der schnellen tage Traum: ", "tokens": ["Der", "schnel\u00b7len", "ta\u00b7ge", "Traum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Rennt mit vn\u00df nach der schwartzen Bar: ", "tokens": ["Rennt", "mit", "vn\u00df", "nach", "der", "schwart\u00b7zen", "Bar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eh' ich die zeit erkent ", "tokens": ["Eh'", "ich", "die", "zeit", "er\u00b7kent"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wird meine zeit vollendt. ", "tokens": ["Wird", "mei\u00b7ne", "zeit", "vol\u00b7lendt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "W", "tokens": ["W"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "(die ", "tokens": ["(", "die"], "token_info": ["punct", "word"], "pos": ["$(", "ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Nach der erbla\u00dften V\u00f6lcker schar! ", "tokens": ["Nach", "der", "er\u00b7bla\u00df\u00b7ten", "V\u00f6l\u00b7cker", "schar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W", "tokens": ["W"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Vnd stets voll herber noth. ", "tokens": ["Vnd", "stets", "voll", "her\u00b7ber", "noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit Thr\u00e4nen gr\u00fcssen wir! ", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "gr\u00fcs\u00b7sen", "wir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In thr\u00e4nen lebt man hier: ", "tokens": ["In", "thr\u00e4\u00b7nen", "lebt", "man", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit thr\u00e4nen gibt man gutte nacht! ", "tokens": ["Mit", "thr\u00e4\u00b7nen", "gibt", "man", "gut\u00b7te", "nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W", "tokens": ["W"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ein herber thr\u00e4nen Thal! ", "tokens": ["Ein", "her\u00b7ber", "thr\u00e4\u00b7nen", "Thal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie Rosen die wir zihn. ", "tokens": ["Wie", "Ro\u00b7sen", "die", "wir", "zihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auff ", "tokens": ["Auff"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "In kummer vntergehn. ", "tokens": ["In", "kum\u00b7mer", "vn\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}