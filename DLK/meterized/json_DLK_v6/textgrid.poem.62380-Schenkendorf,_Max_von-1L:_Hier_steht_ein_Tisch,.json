{"textgrid.poem.62380": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier steht ein Tisch,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier steht ein Tisch,", "tokens": ["Hier", "steht", "ein", "Tisch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und stark und frisch", "tokens": ["Und", "stark", "und", "frisch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein gutes Wort zu schreiben,", "tokens": ["Ein", "gu\u00b7tes", "Wort", "zu", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch andres Werk zu treiben.", "tokens": ["Auch", "and\u00b7res", "Werk", "zu", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein jeder Tisch soll heilig sein,", "tokens": ["Ein", "je\u00b7der", "Tisch", "soll", "hei\u00b7lig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um welchen gute Menschen treten,", "tokens": ["Um", "wel\u00b7chen", "gu\u00b7te", "Men\u00b7schen", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PWAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sei's, ihres Daseins sich zu freun,", "tokens": ["Sei's", ",", "ih\u00b7res", "Da\u00b7seins", "sich", "zu", "freun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sei's, um ein Gratias zu beten.", "tokens": ["Sei's", ",", "um", "ein", "Gra\u00b7ti\u00b7as", "zu", "be\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zur Werkstatt geht mit Lust ein Meister,", "tokens": ["Zur", "Werk\u00b7statt", "geht", "mit", "Lust", "ein", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Lehrstuhl wie zu anderm Werke,", "tokens": ["Zum", "Lehr\u00b7stuhl", "wie", "zu", "an\u00b7derm", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn \u00fcberall sind gute Geister,", "tokens": ["Denn", "\u00fc\u00b7be\u00b7rall", "sind", "gu\u00b7te", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und herrschen Weisheit, Sch\u00f6nheit, St\u00e4rke.", "tokens": ["Und", "herr\u00b7schen", "Weis\u00b7heit", ",", "Sch\u00f6n\u00b7heit", ",", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}