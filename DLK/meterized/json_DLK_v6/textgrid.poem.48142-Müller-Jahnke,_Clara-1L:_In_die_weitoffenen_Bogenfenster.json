{"textgrid.poem.48142": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: In die weitoffenen Bogenfenster", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In die weitoffenen Bogenfenster", "tokens": ["In", "die", "weit\u00b7of\u00b7fe\u00b7nen", "Bo\u00b7gen\u00b7fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "des Konsulats zu Napoli", "tokens": ["des", "Kon\u00b7su\u00b7lats", "zu", "Na\u00b7po\u00b7li"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sang der Golf.", "tokens": ["sang", "der", "Golf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Maisonnenbrand", "tokens": ["Mai\u00b7son\u00b7nen\u00b7brand"], "token_info": ["word"], "pos": ["NE"], "meter": "++-+", "measure": "iambic.di"}, "line.5": {"text": "gl\u00fchte dr\u00fcben", "tokens": ["gl\u00fch\u00b7te", "dr\u00fc\u00b7ben"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "auf den D\u00e4chern der Stra\u00dfe", "tokens": ["auf", "den", "D\u00e4\u00b7chern", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "und tauchte in flackernden Goldschein", "tokens": ["und", "tauch\u00b7te", "in", "fla\u00b7ckern\u00b7den", "Gold\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "die Silberspitzen des Olivenbaums.", "tokens": ["die", "Sil\u00b7ber\u00b7spit\u00b7zen", "des", "O\u00b7li\u00b7ven\u00b7baums", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Am fernsten verblauenden Horizont", "tokens": ["Am", "ferns\u00b7ten", "ver\u00b7blau\u00b7en\u00b7den", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "hingestreckt", "tokens": ["hin\u00b7ge\u00b7streckt"], "token_info": ["word"], "pos": ["VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "lag d\u00e4mmernd in tr\u00e4ger Ruhe", "tokens": ["lag", "d\u00e4m\u00b7mernd", "in", "tr\u00e4\u00b7ger", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Sphinx Capri \u2013", "tokens": ["Sphinx", "Cap\u00b7ri", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Mir gegen\u00fcber", "tokens": ["Mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word"], "pos": ["NE", "APPO"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "an dem eichget\u00e4felten", "tokens": ["an", "dem", "eich\u00b7ge\u00b7t\u00e4\u00b7fel\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "b\u00fccherbedeckten Bureautisch", "tokens": ["b\u00fc\u00b7cher\u00b7be\u00b7deck\u00b7ten", "Bu\u00b7re\u00b7au\u00b7tisch"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "stand ein junger, blonder, deutscher Beamter", "tokens": ["stand", "ein", "jun\u00b7ger", ",", "blon\u00b7der", ",", "deut\u00b7scher", "Be\u00b7am\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "und schrieb.", "tokens": ["und", "schrieb", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Schrieb meinen Namen", "tokens": ["Schrieb", "mei\u00b7nen", "Na\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "in sein Buch,", "tokens": ["in", "sein", "Buch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "meinen Wohn- und Geburtsort", "tokens": ["mei\u00b7nen", "Wohn", "und", "Ge\u00b7burts\u00b7ort"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "TRUNC", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.21": {"text": "und den Tag,", "tokens": ["und", "den", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "an dem ich das Licht erblickt, \u2013", "tokens": ["an", "dem", "ich", "das", "Licht", "er\u00b7blickt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVPP", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "und wie meine Eltern gehei\u00dfen,", "tokens": ["und", "wie", "mei\u00b7ne", "El\u00b7tern", "ge\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "wo sie geboren und gestorben,", "tokens": ["wo", "sie", "ge\u00b7bo\u00b7ren", "und", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "und meine Gro\u00dfeltern und Urgro\u00dfeltern,", "tokens": ["und", "mei\u00b7ne", "Gro\u00df\u00b7el\u00b7tern", "und", "Ur\u00b7gro\u00b7\u00df\u00b7el\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "und erforschte meine ganze Genealogie", "tokens": ["und", "er\u00b7forschte", "mei\u00b7ne", "gan\u00b7ze", "Ge\u00b7nea\u00b7lo\u00b7gie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "bis ins dritte und vierte Glied", "tokens": ["bis", "ins", "drit\u00b7te", "und", "vier\u00b7te", "Glied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "KON", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "r\u00fcckw\u00e4rts hinauf.", "tokens": ["r\u00fcck\u00b7w\u00e4rts", "hin\u00b7auf", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.29": {"text": "Hinter ihm an der Wand,", "tokens": ["Hin\u00b7ter", "ihm", "an", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.30": {"text": "\u2013 mit schwarzen Flecken bes\u00e4t", "tokens": ["\u2013", "mit", "schwar\u00b7zen", "Fle\u00b7cken", "be\u00b7s\u00e4t"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "wie ein Typhussterbender \u2013", "tokens": ["wie", "ein", "Ty\u00b7phus\u00b7ster\u00b7ben\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$("], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.32": {"text": "hing die Malariakarte;", "tokens": ["hing", "die", "Ma\u00b7la\u00b7ri\u00b7a\u00b7kar\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.33": {"text": "und darunter", "tokens": ["und", "da\u00b7run\u00b7ter"], "token_info": ["word", "word"], "pos": ["KON", "PAV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.34": {"text": "auf lederbeschlagenem Lehnstuhl,", "tokens": ["auf", "le\u00b7der\u00b7be\u00b7schla\u00b7ge\u00b7nem", "Lehn\u00b7stuhl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.35": {"text": "schlief", "tokens": ["schlief"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.36": {"text": "die Norddeutsche Allgemeine Zeitung.", "tokens": ["die", "Nord\u00b7deut\u00b7sche", "All\u00b7ge\u00b7mei\u00b7ne", "Zei\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Neben mir aber", "tokens": ["Ne\u00b7ben", "mir", "a\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.38": {"text": "auf leichtem italischem Rohrgeflecht,", "tokens": ["auf", "leich\u00b7tem", "i\u00b7ta\u00b7li\u00b7schem", "Rohr\u00b7ge\u00b7flecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.39": {"text": "den Caprihut auf graumeliertem Kraushaar,", "tokens": ["den", "Cap\u00b7ri\u00b7hut", "auf", "grau\u00b7me\u00b7lier\u00b7tem", "Kraus\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.40": {"text": "in den Augen die blaue Tiefe der Ostsee,", "tokens": ["in", "den", "Au\u00b7gen", "die", "blau\u00b7e", "Tie\u00b7fe", "der", "Ost\u00b7see", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "mein Lebensgl\u00fcck . . .", "tokens": ["mein", "Le\u00b7bens\u00b7gl\u00fcck", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.42": {"text": "und der junge, blonde, deutsche Beamte", "tokens": ["und", "der", "jun\u00b7ge", ",", "blon\u00b7de", ",", "deut\u00b7sche", "Be\u00b7am\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.43": {"text": "fragte auch ihn,", "tokens": ["frag\u00b7te", "auch", "ihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.44": {"text": "wie er hei\u00dfe,", "tokens": ["wie", "er", "hei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.45": {"text": "wo er geboren", "tokens": ["wo", "er", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.46": {"text": "wes Ranges und Standes er sei \u2013", "tokens": ["wes", "Ran\u00b7ges", "und", "Stan\u00b7des", "er", "sei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PPER", "VAFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.47": {"text": "und seine Eltern gewesen seien \u2013", "tokens": ["und", "sei\u00b7ne", "El\u00b7tern", "ge\u00b7we\u00b7sen", "sei\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAPP", "VAFIN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "und er erforschte auch seine Genealogie", "tokens": ["und", "er", "er\u00b7forschte", "auch", "sei\u00b7ne", "Ge\u00b7nea\u00b7lo\u00b7gie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.49": {"text": "bis ins dritte und vierte Glied r\u00fcckw\u00e4rts hinauf.", "tokens": ["bis", "ins", "drit\u00b7te", "und", "vier\u00b7te", "Glied", "r\u00fcck\u00b7w\u00e4rts", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "KON", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.50": {"text": "Wir aber sa\u00dfen", "tokens": ["Wir", "a\u00b7ber", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.51": {"text": "dicht beieinander", "tokens": ["dicht", "bei\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJD", "PTKVZ"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.52": {"text": "und sahen uns an,", "tokens": ["und", "sa\u00b7hen", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.53": {"text": "l\u00e4chelnd und stumm.", "tokens": ["l\u00e4\u00b7chelnd", "und", "stumm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.54": {"text": "Bis die T\u00fcr sich auftat", "tokens": ["Bis", "die", "T\u00fcr", "sich", "auf\u00b7tat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.55": {"text": "und ein hochgewachsener Mann", "tokens": ["und", "ein", "hoch\u00b7ge\u00b7wach\u00b7se\u00b7ner", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.56": {"text": "im hellgrauen Sommerjacket,", "tokens": ["im", "hell\u00b7grau\u00b7en", "Som\u00b7mer\u00b7ja\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.57": {"text": "die rote Nelke im Knopfloch,", "tokens": ["die", "ro\u00b7te", "Nel\u00b7ke", "im", "Kno\u00b7pfloch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.58": {"text": "auf der Schwelle erschien \u2013", "tokens": ["auf", "der", "Schwel\u00b7le", "er\u00b7schien", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.59": {"text": "und der Herr Generalkonsul", "tokens": ["und", "der", "Herr", "Ge\u00b7ne\u00b7ral\u00b7kon\u00b7sul"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.60": {"text": "mit t\u00f6nender Stimme", "tokens": ["mit", "t\u00f6\u00b7nen\u00b7der", "Stim\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.61": {"text": "uns das Protokoll vorlas", "tokens": ["uns", "das", "Pro\u00b7to\u00b7koll", "vor\u00b7las"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "und fragte,", "tokens": ["und", "frag\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.63": {"text": "ob wir Mann und Frau sein wollten.", "tokens": ["ob", "wir", "Mann", "und", "Frau", "sein", "woll\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Wir sagten \u00bbJa\u00ab.", "tokens": ["Wir", "sag\u00b7ten", "\u00bb", "Ja", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$(", "PTKANT", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.65": {"text": "Da brach ein mittagliches Flammenmeer", "tokens": ["Da", "brach", "ein", "mit\u00b7tag\u00b7li\u00b7ches", "Flam\u00b7men\u00b7meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.66": {"text": "durch die Bogenfenster des Konsulats", "tokens": ["durch", "die", "Bo\u00b7gen\u00b7fens\u00b7ter", "des", "Kon\u00b