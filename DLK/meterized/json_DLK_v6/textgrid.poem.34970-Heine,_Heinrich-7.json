{"textgrid.poem.34970": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Sch\u00f6ppenstuhle der Vernunft", "tokens": ["Vom", "Sch\u00f6p\u00b7pen\u00b7stuh\u00b7le", "der", "Ver\u00b7nunft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist du vollst\u00e4ndig freigesprochen;", "tokens": ["Bist", "du", "voll\u00b7st\u00e4n\u00b7dig", "frei\u00b7ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Urteil sagt: \u00bbDie Kleine hat", "tokens": ["Das", "Ur\u00b7teil", "sagt", ":", "\u00bb", "Die", "Klei\u00b7ne", "hat"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ART", "ADJA", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Tun und Reden nichts verbrochen.\u00ab", "tokens": ["Durch", "Tun", "und", "Re\u00b7den", "nichts", "ver\u00b7bro\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, stumm und tatlos standest du,", "tokens": ["Ja", ",", "stumm", "und", "tat\u00b7los", "stan\u00b7dest", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als mich verzehrten tolle Flammen \u2013", "tokens": ["Als", "mich", "ver\u00b7zehr\u00b7ten", "tol\u00b7le", "Flam\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du sch\u00fcrtest nicht, du sprachst kein Wort,", "tokens": ["Du", "sch\u00fcr\u00b7test", "nicht", ",", "du", "sprachst", "kein", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch mu\u00df dich mein Herz verdammen.", "tokens": ["Und", "doch", "mu\u00df", "dich", "mein", "Herz", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In meinen Tr\u00e4umen jede Nacht", "tokens": ["In", "mei\u00b7nen", "Tr\u00e4u\u00b7men", "je\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Klagt eine Stimme, die bezichtet", "tokens": ["Klagt", "ei\u00b7ne", "Stim\u00b7me", ",", "die", "be\u00b7zich\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des b\u00f6sen Willens dich und sagt,", "tokens": ["Des", "b\u00f6\u00b7sen", "Wil\u00b7lens", "dich", "und", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du habest mich zugrund' gerichtet.", "tokens": ["Du", "ha\u00b7best", "mich", "zu\u00b7grund'", "ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie bringt Beweis und Zeugnis bei,", "tokens": ["Sie", "bringt", "Be\u00b7weis", "und", "Zeug\u00b7nis", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schleppt ein B\u00fcndel von Urkunden;", "tokens": ["Sie", "schleppt", "ein", "B\u00fcn\u00b7del", "von", "Ur\u00b7kun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch am Morgen, mit dem Traum,", "tokens": ["Je\u00b7doch", "am", "Mor\u00b7gen", ",", "mit", "dem", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist auch die Kl\u00e4gerin verschwunden.", "tokens": ["Ist", "auch", "die", "Kl\u00e4\u00b7ge\u00b7rin", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie hat in meines Herzens Grund", "tokens": ["Sie", "hat", "in", "mei\u00b7nes", "Her\u00b7zens", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihren Akten sich gefl\u00fcchtet \u2013", "tokens": ["Mit", "ih\u00b7ren", "Ak\u00b7ten", "sich", "ge\u00b7fl\u00fcch\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur eins bleibt im Ged\u00e4chtnis mir,", "tokens": ["Nur", "eins", "bleibt", "im", "Ge\u00b7d\u00e4cht\u00b7nis", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist: ich bin zugrund' gerichtet.", "tokens": ["Das", "ist", ":", "ich", "bin", "zu\u00b7grund'", "ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}