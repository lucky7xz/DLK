{"textgrid.poem.62375": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Weihnachtsabend", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, trautes D\u00e4mmerlicht!", "tokens": ["Will\u00b7kom\u00b7men", ",", "trau\u00b7tes", "D\u00e4m\u00b7mer\u00b7licht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Willkommen, Mondenschein!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Mon\u00b7den\u00b7schein", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr bleibt getreu, verla\u00dft mich nicht,", "tokens": ["Ihr", "bleibt", "ge\u00b7treu", ",", "ver\u00b7la\u00dft", "mich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst bin ich ganz allein.", "tokens": ["Sonst", "bin", "ich", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbwie magst du klagen undankbar,", "tokens": ["\u00bb", "wie", "magst", "du", "kla\u00b7gen", "un\u00b7dank\u00b7bar", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und merkst nicht was geschieht,", "tokens": ["Und", "merkst", "nicht", "was", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PWS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gr\u00fc\u00dfest nicht das Friedensjahr,", "tokens": ["Und", "gr\u00fc\u00b7\u00dfest", "nicht", "das", "Frie\u00b7dens\u00b7jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das heute frisch erbl\u00fcht?", "tokens": ["Das", "heu\u00b7te", "frisch", "er\u00b7bl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist ja frohe Weihnachtszeit,", "tokens": ["Es", "ist", "ja", "fro\u00b7he", "Weih\u00b7nachts\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Engleins- und Kindleins Lust;", "tokens": ["Eng\u00b7leins", "und", "Kin\u00b7dleins", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verbanne Streit und Herzeleid", "tokens": ["Ver\u00b7ban\u00b7ne", "Streit", "und", "Her\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur schnell aus deiner Brust.\u00ab", "tokens": ["Nur", "schnell", "aus", "dei\u00b7ner", "Brust", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das ist es ja, das ist es ja,", "tokens": ["Das", "ist", "es", "ja", ",", "das", "ist", "es", "ja", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$,", "PDS", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das einzig, was mich qu\u00e4lt;", "tokens": ["Das", "ein\u00b7zig", ",", "was", "mich", "qu\u00e4lt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wol denk ich, was vordem geschah", "tokens": ["Wol", "denk", "ich", ",", "was", "vor\u00b7dem", "ge\u00b7schah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ADV", "VVFIN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was mir heute fehlt.", "tokens": ["Und", "was", "mir", "heu\u00b7te", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht mag ich zu dem hellen Stern,", "tokens": ["Nicht", "mag", "ich", "zu", "dem", "hel\u00b7len", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht auf zum Himmel schau'n,", "tokens": ["Nicht", "auf", "zum", "Him\u00b7mel", "schau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ziehet mich in weite Fern'", "tokens": ["Es", "zie\u00b7het", "mich", "in", "wei\u00b7te", "Fern'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wol fort nach andern Au'n.", "tokens": ["Wol", "fort", "nach", "an\u00b7dern", "Au'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "PTKVZ", "APPR", "ADJA", "NN", "NE"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Zu meinem Hof, zu meinem Haus,", "tokens": ["Zu", "mei\u00b7nem", "Hof", ",", "zu", "mei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ihr, der keine gleicht,", "tokens": ["Zu", "ihr", ",", "der", "kei\u00b7ne", "gleicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Gabe mir und Blumenstrau\u00df", "tokens": ["Die", "Ga\u00b7be", "mir", "und", "Blu\u00b7men\u00b7strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Feste sonst gereicht.", "tokens": ["Zum", "Fes\u00b7te", "sonst", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Hausfrau, sch\u00f6n und fromm und mild,", "tokens": ["O", "Haus\u00b7frau", ",", "sch\u00f6n", "und", "fromm", "und", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die jede Tugend schm\u00fcckt,", "tokens": ["Die", "je\u00b7de", "Tu\u00b7gend", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und du, mein Muttergottesbild,", "tokens": ["Und", "du", ",", "mein", "Mut\u00b7ter\u00b7got\u00b7tes\u00b7bild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dem sie sinnend blickt,", "tokens": ["Nach", "dem", "sie", "sin\u00b7nend", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und du, viel s\u00fc\u00dfes, liebes Kind,", "tokens": ["Und", "du", ",", "viel", "s\u00fc\u00b7\u00dfes", ",", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das uns der Herr geschenkt,", "tokens": ["Das", "uns", "der", "Herr", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das, wie die Mutter still gesinnt,", "tokens": ["Das", ",", "wie", "die", "Mut\u00b7ter", "still", "ge\u00b7sinnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des fernen Wand'rers denkt.", "tokens": ["Des", "fer\u00b7nen", "Wan\u00b7d'\u00b7rers", "denkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Ich gr\u00fc\u00df euch, ihr geliebten Drei,", "tokens": ["Ich", "gr\u00fc\u00df", "euch", ",", "ihr", "ge\u00b7lieb\u00b7ten", "Drei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich gr\u00fc\u00df ich, kleine Welt,", "tokens": ["Dich", "gr\u00fc\u00df", "ich", ",", "klei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In der mein Herz und meine Treu", "tokens": ["In", "der", "mein", "Herz", "und", "mei\u00b7ne", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich gar zu wohl gef\u00e4llt.", "tokens": ["Sich", "gar", "zu", "wohl", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wie krank ich bin und einsam hier,", "tokens": ["Wie", "krank", "ich", "bin", "und", "ein\u00b7sam", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir tr\u00e4umt vom Wiedersehn,", "tokens": ["Mir", "tr\u00e4umt", "vom", "Wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von unserm Haus, \u2013 da wollen wir", "tokens": ["Von", "un\u00b7serm", "Haus", ",", "\u2013", "da", "wol\u00b7len", "wir"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "$(", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch manches Fest begehn.", "tokens": ["Noch", "man\u00b7ches", "Fest", "be\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Willkommen, s\u00fc\u00dfe Weihnachtslust,", "tokens": ["Will\u00b7kom\u00b7men", ",", "s\u00fc\u00b7\u00dfe", "Weih\u00b7nachts\u00b7lust", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O wunderbarer Schein!", "tokens": ["O", "wun\u00b7der\u00b7ba\u00b7rer", "Schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Himmel zeuch in meine Brust", "tokens": ["Vom", "Him\u00b7mel", "zeuch", "in", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nimm sie g\u00e4nzlich ein.", "tokens": ["Und", "nimm", "sie", "g\u00e4nz\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}